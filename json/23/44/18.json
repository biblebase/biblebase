    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们愚昧无知，因为眼睛被封住以致看不见，心窍被塞住以致不明白。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們並不知道，也不明白； 因為耶和華閉住他們的眼睛，使他們不能看見， 塞住他們的心，使他們不能明白。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这样的人不明白，也不领悟， 因为耶和华蒙蔽他们的眼使他们看不见， 蒙蔽他们的心使他们不明悟。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們不知道，也不思想；因為耶和華閉住他們的眼，不能看見，塞住他們的心，不能明白。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They know not, nor do they discern, for he has shut their eyes, so that they cannot see, and their hearts, so that they cannot understand."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They do not know, nor do they understand, for He has smeared over their eyes so that they cannot see and their hearts so that they cannot comprehend."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They know nothing, they understand nothing; their eyes are plastered over so they cannot see, and their minds closed so they cannot understand."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They do not know nor understand; For He has shut their eyes, so that they cannot see, And their hearts, so that they cannot understand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Such stupidity and ignorance! Their eyes are closed, and they cannot see. Their minds are shut, and they cannot think."
      }
