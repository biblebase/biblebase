      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你不知道这些事， 甚至闻所未闻。 我知道你生性诡诈， 你自出母胎就被称为悖逆之徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你從來沒有聽過，也不知道； 你的耳朵從來未曾開通。 我原知道你行事非常詭詐； 你自出母胎以來，就被稱為叛徒。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你未曾听过，未曾知道， 你的耳朵也从未开通。 我知道你极其背信， 你在母腹中就被称为叛逆者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你未曾聽見，未曾知道； 你的耳朵從來未曾開通。 我原知道你行事極其詭詐， 你自從出胎以來， 便稱為悖逆的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You have never heard, you have never known, from of old your ear has not been opened. For I knew that you would surely deal treacherously, and that from before birth you were called a rebel."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have not heard, you have not known. Even from long ago your ear has not been open, Because I knew that you would deal very treacherously; And you have been called a rebel from birth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have neither heard nor understood; from of old your ears have not been open. Well do I know how treacherous you are; you were called a rebel from birth."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Surely you did not hear, Surely you did not know; Surely from long ago your ear was not opened. For I knew that you would deal very treacherously, And were called a transgressor from the womb."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yes, I will tell you of things that are entirely new, things you never heard of before. For I know so well what traitors you are. You have been rebels from birth."
      }
