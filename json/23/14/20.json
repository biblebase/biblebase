    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "isa.14.16-21",
            "paragraphs": [
              "16～21.這篇詩又轉回來談地上的情形，把王可能的期望與實際的狀況作對比：不是國葬（18節），而是屍體被踐踏（19、20a～c節）；不是榮耀的行列，而是子孫的屍首（20d～21節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你必得不到君王的葬礼， 因为你祸国殃民， 杀害自己的百姓。 “恶人的子孙必永远被遗忘。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你必不得與別的君王同葬， 因為你敗壞了你的國， 殺戮了你的人民； 願那些惡人的後裔永遠不被人提說。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你必不能与其他君王同葬， 因为你毁坏了你的国， 杀戮了你的民。 愿作恶者的后裔永不被提起！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你不得與君王同葬； 因為你敗壞你的國，殺戮你的民。 惡人後裔的名，必永不提說。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You will not be joined with them in burial, because you have destroyed your land, you have slain your people. “May the offspring of evildoers nevermore be named!"
      },
      "nasb": {
        "version_id": "NASB",
