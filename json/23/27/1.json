    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到那日，耶和华必用祂无坚不摧的利剑惩罚巨龙——那飞快、曲行的蛇。祂必杀死那海中的怪物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到那日，耶和華要施用他銳利、巨大和有力的刀，懲罰鱷魚，就是那快行的蛇；懲罰鱷魚，就是那曲行的蛇；並且要殺海中的大龍。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "到那日，耶和华必用他严酷、巨大、有力的刀，惩罚利维亚坦，就是那逃窜的蛇；惩罚利维亚坦，就是那蜷曲的蛇；并且杀死海中的巨兽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到那日，耶和華必用他剛硬有力的大刀刑罰鱷魚－就是那快行的蛇，刑罰鱷魚－就是那曲行的蛇，並殺海中的大魚。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "In that day the Lord with his hard and great and strong sword will punish Leviathan the fleeing serpent, Leviathan the twisting serpent, and he will slay the dragon that is in the sea."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In that day the Lord will punish Leviathan the fleeing serpent, With His fierce and great and mighty sword, Even Leviathan the twisted serpent; And He will kill the dragon who lives in the sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In that day, the Lord will punish with his sword— his fierce, great and powerful sword— Leviathan the gliding serpent, Leviathan the coiling serpent; he will slay the monster of the sea."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "In that day the Lord with His severe sword, great and strong, Will punish Leviathan the fleeing serpent, Leviathan that twisted serpent; And He will slay the reptile that is in the sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In that day the Lord will take his terrible, swift sword and punish Leviathan, the swiftly moving serpent, the coiling, writhing serpent. He will kill the dragon of the sea."
      }
