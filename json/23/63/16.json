    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "isa.63.16",
            "paragraphs": [
              "六三16本節的意思是：縱使\"亞伯拉罕\"和\"以色列\"（即雅各）否認他們的子民，神仍要作他們的父。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然亚伯拉罕不认识我们， 以色列不承认我们， 但你是我们的父亲。 耶和华啊，你是我们的父亲， 你自古以来就是我们的救赎主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞伯拉罕雖然不認識我們， 以色列也不承認我們，但你是我們的父。 耶和華啊！你是我們的父， 從亙古以來，你的名字就是“我們的救贖主”。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "即使亚伯拉罕不认识我们， 以色列不承认我们， 你还是我们的父。 耶和华啊，你是我们的父！ 自古以来，你的名字就是“我们的救赎主”。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞伯拉罕雖然不認識我們， 以色列也不承認我們， 你卻是我們的父。 耶和華啊，你是我們的父； 從萬古以來，你名稱為「我們的救贖主」。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For you are our Father, though Abraham does not know us, and Israel does not acknowledge us; you, O Lord, are our Father, our Redeemer from of old is your name."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You are our Father, though Abraham does not know us And Israel does not recognize us. You, O Lord, are our Father, Our Redeemer from of old is Your name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you are our Father, though Abraham does not know us or Israel acknowledge us; you, Lord, are our Father, our Redeemer from of old is your name."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Doubtless You are our Father, Though Abraham was ignorant of us, And Israel does not acknowledge us. You, O Lord, are our Father; Our Redeemer from Everlasting is Your name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Surely you are still our Father! Even if Abraham and Jacob would disown us, Lord, you would still be our Father. You are our Redeemer from ages past."
      }
    },
