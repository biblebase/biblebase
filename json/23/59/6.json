    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这网不能做衣服遮盖你们。 你们行为邪恶， 做事残暴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的網，不能作衣服， 他們所作的，也不能遮蓋自己； 他們所作的是罪孽的作為， 他們的手裡滿是強暴的行為。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们的网不能作为衣服， 他们所做的也不能遮盖自己； 他们的作为是邪恶的作为， 他们手中都是残暴的行为；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所結的網不能成為衣服； 所做的也不能遮蓋自己。 他們的行為都是罪孽； 手所做的都是強暴。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Their webs will not serve as clothing; men will not cover themselves with what they make. Their works are works of iniquity, and deeds of violence are in their hands."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their webs will not become clothing, Nor will they cover themselves with their works; Their works are works of iniquity, And an act of violence is in their hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their cobwebs are useless for clothing; they cannot cover themselves with what they make. Their deeds are evil deeds, and acts of violence are in their hands."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Their webs will not become garments, Nor will they cover themselves with their works; Their works are works of iniquity, And the act of violence is in their hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their webs can’t be made into clothing, and nothing they do is productive. All their activity is filled with sin, and violence is their trademark."
      }
