    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以上都是利未各宗族的族长，他们住在耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以上這些人都是利未各家族的首領，按他們的家譜都是首領；這些人都住在耶路撒冷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以上都是利未人着名的族長，住在耶路撒冷。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "These were heads of fathers’ houses of the Levites, according to their generations, leaders. These lived in Jerusalem."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These were heads of fathers’ households of the Levites according to their generations, chief men, who lived in Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All these were heads of Levite families, chiefs as listed in their genealogy, and they lived in Jerusalem."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "These heads of the fathers’ houses of the Levites were heads throughout their generations. They dwelt at Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All these men lived in Jerusalem. They were the heads of Levite families and were listed as prominent leaders in their genealogical records."
      }
