      "fengsheng": {
        "content": [
          {
            "verse": "1ch.25.7",
            "paragraphs": [
              "25:7",
              "頌贊耶和華：唱歌的主要做用是贊美神和榮耀他的名(參弗5:19註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们和其他训练有素、负责歌颂耶和华的亲族共有二百八十八人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們和他們的親族，在歌頌耶和華的事上受過特別訓練，精於歌唱的，人數共有二百八十八人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們和他們的弟兄學習頌讚耶和華；善於歌唱的共有二百八十八人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The number of them along with their brothers, who were trained in singing to the Lord, all who were skillful, was 288."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their number who were trained in singing to the Lord, with their relatives, all who were skillful, was 288."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Along with their relatives—all of them trained and skilled in music for the Lord—they numbered 288."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So the number of them, with their brethren who were instructed in the songs of the Lord, all who were skillful, was two hundred and eighty-eight."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They and their families were all trained in making music before the Lord, and each of them—288 in all—was an accomplished musician."
      }
    },
