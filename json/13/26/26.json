    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "示罗密及其亲族负责管理库房中的奉献之物，这些物品是大卫王、众族长、千夫长、百夫长和将领献给上帝的圣物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這示羅密和他的兄弟掌管庫房裡的一切聖物，就是大衛王、眾家族首領、千夫長、百夫長和軍長所獻的聖物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這示羅密和他的弟兄掌管府庫的聖物，就是大衛王和眾族長、千夫長、百夫長，並軍長所分別為聖的物。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "This Shelomoth and his brothers were in charge of all the treasuries of the dedicated gifts that David the king and the heads of the fathers’ houses and the officers of the thousands and the hundreds and the commanders of the army had dedicated."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This Shelomoth and his relatives had charge of all the treasures of the dedicated gifts which King David and the heads of the fathers’ households, the commanders of thousands and hundreds, and the commanders of the army, had dedicated."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Shelomith and his relatives were in charge of all the treasuries for the things dedicated by King David, by the heads of families who were the commanders of thousands and commanders of hundreds, and by the other army commanders."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "This Shelomith and his brethren were over all the treasuries of the dedicated things which King David and the heads of fathers’ houses, the captains over thousands and hundreds, and the captains of the army, had dedicated."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Shelomoth and his relatives were in charge of the treasuries containing the gifts that King David, the family leaders, and the generals and captains and other officers of the army had dedicated to the Lord."
      }
    },
