    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.27.1-15",
            "paragraphs": [
              "二七1～15大衛的軍隊有二十八萬八千人，分成十二隊，每隊二萬四千人，每隊每年輪流服役一個月。在十二名將領中，有八名來自猶大支派。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "便雅悯的子孙亚拿突人亚比以谢负责第九班，在九月当值，他班内有两万四千人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "九月第九班的軍長是便雅憫支派亞拿突人亞比以謝，他那一班有二萬四千人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "九月第九班的班長是便雅憫族亞拿突人亞比以謝；他班內有二萬四千人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Ninth, for the ninth month, was Abiezer of Anathoth, a Benjaminite; in his division were 24,000."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The ninth for the ninth month was Abiezer the Anathothite of the Benjamites; and in his division were 24,000."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The ninth, for the ninth month, was Abiezer the Anathothite, a Benjamite. There were 24,000 men in his division."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The ninth captain for the ninth month was  Abiezer the Anathothite, of the Benjamites; in his division were twenty-four thousand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Abiezer from Anathoth in the territory of Benjamin was commander of the ninth division of 24,000 troops, which was on duty during the ninth month."
      }
    },
