    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "夏甲人雅悉管理羊群。以上这些人都是替大卫王管理产业的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "管理羊群的，是夏甲人雅悉。以上這些人都是給大衛王管理產業的官員。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這都是給大衛王掌管產業的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "All these were stewards of King David’s property."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jaziz the Hagrite had charge of the flocks. All these were overseers of the property which belonged to King David."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jaziz the Hagrite was in charge of the flocks. All these were the officials in charge of King David’s property."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and Jaziz the Hagrite was over the flocks. All these were the officials over King David’s property."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jaziz the Hagrite was in charge of the king’s flocks of sheep and goats. All these officials were overseers of King David’s property."
      }
    },
