      "leili": {
        "content": [
          {
            "verse": "1ch.19.4",
            "paragraphs": [
              "一九4\"將……胡須剃去一半\"。被視為極大的侮辱。參看撒母耳記下十章4節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "哈嫩便把大卫的臣仆抓起来，剃去他们一半胡子，割去他们下身的衣服，然后放走他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是，哈嫩拿住大衛的臣僕，剃去他們的鬍鬚，又把他們的衣服，從臀部以下割去半截，然後才放他們走。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "哈嫩便將大衛臣僕的鬍鬚剃去一半，又割斷他們下半截的衣服，使他們露出下體，打發他們回去。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So Hanun took David’s servants and shaved them and cut off their garments in the middle, at their hips, and sent them away;"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Hanun took David’s servants and shaved them and cut off their garments in the middle as far as their hips, and sent them away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Hanun seized David’s envoys, shaved them, cut off their garments at the buttocks, and sent them away."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore Hanun took David’s servants, shaved them, and cut off their garments in the middle, at their buttocks, and sent them away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Hanun seized David’s ambassadors and shaved them, cut off their robes at the buttocks, and sent them back to David in shame."
      }
