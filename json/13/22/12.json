    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿耶和华赐你聪明和智慧，以便你照祂的律法治理以色列。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你謹守遵行耶和華吩咐摩西向以色列人頒布的律例和典章，這樣你就必亨通。你要堅強勇敢，不要懼怕，也不要驚惶。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但願耶和華賜你聰明智慧，好治理以色列國，遵行耶和華－你神的律法。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Only, may the Lord grant you discretion and understanding, that when he gives you charge over Israel you may keep the law of the Lord your God."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Only the Lord give you discretion and understanding, and give you charge over Israel, so that you may keep the law of the Lord your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the Lord give you discretion and understanding when he puts you in command over Israel, so that you may keep the law of the Lord your God."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Only may the Lord  give you wisdom and understanding, and give you charge concerning Israel, that you may keep the law of the Lord your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And may the Lord give you wisdom and understanding, that you may obey the Law of the Lord your God as you rule over Israel."
      }
    },
