    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“要说，‘拯救我们的上帝啊， 求你救我们，招聚我们， 从列国把我们救出来， 我们好称谢你的圣名， 以赞美你为荣。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要說：‘拯救我們的　神啊，求你拯救我們， 招聚我們，從萬國中救我們出來， 好使我們稱謝你的聖名， 以讚美你為誇耀。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要說：拯救我們的神啊，求你救我們， 聚集我們，使我們脫離外邦， 我們好稱讚你的聖名，以讚美你為誇勝。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Say also: “Save us, O God of our salvation, and gather and deliver us from among the nations, that we may give thanks to your holy name and glory in your praise."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then say, “Save us, O God of our salvation, And gather us and deliver us from the nations, To give thanks to Your holy name, And glory in Your praise.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Cry out, “Save us, God our Savior; gather us and deliver us from the nations, that we may give thanks to your holy name, and glory in your praise.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And say, “Save us, O God of our salvation; Gather us together, and deliver us from the Gentiles, To give thanks to Your holy name, To triumph in Your praise.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Cry out, “Save us, O God of our salvation! Gather and rescue us from among the nations, so we can thank your holy name and rejoice and praise you.”"
      }
    },
