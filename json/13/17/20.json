    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，我们从未听过有谁能与你相比，你是独一无二的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊，照著我們耳中所聽見的，沒有誰能與你相比；除你以外，再沒有別的神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，照我們耳中聽見，沒有可比你的，除你以外再無神。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "There is none like you, O Lord, and there is no God besides you, according to all that we have heard with our ears."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, there is none like You, nor is there any God besides You, according to all that we have heard with our ears."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“There is no one like you, Lord, and there is no God but you, as we have heard with our own ears."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "O Lord, there  is none like You, nor is  there  any God besides You, according to all that we have heard with our ears."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O Lord, there is no one like you. We have never even heard of another God like you!"
      }
    },
