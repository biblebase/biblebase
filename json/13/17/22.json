        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你让以色列人永远做你的子民，你耶和华做他们的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使你的子民以色列永遠作你的子民；耶和華啊，你也作了他們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你使以色列人作你的子民，直到永遠；你－耶和華也作他們的神。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And you made your people Israel to be your people forever, and you, O Lord, became their God."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For Your people Israel You made Your own people forever, and You, O Lord, became their God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You made your people Israel your very own forever, and you, Lord, have become their God."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For You have made Your people Israel Your very own people forever; and You, Lord, have become their God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You chose Israel to be your very own people forever, and you, O Lord, became their God."
      }
