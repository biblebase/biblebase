    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你刚来不久，我怎么可以叫你跟我们一同四处飘流呢？我甚至不知道往何处去。你还是和你的弟兄回去吧，愿耶和华以慈爱和信实待你！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你昨天才來，我怎可以今天就讓你與我們一同流浪呢？現在我還不知道我要到哪裡去。你回去吧！帶著你的兄弟一同回去吧！願耶和華的慈愛和信實與你同在。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你來的日子不多，我今日怎好叫你與我們一同飄流、沒有一定的住處呢？你不如帶你的弟兄回去吧！願耶和華用慈愛誠實待你。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You came only yesterday, and shall I today make you wander about with us, since I go I know not where? Go back and take your brothers with you, and may the Lord show steadfast love and faithfulness to you.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You came only yesterday, and shall I today make you wander with us, while I go where I will? Return and take back your brothers; mercy and truth be with you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You came only yesterday. And today shall I make you wander about with us, when I do not know where I am going? Go back, and take your people with you. May the Lord show you kindness and faithfulness.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "In fact, you came only yesterday. Should I make you wander up and down with us today, since I go I know not where? Return, and take your brethren back. Mercy and truth be with you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You arrived only recently, and should I force you today to wander with us? I don’t even know where we will go. Go on back and take your kinsmen with you, and may the Lord show you his unfailing love and faithfulness.”"
      }
    },
