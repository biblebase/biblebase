    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约押追赶押尼珥回来后细点人数，发现除亚撒黑外还损失了十九个人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約押追趕押尼珥回來，集合所有人的時候，發現在大衛的臣僕中，除了亞撒黑以外，還少了十九個人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約押追趕押尼珥回來，聚集眾民，見大衛的僕人中缺少了十九個人和亞撒黑。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Joab returned from the pursuit of Abner. And when he had gathered all the people together, there were missing from David’s servants nineteen men besides Asahel."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Joab returned from following Abner; when he had gathered all the people together, nineteen of David’s servants besides Asahel were missing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Joab stopped pursuing Abner and assembled the whole army. Besides Asahel, nineteen of David’s men were found missing."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So Joab returned from pursuing Abner. And when he had gathered all the people together, there were missing of David’s servants nineteen men and Asahel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Meanwhile, Joab and his men also returned home. When Joab counted his casualties, he discovered that only 19 men were missing in addition to Asahel."
      }
    },
