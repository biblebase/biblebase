    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但希腊名字叫以吕马的那个术士反对使徒，试图拦阻总督信主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但術士以呂馬與使徒作對，要使省長轉離真道。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "但那行法术的以路摩——他的名字翻译出来就是“行法术的”——却抵挡他们，企图使省长转离这信仰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是那行法術的以呂馬（這名翻出來就是行法術的意思）敵擋使徒，要叫方伯不信真道。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But Elymas the magician (for that is the meaning of his name) opposed them, seeking to turn the proconsul away from the faith."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Elymas the magician (for so his name is translated) was opposing them, seeking to turn the proconsul away from the faith."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Elymas the sorcerer (for that is what his name means) opposed them and tried to turn the proconsul from the faith."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But Elymas the sorcerer (for so his name is translated) withstood them, seeking to turn the proconsul away from the faith."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Elymas, the sorcerer (as his name means in Greek), interfered and urged the governor to pay no attention to what Barnabas and Saul said. He was trying to keep the governor from believing."
      }
    },
