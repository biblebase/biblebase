    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂命令我们向世人传道，见证祂就是上帝指派来审判活人和死人的那位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他吩咐我們向人民傳講，鄭重證明他是　神所立，審判活人死人的主。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣还吩咐我们要向民众传道，郑重地见证他就是神所设定的那一位，活人和死人的审判者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他吩咐我們傳道給眾人，證明他是神所立定的，要作審判活人、死人的主。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he commanded us to preach to the people and to testify that he is the one appointed by God to be judge of the living and the dead."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He ordered us to preach to the people, and solemnly to testify that this is the One who has been appointed by God as Judge of the living and the dead."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He commanded us to preach to the people and to testify that he is the one whom God appointed as judge of the living and the dead."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And He commanded us to preach to the people, and to testify that it is He who was ordained by God to  be Judge of the living and the dead."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And he ordered us to preach everywhere and to testify that Jesus is the one appointed by God to be the judge of all—the living and the dead."
      }
    },
