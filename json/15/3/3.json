    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们虽然惧怕当地的居民，还是在原来的根基上重建祭坛，并早晚在上面向耶和华献燔祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們懼怕當地的居民，就在殿的原有根基上建立祭壇，在祭壇上向耶和華獻燔祭，就是早晚的燔祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們在原有的根基上築壇，因懼怕鄰國的民，又在其上向耶和華早晚獻燔祭，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They set the altar in its place, for fear was on them because of the peoples of the lands, and they offered burnt offerings on it to the Lord, burnt offerings morning and evening."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they set up the altar on its foundation, for they were terrified because of the peoples of the lands; and they offered burnt offerings on it to the Lord, burnt offerings morning and evening."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Despite their fear of the peoples around them, they built the altar on its foundation and sacrificed burnt offerings on it to the Lord, both the morning and evening sacrifices."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Though fear had  come upon them because of the people of those countries, they set the altar on its bases; and they offered burnt offerings on it to the Lord, both the morning and evening burnt offerings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
