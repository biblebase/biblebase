      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“We also take on ourselves the obligation to give yearly a third part of a shekel for the service of the house of our God:"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We also placed ourselves under obligation to contribute yearly one third of a shekel for the service of the house of our God:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“We assume the responsibility for carrying out the commands to give a third of a shekel each year for the service of the house of our God:"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Also we made ordinances for ourselves, to exact from ourselves yearly one-third of a shekel for the service of the house of our God:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“In addition, we promise to obey the command to pay the annual Temple tax of one-eighth of an ounce of silver for the care of the Temple of our God."
      }
