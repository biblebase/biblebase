    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "neh.10.30-39",
            "paragraphs": [
              "特別誓言（十30～39）",
              "全體會眾都願遵行全部律法（29b節），但其中有些部分特別沖擊他們的心。"
            ]
          },
          {
            "verse": "neh.10.30",
            "paragraphs": [
              "30.異族通婚。律法嚴厲禁止這事（不是基於種族歧視，而是基於宗教的不同，出三十四12～16；因此他們才能輕易接受摩押女子路得）。但在異族統治下的艱難時代，想在社會上升遷的誘惑太大，異族通婚就變成格外吸引人的晉升階梯。瑪拉基（二10～16）、以斯拉（九1及下）與尼希米（十三23～29）都面對這問題，且以不同但堅決的方式處理。最後，免得基督徒認為這已不是個問題，保羅就在哥林多後書六14～七1，以無法反駁的邏輯和激動闡釋這事。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们不将女儿嫁给当地的人，也不为儿子娶当地的女子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們必不把我們的女兒嫁給這地的外族人，也不為我們的兒子娶他們的女兒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並不將我們的女兒嫁給這地的居民，也不為我們的兒子娶他們的女兒。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "We will not give our daughters to the peoples of the land or take their daughters for our sons."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and that we will not give our daughters to the peoples of the land or take their daughters for our sons."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“We promise not to give our daughters in marriage to the peoples around us or take their daughters for our sons."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "We would not give our daughters as wives to the peoples of the land, nor take their daughters for our sons;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“We promise not to let our daughters marry the pagan people of the land, and not to let our sons marry their daughters."
      }
