    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们当众宣读这封信后，也要交给老底嘉教会宣读，你们也要宣读老底嘉教会转给你们的信。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這封信你們宣讀了以後，也要交給老底嘉的教會宣讀；你們也要讀老底嘉的那封信。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这封书信在你们那里宣读了以后，也一定要在劳迪西亚的教会中宣读；你们也要宣读从劳迪西亚转来的书信。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們念了這書信，便交給老底嘉的教會，叫他們也念；你們也要念從老底嘉來的書信。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And when this letter has been read among you, have it also read in the church of the Laodiceans; and see that you also read the letter from Laodicea."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When this letter is read among you, have it also read in the church of the Laodiceans; and you, for your part read my letter that is coming from Laodicea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After this letter has been read to you, see that it is also read in the church of the Laodiceans and that you in turn read the letter from Laodicea."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now when this epistle is read among you, see that it is read also in the church of the Laodiceans, and that you likewise read the epistle from Laodicea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After you have read this letter, pass it on to the church at Laodicea so they can read it, too. And you should read the letter I wrote to them."
      }
    },
