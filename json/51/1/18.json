    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂是教会的头，教会是祂的身体；祂是源头，是首先从死里复活的，这样祂可以在一切事上居首位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他是身體的頭，這身體就是教會。他是元始，是死人中首先復生的，好讓他在凡事上居首位；"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他是身体的头，就是教会的头， 他是元始，是从死人中首先复活的， 为要使他能在万有中居首位，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他也是教會全體之首。他是元始，是從死裏首先復生的，使他可以在凡事上居首位。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he is the head of the body, the church. He is the beginning, the firstborn from the dead, that in everything he might be preeminent."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He is also head of the body, the church; and He is the beginning, the firstborn from the dead, so that He Himself will come to have first place in everything."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he is the head of the body, the church; he is the beginning and the firstborn from among the dead, so that in everything he might have the supremacy."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And He is the head of the body, the church, who is the beginning, the firstborn from the dead, that in all things He may have the preeminence."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Christ is also the head of the church, which is his body. He is the beginning, supreme over all who rise from the dead. So he is first in everything."
      }
