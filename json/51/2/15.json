    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "打败了一切在灵界执政、掌权的邪恶势力，用基督在十字架上的胜利公开羞辱它们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他既然靠著十字架勝過了一切執政掌權的，廢除了他們的權勢，就在凱旋的行列中，把他們公開示眾。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神既然剥夺了那些统治者和掌权者的权势，就公开羞辱他们；藉着基督向他们夸胜了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "既將一切執政的、掌權的擄來，明顯給眾人看，就仗着十字架誇勝。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He disarmed the rulers and authorities and put them to open shame, by triumphing over them in him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He had disarmed the rulers and authorities, He made a public display of them, having triumphed over them through Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And having disarmed the powers and authorities, he made a public spectacle of them, triumphing over them by the cross."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Having disarmed principalities and powers, He made a public spectacle of them, triumphing over them in it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In this way, he disarmed the spiritual rulers and authorities. He shamed them publicly by his victory over them on the cross."
      }
    },
