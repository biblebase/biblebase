    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那天晚上，基比亚人包围我们住宿的房子，要杀死我。他们把我的妾蹂躏致死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基比亞人民起來攻擊我，在夜間包圍我住的房子攻擊我，有意殺害我；他們把我的妾污辱致死。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基比亞人夜間起來，圍了我住的房子，想要殺我，又將我的妾強姦致死。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the leaders of Gibeah rose against me and surrounded the house against me by night. They meant to kill me, and they violated my concubine, and she is dead."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the men of Gibeah rose up against me and surrounded the house at night because of me. They intended to kill me; instead, they ravished my concubine so that she died."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "During the night the men of Gibeah came after me and surrounded the house, intending to kill me. They raped my concubine, and she died."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the men of Gibeah rose against me, and surrounded the house at night because of me. They intended to kill me, but instead they ravished my concubine so that she died."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That night some of the leading citizens of Gibeah surrounded the house, planning to kill me, and they raped my concubine until she was dead."
      }
    },
