    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们在敌营周围各守其位，米甸全军边喊边逃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在營的四周各站在自己的位置；全營的人都亂跑，他們一面叫喊，一面逃跑。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們在營的四圍各站各的地方；全營的人都亂竄。三百人吶喊，使他們逃跑。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Every man stood in his place around the camp, and all the army ran. They cried out and fled."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Each stood in his place around the camp; and all the army ran, crying out as they fled."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While each man held his position around the camp, all the Midianites ran, crying out as they fled."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And every man stood in his place all around the camp; and the whole army ran and cried out and fled."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each man stood at his position around the camp and watched as all the Midianites rushed around in a panic, shouting as they ran to escape."
      }
    },
