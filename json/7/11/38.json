    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.11.38",
            "paragraphs": [
              "一一38\"為她終為處女哀哭\"。即她一生不能生孩子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶弗他答应了，给了她两个月的时间。于是，她便和同伴到山上为自己终身未嫁哀哭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶弗他說：“你去吧！”就讓她離去兩個月；她就帶著她的同伴去了，在山上為她的童貞哀哭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶弗他說：「你去吧！」就容她去兩個月。她便和同伴去了，在山上為她終為處女哀哭。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So he said, “Go.” Then he sent her away for two months, and she departed, she and her companions, and wept for her virginity on the mountains."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he said, “Go.” So he sent her away for two months; and she left with her companions, and wept on the mountains because of her virginity."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You may go,” he said. And he let her go for two months. She and her friends went into the hills and wept because she would never marry."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So he said, “Go.” And he sent her away for two months; and she went with her friends, and bewailed her virginity on the mountains."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You may go,” Jephthah said. And he sent her away for two months. She and her friends went into the hills and wept because she would never have children."
      }
    },
