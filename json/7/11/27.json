    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们没有得罪你们，你们却攻打我们。愿审判者耶和华今天在以色列人和亚扪人之间主持公道。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我沒有得罪你，你竟惡待我，攻打我；願審判者耶和華今日在以色列人和亞捫人中間，判斷是非。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "原來我沒有得罪你，你卻攻打我，惡待我。願審判人的耶和華今日在以色列人和亞捫人中間判斷是非。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I therefore have not sinned against you, and you do me wrong by making war on me. The Lord, the Judge, decide this day between the people of Israel and the people of Ammon.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I therefore have not sinned against you, but you are doing me wrong by making war against me; may the Lord, the Judge, judge today between the sons of Israel and the sons of Ammon.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have not wronged you, but you are doing me wrong by waging war against me. Let the Lord, the Judge, decide the dispute this day between the Israelites and the Ammonites.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore I have not sinned against you, but you wronged me by fighting against me. May the Lord, the Judge, render judgment this day between the children of Israel and the people of Ammon.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Therefore, I have not sinned against you. Rather, you have wronged me by attacking me. Let the Lord, who is judge, decide today which of us is right—Israel or Ammon.”"
      }
    },
