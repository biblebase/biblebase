    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.18.19",
            "paragraphs": [
              "一八19但支派的人有意把約拿單從家庭的祭司提升為支派的祭司！"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们说：“安静，别作声，跟我们来做我们的师父和祭司吧。做以色列一个支派、一个宗族的祭司难道不比做一家人的祭司更好吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們回答他：“不要出聲，用手掩著你的嘴，跟我們走吧，好作我們的師父和祭司。你作一家的祭司好呢？還是作以色列中一個支派一個家族的祭司好呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們回答說：「不要作聲，用手摀口，跟我們去吧！我們必以你為父、為祭司。你作一家的祭司好呢？還是作以色列一族一支派的祭司好呢？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they said to him, “Keep quiet; put your hand on your mouth and come with us and be to us a father and a priest. Is it better for you to be priest to the house of one man, or to be priest to a tribe and clan in Israel?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said to him, “Be silent, put your hand over your mouth and come with us, and be to us a father and a priest. Is it better for you to be a priest to the house of one man, or to be priest to a tribe and a family in Israel?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They answered him, “Be quiet! Don’t say a word. Come with us, and be our father and priest. Isn’t it better that you serve a tribe and clan in Israel as priest rather than just one man’s household?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they said to him, “Be quiet, put your hand over your mouth, and come with us; be a father and a priest to us. Is  it better for you to be a priest to the household of one man, or that you be a priest to a tribe and a family in Israel?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Be quiet and come with us,” they said. “Be a father and priest to all of us. Isn’t it better to be a priest for an entire tribe and clan of Israel than for the household of just one man?”"
      }
