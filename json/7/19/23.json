    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.19.23-24",
            "paragraphs": [
              "一九23,24關於接待客旅的要求，參看創世記十九章8節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "老人出去对他们说：“弟兄们啊，不要做这种恶事！这人是我的客人，不要干这种卑鄙的事！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那房主出來見他們，對他們說：“我的眾兄弟啊，不可這樣，請你們不要作惡；這人既然進了我的家，你們就不要作這羞恥的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那房主出來對他們說：「弟兄們哪，不要這樣作惡；這人既然進了我的家，你們就不要行這醜事。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the man, the master of the house, went out to them and said to them, “No, my brothers, do not act so wickedly; since this man has come into my house, do not do this vile thing."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the man, the owner of the house, went out to them and said to them, “No, my fellows, please do not act so wickedly; since this man has come into my house, do not commit this act of folly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The owner of the house went outside and said to them, “No, my friends, don’t be so vile. Since this man is my guest, don’t do this outrageous thing."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But the man, the master of the house, went out to them and said to them, “No, my brethren! I beg you, do not act so wickedly! Seeing this man has come into my house, do not commit this outrage."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The old man stepped outside to talk to them. “No, my brothers, don’t do such an evil thing. For this man is a guest in my house, and such a thing would be shameful."
      }
    },
