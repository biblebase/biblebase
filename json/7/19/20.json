    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.19.15-21",
            "paragraphs": [
              "一九15～21雖然他們在\"城裏的街上\"等候，但基比拉人沒有按照慣例接待他們。最後，一個旅居在基比拉的以法蓮人接他們進家裏。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那老人说：“到我家来吧，我会供应你们的需要，千万不可在街头露宿。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那老年人說：“願你平安，你所缺乏的，由我負責好了；只是不可在廣場上過夜。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "老年人說：「願你平安！你所需用的我都給你，只是不可在街上過夜。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the old man said, “Peace be to you; I will care for all your wants. Only, do not spend the night in the square.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The old man said, “Peace to you. Only let me take care of all your needs; however, do not spend the night in the open square.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You are welcome at my house,” the old man said. “Let me supply whatever you need. Only don’t spend the night in the square.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the old man said, “Peace be with you! However, let all your needs be my responsibility; only do not spend the night in the open square.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You are welcome to stay with me,” the old man said. “I will give you anything you might need. But whatever you do, don’t spend the night in the square.”"
      }
    },
