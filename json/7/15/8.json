    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "参孙大肆击杀他们，杀死了很多人。事后，他下到以坦，住在那里的岩洞里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "參孫擊打他們，連腿帶腰都砍斷了，那是一場極大的殺戮；然後他下去，住在以坦磐石的穴中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "參孫就大大擊殺他們，連腿帶腰都砍斷了。他便下去，住在以坦磐的穴內。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he struck them hip and thigh with a great blow, and he went down and stayed in the cleft of the rock of Etam."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He struck them ruthlessly with a great slaughter; and he went down and lived in the cleft of the rock of Etam."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He attacked them viciously and slaughtered many of them. Then he went down and stayed in a cave in the rock of Etam."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So he attacked them hip and thigh with a great slaughter; then he went down and dwelt in the cleft of the rock of Etam."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So he attacked the Philistines with great fury and killed many of them. Then he went to live in a cave in the rock of Etam."
      }
