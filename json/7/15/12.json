      "leili": {
        "content": [
          {
            "verse": "jdg.15.12",
            "paragraphs": [
              "一五12\"你們自己不害死我\"。參孫要求以色列人不要攻擊他，這樣他就不必報復，不必流猶太人的血。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大人说：“我们来是要捉拿你，把你交给非利士人。”参孙说：“你们要保证你们不会亲手杀我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大人對他說：“我們下來要捆綁你，把你交在非利士人手裡。”參孫對他們說：“你們要向我發誓，你們不親自殺害我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大人對他說：「我們下來是要捆綁你，將你交在非利士人手中。」參孫說：「你們要向我起誓，應承你們自己不害死我。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they said to him, “We have come down to bind you, that we may give you into the hands of the Philistines.” And Samson said to them, “Swear to me that you will not attack me yourselves.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said to him, “We have come down to bind you so that we may give you into the hands of the Philistines.” And Samson said to them, “Swear to me that you will not kill me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They said to him, “We’ve come to tie you up and hand you over to the Philistines.” Samson said, “Swear to me that you won’t kill me yourselves.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But they said to him, “We have come down to arrest you, that we may deliver you into the hand of the Philistines.” Then Samson said to them, “Swear to me that you will not kill me yourselves.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the men of Judah told him, “We have come to tie you up and hand you over to the Philistines.” “All right,” Samson said. “But promise that you won’t kill me yourselves.”"
      }
