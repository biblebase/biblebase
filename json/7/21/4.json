    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.21.1-7",
            "paragraphs": [
              "二一1～7由於幾乎所有的便雅憫人都被殺害了（比較二○47），又由於其它支派曾發誓不讓他們的處女嫁給便雅憫人，所以便雅憫支派就面對完全滅絕的危險。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第二天，众人清早起来在那里筑了一座祭坛，献上燔祭和平安祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "次日，眾人清早起來，在那裡築了一座祭壇，獻上燔祭和平安祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "次日清早，百姓起來，在那裏築了一座壇，獻燔祭和平安祭。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the next day the people rose early and built there an altar and offered burnt offerings and peace offerings."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It came about the next day that the people arose early and built an altar there and offered burnt offerings and peace offerings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Early the next day the people built an altar and presented burnt offerings and fellowship offerings."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So it was, on the next morning, that the people rose early and built an altar there, and offered burnt offerings and peace offerings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Early the next morning the people built an altar and presented their burnt offerings and peace offerings on it."
      }
