    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.21.1-7",
            "paragraphs": [
              "二一1～7由於幾乎所有的便雅憫人都被殺害了（比較二○47），又由於其它支派曾發誓不讓他們的處女嫁給便雅憫人，所以便雅憫支派就面對完全滅絕的危險。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人为他们的同胞便雅悯人难过，说：“如今以色列绝了一个支派。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人為他們的兄弟便雅憫難過，說：“今天以色列中有一個支派被剪除了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人為他們的弟兄便雅憫後悔，說：「如今以色列中絕了一個支派了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the people of Israel had compassion for Benjamin their brother and said, “One tribe is cut off from Israel this day."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the sons of Israel were sorry for their brother Benjamin and said, “One tribe is cut off from Israel today."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the Israelites grieved for the tribe of Benjamin, their fellow Israelites. “Today one tribe is cut off from Israel,” they said."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the children of Israel grieved for Benjamin their brother, and said, “One tribe is cut off from Israel today."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Israelites felt sorry for their brother Benjamin and said, “Today one of the tribes of Israel has been cut off."
      }
