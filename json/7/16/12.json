    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，大利拉让人埋伏在内室，她用七根新绳子绑住参孙，然后喊道：“参孙，非利士人来捉你了！”结果，参孙挣断了手臂上的绳子，就像挣断一根线一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是大利拉把新繩子拿了來，把參孫捆綁了，對他說：“參孫哪，非利士人上來捉你了！”當時有人埋伏在她的內室裡等候著。參孫把繩子從手臂上掙斷下來，像掙斷一根線一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大利拉就用新繩捆綁他，對他說：「參孫哪，非利士人拿你來了！」有人預先埋伏在內室裏。參孫將臂上的繩掙斷了，如掙斷一條線一樣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So Delilah took new ropes and bound him with them and said to him, “The Philistines are upon you, Samson!” And the men lying in ambush were in an inner chamber. But he snapped the ropes off his arms like a thread."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Delilah took new ropes and bound him with them and said to him, “The Philistines are upon you, Samson!” For the men were lying in wait in the inner room. But he snapped the ropes from his arms like a thread."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Delilah took new ropes and tied him with them. Then, with men hidden in the room, she called to him, “Samson, the Philistines are upon you!” But he snapped the ropes off his arms as if they were threads."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore Delilah took new ropes and bound him with them, and said to him, “The Philistines are upon you, Samson!” And men  were lying in wait, staying in the room. But he broke them off his arms like a thread."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Delilah took new ropes and tied him up with them. The men were hiding in the inner room as before, and again Delilah cried out, “Samson! The Philistines have come to capture you!” But again Samson snapped the ropes from his arms as if they were thread."
      }
