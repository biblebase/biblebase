    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.8.23",
            "paragraphs": [
              "八23基甸拒絕\"管理\"百姓和建立王朝，反而確認耶和華的王權。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基甸回答说：“我和我子孙都不统治你们，统治你们的是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基甸回答他們：“我不統治你們，我的子孫也不統治你們，唯有耶和華統治你們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基甸說：「我不管理你們，我的兒子也不管理你們，惟有耶和華管理你們。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Gideon said to them, “I will not rule over you, and my son will not rule over you; the Lord will rule over you.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Gideon said to them, “I will not rule over you, nor shall my son rule over you; the Lord shall rule over you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Gideon told them, “I will not rule over you, nor will my son rule over you. The Lord will rule over you.”"
      },
      "nkjv": {
