    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要被虚空的道理欺骗，因为上帝的烈怒必临到上述那些悖逆的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要給別人用空言欺騙了你們，正因為這些事，　神的震怒必定臨到那些悖逆的人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "不要让任何人用虚空的话把你们欺骗了；正是为了这些，神的震怒就临到那些不信从的儿女身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要被人虛浮的話欺哄；因這些事，神的忿怒必臨到那悖逆之子。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Let no one deceive you with empty words, for because of these things the wrath of God comes upon the sons of disobedience."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let no one deceive you with empty words, for because of these things the wrath of God comes upon the sons of disobedience."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let no one deceive you with empty words, for because of such things God’s wrath comes on those who are disobedient."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Let no one deceive you with empty words, for because of these things the wrath of God comes upon the sons of disobedience."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t be fooled by those who try to excuse these sins, for the anger of God will fall on all who disobey him."
      }
