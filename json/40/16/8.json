    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣知道他们的心思，就说：“你们的信心太小了！为什么议论没有带饼的事呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌知道了，就說：“小信的人，為甚麼議論沒有餅這件事呢？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣知道了，就说：“小信的人哪，你们为什么彼此讨论没有带饼的事呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌看出來，就說：「你們這小信的人，為甚麼因為沒有餅彼此議論呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But Jesus, aware of this, said, “O you of little faith, why are you discussing among yourselves the fact that you have no bread?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Jesus, aware of this, said, “You men of little faith, why do you discuss among yourselves that you have no bread?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Aware of their discussion, Jesus asked, “You of little faith, why are you talking among yourselves about having no bread?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But Jesus, being aware of it, said to them, “O you of little faith, why do you reason among yourselves because you have brought no bread?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus knew what they were saying, so he said, “You have so little faith! Why are you arguing with each other about having no bread?"
      }
