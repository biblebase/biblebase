    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对他说：“朋友，你要做的事，快动手吧。”于是那些人上前，下手捉拿耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對他說：“朋友，你來要作的事，快作吧！”於是那些人上前來，動手拿住耶穌，逮捕了他。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣对他说：“朋友，你为什么来到这里？” 于是，那些人上前来，下手拘捕了耶稣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他說：「朋友，你來要做的事，就做吧。」於是那些人上前，下手拿住耶穌。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Jesus said to him, “Friend, do what you came to do.” Then they came up and laid hands on Jesus and seized him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus said to him, “Friend, do what you have come for.” Then they came and laid hands on Jesus and seized Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus replied, “Do what you came for, friend.” Then the men stepped forward, seized Jesus and arrested him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But Jesus said to him, “Friend, why have you come?” Then they came and laid hands on Jesus and took Him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus said, “My friend, go ahead and do what you have come for.” Then the others grabbed Jesus and arrested him."
      }
    },
