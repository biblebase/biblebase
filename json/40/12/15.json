    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣知道了，就离开那个地方。很多人跟随祂，耶稣医好了其中所有患病的人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌知道了，就離開那裡。有很多人跟隨他，他醫好他們所有的病人，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣知道了，就离开那里。有一大群人跟随他，耶稣使他们都痊愈了，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌知道了，就離開那裏，有許多人跟着他。他把其中有病的人都治好了；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Jesus, aware of this, withdrew from there. And many followed him, and he healed them all"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Jesus, aware of this, withdrew from there. Many followed Him, and He healed them all,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Aware of this, Jesus withdrew from that place. A large crowd followed him, and he healed all who were ill."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But when Jesus knew it,  He withdrew from there. And great multitudes followed Him, and He healed them all."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jesus knew what they were planning. So he left that area, and many people followed him. He healed all the sick among them,"
      }
    },
