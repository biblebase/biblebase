    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.25.18-22",
            "paragraphs": [
              "18～22.實事求是的亞比該體認到問題的嚴重，一刻也不敢耽擱，收拾起六百人飽餐一頓所需的食物。節期筵席的準備工作無疑已經完成，她就從中取走所需的一切，卻不致使剪羊毛人大量的宴會食物突然變少，因為拿八對他妻子主動進行的這件事一無所知，也沒有懷疑。這期間，大衛正在途中（對於所受不公平的對待曾感慨地說：他向我以惡報善），要報被辱之仇，將拿八與他所有男丁全都剪除；他雖然曾經饒恕掃羅，以善報惡（撒上二十四17），這次卻是除了殺人流血之外，沒有第二個念頭。所以亞比該與大衛的會面滿了引人註目的緊張氣氛：拿八家至少有一個女人可能逃過刀劍的殺害！"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她骑着驴下山的时候，就看见大卫和他的部下迎面而来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞比該騎著驢，正從山坡下來，忽見大衛和跟隨他的人也正從對面迎著她走下來，亞比該就與他相遇了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞比該騎着驢，正下山坡，見大衛和跟隨他的人從對面下來，亞比該就迎接他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And as she rode on the donkey and came down under cover of the mountain, behold, David and his men came down toward her, and she met them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It came about as she was riding on her donkey and coming down by the hidden part of the mountain, that behold, David and his men were coming down toward her; so she met them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As she came riding her donkey into a mountain ravine, there were David and his men descending toward her, and she met them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So it was, as she rode on the donkey, that she went down under cover of the hill; and there were David and his men, coming down toward her, and she met them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As she was riding her donkey into a mountain ravine, she saw David and his men coming toward her."
      }
    },
