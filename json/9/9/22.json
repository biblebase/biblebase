    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.9.22-24",
            "paragraphs": [
              "22～24.不管掃羅是不是害羞，他與仆人都受招待入席，並且坐在首位上，這位子是特別保留的，他們乃是貴客！分開來收存的腿肉是歸給祭司的分，「那……舉祭的腿，……是歸亞倫和他兒子的，這些都要成為聖」（出二十九27）。掃羅一定會感到困惑，這不僅出乎他意料之外，並且儼然當他是個祭司看待。他仍然不知道自己就是耶和華為以色列人揀選的第一位君王，要成為耶和華的受膏者，被賦予特殊的權利，包括分給他腿肉在內。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撒母耳带着扫罗和他的仆人来到一个大厅，让他们在请来的三十位客人中坐上座。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒母耳把掃羅和掃羅的僕人領進了客廳，使他們坐在客人中的首位；那裡約有三十人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "撒母耳領掃羅和他僕人進了客堂，使他們在請來的客中坐首位；客約有三十個人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Samuel took Saul and his young man and brought them into the hall and gave them a place at the head of those who had been invited, who were about thirty persons."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Samuel took Saul and his servant and brought them into the hall and gave them a place at the head of those who were invited, who were about thirty men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Samuel brought Saul and his servant into the hall and seated them at the head of those who were invited—about thirty in number."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now Samuel took Saul and his servant and brought them into the hall, and had them sit in the place of honor among those who were invited; there were about thirty persons."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Samuel brought Saul and his servant into the hall and placed them at the head of the table, honoring them above the thirty special guests."
      }
    },
