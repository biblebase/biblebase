    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.26.1-25",
            "paragraphs": [
              "x.大衛第二次饒恕掃羅（二十六1～25）",
              "西弗人曾經向掃羅泄露大衛的行蹤（撒上二十三19），若不是由於非利士人入侵，掃羅必須向西抵禦而受到打岔，大衛恐怕已經被害了；現在他們又試著要再度幫助掃羅，大衛的一舉一動全都受到嚴密的監視。"
            ]
          },
          {
            "verse": "1sa.26.7-9",
            "paragraphs": [
              "7～9.這兩個人非常大膽，挑了一條路穿過沈睡中的部隊，進到掃羅睡臥的中心點。王放在頭旁的槍是為了保護他的，但它差一點就在亞比篩手中變成取王性命的攻擊武器。亞比篩以自己作戰的專精技巧而自豪：一刺就成，不用再刺。大衛就像素常在掃羅面前的表現一樣，反應毫不遲疑：有誰伸手害耶和華的受膏者而無罪呢？王的身體是神聖不可侵犯的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚比筛对大卫说：“现在上帝把你的敌人交在你手中了，让我一矛把他钉在地上，决不用刺第二下。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞比篩對大衛說：“今天　神把你的仇敵交在你手裡了。現在求你容我用矛把他刺透在地上，一刺就夠，不必再刺。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞比篩對大衛說：「現在神將你的仇敵交在你手裏，求你容我拿槍將他刺透在地，一刺就成，不用再刺。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Abishai said to David, “God has given your enemy into your hand this day. Now please let me pin him to the earth with one stroke of the spear, and I will not strike him twice.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Abishai said to David, “Today God has delivered your enemy into your hand; now therefore, please let me strike him with the spear to the ground with one stroke, and I will not strike him the second time.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Abishai said to David, “Today God has delivered your enemy into your hands. Now let me pin him to the ground with one thrust of the spear; I won’t strike him twice.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Abishai said to David, “God has delivered your enemy into your hand this day. Now therefore, please, let me strike him at once with the spear, right to the earth; and I will not have  to  strike him a second time!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“God has surely handed your enemy over to you this time!” Abishai whispered to David. “Let me pin him to the ground with one thrust of the spear; I won’t need to strike twice!”"
      }
