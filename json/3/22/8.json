    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "lev.22.1-9",
            "paragraphs": [
              "二二1～9祭司象其它人一樣會變得不潔（自己不潔或因接觸不潔的人而變得不潔），而在不潔期間，他們不能執行祭司的職務。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他不可吃自然死亡或被野兽撕裂的动物，否则就不洁净。我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "自死的或是野獸撕裂的動物，他都不可吃，免得玷污自己；我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "自死的或是被野獸撕裂的，他不可吃，因此污穢自己。我是耶和華。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He shall not eat what dies of itself or is torn by beasts, and so make himself unclean by it: I am the Lord.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He shall not eat an animal which dies or is torn by beasts, becoming unclean by it; I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He must not eat anything found dead or torn by wild animals, and so become unclean through it. I am the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Whatever dies naturally or is torn by beasts he shall not eat, to defile himself with it: I am the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He may not eat an animal that has died a natural death or has been torn apart by wild animals, for this would defile him. I am the Lord."
      }
    },
