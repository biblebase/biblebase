    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "lev.22.1-9",
            "paragraphs": [
              "二二1～9祭司象其它人一樣會變得不潔（自己不潔或因接觸不潔的人而變得不潔），而在不潔期間，他們不能執行祭司的職務。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把以下条例告诉他们。 “你们的后代中，如果有人不洁净，却接近以色列人献给我的圣物，必须把他从我面前铲除。我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要對他們說：你們世世代代的後裔中，若是不潔淨，卻接近以色列人分別為聖歸耶和華的聖物，那人必從我面前被剪除；我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要對他們說：你們世世代代的後裔，凡身上有污穢、親近以色列人所分別為聖、歸耶和華聖物的，那人必在我面前剪除。我是耶和華。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Say to them, ‘If any one of all your offspring throughout your generations approaches the holy things that the people of Israel dedicate to the Lord, while he has an uncleanness, that person shall be cut off from my presence: I am the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Say to them, ‘If any man among all your descendants throughout your generations approaches the holy gifts which the sons of Israel dedicate to the Lord, while he has an uncleanness, that person shall be cut off from before Me; I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Say to them: ‘For the generations to come, if any of your descendants is ceremonially unclean and yet comes near the sacred offerings that the Israelites consecrate to the Lord, that person must be cut off from my presence. I am the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Say to them: ‘Whoever of all your descendants throughout your generations, who goes near the holy things which the children of Israel dedicate to the Lord, while he has uncleanness upon him, that person shall be cut off from My presence: I am the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give them the following instructions. “In all future generations, if any of your descendants is ceremonially unclean when he approaches the sacred offerings that the people of Israel consecrate to the Lord, he must be cut off from my presence. I am the Lord."
      }
    },
