      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "分封王希律为了他弟弟的妻子希罗迪娅的事，以及自己所做的一切恶事，受到约翰的责备。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
