    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是那群鬼便离开那人，进入猪群。那群猪就一路奔下陡坡，冲进湖里淹死了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "鬼就從那人身上出來，進入豬群，豬群闖下山崖，掉在海裡淹死了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "鬼魔就从那个人身上出来，进入猪里面。于是那群猪从山崖冲到湖里淹死了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "鬼就從那人出來，進入豬裏去。於是那羣豬闖下山崖，投在湖裏淹死了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then the demons came out of the man and entered the pigs, and the herd rushed down the steep bank into the lake and drowned."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the demons came out of the man and entered the swine; and the herd rushed down the steep bank into the lake and was drowned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the demons came out of the man, they went into the pigs, and the herd rushed down the steep bank into the lake and was drowned."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then the demons went out of the man and entered the swine, and the herd ran violently down the steep place into the lake and drowned."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the demons came out of the man and entered the pigs, and the entire herd plunged down the steep hillside into the lake and drowned."
      }
