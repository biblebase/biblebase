    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当时有一大群猪在附近山坡上觅食，污鬼就央求耶稣准许它们进入猪群，耶稣答应了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那裡有一大群豬正在山上吃東西。他們求耶穌准他們進入豬群。耶穌准許了，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "当时，那里有一大群猪正在山上吃食。那些鬼魔就央求耶稣准许它们进入那些猪里，耶稣准许了它们。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那裏有一大羣豬在山上吃食。鬼央求耶穌，准他們進入豬裏去。耶穌准了他們，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now a large herd of pigs was feeding there on the hillside, and they begged him to let them enter these. So he gave them permission."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now there was a herd of many swine feeding there on the mountain; and the demons implored Him to permit them to enter the swine. And He gave them permission."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A large herd of pigs was feeding there on the hillside. The demons begged Jesus to let them go into the pigs, and he gave them permission."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now a herd of many swine was feeding there on the mountain. So they begged Him that He would permit them to enter them. And He permitted them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There happened to be a large herd of pigs feeding on the hillside nearby, and the demons begged him to let them enter into the pigs. So Jesus gave them permission."
      }
