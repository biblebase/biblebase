    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣读完便收好书卷，还给负责的人，然后坐下。会堂里的人都把目光集中在祂身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把書卷捲好，交還侍役，就坐下。會堂裡眾人都注視他。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "然后耶稣把书卷好，还给会堂助理，坐了下来。会堂里，众人的眼睛都注视着他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是把書捲起來，交還執事，就坐下。會堂裏的人都定睛看他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he rolled up the scroll and gave it back to the attendant and sat down. And the eyes of all in the synagogue were fixed on him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He closed the book, gave it back to the attendant and sat down; and the eyes of all in the synagogue were fixed on Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he rolled up the scroll, gave it back to the attendant and sat down. The eyes of everyone in the synagogue were fastened on him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then He closed the book, and gave it back to the attendant and sat down. And the eyes of all who were in the synagogue were fixed on Him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He rolled up the scroll, handed it back to the attendant, and sat down. All eyes in the synagogue looked at him intently."
      }
