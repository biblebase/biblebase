    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂的门徒雅各和约翰见状，说：“主啊，你要我们叫天上的火降下来烧死他们吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的門徒雅各、約翰看見了，就說：“主啊，你要我們吩咐火從天降下來，燒滅他們嗎？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "门徒雅各和约翰看见了，就说：“主啊，你要我们吩咐火从天上降下来烧灭他们吗？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的門徒雅各、約翰看見了，就說：「主啊，你要我們吩咐火從天上降下來燒滅他們，像以利亞所做的嗎？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And when his disciples James and John saw it, they said, “Lord, do you want us to tell fire to come down from heaven and consume them?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When His disciples James and John saw this, they said, “Lord, do You want us to command fire to come down from heaven and consume them?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the disciples James and John saw this, they asked, “Lord, do you want us to call fire down from heaven to destroy them?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And when His disciples James and John saw this, they said, “Lord, do You want us to command fire to come down from heaven and consume them, just as Elijah did?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When James and John saw this, they said to Jesus, “Lord, should we call down fire from heaven to burn them up?”"
      }
