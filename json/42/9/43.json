    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大家看见了上帝的大能，都很惊奇。 他们正为耶稣所做的一切惊讶不已时，耶稣对门徒说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人都驚奇　神的大能。 眾人還在希奇耶穌所作的這一切時，他對門徒說："
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "大家都对神的威荣惊叹不已。 正当大家对耶稣所做的一切感到惊奇的时候，耶稣对他的门徒们说："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人都詫異神的大能 。 耶穌所做的一切事，眾人正希奇的時候，耶穌對門徒說："
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And all were astonished at the majesty of God. But while they were all marveling at everything he was doing, Jesus said to his disciples,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they were all amazed at the greatness of God. But while everyone was marveling at all that He was doing, He said to His disciples,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And they were all amazed at the greatness of God. While everyone was marveling at all that Jesus did, he said to his disciples,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they were all amazed at the majesty of God. But while everyone marveled at all the things which Jesus did, He said to His disciples,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Awe gripped the people as they saw this majestic display of God’s power. While everyone was marveling at everything he was doing, Jesus said to his disciples,"
      }
    },
