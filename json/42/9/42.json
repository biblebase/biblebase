    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那孩子走过来时，鬼又把他摔倒，使他抽搐，耶稣立刻斥责污鬼，把那孩子治好了，交给他父亲。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那孩子正走過來的時候，污鬼把他摔倒，使他重重地抽瘋。耶穌斥責那污靈，把孩子醫好，交給他父親。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "那孩子上前来的时候，鬼魔把他摔倒，使他剧烈地抽疯。耶稣就斥责那污灵，使孩子痊愈，把孩子交给他父亲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正來的時候，鬼把他摔倒，叫他重重地抽瘋。耶穌就斥責那污鬼，把孩子治好了，交給他父親。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "While he was coming, the demon threw him to the ground and convulsed him. But Jesus rebuked the unclean spirit and healed the boy, and gave him back to his father."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While he was still approaching, the demon slammed him to the ground and threw him into a convulsion. But Jesus rebuked the unclean spirit, and healed the boy and gave him back to his father."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even while the boy was coming, the demon threw him to the ground in a convulsion. But Jesus rebuked the impure spirit, healed the boy and gave him back to his father."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And as he was still coming, the demon threw him down and convulsed him. Then Jesus rebuked the unclean spirit, healed the child, and gave him back to his father."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As the boy came forward, the demon knocked him to the ground and threw him into a violent convulsion. But Jesus rebuked the evil spirit and healed the boy. Then he gave him back to his father."
      }
    },
