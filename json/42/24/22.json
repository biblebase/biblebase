    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们当中有几位妇女一早到耶稣的坟墓，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "而且我們中間有幾個婦女使我們驚奇，她們清早到墳墓那裡去，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "还有，我们中间有些妇女也使我们惊讶：她们清早到了墓穴，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "再者，我們中間有幾個婦女使我們驚奇；她們清早到了墳墓那裏，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Moreover, some women of our company amazed us. They were at the tomb early in the morning,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But also some women among us amazed us. When they were at the tomb early in the morning,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In addition, some of our women amazed us. They went to the tomb early this morning"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Yes, and certain women of our company, who arrived at the tomb early, astonished us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then some women from our group of his followers were at his tomb early this morning, and they came back with an amazing report."
      }
