    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣见状，就说；“有钱人进上帝的国真难啊！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌見他這樣，就說：“富有的人要進　神的國，是多麼難哪！"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣看见他变得很忧愁，就说：“有钱财的人进入神的国，是多么困难！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌看見他，就說：「有錢財的人進神的國是何等的難哪！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Jesus, seeing that he had become sad, said, “How difficult it is for those who have wealth to enter the kingdom of God!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus looked at him and said, “How hard it is for those who are wealthy to enter the kingdom of God!"
      },
      "niv": {
        "version_id": "NIV",
