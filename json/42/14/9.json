    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主人会把他带到你面前，说，‘请你把首位让给他吧！’你就要满面羞愧地退到末位去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那請你又請他的人過來對你說：‘請你讓位給這個人。’那時你就慚愧地退居末位了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "那邀请你们的会来对你说：‘请让座给这一位！’那时候，你就要羞惭地去坐末位了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那請你們的人前來對你說：『讓座給這一位吧！』你就羞羞慚慚地退到末位上去了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "and he who invited you both will come and say to you, ‘Give your place to this person,’ and then you will begin with shame to take the lowest place."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and he who invited you both will come and say to you, ‘Give your place to this man,’ and then in disgrace you proceed to occupy the last place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If so, the host who invited both of you will come and say to you, ‘Give this person your seat.’ Then, humiliated, you will have to take the least important place."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and he who invited you and him come and say to you, ‘Give place to this man,’ and then you begin with shame to take the lowest place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The host will come and say, ‘Give this person your seat.’ Then you will be embarrassed, and you will have to take whatever seat is left at the foot of the table!"
      }
