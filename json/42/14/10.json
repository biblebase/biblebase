    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你去赴宴时，应该先坐在末位，这样主人会对你说，‘朋友，请上坐！’那时，你在宾客面前就有光彩了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你被邀請的時候，要坐在末位上，等請你的人過來對你說：‘朋友，請上座。’那時你在同席的人面前才有光彩。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“你被邀请的时候，要去坐在末位上，好让邀请你的人一旦来，就会对你说：‘朋友，请来上座。’那时候，你就在所有一同坐席的人面前有光彩了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你被請的時候，就去坐在末位上，好叫那請你的人來對你說：『朋友，請上座。』那時，你在同席的人面前就有光彩了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But when you are invited, go and sit in the lowest place, so that when your host comes he may say to you, ‘Friend, move up higher.’ Then you will be honored in the presence of all who sit at table with you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when you are invited, go and recline at the last place, so that when the one who has invited you comes, he may say to you, ‘Friend, move up higher’; then you will have honor in the sight of all who are at the table with you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But when you are invited, take the lowest place, so that when your host comes, he will say to you, ‘Friend, move up to a better place.’ Then you will be honored in the presence of all the other guests."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But when you are invited, go and sit down in the lowest place, so that when he who invited you comes he may say to you, ‘Friend, go up higher.’ Then you will have glory in the presence of those who sit at the table with you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Instead, take the lowest place at the foot of the table. Then when your host sees you, he will come and say, ‘Friend, we have a better place for you!’ Then you will be honored in front of all the other guests."
      }
