    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“同样，你们若不撇下一切，就不能做我的门徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，你們中間不論誰，如果不撇下一切所有的，就不能作我的門徒。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "同样，你们当中任何人，如果不向自己所拥有的一切告别，就不能做我的门徒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，你們無論甚麼人，若不撇下一切所有的，就不能作我的門徒。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So therefore, any one of you who does not renounce all that he has cannot be my disciple."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So then, none of you can be My disciple who does not give up all his own possessions."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the same way, those of you who do not give up everything you have cannot be my disciples."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So likewise, whoever of you does not forsake all that he has cannot be My disciple."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So you cannot become my disciple without giving up everything you own."
      }
