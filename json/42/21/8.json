    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣回答说：“你们要小心提防，不要被迷惑。因为将来会有许多人冒我的名来，说，‘我就是基督’，或说，‘时候到了’，你们切勿跟从他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“你們要小心，不要被人迷惑。因為有許多人要來，假冒我的名說：‘我就是基督’，又說：‘時候近了。’你們不要跟從這些人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣说：“你们要当心，免得被迷惑，因为许多人会以我的名而来，声称‘我就是那一位’，又说‘时候快到了。’你们不要跟从他们。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「你們要謹慎，不要受迷惑；因為將來有好些人冒我的名來，說：『我是基督』，又說：『時候近了』，你們不要跟從他們！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he said, “See that you are not led astray. For many will come in my name, saying, ‘I am he!’ and, ‘The time is at hand!’ Do not go after them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said, “See to it that you are not misled; for many will come in My name, saying, ‘I am He,’ and, ‘The time is near.’ Do not go after them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He replied: “Watch out that you are not deceived. For many will come in my name, claiming, ‘I am he,’ and, ‘The time is near.’ Do not follow them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And He said: “Take heed that you not be deceived. For many will come in My name, saying, ‘I am He,’ and, ‘The time has drawn near.’ Therefore do not go after them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He replied, “Don’t let anyone mislead you, for many will come in my name, claiming, ‘I am the Messiah,’ and saying, ‘The time has come!’ But don’t believe them."
      }
