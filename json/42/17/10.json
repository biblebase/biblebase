    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "同样，你们照着吩咐把事情办妥后，也该这样说，‘我们是无用的奴仆，所做的不过是分内的事。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們也是這樣，作完一切吩咐你們的事，應該說：‘我們是無用的僕人，我們只作了應分作的。’”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你们也是这样，当你们做完了所吩咐你们的一切事以后，应该说‘我们是无用的奴仆，只是做了自己该做的。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，你們做完了一切所吩咐的，只當說：『我們是無用的僕人，所做的本是我們應分做的。』」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So you also, when you have done all that you were commanded, say, ‘We are unworthy servants;  we have only done what was our duty.’”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So you too, when you do all the things which are commanded you, say, ‘We are unworthy slaves; we have done only that which we ought to have done.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So you also, when you have done everything you were told to do, should say, ‘We are unworthy servants; we have only done our duty.’ ”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So likewise you, when you have done all those things which you are commanded, say, ‘We are unprofitable servants. We have done what was our duty to do.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In the same way, when you obey me you should say, ‘We are unworthy servants who have simply done our duty.’”"
      }
    },
