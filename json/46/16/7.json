    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我不想只是现在顺道探望你们，主若许可，我盼望能够和你们同住一段时期。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我不願意只是順路見見你們，主若許可，我盼望和你們同住一個時期。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "实际上，我现在并不愿意在赶路的时候见你们；主如果准许，我希望在你们那里住一段时间。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我如今不願意路過見你們；主若許我，我就指望和你們同住幾時。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For I do not want to see you now just in passing. I hope to spend some time with you, if the Lord permits."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I do not wish to see you now just in passing; for I hope to remain with you for some time, if the Lord permits."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I do not want to see you now and make only a passing visit; I hope to spend some time with you, if the Lord permits."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For I do not wish to see you now on the way; but I hope to stay a while with you, if the Lord permits."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This time I don’t want to make just a short visit and then go right on. I want to come and stay awhile, if the Lord will let me."
      }
    },
