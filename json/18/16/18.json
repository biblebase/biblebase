    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.16.18-22",
            "paragraphs": [
              "18～22.約伯呼天喚地（參賽一2）──永不打盹地觀看人類行動的、與古代諸約的守護者──來為他被殺害作見證，以支持他的自訴。他使用血字，暗示以為在他的哀求矯正蒙垂聽之前就死去（22節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“大地啊！不要掩盖我的血， 不要拦阻我的呼求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "地啊，不要遮蓋我的血； 不要讓我的哀求有停留的地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "地啊，不要遮蓋我的血！ 不要阻擋我的哀求！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“O earth, cover not my blood, and let my cry find no resting place."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“O earth, do not cover my blood, And let there be no resting place for my cry."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Earth, do not cover my blood; may my cry never be laid to rest!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“O earth, do not cover my blood, And let my cry have no resting place!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O earth, do not conceal my blood. Let it cry out on my behalf."
      }
    },
