        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你必免受恶毒的毁谤， 灾难来临也不惧怕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你必不受人口舌之害， 災殃臨到，也不害怕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必被隱藏，不受口舌之害； 災殃臨到，你也不懼怕。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You shall be hidden from the lash of the tongue, and shall not fear destruction when it comes."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will be hidden from the scourge of the tongue, And you will not be afraid of violence when it comes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will be protected from the lash of the tongue, and need not fear when destruction comes."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You shall be hidden from the scourge of the tongue, And you shall not be afraid of destruction when it comes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will be safe from slander and have no fear when destruction comes."
      }
