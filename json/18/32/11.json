    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，我一直等候你们发言， 倾听你们的理论， 等你们斟词酌句。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們砌詞說話的時候， 我等待著你們的話， 留心聽你們的理論。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們查究所要說的話； 那時我等候你們的話， 側耳聽你們的辯論，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Behold, I waited for your words, I listened for your wise sayings, while you searched out what to say."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, I waited for your words, I listened to your reasonings, While you pondered what to say."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I waited while you spoke, I listened to your reasoning; while you were searching for words,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Indeed I waited for your words, I listened to your reasonings, while you searched out what to say."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I have waited all this time, listening very carefully to your arguments, listening to you grope for words."
      }
    },
