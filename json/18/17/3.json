    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.17.3-5",
            "paragraphs": [
              "一七3～5約伯求神親自拿憑據（擔保）保證他的無辜。\"擊掌\"。作擔保的一種方法。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，求你为我作保。 除你以外，谁肯做我的保人？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願你給我保證，親自為我作保； 除你以外有誰肯與我擊掌作保呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願主拿憑據給我，自己為我作保。 在你以外誰肯與我擊掌呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Lay down a pledge for me with you; who is there who will put up security for me?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Lay down, now, a pledge for me with Yourself; Who is there that will be my guarantor?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Give me, O God, the pledge you demand. Who else will put up security for me?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Now put down a pledge for me with Yourself. Who is he who  will shake hands with me?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must defend my innocence, O God, since no one else will stand up for me."
      }
    },
