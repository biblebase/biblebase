    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.12.1-6",
            "paragraphs": [
              "一二1～6耶利米發出古老的問題：為何惡人得以興旺呢？神以警告來回應他；神說他必受到耶路撒冷百姓（\"馬\"，5節）更大的反對，比他在亞拿突（\"步行的人\"，5節）所經歷的更甚。\"約但河邊的叢林\"。約但河邊有野獸居住的肥沃低地（比較四九19）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你栽培他们，让他们生根长大， 结出果实。 他们嘴上尊崇你， 心却远离你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你栽種了他們，他們也扎了根， 並且長大，結出果實； 他們的口中有你， 心裡卻沒有你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你栽培了他們， 他們也扎了根， 長大，而且結果。 他們的口是與你相近， 心卻與你遠離。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You plant them, and they take root; they grow and produce fruit; you are near in their mouth and far from their heart."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have planted them, they have also taken root; They grow, they have even produced fruit. You are near to their lips But far from their mind."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have planted them, and they have taken root; they grow and bear fruit. You are always on their lips but far from their hearts."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You have planted them, yes, they have taken root; They grow, yes, they bear fruit. You are near in their mouth But far from their mind."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have planted them, and they have taken root and prospered. Your name is on their lips, but you are far from their hearts."
      }
    },
