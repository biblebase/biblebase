    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.12.7-13",
            "paragraphs": [
              "一二7～13耶利米描述即將來到的毀壞，好象是已經發生了一樣。"
            ]
          },
          {
            "verse": "jer.12.12",
            "paragraphs": [
              "一二12\"耶和華的刀\"。巴比倫的軍隊，被神操縱使用來懲罰祂的子民。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "杀戮者已来到旷野中光秃的山岭， 耶和华使刀剑横扫全境， 无人幸免。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在曠野一切光禿的高岡上， 行毀滅的已經來到了； 因為耶和華的刀。 從地這邊直到地那邊，要吞滅一切； 人人都沒有平安。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "滅命的都來到曠野中一切淨光的高處； 耶和華的刀從地這邊直到地那邊盡行殺滅。 凡有血氣的都不得平安。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Upon all the bare heights in the desert destroyers have come, for the sword of the Lord devours from one end of the land to the other; no flesh has peace."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On all the bare heights in the wilderness Destroyers have come, For a sword of the Lord is devouring From one end of the land even to the other; There is no peace for anyone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Over all the barren heights in the desert destroyers will swarm, for the sword of the Lord will devour from one end of the land to the other; no one will be safe."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The plunderers have come On all the desolate heights in the wilderness, For the sword of the Lord shall devour From one end of the land to the other end of the land; No flesh shall have peace."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On all the bare hilltops, destroying armies can be seen. The sword of the Lord devours people from one end of the nation to the other. No one will escape!"
      }
    },
