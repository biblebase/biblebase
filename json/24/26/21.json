    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.26.20-23",
            "paragraphs": [
              "二六20～23約雅敬把怒氣發泄在另一位先知烏利亞身上，雖然他已逃往埃及去，但仍被引渡回國處死。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约雅敬王和他的王公大臣听见乌利亚的话，就想处死他。乌利亚得知后，吓得逃到埃及去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約雅敬王和他所有的勇士，以及猶大的眾領袖聽見了烏利亞的話，王就想要殺他。烏利亞聽見了，就懼怕，就逃往埃及去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約雅敬王和他眾勇士、眾首領聽見了烏利亞的話，王就想要把他治死。烏利亞聽見就懼怕，逃往埃及去了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And when King Jehoiakim, with all his warriors and all the officials, heard his words, the king sought to put him to death. But when Uriah heard of it, he was afraid and fled and escaped to Egypt."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When King Jehoiakim and all his mighty men and all the officials heard his words, then the king sought to put him to death; but Uriah heard it, and he was afraid and fled and went to Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When King Jehoiakim and all his officers and officials heard his words, the king was determined to put him to death. But Uriah heard of it and fled in fear to Egypt."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And when Jehoiakim the king, with all his mighty men and all the princes, heard his words, the king sought to put him to death; but when Urijah heard it, he was afraid and fled, and went to Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When King Jehoiakim and the army officers and officials heard what he was saying, the king sent someone to kill him. But Uriah heard about the plan and escaped in fear to Egypt."
      }
    },
