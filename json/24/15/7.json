    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要在这片土地上的各城门口用簸箕簸散我的子民， 使他们丧掉儿女； 我要灭绝他们， 因为他们不思悔改。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我在這地的城門口， 用簸箕簸了我的子民， 使他們喪掉兒女，使他們滅亡； 他們仍不轉離自己所行的路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我在境內各城門口， 用簸箕簸了我的百姓， 使他們喪掉兒女。 我毀滅他們， 他們仍不轉離所行的道。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I have winnowed them with a winnowing fork in the gates of the land; I have bereaved them; I have destroyed my people; they did not turn from their ways."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will winnow them with a winnowing fork At the gates of the land; I will bereave them of children, I will destroy My people; They did not repent of their ways."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will winnow them with a winnowing fork at the city gates of the land. I will bring bereavement and destruction on my people, for they have not changed their ways."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And I will winnow them with a winnowing fan in the gates of the land; I will bereave them of children; I will destroy My people, Since they do not return from their ways."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will winnow you like grain at the gates of your cities and take away the children you hold dear. I will destroy my own people, because they refuse to change their evil ways."
      }
    },
