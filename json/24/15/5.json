        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“耶路撒冷啊！誰會可憐你呢？ 誰會為你悲傷呢？ 誰會轉身向你問安呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷啊，誰可憐你呢？ 誰為你悲傷呢？ 誰轉身問你的安呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Who will have pity on you, O Jerusalem, or who will grieve for you? Who will turn aside to ask about your welfare?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Indeed, who will have pity on you, O Jerusalem, Or who will mourn for you, Or who will turn aside to ask about your welfare?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Who will have pity on you, Jerusalem? Who will mourn for you? Who will stop to ask how you are?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“For who will have pity on you, O Jerusalem? Or who will bemoan you? Or who will turn aside to ask how you are doing?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Who will feel sorry for you, Jerusalem? Who will weep for you? Who will even bother to ask how you are?"
      }
