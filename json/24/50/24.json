    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴比伦啊，我给你设下了陷阱， 你在懵然不知中被捉住， 因为你与耶和华为敌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“巴比倫哪！我設下網羅捉拿你， 你被捉住，仍不自覺； 你被發現，也被抓住， 因為你和耶和華爭鬥。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴比倫哪，我為你設下網羅， 你不知不覺被纏住。 你被尋着，也被捉住； 因為你與耶和華爭競。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I set a snare for you and you were taken, O Babylon, and you did not know it; you were found and caught, because you opposed the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I set a snare for you and you were also caught, O Babylon, While you yourself were not aware; You have been found and also seized Because you have engaged in conflict with the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I set a trap for you, Babylon, and you were caught before you knew it; you were found and captured because you opposed the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I have laid a snare for you; You have indeed been trapped, O Babylon, And you were not aware; You have been found and also caught, Because you have contended against the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen, Babylon, for I have set a trap for you. You are caught, for you have fought against the Lord."
      }
    },
