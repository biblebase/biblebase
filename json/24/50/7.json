    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "敌人遇见他们，就吞噬他们，还说，‘我们没有错！这是因为他们冒犯了他们真正的安歇之处——耶和华，他们祖先的盼望——耶和华。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有遇見他們的都吞吃他們； 他們的敵人說：‘我們沒有罪， 因為他們得罪了耶和華，那真正的居所； 耶和華是他們列祖的盼望。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡遇見他們的，就把他們吞滅。敵人說：『我們沒有罪；因他們得罪那作公義居所的耶和華，就是他們列祖所仰望的耶和華。』"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "All who found them have devoured them, and their enemies have said, ‘We are not guilty, for they have sinned against the Lord, their habitation of righteousness, the Lord, the hope of their fathers.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All who came upon them have devoured them; And their adversaries have said, ‘We are not guilty, Inasmuch as they have sinned against the Lord  who is the habitation of righteousness, Even the Lord, the hope of their fathers.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever found them devoured them; their enemies said, ‘We are not guilty, for they sinned against the Lord, their verdant pasture, the Lord, the hope of their ancestors.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "All who found them have devoured them; And their adversaries said, ‘We have not offended, Because they have sinned against the Lord, the habitation of justice, The Lord, the hope of their fathers.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All who found them devoured them. Their enemies said, ‘We did nothing wrong in attacking them, for they sinned against the Lord, their true place of rest, and the hope of their ancestors.’"
      }
    },
