    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弓箭手啊，你们要列阵围攻巴比伦， 向她猛射， 不要吝啬箭枝， 因为她得罪了耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有拉弓的啊！ 你們要在巴比倫四圍列陣； 射箭攻擊它，不要吝嗇箭枝， 因為它得罪了耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所有拉弓的，你們要在巴比倫的四圍擺陣， 射箭攻擊她。 不要愛惜箭枝， 因她得罪了耶和華。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Set yourselves in array against Babylon all around, all you who bend the bow; shoot at her, spare no arrows, for she has sinned against the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Draw up your battle lines against Babylon on every side, All you who bend the bow; Shoot at her, do not be sparing with your arrows, For she has sinned against the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Take up your positions around Babylon, all you who draw the bow. Shoot at her! Spare no arrows, for she has sinned against the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Put yourselves in array against Babylon all around, All you who bend the bow; Shoot at her, spare no arrows, For she has sinned against the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yes, prepare to attack Babylon, all you surrounding nations. Let your archers shoot at her; spare no arrows. For she has sinned against the Lord."
      }
    },
