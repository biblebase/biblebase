      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now the king was sitting in the winter house in the ninth month, with a  fire burning on the hearth before him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It was late autumn, and the king was in a winterized part of the palace, sitting in front of a fire to keep warm."
      }
