    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.49.9-10",
            "paragraphs": [
              "四九9,10通常\"摘葡萄的\"和\"盜賊\"都會有所保留，不會摘盡或盜盡，但以東將要全然被毀滅。在主前第三世紀，以東被拿巴提人占領了。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人们来以东摘葡萄，不会摘尽； 盗贼夜间来偷东西，不会偷光。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "收取葡萄的若來到你那裡， 豈不剩下一些葡萄嗎？ 盜賊若在夜間來到你那裡， 豈不是只偷他們所要的嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摘葡萄的若來到他那裏， 豈不剩下些葡萄呢？ 盜賊若夜間而來， 豈不毀壞直到夠了呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If grape gatherers came to you, would they not leave gleanings? If thieves came by night, would they not destroy only enough for themselves?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If grape gatherers came to you, Would they not leave gleanings? If thieves came by night, They would destroy only until they had enough."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If grape pickers came to you, would they not leave a few grapes? If thieves came during the night, would they not steal only as much as they wanted?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "If grape-gatherers came to you, Would they not leave some gleaning grapes? If thieves by night, Would they not destroy until they have enough?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who harvest grapes always leave a few for the poor. If thieves came at night, they would not take everything."
      }
    },
