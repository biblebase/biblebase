    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "城中的青年将倒毙街头， 那时所有的战士都要灭亡。 这是万军之耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "它的年輕人必仆倒在它的街道上， 到那日，所有的戰士都必被消滅。” 這是萬軍之耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她的少年人必仆倒在街上； 當那日，一切兵丁必默默無聲。 這是萬軍之耶和華說的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Therefore her young men shall fall in her squares, and all her soldiers shall be destroyed in that day, declares the Lord of hosts."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore, her young men will fall in her streets, And all the men of war will be silenced in that day,” declares the Lord of hosts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely, her young men will fall in the streets; all her soldiers will be silenced in that day,” declares the Lord Almighty."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore her young men shall fall in her streets, And all the men of war shall be cut off in that day,” says the Lord of hosts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Her young men will fall in the streets and die. Her soldiers will all be killed,” says the Lord of Heaven’s Armies."
      }
    },
