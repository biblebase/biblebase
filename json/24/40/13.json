        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now Johanan the son of Kareah and all the leaders of the forces in the open country came to Gedaliah at Mizpah"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Johanan the son of Kareah and all the commanders of the forces that were in the field came to Gedaliah at Mizpah"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Johanan son of Kareah and all the army officers still in the open country came to Gedaliah at Mizpah"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Moreover Johanan the son of Kareah and all the captains of the forces that were in the fields came to Gedaliah at Mizpah,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Soon after this, Johanan son of Kareah and the other military leaders came to Gedaliah at Mizpah."
      }
