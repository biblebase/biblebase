    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华已为我们申冤。 来吧！我们要在锡安述说我们上帝耶和华的作为。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華已為我們伸冤； 來吧！我們要在錫安宣揚耶和華我們　神的作為。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華已經彰顯我們的公義。 來吧！我們可以在錫安 報告耶和華－我們神的作為。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The Lord has brought about our vindication; come, let us declare in Zion the work of the Lord our God."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord has brought about our vindication; Come and let us recount in Zion The work of the Lord our God!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘The Lord has vindicated us; come, let us tell in Zion what the Lord our God has done.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The Lord has revealed our righteousness. Come and let us declare in Zion the work of the Lord our God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord has vindicated us. Come, let us announce in Jerusalem everything the Lord our God has done."
      }
    },
