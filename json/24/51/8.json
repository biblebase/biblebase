    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.51.7-8",
            "paragraphs": [
              "五一7,8\"金杯\"。巴比倫在外表是燦爛的，但它的\"杯\"充滿了偶像膜拜。啟示錄十四章8節、十七章1至6節也使用相同的比喻來形容末世的巴比倫。現在再沒有\"乳香\"（8節）能醫治巴比倫。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "转瞬之间，巴比伦必毁灭。 你们要为她哀悼， 拿药为她止痛， 或许可以治好她。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴比倫必突然傾覆毀滅， 你們要為它哀號！ 拿乳香來消除它的痛楚， 也許可以治好它。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴比倫忽然傾覆毀壞； 要為她哀號； 為止她的疼痛， 拿乳香或者可以治好。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Suddenly Babylon has fallen and been broken; wail for her! Take balm for her pain; perhaps she may be healed."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Suddenly Babylon has fallen and been broken; Wail over her! Bring balm for her pain; Perhaps she may be healed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Babylon will suddenly fall and be broken. Wail over her! Get balm for her pain; perhaps she can be healed."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Babylon has suddenly fallen and been destroyed. Wail for her! Take balm for her pain; Perhaps she may be healed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But suddenly Babylon, too, has fallen. Weep for her. Give her medicine. Perhaps she can yet be healed."
      }
    },
