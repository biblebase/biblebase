    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“看啊，时候将到，我必惩罚巴比伦的神像， 使巴比伦全国蒙羞， 尸横遍野。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，你看，日子快到， 我必懲罰巴比倫的偶像； 巴比倫全地都必蒙羞， 所有被刺殺的人都必倒在境內。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "日子將到，我必刑罰巴比倫雕刻的偶像。 她全地必然抱愧； 她被殺的人必在其中仆倒。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Therefore, behold, the days are coming when I will punish the images of Babylon; her whole land shall be put to shame, and all her slain shall fall in the midst of her."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore behold, days are coming When I will punish the idols of Babylon; And her whole land will be put to shame And all her slain will fall in her midst."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the time will surely come when I will punish the idols of Babylon; her whole land will be disgraced and her slain will all lie fallen within her."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore behold, the days are coming That I will bring judgment on the carved images of Babylon; Her whole land shall be ashamed, And all her slain shall fall in her midst."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the time is surely coming when I will punish this great city and all her idols. Her whole land will be disgraced, and her dead will lie in the streets."
      }
    },
