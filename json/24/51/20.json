    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “巴比伦啊！你是我的锤子， 我作战的兵器， 我要用你打碎列国， 毁灭列邦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：“你是我的鐵鎚， 作我的兵器； 我用你們打碎列國， 用你毀滅萬邦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你是我爭戰的斧子和打仗的兵器； 我要用你打碎列國， 用你毀滅列邦；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“You are my hammer and weapon of war: with you I break nations in pieces; with you I destroy kingdoms;"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He says, “You are My war-club, My weapon of war; And with you I shatter nations, And with you I destroy kingdoms."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You are my war club, my weapon for battle— with you I shatter nations, with you I destroy kingdoms,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“You  are My battle-ax and weapons of war: For with you I will break the nation in pieces; With you I will destroy kingdoms;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You are my battle-ax and sword,” says the Lord. “With you I will shatter nations and destroy many kingdoms."
      }
    },
