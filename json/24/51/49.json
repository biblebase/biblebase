    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴比伦必灭亡， 因为她大肆屠杀以色列人和其他各国的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“巴比倫必因以色列被刺殺的人而倒下， 就如全地被刺殺的人因巴比倫而倒下。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴比倫怎樣使以色列被殺的人仆倒， 照樣她全地被殺的人也必在巴比倫仆倒。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Babylon must fall for the slain of Israel, just as for Babylon have fallen the slain of all the earth."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Indeed Babylon is to fall for the slain of Israel, As also for Babylon the slain of all the earth have fallen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Babylon must fall because of Israel’s slain, just as the slain in all the earth have fallen because of Babylon."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "As Babylon has  caused the slain of Israel to fall, So at Babylon the slain of all the earth shall fall."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Just as Babylon killed the people of Israel and others throughout the world, so must her people be killed."
      }
    },
