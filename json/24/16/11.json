    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要对他们说，‘耶和华说，你们的祖先背弃我，去跟随、供奉、祭拜别的神明；他们背弃我，不遵守我的律法。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你就要回答他們：‘因為你們的列祖離棄了我，他們隨從別的神，事奉敬拜他們；他們離棄了我，不遵守我的律法。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你就對他們說：『耶和華說：因為你們列祖離棄我，隨從別神，事奉敬拜，不遵守我的律法，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "then you shall say to them: ‘Because your fathers have forsaken me, declares the Lord, and have gone after other gods and have served and worshiped them, and have forsaken me and have not kept my law,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then you are to say to them, ‘It is because your forefathers have forsaken Me,’ declares the Lord, ‘and have followed other gods and served them and bowed down to them; but Me they have forsaken and have not kept My law."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "then say to them, ‘It is because your ancestors forsook me,’ declares the Lord, ‘and followed other gods and served and worshiped them. They forsook me and did not keep my law."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "then you shall say to them, ‘Because your fathers have forsaken Me,’ says the Lord; ‘they have walked after other gods and have served them and worshiped them, and have forsaken Me and not kept My law."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then you will give them the Lord’s reply: ‘It is because your ancestors were unfaithful to me. They worshiped other gods and served them. They abandoned me and did not obey my word."
      }
    },
