    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.13.20-27",
            "paragraphs": [
              "一三20～27最後一個警告，指出猶大若繼續犯罪，懲罰（被擄到巴比倫）就是他們無可避免的後果。第23節指出要猶大改變她的行為，是多麽不可能的事。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "古实人能改变他们的肤色吗？ 豹子能脱去它们的斑点吗？ 你这作恶成性的人怎能行善呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "古實人怎能改變他的膚色？ 花豹怎能改變牠的斑點？ 你們這些慣作壞事的人， 又怎能行善呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "古實人豈能改變皮膚呢？ 豹豈能改變斑點呢？ 若能，你們這習慣行惡的便能行善了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Can the Ethiopian change his skin or the leopard his spots? Then also you can do good who are accustomed to do evil."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Can the Ethiopian change his skin Or the leopard his spots? Then you also can do good Who are accustomed to doing evil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Can an Ethiopian change his skin or a leopard its spots? Neither can you do good who are accustomed to doing evil."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Can the Ethiopian change his skin or the leopard its spots? Then may you also do good who are accustomed to do evil."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can an Ethiopian change the color of his skin? Can a leopard take away its spots? Neither can you start doing good, for you have always done evil."
      }
    },
