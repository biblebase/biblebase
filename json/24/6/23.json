    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们残酷无情，毫无怜悯， 手持弓箭长枪， 骑马奔来，声势如狂涛怒吼。 锡安城啊，他们准备列阵攻击你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們緊握弓和槍， 性情殘忍，毫無憐憫； 他們的聲音像海洋怒吼； 錫安的居民哪！ 他們騎著戰馬， 在戰場上如同一人，列陣攻擊你們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們拿弓和槍， 性情殘忍，不施憐憫； 他們的聲音像海浪匉訇。 錫安城啊， 他們騎馬都擺隊伍， 如上戰場的人要攻擊你。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They lay hold on bow and javelin; they are cruel and have no mercy; the sound of them is like the roaring sea; they ride on horses, set in array as a man for battle, against you, O daughter of Zion!”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They seize bow and spear; They are cruel and have no mercy; Their voice roars like the sea, And they ride on horses, Arrayed as a man for the battle Against you, O daughter of Zion!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are armed with bow and spear; they are cruel and show no mercy. They sound like the roaring sea as they ride on their horses; they come like men in battle formation to attack you, Daughter Zion.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They will lay hold on bow and spear; They are cruel and have no mercy; Their voice roars like the sea; And they ride on horses, As men of war set in array against you, O daughter of Zion.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They are armed with bows and spears. They are cruel and show no mercy. They sound like a roaring sea as they ride forward on horses. They are coming in battle formation, planning to destroy you, beautiful Jerusalem.”"
      }
