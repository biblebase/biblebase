    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华说： “你们要砍伐树木， 修筑高台围攻耶路撒冷， 因为这城充满压迫， 该受惩罚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為萬軍之耶和華這樣說： “砍下她的樹木， 築壘攻打耶路撒冷； 她是那該受懲罰的城， 其中盡是欺壓的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為萬軍之耶和華如此說： 你們要砍伐樹木， 築壘攻打耶路撒冷。 這就是那該罰的城， 其中盡是欺壓。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For thus says the Lord of hosts: “Cut down her trees; cast up a siege mound against Jerusalem. This is the city that must be punished; there is nothing but oppression within her."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus says the Lord of hosts, “Cut down her trees And cast up a siege against Jerusalem. This is the city to be punished, In whose midst there is only oppression."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord Almighty says: “Cut down the trees and build siege ramps against Jerusalem. This city must be punished; it is filled with oppression."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For thus has the Lord of hosts said: “Cut down trees, And build a mound against Jerusalem. This is the city to be punished. She is full of oppression in her midst."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord of Heaven’s Armies says: “Cut down the trees for battering rams. Build siege ramps against the walls of Jerusalem. This is the city to be punished, for she is wicked through and through."
      }
    },
