    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.11.3",
            "paragraphs": [
              "11:3",
              "不聽從……的人，必受咒詛：以色列早已與神立約，應允要順服他，以他為他們的主和神。神也應允要做他們的神，賜給他們迦南美地為業，並且要隨時保守他們(參“神與以色列人之約”一文)。但是，以色列人卻不斷悖逆神，轉去歸向別神；所以，背約的咒詛必將臨到他們(比較申28章)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要告诉他们，以色列的上帝耶和华说，‘不遵守这约的人必受咒诅。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要對他們說：‘耶和華以色列的　神這樣說：不聽從這約的話的，必受咒詛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對他們說，耶和華－以色列的神如此說：不聽從這約之話的人必受咒詛。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You shall say to them, Thus says the Lord, the God of Israel: Cursed be the man who does not hear the words of this covenant"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and say to them, ‘Thus says the Lord, the God of Israel, “Cursed is the man who does not heed the words of this covenant"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Tell them that this is what the Lord, the God of Israel, says: ‘Cursed is the one who does not obey the terms of this covenant—"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and say to them, ‘Thus says the Lord God of Israel: “Cursed is the man who does not obey the words of this covenant"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Say to them, ‘This is what the Lord, the God of Israel, says: Cursed is anyone who does not obey the terms of my covenant!"
      }
    },
