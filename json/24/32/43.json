    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米啊，你们说这地方落在了迦勒底人手中，变得一片荒凉，人兽绝迹。但将来这地方必再有人置买田产。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們說：“這地荒涼，沒有人煙，也沒有牲畜，是交在迦勒底人手中的。”但將來在這地上，必有人買賣田地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們說，這地是荒涼、無人民、無牲畜，是交付迦勒底人手之地。日後在這境內，必有人置買田地。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Fields shall be bought in this land of which you are saying, ‘It is a desolation, without man or beast; it is given into the hand of the Chaldeans.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Fields will be bought in this land of which you say, “It is a desolation, without man or beast; it is given into the hand of the Chaldeans.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Once more fields will be bought in this land of which you say, ‘It is a desolate waste, without people or animals, for it has been given into the hands of the Babylonians.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And fields will be bought in this land of which you say, “It  is desolate, without man or beast; it has been given into the hand of the Chaldeans.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fields will again be bought and sold in this land about which you now say, ‘It has been ravaged by the Babylonians, a desolate land where people and animals have all disappeared.’"
      }
    },
