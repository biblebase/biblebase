    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.25.2-7",
            "paragraphs": [
              "二五2～7二十三年以來，耶利米一直勸百姓從他們邪惡的道路轉回。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“然而，你们不听我的话，制造神像惹我发怒，自招惩罚。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但你們沒有聽從我，竟容你們手所作的惹我發怒，自招禍害。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "然而你們沒有聽從我，竟以手所做的惹我發怒，陷害自己。這是耶和華說的。』」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Yet you have not listened to me, declares the Lord, that you might provoke me to anger with the work of your hands to your own harm."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet you have not listened to Me,” declares the Lord, “in order that you might provoke Me to anger with the work of your hands to your own harm."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But you did not listen to me,” declares the Lord, “and you have aroused my anger with what your hands have made, and you have brought harm to yourselves.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Yet you have not listened to Me,” says the Lord, “that you might provoke Me to anger with the works of your hands to your own hurt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But you would not listen to me,” says the Lord. “You made me furious by worshiping idols you made with your own hands, bringing on yourselves all the disasters you now suffer."
      }
