    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.25.15",
            "paragraphs": [
              "25:15",
              "忿怒的酒：聖經時常以至醉的酒象征神的忿怒(參49:12；51:7；伯21:20；詩60:3；賽51:17,22；結23:31；啟14:8,10；16:19；18:6)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列的上帝耶和华对我说：“你从我手中接过这盛满我烈怒的酒杯，到我差遣你去的国家给他们喝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華以色列的　神對我這樣說：“你從我的手中拿這杯烈怒的酒，給列國喝，就是我差遣你去的列國。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－以色列的神對我如此說：「你從我手中接這杯忿怒的酒，使我所差遣你去的各國的民喝。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Thus the Lord, the God of Israel, said to me: “Take from my hand this cup of the wine of wrath, and make all the nations to whom I send you drink it."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus the Lord, the God of Israel, says to me, “Take this cup of the wine of wrath from My hand and cause all the nations to whom I send you to drink it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord, the God of Israel, said to me: “Take from my hand this cup filled with the wine of my wrath and make all the nations to whom I send you drink it."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For thus says the Lord God of Israel to me: “Take this wine cup of fury from My hand, and cause all the nations, to whom I send you, to drink it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord, the God of Israel, said to me: “Take from my hand this cup filled to the brim with my anger, and make all the nations to whom I send you drink from it."
      }
    },
