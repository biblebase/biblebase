    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的声音传到地极， 因为祂要指控列国，审判万民， 使恶人丧身刀下。 这是耶和华说的。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "吶喊的聲音要傳到地極， 因為耶和華要控告列國， 也要審判萬人； 至於惡人，他必把他們交給刀劍。’” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "必有響聲達到地極， 因為耶和華與列國相爭； 凡有血氣的，他必審問； 至於惡人，他必交給刀劍。 這是耶和華說的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The clamor will resound to the ends of the earth, for the Lord has an indictment against the nations; he is entering into judgment with all flesh, and the wicked he will put to the sword, declares the Lord.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A clamor has come to the end of the earth, Because the Lord has a controversy with the nations. He is entering into judgment with all flesh; As for the wicked, He has given them to the sword,’ declares the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The tumult will resound to the ends of the earth, for the Lord will bring charges against the nations; he will bring judgment on all mankind and put the wicked to the sword,’ ” declares the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "A noise will come to the ends of the earth— For the Lord has a controversy with the nations; He will plead His case with all flesh. He will give those who  are wicked to the sword,’ says the Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "His cry of judgment will reach the ends of the earth, for the Lord will bring his case against all the nations. He will judge all the people of the earth, slaughtering the wicked with the sword. I, the Lord, have spoken!’”"
      }
    },
