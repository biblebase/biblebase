    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“犹大人做了我视为邪恶的事，把可憎的偶像放在用来敬拜我的圣殿中，玷污了圣殿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“因為猶大人行了我看為惡的事，把他們可憎的偶像安放在稱為我名下的殿中，污瀆了這殿。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「猶大人行我眼中看為惡的事，將可憎之物設立在稱為我名下的殿中，污穢這殿。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“For the sons of Judah have done evil in my sight, declares the Lord. They have set their detestable things in the house that is called by my name, to defile it."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the sons of Judah have done that which is evil in My sight,” declares the Lord, “they have set their detestable things in the house which is called by My name, to defile it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘The people of Judah have done evil in my eyes, declares the Lord. They have set up their detestable idols in the house that bears my Name and have defiled it."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For the children of Judah have done evil in My sight,” says the Lord. “They have set their abominations in the house which is called by My name, to pollute it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The people of Judah have sinned before my very eyes,” says the Lord. “They have set up their abominable idols right in the Temple that bears my name, defiling it."
      }
    },
