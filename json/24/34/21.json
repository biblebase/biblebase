    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要把犹大王西底迦及其官员交在想杀他们的敌人手中，交在刚撤军的巴比伦王手中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我也必把猶大王西底家和眾領袖交在他們仇敵的手中，和那些尋索他們性命的人的手中，以及那些從你們面前暫時撤退的巴比倫王的軍隊的手中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且我必將猶大王西底家和他的首領交在他們仇敵和尋索其命的人，與那暫離你們而去巴比倫王軍隊的手中。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And Zedekiah king of Judah and his officials I will give into the hand of their enemies and into the hand of those who seek their lives, into the hand of the army of the king of Babylon which has withdrawn from you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Zedekiah king of Judah and his officials I will give into the hand of their enemies and into the hand of those who seek their life, and into the hand of the army of the king of Babylon which has gone away from you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will deliver Zedekiah king of Judah and his officials into the hands of their enemies who want to kill them, to the army of the king of Babylon, which has withdrawn from you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And I will give Zedekiah king of Judah and his princes into the hand of their enemies, into the hand of those who seek their life, and into the hand of the king of Babylon’s army which has gone back from you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I will hand over King Zedekiah of Judah and his officials to the army of the king of Babylon. And although they have left Jerusalem for a while,"
      }
