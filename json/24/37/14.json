    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米说：“不，我不是去投奔迦勒底人。”但伊利雅不信，把他押到众官长那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米說：“這是謊話！我並不是去投降迦勒底人。”但伊利雅不聽他，仍拿住他，把他帶到眾領袖那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶利米說：「你這是謊話，我並不是投降迦勒底人。」伊利雅不聽他的話，就拿住他，解到首領那裏。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And Jeremiah said, “It is a lie; I am not deserting to the Chaldeans.” But Irijah would not listen to him, and seized Jeremiah and brought him to the officials."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Jeremiah said, “A lie! I am not going over to the Chaldeans”; yet he would not listen to him. So Irijah arrested Jeremiah and brought him to the officials."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“That’s not true!” Jeremiah said. “I am not deserting to the Babylonians.” But Irijah would not listen to him; instead, he arrested Jeremiah and brought him to the officials."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Jeremiah said, “False! I am not defecting to the Chaldeans.” But he did not listen to him. So Irijah seized Jeremiah and brought him to the princes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“That’s not true!” Jeremiah protested. “I had no intention of doing any such thing.” But Irijah wouldn’t listen, and he took Jeremiah before the officials."
      }
    },
