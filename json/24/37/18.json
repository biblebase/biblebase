    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米又问他：“我在什么事上得罪了你、你的臣仆或人民，以致你们把我囚在监牢中？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米再對西底家王說：“我有甚麼得罪你，或你的臣僕，或這人民，你竟然把我送進監獄裡呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶利米又對西底家王說：「我在甚麼事上得罪你，或你的臣僕，或這百姓，你竟將我囚在監裏呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Jeremiah also said to King Zedekiah, “What wrong have I done to you or your servants or this people, that you have put me in prison?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover Jeremiah said to King Zedekiah, “In what way have I sinned against you, or against your servants, or against this people, that you have put me in prison?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jeremiah said to King Zedekiah, “What crime have I committed against you or your attendants or this people, that you have put me in prison?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Moreover Jeremiah said to King Zedekiah, “What offense have I committed against you, against your servants, or against this people, that you have put me in prison?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jeremiah asked the king, “What crime have I committed? What have I done against you, your attendants, or the people that I should be imprisoned like this?"
      }
    },
