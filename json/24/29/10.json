    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.29.10",
            "paragraphs": [
              "29:10",
              "七十年：參25:11註。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.29.1-23",
            "paragraphs": [
              "二九1～23\"信\"。寄給那些在主前597年被擄到巴比倫的三千零二十三個猶太人（比較五二28），勸勉他們盡可能過正常的生活（二九4～9），等候神在七十年之後的拯救（10～14節），並且不要理會假先知如\"亞哈\"和\"西底家\"的話（21節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“你们被掳到巴比伦七十年期满后，我就要眷顾你们，成就我充满恩典的应许，把你们带回这地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“耶和華這樣說：‘為巴比倫所定的七十年滿了以後，我必眷顧你們，向你們實行我滿有恩惠的諾言，把你們帶回這地方來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「耶和華如此說：為巴比倫所定的七十年滿了以後，我要眷顧你們，向你們成就我的恩言，使你們仍回此地。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“For thus says the Lord: When seventy years are completed for Babylon, I will visit you, and I will fulfill to you my promise and bring you back to this place."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“For thus says the Lord, ‘When seventy years have been completed for Babylon, I will visit you and fulfill My good word to you, to bring you back to this place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord says: “When seventy years are completed for Babylon, I will come to you and fulfill my good promise to bring you back to this place."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For thus says the Lord: After seventy years are completed at Babylon, I will visit you and perform My good word toward you, and cause you to return to this place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord says: “You will be in Babylon for seventy years. But then I will come and do for you all the good things I have promised, and I will bring you home again."
      }
    },
