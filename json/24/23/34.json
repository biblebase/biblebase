    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果有先知、祭司或百姓宣称有耶和华的启示，我必惩罚他和他全家。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“無論是先知、是祭司，或是人民，凡說‘這是耶和華的默示’的，我都要懲罰那人和他的家。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無論是先知，是祭司，是百姓，說『耶和華的默示』，我必刑罰那人和他的家。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And as for the prophet, priest, or one of the people who says, ‘The burden of the Lord,’ I will punish that man and his household."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then as for the prophet or the priest or the people who say, ‘The oracle of the Lord,’ I will bring punishment upon that man and his household."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If a prophet or a priest or anyone else claims, ‘This is a message from the Lord,’ I will punish them and their household."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“And as  for the prophet and the priest and the people who say, ‘The oracle of the Lord!’ I will even punish that man and his house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If any prophet, priest, or anyone else says, ‘I have a prophecy from the Lord,’ I will punish that person along with his entire family."
      }
    },
