    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.17.5-8",
            "paragraphs": [
              "一七5～8本段的背景是猶大間歇地尋求埃及的幫助，試圖抵抗巴比倫。\"杜松\"（6節）指檉柳，一種小喬木，有結實的外觀。有關第7、8節，比較詩篇第一篇。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "信靠耶和华、倚靠耶和华的人有福了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但倚靠耶和華， 以耶和華為他所信賴的， 這人是有福的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "倚靠耶和華、以耶和華為可靠的， 那人有福了！"
      },
      "esv": {
