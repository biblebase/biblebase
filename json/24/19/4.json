    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.19.4",
            "paragraphs": [
              "一九4\"無辜人的血\"。嬰孩獻祭（七31）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为他们背弃了我，玷污了这地方。他们向自己、自己的祖先及犹大君王都不认识的神明烧香，使这地方流满了无辜人的血。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們離棄了我，把這地方玷污了，在這地方向列神燒香。這些神是他們和他們的列祖，以及猶大列王所不認識的；他們又使這地方灑滿無辜人的血。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他們和他們列祖，並猶大君王離棄我，將這地方看為平常，在這裏向素不認識的別神燒香，又使這地方滿了無辜人的血，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Because the people have forsaken me and have profaned this place by making offerings in it to other gods whom neither they nor their fathers nor the kings of Judah have known; and because they have filled this place with the blood of innocents,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because they have forsaken Me and have made this an alien place and have burned sacrifices in it to other gods, that neither they nor their forefathers nor the kings of Judah had ever known, and because they have filled this place with the blood of the innocent"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For they have forsaken me and made this a place of foreign gods; they have burned incense in it to gods that neither they nor their ancestors nor the kings of Judah ever knew, and they have filled this place with the blood of the innocent."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Because they have forsaken Me and made this an alien place, because they have burned incense in it to other gods whom neither they, their fathers, nor the kings of Judah have known, and have filled this place with the blood of the innocents"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“‘For Israel has forsaken me and turned this valley into a place of wickedness. The people burn incense to foreign gods—idols never before acknowledged by this generation, by their ancestors, or by the kings of Judah. And they have filled this place with the blood of innocent children."
      }
    },
