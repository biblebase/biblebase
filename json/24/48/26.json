    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.48.26",
            "paragraphs": [
              "四八26摩押面對神的審判時，要因恐懼而\"沈醉\"。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们要灌醉她，因为她藐视耶和华。她必在自己的呕吐物中打滚，被人耻笑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要使摩押沉醉，因為它向耶和華妄自尊大；摩押必跌在自己所吐之物上面，也必成為被人譏笑的對象。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們要使摩押沉醉，因她向耶和華誇大。她要在自己所吐之中打滾，又要被人嗤笑。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Make him drunk, because he magnified himself against the Lord, so that Moab shall wallow in his vomit, and he too shall be held in derision."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Make him drunk, for he has become arrogant toward the Lord; so Moab will wallow in his vomit, and he also will become a laughingstock."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Make her drunk, for she has defied the Lord. Let Moab wallow in her vomit; let her be an object of ridicule."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Make him drunk, Because he exalted himself against the Lord. Moab shall wallow in his vomit, And he shall also be in derision."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Let him stagger and fall like a drunkard, for he has rebelled against the Lord. Moab will wallow in his own vomit, ridiculed by all."
      }
    },
