    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.48.29-39",
            "paragraphs": [
              "四八29～39一度驕傲的摩押將要成為被嘲笑的對象（比較賽一六6）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在摩押境内的屋顶和街头巷尾，哀声处处可闻，因为我已把摩押打得粉碎，好像打碎没人要的器皿。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩押境內一切房頂上和廣場上，處處都有哀哭的聲音；因為我擊碎了摩押，好像打碎沒有人要的器皿。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在摩押的各房頂上和街市上處處有人哀哭；因我打碎摩押，好像打碎無人喜悅的器皿。這是耶和華說的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "On all the housetops of Moab and in the squares there is nothing but lamentation, for I have broken Moab like a vessel for which no one cares, declares the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On all the housetops of Moab and in its streets there is lamentation everywhere; for I have broken Moab like an undesirable vessel,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On all the roofs in Moab and in the public squares there is nothing but mourning, for I have broken Moab like a jar that no one wants,” declares the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "A general lamentation On all the housetops of Moab, And in its streets; For I have broken Moab like a vessel in which is no pleasure,” says the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There is crying and sorrow in every Moabite home and on every street. For I have smashed Moab like an old, unwanted jar."
      }
    },
