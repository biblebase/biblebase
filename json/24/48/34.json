    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“哀哭声从希实本传到以利亚利和雅杂，又从琐珥传到何罗念和伊基拉·施利施亚，因为连宁林的水也干涸了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“希實本的哀叫傳到以利亞利，他們發出的聲音傳到雅雜，從瑣珥達到何羅念和伊基拉．施利施亞；因為連寧林的河水也都乾涸了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「希實本人發的哀聲達到以利亞利，直達到雅雜；從瑣珥達到何羅念，直到伊基拉‧施利施亞，因為寧林的水必然乾涸。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“From the outcry at Heshbon even to Elealeh, as far as Jahaz they utter their voice, from Zoar to Horonaim and Eglath-shelishiyah. For the waters of Nimrim also have become desolate."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the outcry at Heshbon even to Elealeh, even to Jahaz they have raised their voice, from Zoar even to Horonaim and to Eglath-shelishiyah; for even the waters of Nimrim will become desolate."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The sound of their cry rises from Heshbon to Elealeh and Jahaz, from Zoar as far as Horonaim and Eglath Shelishiyah, for even the waters of Nimrim are dried up."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“From the cry of Heshbon to Elealeh and to Jahaz They have uttered their voice, From Zoar to Horonaim, Like a three-year-old heifer; For the waters of Nimrim also shall be desolate."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Instead, their awful cries of terror can be heard from Heshbon clear across to Elealeh and Jahaz; from Zoar all the way to Horonaim and Eglath-shelishiyah. Even the waters of Nimrim are dried up now."
      }
    },
