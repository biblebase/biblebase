    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿那告诉我父亲喜得贵子、 使他欢喜的人受咒诅！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那向我父親報喜信： “你得了一個男孩”，叫我父親十分高興的， 願那人受咒詛！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "給我父親報信說「你得了兒子」， 使我父親甚歡喜的， 願那人受咒詛。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Cursed be the man who brought the news to my father, “A son is born to you,” making him very glad."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Cursed be the man who brought the news To my father, saying, “A baby boy has been born to you!” And made him very happy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Cursed be the man who brought my father the news, who made him very glad, saying, “A child is born to you—a son!”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Let the man be cursed Who brought news to my father, saying, “A male child has been born to you!” Making him very glad."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I curse the messenger who told my father, “Good news—you have a son!”"
      }
    },
