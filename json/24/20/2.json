    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.20.2",
            "paragraphs": [
              "20:2",
              "2~3就打先知耶利米：耶利米很可能被打了40下(參申25:2~3)，並且被帶枷囚禁起來。但是，先知耶利米仍然拒絕因肉體所受的痛苦而妥協，他堅定不移地宣告民眾即將滅亡的惡運。他給巴施戶珥起了一個新名：稱他為“瑪歌珥米撒畢”，意思是“四面驚嚇”。巴施戶珥和整個民族很快將從四面受到驚嚇。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "便下令殴打耶利米先知，把他囚禁在耶和华殿的便雅悯上门处。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就毆打耶利米先知，把他枷鎖在耶和華殿裡，朝向便雅憫的上門內。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他就打先知耶利米，用耶和華殿裏便雅憫高門內的枷，將他枷在那裏。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Pashhur beat Jeremiah the prophet, and put him in the stocks that were in the upper Benjamin Gate of the house of the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Pashhur had Jeremiah the prophet beaten and put him in the stocks that were at the upper Benjamin Gate, which was by the house of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "he had Jeremiah the prophet beaten and put in the stocks at the Upper Gate of Benjamin at the Lord’s temple."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Pashhur struck Jeremiah the prophet, and put him in the stocks that were in the high gate of Benjamin, which was by the house of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So he arrested Jeremiah the prophet and had him whipped and put in stocks at the Benjamin Gate of the Lord’s Temple."
      }
    },
