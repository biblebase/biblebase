    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为他没有把我杀死在母腹中， 没有使母腹成为我的坟墓， 让我永远留在里面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他沒有使我死於母腹， 好使我的母親成了我的墳墓， 我永遠留在母腹中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他在我未出胎的時候不殺我， 使我母親成了我的墳墓， 胎就時常重大。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "because he did not kill me in the womb; so my mother would have been my grave, and her womb forever great."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because he did not kill me before birth, So that my mother would have been my grave, And her womb ever pregnant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For he did not kill me in the womb, with my mother as my grave, her womb enlarged forever."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Because he did not kill me from the womb, That my mother might have been my grave, And her womb always enlarged with  me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "because he did not kill me at birth. Oh, that I had died in my mother’s womb, that her body had been my grave!"
      }
    },
