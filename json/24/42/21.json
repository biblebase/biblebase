    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.42.7-22",
            "paragraphs": [
              "四二7～22耶利米的警告是最清楚不過的：留在巴勒斯坦信靠那保護你們的神；或前往埃及然後死在那裏。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "今日，我把你们上帝耶和华的话告诉了你们，你们却不听从。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我今天告訴了你們，你們卻不聽從耶和華你們的　神派我來告訴你們的一切話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我今日將這話告訴你們，耶和華－你們的神為你們的事差遣我到你們那裏說的，你們卻一樣沒有聽從。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And I have this day declared it to you, but you have not obeyed the voice of the Lord your God in anything that he sent me to tell you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I have told you today, but you have not obeyed the Lord your God, even in whatever He has sent me to tell you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have told you today, but you still have not obeyed the Lord your God in all he sent me to tell you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And I have this day declared it to you, but you have not obeyed the voice of the Lord your God, or anything which He has sent you by me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And today I have told you exactly what he said, but you will not obey the Lord your God any better now than you have in the past."
      }
    },
