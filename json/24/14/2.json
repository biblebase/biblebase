    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.14.1-6",
            "paragraphs": [
              "一四1～6一次影響城中居民（2,3節）、農夫（4節），甚至動物（5,6節）的\"幹旱\"（旱災），是從神而來的警告，但卻沒有人註意。\"家僮\"。奴仆。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“犹大充满悲泣， 她的城邑衰败， 人人坐在地上痛哭， 耶路撒冷哀鸣四起。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“猶大悲哀， 她的城鎮衰落， 人人都坐在地上悲傷痛哭； 耶路撒冷的哀聲上達於天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大悲哀，城門衰敗。 眾人披上黑衣坐在地上； 耶路撒冷的哀聲上達。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Judah mourns, and her gates languish; her people lament on the ground, and the cry of Jerusalem goes up."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Judah mourns And her gates languish; They sit on the ground in mourning, And the cry of Jerusalem has ascended."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Judah mourns, her cities languish; they wail for the land, and a cry goes up from Jerusalem."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Judah mourns, And her gates languish; They mourn for the land, And the cry of Jerusalem has gone up."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Judah wilts; commerce at the city gates grinds to a halt. All the people sit on the ground in mourning, and a great cry rises from Jerusalem."
      }
    },
