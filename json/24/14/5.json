    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.14.1-6",
            "paragraphs": [
              "一四1～6一次影響城中居民（2,3節）、農夫（4節），甚至動物（5,6節）的\"幹旱\"（旱災），是從神而來的警告，但卻沒有人註意。\"家僮\"。奴仆。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为没有草， 田野的母鹿抛弃了刚生下来的小鹿；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "甚至母鹿在田野生產了小鹿，也得把牠丟棄， 因為沒有青草。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "田野的母鹿生下小鹿，就撇棄， 因為無草。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Even the doe in the field forsakes her newborn fawn because there is no grass."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For even the doe in the field has given birth only to abandon her young, Because there is no grass."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even the doe in the field deserts her newborn fawn because there is no grass."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Yes, the deer also gave birth in the field, But left because there was no grass."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even the doe abandons her newborn fawn because there is no grass in the field."
      }
    },
