    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.22.24-30",
            "paragraphs": [
              "二二24～30這段描述約雅敬之子約雅斤的結局（這裏和在第三十七章1節稱為\"哥尼雅\"；也稱為\"耶哥尼雅\"。二四1；二七20）。他即位三個月之後，便被帶往巴比倫，後來再也沒有返國（王下二四8～15；二五27～30），盡管哈拿尼雅說了相反的假預言（耶二八4；比較五二31～34）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道哥尼雅是个被人鄙视、无人理睬的破瓶子吗？ 为什么他和他的子孙被赶到陌生之地？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "哥尼雅這個人是被鄙視、 被摔碎的瓶子， 是沒有人喜悅的器皿嗎？ 他和他的後裔為甚麼被驅趕， 被拋棄到他們不認識的地去呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "哥尼雅這人是被輕看、破壞的器皿嗎？ 是無人喜愛的器皿嗎？ 他和他的後裔為何被趕到不認識之地呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Is this man Coniah a despised, broken pot, a vessel no one cares for? Why are he and his children hurled and cast into a land that they do not know?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Is this man Coniah a despised, shattered jar? Or is he an undesirable vessel? Why have he and his descendants been hurled out And cast into a land that they had not known?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Is this man Jehoiachin a despised, broken pot, an object no one wants? Why will he and his children be hurled out, cast into a land they do not know?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Is this man Coniah a despised, broken idol— A vessel in which is no pleasure? Why are they cast out, he and his descendants, And cast into a land which they do not know?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why is this man Jehoiachin like a discarded, broken jar? Why are he and his children to be exiled to a foreign land?"
      }
    },
