    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“各国的人经过这城时都会彼此议论说，‘为什么耶和华这样对待这座大城？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "多國的人必經過這城，彼此發問：‘耶和華為甚麼這樣對待這大城呢？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「許多國的民要經過這城，各人對鄰舍說：『耶和華為何向這大城如此行呢？』"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“‘And many nations will pass by this city, and every man will say to his neighbor, “Why has the Lord dealt thus with this great city?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Many nations will pass by this city; and they will say to one another, ‘Why has the Lord done thus to this great city?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“People from many nations will pass by this city and will ask one another, ‘Why has the Lord done such a thing to this great city?’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And many nations will pass by this city; and everyone will say to his neighbor, ‘Why has the Lord done so to this great city?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“People from many nations will pass by the ruins of this city and say to one another, ‘Why did the Lord destroy such a great city?’"
      }
    },
