    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘你曾说，我真悲惨！因为耶和华使我痛上加痛，呻吟不止，身心疲惫，不得安宁。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘你曾說：我有禍了！因為耶和華在我的痛苦上加添憂愁。我因唉哼而困乏，得不著安寧。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴錄曾說：『哀哉！耶和華將憂愁加在我的痛苦上，我因唉哼而困乏，不得安歇。』"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You said, ‘Woe is me! For the Lord has added sorrow to my pain. I am weary with my groaning, and I find no rest.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘You said, “Ah, woe is me! For the Lord has added sorrow to my pain; I am weary with my groaning and have found no rest.” ’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You said, ‘Woe to me! The Lord has added sorrow to my pain; I am worn out with groaning and find no rest.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "‘You said, “Woe is me now! For the Lord has added grief to my sorrow. I fainted in my sighing, and I find no rest.” ’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have said, ‘I am overwhelmed with trouble! Haven’t I had enough pain already? And now the Lord has added more! I am worn out from sighing and can find no rest.’"
      }
    },
