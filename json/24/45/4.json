    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要告诉你，耶和华说，‘我必在整个大地上拆毁我所建立的，铲除我所栽培的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對耶利米說：“你要這樣對他說：‘耶和華這樣說：看哪！我所建立的，我必拆毀；我所栽種的，我必拔除。我必這樣對待全地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要這樣告訴他，耶和華如此說：我所建立的，我必拆毀；我所栽植的，我必拔出；在全地我都如此行。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Thus shall you say to him, Thus says the Lord: Behold, what I have built I am breaking down, and what I have planted I am plucking up—that is, the whole land."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus you are to say to him, ‘Thus says the Lord, “Behold, what I have built I am about to tear down, and what I have planted I am about to uproot, that is, the whole land.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the Lord has told me to say to you, ‘This is what the Lord says: I will overthrow what I have built and uproot what I have planted, throughout the earth."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Thus you shall say to him, ‘Thus says the Lord: “Behold, what I have built I will break down, and what I have planted I will pluck up, that is, this whole land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Baruch, this is what the Lord says: ‘I will destroy this nation that I built. I will uproot what I planted."
      }
    },
