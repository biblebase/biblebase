    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "是尼利亚的儿子巴录怂恿你反对我们，好叫我们落在迦勒底人手中，任由他们宰杀或掳到巴比伦。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只是尼利亞的兒子巴錄慫恿你來反對我們，為要把我們交在迦勒底人的手中，好使我們被殺害，或被擄到巴比倫去。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這是尼利亞的兒子巴錄挑唆你害我們，要將我們交在迦勒底人的手中，使我們有被殺的，有被擄到巴比倫去的。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "but Baruch the son of Neriah has set you against us, to deliver us into the hand of the Chaldeans, that they may kill us or take us into exile in Babylon.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but Baruch the son of Neriah is inciting you against us to give us over into the hand of the Chaldeans, so they will put us to death or exile us to Babylon.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Baruch son of Neriah is inciting you against us to hand us over to the Babylonians, so they may kill us or carry us into exile to Babylon.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But Baruch the son of Neriah has set you against us, to deliver us into the hand of the Chaldeans, that they may put us to death or carry us away captive to Babylon.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Baruch son of Neriah has convinced you to say this, because he wants us to stay here and be killed by the Babylonians or be carried off into exile.”"
      }
    },
