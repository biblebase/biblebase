    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.43.1-6",
            "paragraphs": [
              "四三1～6雖然眾人答應會遵從神透過耶利米所說的話（四二5），但他們背信而前往埃及去了。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，加利亚的儿子约哈难和众将领及民众都不听从耶和华让他们留在犹大的命令。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是加利亞的兒子約哈難和眾將領，以及眾民，都不聽從耶和華要他們留在猶大地的命令。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是加利亞的兒子約哈難和一切軍長，並眾百姓，不聽從耶和華的話住在猶大地。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So Johanan the son of Kareah and all the commanders of the forces and all the people did not obey the voice of the Lord, to remain in the land of Judah."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Johanan the son of Kareah and all the commanders of the forces, and all the people, did not obey the voice of the Lord to stay in the land of Judah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Johanan son of Kareah and all the army officers and all the people disobeyed the Lord’s command to stay in the land of Judah."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So Johanan the son of Kareah, all the captains of the forces, and all the people would not obey the voice of the Lord, to remain in the land of Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Johanan and the other military leaders and all the people refused to obey the Lord’s command to stay in Judah."
      }
    },
