    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为从锡安传出哀鸣， ‘我们灭亡了！ 我们受了奇耻大辱！ 我们的家园被毁， 我们只好背井离乡。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為從錫安可以聽到哀聲，說： ‘我們何等衰敗！ 我們多麼羞恥！ 因我們丟棄了這地， 我們的家園被拆毀了。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為聽見哀聲出於錫安，說： 我們怎樣敗落了！ 我們大大地慚愧！ 我們撇下地土； 人也拆毀了我們的房屋。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For a sound of wailing is heard from Zion: ‘How we are ruined! We are utterly shamed, because we have left the land, because they have cast down our dwellings.’”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For a voice of wailing is heard from Zion, ‘How are we ruined! We are put to great shame, For we have left the land, Because they have cast down our dwellings.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The sound of wailing is heard from Zion: ‘How ruined we are! How great is our shame! We must leave our land because our houses are in ruins.’ ”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For a voice of wailing is heard from Zion: ‘How we are plundered! We are greatly ashamed, Because we have forsaken the land, Because we have been cast out of our dwellings.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Hear the people of Jerusalem crying in despair, ‘We are ruined! We are completely humiliated! We must leave our land, because our homes have been torn down.’”"
      }
    },
