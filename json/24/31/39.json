    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.31.39",
            "paragraphs": [
              "三一39\"迦立山……歌亞\"。位置不能確定，但本節似乎顯示耶路撒冷的重建將要向西面伸展。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "城界要向外延伸到迦立山，再转到歌亚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "量度的準繩還要往外量，直到迦立山，繞到歌亞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "準繩要往外量出，直到迦立山，又轉到歌亞。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the measuring line shall go out farther, straight to the hill Gareb, and shall then turn to Goah."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The measuring line will go out farther straight ahead to the hill Gareb; then it will turn to Goah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The measuring line will stretch from there straight to the hill of Gareb and then turn to Goah."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The surveyor’s line shall again extend straight forward over the hill Gareb; then it shall turn toward Goath."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A measuring line will be stretched out over the hill of Gareb and across to Goah."
      }
    },
