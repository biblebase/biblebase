    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们曾经离开你， 但我们后悔了； 我们醒悟之后捶胸顿足， 痛悔不已， 因早年的所作所为而羞愧难当。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我回轉以後，就深深悔悟； 我醒覺以後，就拍腿悔恨。 因為我承受年幼時的恥辱， 我感到羞恥和慚愧。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我回轉以後就真正懊悔； 受教以後就拍腿歎息； 我因擔當幼年的凌辱就抱愧蒙羞。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For after I had turned away, I relented, and after I was instructed, I struck my thigh; I was ashamed, and I was confounded, because I bore the disgrace of my youth.’"
