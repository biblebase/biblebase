    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.38.14-23",
            "paragraphs": [
              "三八14～23西底家再次秘密會見耶利米，求耶利米把一切事情告訴他。耶利米不答應，因為他這樣做，西底家必定把他處死，而且也不會接受他的信息。西底家答應不把他處死，而耶利米只是重復他已說過多次的話--向巴比倫投降。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米说：“我若从实禀告，你一定会杀我。我就是给你建议，你也不会听。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米對西底家說：“我若告訴你，你不是要殺死我嗎？我若向你建議，你必不聽從我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶利米對西底家說：「我若告訴你，你豈不定要殺我嗎？我若勸戒你，你必不聽從我。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Jeremiah said to Zedekiah, “If I tell you, will you not surely put me to death? And if I give you counsel, you will not listen to me.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jeremiah said to Zedekiah, “If I tell you, will you not certainly put me to death? Besides, if I give you advice, you will not listen to me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jeremiah said to Zedekiah, “If I give you an answer, will you not kill me? Even if I did give you counsel, you would not listen to me.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Jeremiah said to Zedekiah, “If I declare it to you, will you not surely put me to death? And if I give you advice, you will not listen to me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jeremiah said, “If I tell you the truth, you will kill me. And if I give you advice, you won’t listen to me anyway.”"
      }
    },
