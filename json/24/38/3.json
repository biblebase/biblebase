    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说，‘这城必落在巴比伦王的军队手中，他们必攻占这城。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華這樣說：‘這城必交在巴比倫王軍隊的手中，巴比倫王必攻取這城。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華如此說：這城必要交在巴比倫王軍隊的手中，他必攻取這城。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Thus says the Lord: This city shall surely be given into the hand of the army of the king of Babylon and be taken.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord, ‘This city will certainly be given into the hand of the army of the king of Babylon and he will capture it.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And this is what the Lord says: ‘This city will certainly be given into the hands of the army of the king of Babylon, who will capture it.’ ”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Thus says the Lord: ‘This city shall surely be given into the hand of the king of Babylon’s army, which shall take it.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord also says: ‘The city of Jerusalem will certainly be handed over to the army of the king of Babylon, who will capture it.’”"
      }
    },
