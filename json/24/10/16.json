    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但雅各的上帝截然不同， 祂是万物的创造者， 被称为“万军之耶和华”， 以色列是祂的子民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那作雅各產業的並不像這些偶像， 因為他是萬物的創造者； 以色列是屬他的民族， 萬軍之耶和華是他的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雅各的分不像這些， 因他是造作萬有的主。 以色列也是他產業的支派， 萬軍之耶和華是他的名。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Not like these is he who is the portion of Jacob, for he is the one who formed all things, and Israel is the tribe of his inheritance; the Lord of hosts is his name."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The portion of Jacob is not like these; For the Maker of all is He, And Israel is the tribe of His inheritance; The Lord of hosts is His name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He who is the Portion of Jacob is not like these, for he is the Maker of all things, including Israel, the people of his inheritance— the Lord Almighty is his name."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The Portion of Jacob is not like them, For He is the Maker of all things, And Israel is the tribe of His inheritance; The Lord of hosts is His name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the God of Israel is no idol! He is the Creator of everything that exists, including Israel, his own special possession. The Lord of Heaven’s Armies is his name!"
      }
    },
