    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "nam.3.1-19",
            "paragraphs": [
              "Ⅶ禍哉尼尼微（三1～19）",
              "尼尼微的敗落從幾個不同時間的觀點來描繪∶前瞻敵對的局面（5～12節）、圍城與戰爭的預備（14～17節）、實際戰爭的預示（2～4節）、挫敗的回顧（13、18～19節）。"
            ]
          },
          {
            "verse": "nam.3.2-3",
            "paragraphs": [
              "Ｂ戰鬥與死亡（三2～3）",
              "用攻擊者行動的景象與響聲（2節），以及戰爭的余殃──無數的屍首（3節），重述二章3～10節所描繪之生動的戰爭情景。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "骑士冲锋，刀光闪闪，矛枪生辉； 被杀者众多，尸骨成堆， 人们被遍地的尸体绊倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "騎兵騰躍衝鋒； 刀劍爍爍發亮， 槍矛閃閃生光； 被殺的人眾多， 死屍成堆； 屍體無數， 眾人都被屍體絆倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "馬兵爭先，刀劍發光， 槍矛閃爍，被殺的甚多， 屍首成了大堆， 屍骸無數，人碰着而跌倒，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Horsemen charging, flashing sword and glittering spear, hosts of slain, heaps of corpses, dead bodies without end— they stumble over the bodies!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Horsemen charging, Swords flashing, spears gleaming, Many slain, a mass of corpses, And countless dead bodies— They stumble over the dead bodies!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Charging cavalry, flashing swords and glittering spears! Many casualties, piles of dead, bodies without number, people stumbling over the corpses—"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Horsemen charge with bright sword and glittering spear. There  is a multitude of slain, A great number of bodies, Countless corpses— They stumble over the corpses—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "See the flashing swords and glittering spears as the charioteers charge past! There are countless casualties, heaps of bodies— so many bodies that people stumble over them."
      }
    },
