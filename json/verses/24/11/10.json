    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们重蹈祖先的覆辙，不听从我的话，跟从、祭拜别的神明。以色列和犹大都违背了我与他们祖先立的约。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們回到他們祖先所犯的罪孽中。他們的祖先不肯聽從我的話，卻隨從事奉別的神。以色列家和猶大家都違犯了我和他們列祖所立的約。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們轉去效法他們的先祖，不肯聽我的話，犯罪作孽，又隨從別神，事奉它。以色列家和猶大家背了我與他們列祖所立的約。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have turned back to the iniquities of their ancestors who refused to hear My words, and they have gone after other gods to serve them; the house of Israel and the house of Judah have broken My covenant which I made with their fathers.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They have returned to the sins of their ancestors, who refused to listen to my words. They have followed other gods to serve them. Both Israel and Judah have broken the covenant I made with their ancestors."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They have returned to the sins of their ancestors. They have refused to listen to me and are worshiping other gods. Israel and Judah have both broken the covenant I made with their ancestors."
      }
    },
