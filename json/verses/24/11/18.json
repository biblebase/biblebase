    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.11.18-23",
            "paragraphs": [
              "一一18～23這裏描述耶利米在家鄉面對百姓的敵意。他們想把他（\"樹\"）殺了，使他的信息（\"果子\"，19節）安靜下來。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人想谋害我，耶和华告诉了我，并让我看见他们的所作所为。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華把他們的陰謀指示我，我就知道了；那時，你把他們所行的給我指明。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華指示我，我就知道； 你將他們所行的給我指明。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, the Lord made it known to me and I knew it; Then You showed me their deeds."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because the Lord revealed their plot to me, I knew it, for at that time he showed me what they were doing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord told me about the plots my enemies were making against me."
      }
