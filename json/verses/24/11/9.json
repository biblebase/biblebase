    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对我说：“犹大人和耶路撒冷的居民阴谋背叛我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華又對我說：“在猶大人和耶路撒冷的居民中間，有背叛的陰謀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對我說：「在猶大人和耶路撒冷居民中有同謀背叛的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord said to me, “A conspiracy has been found among the men of Judah and among the inhabitants of Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the Lord said to me, “There is a conspiracy among the people of Judah and those who live in Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Again the Lord spoke to me and said, “I have discovered a conspiracy against me among the people of Judah and Jerusalem."
      }
