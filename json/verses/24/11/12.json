    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时，犹大各城和耶路撒冷的居民必去呼求他们烧香供奉的神明，却得不到任何帮助。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，猶大各城的人和耶路撒冷的居民，都必去哀求他們燒香供奉的神，只是在他們遭難的時候，這些神卻完全不能拯救他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，猶大城邑的人和耶路撒冷的居民要去哀求他們燒香所供奉的神；只是遭難的時候，這些神毫不拯救他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the cities of Judah and the inhabitants of Jerusalem will go and cry to the gods to whom they burn incense, but they surely will not save them in the time of their disaster."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The towns of Judah and the people of Jerusalem will go and cry out to the gods to whom they burn incense, but they will not help them at all when disaster strikes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the people of Judah and Jerusalem will pray to their idols and burn incense before them. But the idols will not save them when disaster strikes!"
      }
    },
