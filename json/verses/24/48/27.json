    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押啊，你不是曾经嘲笑以色列吗？以色列难道是强盗吗？你每次谈到她总是不屑地摇头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩押啊，以色列不是曾經成為你譏笑的對象嗎？它豈是被捕獲的強盜，以致你每逢提到它，總是搖頭嘲笑。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩押啊，你不曾嗤笑以色列嗎？她豈是在賊中查出來的呢？你每逢提到她便搖頭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now was not Israel a laughingstock to you? Or was he caught among thieves? For each time you speak about him you shake your head in scorn."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Was not Israel the object of your ridicule? Was she caught among thieves, that you shake your head in scorn whenever you speak of her?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Did you not ridicule the people of Israel? Were they caught in the company of thieves that you should despise them as you do?"
      }
    },
