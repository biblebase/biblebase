    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚罗珥的居民啊！ 你们要站在路旁观望， 询问逃难的男女究竟发生了什么事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞羅珥的居民哪！ 要站在路旁觀望， 詢問那些正在逃跑的男女，說： ‘發生了甚麼事？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住亞羅珥的啊， 要站在道旁觀望， 問逃避的男人和逃脫的女人說： 是甚麼事呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Stand by the road and keep watch, O inhabitant of Aroer; Ask him who flees and her who escapes And say, ‘What has happened?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Stand by the road and watch, you who live in Aroer. Ask the man fleeing and the woman escaping, ask them, ‘What has happened?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You people of Aroer, stand beside the road and watch. Shout to those who flee from Moab, ‘What has happened there?’"
      }
    },
