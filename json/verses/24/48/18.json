    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“底本的居民啊， 你们要从荣耀的位子上下来， 坐在干旱的地上， 因为毁灭摩押的要来攻击你们， 摧毁你们的堡垒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "住在底本的人民哪！ 要從你們榮耀的位上下來， 坐在乾旱的地上， 因為毀滅摩押的要上來攻擊你們， 摧毀你們的堅固城。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住在底本的民哪， 要從你榮耀的位上下來， 坐受乾渴； 因毀滅摩押的上來攻擊你， 毀壞了你的保障。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Come down from your glory And sit on the parched ground, O daughter dwelling in Dibon, For the destroyer of Moab has come up against you, He has ruined your strongholds."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Come down from your glory and sit on the parched ground, you inhabitants of Daughter Dibon, for the one who destroys Moab will come up against you and ruin your fortified cities."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Come down from your glory and sit in the dust, you people of Dibon, for those who destroy Moab will shatter Dibon, too. They will tear down all your towers."
      }
    },
