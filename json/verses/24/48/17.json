    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她的近邻和知道她名望的人啊， 你们都要为她哀悼， ‘那强大、辉煌的权势竟然崩溃了！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有在它四圍的， 所有認識它名的， 你們都要為它哀悼，說： ‘那有能力的權杖、 華美的杖竟然折斷了！’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡在她四圍的和認識她名的， 你們都要為她悲傷，說： 那結實的杖和那美好的棍， 何竟折斷了呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Mourn for him, all you who live around him, Even all of you who know his name; Say, ‘How has the mighty scepter been broken, A staff of splendor!’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Mourn for her, all who live around her, all who know her fame; say, ‘How broken is the mighty scepter, how broken the glorious staff!’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You friends of Moab, weep for him and cry! See how the strong scepter is broken, how the beautiful staff is shattered!"
      }
    },
