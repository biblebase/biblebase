    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.13.20-27",
            "paragraphs": [
              "一三20～27最後一個警告，指出猶大若繼續犯罪，懲罰（被擄到巴比倫）就是他們無可避免的後果。第23節指出要猶大改變她的行為，是多麽不可能的事。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这是你应得的报应， 是我量给你的， 因为你把我抛诸脑后， 信靠假神。 这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這是你應得的，是我量給你的分。” 這是耶和華的宣告。 “因為你忘記了我， 去倚靠虛假的偶像；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：這是你所當得的， 是我量給你的分； 因為你忘記我， 倚靠虛假。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "This is your lot, the portion I have measured out to you, declares the Lord, because you have forgotten me and trusted in lies."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is your lot, the portion measured to you From Me,” declares the Lord, “Because you have forgotten Me And trusted in falsehood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is your lot, the portion I have decreed for you,” declares the Lord, “because you have forgotten me and trusted in false gods."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "This is your lot, The portion of your measures from Me,” says the Lord, “Because you have forgotten Me And trusted in falsehood."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is your allotment, the portion I have assigned to you,” says the Lord, “for you have forgotten me, putting your trust in false gods."
      }
    },
