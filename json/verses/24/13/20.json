    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.13.20-27",
            "paragraphs": [
              "一三20～27最後一個警告，指出猶大若繼續犯罪，懲罰（被擄到巴比倫）就是他們無可避免的後果。第23節指出要猶大改變她的行為，是多麽不可能的事。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶路撒冷啊， 举目看看那些从北方来的敌人吧， 从前赐给你的佳美羊群——你的百姓如今在哪里呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶路撒冷啊！你要舉目觀看 那些從北方來的人。 從前交給你的羊群， 就是你引以為榮的群羊，現今在哪裡呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要舉目觀看從北方來的人。 先前賜給你的羣眾， 就是你佳美的羣眾， 如今在哪裏呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Lift up your eyes and see Those coming from the north. Where is the flock that was given you, Your beautiful sheep?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Look up and see those who are coming from the north. Where is the flock that was entrusted to you, the sheep of which you boasted?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Open up your eyes and see the armies marching down from the north! Where is your flock— your beautiful flock— that he gave you to care for?"
      }
    },
