    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.14.7-22",
            "paragraphs": [
              "一四7～22耶利米為百姓代求、認罪，並憑約的關系來求神（7～9節），但神卻吩咐他不要再祈禱（11,12節），因為審判假先知和聽從先知之人，是已經確定的事（15,16節）。耶利米仍然承襲亞伯拉罕的做法（創一八22～33），繼續為百姓求情（17～22節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为了你的尊名， 求你不要厌恶我们， 不要让你荣耀的宝座蒙羞。 求你顾念你与我们立的约， 不要废除它。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你為你名的緣故，不要厭棄我們！ 不要叫你榮耀的寶座蒙羞！ 求你記念你和我們所立的約，不要廢除！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你為你名的緣故， 不厭惡我們， 不辱沒你榮耀的寶座。 求你追念， 不要背了與我們所立的約。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Do not spurn us, for your name’s sake; do not dishonor your glorious throne; remember and do not break your covenant with us."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not despise us, for Your own name’s sake; Do not disgrace the throne of Your glory; Remember and do not annul Your covenant with us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the sake of your name do not despise us; do not dishonor your glorious throne. Remember your covenant with us and do not break it."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Do not abhor us, for Your name’s sake; Do not disgrace the throne of Your glory. Remember, do not break Your covenant with us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the sake of your reputation, Lord, do not abandon us. Do not disgrace your own glorious throne. Please remember us, and do not break your covenant with us."
      }
    },
