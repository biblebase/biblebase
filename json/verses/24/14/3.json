    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.14.1-6",
            "paragraphs": [
              "一四1～6一次影響城中居民（2,3節）、農夫（4節），甚至動物（5,6節）的\"幹旱\"（旱災），是從神而來的警告，但卻沒有人註意。\"家僮\"。奴仆。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贵族派仆人去水池打水， 仆人去那里发现没有水， 就带着空瓶垂头丧气、 羞愧难当地回去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的顯貴差派童僕去打水； 童僕來到池邊，卻找不到水， 就拿著空的器皿回來； 他們失望難過，蒙著自己的頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的貴冑打發家僮打水； 他們來到水池， 見沒有水，就拿着空器皿， 蒙羞慚愧，抱頭而回。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Her nobles send their servants for water; they come to the cisterns; they find no water; they return with their vessels empty; they are ashamed and confounded and cover their heads."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their nobles have sent their servants for water; They have come to the cisterns and found no water. They have returned with their vessels empty; They have been put to shame and humiliated, And they cover their heads."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The nobles send their servants for water; they go to the cisterns but find no water. They return with their jars unfilled; dismayed and despairing, they cover their heads."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Their nobles have sent their lads for water; They went to the cisterns and found no water. They returned with their vessels empty; They were ashamed and confounded And covered their heads."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The nobles send servants to get water, but all the wells are dry. The servants return with empty pitchers, confused and desperate, covering their heads in grief."
      }
