    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "论到那些住在埃及境内的密夺、答比匿、挪弗和巴特罗地区的犹大人，耶和华的话传给了耶利米，说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以下的話臨到耶利米，論到所有住在埃及地的猶大人，就是那些住在密奪、答比匿、挪弗和巴忒羅地的人，說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有臨到耶利米的話，論及一切住在埃及地的猶大人，就是住在密奪、答比匿、挪弗、巴忒羅境內的猶大人，說："
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The word that came to Jeremiah concerning all the Judeans who lived in the land of Egypt, at Migdol, at Tahpanhes, at Memphis, and in the land of Pathros,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The word that came to Jeremiah for all the Jews living in the land of Egypt, those who were living in Migdol, Tahpanhes, Memphis, and the land of Pathros, saying,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This word came to Jeremiah concerning all the Jews living in Lower Egypt—in Migdol, Tahpanhes and Memphis—and in Upper Egypt:"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The word that came to Jeremiah concerning all the Jews who dwell in the land of Egypt, who dwell at Migdol, at Tahpanhes, at Noph, and in the country of Pathros, saying,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is the message Jeremiah received concerning the Judeans living in northern Egypt in the cities of Migdol, Tahpanhes, and Memphis, and in southern Egypt as well:"
      }
