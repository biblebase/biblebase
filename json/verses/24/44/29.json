    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必在这地方惩罚你们，我要给你们一个预兆，好叫你们知道我言出必行。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘我要在這地方懲罰你們，這是給你們的徵兆，好使你們知道我說要降禍給你們的話，一定實現。’這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：我在這地方刑罰你們，必有預兆，使你們知道我降禍與你們的話必要立得住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This will be the sign to you,’ declares the Lord, ‘that I am going to punish you in this place, so that you may know that My words will surely stand against you for harm.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘This will be the sign to you that I will punish you in this place,’ declares the Lord, ‘so that you will know that my threats of harm against you will surely stand.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“‘And this is the proof I give you,’ says the Lord, ‘that all I have threatened will happen to you and that I will punish you here.’"
      }
    },
