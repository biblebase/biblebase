    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.44.15-19",
            "paragraphs": [
              "四四15～19百姓充滿輕蔑的回答，顯示他們仍未從耶路撒冷的毀滅中學到教訓。\"天後\"（17節）。參看第七章18節的腳註。\"餅\"（19節）。可能制成女神的形狀。丈夫都知道妻子拜偶像的行為，但沒有阻止她們。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我们不会听从你奉耶和华的名对我们说的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你奉耶和華的名對我們所說的話，我們不聽從。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「論到你奉耶和華的名向我們所說的話，我們必不聽從。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“As for the message that you have spoken to us in the name of the Lord, we are not going to listen to you!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“We will not listen to the message you have spoken to us in the name of the Lord!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“We will not listen to your messages from the Lord!"
      }
