    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.43.1-6",
            "paragraphs": [
              "四三1～6雖然眾人答應會遵從神透過耶利米所說的話（四二5），但他們背信而前往埃及去了。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米向众人宣告了他们的上帝耶和华的话，就是他们的上帝耶和华派他去说的一切话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米向眾民說完了耶和華他們的　神吩咐他的一切話，就是耶和華他們的　神差遣他去對他們說的一切話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶利米向眾百姓說完了耶和華－他們神的一切話，就是耶和華－他們神差遣他去所說的一切話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as soon as Jeremiah, whom the Lord their God had sent, had finished telling all the people all the words of the Lord their God—that is, all these words—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jeremiah had finished telling the people all the words of the Lord their God—everything the Lord had sent him to tell them—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jeremiah had finished giving this message from the Lord their God to all the people,"
      }
