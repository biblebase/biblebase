    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "jer.3.3-4",
            "paragraphs": [
              "3～4.神藉大自然的各種力量來促使以色列醒悟。但她卻厚顏如妓女，絲毫不覺羞愧。直至幹旱愈見嚴重時，人們便開始抱怨並向神求助，聲明神一直引導著他們。雖然如此，他們卻無法隱藏褻瀆了西乃之約的事實（參：瑪二10～14）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你不是刚对我说， ‘我父啊！你从我年幼时就做我的朋友。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你剛剛不是還呼叫我：‘我父啊！ 你是我年輕時的良伴；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從今以後，你豈不向我呼叫說： 我父啊，你是我幼年的恩主。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Have you not just now called to Me, ‘My Father, You are the friend of my youth?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Have you not just called to me: ‘My Father, my friend from my youth,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet you say to me, ‘Father, you have been my guide since my youth."
      }
    },
