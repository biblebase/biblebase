    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果有官长知道我曾与你会面，前来以死亡相威胁，逼你说出我们谈话的内容。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾領袖若聽見了我和你說過話，就會來見你，對你說：‘告訴我們，你對王說了些甚麼？不可向我們隱瞞，我們就不會殺死你。王又對你說了些甚麼？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "首領若聽見了我與你說話，就來見你，問你說：『你對王說甚麼話不要向我們隱瞞，我們就不殺你。王向你說甚麼話也要告訴我們。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But if the officials hear that I have talked with you and come to you and say to you, ‘Tell us now what you said to the king and what the king said to you; do not hide it from us and we will not put you to death,’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If the officials hear that I talked with you, and they come to you and say, ‘Tell us what you said to the king and what the king said to you; do not hide it from us or we will kill you,’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My officials may hear that I spoke to you, and they may say, ‘Tell us what you and the king were talking about. If you don’t tell us, we will kill you.’"
      }
    },
