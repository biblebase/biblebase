    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，王吩咐古实人以伯·米勒率领三十人，在耶利米先知未死之前把他从池中救出来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是王吩咐古實人以伯．米勒說：“你從這裡帶三十個人，在耶利米先知未死以前，把他從井裡拉上來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王就吩咐古實人以伯‧米勒說：「你從這裏帶領三十人，趁着先知耶利米未死以前，將他從牢獄中提上來。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then the king commanded Ebed-melech the Ethiopian, “Take thirty men with you from here, and lift Jeremiah the prophet out of the cistern before he dies.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king commanded Ebed-melech the Ethiopian, saying, “Take thirty men from here under your authority and bring up Jeremiah the prophet from the cistern before he dies.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the king commanded Ebed-Melek the Cushite, “Take thirty men from here with you and lift Jeremiah the prophet out of the cistern before he dies.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then the king commanded Ebed-Melech the Ethiopian, saying, “Take from here thirty men with you, and lift Jeremiah the prophet out of the dungeon before he dies.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the king told Ebed-melech, “Take thirty of my men with you, and pull Jeremiah out of the cistern before he dies.”"
      }
    },
