    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你可以告诉他们，你是在求我不要把你押回约拿单的房子，以免死在那里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你就要對他們說：‘我只是把我的懇求呈到王面前，求他不要把我送回約拿單的房子去，免得我死在那裡。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你就對他們說：『我在王面前懇求不要叫我回到約拿單的房屋死在那裏。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then you are to say to them, ‘I was presenting my petition before the king, not to make me return to the house of Jonathan to die there.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "then tell them, ‘I was pleading with the king not to send me back to Jonathan’s house to die there.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If this happens, just tell them you begged me not to send you back to Jonathan’s dungeon, for fear you would die there.”"
      }
