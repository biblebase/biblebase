    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝——万军之耶和华说： “看啊，我要使你们四面受敌， 惊恐万状，四散奔逃， 溃不成军。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！我必使你們因四圍的列國而恐慌， 這是主萬軍之耶和華的宣告； 你們必被趕逐，各自逃命， 沒有人把四散奔逃的人聚集起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主－萬軍之耶和華說： 我要使恐嚇從四圍的人中臨到你們； 你們必被趕出， 各人一直前往， 沒有人收聚逃民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I am going to bring terror upon you,” Declares the Lord God of hosts, “From all directions around you; And each of you will be driven out headlong, With no one to gather the fugitives together."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will bring terror on you from all those around you,” declares the Lord, the Lord Almighty. “Every one of you will be driven away, and no one will gather the fugitives."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But look! I will bring terror upon you,” says the Lord, the Lord of Heaven’s Armies. “Your neighbors will chase you from your land, and no one will help your exiles as they flee."
      }
    },
