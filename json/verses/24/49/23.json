    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "论到大马士革，耶和华说： “哈马和亚珥拔惊恐不安， 因为他们听见了坏消息。 他们心惊胆战，像大海一样无法平静。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "論到大馬士革，耶和華說： “哈馬和亞珥拔紛亂不安， 因為他們聽到了壞消息； 他們的心融化，焦慮慌張， 像不能平靜的海洋。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論大馬士革。 哈馬和亞珥拔蒙羞， 因他們聽見凶惡的信息就消化了。 海上有憂愁，不得平靜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Concerning Damascus. “Hamath and Arpad are put to shame, For they have heard bad news; They are disheartened. There is anxiety by the sea, It cannot be calmed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Concerning Damascus: “Hamath and Arpad are dismayed, for they have heard bad news. They are disheartened, troubled like the restless sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This message was given concerning Damascus. This is what the Lord says: “The towns of Hamath and Arpad are struck with fear, for they have heard the news of their destruction. Their hearts are troubled like a wild sea in a raging storm."
      }
