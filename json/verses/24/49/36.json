    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使敌人从四面八方如风而来，把以拦人驱散到四方，使他们流落到列国。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要使四風從天的四邊颳到以攔身上， 把他們分散到四方， 以致沒有一個國家沒有以攔難民的蹤跡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要使四風從天的四方颳來，臨到以攔人，將他們分散四方。這被趕散的人沒有一國不到的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will bring upon Elam the four winds From the four ends of heaven, And will scatter them to all these winds; And there will be no nation To which the outcasts of Elam will not go."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will bring against Elam the four winds from the four quarters of heaven; I will scatter them to the four winds, and there will not be a nation where Elam’s exiles do not go."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will bring enemies from all directions, and I will scatter the people of Elam to the four winds. They will be exiled to countries around the world."
      }
    },
