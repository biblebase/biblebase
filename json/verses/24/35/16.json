    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.35.12-19",
            "paragraphs": [
              "三五12～19耶利米利用利甲人的忠誠給猶大上了一課；猶大與利甲人相反，他們違反了神的命令。神賞賜利甲人，讓他們必有後裔來敬拜和事奉神。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "利甲的儿子约拿达的子孙尚且听从他们祖先的吩咐，你们却不听从我的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "利甲的兒子約拿達的子孫實行了他們祖先吩咐他們的命令，但這人民沒有聽從我。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利甲的兒子約拿達的子孫能遵守先人所吩咐他們的命，這百姓卻沒有聽從我！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Indeed, the sons of Jonadab the son of Rechab have observed the command of their father which he commanded them, but this people has not listened to Me.’ ” ’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The descendants of Jehonadab son of Rekab have carried out the command their forefather gave them, but these people have not obeyed me.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The descendants of Jehonadab son of Recab have obeyed their ancestor completely, but you have refused to listen to me.’"
      }
    },
