    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说，‘看啊，时候将到，我要使我被掳的子民——以色列人和犹大人回到故土，回到我赐给他们祖先的土地上。这是耶和华说的。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！日子快到，我必使我的子民以色列和猶大被擄的人歸回。’耶和華說：‘我必使他們回到我賜給他們列祖之地，他們要重得這地為業。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：日子將到，我要使我的百姓以色列和猶大被擄的人歸回；我也要使他們回到我所賜給他們列祖之地，他們就得這地為業。這是耶和華說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For behold, days are coming,’ declares the Lord, ‘when I will restore the fortunes of My people Israel and Judah.’ The Lord says, ‘I will also bring them back to the land that I gave to their forefathers and they shall possess it.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The days are coming,’ declares the Lord, ‘when I will bring my people Israel and Judah back from captivity and restore them to the land I gave their ancestors to possess,’ says the Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the time is coming when I will restore the fortunes of my people of Israel and Judah. I will bring them home to this land that I gave to their ancestors, and they will possess it again. I, the Lord, have spoken!”"
      }
