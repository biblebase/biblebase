    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.30.9",
            "paragraphs": [
              "30:9",
              "9~10王大衛：神子民將來要侍奉的是救主耶穌，他是大衛的後裔(比較何3:5；結37:24~25)。“雅各”在此指從以色列和猶大而出的公義余民，他們將得享平安穩妥，所有抗拒神並且逼迫神的仆人的國民都將被滅絕凈盡(11節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们要事奉我——他们的上帝耶和华，听从我为他们选立来继承大卫宝座的王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們卻要事奉耶和華他們的　神，和我為他們興起的大衛王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們卻要事奉耶和華－你們的神和我為你們所要興起的王大衛。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But they shall serve the Lord their God and David their king, whom I will raise up for them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Instead, they will serve the Lord their God and David their king, whom I will raise up for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For my people will serve the Lord their God and their king descended from David— the king I will raise up for them."
      }
