    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说，‘以色列的上帝——万军之耶和华说，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要吩咐使臣對他們的王說：萬軍之耶和華以色列的　神這樣說：你們要這樣對你們的王說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "且囑咐使臣，傳與他們的主人說，萬軍之耶和華－以色列的神如此說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Command them to go to their masters, saying, ‘Thus says the Lord of hosts, the God of Israel, thus you shall say to your masters,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Give them a message for their masters and say, ‘This is what the Lord Almighty, the God of Israel, says: “Tell this to your masters:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give them this message for their masters: ‘This is what the Lord of Heaven’s Armies, the God of Israel, says:"
      }
