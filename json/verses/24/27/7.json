    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "各国都要臣服于他和他的子孙，直到他的国家灭亡。那时，许多国家和强大的君王必奴役他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "列國都要服事他和他的兒子、孫子，直到他本國遭報的時候來到；那時，許多國家和大君王都要奴役他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "列國都必服事他和他的兒孫，直到他本國遭報的日期來到。那時，多國和大君王要使他作他們的奴僕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the nations shall serve him and his son and his grandson until the time of his own land comes; then many nations and great kings will make him their servant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All nations will serve him and his son and his grandson until the time for his land comes; then many nations and great kings will subjugate him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the nations will serve him, his son, and his grandson until his time is up. Then many nations and great kings will conquer and rule over Babylon."
      }
    },
