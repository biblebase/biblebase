    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你和你的人民何必死于战争、饥荒和瘟疫呢？耶和华要降这些灾难给不肯臣服于巴比伦王的国家。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你和你的人民為甚麼要因刀劍、饑荒和瘟疫而死，正如耶和華論到不肯服事巴比倫王的國家所說的一樣呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你和你的百姓為何要因刀劍、饑荒、瘟疫死亡，正如耶和華論到不服事巴比倫王的那國說的話呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why will you die, you and your people, by the sword, famine and pestilence, as the Lord has spoken to that nation which will not serve the king of Babylon?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why will you and your people die by the sword, famine and plague with which the Lord has threatened any nation that will not serve the king of Babylon?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why do you insist on dying—you and your people? Why should you choose war, famine, and disease, which the Lord will bring against every nation that refuses to submit to Babylon’s king?"
      }
    },
