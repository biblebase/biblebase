    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.27.6",
            "paragraphs": [
              "27:6",
              "我仆人……尼布甲尼撒：尼布甲尼撒王被稱為神的仆人，並不是因為他的公義，而是因為神將借著他和他的大軍刑罰多國的罪惡，其中也包括神的百姓猶大。然而，當神所定的日子到來時，他將親自除滅巴比倫(7節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在，我要把你们的国家交给我的仆人巴比伦王尼布甲尼撒，连田野的走兽也要归他管辖。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在我已經親自把這一切國家都交在我僕人巴比倫王尼布甲尼撒的手中，連田野的走獸，我也交給他，供他奴役。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在我將這些地都交給我僕人巴比倫王尼布甲尼撒的手，我也將田野的走獸給他使用。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now I have given all these lands into the hand of Nebuchadnezzar, the king of Babylon, my servant, and I have given him also the beasts of the field to serve him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now I have given all these lands into the hand of Nebuchadnezzar king of Babylon, My servant, and I have given him also the wild animals of the field to serve him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now I will give all your countries into the hands of my servant Nebuchadnezzar king of Babylon; I will make even the wild animals subject to him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And now I have given all these lands into the hand of Nebuchadnezzar the king of Babylon, My servant; and the beasts of the field I have also given him to serve him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now I will give your countries to King Nebuchadnezzar of Babylon, who is my servant. I have put everything, even the wild animals, under his control."
      }
    },
