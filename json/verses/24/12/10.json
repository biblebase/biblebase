    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "列国的首领毁坏我的葡萄园， 践踏我美好的土地， 使它荒凉；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "許多牧人毀壞了我的葡萄園， 踐踏了我的田地； 他們使我美好的田地 變為荒涼的曠野。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "許多牧人毀壞我的葡萄園， 踐踏我的分， 使我美好的分變為荒涼的曠野。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Many shepherds have destroyed my vineyard; they have trampled down my portion; they have made my pleasant portion a desolate wilderness."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Many shepherds have ruined My vineyard, They have trampled down My field; They have made My pleasant field A desolate wilderness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Many shepherds will ruin my vineyard and trample down my field; they will turn my pleasant field into a desolate wasteland."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Many rulers have destroyed My vineyard, They have trodden My portion underfoot; They have made My pleasant portion a desolate wilderness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Many rulers have ravaged my vineyard, trampling down the vines and turning all its beauty into a barren wilderness."
      }
