    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.12.7",
            "paragraphs": [
              "12:7",
              "7~13我離了我的殿宇：神親自為著民眾悲慘的屬靈光景和全地即將荒涼的惡運而發出哀嘆。每當神任憑他的子民承受罪的可怕後果時，他的內心都會經歷極大的痛苦和憂傷。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.12.7-13",
            "paragraphs": [
              "一二7～13耶利米描述即將來到的毀壞，好象是已經發生了一樣。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我已离开我的殿， 撇弃我的产业， 把我爱的子民交给他们的敌人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我撇下了我的家， 丟棄了我的產業； 我把我心愛的 交在她仇敵的手中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我離了我的殿宇， 撇棄我的產業， 將我心裏所親愛的交在她仇敵的手中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I have forsaken My house, I have abandoned My inheritance; I have given the beloved of My soul Into the hand of her enemies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will forsake my house, abandon my inheritance; I will give the one I love into the hands of her enemies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I have abandoned my people, my special possession. I have surrendered my dearest ones to their enemies."
      }
    },
