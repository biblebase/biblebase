    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.12.14",
            "paragraphs": [
              "一二14\"一切惡鄰\"。敘利亞、摩押和亞捫，他們與猶大一同受到巴比倫的懲罰。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“邪恶的邻国侵占了我赐给我以色列子民的土地，我要把这些恶邻逐出他们的本土，正如我要逐出犹大一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華這樣說：“至於所有邪惡的鄰國，他們侵犯我賜給我子民以色列的產業；看哪！我要把他們從本地拔出來，也要把猶大家從他們中間拔出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華如此說：「一切惡鄰，就是佔據我使百姓以色列所承受產業的，我要將他們拔出本地，又要將猶大家從他們中間拔出來。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Thus says the Lord concerning all my evil neighbors who touch the heritage that I have given my people Israel to inherit: “Behold, I will pluck them up from their land, and I will pluck up the house of Judah from among them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord concerning all My wicked neighbors who strike at the inheritance with which I have endowed My people Israel, “Behold I am about to uproot them from their land and will uproot the house of Judah from among them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord says: “As for all my wicked neighbors who seize the inheritance I gave my people Israel, I will uproot them from their lands and I will uproot the people of Judah from among them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Thus says the Lord: “Against all My evil neighbors who touch the inheritance which I have caused My people Israel to inherit—behold, I will pluck them out of their land and pluck out the house of Judah from among them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now this is what the Lord says: “I will uproot from their land all the evil nations reaching out for the possession I gave my people Israel. And I will uproot Judah from among them."
      }
    },
