    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.17.1",
            "paragraphs": [
              "17:1",
              "猶大的罪：耶利米斥責猶大民眾的罪大惡極。罪惡已經深深地侵蝕了他們的本性，以致拜偶像和作惡成了他們生活中必不可少的一部分。猶大的百姓將因著自己的背信棄義而失去地土，受人奴役(4節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.17.1",
            "paragraphs": [
              "一七1\"猶大的罪\"甚至被銘刻在\"壇角上\"，難以消除，如此，現在贖罪也變成不可能的了。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“犹大人的罪是用铁笔记录的，用钻石镌刻的，写在了他们心坎上和祭坛角上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“猶大的罪是用鐵筆銘記的，是用金鋼鑽刻在他們的心版上，和他們祭壇的角上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大的罪是用鐵筆、用金鋼鑽記錄的，銘刻在他們的心版上和壇角上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sin of Judah is written down with an iron stylus; With a diamond point it is engraved upon the tablet of their heart And on the horns of their altars,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Judah’s sin is engraved with an iron tool, inscribed with a flint point, on the tablets of their hearts and on the horns of their altars."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The sin of Judah is inscribed with an iron chisel— engraved with a diamond point on their stony hearts and on the corners of their altars."
      }
    },
