    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.17.14",
            "paragraphs": [
              "17:14",
              "14~18醫治我……拯救我：在殘酷的逼迫和強大的敵對勢力面前，耶利米祈求神賜下恩惠幫助他繼續持守先知的職分。耶利米的預言尚未應驗，先知本人和他所傳的信息都遭到民眾和假先知的奚落和嘲諷(15節)。盡管耶利米經受了極大的痛苦和折磨，但是他仍然不放棄事奉，只是不斷地仰望神賜下力量和幫助。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.17.14-18",
            "paragraphs": [
              "一七14～18耶利米作出有理的請求，求神為他辯解。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 你医治我，我必痊愈； 你拯救我，我必脱离危难， 因为你是我所赞美的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你醫治我，我就得醫治； 求你拯救我，我就得拯救； 因為你是我所讚美的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你醫治我，我便痊癒， 拯救我，我便得救； 因你是我所讚美的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Heal me, O Lord, and I will be healed; Save me and I will be saved, For You are my praise."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Heal me, Lord, and I will be healed; save me and I will be saved, for you are the one I praise."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, if you heal me, I will be truly healed; if you save me, I will be truly saved. My praises are for you alone!"
      }
    },
