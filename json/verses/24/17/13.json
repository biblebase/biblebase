    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你是以色列的盼望。 背弃你的人必蒙羞， 背叛你的人必像写在尘土上的名字一样消逝， 因为他们背弃了耶和华——活水的源泉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華以色列的盼望啊！ 離棄你的，都必蒙羞； 偏離你的，他們的名字必寫在沙土上， 因為他們離棄了耶和華這活水的源頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－以色列的盼望啊， 凡離棄你的必致蒙羞。 耶和華說：離開我的， 他們的名字必寫在土裏， 因為他們離棄我這活水的泉源。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "O Lord, the hope of Israel, all who forsake you shall be put to shame; those who turn away from you  shall be written in the earth, for they have forsaken the Lord, the fountain of living water."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, the hope of Israel, All who forsake You will be put to shame. Those who turn away on earth will be written down, Because they have forsaken the fountain of living water, even the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, you are the hope of Israel; all who forsake you will be put to shame. Those who turn away from you will be written in the dust because they have forsaken the Lord, the spring of living water."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "O Lord, the hope of Israel, All who forsake You shall be ashamed. “Those who depart from Me Shall be written in the earth, Because they have forsaken the Lord, The fountain of living waters.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, the hope of Israel, all who turn away from you will be disgraced. They will be buried in the dust of the earth, for they have abandoned the Lord, the fountain of living water."
      }
    },
