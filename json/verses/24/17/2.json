    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.17.2",
            "paragraphs": [
              "一七2\"木偶\"。直譯作：亞舍拉。迦南女神亞舍拉的像立在偶像的祭壇旁邊（比較申一六21）。連兒童也參與異教的膜拜。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的儿女没有忘记绿树下、高岗上、"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的兒女也懷念著 那些在各高岡上、 在繁茂樹旁的祭壇和亞舍拉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的兒女記念他們高岡上、青翠樹旁的壇和木偶。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "while their children remember their altars and their Asherim, beside every green tree and on the high hills,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As they remember their children, So they remember their altars and their Asherim By green trees on the high hills."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even their children remember their altars and Asherah poles beside the spreading trees and on the high hills."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "While their children remember Their altars and their wooden images By the green trees on the high hills."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even their children go to worship at their pagan altars and Asherah poles, beneath every green tree and on every high hill."
      }
    },
