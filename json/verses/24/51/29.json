    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大地要颤抖，痛苦地扭动， 因为耶和华决意要攻击巴比伦， 使她荒无人烟。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大地震動翻騰， 因為耶和華攻擊巴比倫所定的旨意立定不移， 要使巴比倫的地荒涼， 沒有人居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "地必震動而瘠苦； 因耶和華向巴比倫所定的旨意成立了， 使巴比倫之地荒涼，無人居住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the land quakes and writhes, For the purposes of the Lord against Babylon stand, To make the land of Babylon A desolation without inhabitants."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The land trembles and writhes, for the Lord’s purposes against Babylon stand— to lay waste the land of Babylon so that no one will live there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The earth trembles and writhes in pain, for everything the Lord has planned against Babylon stands unchanged. Babylon will be left desolate without a single inhabitant."
      }
    },
