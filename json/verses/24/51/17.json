    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.51.17-18",
            "paragraphs": [
              "五一17,18巴比倫的偶像要與巴比倫人一同毀滅。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世人都愚昧无知， 工匠都因所制的偶像而羞愧， 因为神像没有生命气息， 是虚假的，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人人都是頑梗無知； 所有的金匠都因偶像羞愧， 因為他們鑄造的像都是虛假，沒有氣息。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "各人都成了畜類，毫無知識。 各銀匠都因他的偶像羞愧； 他所鑄的偶像本是虛假的， 其中並無氣息，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All mankind is stupid, devoid of knowledge; Every goldsmith is put to shame by his idols, For his molten images are deceitful, And there is no breath in them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Everyone is senseless and without knowledge; every goldsmith is shamed by his idols. The images he makes are a fraud; they have no breath in them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The whole human race is foolish and has no knowledge! The craftsmen are disgraced by the idols they make, for their carefully shaped works are a fraud. These idols have no breath or power."
      }
    },
