    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并且要说，‘耶和华啊！你曾说要毁灭这地方，使这里人兽绝迹、永远荒凉。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後你要說：‘耶和華啊！論到這地方，你曾經親自說過要把它剪除，使這地永遠荒涼，沒有人畜居住。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又說：『耶和華啊，你曾論到這地方說：要剪除，甚至連人帶牲畜沒有在這裏居住的，必永遠荒涼。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and say, ‘You, O Lord, have promised concerning this place to cut it off, so that there will be nothing dwelling in it, whether man or beast, but it will be a perpetual desolation.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then say, ‘Lord, you have said you will destroy this place, so that neither people nor animals will live in it; it will be desolate forever.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then say, ‘Lord, you have said that you will destroy Babylon so that neither people nor animals will remain here. She will lie empty and abandoned forever.’"
      }
    },
