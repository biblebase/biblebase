    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.51.7-8",
            "paragraphs": [
              "五一7,8\"金杯\"。巴比倫在外表是燦爛的，但它的\"杯\"充滿了偶像膜拜。啟示錄十四章8節、十七章1至6節也使用相同的比喻來形容末世的巴比倫。現在再沒有\"乳香\"（8節）能醫治巴比倫。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴比伦在耶和华手中曾是灌醉天下的金杯， 列国喝了她的酒都变得癫狂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴比倫曾是耶和華手中的金杯， 使全地都喝醉； 列國喝了它的酒， 就都瘋狂亂性。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴比倫素來是耶和華手中的金杯， 使天下沉醉； 萬國喝了她的酒就顛狂了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Babylon was a golden cup in the Lord’s hand, making all the earth drunken; the nations drank of her wine; therefore the nations went mad."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Babylon has been a golden cup in the hand of the Lord, Intoxicating all the earth. The nations have drunk of her wine; Therefore the nations are going mad."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Babylon was a gold cup in the Lord’s hand; she made the whole earth drunk. The nations drank her wine; therefore they have now gone mad."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Babylon was a golden cup in the Lord’s hand, That made all the earth drunk. The nations drank her wine; Therefore the nations are deranged."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Babylon has been a gold cup in the Lord’s hands, a cup that made the whole earth drunk. The nations drank Babylon’s wine, and it drove them all mad."
      }
    },
