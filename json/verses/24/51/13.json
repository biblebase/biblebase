      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "住在河边、拥有财宝的巴比伦人啊， 你们的结局到了， 你们的末日来了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你這住在眾水旁， 多有財寶的啊！ 你的結局到了， 你被剪除的時刻到了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住在眾水之上多有財寶的啊， 你的結局到了！ 你貪婪之量滿了！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O you who dwell by many waters, Abundant in treasures, Your end has come, The measure of your end."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You who live by many waters and are rich in treasures, your end has come, the time for you to be destroyed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You are a city by a great river, a great center of commerce, but your end has come. The thread of your life is cut."
      }
