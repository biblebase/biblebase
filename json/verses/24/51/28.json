    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要让列国预备攻打巴比伦， 使玛代的诸王及其省长、总督和所统治的人攻打巴比伦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要使列國作好準備攻擊她， 使瑪代諸王， 和她的省長、總監， 以及他們轄下各地的人民都作好準備攻擊她。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使列國和米底亞君王，與省長和副省長， 並他們所管全地之人，都預備攻擊她。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Consecrate the nations against her, The kings of the Medes, Their governors and all their prefects, And every land of their dominion."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Prepare the nations for battle against her— the kings of the Medes, their governors and all their officials, and all the countries they rule."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Bring against her the armies of the nations— led by the kings of the Medes and all their captains and officers."
      }
    },
