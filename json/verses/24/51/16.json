    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂一声令下，天上便大水澎湃； 祂使云雾从地极上升， 使闪电在雨中划过， 使风从祂的仓库吹出。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他一發聲，空中諸水就澎湃； 他使雲霧從地極上騰； 他造了閃電和雷雨， 又使風從他的倉庫裡吹出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他一發聲，空中便有多水激動； 他使雲霧從地極上騰。 他造電隨雨而閃， 從他府庫中帶出風來。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When he utters his voice there is a tumult of waters in the heavens, and he makes the mist rise from the ends of the earth. He makes lightning for the rain, and he brings forth the wind from his storehouses."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He utters His voice, there is a tumult of waters in the heavens, And He causes the clouds to ascend from the end of the earth; He makes lightning for the rain And brings forth the wind from His storehouses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he thunders, the waters in the heavens roar; he makes clouds rise from the ends of the earth. He sends lightning with the rain and brings out the wind from his storehouses."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "When He utters His voice— There  is a multitude of waters in the heavens: “He causes the vapors to ascend from the ends of the earth; He makes lightnings for the rain; He brings the wind out of His treasuries.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When he speaks in the thunder, the heavens roar with rain. He causes the clouds to rise over the earth. He sends the lightning with the rain and releases the wind from his storehouses."
      }
    },
