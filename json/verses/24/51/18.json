    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.51.17-18",
            "paragraphs": [
              "五一17,18巴比倫的偶像要與巴比倫人一同毀滅。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "毫无益处，荒唐可笑， 刑罚一到，必被毁灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "偶像都是虛無的，是荒謬可笑的作品； 到了討罪的時候，它們必被除滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "都是虛無的，是迷惑人的工作， 到追討的時候，必被除滅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They are worthless, a work of mockery; In the time of their punishment they will perish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are worthless, the objects of mockery; when their judgment comes, they will perish."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Idols are worthless; they are ridiculous lies! On the day of reckoning they will all be destroyed."
      }
    },
