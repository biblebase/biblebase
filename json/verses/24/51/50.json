    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“刀下逃生的人啊， 你们快走吧！ 不要停留！ 在远方要记住耶和华， 要追想耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些已逃脫了刀劍的人哪！ 走吧！不要站著， 要從遠方記念耶和華， 心中惦念耶路撒冷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們躲避刀劍的要快走， 不要站住！ 要在遠方記念耶和華， 心中追想耶路撒冷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You who have escaped the sword, Depart! Do not stay! Remember the Lord from afar, And let Jerusalem come to your mind."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You who have escaped the sword, leave and do not linger! Remember the Lord in a distant land, and call to mind Jerusalem.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Get out, all you who have escaped the sword! Do not stand and watch—flee while you can! Remember the Lord, though you are in a far-off land, and think about your home in Jerusalem.”"
      }
    },
