      "leili": {
        "content": [
          {
            "verse": "jer.18.1-11",
            "paragraphs": [
              "一八1～11這個陶匠的比喻指出神對列國有絕對的主權，祂並非反復多變，而是對百姓的悔改（8節）作出敏感的回應。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当我宣布要铲除、推翻、毁灭哪一邦或哪一国的时候，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我甚麼時候宣布要拔出、拆毀、毀滅一邦或一國，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我何時論到一邦或一國說，要拔出、拆毀、毀壞；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If at any time I declare concerning a nation or a kingdom, that I will pluck up and break down and destroy it,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "At one moment I might speak concerning a nation or concerning a kingdom to uproot, to pull down, or to destroy it;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If at any time I announce that a nation or kingdom is to be uprooted, torn down and destroyed,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The instant I speak concerning a nation and concerning a kingdom, to pluck up, to pull down, and to destroy it,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If I announce that a certain nation or kingdom is to be uprooted, torn down, and destroyed,"
      }
    },
