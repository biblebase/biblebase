    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.18.17",
            "paragraphs": [
              "一八17\"東風\"。參看第四章11節。\"以背向他們，不以面向他們\"。因為神要離開這個民族。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要在敌人面前驱散他们， 就像东风吹散尘土一样。 在他们遭难的时候， 我必以背相向， 不理睬他们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在他們的敵人面前， 我要像東風吹散他們； 在他們遭難的日子， 我必使他們只見我的背，不見我的面。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必在仇敵面前分散他們， 好像用東風吹散一樣。 遭難的日子，我必以背向他們， 不以面向他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like an east wind I will scatter them Before the enemy; I will show them My back and not My face In the day of their calamity.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like a wind from the east, I will scatter them before their enemies; I will show them my back and not my face in the day of their disaster.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will scatter my people before their enemies as the east wind scatters dust. And in all their trouble I will turn my back on them and refuse to notice their distress.”"
      }
    },
