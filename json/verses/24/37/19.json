    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些曾向你们预言‘巴比伦王绝不会来这里攻打你们’的先知如今在哪里呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們的先知曾向你們預言說：‘巴比倫王必不會來攻擊你們和這地。’他們現今在哪裡呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對你們預言巴比倫王必不來攻擊你們和這地的先知，現今在哪裏呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Where then are your prophets who prophesied to you, saying, ‘The king of Babylon will not come against you or against this land’?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Where are your prophets who prophesied to you, ‘The king of Babylon will not attack you or this land’?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Where are your prophets now who told you the king of Babylon would not attack you or this land?"
      }
