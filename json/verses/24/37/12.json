    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.37.12-13",
            "paragraphs": [
              "三七12,13\"伊利雅\"以為耶利米要向敵人投誠，然而，耶利米只是打算回鄉\"得自己的地業\"。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米起身离开耶路撒冷，前往便雅悯境内去接收他在同族中应得的地业。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米離開耶路撒冷，往便雅憫地去，要在那裡，在同鄉中間，取得自己的產業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶利米就雜在民中出離耶路撒冷，要往便雅憫地去，在那裏得自己的地業。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that Jeremiah went out from Jerusalem to go to the land of Benjamin in order to take possession of some property there among the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jeremiah started to leave the city to go to the territory of Benjamin to get his share of the property among the people there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jeremiah started to leave the city on his way to the territory of Benjamin, to claim his share of the property among his relatives there."
      }
    },
