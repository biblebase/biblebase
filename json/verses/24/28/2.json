    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.28.1-17",
            "paragraphs": [
              "二八1～17假先知哈拿尼雅質疑耶利米說猶大要被擄七十年的預言，並且自己預言聖殿的器皿和耶哥尼雅要在兩年之內返回耶路撒冷。耶利米確實希望情況會是這樣（6節），但再次預言事實並非如此（14節）。留意耶利米仍然帶他的\"視覺教具\"--頸上的軛（10節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以色列的上帝——万军之耶和华说，‘我必折断巴比伦王奴役你们的轭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“萬軍之耶和華以色列的　神這樣說：‘我已經折斷了巴比倫王的軛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「萬軍之耶和華－以色列的神如此說：我已經折斷巴比倫王的軛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Thus says the Lord of hosts, the God of Israel, ‘I have broken the yoke of the king of Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Lord Almighty, the God of Israel, says: ‘I will break the yoke of the king of Babylon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Lord of Heaven’s Armies, the God of Israel, says: ‘I will remove the yoke of the king of Babylon from your necks."
      }
