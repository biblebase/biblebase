    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.28.1-17",
            "paragraphs": [
              "二八1～17假先知哈拿尼雅質疑耶利米說猶大要被擄七十年的預言，並且自己預言聖殿的器皿和耶哥尼雅要在兩年之內返回耶路撒冷。耶利米確實希望情況會是這樣（6節），但再次預言事實並非如此（14節）。留意耶利米仍然帶他的\"視覺教具\"--頸上的軛（10節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不过，请你和众人都听我说，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不過，你應該聽我向你和眾民耳中所說的這話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "然而我向你和眾民耳中所要說的話，你應當聽。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Yet hear now this word that I speak in your hearing and in the hearing of all the people."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet hear now this word which I am about to speak in your hearing and in the hearing of all the people!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nevertheless, listen to what I have to say in your hearing and in the hearing of all the people:"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Nevertheless hear now this word that I speak in your hearing and in the hearing of all the people:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But listen now to the solemn words I speak to you in the presence of all these people."
      }
    },
