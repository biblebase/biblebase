    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.28.1-17",
            "paragraphs": [
              "二八1～17假先知哈拿尼雅質疑耶利米說猶大要被擄七十年的預言，並且自己預言聖殿的器皿和耶哥尼雅要在兩年之內返回耶路撒冷。耶利米確實希望情況會是這樣（6節），但再次預言事實並非如此（14節）。留意耶利米仍然帶他的\"視覺教具\"--頸上的軛（10節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在你我之前的古代先知曾预言许多地方和强国要遭遇战争、灾祸和瘟疫。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "自古以來，在你我以前的先知，已經預言有戰爭、災禍和瘟疫要臨到許多地方和強國。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從古以來，在你我以前的先知，向多國和大邦說預言，論到爭戰、災禍、瘟疫的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The prophets who were before me and before you from ancient times prophesied against many lands and against great kingdoms, of war and of calamity and of pestilence."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From early times the prophets who preceded you and me have prophesied war, disaster and plague against many countries and great kingdoms."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The ancient prophets who preceded you and me spoke against many nations, always warning of war, disaster, and disease."
      }
    },
