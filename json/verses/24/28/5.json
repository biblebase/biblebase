    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.28.1-17",
            "paragraphs": [
              "二八1～17假先知哈拿尼雅質疑耶利米說猶大要被擄七十年的預言，並且自己預言聖殿的器皿和耶哥尼雅要在兩年之內返回耶路撒冷。耶利米確實希望情況會是這樣（6節），但再次預言事實並非如此（14節）。留意耶利米仍然帶他的\"視覺教具\"--頸上的軛（10節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米先知当着站在耶和华殿里的祭司和民众的面对哈拿尼雅先知说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米先知當著祭司和站在耶和華殿裡的眾民面前，對哈拿尼雅先知說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "先知耶利米當着祭司和站在耶和華殿裏的眾民對先知哈拿尼雅說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the prophet Jeremiah spoke to the prophet Hananiah in the presence of the priests and in the presence of all the people who were standing in the house of the Lord,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the prophet Jeremiah replied to the prophet Hananiah before the priests and all the people who were standing in the house of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jeremiah responded to Hananiah as they stood in front of all the priests and people at the Temple."
      }
    },
