    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.36.2",
            "paragraphs": [
              "36:2",
              "都寫在其上：耶利米的預言首次被寫成書。起初，記錄預言的目的是為了向民眾大聲宣讀，神期望猶大的民眾能夠聽從他書面的話語，轉離他們罪惡的道路並且得到神的赦罪之恩，如此就能免遭神烈怒的攻擊(3,6~7節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.36.2",
            "paragraphs": [
              "三六2\"一書卷\"。一個卷軸。用蒲草紙粘合而成（比較23節）。當中包含耶利米從主前627至605年所說的預言。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你拿一个卷轴来，把从约西亚登基直到现在我告诉你有关以色列、犹大和各国的事都写在上面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你取一卷書卷來，在上面寫下我在約西亞作王的時候開始，直到今日，對你所說論到以色列和猶大，以及列國的一切話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你取一書卷，將我對你說攻擊以色列和猶大，並各國的一切話，從我對你說話的那日，就是從約西亞的日子起直到今日，都寫在其上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Take a scroll and write on it all the words which I have spoken to you concerning Israel and concerning Judah, and concerning all the nations, from the day I first spoke to you, from the days of Josiah, even to this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Take a scroll and write on it all the words I have spoken to you concerning Israel, Judah and all the other nations from the time I began speaking to you in the reign of Josiah till now."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Get a scroll, and write down all my messages against Israel, Judah, and the other nations. Begin with the first message back in the days of Josiah, and write down every message, right up to the present time."
      }
    },
