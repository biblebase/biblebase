    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.32.26-44",
            "paragraphs": [
              "三二26～44神重新向祂的先知作出保證，被擄歸回之後，祂要使子民在地上再次興旺起來。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们必放火焚城，把城中的房屋付之一炬，城中的居民曾在房顶上向巴力烧香、向假神奠酒，惹我发怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "攻打這城的迦勒底人必進來，放火燒城，城中的房屋都必燒光，因為這城的居民曾在那些房屋的屋頂上向巴力燒香，向別神澆奠祭，惹我發怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "攻城的迦勒底人必來放火焚燒這城和其中的房屋。在這房屋上，人曾向巴力燒香，向別神澆奠，惹我發怒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Chaldeans who are fighting against this city will enter and set this city on fire and burn it, with the houses where people have offered incense to Baal on their roofs and poured out drink offerings to other gods to provoke Me to anger."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Babylonians who are attacking this city will come in and set it on fire; they will burn it down, along with the houses where the people aroused my anger by burning incense on the roofs to Baal and by pouring out drink offerings to other gods."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Babylonians outside the walls will come in and set fire to the city. They will burn down all these houses where the people provoked my anger by burning incense to Baal on the rooftops and by pouring out liquid offerings to other gods."
      }
    },
