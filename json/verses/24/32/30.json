    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.32.26-44",
            "paragraphs": [
              "三二26～44神重新向祂的先知作出保證，被擄歸回之後，祂要使子民在地上再次興旺起來。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人和犹大人从一开始就行我视为恶的事，他们的所作所为惹我发怒。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "事實上，以色列人和猶大人自幼以來，就專行我眼中看為惡的事；以色列人的手所作的，無不惹我發怒。’這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人和猶大人自從幼年以來，專行我眼中看為惡的事；以色列人盡以手所做的惹我發怒。這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Indeed the sons of Israel and the sons of Judah have been doing only evil in My sight from their youth; for the sons of Israel have been only provoking Me to anger by the work of their hands,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The people of Israel and Judah have done nothing but evil in my sight from their youth; indeed, the people of Israel have done nothing but arouse my anger with what their hands have made, declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Israel and Judah have done nothing but wrong since their earliest days. They have infuriated me with all their evil deeds,” says the Lord."
      }
    },
