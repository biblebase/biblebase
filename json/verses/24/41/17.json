    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.41.17",
            "paragraphs": [
              "四一17\"金罕\"。參看撒母耳記下十九章37至40節。這是大衛封地的一部分。眾人打算前往埃及去，因為他們害怕巴比倫人為基大利被謀殺而進行報復。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们启程前往埃及，途中在伯利恒附近的基罗特金罕住下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們去到伯利恆附近的基羅特金罕，停留在那裡，想要往埃及去，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "帶到靠近伯利恆的金罕寓住下，要進入埃及去；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they went and stayed in Geruth Chimham, which is beside Bethlehem, in order to proceed into Egypt"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And they went on, stopping at Geruth Kimham near Bethlehem on their way to Egypt"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They took them all to the village of Geruth-kimham near Bethlehem, where they prepared to leave for Egypt."
      }
