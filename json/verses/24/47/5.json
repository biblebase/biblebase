    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.47.5",
            "paragraphs": [
              "四七5\"光禿\"。是哀悼的記號，或指\"迦薩\"完全被夷為平地。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "迦萨一片荒凉， 亚实基伦沦为废墟。 谷中的余民啊！ 你们要割伤身体到何时呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "迦薩成了禿頭的， 亞實基倫靜默無聲， 在它們的平原上餘剩的人哪， 你們劃傷自己的身體要到幾時呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "迦薩成了光禿； 平原中所剩的亞實基倫歸於無有。 你用刀劃身，要到幾時呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Baldness has come upon Gaza; Ashkelon has been ruined. O remnant of their valley, How long will you gash yourself?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Gaza will shave her head in mourning; Ashkelon will be silenced. You remnant on the plain, how long will you cut yourselves?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Gaza will be humiliated, its head shaved bald; Ashkelon will lie silent. You remnant from the Mediterranean coast, how long will you cut yourselves in mourning?"
      }
    },
