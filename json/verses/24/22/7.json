    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要派人拿着兵器来摧毁它， 他们要砍下它最好的香柏木， 扔进火里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要指派毀滅者， 各有兵器攻擊你； 他們必砍下你最好的香柏樹， 丟進火裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要預備行毀滅的人， 各拿器械攻擊你； 他們要砍下你佳美的香柏樹， 扔在火中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I will set apart destroyers against you, Each with his weapons; And they will cut down your choicest cedars And throw them on the fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will send destroyers against you, each man with his weapons, and they will cut up your fine cedar beams and throw them into the fire."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will call for wreckers, who will bring out their tools to dismantle you. They will tear out all your fine cedar beams and throw them on the fire."
      }
