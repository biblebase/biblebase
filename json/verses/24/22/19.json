    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.22.13-19",
            "paragraphs": [
              "二二13～19對沙龍之兄長和繼任人約雅敬的譴責。他強迫百姓建造精美華麗的宮殿（13,14節）。耶利米預言約雅敬死後，人們不會照例給他舉哀，他也不會被埋葬，而是被拖到城外，丟棄在垃圾堆中（列王紀下二十四章6節沒有提到\"葬在……\"是值得註意的）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你必像死驴一样被拖到耶路撒冷的城门外，暴尸荒野。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他像驢子一樣被埋葬， 被拖走，拋在耶路撒冷城門外。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他被埋葬，好像埋驢一樣， 要拉出去扔在耶路撒冷的城門之外。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will be buried with a donkey’s burial, Dragged off and thrown out beyond the gates of Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will have the burial of a donkey— dragged away and thrown outside the gates of Jerusalem.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will be buried like a dead donkey— dragged out of Jerusalem and dumped outside the gates!"
      }
    },
