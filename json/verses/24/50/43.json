      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴比伦王闻风丧胆， 四肢发软， 像分娩的妇人一样痛苦不堪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴比倫王聽了有關他們的消息， 手就發軟； 痛苦緊緊抓住他， 他疼痛好像正在生產的婦人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴比倫王聽見他們的風聲， 手就發軟， 痛苦將他抓住， 疼痛彷彿產難的婦人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king of Babylon has heard the report about them, And his hands hang limp; Distress has gripped him, Agony like a woman in childbirth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king of Babylon has heard reports about them, and his hands hang limp. Anguish has gripped him, pain like that of a woman in labor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king of Babylon has heard reports about the enemy, and he is weak with fright. Pangs of anguish have gripped him, like those of a woman in labor."
      }
