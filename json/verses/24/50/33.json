    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华说：“以色列人和犹大人要一同受压迫，掳走他们的人必控制他们，不肯释放他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華這樣說： “以色列人被壓迫， 猶大人也一樣受到欺壓； 所有俘擄他們的都緊緊抓住他們， 不肯釋放他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華如此說：「以色列人和猶大人一同受欺壓；凡擄掠他們的都緊緊抓住他們，不肯釋放。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord of hosts, “The sons of Israel are oppressed, And the sons of Judah as well; And all who took them captive have held them fast, They have refused to let them go."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord Almighty says: “The people of Israel are oppressed, and the people of Judah as well. All their captors hold them fast, refusing to let them go."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord of Heaven’s Armies says: “The people of Israel and Judah have been wronged. Their captors hold them and refuse to let them go."
      }
    },
