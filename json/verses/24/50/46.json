      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因巴比倫被取的聲音，地就震動，人在列邦都聽見呼喊的聲音。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "At the shout, “Babylon has been seized!” the earth is shaken, and an outcry is heard among the nations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At the sound of Babylon’s capture the earth will tremble; its cry will resound among the nations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The earth will shake with the shout, “Babylon has been taken!” and its cry of despair will be heard around the world."
      }
    },
