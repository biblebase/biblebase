    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“主——万军之耶和华说， ‘狂傲的人啊，我要与你为敌， 我惩罚你的时候到了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“傲慢的人哪！我必敵對你 ， 因為你的日子，就是我懲罰你的時候，來到了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主－萬軍之耶和華說： 你這狂傲的啊，我與你反對， 因為我追討你的日子已經來到。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, I am against you, O arrogant one,” Declares the Lord God of hosts, “For your day has come, The time when I will punish you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“See, I am against you, you arrogant one,” declares the Lord, the Lord Almighty, “for your day has come, the time for you to be punished."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“See, I am your enemy, you arrogant people,” says the Lord, the Lord of Heaven’s Armies. “Your day of reckoning has arrived— the day when I will punish you."
      }
    },
