    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "敌人要攻击巴比伦的假先知， 使他们显出愚昧； 敌人要攻击巴比伦的勇士， 使他们充满恐惧；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“刀劍必攻擊胡說八道的假先知， 他們必變成愚昧； 刀劍必攻擊它的勇士， 他們必驚惶失措；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有刀劍臨到矜誇的人， 他們就成為愚昧； 有刀劍臨到她的勇士， 他們就驚惶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A sword against the oracle priests, and they will become fools! A sword against her mighty men, and they will be shattered!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A sword against her false prophets! They will become fools. A sword against her warriors! They will be filled with terror."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The sword will strike her wise counselors, and they will become fools. The sword will strike her mightiest warriors, and panic will seize them."
      }
    },
