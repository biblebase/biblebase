    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "旱灾必临到巴比伦， 使那里的河流干涸， 因为那里偶像遍地， 人们都沉迷于祭拜神像。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "乾旱必攻擊它的河流， 它們必乾涸； 因為這是偶像的地， 人必因可怕的偶像迷失本性。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有乾旱臨到她的眾水， 就必乾涸； 因為這是有雕刻偶像之地， 人因偶像而顛狂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A drought on her waters, and they will be dried up! For it is a land of idols, And they are mad over fearsome idols."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A drought on her waters! They will dry up. For it is a land of idols, idols that will go mad with terror."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A drought will strike her water supply, causing it to dry up. And why? Because the whole land is filled with idols, and the people are madly in love with them."
      }
    },
