    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的勇士为何扑倒？ 他们无法坚立， 因为耶和华击倒了他们，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的壯士為甚麼俯伏？ 他們站立不住，是因耶和華把他們推倒了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的壯士為何被沖去呢？ 他們站立不住； 因為耶和華驅逐他們，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Why are your mighty ones face down? They do not stand because the Lord thrust them down."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why have your mighty ones become prostrate? They do not stand because the Lord has thrust them down."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why will your warriors be laid low? They cannot stand, for the Lord will push them down."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Why are your valiant men swept away? They did not stand Because the Lord drove them away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why have your warriors fallen? They cannot stand, for the Lord has knocked them down."
      }
    },
