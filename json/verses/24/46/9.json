    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.46.9",
            "paragraphs": [
              "四六9\"古實人\"（從尼羅河上遊而來）和\"弗人\"（或呂彼亞人，從非洲北面海岸而來），以及\"路德族\"人（小亞細亞的居民）被埃及人聘用為雇傭兵。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "战马奔驰吧！ 战车疾驶吧！ 勇士啊，就是那些手拿盾牌的古实人和弗人以及箭法娴熟的吕底亚人啊，出战吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "馬匹啊，上前吧！ 戰車啊，狂奔吧！ 勇士啊，就是緊握盾牌的古實人和弗人， 以及拉弓的呂底亞人哪，上陣吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "馬匹上去吧！ 車輛急行吧！ 勇士，就是手拿盾牌的古實人和弗人， 並拉弓的路德族，都出去吧！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Go up, you horses, and drive madly, you chariots, That the mighty men may march forward: Ethiopia and Put, that handle the shield, And the Lydians, that handle and bend the bow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Charge, you horses! Drive furiously, you charioteers! March on, you warriors—men of Cush and Put who carry shields, men of Lydia who draw the bow."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Charge, you horses and chariots; attack, you mighty warriors of Egypt! Come, all you allies from Ethiopia, Libya, and Lydia who are skilled with the shield and bow!"
      }
    },
