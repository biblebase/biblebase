    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.46.7-8",
            "paragraphs": [
              "四六7,8尼羅河的水會周期性地溢出兩岸；埃及也效法尼羅河（\"漲發\"即泛濫）那樣去征服更多的領土。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "埃及如尼罗河涨溢， 如江水奔腾。 她说，‘我要涨溢，淹没大地， 毁灭城邑和其中的居民。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "埃及像尼羅河上漲， 像江河的水翻騰； 它說：‘我要上漲，淹蓋大地； 我要毀滅城鎮和城裡的居民。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "埃及像尼羅河漲發， 像江河的水翻騰。 他說：我要漲發遮蓋遍地； 我要毀滅城邑和其中的居民。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Egypt rises like the Nile, like rivers whose waters surge. He said, ‘I will rise, I will cover the earth, I will destroy cities and their inhabitants.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Egypt rises like the Nile, Even like the rivers whose waters surge about; And He has said, “I will rise and cover that land; I will surely destroy the city and its inhabitants.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Egypt rises like the Nile, like rivers of surging waters. She says, ‘I will rise and cover the earth; I will destroy cities and their people.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Egypt rises up like a flood, And its waters move like the rivers; And he says, ‘I will go up and cover the earth, I will destroy the city and its inhabitants.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is the Egyptian army, overflowing all the land, boasting that it will cover the earth like a flood, destroying cities and their people."
      }
    },
