    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.42.1-6",
            "paragraphs": [
              "四二1～6眾人求問耶利米，看神是否贊成他們移居埃及的計劃。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿你的上帝耶和华指示我们该去哪里，该做什么。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "請為我們求耶和華你的　神指示我們當走的路和該作的事。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願耶和華－你的神指示我們所當走的路，所當做的事。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that the Lord your God may tell us the way in which we should walk and the thing that we should do.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Pray that the Lord your God will tell us where we should go and what we should do.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Pray that the Lord your God will show us what to do and where to go.”"
      }
