    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.42.1-6",
            "paragraphs": [
              "四二1～6眾人求問耶利米，看神是否贊成他們移居埃及的計劃。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对耶利米先知说：“求你答应我们的恳求，为我们这些余民向我们的上帝耶和华祈求。你也看见了，我们本来人数众多，现在却寥寥无几。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對耶利米先知說：“求你容我們向你懇求，請為我們這餘剩的民向耶和華你的　神禱告；因為我們從前人數眾多，現在剩下的極少，就像你現在親眼所見的一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對先知耶利米說：「求你准我們在你面前祈求，為我們這剩下的人禱告耶和華－你的神。我們本來眾多，現在剩下的極少，這是你親眼所見的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and said to Jeremiah the prophet, “Please let our petition come before you, and pray for us to the Lord your God, that is for all this remnant; because we are left but a few out of many, as your own eyes now see us,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jeremiah the prophet and said to him, “Please hear our petition and pray to the Lord your God for this entire remnant. For as you now see, though we were once many, now only a few are left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jeremiah the prophet. They said, “Please pray to the Lord your God for us. As you can see, we are only a tiny remnant compared to what we were before."
      }
    },
