    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.42.1-6",
            "paragraphs": [
              "四二1～6眾人求問耶利米，看神是否贊成他們移居埃及的計劃。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们说：“倘若我们不照你的上帝耶和华说的去做，愿诚实可靠的耶和华作证指控我们！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他們對耶利米說：“我們若不照著耶和華你的　神差遣你到我們這裡來所說的一切話去作，願耶和華作真實和誠信的見證，指控我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是他們對耶利米說：「我們若不照耶和華－你的神差遣你來說的一切話行，願耶和華在我們中間作真實誠信的見證。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they said to Jeremiah, “May the Lord be a true and faithful witness against us if we do not act in accordance with the whole message with which the Lord your God will send you to us."
