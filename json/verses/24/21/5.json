    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.21.5",
            "paragraphs": [
              "21:5",
              "親自攻擊你們：西底家王打發兩位祭司去找耶利米，詢問神是否會使尼布甲尼撒王撤回(1~3節)。神借著先知有力地宣告說他絕對“不會”如此行。事實上，神將親自攻擊猶大，將民眾交在仇敵的手中。他們的一切抵抗都是徒勞無益的。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要在烈怒中伸出大能的臂膀下手击打你们，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必在怒氣、烈怒和忿怒中，用伸出來的手和強有力的膀臂，親自攻擊你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且我要在怒氣、忿怒，和大惱恨中，用伸出來的手，並大能的膀臂，親自攻擊你們；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I myself will fight against you with outstretched hand and strong arm, in anger and in fury and in great wrath."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I Myself will war against you with an outstretched hand and a mighty arm, even in anger and wrath and great indignation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I myself will fight against you with an outstretched hand and a mighty arm in furious anger and in great wrath."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I Myself will fight against you with an outstretched hand and with a strong arm, even in anger and fury and great wrath."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I myself will fight against you with a strong hand and a powerful arm, for I am very angry. You have made me furious!"
      }
    },
