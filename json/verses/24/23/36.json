    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.23.18-40",
            "paragraphs": [
              "二三18～40這段指出真先知的生命（18節）與信息（28節）跟假先知（32節）有所不同。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们不可再说有耶和华的启示，因为你们把自己的话当成祂的启示，曲解你们永活的上帝——万军之耶和华的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不可再提耶和華的默示，你們各人所說的話將成為自己的重擔；因為你們曲解永活的　神萬軍之耶和華我們　神的話了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "『耶和華的默示』你們不可再提，各人所說的話必作自己的重擔，因為你們謬用永生神、萬軍之耶和華－我們神的言語。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But ‘the burden of the Lord’ you shall mention no more, for the burden is every man’s own word, and you pervert the words of the living God, the Lord of hosts, our God."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For you will no longer remember the oracle of the Lord, because every man’s own word will become the oracle, and you have perverted the words of the living God, the Lord of hosts, our God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you must not mention ‘a message from the Lord’ again, because each one’s word becomes their own message. So you distort the words of the living God, the Lord Almighty, our God."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the oracle of the Lord you shall mention no more. For every man’s word will be his oracle, for you have perverted the words of the living God, the Lord of hosts, our God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But stop using this phrase, ‘prophecy from the Lord.’ For people are using it to give authority to their own ideas, turning upside down the words of our God, the living God, the Lord of Heaven’s Armies."
      }
    },
