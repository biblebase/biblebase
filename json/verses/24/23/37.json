    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.23.18-40",
            "paragraphs": [
              "二三18～40這段指出真先知的生命（18節）與信息（28節）跟假先知（32節）有所不同。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米啊，你要这样问先知，‘耶和华有什么答复？耶和华说了什么？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要對　神的先知說：“耶和華回答了你甚麼？耶和華說了甚麼呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要對先知如此說：『耶和華回答你甚麼？』『耶和華說了甚麼呢？』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus you will say to that prophet, ‘What has the Lord answered you?’ and, ‘What has the Lord spoken?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what you keep saying to a prophet: ‘What is the Lord’s answer to you?’ or ‘What has the Lord spoken?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what you should say to the prophets: ‘What is the Lord’s answer?’ or ‘What is the Lord saying?’"
      }
    },
