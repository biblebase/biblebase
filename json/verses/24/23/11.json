    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.23.10-15",
            "paragraphs": [
              "二三10～15這段描述假先知邪惡的行為。\"因妄自賭咒\"。更可作：由於咒詛（因他們行淫引致從耶和華而來的咒詛）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“先知和祭司都不敬虔， 我甚至看见他们在我的圣殿里行恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“先知和祭司都不敬虔， 甚至在我殿裡我也看見他們的邪惡。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "連先知帶祭司都是褻瀆的， 就是在我殿中我也看見他們的惡。 這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“For both prophet and priest are polluted; Even in My house I have found their wickedness,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Both prophet and priest are godless; even in my temple I find their wickedness,” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Even the priests and prophets are ungodly, wicked men. I have seen their despicable acts right here in my own Temple,” says the Lord."
      }
    },
