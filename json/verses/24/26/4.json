    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要告诉他们，耶和华这样说，“如果你们不听从我的话，不遵行我赐给你们的律法，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要對他們說：耶和華這樣說：如果你們不聽從我，不遵行我設立在你們面前的律法，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要對他們說，耶和華如此說：『你們若不聽從我，不遵行我設立在你們面前的律法，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You shall say to them, ‘Thus says the Lord: If you will not listen to me, to walk in my law that I have set before you,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And you will say to them, ‘Thus says the Lord, “If you will not listen to Me, to walk in My law which I have set before you,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Say to them, ‘This is what the Lord says: If you do not listen to me and follow my law, which I have set before you,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And you shall say to them, ‘Thus says the Lord: “If you will not listen to Me, to walk in My law which I have set before you,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Say to them, ‘This is what the Lord says: If you will not listen to me and obey my word I have given you,"
      }
