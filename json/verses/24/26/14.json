    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.26.12-15",
            "paragraphs": [
              "二六12～15耶利米的自辯是：神差遣了他；他們仍可悔改；而他們若是殺了他，便是殺了一個無辜的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "至于我，我已落在你们手中，你们认为怎样好就怎样待我吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於我，我在你們手中；你們看怎樣好，怎樣對，就怎樣待我吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於我，我在你們手中，你們眼看何為善，何為正，就那樣待我吧！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, behold, I am in your hands; do with me as is good and right in your sight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As for me, I am in your hands; do with me whatever you think is good and right."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As for me, I am in your hands—do with me as you think best."
      }
