    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，我要从北方， 从地极招聚他们， 其中有瞎子、跛子、孕妇和产妇， 他们要成群结队地回到这里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！我必把他們從北方之地領回來， 把他們從地極招集起來； 在他們中間有瞎眼的、瘸腿的、 孕婦、產婦，都在一起； 他們成群結隊返回這裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必將他們從北方領來， 從地極招聚； 同着他們來的有瞎子、瘸子、孕婦、產婦； 他們必成為大幫回到這裏來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I am bringing them from the north country, And I will gather them from the remote parts of the earth, Among them the blind and the lame, The woman with child and she who is in labor with child, together; A great company, they will return here."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See, I will bring them from the land of the north and gather them from the ends of the earth. Among them will be the blind and the lame, expectant mothers and women in labor; a great throng will return."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I will bring them from the north and from the distant corners of the earth. I will not forget the blind and lame, the expectant mothers and women in labor. A great company will return!"
      }
    },
