    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.31.38",
            "paragraphs": [
              "31:38",
              "這城：耶利米預見到耶路撒冷將被重新建造，不再傾覆(40節)。當基督在地上執掌王權時，這個預言就要完全應驗(啟19~22章)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.31.38",
            "paragraphs": [
              "三一38\"哈楠業樓\"。位於耶路撒冷的東北角。\"角門\"。位於西北角（尼三1；一二39；亞一四10）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“看啊，时候将到，从哈楠业楼直到角门，整个耶路撒冷城要重建起来归给耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪！日子快到，這城從哈楠業城樓直到角門，必為耶和華重建起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「日子將到，這城必為耶和華建造，從哈楠業樓直到角門。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, days are coming,” declares the Lord, “when the city will be rebuilt for the Lord from the Tower of Hananel to the Corner Gate."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The days are coming,” declares the Lord, “when this city will be rebuilt for me from the Tower of Hananel to the Corner Gate."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The day is coming,” says the Lord, “when all Jerusalem will be rebuilt for me, from the Tower of Hananel to the Corner Gate."
      }
    },
