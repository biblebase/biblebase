    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.31.29-30",
            "paragraphs": [
              "三一29,30百姓為自己辯解說他們是因祖先的罪而受罰；事實上，每個人都是為自己的罪而受罰（比較申二四16；結一八2～4）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时，人再不会说， “‘父亲吃酸葡萄， 儿子的牙酸倒了。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“在那些日子，人必不再說： ‘父親吃了酸葡萄， 兒子的牙齒酸倒了。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當那些日子，人不再說： 父親吃了酸葡萄， 兒子的牙酸倒了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“In those days they will not say again, ‘The fathers have eaten sour grapes, And the children’s teeth are set on edge.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“In those days people will no longer say, ‘The parents have eaten sour grapes, and the children’s teeth are set on edge.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The people will no longer quote this proverb: ‘The parents have eaten sour grapes, but their children’s mouths pucker at the taste.’"
      }
    },
