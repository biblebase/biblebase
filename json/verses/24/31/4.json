      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人啊， 我要重建你们的家园， 你们要再次拿起铃鼓与欢乐的人一同跳舞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "童女以色列啊！ 我要再次建立你，你就必被建立。 你要再次拿起鈴鼓， 出去和那些歡樂的人一同跳舞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列的民哪， 我要再建立你，你就被建立； 你必再以擊鼓為美， 與歡樂的人一同跳舞而出；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Again I will build you and you will be rebuilt, O virgin of Israel! Again you will take up your tambourines, And go forth to the dances of the merrymakers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will build you up again, and you, Virgin Israel, will be rebuilt. Again you will take up your timbrels and go out to dance with the joyful."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will rebuild you, my virgin Israel. You will again be happy and dance merrily with your tambourines."
      }
    },
