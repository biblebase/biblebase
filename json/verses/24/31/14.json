    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使祭司有充足的供应， 使我的子民饱享美物。 这是耶和华说的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必用肥脂使祭司的心滿足； 我的子民必因我的美物飽足。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必以肥油使祭司的心滿足； 我的百姓也要因我的恩惠知足。 這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will fill the soul of the priests with abundance, And My people will be satisfied with My goodness,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will satisfy the priests with abundance, and my people will be filled with my bounty,” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The priests will enjoy abundance, and my people will feast on my good gifts. I, the Lord, have spoken!”"
      }
    },
