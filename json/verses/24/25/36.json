    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听听牧人的哭喊， 百姓首领的哀号吧， 因为耶和华正在毁灭他们的家园。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "聽啊！牧人的哀叫， 羊群的領袖的悲號， 因為耶和華正在毀壞他們的牧場。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "聽啊，有牧人呼喊， 有羣眾頭目哀號的聲音， 因為耶和華使他們的草場變為荒場。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hear the sound of the cry of the shepherds, And the wailing of the masters of the flock! For the Lord is destroying their pasture,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hear the cry of the shepherds, the wailing of the leaders of the flock, for the Lord is destroying their pasture."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen to the frantic cries of the shepherds. The leaders of the flock are wailing in despair, for the Lord is ruining their pastures."
      }
    },
