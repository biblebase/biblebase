    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们这群牧人啊，要悲哀哭泣！ 你们这群百姓的首领啊， 要在灰尘中伤心地打滚！ 因为你们被宰杀、驱散的时候到了， 你们将像精美的器皿一样被摔得粉碎。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牧人啊，你們要哀號，呼叫！ 羊群的領袖啊，你們要在灰塵中打滾！ 因為你們被宰殺的日子已經到了； 我要粉碎你們， 你們要跌倒，像珍貴的器皿跌碎一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牧人哪，你們當哀號，呼喊； 羣眾的頭目啊，你們要滾在灰中； 因為你們被殺戮分散的日子足足來到。 你們要跌碎，好像美器打碎一樣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Wail, you shepherds, and cry out, and roll in ashes, you lords of the flock, for the days of your slaughter and dispersion have come, and you shall fall like a choice vessel."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Wail, you shepherds, and cry; And wallow in ashes, you masters of the flock; For the days of your slaughter and your dispersions have come, And you will fall like a choice vessel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Weep and wail, you shepherds; roll in the dust, you leaders of the flock. For your time to be slaughtered has come; you will fall like the best of the rams."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Wail, shepherds, and cry! Roll about in  the  ashes, You leaders of the flock! For the days of your slaughter and your dispersions are fulfilled; You shall fall like a precious vessel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Weep and moan, you evil shepherds! Roll in the dust, you leaders of the flock! The time of your slaughter has arrived; you will fall and shatter like a fragile vase."
      }
    },
