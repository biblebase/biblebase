    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使他们的欢乐声、新郎新娘的欢笑声、推磨声和灯光都消失。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我要從他們中間除掉歡喜和快樂的聲音，新郎和新婦的聲音，推磨的聲音和燈的亮光。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我又要使歡喜和快樂的聲音，新郎和新婦的聲音，推磨的聲音和燈的亮光，從他們中間止息。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, I will take from them the voice of joy and the voice of gladness, the voice of the bridegroom and the voice of the bride, the sound of the millstones and the light of the lamp."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will banish from them the sounds of joy and gladness, the voices of bride and bridegroom, the sound of millstones and the light of the lamp."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will take away your happy singing and laughter. The joyful voices of bridegrooms and brides will no longer be heard. Your millstones will fall silent, and the lights in your homes will go out."
      }
    },
