    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们喝了这杯烈怒后必东倒西歪、行为疯狂，因为我要使战祸临到他们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們喝了，就會東倒西歪，瘋狂亂性，因為我打發刀劍到他們中間去。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們喝了就要東倒西歪，並要發狂，因我使刀劍臨到他們中間。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will drink and stagger and go mad because of the sword that I will send among them.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they drink it, they will stagger and go mad because of the sword I will send among them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they drink from it, they will stagger, crazed by the warfare I will send against them.”"
      }
    },
