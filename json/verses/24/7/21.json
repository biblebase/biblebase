    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.7.21",
            "paragraphs": [
              "七21\"燔祭\"。雖然燔祭的祭牲要全燒在壇上（利一9），但百姓可能也應吃祭肉，因為在他們信心倒退的狀況下，所獻的祭對神來說並沒有意義。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列的上帝——万军之耶和华说：“你们把平安祭连同燔祭都拿去吃吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華以色列的　神這樣說：“你們儘管把燔祭加在平安祭上，自己吃祭肉吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華－以色列的神如此說：「你們將燔祭加在平安祭上，吃肉吧！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord of hosts, the God of Israel, “Add your burnt offerings to your sacrifices and eat flesh."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘This is what the Lord Almighty, the God of Israel, says: Go ahead, add your burnt offerings to your other sacrifices and eat the meat yourselves!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord of Heaven’s Armies, the God of Israel, says: “Take your burnt offerings and your other sacrifices and eat them yourselves!"
      }
    },
