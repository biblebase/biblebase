    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "jer.7.33-34",
            "paragraphs": [
              "33～34.耶利米鮮明地概述猶大將受的可怕審判。因為屍首無處可葬，所以便會招來吃腐肉的鳥和小動物。這對古時的猶太人來說，是不可言喻的恐怖。諷刺的是，當親愛的家園慘遭蹂躪時，他們的邱壇將變成他們的墓地。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.7.33",
            "paragraphs": [
              "七33\"哄趕\"指嚇走。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "飞鸟和走兽必吃这百姓的尸体，无人驱赶它们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“這人民的屍體，也必給空中的飛鳥和地上的野獸作食物，沒有人來驅趕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且這百姓的屍首必給空中的飛鳥和地上的野獸作食物，並無人鬨趕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The dead bodies of this people will be food for the birds of the sky and for the beasts of the earth; and no one will frighten them away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the carcasses of this people will become food for the birds and the wild animals, and there will be no one to frighten them away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The bodies of my people will be food for the vultures and wild animals, and no one will be left to scare them away."
      }
    },
