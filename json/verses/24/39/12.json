    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你好好照顾耶利米，切不可伤害他。他有什么要求，你都要满足。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你把他帶去，好好照顧他，切不可傷害他；他對你怎樣說，你就給他照辦。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你領他去，好好地看待他，切不可害他；他對你怎麼說，你就向他怎麼行。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Take him and look after him, and do nothing harmful to him, but rather deal with him just as he tells you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Take him and look after him; don’t harm him but do for him whatever he asks.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“See that he isn’t hurt,” he said. “Look after him well, and give him anything he wants.”"
      }
    },
