    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，护卫长尼布撒拉旦、尼布沙斯班、尼甲·沙利薛等巴比伦王的所有将领，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是護衛長尼布撒拉旦、大臣尼布沙斯班、術士長尼甲．沙利薛，以及巴比倫王所有的大臣都派人去，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "護衛長尼布撒拉旦和尼布沙斯班－拉撒力、尼甲‧沙利薛－拉墨，並巴比倫王的一切官長，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So Nebuzaradan the captain of the guard, Nebushazban the Rab-saris, Nergal-sar-ezer the Rab-mag, and all the chief officers of the king of Babylon"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Nebuzaradan the captain of the bodyguard sent word, along with Nebushazban the Rab-saris, and Nergal-sar-ezer the Rab-mag, and all the leading officers of the king of Babylon;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Nebuzaradan the commander of the guard, Nebushazban a chief officer, Nergal-Sharezer a high official and all the other officers of the king of Babylon"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So Nebuzaradan the captain of the guard sent Nebushasban, Rabsaris, Nergal-Sharezer, Rabmag, and all the king of Babylon’s chief officers;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Nebuzaradan, the captain of the guard; Nebushazban, a chief officer; Nergal-sharezer, the king’s adviser; and the other officers of Babylon’s king"
      }
    },
