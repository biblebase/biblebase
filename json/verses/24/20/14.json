    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.20.14",
            "paragraphs": [
              "20:14",
              "14~18那日受咒詛：耶利米所經歷的諸多痛苦、挫折和失敗使他寧願自己未生在世上(比較伯3章)。盡管如此，他仍然堅持不懈地宣講神的話語，在最黑暗的時候得到神恩惠的扶持(11~13節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿我出生的那天受咒诅！ 愿我母亲生我的那天不蒙祝福！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願我生的那一天受咒詛！ 願我母親生我的那一天不蒙祝福！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願我生的那日受咒詛； 願我母親產我的那日不蒙福！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Cursed be the day on which I was born! The day when my mother bore me, let it not be blessed!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Cursed be the day when I was born; Let the day not be blessed when my mother bore me!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Cursed be the day I was born! May the day my mother bore me not be blessed!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Cursed be the day in which I was born! Let the day not be blessed in which my mother bore me!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet I curse the day I was born! May no one celebrate the day of my birth."
      }
    },
