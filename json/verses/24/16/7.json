    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.16.1-9",
            "paragraphs": [
              "一六1～9耶利米被禁止結婚（2節），也不能參加喪禮（5節）和快樂的宴會（8節），以此為即將來臨之被擄的強烈警告。"
            ]
          },
          {
            "verse": "jer.16.7",
            "paragraphs": [
              "一六7\"為他們掰餅\"。一種風俗，朋友會在喪禮之後預備一頓飯。這些禁令怎樣影響先知敏感的心靈，很難想象。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无人用食物和酒安慰哀悼者，甚至无人给丧父或丧母者一杯安慰酒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有人為哀悼的人擘餅，為死者安慰他們；喪父或喪母的人，都沒有人給他們喝一杯安慰酒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們有喪事，人必不為他們擘餅，因死人安慰他們；他們喪父喪母，人也不給他們一杯酒安慰他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Men will not break bread in mourning for them, to comfort anyone for the dead, nor give them a cup of consolation to drink for anyone’s father or mother."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one will offer food to comfort those who mourn for the dead—not even for a father or a mother—nor will anyone give them a drink to console them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No one will offer a meal to comfort those who mourn for the dead—not even at the death of a mother or father. No one will send a cup of wine to console them."
      }
    },
