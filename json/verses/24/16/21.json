    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“看啊，这一次我要让他们知道我的权柄和能力，让他们知道我是耶和华。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“因此，看哪！我要使他們知道， 這一次我必叫他們認識 我的手和我的能力， 他們就知道我的名是耶和華。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「我要使他們知道，就是這一次使他們知道我的手和我的能力。他們就知道我的名是耶和華了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Therefore behold, I am going to make them know— This time I will make them know My power and My might; And they shall know that My name is the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Therefore I will teach them— this time I will teach them my power and might. Then they will know that my name is the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord says, “Now I will show them my power; now I will show them my might. At last they will know and understand that I am the Lord."
      }
