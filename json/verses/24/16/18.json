    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.16.18",
            "paragraphs": [
              "16:18",
              "加倍報應：該處譯為“加倍”的希伯來詞語也含有“相當”的意思。神將按照神子民的罪孽和惡行足足地報應他們。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "首先，我要让他们因自己的罪与恶而受加倍的报应，因为他们用没有生命气息的可憎偶像玷污我的土地，使可厌之物充满我的产业。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "首先我要加倍報應他們的罪孽和罪惡，因為他們用自己那些毫無生氣、可憎的偶像玷污了我的土地，他們那些可厭之物充滿了我的產業。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我先要加倍報應他們的罪孽和罪惡；因為他們用可憎之屍玷污我的地土，又用可厭之物充滿我的產業。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will first doubly repay their iniquity and their sin, because they have polluted My land; they have filled My inheritance with the carcasses of their detestable idols and with their abominations.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will repay them double for their wickedness and their sin, because they have defiled my land with the lifeless forms of their vile images and have filled my inheritance with their detestable idols.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will double their punishment for all their sins, because they have defiled my land with lifeless images of their detestable gods and have filled my territory with their evil deeds.”"
      }
    },
