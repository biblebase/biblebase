    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是，你们的行为比他们更邪恶，一意孤行，不听从我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於你們，你們作壞事比你們的列祖更厲害；看哪！你們各人都隨從自己頑梗的惡心行事，不聽從我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "而且你們行惡比你們列祖更甚；因為各人隨從自己頑梗的惡心行事，甚至不聽從我，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You too have done evil, even more than your forefathers; for behold, you are each one walking according to the stubbornness of his own evil heart, without listening to Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you have behaved more wickedly than your ancestors. See how all of you are following the stubbornness of your evil hearts instead of obeying me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And you are even worse than your ancestors! You stubbornly follow your own evil desires and refuse to listen to me."
      }
    },
