    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.33.6-13",
            "paragraphs": [
              "三三6～13神再次應許要復興祂的民和耶路撒冷。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要除去他们的一切罪恶，赦免他们的一切罪过。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必潔淨他們得罪我的一切罪孽，並且赦免他們得罪我和悖逆我的一切罪孽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要除淨他們的一切罪，就是向我所犯的罪；又要赦免他們的一切罪，就是干犯我、違背我的罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will cleanse them from all their iniquity by which they have sinned against Me, and I will pardon all their iniquities by which they have sinned against Me and by which they have transgressed against Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will cleanse them from all the sin they have committed against me and will forgive all their sins of rebellion against me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will cleanse them of their sins against me and forgive all their sins of rebellion."
      }
    },
