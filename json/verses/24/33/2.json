    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华创造大地，使其成形、根基稳固，祂名叫耶和华。祂说，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“那創造、塑造和堅定大地的是耶和華，耶和華是他的名。他這樣說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「成就的是耶和華，造作、為要建立的也是耶和華；耶和華是他的名。他如此說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Thus says the Lord who made the earth, the Lord who formed it to establish it, the Lord is His name,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Lord says, he who made the earth, the Lord who formed it and established it—the Lord is his name:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Lord says—the Lord who made the earth, who formed and established it, whose name is the Lord:"
      }
    },
