    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.33.6-13",
            "paragraphs": [
              "三三6～13神再次應許要復興祂的民和耶路撒冷。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华说：“在这一片荒凉、人畜绝迹的地方，在它所有城邑的周围必再有供牧人放羊的草场。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“萬軍之耶和華這樣說：‘在這荒廢、沒有人煙、沒有牲畜的地方，和這地所有城中，必再有牧人放羊的牧場。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華如此說：「在這荒廢無人民無牲畜之地，並其中所有的城邑，必再有牧人的住處；他們要使羊羣躺臥在那裏。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Thus says the Lord of hosts: In this place that is waste, without man or beast, and in all of its cities, there shall again be habitations of shepherds resting their flocks."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Thus says the Lord of hosts, ‘There will again be in this place which is waste, without man or beast, and in all its cities, a habitation of shepherds who rest their flocks."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Lord Almighty says: ‘In this place, desolate and without people or animals—in all its towns there will again be pastures for shepherds to rest their flocks."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Thus says the Lord of hosts: ‘In this place which is desolate, without man and without beast, and in all its cities, there shall again be a dwelling place of shepherds causing their flocks to lie down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Lord of Heaven’s Armies says: This land—though it is now desolate and has no people and animals—will once more have pastures where shepherds can lead their flocks."
      }
    },
