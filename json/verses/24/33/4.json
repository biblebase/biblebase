    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列的上帝耶和华说，耶路撒冷城中的房屋和王宫被拆毁，用来修建防卫工事，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "論到這城裡的房屋和猶大王的宮殿，就是為了抵擋敵人的攻城土壘和刀劍而拆毀的，耶和華以色列的　神這樣說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論到這城中的房屋和猶大王的宮室，就是拆毀為擋敵人高壘和刀劍的，耶和華－以色列的神如此說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus says the Lord God of Israel concerning the houses of this city, and concerning the houses of the kings of Judah which are broken down to make a defense against the siege ramps and against the sword,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this is what the Lord, the God of Israel, says about the houses in this city and the royal palaces of Judah that have been torn down to be used against the siege ramps and the sword"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For this is what the Lord, the God of Israel, says: You have torn down the houses of this city and even the king’s palace to get materials to strengthen the walls against the siege ramps and swords of the enemy."
      }
    },
