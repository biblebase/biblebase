    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你公正地惩罚我， 不要带着怒气惩罚我， 否则我将不复存在。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你只按著公正懲治我， 不要按著你的忿怒施行懲治，免得你使我衰微。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你從寬懲治我， 不要在你的怒中懲治我， 恐怕使我歸於無有。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Correct me, O Lord, but with justice; Not with Your anger, or You will bring me to nothing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Discipline me, Lord, but only in due measure— not in your anger, or you will reduce me to nothing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So correct me, Lord, but please be gentle. Do not correct me in anger, for I would die."
      }
