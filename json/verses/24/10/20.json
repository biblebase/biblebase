    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的帐篷已毁， 绳索已断； 我的儿女都离我而去， 再没有人为我支搭帐篷， 挂上幔子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的帳棚毀壞， 我的繩索都折斷了； 我的兒女都離我而去，不在這裡了； 再沒有人支搭我的帳棚， 掛起我的幔子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的帳棚毀壞； 我的繩索折斷。 我的兒女離我出去，沒有了。 無人再支搭我的帳棚，掛起我的幔子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My tent is destroyed, And all my ropes are broken; My sons have gone from me and are no more. There is no one to stretch out my tent again Or to set up my curtains."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My tent is destroyed; all its ropes are snapped. My children are gone from me and are no more; no one is left now to pitch my tent or to set up my shelter."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My home is gone, and no one is left to help me rebuild it. My children have been taken away, and I will never see them again."
      }
    },
