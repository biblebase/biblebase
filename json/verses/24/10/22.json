    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听啊，有消息传来， 喧嚣的敌军从北方冲来， 要使犹大的城邑荒凉， 沦为豺狼的巢穴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "聽哪！有風聲傳來， 有大騷動從北方之地而來， 要使猶大的城市荒涼， 成為野狗的巢穴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有風聲！看哪，敵人來了！ 有大擾亂從北方出來， 要使猶大城邑變為荒涼， 成為野狗的住處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sound of a report! Behold, it comes— A great commotion out of the land of the north— To make the cities of Judah A desolation, a haunt of jackals."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Listen! The report is coming— a great commotion from the land of the north! It will make the towns of Judah desolate, a haunt of jackals."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen! Hear the terrifying roar of great armies as they roll down from the north. The towns of Judah will be destroyed and become a haunt for jackals."
      }
    },
