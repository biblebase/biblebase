      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要这样对他们说：“那些神明没有创造天地，它们将从天下消亡。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要對他們這樣說：“那些沒有創造天地的神，必從地上、從天下消滅。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要對他們如此說：不是那創造天地的神，必從地上從天下被除滅！）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus you shall say to them, “The gods that did not make the heavens and the earth will perish from the earth and from under the heavens.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Tell them this: ‘These gods, who did not make the heavens and the earth, will perish from the earth and from under the heavens.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Say this to those who worship other gods: “Your so-called gods, who did not make the heavens and earth, will vanish from the earth and from under the heavens.”"
      }
