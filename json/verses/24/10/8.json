    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们都愚昧无知， 毫无用处的木制偶像能教导他们什么呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們全是頑梗愚昧， 他們所領受的教導是來自那些虛無、 木做的偶像。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們盡都是畜類，是愚昧的。 偶像的訓誨算甚麼呢？ 偶像不過是木頭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But they are altogether stupid and foolish In their discipline of delusion—their idol is wood!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are all senseless and foolish; they are taught by worthless wooden idols."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People who worship idols are stupid and foolish. The things they worship are made of wood!"
      }
