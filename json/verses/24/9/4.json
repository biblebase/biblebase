    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要提防邻居， 不要信赖弟兄， 因为弟兄诡计多端， 邻居满口是非。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們各人要提防自己的鄰舍， 不要信靠兄弟； 因為每一個兄弟都在欺騙人， 每一個鄰舍都搬弄是非。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們各人當謹防鄰舍， 不可信靠弟兄； 因為弟兄盡行欺騙， 鄰舍都往來讒謗人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Let everyone be on guard against his neighbor, And do not trust any brother; Because every brother deals craftily, And every neighbor goes about as a slanderer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Beware of your friends; do not trust anyone in your clan. For every one of them is a deceiver, and every friend a slanderer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Beware of your neighbor! Don’t even trust your brother! For brother takes advantage of brother, and friend slanders friend."
      }
    },
