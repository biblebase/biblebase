    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们彼此欺骗，不说真话； 他们说谎成性， 不知疲倦地肆意犯罪；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們各人都欺騙自己的鄰舍， 不說誠實話。 他們訓練自己的舌頭說謊話， 竭力作孽，以致他們不能回轉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們各人欺哄鄰舍，不說真話； 他們教舌頭學習說謊， 勞勞碌碌地作孽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Everyone deceives his neighbor And does not speak the truth, They have taught their tongue to speak lies; They weary themselves committing iniquity."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Friend deceives friend, and no one speaks the truth. They have taught their tongues to lie; they weary themselves with sinning."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They all fool and defraud each other; no one tells the truth. With practiced tongues they tell lies; they wear themselves out with all their sinning."
      }
    },
