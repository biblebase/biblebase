    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华说： “把陪哭的妇人请来， 把最会哭的妇人带来，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華這樣說： “你們要思想， 要把唱哀歌的婦女召來， 把那些擅長哀哭的婦女請來；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華如此說： 你們應當思想， 將善唱哀歌的婦女召來， 又打發人召善哭的婦女來，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord of hosts, “Consider and call for the mourning women, that they may come; And send for the wailing women, that they may come!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord Almighty says: “Consider now! Call for the wailing women to come; send for the most skillful of them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord of Heaven’s Armies says: “Consider all this, and call for the mourners. Send for the women who mourn at funerals."
      }
    },
