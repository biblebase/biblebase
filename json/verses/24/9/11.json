    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“我要使耶路撒冷沦为废墟， 成为豺狼出没之地； 我要使犹大的城邑荒凉， 不见人烟。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我要使耶路撒冷變成亂堆， 成為野狗的巢穴； 我要使猶大的城市荒涼， 無人居住。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必使耶路撒冷變為亂堆，為野狗的住處， 也必使猶大的城邑變為荒場，無人居住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will make Jerusalem a heap of ruins, A haunt of jackals; And I will make the cities of Judah a desolation, without inhabitant.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will make Jerusalem a heap of ruins, a haunt of jackals; and I will lay waste the towns of Judah so no one can live there.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I will make Jerusalem into a heap of ruins,” says the Lord. “It will be a place haunted by jackals. The towns of Judah will be ghost towns, with no one living in them.”"
      }
    },
