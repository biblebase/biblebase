      "leili": {
        "content": [
          {
            "verse": "jer.34.20",
            "paragraphs": [
              "三四20所有違約不讓奴隸自由出去的人將要死。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要把他们交在想杀他们的敌人手中，他们的尸体要成为飞禽走兽的食物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必把他們交在他們仇敵的手中，和那些尋索他們性命的人的手中；他們的屍體必給空中的飛鳥和地上的走獸作食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必將他們交在仇敵和尋索其命的人手中；他們的屍首必給空中的飛鳥和地上的野獸作食物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will give them into the hand of their enemies and into the hand of those who seek their life. And their dead bodies will be food for the birds of the sky and the beasts of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will deliver into the hands of their enemies who want to kill them. Their dead bodies will become food for the birds and the wild animals."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will give you to your enemies, and they will kill you. Your bodies will be food for the vultures and wild animals."
      }
    },
