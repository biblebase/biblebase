    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "jer.2.14",
            "paragraphs": [
              "14.眼看著北國突然面臨的下場，耶利米指出生而自由的以色列人快要變為奴仆。他感到不解，如果以色列真的屬神，為何任亞述這只貪婪的獅子擺布如此之久？家中生的奴仆，意指在主人家中誕生的仆人，是主人自己的產業，因此有別於購買而得的仆人。41"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.2.14",
            "paragraphs": [
              "二14\"家中生的奴仆\"。一個生在主人家裏的奴仆，有別於買回來的奴仆。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以色列是奴隶吗？ 是生来为奴的吗？ 为什么她竟成了别人的猎物呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“以色列豈是奴隸，或是在家中生的奴僕呢？ 為甚麼竟成了掠物呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列是僕人嗎？ 是家中生的奴僕嗎？ 為何成為掠物呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Is Israel a slave? Or is he a homeborn servant? Why has he become a prey?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Is Israel a servant, a slave by birth? Why then has he become plunder?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why has Israel become a slave? Why has he been carried away as plunder?"
      }
