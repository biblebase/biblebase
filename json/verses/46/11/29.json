    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为守圣餐的时候，若有人随便吃喝，忘记了这是主的身体，他就是自招审判。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為那吃喝的人，如果不辨明是主的身體，就是吃喝定在自己的身上的罪了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為人吃喝，若不分辨是主的身體，就是吃喝自己的罪了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For he who eats and drinks, eats and drinks judgment to himself if he does not judge the body rightly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For those who eat and drink without discerning the body of Christ eat and drink judgment on themselves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For if you eat the bread or drink the cup without honoring the body of Christ, you are eating and drinking God’s judgment upon yourself."
      }
    },
