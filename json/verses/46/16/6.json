    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我也许会和你们住一段时期，甚至在你们那里过冬。之后，我无论去什么地方，你们都可以给我送行。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我也許會和你們同住一些時候，甚至和你們一同過冬；這樣，我無論要到哪裡去，你們都可以給我送行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "或者和你們同住幾時，或者也過冬。無論我往哪裏去，你們就可以給我送行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and perhaps I will stay with you, or even spend the winter, so that you may send me on my way wherever I may go."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Perhaps I will stay with you for a while, or even spend the winter, so that you can help me on my journey, wherever I go."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Perhaps I will stay awhile with you, possibly all winter, and then you can send me on my way to my next destination."
      }
    },
