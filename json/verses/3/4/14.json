    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们意识到后，必须献上一头公牛犊作赎罪祭。要将公牛犊带到会幕前，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當發覺他們犯了罪的時候，會眾就要獻一頭公牛作贖罪祭，把牠牽到會幕前面；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "會眾一知道所犯的罪就要獻一隻公牛犢為贖罪祭，牽到會幕前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "when the sin which they have committed becomes known, then the assembly shall offer a bull of the herd for a sin offering and bring it before the tent of meeting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and the sin they committed becomes known, the assembly must bring a young bull as a sin offering and present it before the tent of meeting."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they become aware of their sin, the people must bring a young bull as an offering for their sin and present it before the Tabernacle."
      }
    },
