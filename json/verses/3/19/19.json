      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must obey all my decrees. “Do not mate two different kinds of animals. Do not plant your field with two different kinds of seed. Do not wear clothing woven from two different kinds of thread."
      }
