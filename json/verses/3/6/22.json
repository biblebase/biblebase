      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦子孙中继任大祭司的，受膏时要献上素祭。这种祭物属于耶和华，必须全部烧掉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫的子孫繼承他作受膏立的祭司的，都要獻這素祭，把它完全焚燒，獻給耶和華，作永遠的律例。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫的子孫中，接續他為受膏的祭司，要把這素祭獻上，要全燒給耶和華。這是永遠的定例。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The priest from among Aaron’s sons, who is anointed to succeed him, shall offer it to the Lord as decreed forever. The whole of it shall be burned."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The anointed priest who will be in his place among his sons shall offer it. By a permanent ordinance it shall be entirely offered up in smoke to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The son who is to succeed him as anointed priest shall prepare it. It is the Lord’s perpetual share and is to be burned completely."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The priest from among his sons, who is anointed in his place, shall offer it. It is a statute forever to the Lord. It shall be wholly burned."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In each generation, the high priest who succeeds Aaron must prepare this same offering. It belongs to the Lord and must be burned up completely. This is a permanent law."
      }
