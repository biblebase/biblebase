    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "lev.22.1-9",
            "paragraphs": [
              "二二1～9祭司象其它人一樣會變得不潔（自己不潔或因接觸不潔的人而變得不潔），而在不潔期間，他們不能執行祭司的職務。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "日落之后，他就洁净了，可以吃圣物，因为那是他应得的食物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "日落之後，他就潔淨了，可以吃聖物，因為那是他的食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "日落的時候，他就潔淨了，然後可以吃聖物，因為這是他的食物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when the sun sets, he will be clean, and afterward he shall eat of the holy gifts, for it is his food."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the sun goes down, he will be clean, and after that he may eat the sacred offerings, for they are his food."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the sun goes down, he will be ceremonially clean again and may eat from the sacred offerings, for this is his food."
      }
