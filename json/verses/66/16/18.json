    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "rev.16.17-21",
            "paragraphs": [
              "Ｈ第七碗（十六17～21）",
              "第七碗乃是全系列的高峰。這兒所敘述的是全然的毀滅，但沒有說人類全被殺盡，剩下的人還繼續要面對神全能者的審判。雖是這樣，第七碗所形容的是世上生命的全面拆毀。"
            ]
          },
          {
            "verse": "rev.16.18",
            "paragraphs": [
              "18.高潮的宣布引起很大的興奮，正如七號所引起的一樣（十一19）。關於閃電雷轟等等，請參四5註釋。雷轟閃電為那一刻增加肅穆。大地震尤其強調是空前的大地震；大地震在原文中共形容三次，先是反面描寫，是自從地上有人以來，從未有過的地震；然後又正面強調兩次：這地震是“這樣的巨（te{likoutos）”，和“這樣的大（megas；這一個字在七碗中用了七次，說明神這最後作為的驚天動地，無可比疑）”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "matangna": {
        "content": [
          {
            "verse": "rev.16.18",
            "paragraphs": [
              "一六18倒出最後一碗的時候，大自然發生劇變，爆炸、雷轟、閃電和前所未有的大地震。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "随后雷电交加，伴随着巨响，还有空前剧烈的大地震。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是有閃電、響聲、雷轟和大地震，自從地上有人以來，沒發生過這麼大的地震，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又有閃電、聲音、雷轟、大地震，自從地上有人以來，沒有這樣大、這樣厲害的地震。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And there were flashes of lightning and sounds and peals of thunder; and there was a great earthquake, such as there had not been since man came to be upon the earth, so great an earthquake was it, and so mighty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then there came flashes of lightning, rumblings, peals of thunder and a severe earthquake. No earthquake like it has ever occurred since mankind has been on earth, so tremendous was the quake."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the thunder crashed and rolled, and lightning flashed. And a great earthquake struck—the worst since people were placed on the earth."
      }
    },
