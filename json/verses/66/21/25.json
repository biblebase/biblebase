      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On no day will its gates ever be shut, for there will be no night there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Its gates will never be closed at the end of day because there is no night there."
      }
    },
