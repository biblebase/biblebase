      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又量了城牆，約有六十公尺。天使用的標準，就是人量度的標準。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又量了城牆，按着人的尺寸，就是天使的尺寸，共有一百四十四肘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he measured its wall, seventy-two yards, according to human measurements, which are also angelic measurements."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The angel measured the wall using human measurement, and it was 144 cubits thick."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he measured the walls and found them to be 216 feet thick (according to the human standard used by the angel)."
      }
