    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.14.6-15",
            "paragraphs": [
              "一四6～15迦勒與約書亞在加低斯巴尼亞作少數派報告，要求並獲得希伯侖城作為他特殊的產業（比較一五13）。迦勒雖然年屆八十五，但仍然精壯，協助趕出\"亞衲族人\"（一四12），而且後來自願把希伯侖給了利未人，自己住在郊野地帶（二一12）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_sermon-2012-06-03",
        "title": "一生成功的信徒",
        "date": "2012-06-03",
        "preacher": "鍾定基牧師",
        "verses": "約書亞記 14:6-15",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-06-03.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-06-03.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "与我同去的弟兄的禀告令以色列人胆战心惊，但我忠心地追随我的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可是，與我一同上去的眾兄弟卻使人民的心驚懼，我卻完全順從耶和華我的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "然而，同我上去的眾弟兄使百姓的心消化；但我專心跟從耶和華－我的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nevertheless my brethren who went up with me made the heart of the people melt with fear; but I followed the Lord my God fully."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but my fellow Israelites who went up with me made the hearts of the people melt in fear. I, however, followed the Lord my God wholeheartedly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "but my brothers who went with me frightened the people from entering the Promised Land. For my part, I wholeheartedly followed the Lord my God."
      }
    },
