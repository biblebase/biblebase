    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人杀了城内的人，一个没留。约书亚放火烧了夏琐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人用刀擊殺了城中所有的人，把他們完全毀滅；有氣息的一個也沒有留下；約書亞又放火燒了夏瑣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人用刀擊殺城中的人口，將他們盡行殺滅；凡有氣息的沒有留下一個。約書亞又用火焚燒夏瑣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They struck every person who was in it with the edge of the sword, utterly destroying them; there was no one left who breathed. And he burned Hazor with fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Everyone in it they put to the sword. They totally destroyed them, not sparing anyone that breathed, and he burned Hazor itself."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Israelites completely destroyed every living thing in the city, leaving no survivors. Not a single person was spared. And then Joshua burned the city."
      }
    },
