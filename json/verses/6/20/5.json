    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果他们的仇人追到，长老们不可把他们交出来，因为他们是无意杀的，跟被杀者无冤无仇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果報血仇的人追趕他，長老不可把他交在報血仇的人手裡，因為他並非蓄意殺人，他以前與他並沒有仇恨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若是報血仇的追了他來，長老不可將他交在報血仇的手裏；因為他是素無仇恨，無心殺了人的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now if the avenger of blood pursues him, then they shall not deliver the manslayer into his hand, because he struck his neighbor without premeditation and did not hate him beforehand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If the avenger of blood comes in pursuit, the elders must not surrender the fugitive, because the fugitive killed their neighbor unintentionally and without malice aforethought."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If the relatives of the victim come to avenge the killing, the leaders must not release the slayer to them, for he killed the other person unintentionally and without previous hostility."
      }
    },
