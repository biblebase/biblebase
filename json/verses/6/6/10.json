    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚吩咐民众：“在我下令叫你们吶喊以前，谁也不许作声，一句话也不许说。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞吩咐人民說：“你們不可呼喊，不可讓人聽見你們的聲音，連一句話也不可出口，等到我吩咐你們‘呼喊’的那天，你們才可以呼喊。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞吩咐百姓說：「你們不可呼喊，不可出聲，連一句話也不可出你們的口，等到我吩咐你們呼喊的日子，那時才可以呼喊。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Joshua commanded the people, saying, “You shall not shout nor let your voice be heard nor let a word proceed out of your mouth, until the day I tell you, ‘Shout!’ Then you shall shout!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Joshua had commanded the army, “Do not give a war cry, do not raise your voices, do not say a word until the day I tell you to shout. Then shout!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do not shout; do not even talk,” Joshua commanded. “Not a single word from any of you until I tell you to shout. Then shout!”"
      }
    },
