    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.15.63",
            "paragraphs": [
              "一五63猶大人確實占據了下城（士一8,21），但上城到了大衛王的時候才被占領，而耶布斯人也被殲滅（撒下五6,7）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大人没能把住在耶路撒冷的耶布斯人赶出去，他们至今仍然跟犹大人一同居住。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於住在耶路撒冷的耶布斯人，猶大人不能把他們趕走；所以耶布斯人在耶路撒冷與猶大人同住，直到今日。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於住耶路撒冷的耶布斯人，猶大人不能把他們趕出去，耶布斯人卻在耶路撒冷與猶大人同住，直到今日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now as for the Jebusites, the inhabitants of Jerusalem, the sons of Judah could not drive them out; so the Jebusites live with the sons of Judah at Jerusalem until this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Judah could not dislodge the Jebusites, who were living in Jerusalem; to this day the Jebusites live there with the people of Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the tribe of Judah could not drive out the Jebusites, who lived in the city of Jerusalem, so the Jebusites live there among the people of Judah to this day."
      }
    },
