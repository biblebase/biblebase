    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚和全体以色列人把谢拉家族的亚干、那些银子、衣服和金子及其儿女、牛、驴、羊、帐篷和一切所有都带到亚割谷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞和全體以色列人一起把謝拉的曾孫亞干和那銀子、那件衣服、那金條，以及亞干的兒女、牛、驢、羊、帳棚和他所有的一切，都帶上亞割谷去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞和以色列眾人把謝拉的曾孫亞干和那銀子、那件衣服、那條金子，並亞干的兒女、牛、驢、羊、帳棚，以及他所有的，都帶到亞割谷去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Joshua and all Israel with him, took Achan the son of Zerah, the silver, the mantle, the bar of gold, his sons, his daughters, his oxen, his donkeys, his sheep, his tent and all that belonged to him; and they brought them up to the valley of Achor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Joshua, together with all Israel, took Achan son of Zerah, the silver, the robe, the gold bar, his sons and daughters, his cattle, donkeys and sheep, his tent and all that he had, to the Valley of Achor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Joshua and all the Israelites took Achan, the silver, the robe, the bar of gold, his sons, daughters, cattle, donkeys, sheep, goats, tent, and everything he had, and they brought them to the valley of Achor."
      }
    },
