    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "随后约书亚将这五个王杀死，把尸体分别挂在五棵树上，直到傍晚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "隨後，約書亞把那五個王殺死，懸在五棵樹上；他們就懸在樹上，直到晚上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "隨後約書亞將這五王殺死，掛在五棵樹上。他們就在樹上直掛到晚上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So afterward Joshua struck them and put them to death, and he hanged them on five trees; and they hung on the trees until evening."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Joshua put the kings to death and exposed their bodies on five poles, and they were left hanging on the poles until evening."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Joshua killed each of the five kings and impaled them on five sharpened poles, where they hung until evening."
      }
