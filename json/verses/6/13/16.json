    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.13.15-23",
            "paragraphs": [
              "一三15～23從前由摩押人占領，這時分給流便支派的土地，位於死海的正東面。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从亚嫩谷的城、谷旁的亚罗珥和米底巴附近的整个平原，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的境界是從亞嫩谷邊的亞羅珥起，包括谷中間的城、靠近米底巴的整片平原、"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的境界是亞嫩谷邊的亞羅珥和谷中的城，靠近米底巴的全平原；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So their territory was from Aroer, which is on the edge of the Valley of the Arnon, and the city that is in the middle of the valley, and all the tableland by Medeba;"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their territory was from Aroer, which is on the edge of the valley of the Arnon, with the city which is in the middle of the valley and all the plain by Medeba;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The territory from Aroer on the rim of the Arnon Gorge, and from the town in the middle of the gorge, and the whole plateau past Medeba"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Their territory was from Aroer, which is on the bank of the River Arnon, and the city that is in the midst of the ravine, and all the plain by Medeba;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their territory extended from Aroer on the edge of the Arnon Gorge (including the town in the middle of the gorge) to the plain beyond Medeba."
      }
    },
