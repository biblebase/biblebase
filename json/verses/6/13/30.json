    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从玛哈念开始，包括整个巴珊，就是巴珊王噩的国土及其境内雅珥的六十座城邑，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的境界是從瑪哈念起，包括巴珊全地、巴珊王噩的全國，並且在巴珊的睚珥的所有村落，共六十座城，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的境界是從瑪哈念起，包括巴珊全地，就是巴珊王噩的全國，並在巴珊、睚珥的一切城邑，共六十個。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their territory was from Mahanaim, all Bashan, all the kingdom of Og king of Bashan, and all the towns of Jair, which are in Bashan, sixty cities;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The territory extending from Mahanaim and including all of Bashan, the entire realm of Og king of Bashan—all the settlements of Jair in Bashan, sixty towns,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their territory extended from Mahanaim, including all of Bashan, all the former kingdom of King Og, and the sixty towns of Jair in Bashan."
      }
    },
