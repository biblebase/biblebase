    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.16.5-10",
            "paragraphs": [
              "一六5～10以法蓮抽簽得到中央山地的南部，從約但河延伸至地中海。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又从他普亚往西到加拿河，直到地中海。这就是以法莲的子孙按宗族所分的产业。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又從他普亞向西行，到加拿河，直通到海；這就是以法蓮支派按著家族所得的產業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從他普亞往西，到加拿河，直通到海為止。這就是以法蓮支派按着宗族所得的地業。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "From Tappuah the boundary goes westward to the brook Kanah and ends at the sea. Such is the inheritance of the tribe of the people of Ephraim by their clans,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From Tappuah the border continued westward to the brook of Kanah, and it ended at the sea. This is the inheritance of the tribe of the sons of Ephraim according to their families,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From Tappuah the border went west to the Kanah Ravine and ended at the Mediterranean Sea. This was the inheritance of the tribe of the Ephraimites, according to its clans."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The border went out from Tappuah westward to the Brook Kanah, and it ended at the sea. This was the inheritance of the tribe of the children of Ephraim according to their families."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From Tappuah the boundary extended westward, following the Kanah Ravine to the Mediterranean Sea. This is the homeland allocated to the clans of the tribe of Ephraim."
      }
    },
