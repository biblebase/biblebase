    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.9.3-27",
            "paragraphs": [
              "九3～27以色列人可以跟遠方的城市和平相處，但他們卻必須把迦南地的城全然毀滅（申七1,2；二○11～15）。由於\"基遍\"距離艾城西南面只有六英裏半（10.5公裏），所以它的特使必須裝作從遠方來，才可以逃過正在入侵的以色列人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚与他们立了和平盟约，容他们存活，会众的首领也向他们起誓守约。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是約書亞與他們議和，和他們立約，容他們存活；會眾的首領也向他們起了誓。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是約書亞與他們講和，與他們立約，容他們活着；會眾的首領也向他們起誓。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joshua made peace with them and made a covenant with them, to let them live; and the leaders of the congregation swore an oath to them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Joshua made a treaty of peace with them to let them live, and the leaders of the assembly ratified it by oath."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Joshua made a peace treaty with them and guaranteed their safety, and the leaders of the community ratified their agreement with a binding oath."
      }
    },
