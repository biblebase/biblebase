    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们必定乘胜追击，我们便引他们离城，因为敌人一定会以为我们又像上次一样临阵逃跑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們必定出來追趕我們，我們就把他們引出城來，因為他們必定說：‘這些人像前一次一樣在我們面前逃跑了。’所以我們要在他們面前逃跑。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必出來追趕我們，直到我們引誘他們離開城；因為他們必說：『這些人像初次在我們面前逃跑。』所以我們要在他們面前逃跑，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will come out after us until we have drawn them away from the city, for they will say, ‘They are fleeing before us as at the first.’ So we will flee before them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will pursue us until we have lured them away from the city, for they will say, ‘They are running away from us as they did before.’ So when we flee from them,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We will let them chase us until we have drawn them away from the town. For they will say, ‘The Israelites are running away from us as they did before.’ Then, while we are running from them,"
      }
