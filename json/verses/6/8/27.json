    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人照耶和华对约书亚的吩咐，只带走了城中的牲畜和财物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只有那城裡的牲畜和戰利品，以色列人照著耶和華吩咐約書亞的話，都據為己有。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟獨城中的牲畜和財物，以色列人都取為自己的掠物，是照耶和華所吩咐約書亞的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Israel took only the cattle and the spoil of that city as plunder for themselves, according to the word of the Lord which He had commanded Joshua."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Israel did carry off for themselves the livestock and plunder of this city, as the Lord had instructed Joshua."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Only the livestock and the treasures of the town were not destroyed, for the Israelites kept these as plunder for themselves, as the Lord had commanded Joshua."
      }
