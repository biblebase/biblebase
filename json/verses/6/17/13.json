      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "及至以色列人強盛了，就使迦南人做苦工，沒有把他們全然趕出。"
      },
      "esv": {
        "version_id": "ESV",
