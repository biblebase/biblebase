    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华把约旦河定为我们跟你们吕便和迦得人的边界，你们与耶和华一点关系也没有。’这样，恐怕你们的子孙会使我们的子孙不再敬畏耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "流本人和迦得人啊，耶和華定了約旦河作我們和你們中間的疆界；你們與耶和華無分了。’這樣，你們的子孫就使我們的子孫不再敬畏耶和華了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為耶和華把約旦河定為我們和你們這呂便人、迦得人的交界，你們與耶和華無分了。』這樣，你們的子孫就使我們的子孫不再敬畏耶和華了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord has made the Jordan a border between us and you, you sons of Reuben and sons of Gad; you have no portion in the Lord.” So your sons may make our sons stop fearing the Lord.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord has made the Jordan a boundary between us and you—you Reubenites and Gadites! You have no share in the Lord.’ So your descendants might cause ours to stop fearing the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord has placed the Jordan River as a barrier between our people and you people of Reuben and Gad. You have no claim to the Lord.’ So your descendants may prevent our descendants from worshiping the Lord."
      }
    },
