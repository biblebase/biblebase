    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果我们离弃耶和华，为自己造祭坛，或是在坛上献燔祭、素祭和平安祭，愿耶和华亲自惩罚我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們若是為自己築壇，轉離不跟從耶和華，或是把燔祭、素祭和平安祭獻在壇上，願耶和華親自追究我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為自己築壇，要轉去不跟從耶和華，或是要將燔祭、素祭、平安祭獻在壇上，願耶和華親自討我們的罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If we have built us an altar to turn away from following the Lord, or if to offer a burnt offering or grain offering on it, or if to offer sacrifices of peace offerings on it, may the Lord Himself require it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If we have built our own altar to turn away from the Lord and to offer burnt offerings and grain offerings, or to sacrifice fellowship offerings on it, may the Lord himself call us to account."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If we have built an altar for ourselves to turn away from the Lord or to offer burnt offerings or grain offerings or peace offerings, may the Lord himself punish us."
      }
    },
