    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚说：“你们不能事奉耶和华，祂是一位圣洁的上帝，祂痛恨不贞，必不赦免你们的过犯和罪恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞對眾民說：“你們不能事奉耶和華，因為他是聖潔的　神；他是嫉妒的　神，他必不赦免你們的過犯和罪惡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞對百姓說：「你們不能事奉耶和華；因為他是聖潔的神，是忌邪的神，必不赦免你們的過犯罪惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Joshua said to the people, “You will not be able to serve the Lord, for He is a holy God. He is a jealous God; He will not forgive your transgression or your sins."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Joshua said to the people, “You are not able to serve the Lord. He is a holy God; he is a jealous God. He will not forgive your rebellion and your sins."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Joshua warned the people, “You are not able to serve the Lord, for he is a holy and jealous God. He will not forgive your rebellion and your sins."
      }
    },
