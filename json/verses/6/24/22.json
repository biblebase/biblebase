    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚说：“现在你们自己作证，你们已选择事奉耶和华。”民众答道：“我们愿意自己作证。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞對眾民說：“你們選擇了耶和華，要事奉他，你們為自己作證吧。”他們說：“我們願意作證。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞對百姓說：「你們選定耶和華，要事奉他，你們自己作見證吧！」他們說：「我們願意作見證。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joshua said to the people, “You are witnesses against yourselves that you have chosen for yourselves the Lord, to serve Him.” And they said, “We are witnesses.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Joshua said, “You are witnesses against yourselves that you have chosen to serve the Lord.” “Yes, we are witnesses,” they replied."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You are a witness to your own decision,” Joshua said. “You have chosen to serve the Lord.” “Yes,” they replied, “we are witnesses to what we have said.”"
      }
