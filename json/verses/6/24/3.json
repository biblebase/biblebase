      "leili": {
        "content": [
          {
            "verse": "jos.24.3",
            "paragraphs": [
              "二四3\"大河那邊\"。即幼發拉底河北面和東面的土地。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我把你们的祖先亚伯拉罕从幼发拉底河那边带出来，领他走遍迦南，使他人丁兴旺。我把以撒赐给他，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我把你們的祖先亞伯拉罕，從大河那邊帶來，領他走遍迦南地，使他的後裔增多，把以撒賜給他；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我將你們的祖宗亞伯拉罕從大河那邊帶來，領他走遍迦南全地，又使他的子孫眾多，把以撒賜給他；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I took your father Abraham from beyond the River, and led him through all the land of Canaan, and multiplied his descendants and gave him Isaac."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I took your father Abraham from the land beyond the Euphrates and led him throughout Canaan and gave him many descendants. I gave him Isaac,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I took your ancestor Abraham from the land beyond the Euphrates and led him into the land of Canaan. I gave him many descendants through his son Isaac."
      }
    },
