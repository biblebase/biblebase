    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我领你们来到约旦河东边亚摩利人居住的地方。他们跟你们争战，我把他们交在你们手中，使你们占领他们的土地作自己的产业。我把他们从你们面前全部消灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我領你們到了住在約旦河東的亞摩利人的地，他們和你們爭戰，我把他們交在你們手裡，你們就得了他們的地作產業；我也在你們面前把他們消滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我領你們到約旦河東亞摩利人所住之地。他們與你們爭戰，我將他們交在你們手中，你們便得了他們的地為業；我也在你們面前將他們滅絕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I brought you into the land of the Amorites who lived beyond the Jordan, and they fought with you; and I gave them into your hand, and you took possession of their land when I destroyed them before you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘I brought you to the land of the Amorites who lived east of the Jordan. They fought against you, but I gave them into your hands. I destroyed them from before you, and you took possession of their land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Finally, I brought you into the land of the Amorites on the east side of the Jordan. They fought against you, but I destroyed them before you. I gave you victory over them, and you took possession of their land."
      }
    },
