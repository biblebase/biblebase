    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在你有生之年，没有人能够抵挡你。过去我怎样与摩西同在，也必照样与你同在，我必不离开你，也不丢弃你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你一生的日子必沒有一人能在你面前立得住；我怎樣與摩西同在，也必照樣與你同在；我決不離開你，也不離棄你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你平生的日子，必無一人能在你面前站立得住。我怎樣與摩西同在，也必照樣與你同在；我必不撇下你，也不丟棄你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No man will be able to stand before you all the days of your life. Just as I have been with Moses, I will be with you; I will not fail you or forsake you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one will be able to stand against you all the days of your life. As I was with Moses, so I will be with you; I will never leave you nor forsake you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No one will be able to stand against you as long as you live. For I will be with you as I was with Moses. I will not fail you or abandon you."
      }
    },
