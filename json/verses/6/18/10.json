    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚便在示罗按照以色列的支派，在耶和华面前为他们抽签分地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞就在示羅，在耶和華面前為他們抽籤；約書亞在那裡按著他們的支派把地分給以色列人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞就在示羅，耶和華面前，為他們拈鬮。約書亞在那裏，按着以色列人的支派，將地分給他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Joshua cast lots for them in Shiloh before the Lord, and there Joshua divided the land to the sons of Israel according to their divisions."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Joshua then cast lots for them in Shiloh in the presence of the Lord, and there he distributed the land to the Israelites according to their tribal divisions."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And there at Shiloh, Joshua cast sacred lots in the presence of the Lord to determine which tribe should have each section."
      }
    },
