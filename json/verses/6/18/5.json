    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.18.2-9",
            "paragraphs": [
              "一八2～9約書亞派遣二十一人去勘察那仍未分配給猶大、瑪拿西和以法蓮的土地。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们要把土地划分作七份。犹大要留在南部自己的领土上，约瑟家族要留在北部自己的领土上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們要把地業分作七份，猶大留在南方他們的境界內；約瑟家留在北方他們的境界內。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們要將地分做七分；猶大仍在南方，住在他的境內。約瑟家仍在北方，住在他的境內。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They shall divide it into seven portions; Judah shall stay in its territory on the south, and the house of Joseph shall stay in their territory on the north."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are to divide the land into seven parts. Judah is to remain in its territory on the south and the tribes of Joseph in their territory on the north."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let them divide the land into seven sections, excluding Judah’s territory in the south and Joseph’s territory in the north."
      }
    },
