    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.19.10-16",
            "paragraphs": [
              "一九10～16\"西布倫\"分得下加利利四面被高地包圍的土地，其中包括拿撒勒，而且有貿易路線橫過。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "再往东，经迦特·希弗、以特·加汛、临门，直到尼亚；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又從那裡往東面日出的方向，經過迦特．希弗，到以特．加汛、臨門，再伸展到尼亞；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從那裏往東，接連到迦特‧希弗，至以特‧加汛，通到臨門，臨門延到尼亞；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From there it continued eastward toward the sunrise to Gath-hepher, to Eth-kazin, and it proceeded to Rimmon which stretches to Neah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then it continued eastward to Gath Hepher and Eth Kazin; it came out at Rimmon and turned toward Neah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then it continued east to Gath-hepher, Eth-kazin, and Rimmon and turned toward Neah."
      }
    },
