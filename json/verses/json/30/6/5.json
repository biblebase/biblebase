    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们伴着琴声唱慵懒的歌， 像大卫一样为自己制造乐器。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們閒逸地彈琴奏樂， 又為自己製造樂器，像大衛一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彈琴鼓瑟唱消閑的歌曲， 為自己製造樂器，如同大衛所造的；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who improvise to the sound of the harp, And like David have composed songs for themselves,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You strum away on your harps like David and improvise on musical instruments."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You sing trivial songs to the sound of the harp and fancy yourselves to be great musicians like David."
      }
    },
