    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们躺在象牙床上，靠在卧榻上， 你们吃养在圈里的肥嫩牛羊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們臥在象牙床上， 躺在榻上； 你們吃羊群中的羊羔， 和牛棚裡的牛犢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們躺臥在象牙牀上，舒身在榻上， 吃羣中的羊羔，棚裏的牛犢；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Those who recline on beds of ivory And sprawl on their couches, And eat lambs from the flock And calves from the midst of the stall,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You lie on beds adorned with ivory and lounge on your couches. You dine on choice lambs and fattened calves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How terrible for you who sprawl on ivory beds and lounge on your couches, eating the meat of tender lambs from the flock and of choice calves fattened in the stall."
      }
    },
