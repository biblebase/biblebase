    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我在各城降下饥荒， 使你们遍地缺粮。 尽管如此，你们仍不归向我。 这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“雖然我使你們各城的人牙齒乾淨， 各處都缺乏糧食； 但你們仍不歸向我。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我使你們在一切城中牙齒乾淨， 在你們各處糧食缺乏， 你們仍不歸向我。 這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But I gave you also cleanness of teeth in all your cities And lack of bread in all your places, Yet you have not returned to Me,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I gave you empty stomachs in every city and lack of bread in every town, yet you have not returned to me,” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I brought hunger to every city and famine to every town. But still you would not return to me,” says the Lord."
      }
    },
