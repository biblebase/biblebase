    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以色列人啊， 你们去伯特利犯罪吧， 去吉甲增添罪恶吧！ 要每天早晨献上祭物， 每三天奉上你们的十分之一。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們去伯特利犯罪， 在吉甲增加過犯吧！ 每早獻上你們的祭物， 每三日獻上你們的十分之一吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人哪，任你們往伯特利去犯罪， 到吉甲加增罪過； 每日早晨獻上你們的祭物， 每三日奉上你們的十分之一。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Enter Bethel and transgress; In Gilgal multiply transgression! Bring your sacrifices every morning, Your tithes every three days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Go to Bethel and sin; go to Gilgal and sin yet more. Bring your sacrifices every morning, your tithes every three years."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Go ahead and offer sacrifices to the idols at Bethel. Keep on disobeying at Gilgal. Offer sacrifices each morning, and bring your tithes every three days."
      }
    },
