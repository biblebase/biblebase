    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "住在撒玛利亚山上的巴珊肥母牛啊， 你们要听！ 你们压榨贫寒人，欺凌穷苦人， 又使唤丈夫倒酒畅饮。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒瑪利亞山上的巴珊母牛啊！要聽這話； 你們欺負貧窮人，壓迫窮苦人， 又對自己的丈夫說：“拿酒來，給我們飲用。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們住撒馬利亞山如巴珊母牛的啊， 當聽我的話－ 你們欺負貧寒的，壓碎窮乏的， 對家主說：拿酒來，我們喝吧！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hear this word, you cows of Bashan who are on the mountain of Samaria, Who oppress the poor, who crush the needy, Who say to your husbands, “Bring now, that we may drink!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hear this word, you cows of Bashan on Mount Samaria, you women who oppress the poor and crush the needy and say to your husbands, “Bring us some drinks!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen to me, you fat cows living in Samaria, you women who oppress the poor and crush the needy, and who are always calling to your husbands, “Bring us another drink!”"
      }
    },
