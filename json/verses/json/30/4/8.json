    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "两三座城的人都踉踉跄跄地涌到一座城找水喝， 水却不够喝。 尽管如此，你们仍不归向我。 这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "兩三個城的居民擠到一個城去找水喝， 卻沒有足夠的水喝； 但你們仍不歸向我。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，兩三城的人湊到一城去找水， 卻喝不足； 你們仍不歸向我。 這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So two or three cities would stagger to another city to drink water, But would not be satisfied; Yet you have not returned to Me,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "People staggered from town to town for water but did not get enough to drink, yet you have not returned to me,” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People staggered from town to town looking for water, but there was never enough. But still you would not return to me,” says the Lord."
      }
    },
