      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主——万军之上帝耶和华说： “你们要听这话，去警告雅各家。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要聽，要向雅各家作證。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華－萬軍之神說： 當聽這話，警戒雅各家。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hear and testify against the house of Jacob,” Declares the Lord God, the God of hosts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Hear this and testify against the descendants of Jacob,” declares the Lord, the Lord God Almighty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Now listen to this, and announce it throughout all Israel,” says the Lord, the Lord God of Heaven’s Armies."
      }
