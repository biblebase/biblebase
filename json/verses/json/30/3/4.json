      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若非捕到猎物， 狮子怎会在林中咆哮呢？ 若非猎获食物， 猛狮怎会在洞中吼叫呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "獅子如果不是捕獲獵物， 怎會在林中吼叫？ 少壯獅子如果沒有捕獲甚麼， 怎會在洞中咆哮呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "獅子若非抓食，豈能在林中咆哮呢？ 少壯獅子若無所得，豈能從洞中發聲呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Does a lion roar in the forest when he has no prey? Does a young lion growl from his den unless he has captured something?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Does a lion roar in the thicket when it has no prey? Does it growl in its den when it has caught nothing?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Does a lion ever roar in a thicket without first finding a victim? Does a young lion growl in its den without first catching its prey?"
      }
    },
