    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要恨恶邪恶，喜爱良善， 在城门口伸张正义， 也许万军之上帝耶和华会恩待约瑟的余民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要喜愛良善，恨惡邪惡， 在城門口伸張正義； 這樣，耶和華萬軍的　神或會恩待約瑟的餘民。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要惡惡好善， 在城門口秉公行義； 或者耶和華－萬軍之神向約瑟的餘民施恩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hate evil, love good, And establish justice in the gate! Perhaps the Lord God of hosts May be gracious to the remnant of Joseph."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hate evil, love good; maintain justice in the courts. Perhaps the Lord God Almighty will have mercy on the remnant of Joseph."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Hate evil and love what is good; turn your courts into true halls of justice. Perhaps even yet the Lord God of Heaven’s Armies will have mercy on the remnant of his people."
      }
    },
