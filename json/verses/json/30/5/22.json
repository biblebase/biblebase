    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即使你们献上燔祭和素祭， 我也不悦纳； 即使你们用肥美的牲畜献上平安祭， 我也不屑一顾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然你們給我獻上燔祭和素祭， 我卻不接受。 你們獻上肥美的牲畜作平安祭， 我也不悅納。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們雖然向我獻燔祭和素祭， 我卻不悅納， 也不顧你們用肥畜獻的平安祭；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even though you offer up to Me burnt offerings and your grain offerings, I will not accept them; And I will not even look at the peace offerings of your fatlings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even though you bring me burnt offerings and grain offerings, I will not accept them. Though you bring choice fellowship offerings, I will have no regard for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will not accept your burnt offerings and grain offerings. I won’t even notice all your choice peace offerings."
      }
    },
