      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I hate, I reject your festivals, Nor do I delight in your solemn assemblies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I hate, I despise your religious festivals; your assemblies are a stench to me."
      },
      "nlt": {
