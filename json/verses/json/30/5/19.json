    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的光景将好像人逃避狮子， 却遇上了熊， 逃回家里，手扶墙壁， 又被蛇咬。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那日就像一個人避過了獅子， 卻遇上了熊； 回到家裡，手靠在牆上， 卻被蛇咬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "景況好像人躲避獅子又遇見熊， 或是進房屋以手靠牆，就被蛇咬。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As when a man flees from a lion And a bear meets him, Or goes home, leans his hand against the wall And a snake bites him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It will be as though a man fled from a lion only to meet a bear, as though he entered his house and rested his hand on the wall only to have a snake bite him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In that day you will be like a man who runs from a lion— only to meet a bear. Escaping from the bear, he leans his hand against a wall in his house— and he’s bitten by a snake."
      }
    },
