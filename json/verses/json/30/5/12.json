    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我知道你们的过犯众多、罪恶无数。 你们迫害义人，收受贿赂， 在城门口冤枉穷人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為我知道你們的過犯眾多，罪大惡極； 你們迫害義人，收受賄賂， 在城門口屈枉窮乏人；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我知道你們的罪過何等多， 你們的罪惡何等大。 你們苦待義人，收受賄賂，在城門口屈枉窮乏人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I know your transgressions are many and your sins are great, You who distress the righteous and accept bribes And turn aside the poor in the gate."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I know how many are your offenses and how great your sins. There are those who oppress the innocent and take bribes and deprive the poor of justice in the courts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I know the vast number of your sins and the depth of your rebellions. You oppress good people by taking bribes and deprive the poor of justice in the courts."
      }
    },
