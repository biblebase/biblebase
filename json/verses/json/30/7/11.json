    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为阿摩司这样说，‘耶罗波安将丧身刀下，以色列人必被掳去，远离家乡。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為阿摩司這樣說： ‘耶羅波安必死在刀下， 以色列必被擄， 離開本國。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為阿摩司如此說：『耶羅波安必被刀殺；以色列民定被擄去離開本地。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus Amos says, ‘Jeroboam will die by the sword and Israel will certainly go from its land into exile.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this is what Amos is saying: “ ‘Jeroboam will die by the sword, and Israel will surely go into exile, away from their native land.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He is saying, ‘Jeroboam will soon be killed, and the people of Israel will be sent away into exile.’”"
      }
