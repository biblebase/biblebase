    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以下是祂让我看见的异象：看啊，主站在一道按照准绳建造的墙边，手里拿着准绳。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他向我這樣顯示：看哪！主站在一道按準繩建造的牆旁邊，手裡拿著準繩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又指示我一件事：有一道牆是按準繩建築的，主手拿準繩站在其上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus He showed me, and behold, the Lord was standing by a vertical wall with a plumb line in His hand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what he showed me: The Lord was standing by a wall that had been built true to plumb, with a plumb line in his hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he showed me another vision. I saw the Lord standing beside a wall that had been built using a plumb line. He was using a plumb line to see if it was still straight."
      }
    },
