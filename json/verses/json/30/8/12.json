    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人们从南到北，从东到西， 在境内四处流荡， 要寻找耶和华的话却找不到。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們從這海飄流到那海， 從北到東， 到處奔走，要尋求耶和華的話， 卻尋不見。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必飄流，從這海到那海， 從北邊到東邊，往來奔跑， 尋求耶和華的話，卻尋不着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "People will stagger from sea to sea And from the north even to the east; They will go to and fro to seek the word of the Lord, But they will not find it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "People will stagger from sea to sea and wander from north to east, searching for the word of the Lord, but they will not find it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People will stagger from sea to sea and wander from border to border searching for the word of the Lord, but they will not find it."
      }
    },
