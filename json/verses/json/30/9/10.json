    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在我的子民中， 所有夸口说‘灾祸不会追上我们，也不会迎面而来’的罪人， 都要死在刀下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我民中所有犯罪的人， 就是那些說：‘災禍必不臨近、追上我們’的人， 他們必死在刀下。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我民中的一切罪人說： 災禍必追不上我們， 也迎不着我們。 他們必死在刀下。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the sinners of My people will die by the sword, Those who say, ‘The calamity will not overtake or confront us.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the sinners among my people will die by the sword, all those who say, ‘Disaster will not overtake or meet us.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But all the sinners will die by the sword— all those who say, ‘Nothing bad will happen to us.’"
      }
    },
