    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华的眼目察看这罪恶的国家， 我要从地上除灭它， 但不会完全毁灭雅各家。 这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪！耶和華的眼目察看這有罪的國， 我要把它從地上除滅； 卻必不把雅各家完全除滅。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華的眼目察看這有罪的國， 必將這國從地上滅絕， 卻不將雅各家滅絕淨盡。 這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, the eyes of the Lord God are on the sinful kingdom, And I will destroy it from the face of the earth; Nevertheless, I will not totally destroy the house of Jacob,” Declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Surely the eyes of the Sovereign Lord are on the sinful kingdom. I will destroy it from the face of the earth. Yet I will not totally destroy the descendants of Jacob,” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I, the Sovereign Lord, am watching this sinful nation of Israel. I will destroy it from the face of the earth. But I will never completely destroy the family of Israel,” says the Lord."
      }
    },
