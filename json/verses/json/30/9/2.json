    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即使他们挖洞钻进阴间， 我也要把他们揪出来； 即使他们爬上高天， 我也要把他们拉下来；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果他們下到陰間， 就是在那裡，我的手必把他們揪上來； 如果他們升到天上， 就是在那裡，我必把他們拉下來；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們雖然挖透陰間， 我的手必取出他們來； 雖然爬上天去， 我必拿下他們來；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Though they dig into Sheol, From there will My hand take them; And though they ascend to heaven, From there will I bring them down."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Though they dig down to the depths below, from there my hand will take them. Though they climb up to the heavens above, from there I will bring them down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Even if they dig down to the place of the dead, I will reach down and pull them up. Even if they climb up into the heavens, I will bring them down."
      }
    },
