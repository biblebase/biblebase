    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果他们被抛弃，人类便有机会与上帝和好，那么，当他们被接纳的时候，又将怎样呢？岂不是要死而复生吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果他們被捨棄，世人就可以與　神復和；他們蒙接納，不就等於從死人中復活嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若他們被丟棄，天下就得與神和好，他們被收納，豈不是死而復生嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if their rejection is the reconciliation of the world, what will their acceptance be but life from the dead?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For if their rejection brought reconciliation to the world, what will their acceptance be but life from the dead?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For since their rejection meant that God offered salvation to the rest of the world, their acceptance will be even more wonderful. It will be life for those who were dead!"
      }
    },
