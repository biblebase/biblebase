    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为上帝只有一位，祂本着信称受割礼的人为义人，也本着信称未受割礼的人为义人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神既然只有一位，他就以信為準則稱受割禮的為義，也要以信為準則稱沒有受割禮的為義。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神既是一位，他就要因信稱那受割禮的為義，也要因信稱那未受割禮的為義。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "since indeed God who will justify the circumcised by faith and the uncircumcised through faith is one."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "since there is only one God, who will justify the circumcised by faith and the uncircumcised through that same faith."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There is only one God, and he makes people right with himself only by faith, whether they are Jews or Gentiles."
      }
    },
