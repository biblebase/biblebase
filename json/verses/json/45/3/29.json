    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道上帝只是犹太人的上帝吗？祂不也是外族人的上帝吗？祂当然也是外族人的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "難道　神只是猶太人的　神嗎？不也是外族人的　神嗎？是的，他也是外族人的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "難道神只作猶太人的神嗎？不也是作外邦人的神嗎？是的，也作外邦人的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Or is God the God of Jews only? Is He not the God of Gentiles also? Yes, of Gentiles also,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Or is God the God of Jews only? Is he not the God of Gentiles too? Yes, of Gentiles too,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After all, is God the God of the Jews only? Isn’t he also the God of the Gentiles? Of course he is."
      }
