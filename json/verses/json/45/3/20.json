    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为无人能够靠遵行律法而被上帝视为义人，律法的本意是要使人知罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有一個人可以靠行律法，在　神面前得稱為義，因為藉著律法，人對於罪才有充分的認識。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以凡有血氣的，沒有一個因行律法能在神面前稱義，因為律法本是叫人知罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "because by the works of the Law no flesh will be justified in His sight; for through the Law comes the knowledge of sin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore no one will be declared righteous in God’s sight by the works of the law; rather, through the law we become conscious of our sin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For no one can ever be made right with God by doing what the law commands. The law simply shows us how sinful we are."
      }
    },
