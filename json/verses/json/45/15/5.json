    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但愿赐忍耐和鼓励的上帝给你们合一的心去效法基督耶稣的榜样，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願賜忍耐和安慰的　神，使你們彼此同心，效法基督耶穌，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但願賜忍耐安慰的神叫你們彼此同心，效法基督耶穌，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now may the God who gives perseverance and encouragement grant you to be of the same mind with one another according to Christ Jesus,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the God who gives endurance and encouragement give you the same attitude of mind toward each other that Christ Jesus had,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May God, who gives this patience and encouragement, help you live in complete harmony with each other, as is fitting for followers of Christ Jesus."
      }
    },
