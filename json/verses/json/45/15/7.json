    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，你们应该像基督接纳你们一样彼此接纳，使上帝得荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，你們應當彼此接納，就像基督接納了你們一樣，使榮耀歸於　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，你們要彼此接納，如同基督接納你們一樣，使榮耀歸與神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore, accept one another, just as Christ also accepted us to the glory of God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Accept one another, then, just as Christ accepted you, in order to bring praise to God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Therefore, accept each other just as Christ has accepted you so that God will be given glory."
      }
    },
