    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并使我按照上帝的旨意欢然去与你们相聚，一同重新得力。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也使我照著　神的旨意，高高興興地到你們那裡去，好和你們一同得到安息。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並叫我順着神的旨意，歡歡喜喜地到你們那裏，與你們同得安息。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that I may come to you in joy by the will of God and find refreshing rest in your company."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "so that I may come to you with joy, by God’s will, and in your company be refreshed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then, by the will of God, I will be able to come to you with a joyful heart, and we will be an encouragement to each other."
      }
