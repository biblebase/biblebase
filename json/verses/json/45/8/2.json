    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为赐人生命之圣灵的律已经在基督耶稣里使我获得自由，脱离了罪与死的律。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為生命之靈的律在基督耶穌裡使我自由，脫離了罪和死的律。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為賜生命聖靈的律，在基督耶穌裏釋放了我，使我脫離罪和死的律了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the law of the Spirit of life in Christ Jesus has set you free from the law of sin and of death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "because through Christ Jesus the law of the Spirit who gives life has set you free from the law of sin and death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And because you belong to him, the power of the life-giving Spirit has freed you from the power of sin that leads to death."
      }
    },
