    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果基督在你们里面，你们的身体虽然因罪恶而死亡，灵魂却因上帝所赐的义而活着。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基督若在你們裡面，你們的身體因著罪的緣故是死的，而聖靈卻因著義的緣故賜給你們生命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基督若在你們心裏，身體就因罪而死，心靈卻因義而活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If Christ is in you, though the body is dead because of sin, yet the spirit is alive because of righteousness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if Christ is in you, then even though your body is subject to death because of sin, the Spirit gives life because of righteousness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And Christ lives within you, so even though your body will die because of sin, the Spirit gives you life because you have been made right with God."
      }
    },
