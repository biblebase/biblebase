    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为了救我将生死置之度外。不但我感谢他们，就是外族人的教会也感谢他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們為了我的性命，置生死於度外，不但我感激他們，連外族的眾教會也感激他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也為我的命將自己的頸項置之度外。不但我感謝他們，就是外邦的眾教會也感謝他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "who for my life risked their own necks, to whom not only do I give thanks, but also all the churches of the Gentiles;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They risked their lives for me. Not only I but all the churches of the Gentiles are grateful to them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In fact, they once risked their lives for me. I am thankful to them, and so are all the Gentile churches."
      }
    },
