    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在，我们既然因祂所流的血而被称为义人，岂不更要靠着祂免受上帝的烈怒吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，我們現在既然因他的血稱義，就更要藉著他免受　神的忿怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在我們既靠着他的血稱義，就更要藉着他免去神的忿怒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Much more then, having now been justified by His blood, we shall be saved from the wrath of God through Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Since we have now been justified by his blood, how much more shall we be saved from God’s wrath through him!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And since we have been made right in God’s sight by the blood of Christ, he will certainly save us from God’s condemnation."
      }
    },
