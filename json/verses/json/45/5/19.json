    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为一人的悖逆，众人成为罪人；照样，因为一人的顺服，众人将被称为义人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因著那一人的悖逆，眾人就被列為罪人；照樣，因著這一人的順服，眾人也被列為義人了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因一人的悖逆，眾人成為罪人；照樣，因一人的順從，眾人也成為義了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For as through the one man’s disobedience the many were made sinners, even so through the obedience of the One the many will be made righteous."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For just as through the disobedience of the one man the many were made sinners, so also through the obedience of the one man the many will be made righteous."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because one person disobeyed God, many became sinners. But because one other person obeyed God, many will be made righteous."
      }
    },
