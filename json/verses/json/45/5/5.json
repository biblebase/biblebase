    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这种盼望不会落空，因为上帝的爱借着所赐给我们的圣灵已倾注在我们心中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "盼望是不會令人蒙羞的，因為　神藉著所賜給我們的聖靈，把他的愛澆灌在我們的心裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "盼望不至於羞恥，因為所賜給我們的聖靈將神的愛澆灌在我們心裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and hope does not disappoint, because the love of God has been poured out within our hearts through the Holy Spirit who was given to us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And hope does not put us to shame, because God’s love has been poured out into our hearts through the Holy Spirit, who has been given to us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And this hope will not lead to disappointment. For we know how dearly God loves us, because he has given us the Holy Spirit to fill our hearts with his love."
      }
    },
