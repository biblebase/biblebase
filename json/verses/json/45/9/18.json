    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "总之，上帝要怜悯谁，就怜悯谁；要叫谁顽固，就叫谁顽固。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣看來，他願意憐憫誰就憐憫誰，願意誰剛硬就使誰剛硬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "如此看來，神要憐憫誰就憐憫誰，要叫誰剛硬就叫誰剛硬。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So then He has mercy on whom He desires, and He hardens whom He desires."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore God has mercy on whom he wants to have mercy, and he hardens whom he wants to harden."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So you see, God chooses to show mercy to some, and he chooses to harden the hearts of others so they refuse to listen."
      }
