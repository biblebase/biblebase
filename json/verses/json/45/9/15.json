    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂曾对摩西说： “我要怜悯谁就怜悯谁， 要恩待谁就恩待谁。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他對摩西說： “我要憐憫誰，就憐憫誰； 我要恩待誰，就恩待誰。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他對摩西說： 我要憐憫誰就憐憫誰， 要恩待誰就恩待誰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He says to Moses, “I will have mercy on whom I have mercy, and I will have compassion on whom I have compassion.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For he says to Moses, “I will have mercy on whom I have mercy, and I will have compassion on whom I have compassion.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For God said to Moses, “I will show mercy to anyone I choose, and I will show compassion to anyone I choose.”"
      }
    },
