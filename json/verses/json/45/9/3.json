    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为了我的弟兄——我的同胞以色列人，即使我自己被咒诅、与基督隔绝，我也愿意！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為我的同胞，就是我骨肉之親，就算自己受咒詛，與基督隔絕，我也甘心。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為我弟兄，我骨肉之親，就是自己被咒詛，與基督分離，我也願意。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I could wish that I myself were accursed, separated from Christ for the sake of my brethren, my kinsmen according to the flesh,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I could wish that I myself were cursed and cut off from Christ for the sake of my people, those of my own race,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "for my people, my Jewish brothers and sisters. I would be willing to be forever cursed—cut off from Christ!—if that would save them."
      }
    },
