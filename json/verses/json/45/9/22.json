    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若上帝要显示祂的烈怒和权能，就尽量容忍那些祂预备要毁灭的器皿，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果　神有意要顯明他的忿怒，彰顯他的大能，而多多容忍那可怒、預備遭毀滅的器皿，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "倘若神要顯明他的忿怒，彰顯他的權能，就多多忍耐寬容那可怒、預備遭毀滅的器皿，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "What if God, although willing to demonstrate His wrath and to make His power known, endured with much patience vessels of wrath prepared for destruction?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What if God, although choosing to show his wrath and make his power known, bore with great patience the objects of his wrath—prepared for destruction?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In the same way, even though God has the right to show his anger and his power, he is very patient with those on whom his anger falls, who are destined for destruction."
      }
    },
