    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可见这并不在于人的意志和努力，而在于祂的怜悯。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣看來，既不是出於人意，也不是由於人為，只在於那憐憫人的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "據此看來，這不在乎那定意的，也不在乎那奔跑的，只在乎發憐憫的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So then it does not depend on the man who wills or the man who runs, but on God who has mercy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It does not, therefore, depend on human desire or effort, but on God’s mercy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So it is God who decides to show mercy. We can neither choose it nor work for it."
      }
