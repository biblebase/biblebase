    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "按照我所传的福音，到了审判之日，上帝要借着耶稣基督审判人一切隐秘的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這也要照著我所傳的福音，在　神藉著耶穌基督審判各人隱情的那一天，彰顯出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就在神藉耶穌基督審判人隱祕事的日子，照着我的福音所言。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "on the day when, according to my gospel, God will judge the secrets of men through Christ Jesus."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This will take place on the day when God judges people’s secrets through Jesus Christ, as my gospel declares."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And this is the message I proclaim—that the day is coming when God, through Christ Jesus, will judge everyone’s secret life."
      }
    },
