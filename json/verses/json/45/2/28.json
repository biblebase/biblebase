    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为徒具外表的犹太人不是真正的犹太人，身体上的割礼也不是真正的割礼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為表面上作猶太人的並不是猶太人，在肉身上表面的割禮也不是割禮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為外面作猶太人的，不是真猶太人；外面肉身的割禮，也不是真割禮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For he is not a Jew who is one outwardly, nor is circumcision that which is outward in the flesh."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A person is not a Jew who is one only outwardly, nor is circumcision merely outward and physical."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you are not a true Jew just because you were born of Jewish parents or because you have gone through the ceremony of circumcision."
      }
    },
