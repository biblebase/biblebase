    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你这人啊，你论断别人，自己却做同样的事，你以为自己能逃脱上帝的审判吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你這個人，你判斷行這些事的人，自己所行的卻是一樣，你以為能逃脫　神的審判嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你這人哪，你論斷行這樣事的人，自己所行的卻和別人一樣，你以為能逃脫神的審判嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But do you suppose this, O man, when you pass judgment on those who practice such things and do the same yourself, that you will escape the judgment of God?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So when you, a mere human being, pass judgment on them and yet do the same things, do you think you will escape God’s judgment?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Since you judge others for doing these things, why do you think you can avoid God’s judgment when you do the same things?"
      }
    },
