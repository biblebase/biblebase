    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝应许将世界赐给亚伯拉罕和他的后裔，不是因为亚伯拉罕遵行了律法，而是因为他因信而成了义人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "原來　神給亞伯拉罕和他後裔承受世界的應許，並不是因著律法，而是藉著因信而來的義。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為神應許亞伯拉罕和他後裔，必得承受世界，不是因律法，乃是因信而得的義。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the promise to Abraham or to his descendants that he would be heir of the world was not through the Law, but through the righteousness of faith."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It was not through the law that Abraham and his offspring received the promise that he would be heir of the world, but through the righteousness that comes by faith."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Clearly, God’s promise to give the whole earth to Abraham and his descendants was based not on his obedience to God’s law, but on a right relationship with God that comes by faith."
      }
    },
