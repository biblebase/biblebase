    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为有律法，就有刑罚；哪里没有律法，哪里就没有违法的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為律法帶來刑罰，沒有律法，就沒有違背律法的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為律法是惹動忿怒的；哪裏沒有律法，那裏就沒有過犯。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "for the Law brings about wrath, but where there is no law, there also is no violation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "because the law brings wrath. And where there is no law there is no transgression."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the law always brings punishment on those who try to obey it. (The only way to avoid breaking the law is to have no law to break!)"
      }
    },
