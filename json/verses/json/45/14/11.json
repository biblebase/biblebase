    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "圣经上说：“主说，‘我凭我的永恒起誓，万膝必向我跪拜，万口必称颂上帝。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為經上記著： “主說，我指著我的永生起誓： 萬膝必向我跪拜， 萬口必稱頌　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "經上寫着： 主說：我憑着我的永生起誓： 萬膝必向我跪拜； 萬口必向我承認。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it is written, “As I live, says the Lord, every knee shall bow to Me, And every tongue shall give praise to God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is written: “ ‘As surely as I live,’ says the Lord, ‘every knee will bow before me; every tongue will acknowledge God.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Scriptures say, “‘As surely as I live,’ says the Lord, ‘every knee will bend to me, and every tongue will declare allegiance to God.’”"
      }
