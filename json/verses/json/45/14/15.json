    "sermons": [
      {
        "id": "home_sermons_romans-chp14-1-23",
        "title": "羅馬書14:1-23",
        "date": "2012-09-09",
        "preacher": "王文堂牧師",
        "verses": "羅馬書 14:1-23",
        "audio": "http://www.cbcwla.org/media/ch/SundaySchool/2012/NT-2012-09-09.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySchool/2012/NT-2012-09-09.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你吃的东西令弟兄姊妹不安，你就不是凭爱心行事。你不可因为一点食物而损害基督舍命救赎的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你為了食物使你的弟兄憂愁，你就不再是憑著愛心行事了。你不可因著你的食物，使基督已經替他死了的人滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你若因食物叫弟兄憂愁，就不是按着愛人的道理行。基督已經替他死，你不可因你的食物叫他敗壞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if because of food your brother is hurt, you are no longer walking according to love. Do not destroy with your food him for whom Christ died."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If your brother or sister is distressed because of what you eat, you are no longer acting in love. Do not by your eating destroy someone for whom Christ died."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And if another believer is distressed by what you eat, you are not acting in love if you eat it. Don’t let your eating ruin someone for whom Christ died."
      }
    },
