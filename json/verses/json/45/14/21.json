    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无论是吃肉、喝酒还是做任何别的事，如果会绊倒别人，就应该一概不做。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "無論是吃肉，是喝酒，或是甚麼使你的弟兄跌倒的事，一律不要作才好。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無論是吃肉是喝酒，是甚麼別的事，叫弟兄跌倒，一概不做才好。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is good not to eat meat or to drink wine, or to do anything by which your brother stumbles."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is better not to eat meat or drink wine or to do anything else that will cause your brother or sister to fall."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is better not to eat meat or drink wine or do anything else if it might cause another believer to stumble."
      }
    },
