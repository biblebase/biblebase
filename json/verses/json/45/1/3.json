      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論到他兒子－我主耶穌基督。按肉體說，是從大衛後裔生的；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "concerning His Son, who was born of a descendant of David according to the flesh,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "regarding his Son, who as to his earthly life was a descendant of David,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Good News is about his Son. In his earthly life he was born into King David’s family line,"
      }
