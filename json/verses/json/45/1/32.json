    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们明知，按上帝公义的法则，做这些事的人该死，不但执迷不悟，还喜欢别人跟他们同流合污。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們雖然明明知道行這些事的人，　神判定他們是該死的，然而他們不單自己去行，也喜歡別人去行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們雖知道神判定行這樣事的人是當死的，然而他們不但自己去行，還喜歡別人去行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and although they know the ordinance of God, that those who practice such things are worthy of death, they not only do the same, but also give hearty approval to those who practice them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Although they know God’s righteous decree that those who do such things deserve death, they not only continue to do these very things but also approve of those who practice them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They know God’s justice requires that those who do these things deserve to die, yet they do them anyway. Worse yet, they encourage others to do them, too."
      }
    },
