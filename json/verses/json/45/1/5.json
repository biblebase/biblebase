    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们从祂领受了恩典并成为使徒，要带领万民信从祂，使祂的名得荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們從他領受了恩典和使徒的職分，在萬族中使人因他的名相信而順服，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們從他受了恩惠並使徒的職分，在萬國之中叫人為他的名信服真道；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "through whom we have received grace and apostleship to bring about the obedience of faith among all the Gentiles for His name’s sake,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Through him we received grace and apostleship to call all the Gentiles to the obedience that comes from faith for his name’s sake."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Through Christ, God has given us the privilege and authority as apostles to tell Gentiles everywhere what God has done for them, so that they will believe and obey him, bringing glory to his name."
      }
    },
