    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我问候所有住在罗马、上帝所爱、蒙召做圣徒的人。愿我们的父上帝和主耶稣基督赐给你们恩典和平安！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我寫信給各位住在羅馬，為　神所愛，蒙召為聖徒的人。願恩惠平安從我們的父　神和主耶穌基督臨到你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我寫信給你們在羅馬、為神所愛、奉召作聖徒的眾人。願恩惠、平安從我們的父神並主耶穌基督歸與你們！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "to all who are beloved of God in Rome, called as saints: Grace to you and peace from God our Father and the Lord Jesus Christ."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To all in Rome who are loved by God and called to be his holy people: Grace and peace to you from God our Father and from the Lord Jesus Christ."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am writing to all of you in Rome who are loved by God and are called to be his own holy people. May God our Father and the Lord Jesus Christ give you grace and peace."
      }
    },
