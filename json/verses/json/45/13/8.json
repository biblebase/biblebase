    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡事都不可亏欠人，但在彼此相爱方面要常常觉得做得不够。一个人爱别人，就成全了律法。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要欠人的債；但在彼此相愛的事上，要覺得是欠了人的債。愛別人的，就成全了律法。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡事都不可虧欠人，惟有彼此相愛要常以為虧欠，因為愛人的就完全了律法。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Owe nothing to anyone except to love one another; for he who loves his neighbor has fulfilled the law."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let no debt remain outstanding, except the continuing debt to love one another, for whoever loves others has fulfilled the law."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Owe nothing to anyone—except for your obligation to love one another. If you love your neighbor, you will fulfill the requirements of God’s law."
      }
    },
