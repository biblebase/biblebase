    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果我们和基督一同死了，也必和祂一同复活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們若在他死的樣式上與他聯合，也要在他復活的樣式上與他聯合。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們若在他死的形狀上與他聯合，也要在他復活的形狀上與他聯合；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if we have become united with Him in the likeness of His death, certainly we shall also be in the likeness of His resurrection,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For if we have been united with him in a death like his, we will certainly also be united with him in a resurrection like his."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Since we have been united with him in his death, we will also be raised to life as he was."
      }
    },
