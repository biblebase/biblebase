    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道你们不知道，我们受洗归属基督耶稣是和祂一同死吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "難道你們不曉得我們這受洗歸入基督耶穌的人，是受洗歸入他的死嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "豈不知我們這受洗歸入基督耶穌的人是受洗歸入他的死嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Or do you not know that all of us who have been baptized into Christ Jesus have been baptized into His death?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Or don’t you know that all of us who were baptized into Christ Jesus were baptized into his death?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Or have you forgotten that when we were joined with Christ Jesus in baptism, we joined him in his death?"
      }
    },
