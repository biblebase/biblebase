    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那么，我们该怎么说呢？我们可以继续犯罪，使恩典显得更丰富吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，我們可以說甚麼呢？我們可以常在罪中，叫恩典增多嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，怎麼說呢？我們可以仍在罪中、叫恩典顯多嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "What shall we say then? Are we to continue in sin so that grace may increase?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What shall we say, then? Shall we go on sinning so that grace may increase?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Well then, should we keep on sinning so that God can show us more and more of his wonderful grace?"
      }
    },
