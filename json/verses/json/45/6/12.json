    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，不要让罪辖制你们必死的身体，使你们顺从身体的私欲；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，不要容罪在你們必死的肉身上掌權，使你們順從肉身的私慾，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，不要容罪在你們必死的身上作王，使你們順從身子的私慾。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore do not let sin reign in your mortal body so that you obey its lusts,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore do not let sin reign in your mortal body so that you obey its evil desires."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not let sin control the way you live; do not give in to sinful desires."
      }
    },
