    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "绝不可以！我们既然向罪死了，岂可继续活在罪中？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "絕對不可！我們向罪死了的人，怎麼可以仍然活在罪中呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "斷乎不可！我們在罪上死了的人豈可仍在罪中活着呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May it never be! How shall we who died to sin still live in it?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "By no means! We are those who have died to sin; how can we live in it any longer?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Of course not! Since we have died to sin, how can we continue to live in it?"
      }
    },
