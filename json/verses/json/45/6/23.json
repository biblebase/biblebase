    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为罪的代价就是死亡，而上帝借着主基督耶稣赐下的礼物则是永生。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為罪的工價就是死，但　神的恩賞，在我們的主基督耶穌裡，卻是永生。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為罪的工價乃是死；惟有神的恩賜，在我們的主基督耶穌裏，乃是永生。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the wages of sin is death, but the free gift of God is eternal life in Christ Jesus our Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the wages of sin is death, but the gift of God is eternal life in Christ Jesus our Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the wages of sin is death, but the free gift of God is eternal life through Christ Jesus our Lord."
      }
    },
