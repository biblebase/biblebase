    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂死，是向罪死，只死一次；祂活，却是向上帝永远活着。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他死，是向罪死了，只有這一次；他活，是向　神活著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他死是向罪死了，只有一次；他活是向神活着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the death that He died, He died to sin once for all; but the life that He lives, He lives to God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The death he died, he died to sin once for all; but the life he lives, he lives to God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When he died, he died once to break the power of sin. But now that he lives, he lives for the glory of God."
      }
    },
