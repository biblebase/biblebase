    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们知道，我们的旧人已经和祂一同钉在十字架上，使辖制我们身体的罪失去力量，使我们不再做罪的奴隶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們知道，我們的舊人已經與基督同釘十字架，使罪身喪失機能，使我們不再作罪的奴僕，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為知道我們的舊人和他同釘十字架，使罪身滅絕，叫我們不再作罪的奴僕；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "knowing this, that our old self was crucified with Him, in order that our body of sin might be done away with, so that we would no longer be slaves to sin;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For we know that our old self was crucified with him so that the body ruled by sin might be done away with, that we should no longer be slaves to sin—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We know that our old sinful selves were crucified with Christ so that sin might lose its power in our lives. We are no longer slaves to sin."
      }
    },
