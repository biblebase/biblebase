    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，我现在对那些熟悉律法的人说：你们难道不知道律法只在人活着的时候管辖人吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，我對懂得律法的人說，你們不曉得律法管轄人，是在他活著的期間嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，我現在對明白律法的人說，你們豈不曉得律法管人是在活着的時候嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Or do you not know, brethren (for I am speaking to those who know the law), that the law has jurisdiction over a person as long as he lives?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do you not know, brothers and sisters—for I am speaking to those who know the law—that the law has authority over someone only as long as that person lives?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now, dear brothers and sisters—you who are familiar with the law—don’t you know that the law applies only while a person is living?"
      }
    },
