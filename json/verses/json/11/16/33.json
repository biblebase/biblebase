    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他还造了亚舍拉神像，所作所为比先前诸王更惹以色列的上帝耶和华发怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞哈又做了亞舍拉，亞哈所行的，惹耶和華以色列的　神發怒，比他以前所有的以色列王更厲害。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈又做亞舍拉，他所行的惹耶和華－以色列神的怒氣，比他以前的以色列諸王更甚。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ahab also made the Asherah. Thus Ahab did more to provoke the Lord God of Israel than all the kings of Israel who were before him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ahab also made an Asherah pole and did more to arouse the anger of the Lord, the God of Israel, than did all the kings of Israel before him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he set up an Asherah pole. He did more to provoke the anger of the Lord, the God of Israel, than any of the other kings of Israel before him."
      }
    },
