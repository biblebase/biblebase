    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他吩咐仆人：“你去向海的方向观看。”仆人就去观看，然后说：“什么也没有。”以利亚又让他去观看，前后共七次。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以利亞對自己的僕人說：“你上去向海那邊觀看。”他就上去觀看，說：“甚麼也沒有。”以利亞說：“你再回去觀看。”這樣說了七次。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對僕人說：「你上去，向海觀看。」僕人就上去觀看，說：「沒有甚麼。」他說：「你再去觀看。」如此七次。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to his servant, “Go up now, look toward the sea.” So he went up and looked and said, “There is nothing.” And he said, “Go back” seven times."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Go and look toward the sea,” he told his servant. And he went up and looked. “There is nothing there,” he said. Seven times Elijah said, “Go back.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said to his servant, “Go and look out toward the sea.” The servant went and looked, then returned to Elijah and said, “I didn’t see anything.” Seven times Elijah told him to go and look."
      }
    },
