    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "先知又来见以色列王，说：“你要加强防卫，做好准备，因为明年春天亚兰王必卷土重来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那先知前來見以色列王，對他說：“你要自強，留意看看你所要預備的，因為明年春天亞蘭王必再上來攻擊你。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那先知來見以色列王，對他說：「你當自強，留心怎樣防備；因為到明年這時候，亞蘭王必上來攻擊你。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the prophet came near to the king of Israel and said to him, “Go, strengthen yourself and observe and see what you have to do; for at the turn of the year the king of Aram will come up against you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Afterward, the prophet came to the king of Israel and said, “Strengthen your position and see what must be done, because next spring the king of Aram will attack you again.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Afterward the prophet said to King Ahab, “Get ready for another attack. Begin making plans now, for the king of Aram will come back next spring.”"
      }
    },
