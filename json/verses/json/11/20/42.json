    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他对王说：“耶和华说，‘你放走了我决定要毁灭的人，所以你的命要抵他的命，你百姓的命要抵他百姓的命。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "先知對王說：“耶和華這樣說：因為你把我要滅絕的人從你手中放走，所以要你替他償命。要你的人民抵償他的人民。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對王說：「耶和華如此說：『因你將我定要滅絕的人放去，你的命就必代替他的命，你的民也必代替他的民。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to him, “Thus says the Lord, ‘Because you have let go out of your hand the man whom I had devoted to destruction, therefore your life shall go for his life, and your people for his people.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to the king, “This is what the Lord says: ‘You have set free a man I had determined should die. Therefore it is your life for his life, your people for his people.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The prophet said to him, “This is what the Lord says: Because you have spared the man I said must be destroyed, now you must die in his place, and your people will die instead of his people.”"
      }
    },
