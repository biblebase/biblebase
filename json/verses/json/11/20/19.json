    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "省长属下的青年军率先出城，大军紧随其后，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從城裡出來的這些人，就是跟從各省省長的年輕人，軍兵跟在他們後面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "跟從省長的少年人出城，軍兵跟隨他們；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So these went out from the city, the young men of the rulers of the provinces, and the army which followed them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The junior officers under the provincial commanders marched out of the city with the army behind them"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Ahab’s provincial commanders and the entire army had now come out to fight."
      }
    },
