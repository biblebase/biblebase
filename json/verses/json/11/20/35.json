    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众先知中有一位奉耶和华的命令对他的同伴说：“你打我吧！”同伴却不肯动手。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "先知門徒中有一個人奉耶和華的命令對他的一個同伴說：“你打我吧！”可是，那人卻不肯打他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有先知的一個門徒奉耶和華的命對他的同伴說：「你打我吧！」那人不肯打他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now a certain man of the sons of the prophets said to another by the word of the Lord, “Please strike me.” But the man refused to strike him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "By the word of the Lord one of the company of the prophets said to his companion, “Strike me with your weapon,” but he refused."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Meanwhile, the Lord instructed one of the group of prophets to say to another man, “Hit me!” But the man refused to hit the prophet."
      }
