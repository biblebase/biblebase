    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在，耶和华已经把谎言之灵放进这些先知口中，耶和华已决意降祸给你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在，耶和華已經把說謊的靈放在你這些先知的口中，耶和華已經命定災禍臨到你。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在耶和華使謊言的靈入了你這些先知的口，並且耶和華已經命定降禍與你。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now therefore, behold, the Lord has put a deceiving spirit in the mouth of all these your prophets; and the Lord has proclaimed disaster against you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“So now the Lord has put a deceiving spirit in the mouths of all these prophets of yours. The Lord has decreed disaster for you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“So you see, the Lord has put a lying spirit in the mouths of all your prophets. For the Lord has pronounced your doom.”"
      }
    },
