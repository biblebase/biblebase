    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚兰王已经吩咐他的三十二名战车长不要跟对方的大小军兵交锋，只攻击以色列王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞蘭王曾經囑咐他的三十二個戰車長說：“無論大小將兵，你們都不要與他們爭戰，只要與以色列王爭戰。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "先是亞蘭王吩咐他的三十二個車兵長說：「他們的兵將，無論大小，你們都不可與他們爭戰，只要與以色列王爭戰。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the king of Aram had commanded the thirty-two captains of his chariots, saying, “Do not fight with small or great, but with the king of Israel alone.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the king of Aram had ordered his thirty-two chariot commanders, “Do not fight with anyone, small or great, except the king of Israel.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Meanwhile, the king of Aram had issued these orders to his thirty-two chariot commanders: “Attack only the king of Israel. Don’t bother with anyone else!”"
      }
    },
