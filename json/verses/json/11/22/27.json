    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "告诉他们，‘王说，要把这人关在监牢，只给他一些饼和水，直到我平安地回来。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說：‘王這樣吩咐：把這人囚在監裡，少給他食物和水，直到我平平安安回來。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王如此說，把這個人下在監裏，使他受苦，吃不飽喝不足，等候我平平安安地回來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and say, ‘Thus says the king, “Put this man in prison and feed him sparingly with bread and water until I return safely.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and say, ‘This is what the king says: Put this fellow in prison and give him nothing but bread and water until I return safely.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give them this order from the king: ‘Put this man in prison, and feed him nothing but bread and water until I return safely from the battle!’”"
      }
    },
