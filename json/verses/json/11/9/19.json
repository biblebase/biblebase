    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他还建造了所有的储货城、屯车城、养马城和计划在耶路撒冷、黎巴嫩及全国兴建的城邑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以及屬於所羅門所有的一切積貨城、屯車城、馬兵城，與所羅門在耶路撒冷、黎巴嫩和他統治的各地他喜愛建造的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又建造所有的積貨城，並屯車和馬兵的城，與耶路撒冷、黎巴嫩，以及自己治理的全國中所願建造的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and all the storage cities which Solomon had, even the cities for his chariots and the cities for his horsemen, and all that it pleased Solomon to build in Jerusalem, in Lebanon, and in all the land under his rule."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "as well as all his store cities and the towns for his chariots and for his horses—whatever he desired to build in Jerusalem, in Lebanon and throughout all the territory he ruled."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He built towns as supply centers and constructed towns where his chariots and horses could be stationed. He built everything he desired in Jerusalem and Lebanon and throughout his entire realm."
      }
    },
