    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "殿前的门廊和殿一样宽九米，深四米半。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "殿的前廊長九公尺，和殿的寬度一樣，寬四公尺半。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "殿前的廊子長二十肘，與殿的寬窄一樣，闊十肘；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The porch in front of the nave of the house was twenty cubits in length, corresponding to the width of the house, and its depth along the front of the house was ten cubits."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The portico at the front of the main hall of the temple extended the width of the temple, that is twenty cubits, and projected ten cubits from the front of the temple."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The entry room at the front of the Temple was 30 feet wide, running across the entire width of the Temple. It projected outward 15 feet from the front of the Temple."
      }
    },
