    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "底层厢房的进口在殿的南面，沿着一道螺旋楼梯可以通到第二层和第三层。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "廂房的下層門在殿的右邊，人可以從螺旋形梯子上廂房的中層，再從中層上到第三層。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在殿右邊當中的旁屋有門，門內有旋螺的樓梯，可以上到第二層，從第二層可以上到第三層。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The doorway for the lowest side chamber was on the right side of the house; and they would go up by winding stairs to the middle story, and from the middle to the third."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The entrance to the lowest floor was on the south side of the temple; a stairway led up to the middle level and from there to the third."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The entrance to the bottom floor was on the south side of the Temple. There were winding stairs going up to the second floor, and another flight of stairs between the second and third floors."
      }
    },
