    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天下列王风闻所罗门的智慧，都纷纷遣使来求教。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬族之中和地上列王之中，聽聞所羅門的智慧的，都有人來聽所羅門的智慧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天下列王聽見所羅門的智慧，就都差人來聽他的智慧話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Men came from all peoples to hear the wisdom of Solomon, from all the kings of the earth who had heard of his wisdom."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From all nations people came to listen to Solomon’s wisdom, sent by all the kings of the world, who had heard of his wisdom."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And kings from every nation sent their ambassadors to listen to the wisdom of Solomon."
      }
    },
