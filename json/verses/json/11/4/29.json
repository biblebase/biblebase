    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝赐予所罗门无比的智慧、悟性和渊博的学识。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神賜給所羅門智慧、極大的聰明和廣大的心，好像海邊的沙那樣無限無量。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神賜給所羅門極大的智慧聰明和廣大的心，如同海沙不可測量。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now God gave Solomon wisdom and very great discernment and breadth of mind, like the sand that is on the seashore."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God gave Solomon wisdom and very great insight, and a breadth of understanding as measureless as the sand on the seashore."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God gave Solomon very great wisdom and understanding, and knowledge as vast as the sands of the seashore."
      }
    },
