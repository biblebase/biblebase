    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他做了耶和华视为恶的事，没有像他父亲大卫那样忠心顺服耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門行耶和華看為惡的事，不效法他的父親大衛完完全全跟從耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門行耶和華眼中看為惡的事，不效法他父親大衛專心順從耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Solomon did what was evil in the sight of the Lord, and did not follow the Lord fully, as David his father had done."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Solomon did evil in the eyes of the Lord; he did not follow the Lord completely, as David his father had done."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In this way, Solomon did what was evil in the Lord’s sight; he refused to follow the Lord completely, as his father, David, had done."
      }
