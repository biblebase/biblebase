    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说：“你既然违背我的命令，不遵守我的约和律例，我必夺走你的国，将它赐给你的一个臣仆。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對所羅門說：“你既然作了這事，不遵守我吩咐你的約和律例，我必奪去你的國，把國賜給你的一個臣子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以耶和華對他說：「你既行了這事，不遵守我所吩咐你守的約和律例，我必將你的國奪回，賜給你的臣子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the Lord said to Solomon, “Because you have done this, and you have not kept My covenant and My statutes, which I have commanded you, I will surely tear the kingdom from you, and will give it to your servant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the Lord said to Solomon, “Since this is your attitude and you have not kept my covenant and my decrees, which I commanded you, I will most certainly tear the kingdom away from you and give it to one of your subordinates."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So now the Lord said to him, “Since you have not kept my covenant and have disobeyed my decrees, I will surely tear the kingdom away from you and give it to one of your servants."
      }
    },
