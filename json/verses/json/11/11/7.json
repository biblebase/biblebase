    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在耶路撒冷东面的山上，所罗门为摩押人可憎的神明基抹和亚扪人可憎的神明摩洛修建丘坛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，所羅門為摩押人可憎的偶像基抹和亞捫人可憎的偶像摩洛，在耶路撒冷東面的山上建造邱壇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門為摩押可憎的神基抹和亞捫人可憎的神摩洛，在耶路撒冷對面的山上建築邱壇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Solomon built a high place for Chemosh the detestable idol of Moab, on the mountain which is east of Jerusalem, and for Molech the detestable idol of the sons of Ammon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On a hill east of Jerusalem, Solomon built a high place for Chemosh the detestable god of Moab, and for Molek the detestable god of the Ammonites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On the Mount of Olives, east of Jerusalem, he even built a pagan shrine for Chemosh, the detestable god of Moab, and another for Molech, the detestable god of the Ammonites."
      }
    },
