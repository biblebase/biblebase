    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时，哈达年纪还小，他和他父亲的几个以东臣仆一起逃往埃及。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "哈達與他父親的幾個以東臣僕一起逃往埃及，那時哈達還是一個小孩子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時哈達還是幼童；他和他父親的臣僕，幾個以東人逃往埃及。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that Hadad fled to Egypt, he and certain Edomites of his father’s servants with him, while Hadad was a young boy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Hadad, still only a boy, fled to Egypt with some Edomite officials who had served his father."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Hadad and a few of his father’s royal officials escaped and headed for Egypt. (Hadad was just a boy at the time.)"
      }
    },
