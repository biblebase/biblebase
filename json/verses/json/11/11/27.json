    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以下是他反叛所罗门的缘由： 当年所罗门建造米罗堡，修补大卫城的城墙时，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他舉手攻擊王的情形是這樣的：所羅門重建米羅，修補他父親大衛城的破口；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他舉手攻擊王的緣故，乃由先前所羅門建造米羅，修補他父親大衛城的破口。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now this was the reason why he rebelled against the king: Solomon built the Millo, and closed up the breach of the city of his father David."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Here is the account of how he rebelled against the king: Solomon had built the terraces and had filled in the gap in the wall of the city of David his father."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is the story behind his rebellion. Solomon was rebuilding the supporting terraces and repairing the walls of the city of his father, David."
      }
    },
