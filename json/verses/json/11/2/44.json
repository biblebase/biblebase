    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王又说：“你以前如何恶待我父大卫，你自己心里明白，现在耶和华要追究你的罪行了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王又對示每說：“你自己知道你心裡的一切惡事，就是你向我父親大衛所行的；現在耶和華要把你的罪歸到你的頭上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王又對示每說：「你向我父親大衛所行的一切惡事，你自己心裏也知道，所以耶和華必使你的罪惡歸到自己的頭上；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king also said to Shimei, “You know all the evil which you acknowledge in your heart, which you did to my father David; therefore the Lord shall return your evil on your own head."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king also said to Shimei, “You know in your heart all the wrong you did to my father David. Now the Lord will repay you for your wrongdoing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king also said to Shimei, “You certainly remember all the wicked things you did to my father, David. May the Lord now bring that evil on your own head."
      }
    },
