    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "示每说：“好，仆人必照我主我王说的做。”示每就一直住在耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "示每對王說：“這話很好，我主我王怎麼說，你僕人就照著行。”於是示每在耶路撒冷住了很久。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "示每對王說：「這話甚好！我主我王怎樣說，僕人必怎樣行。」於是示每多日住在耶路撒冷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Shimei then said to the king, “The word is good. As my lord the king has said, so your servant will do.” So Shimei lived in Jerusalem many days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Shimei answered the king, “What you say is good. Your servant will do as my lord the king has said.” And Shimei stayed in Jerusalem for a long time."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Shimei replied, “Your sentence is fair; I will do whatever my lord the king commands.” So Shimei lived in Jerusalem for a long time."
      }
    },
