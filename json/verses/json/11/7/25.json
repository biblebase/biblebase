    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有十二头铜牛驮着铜海，三头向北，三头向西，三头向南，三头向东。铜海安在牛上，牛尾都向内。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "銅海安放在十二頭銅牛背上，三頭向北，三頭向西，三頭向南，三頭向東，銅海是在銅牛之上，牛尾都向內。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有十二隻銅牛馱海：三隻向北，三隻向西，三隻向南，三隻向東；海在牛上，牛尾都向內。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It stood on twelve oxen, three facing north, three facing west, three facing south, and three facing east; and the sea was set on top of them, and all their rear parts turned inward."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Sea stood on twelve bulls, three facing north, three facing west, three facing south and three facing east. The Sea rested on top of them, and their hindquarters were toward the center."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Sea was placed on a base of twelve bronze oxen, all facing outward. Three faced north, three faced west, three faced south, and three faced east, and the Sea rested on them."
      }
    },
