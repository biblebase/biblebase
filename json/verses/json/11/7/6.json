    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他又建了一座有柱子的廊子，长二十二米半，宽十三米半，前面还有带柱子的门廊和篷子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又建造柱廊，長二十二公尺，寬十三公尺半。在柱廊前有廊子，在廊子前又有柱子和臺階。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並建造有柱子的廊子，長五十肘，寬三十肘；在這廊前又有廊子，廊外有柱子和臺階。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he made the hall of pillars; its length was 50 cubits and its width 30 cubits, and a porch was in front of them and pillars and a threshold in front of them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He made a colonnade fifty cubits long and thirty wide. In front of it was a portico, and in front of that were pillars and an overhanging roof."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon also built the Hall of Pillars, which was 75 feet long and 45 feet wide. There was a porch in front, along with a canopy supported by pillars."
      }
