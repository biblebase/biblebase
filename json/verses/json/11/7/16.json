    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后用铜铸造了两个高二点二五米的柱冠，安在柱顶上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又用銅鑄造了兩個柱頂，安放在柱頭上，每個柱頂高兩公尺兩公寸。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又用銅鑄了兩個柱頂安在柱上，各高五肘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also made two capitals of molten bronze to set on the tops of the pillars; the height of the one capital was five cubits and the height of the other capital was five cubits."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He also made two capitals of cast bronze to set on the tops of the pillars; each capital was five cubits high."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the tops of the pillars he cast bronze capitals, each 71/2 feet tall."
      }
