      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他又铸造了一个圆形的铜海，高二点二五米，直径四点五米，周长十三点五米。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又鑄造了一座圓形的銅海，從這邊到那邊四公尺四公寸，高兩公尺兩公寸，圓周十三公尺兩公寸。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又鑄一個銅海，樣式是圓的，高五肘，徑十肘，圍三十肘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now he made the sea of cast metal ten cubits from brim to brim, circular in form, and its height was five cubits, and thirty cubits in circumference."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He made the Sea of cast metal, circular in shape, measuring ten cubits from rim to rim and five cubits high. It took a line of thirty cubits to measure around it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Huram cast a great round basin, 15 feet across from rim to rim, called the Sea. It was 71/2 feet deep and about 45 feet in circumference."
      }
    },
