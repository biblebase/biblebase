    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "至于耶洗别，耶和华说，‘狗必在耶斯列的城墙边吃耶洗别的肉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於耶洗別，耶和華也說：‘狗必在耶斯列的地方吃耶洗別的肉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論到耶洗別，耶和華也說：『狗在耶斯列的外郭必吃耶洗別的肉。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Of Jezebel also has the Lord spoken, saying, ‘The dogs will eat Jezebel in the district of Jezreel.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“And also concerning Jezebel the Lord says: ‘Dogs will devour Jezebel by the wall of Jezreel.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And regarding Jezebel, the Lord says, ‘Dogs will eat Jezebel’s body at the plot of land in Jezreel.’"
      }
    },
