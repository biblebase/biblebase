    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚哈见了以利亚，说：“我的对头啊，你又来了！”以利亚说：“我又来了，因为你一心做耶和华视为恶的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞哈對以利亞說：“我的仇人哪，你找到了我嗎？”以利亞說：“我找到你了，因為你出賣了你自己，去行耶和華看為惡的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈對以利亞說：「我仇敵啊，你找到我嗎？」他回答說：「我找到你了；因為你賣了自己，行耶和華眼中看為惡的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ahab said to Elijah, “Have you found me, O my enemy?” And he answered, “I have found you, because you have sold yourself to do evil in the sight of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ahab said to Elijah, “So you have found me, my enemy!” “I have found you,” he answered, “because you have sold yourself to do evil in the eyes of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“So, my enemy, you have found me!” Ahab exclaimed to Elijah. “Yes,” Elijah answered, “I have come because you have sold yourself to what is evil in the Lord’s sight."
      }
    },
