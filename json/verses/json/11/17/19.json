    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利亚对妇人说：“把你儿子交给我。”随后，他从妇人怀中接过孩子，抱到楼上自己的房间，放在床上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以利亞對她說：“把你的兒子交給我吧！”以利亞就從她的懷中把孩子接過來，抱他上到自己所住的樓上去，放在自己的床上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利亞對她說：「把你兒子交給我。」以利亞就從婦人懷中將孩子接過來，抱到他所住的樓中，放在自己的牀上，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to her, “Give me your son.” Then he took him from her bosom and carried him up to the upper room where he was living, and laid him on his own bed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Give me your son,” Elijah replied. He took him from her arms, carried him to the upper room where he was staying, and laid him on his bed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Elijah replied, “Give me your son.” And he took the child’s body from her arms, carried him up the stairs to the room where he was staying, and laid the body on his bed."
      }
    },
