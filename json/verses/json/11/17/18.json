    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "妇人对以利亚说：“上帝的仆人啊，我跟你有什么关系？你到我这里来是要让上帝想起我的罪，取我儿子的性命吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是婦人對以利亞說：“神人哪，我跟你有甚麼關係呢？你竟到我這裡來，使耶和華想起我的罪孽，殺死我的兒子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "婦人對以利亞說：「神人哪，我與你何干？你竟到我這裏來，使神想念我的罪，以致我的兒子死呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So she said to Elijah, “What do I have to do with you, O man of God? You have come to me to bring my iniquity to remembrance and to put my son to death!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She said to Elijah, “What do you have against me, man of God? Did you come to remind me of my sin and kill my son?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then she said to Elijah, “O man of God, what have you done to me? Have you come here to point out my sins and kill my son?”"
      }
    },
