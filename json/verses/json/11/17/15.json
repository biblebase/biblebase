    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那妇人就依照以利亚的话做了。妇人一家和以利亚就这样吃了许多天，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她就去照著以利亞所說的作了。她和以利亞，以及她的家人，吃了許多日子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "婦人就照以利亞的話去行。她和她家中的人，並以利亞，吃了許多日子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So she went and did according to the word of Elijah, and she and he and her household ate for many days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She went away and did as Elijah had told her. So there was food every day for Elijah and for the woman and her family."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So she did as Elijah said, and she and Elijah and her family continued to eat for many days."
      }
