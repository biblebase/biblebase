    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你离开这里，往东去，到约旦河以东的基立溪旁躲起来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你離開這裡，向東方去，躲藏在約旦河東面的基立溪旁。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你離開這裏往東去，藏在約旦河東邊的基立溪旁。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Go away from here and turn eastward, and hide yourself by the brook Cherith, which is east of the Jordan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Leave here, turn eastward and hide in the Kerith Ravine, east of the Jordan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Go to the east and hide by Kerith Brook, near where it enters the Jordan River."
      }
    },
