    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利亚遵从耶和华的话，到约旦河东的基立溪旁住下来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是以利亞去了，照著耶和華的話行了；他去住在約旦河東面的基立溪旁。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是以利亞照着耶和華的話，去住在約旦河東的基立溪旁。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he went and did according to the word of the Lord, for he went and lived by the brook Cherith, which is east of the Jordan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So he did what the Lord had told him. He went to the Kerith Ravine, east of the Jordan, and stayed there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Elijah did as the Lord told him and camped beside Kerith Brook, east of the Jordan."
      }
    },
