    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿我们的上帝耶和华与我们同在，就像与我们的祖先同在一样。愿祂永远都不撇下我们，不丢弃我们，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願耶和華我們的　神與我們同在，像與我們的列祖同在一樣。願他不離棄我們，也不撇下我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願耶和華－我們的神與我們同在，像與我們列祖同在一樣，不撇下我們，不丟棄我們，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May the Lord our God be with us, as He was with our fathers; may He not leave us or forsake us,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the Lord our God be with us as he was with our ancestors; may he never leave us nor forsake us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May the Lord our God be with us as he was with our ancestors; may he never leave us or abandon us."
      }
