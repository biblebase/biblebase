    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“当你的以色列子民因得罪你而败在敌人手中时，如果他们回心转意归向你，承认你的名，在这殿里祈祷，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你的子民以色列若是得罪了你，以致在仇敵面前被打敗，又回轉歸向你，承認你的名，在這殿裡向你禱告懇求的時候，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你的民以色列若得罪你，敗在仇敵面前，又歸向你，承認你的名，在這殿裏祈求禱告，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“When Your people Israel are defeated before an enemy, because they have sinned against You, if they turn to You again and confess Your name and pray and make supplication to You in this house,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When your people Israel have been defeated by an enemy because they have sinned against you, and when they turn back to you and give praise to your name, praying and making supplication to you in this temple,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If your people Israel are defeated by their enemies because they have sinned against you, and if they turn to you and acknowledge your name and pray to you here in this Temple,"
      }
    },
