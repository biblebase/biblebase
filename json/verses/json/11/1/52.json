    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门说：“如果他是个忠义的人，必毫发无损；如果他作恶，必死无疑。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門說：“他若是作一個忠義的人，他的頭髮連一根也不會掉在地上；但在他身上若是發現有甚麼惡行，他必定死。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門說：「他若作忠義的人，連一根頭髮也不致落在地上；他若行惡，必要死亡。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Solomon said, “If he is a worthy man, not one of his hairs will fall to the ground; but if wickedness is found in him, he will die.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Solomon replied, “If he shows himself to be worthy, not a hair of his head will fall to the ground; but if evil is found in him, he will die.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon replied, “If he proves himself to be loyal, not a hair on his head will be touched. But if he makes trouble, he will die.”"
      }
    },
