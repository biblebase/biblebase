    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拔示芭回答说：“我主啊，你曾经凭你的上帝耶和华起誓说让臣妾的儿子所罗门继承你的王位，坐你的宝座。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她回答說：“我主啊，你曾經指著耶和華你的　神向婢女起誓，說：‘你的兒子所羅門必接續我作王，他要坐在我的王位上’，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她說：「我主啊，你曾向婢女指着耶和華－你的神起誓說：『你兒子所羅門必接續我作王，坐在我的位上。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She said to him, “My lord, you swore to your maidservant by the Lord your God, saying, ‘Surely your son Solomon shall be king after me and he shall sit on my throne.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She said to him, “My lord, you yourself swore to me your servant by the Lord your God: ‘Solomon your son shall be king after me, and he will sit on my throne.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She replied, “My lord, you made a vow before the Lord your God when you said to me, ‘Your son Solomon will surely be the next king and will sit on my throne.’"
      }
    },
