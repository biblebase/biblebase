    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，他们在以色列全国寻找美貌女子，找到了一位名叫雅比莎的书念少女，把她带到王那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他們在以色列全境尋找一個美貌的少女，結果找著了一個書念的女子亞比煞，就把她帶到王面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是在以色列全境尋找美貌的童女，尋得書念的一個童女亞比煞，就帶到王那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they searched for a beautiful girl throughout all the territory of Israel, and found Abishag the Shunammite, and brought her to the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they searched throughout Israel for a beautiful young woman and found Abishag, a Shunammite, and brought her to the king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So they searched throughout the land of Israel for a beautiful girl, and they found Abishag from Shunem and brought her to the king."
      }
    },
