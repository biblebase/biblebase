    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他派人回复所罗门说：“我已收到你派人带来的口信。我一定会照你的心愿提供香柏木和松木。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是希蘭派遣人去見所羅門，說：“你派人來向我所傳的消息，我都聽到了。關於香柏木和松木的事，你心所願的，我都照辦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希蘭打發人去見所羅門，說：「你差遣人向我所提的那事，我都聽見了；論到香柏木和松木，我必照你的心願而行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Hiram sent word to Solomon, saying, “I have heard the message which you have sent me; I will do what you desire concerning the cedar and cypress timber."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Hiram sent word to Solomon: “I have received the message you sent me and will do all you want in providing the cedar and juniper logs."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he sent this reply to Solomon: “I have received your message, and I will supply all the cedar and cypress timber you need."
      }
    },
