    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为耶罗波安犯罪作孽，使以色列人陷入罪中，耶和华必抛弃以色列。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為了耶羅波安所犯的罪，又為了他使以色列人所犯的罪，耶和華必丟棄以色列人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因耶羅波安所犯的罪，又使以色列人陷在罪裏，耶和華必將以色列人交給仇敵。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will give up Israel on account of the sins of Jeroboam, which he committed and with which he made Israel to sin.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he will give Israel up because of the sins Jeroboam has committed and has caused Israel to commit.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will abandon Israel because Jeroboam sinned and made Israel sin along with him.”"
      }
    },
