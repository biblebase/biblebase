    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的上帝耶和华啊，你让仆人继承我父亲大卫的王位，但仆人还年幼无知，不懂得如何治理国家。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華我的　神啊，現在你使僕人接續我的父親大衛作王，但我還年輕，不知道應當怎樣處事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－我的神啊，如今你使僕人接續我父親大衛作王；但我是幼童，不知道應當怎樣出入。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now, O Lord my God, You have made Your servant king in place of my father David, yet I am but a little child; I do not know how to go out or come in."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Now, Lord my God, you have made your servant king in place of my father David. But I am only a little child and do not know how to carry out my duties."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Now, O Lord my God, you have made me king instead of my father, David, but I am like a little child who doesn’t know his way around."
      }
