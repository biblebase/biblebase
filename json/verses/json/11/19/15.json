    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华吩咐他说：“你原路返回，前往大马士革附近的旷野，到了之后要膏立哈薛做亚兰王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對他說：“你離去吧，從你來的路回到大馬士革的曠野去。你到了那裡，就要膏哈薛作亞蘭王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對他說：「你回去，從曠野往大馬士革去。到了那裏，就要膏哈薛作亞蘭王，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord said to him, “Go, return on your way to the wilderness of Damascus, and when you have arrived, you shall anoint Hazael king over Aram;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said to him, “Go back the way you came, and go to the Desert of Damascus. When you get there, anoint Hazael king over Aram."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord told him, “Go back the same way you came, and travel to the wilderness of Damascus. When you arrive there, anoint Hazael to be king of Aram."
      }
    },
