    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我们之间应立盟约，像我们的父亲之间立约一样，现在奉上金银为礼，请你取消与以色列王巴沙立的盟约。这样，他就会从我境内退兵。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你父與我父之間曾經立過約，我也要與你立約。現在我把金銀送給你作禮物，請你去廢除與以色列王巴沙所立的約，好使他離開我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：「你父曾與我父立約，我與你也要立約。現在我將金銀送你為禮物，求你廢掉你與以色列王巴沙所立的約，使他離開我。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Let there be a treaty between you and me, as between my father and your father. Behold, I have sent you a present of silver and gold; go, break your treaty with Baasha king of Israel so that he will withdraw from me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Let there be a treaty between me and you,” he said, “as there was between my father and your father. See, I am sending you a gift of silver and gold. Now break your treaty with Baasha king of Israel so he will withdraw from me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Let there be a treaty between you and me like the one between your father and my father. See, I am sending you a gift of silver and gold. Break your treaty with King Baasha of Israel so that he will leave me alone.”"
      }
    },
