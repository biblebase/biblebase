    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的臣仆能经常侍立在你面前聆听智慧之言，真有福气！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "屬你的人是有福的，你的這些臣僕是有福的，他們常常侍立在你面前，聆聽你的智慧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的臣子、你的僕人常侍立在你面前聽你智慧的話是有福的！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How blessed are your men, how blessed are these your servants who stand before you continually and hear your wisdom."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How happy your people must be! How happy your officials, who continually stand before you and hear your wisdom!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How happy your people must be! What a privilege for your officials to stand here day after day, listening to your wisdom!"
      }
    },
