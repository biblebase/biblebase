    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "示巴女王听说所罗门因耶和华而名声大震，便来用难题考问他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "示巴女王聽到所羅門因耶和華的名所得的名聲，就來要用難題試試他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "示巴女王聽見所羅門因耶和華之名所得的名聲，就來要用難解的話試問所羅門。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when the queen of Sheba heard about the fame of Solomon concerning the name of the Lord, she came to test him with difficult questions."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the queen of Sheba heard about the fame of Solomon and his relationship to the Lord, she came to test Solomon with hard questions."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the queen of Sheba heard of Solomon’s fame, which brought honor to the name of the Lord, she came to test him with hard questions."
      }
    },
