    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她对所罗门说：“我在本国听到的有关你的功业和智慧原来都是真的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是她對王說：“我在本國聽見有關你的事和你的智慧，原來都是真的；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對王說：「我在本國裏所聽見論到你的事和你的智慧實在是真的！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then she said to the king, “It was a true report which I heard in my own land about your words and your wisdom."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She said to the king, “The report I heard in my own country about your achievements and your wisdom is true."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She exclaimed to the king, “Everything I heard in my country about your achievements and wisdom is true!"
      }
