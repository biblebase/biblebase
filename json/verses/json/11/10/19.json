    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这宝座有六级台阶，靠背是圆形的，两旁有扶手，扶手两边各站着一头狮子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那寶座有六級臺階，寶座的後背有一個圓頂，座處的兩旁有一個扶手。扶手的旁邊各有一隻獅子站立著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "寶座有六層臺階，座的後背是圓的，兩旁有扶手，靠近扶手有兩個獅子站立。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There were six steps to the throne and a round top to the throne at its rear, and arms on each side of the seat, and two lions standing beside the arms."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The throne had six steps, and its back had a rounded top. On both sides of the seat were armrests, with a lion standing beside each of them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The throne had six steps and a rounded back. There were armrests on both sides of the seat, and the figure of a lion stood on each side of the throne."
      }
    },
