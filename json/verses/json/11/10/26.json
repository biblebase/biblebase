    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门组建了战车和骑兵，有一千四百辆战车、一万二千名骑兵，驻扎在屯车城和他所在的耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門聚集戰車和騎兵，他有戰車一千四百輛，騎兵一萬二千人，安置在屯車城和耶路撒冷，就是與王在一起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門聚集戰車馬兵，有戰車一千四百輛，馬兵一萬二千名，安置在屯車的城邑和耶路撒冷，就是王那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Solomon gathered chariots and horsemen; and he had 1,400 chariots and 12,000 horsemen, and he stationed them in the chariot cities and with the king in Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Solomon accumulated chariots and horses; he had fourteen hundred chariots and twelve thousand horses, which he kept in the chariot cities and also with him in Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon built up a huge force of chariots and horses. He had 1,400 chariots and 12,000 horses. He stationed some of them in the chariot cities and some near him in Jerusalem."
      }
    },
