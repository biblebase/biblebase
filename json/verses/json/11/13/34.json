    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这罪使耶罗波安家走向没落，最终彻底灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這事成為耶羅波安的家的罪，以致他的家從地上被塗抹與除滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這事叫耶羅波安的家陷在罪裏，甚至他的家從地上除滅了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This event became sin to the house of Jeroboam, even to blot it out and destroy it from off the face of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This was the sin of the house of Jeroboam that led to its downfall and to its destruction from the face of the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This became a great sin and resulted in the utter destruction of Jeroboam’s dynasty from the face of the earth."
      }
    },
