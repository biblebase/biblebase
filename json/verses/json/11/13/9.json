    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为耶和华吩咐我不可在伯特利用饭喝水，也不可原路返回。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為有耶和華的話吩咐我說：‘你不可吃飯喝水，也不可從你去的原路回來。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為有耶和華的話囑咐我，說不可在伯特利吃飯喝水，也不可從你去的原路回來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For so it was commanded me by the word of the Lord, saying, ‘You shall eat no bread, nor drink water, nor return by the way which you came.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I was commanded by the word of the Lord: ‘You must not eat bread or drink water or return by the way you came.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Lord gave me this command: ‘You must not eat or drink anything while you are there, and do not return to Judah by the same way you came.’”"
      }
