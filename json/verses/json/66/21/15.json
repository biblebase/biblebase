    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那和我说话的天使拿了一根金量秆要丈量圣城、城墙和城门。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那對我說話的天使拿著一根金的蘆葦，要量那城、城門和城牆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對我說話的，拿着金葦子當尺，要量那城和城門城牆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The one who spoke with me had a gold measuring rod to measure the city, and its gates and its wall."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The angel who talked with me had a measuring rod of gold to measure the city, its gates and its walls."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The angel who talked to me held in his hand a gold measuring stick to measure the city, its gates, and its wall."
      }
