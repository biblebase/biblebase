      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我看见城中没有圣殿，因为全能的主上帝和羔羊就是圣城的殿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我沒有看見城裡有聖所，因為主全能的　神和羊羔就是城的聖所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我未見城內有殿，因主神－全能者和羔羊為城的殿。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I saw no temple in it, for the Lord God the Almighty and the Lamb are its temple."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I did not see a temple in the city, because the Lord God Almighty and the Lamb are its temple."
      },
      "nlt": {
