    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "七位天使手里拿着盛满最后七灾的七个碗，其中有一位对我说：“你来，我要将新娘，就是羔羊的妻子指给你看。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "拿著七個盛滿著末後七災的碗的七位天使中，有一位走來對我說：“你來！我要把新娘，就是羊羔的妻子，指示你。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拿着七個金碗、盛滿末後七災的七位天使中，有一位來對我說：「你到這裏來，我要將新婦，就是羔羊的妻，指給你看。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then one of the seven angels who had the seven bowls full of the seven last plagues came and spoke with me, saying, “Come here, I will show you the bride, the wife of the Lamb.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One of the seven angels who had the seven bowls full of the seven last plagues came and said to me, “Come, I will show you the bride, the wife of the Lamb.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then one of the seven angels who held the seven bowls containing the seven last plagues came and said to me, “Come with me! I will show you the bride, the wife of the Lamb.”"
      }
    },
