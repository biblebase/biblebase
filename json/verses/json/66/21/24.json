    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万民要在圣城的光中行走，地上的君王也要将他们的荣耀带进圣城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "列國要藉著城的光行走，地上的眾王要把他們的榮華帶到這城來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "列國要在城的光裏行走；地上的君王必將自己的榮耀歸與那城。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The nations will walk by its light, and the kings of the earth will bring their glory into it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The nations will walk by its light, and the kings of the earth will bring their splendor into it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The nations will walk in its light, and the kings of the world will enter the city in all their glory."
      }
    },
