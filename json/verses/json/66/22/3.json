    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "再也没有咒诅，因为城里有上帝和羔羊的宝座，祂的奴仆都要事奉祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有咒詛都不再有了。城裡有　神和羊羔的寶座，他的僕人都要事奉他，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以後再沒有咒詛；在城裏有神和羔羊的寶座；他的僕人都要事奉他，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There will no longer be any curse; and the throne of God and of the Lamb will be in it, and His bond-servants will serve Him;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No longer will there be any curse. The throne of God and of the Lamb will be in the city, and his servants will serve him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No longer will there be a curse upon anything. For the throne of God and of the Lamb will be there, and his servants will worship him."
      }
    },
