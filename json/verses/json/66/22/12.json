    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“看啊，我快要来了，到时候我要按各人的行为施行赏罚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪，我必快來！賞賜在我，我要照著各人所行的報應他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「看哪，我必快來！賞罰在我，要照各人所行的報應他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, I am coming quickly, and My reward is with Me, to render to every man according to what he has done."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Look, I am coming soon! My reward is with me, and I will give to each person according to what they have done."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Look, I am coming soon, bringing my reward with me to repay all people according to their deeds."
      }
    },
