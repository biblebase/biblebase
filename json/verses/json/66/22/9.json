    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天使对我说：“千万不可！我与你、你的众先知弟兄和那些遵行这书上话语的人同是上帝的奴仆，你要敬拜上帝。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他對我說：“千萬不可以這樣！我和你，以及你的弟兄眾先知，還有那些遵守這書上的話的人，都是同作僕人的。你應當敬拜　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對我說：「千萬不可！我與你和你的弟兄眾先知，並那些守這書上言語的人，同是作僕人的。你要敬拜神。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But he *said to me, “Do not do that. I am a fellow servant of yours and of your brethren the prophets and of those who heed the words of this book. Worship God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But he said to me, “Don’t do that! I am a fellow servant with you and with your fellow prophets and with all who keep the words of this scroll. Worship God!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But he said, “No, don’t worship me. I am a servant of God, just like you and your brothers the prophets, as well as all who obey what is written in this book. Worship only God!”"
      }
    },
