    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他捉住巨龙，就是那古蛇——魔鬼，又名撒旦，将它捆绑一千年。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他捉住了那龍，那古蛇，就是魔鬼，撒但，把牠捆綁了一千年。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他捉住那龍，就是古蛇，又叫魔鬼，也叫撒但，把牠捆綁一千年，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he laid hold of the dragon, the serpent of old, who is the devil and Satan, and bound him for a thousand years;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He seized the dragon, that ancient serpent, who is the devil, or Satan, and bound him for a thousand years."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He seized the dragon—that old serpent, who is the devil, Satan—and bound him in chains for a thousand years."
      }
