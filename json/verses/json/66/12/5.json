    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "妇人生下一个男婴，就是将来要用铁杖治理列国的。那男婴被提到了上帝的宝座那里，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她生了一個男孩子，就是將來要用鐵杖治理列國的。她的孩子被提取到　神和他寶座那裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "婦人生了一個男孩子，是將來要用鐵杖轄管萬國的；她的孩子被提到神寶座那裏去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And she gave birth to a son, a male child, who is to rule all the nations with a rod of iron; and her child was caught up to God and to His throne."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She gave birth to a son, a male child, who “will rule all the nations with an iron scepter.” And her child was snatched up to God and to his throne."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She gave birth to a son who was to rule all nations with an iron rod. And her child was snatched away from the dragon and was caught up to God and to his throne."
      }
    },
