    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在天上出现了一个奇异的景象：有一个妇人，身披太阳，脚踏月亮，头戴有十二颗星的冠冕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，天上出現了一個奇偉的景象：有一個婦人，身披太陽，腳踏月亮，頭戴十二顆星的冠冕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天上現出大異象來：有一個婦人身披日頭，腳踏月亮，頭戴十二星的冠冕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A great sign appeared in heaven: a woman clothed with the sun, and the moon under her feet, and on her head a crown of twelve stars;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A great sign appeared in heaven: a woman clothed with the sun, with the moon under her feet and a crown of twelve stars on her head."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I witnessed in heaven an event of great significance. I saw a woman clothed with the sun, with the moon beneath her feet, and a crown of twelve stars on her head."
      }
    },
