    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巨龙紧追不舍，张口吐出大量的水，如同江河一般，要冲走那妇人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "蛇在婦人後面，從口中吐出水來，好像江河一樣，要把婦人沖去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "蛇就在婦人身後，從口中吐出水來，像河一樣，要將婦人沖去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the serpent poured water like a river out of his mouth after the woman, so that he might cause her to be swept away with the flood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then from his mouth the serpent spewed water like a river, to overtake the woman and sweep her away with the torrent."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the dragon tried to drown the woman with a flood of water that flowed from his mouth."
      }
