    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这时天上出现了另一个奇异的景象：有一条红色巨龙出现，它有七头十角，每个头上都戴着冠冕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天上又出現了另一個景象。看哪！有一條大紅龍，有七頭十角，七頭上戴著七個皇冠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天上又現出異象來：有一條大紅龍，七頭十角；七頭上戴着七個冠冕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then another sign appeared in heaven: and behold, a great red dragon having seven heads and ten horns, and on his heads were seven diadems."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then another sign appeared in heaven: an enormous red dragon with seven heads and ten horns and seven crowns on its heads."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I witnessed in heaven another significant event. I saw a large red dragon with seven heads and ten horns, with seven crowns on his heads."
      }
    },
