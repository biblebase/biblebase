    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄们是靠着羔羊的血和自己所见证的道战胜了它，他们甘愿牺牲，视死如归。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄勝過牠，是因著羊羔的血， 也因著自己所見證的道， 他們雖然至死， 也不愛惜自己的性命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄勝過牠，是因羔羊的血和自己所見證的道。他們雖至於死，也不愛惜性命。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they overcame him because of the blood of the Lamb and because of the word of their testimony, and they did not love their life even when faced with death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They triumphed over him by the blood of the Lamb and by the word of their testimony; they did not love their lives so much as to shrink from death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And they have defeated him by the blood of the Lamb and by their testimony. And they did not love their lives so much that they were afraid to die."
      }
    },
