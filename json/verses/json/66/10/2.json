    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "手中拿着一卷打开了的小书卷，右脚踏在海中，左脚踏在地上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "手裡拿著展開的小書卷。他的右腳踏在海上，左腳踏在地上，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他手裏拿着小書卷，是展開的。他右腳踏海，左腳踏地，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and he had in his hand a little book which was open. He placed his right foot on the sea and his left on the land;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He was holding a little scroll, which lay open in his hand. He planted his right foot on the sea and his left foot on the land,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And in his hand was a small scroll that had been opened. He stood with his right foot on the sea and his left foot on the land."
      }
