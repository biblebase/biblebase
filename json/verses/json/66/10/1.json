    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我看见另一位大力天使从天而降。他身披云霞，头顶彩虹，脸如太阳，双腿如火柱，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我又看見一位大力的天使從天上降下來，身披雲彩，頭上有彩虹，臉像太陽，兩腳像火柱一樣，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我又看見另有一位大力的天使從天降下，披着雲彩，頭上有虹，臉面像日頭，兩腳像火柱。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I saw another strong angel coming down out of heaven, clothed with a cloud; and the rainbow was upon his head, and his face was like the sun, and his feet like pillars of fire;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I saw another mighty angel coming down from heaven. He was robed in a cloud, with a rainbow above his head; his face was like the sun, and his legs were like fiery pillars."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I saw another mighty angel coming down from heaven, surrounded by a cloud, with a rainbow over his head. His face shone like the sun, and his feet were like pillars of fire."
      }
    },
