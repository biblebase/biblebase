    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天使对我说：“你为什么这样惊奇呢？我要将这妇人和她骑的七头十角怪兽的奥秘告诉你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天使對我說：“你為甚麼驚奇呢？我要把這婦人的奧祕，和馱著她的那七頭十角的獸的奧祕告訴你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天使對我說：「你為甚麼希奇呢？我要將這女人和馱着她的那七頭十角獸的奧祕告訴你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the angel said to me, “Why do you wonder? I will tell you the mystery of the woman and of the beast that carries her, which has the seven heads and the ten horns."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the angel said to me: “Why are you astonished? I will explain to you the mystery of the woman and of the beast she rides, which has the seven heads and ten horns."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why are you so amazed?” the angel asked. “I will tell you the mystery of this woman and of the beast with seven heads and ten horns on which she sits."
      }
    },
