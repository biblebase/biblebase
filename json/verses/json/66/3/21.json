    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "得胜者可以和我一同坐在我的宝座上，正如我得胜后与我父一同坐在祂的宝座上一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "得勝的，我必定賜他和我一同坐在我的寶座上，正像我得了勝和我父一同坐在他的寶座上一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "得勝的，我要賜他在我寶座上與我同坐，就如我得了勝，在我父的寶座上與他同坐一般。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who overcomes, I will grant to him to sit down with Me on My throne, as I also overcame and sat down with My Father on His throne."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To the one who is victorious, I will give the right to sit with me on my throne, just as I was victorious and sat down with my Father on his throne."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who are victorious will sit with me on my throne, just as I was victorious and sat with my Father on his throne."
      }
    },
