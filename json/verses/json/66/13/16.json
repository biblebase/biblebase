    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它又强迫所有的人，不论老少、尊卑、贫富、自由人或奴隶，都在右手或额上接受印记。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那從地裡上來的獸，又要所有的人，無論大小貧富，自由的和作奴隸的，都在右手或額上，給自己作個記號。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牠又叫眾人，無論大小、貧富、自主的、為奴的，都在右手上或是在額上受一個印記。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he causes all, the small and the great, and the rich and the poor, and the free men and the slaves, to be given a mark on their right hand or on their forehead,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It also forced all people, great and small, rich and poor, free and slave, to receive a mark on their right hands or on their foreheads,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He required everyone—small and great, rich and poor, free and slave—to be given a mark on the right hand or on the forehead."
      }
    },
