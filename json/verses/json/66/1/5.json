    "sermons": [
      {
        "id": "home_sermons_sermon-2018-07-29",
        "title": "基督的血",
        "date": "2018-07-29",
        "preacher": "陳星豪弟兄",
        "verses": "創世記 9:3-6; 馬太福音 26:26-30; 約翰福音 6:35-58; 羅馬書 3:23-26, 5:8-9; 哥林多前書 10:16-17, 11:23-29; 以弗所書 2:13-15; 希伯來書 9:12,14,22, 10:19; 彼得前書 1:2,18-19, 2:9; 啟示錄 1:5-6, 5:9-10, 12:11",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2018/Sermon-2018-07-29.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2018/Sermon-2018-07-29.pdf"
      }
    ],
    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又從那信實的見證人、死人中首先復生的、地上眾君王的統治者耶穌基督臨到你們。 他愛我們，用自己的血把我們從我們的罪中釋放出來，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並那誠實作見證的、從死裏首先復活、為世上君王元首的耶穌基督，有恩惠、平安歸與你們！ 他愛我們，用自己的血使我們脫離罪惡，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and from Jesus Christ, the faithful witness, the firstborn of the dead, and the ruler of the kings of the earth. To Him who loves us and released us from our sins by His blood—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and from Jesus Christ, who is the faithful witness, the firstborn from the dead, and the ruler of the kings of the earth. To him who loves us and has freed us from our sins by his blood,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and from Jesus Christ. He is the faithful witness to these things, the first to rise from the dead, and the ruler of all the kings of the world. All glory to him who loves us and has freed us from our sins by shedding his blood for us."
      }
    },
