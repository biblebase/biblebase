      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約翰寫信給在亞西亞的七個教會。願恩惠平安，從那位今在、昔在、以後要來的　神，從他寶座前的七靈，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約翰寫信給亞細亞的七個教會。但願從那昔在、今在、以後永在的神，和他寶座前的七靈，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "John to the seven churches that are in Asia: Grace to you and peace, from Him who is and who was and who is to come, and from the seven Spirits who are before His throne,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "John, To the seven churches in the province of Asia: Grace and peace to you from him who is, and who was, and who is to come, and from the seven spirits before his throne,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This letter is from John to the seven churches in the province of Asia. Grace and peace to you from the one who is, who always was, and who is still to come; from the sevenfold Spirit before his throne;"
      }
    },
