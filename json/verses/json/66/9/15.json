    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那四个天使就被释放了，他们为了此年、此月、此日、此刻已经预备好了，要杀掉人类的三分之一。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那四個使者就被釋放了，他們已經預備好，要在某年某月某日某時殺害人類的三分之一。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那四個使者就被釋放；他們原是預備好了，到某年某月某日某時，要殺人的三分之一。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the four angels, who had been prepared for the hour and day and month and year, were released, so that they would kill a third of mankind."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the four angels who had been kept ready for this very hour and day and month and year were released to kill a third of mankind."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the four angels who had been prepared for this hour and day and month and year were turned loose to kill one-third of all the people on earth."
      }
    },
