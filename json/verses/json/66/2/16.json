      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以你要悔改，否则我必迅速到你那里，用我口中的剑攻击他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，你應當悔改；如果不悔改，我就很快地來到你們那裡，用我口中的劍跟他們作戰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，你當悔改；若不悔改，我就快臨到你那裏，用我口中的劍攻擊他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore repent; or else I am coming to you quickly, and I will make war against them with the sword of My mouth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Repent therefore! Otherwise, I will soon come to you and will fight against them with the sword of my mouth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Repent of your sin, or I will come to you suddenly and fight against them with the sword of my mouth."
      }
