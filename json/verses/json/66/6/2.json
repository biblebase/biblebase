    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我便观看，见有一匹白马，马上的骑士拿着弓，他接受了赏赐给他的冠冕后，便四处征战，战无不胜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我觀看，見有一匹白馬；騎在馬上的拿著弓，有冠冕賜給他，他就出去，得勝並且要再得勝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我就觀看，見有一匹白馬；騎在馬上的，拿着弓，並有冠冕賜給他。他便出來，勝了又要勝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I looked, and behold, a white horse, and he who sat on it had a bow; and a crown was given to him, and he went out conquering and to conquer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I looked, and there before me was a white horse! Its rider held a bow, and he was given a crown, and he rode out as a conqueror bent on conquest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I looked up and saw a white horse standing there. Its rider carried a bow, and a crown was placed on his head. He rode out to win many battles and gain the victory."
      }
    },
