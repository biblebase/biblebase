    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们哀求群山和岩石，说：“倒下来遮盖我们，将我们藏起来吧！我们不敢面对宝座上的那位和羔羊的烈怒啊！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們向山嶺和巖石說：“倒在我們身上吧！把我們藏起來，躲避坐在寶座上那位的面，和羊羔的震怒！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "向山和巖石說：「倒在我們身上吧！把我們藏起來，躲避坐寶座者的面目和羔羊的忿怒；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they *said to the mountains and to the rocks, “Fall on us and hide us from the presence of Him who sits on the throne, and from the wrath of the Lamb;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They called to the mountains and the rocks, “Fall on us and hide us from the face of him who sits on the throne and from the wrath of the Lamb!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And they cried to the mountains and the rocks, “Fall on us and hide us from the face of the one who sits on the throne and from the wrath of the Lamb."
      }
    },
