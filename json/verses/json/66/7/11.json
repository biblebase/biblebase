    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众天使都站在宝座、众长老和四个活物的周围，在宝座前俯伏敬拜上帝，说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有的天使都站在寶座、眾長老和四個活物的四周。他們在寶座前，面伏在地上敬拜　神，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾天使都站在寶座和眾長老並四活物的周圍，在寶座前，面伏於地，敬拜神，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And all the angels were standing around the throne and around the elders and the four living creatures; and they fell on their faces before the throne and worshiped God,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the angels were standing around the throne and around the elders and the four living creatures. They fell down on their faces before the throne and worshiped God,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And all the angels were standing around the throne and around the elders and the four living beings. And they fell before the throne with their faces to the ground and worshiped God."
      }
    },
