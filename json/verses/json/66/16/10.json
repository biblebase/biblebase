    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第五位天使将碗倒在那海中怪兽的座位上，它的国便被黑暗笼罩，它的国民痛苦难当，咬自己的舌头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "第五位天使把碗倒在獸的座位上，獸的國就黑暗了。人因為痛苦就咬自己的舌頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "第五位天使把碗倒在獸的座位上，獸的國就黑暗了。人因疼痛就咬自己的舌頭；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the fifth angel poured out his bowl on the throne of the beast, and his kingdom became darkened; and they gnawed their tongues because of pain,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The fifth angel poured out his bowl on the throne of the beast, and its kingdom was plunged into darkness. People gnawed their tongues in agony"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the fifth angel poured out his bowl on the throne of the beast, and his kingdom was plunged into darkness. His subjects ground their teeth in anguish,"
      }
    },
