    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第七位天使吹响号角的时候，天上有大声音说：“世上的国度现在已经属于我们的主和祂所立的基督了。祂要做王，直到永永远远。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "第七位天使吹號，天上就有大聲音說： “世上的國成了我們的主 和他所立的基督的國， 他要作王，直到永永遠遠！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "第七位天使吹號，天上就有大聲音說： 世上的國成了我主和主基督的國； 他要作王，直到永永遠遠。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the seventh angel sounded; and there were loud voices in heaven, saying, “The kingdom of the world has become the kingdom of our Lord and of His Christ; and He will reign forever and ever.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The seventh angel sounded his trumpet, and there were loud voices in heaven, which said: “The kingdom of the world has become the kingdom of our Lord and of his Messiah, and he will reign for ever and ever.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the seventh angel blew his trumpet, and there were loud voices shouting in heaven: “The world has now become the Kingdom of our Lord and of his Christ, and he will reign forever and ever.”"
      }
    },
