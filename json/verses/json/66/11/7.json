    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当他们做完见证以后，从无底坑上来的兽要和他们交战，并战胜他们，把他们杀掉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們作完了見證的時候，那從無底坑上來的獸要跟他們作戰，勝過他們，把他們殺死。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們作完見證的時候，那從無底坑裏上來的獸必與他們交戰，並且得勝，把他們殺了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they have finished their testimony, the beast that comes up out of the abyss will make war with them, and overcome them and kill them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now when they have finished their testimony, the beast that comes up from the Abyss will attack them, and overpower and kill them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they complete their testimony, the beast that comes up out of the bottomless pit will declare war against them, and he will conquer them and kill them."
      }
    },
