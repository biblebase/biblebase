    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若有人要伤害他们，他们便从口中喷出火焰，烧灭敌人；若有人企图加害他们，必定会这样被杀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果有人想要傷害他們，就有火從他們口中出來，吞滅他們的仇敵。凡是想要傷害他們的，都必這樣被殺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若有人想要害他們，就有火從他們口中出來，燒滅仇敵。凡想要害他們的都必這樣被殺。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And if anyone wants to harm them, fire flows out of their mouth and devours their enemies; so if anyone wants to harm them, he must be killed in this way."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If anyone tries to harm them, fire comes from their mouths and devours their enemies. This is how anyone who wants to harm them must die."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If anyone tries to harm them, fire flashes from their mouths and consumes their enemies. This is how anyone who tries to harm them must die."
      }
    },
