    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那掌管七种灾祸的七位天使都从殿中出来，身穿洁白发光的细麻布衣服，胸佩金带。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那掌管著末後七災的七位天使，從聖所出來，穿著清潔明亮的細麻衣，胸間束著金帶。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那掌管七災的七位天使從殿中出來，穿着潔白光明的細麻衣，胸間束着金帶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and the seven angels who had the seven plagues came out of the temple, clothed in linen, clean and bright, and girded around their chests with golden sashes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Out of the temple came the seven angels with the seven plagues. They were dressed in clean, shining linen and wore golden sashes around their chests."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The seven angels who were holding the seven plagues came out of the Temple. They were clothed in spotless white linen with gold sashes across their chests."
      }
    },
