    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "外族人耗尽了他的力量， 他却茫然不知。 他头发斑白， 却浑然不觉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "外族人吞吃了他的力量， 他卻不知道； 他頭上滿是白髮， 他也不曉得。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "外邦人吞吃他勞力得來的，他卻不知道； 頭髮斑白，他也不覺得。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Strangers devour his strength, Yet he does not know it; Gray hairs also are sprinkled on him, Yet he does not know it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Foreigners sap his strength, but he does not realize it. His hair is sprinkled with gray, but he does not notice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Worshiping foreign gods has sapped their strength, but they don’t even know it. Their hair is gray, but they don’t realize they’re old and weak."
      }
    },
