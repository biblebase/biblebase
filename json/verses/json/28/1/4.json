    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对何西阿说：“给他取名叫耶斯列，因为再过不久，我就要因耶户家在耶斯列的屠杀而惩罚他们，消灭以色列国。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華就對何西阿說：“給他起名叫耶斯列；因為不久我就要懲罰耶戶家在耶斯列流人血的罪，我要滅絕以色列家的國。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對何西阿說：「給他起名叫耶斯列；因為再過片時，我必討耶戶家在耶斯列殺人流血的罪，也必使以色列家的國滅絕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the Lord said to him, “Name him Jezreel; for yet a little while, and I will punish the house of Jehu for the bloodshed of Jezreel, and I will put an end to the kingdom of the house of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the Lord said to Hosea, “Call him Jezreel, because I will soon punish the house of Jehu for the massacre at Jezreel, and I will put an end to the kingdom of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the Lord said, “Name the child Jezreel, for I am about to punish King Jehu’s dynasty to avenge the murders he committed at Jezreel. In fact, I will bring an end to Israel’s independence."
      }
    },
