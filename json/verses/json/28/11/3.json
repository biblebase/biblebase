    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "是我教以法莲学步，张开双臂抱他们， 他们却不知道是我医治他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然而是我教導以法蓮走路的； 我用雙臂抱他們， 他們卻不知道是我醫治了他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我原教導以法蓮行走， 用膀臂抱着他們， 他們卻不知道是我醫治他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet it is I who taught Ephraim to walk, I took them in My arms; But they did not know that I healed them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It was I who taught Ephraim to walk, taking them by the arms; but they did not realize it was I who healed them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I myself taught Israel how to walk, leading him along by the hand. But he doesn’t know or even care that it was I who took care of him."
      }
    },
