    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我鄙弃撒玛利亚人所拜的牛犊， 我要向他们发怒， 他们到什么时候才能洁身自守呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒瑪利亞啊！我要除掉你的牛犢， 我的怒氣要向撒瑪利亞人發作， 他們要到幾時才能無罪免罰呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "撒馬利亞啊，耶和華已經丟棄你的牛犢； 我的怒氣向拜牛犢的人發作。 他們到幾時方能無罪呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has rejected your calf, O Samaria, saying, “My anger burns against them!” How long will they be incapable of innocence?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Samaria, throw out your calf-idol! My anger burns against them. How long will they be incapable of purity?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O Samaria, I reject this calf— this idol you have made. My fury burns against you. How long will you be incapable of innocence?"
      }
    },
