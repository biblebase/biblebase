    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "先知为了上帝做以法莲的守望者， 可是他走的路上布满网罗， 在他上帝的家里遭人憎恨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "先知與　神一同 作以法蓮的守望者， 在他所行的一切路上，都滿有捕鳥人的網羅， 在他　神的家中只有仇恨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮曾作我神守望的； 至於先知，在他一切的道上作為捕鳥人的網羅， 在他神的家中懷怨恨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ephraim was a watchman with my God, a prophet; Yet the snare of a bird catcher is in all his ways, And there is only hostility in the house of his God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The prophet, along with my God, is the watchman over Ephraim, yet snares await him on all his paths, and hostility in the house of his God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The prophet is a watchman over Israel for my God, yet traps are laid for him wherever he goes. He faces hostility even in the house of God."
      }
    },
