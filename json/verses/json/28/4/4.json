    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “谁都不要指控别人， 谁都不要责备别人， 因为我要指控的是你们祭司。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然這樣，人人都不必爭論，不要彼此指責。 你的民像爭辯的祭司一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "然而，人都不必爭辯，也不必指責， 因為這民與抗拒祭司的人一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet let no one find fault, and let none offer reproof; For your people are like those who contend with the priest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But let no one bring a charge, let no one accuse another, for your people are like those who bring charges against a priest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Don’t point your finger at someone else and try to pass the blame! My complaint, you priests, is with you."
      }
    },
