    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们说虚话，起假誓，立空盟， 因此不法之事滋长， 犹如田间犁沟中蔓延的毒草。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們說很多話， 起假誓，又立盟約； 所以審判像毒草一般在田間的犁溝中生長起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們為立約說謊言，起假誓； 因此，災罰如苦菜滋生在田間的犂溝中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They speak mere words, With worthless oaths they make covenants; And judgment sprouts like poisonous weeds in the furrows of the field."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They make many promises, take false oaths and make agreements; therefore lawsuits spring up like poisonous weeds in a plowed field."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They spout empty words and make covenants they don’t intend to keep. So injustice springs up among them like poisonous weeds in a farmer’s field."
      }
    },
