    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们在基列·雅比的居民中找到四百名处女，把她们带回迦南的示罗营地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在基列．雅比的居民中，找到了四百個未曾與人同房，也未曾與男子同寢的年輕少女，就把她們帶到迦南地的示羅營那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們在基列‧雅比人中，遇見了四百個未嫁的處女，就帶到迦南地的示羅營裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they found among the inhabitants of Jabesh-gilead 400 young virgins who had not known a man by lying with him; and they brought them to the camp at Shiloh, which is in the land of Canaan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They found among the people living in Jabesh Gilead four hundred young women who had never slept with a man, and they took them to the camp at Shiloh in Canaan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Among the residents of Jabesh-gilead they found 400 young virgins who had never slept with a man, and they brought them to the camp at Shiloh in the land of Canaan."
      }
    },
