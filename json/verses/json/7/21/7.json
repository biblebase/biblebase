    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们曾经在耶和华面前起誓，决不把女儿嫁给便雅悯人，现在我们应该怎么办才能使他们剩下来的人得到妻子呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們曾經指著耶和華起過誓，必不把我們的女兒給便雅憫人作妻子，那麼現在我們當怎樣辦理，使他們剩下的人有妻子呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們既在耶和華面前起誓說，必不將我們的女兒給便雅憫人為妻，現在我們當怎樣辦理、使他們剩下的人有妻呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "What shall we do for wives for those who are left, since we have sworn by the Lord not to give them any of our daughters in marriage?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“How can we provide wives for those who are left, since we have taken an oath by the Lord not to give them any of our daughters in marriage?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How can we find wives for the few who remain, since we have sworn by the Lord not to give them our daughters in marriage?”"
      }
    },
