    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "参孙说： “食物出自食者， 甜物出自强者。” 三天过后，他们仍然猜不出来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "參孫對他們說： “可吃的從吃者出來； 甜的從強者出來。” 他們過了三日都不能說出謎語的意思。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "參孫對他們說： 吃的從吃者出來； 甜的從強者出來。 他們三日不能猜出謎語的意思。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he said to them, “Out of the eater came something to eat, And out of the strong came something sweet.” But they could not tell the riddle in three days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He replied, “Out of the eater, something to eat; out of the strong, something sweet.” For three days they could not give the answer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So he said: “Out of the one who eats came something to eat; out of the strong came something sweet.” Three days later they were still trying to figure it out."
      }
