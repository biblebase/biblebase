    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们想想，从前我父亲为你们出生入死，从米甸人手中拯救你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我父親從前冒死為你們爭戰，把你們從米甸人手中救了出來；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從前我父冒死為你們爭戰，救了你們脫離米甸人的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "for my father fought for you and risked his life and delivered you from the hand of Midian;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember that my father fought for you and risked his life to rescue you from the hand of Midian."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For he fought for you and risked his life when he rescued you from the Midianites."
      }
