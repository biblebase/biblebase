    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约坦听见这消息，便上到基利心山顶，高喊：“示剑人啊，你们要听我说，这样上帝也会听你们说。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有人把這事告訴了約坦，約坦就去，站在基利心山頂上，高聲向他們呼喊，說：“示劍人哪，你們要聽我的話，　神也就聽你們的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人將這事告訴約坦，他就去站在基利心山頂上，向眾人大聲喊叫說：「示劍人哪，你們要聽我的話，神也就聽你們的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when they told Jotham, he went and stood on the top of Mount Gerizim, and lifted his voice and called out. Thus he said to them, “Listen to me, O men of Shechem, that God may listen to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jotham was told about this, he climbed up on the top of Mount Gerizim and shouted to them, “Listen to me, citizens of Shechem, so that God may listen to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jotham heard about this, he climbed to the top of Mount Gerizim and shouted, “Listen to me, citizens of Shechem! Listen to me if you want God to listen to you!"
      }
    },
