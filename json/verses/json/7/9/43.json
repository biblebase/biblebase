    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把部下兵分三队，埋伏在田间，等城里的人出来时，伏击他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞比米勒就把他的人分作三隊，埋伏在田間；他在那裡觀看，見有人從城裡出來，就起來攻擊他們，把他們擊殺了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他就把他的人分作三隊，埋伏在田間，看見示劍人從城裏出來，就起來擊殺他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he took his people and divided them into three companies, and lay in wait in the field; when he looked and saw the people coming out from the city, he arose against them and slew them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So he took his men, divided them into three companies and set an ambush in the fields. When he saw the people coming out of the city, he rose to attack them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "he divided his men into three groups and set an ambush in the fields. When Abimelech saw the people coming out of the city, he and his men jumped up from their hiding places and attacked them."
      }
    },
