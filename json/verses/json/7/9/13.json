    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "葡萄树说，‘我怎能因为贪图凌驾于众树之上而停止出产令神明和世人都快乐的美酒呢？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "葡萄樹對它們說：‘我怎可以放棄生產那使　神和世人都喜樂的新酒，飄搖在眾樹之上呢？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "葡萄樹回答說：『我豈肯止住使神和人喜樂的新酒，飄颻在眾樹之上呢？』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the vine said to them, ‘Shall I leave my new wine, which cheers God and men, and go to wave over the trees?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But the vine answered, ‘Should I give up my wine, which cheers both gods and humans, to hold sway over the trees?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the grapevine also refused, saying, ‘Should I quit producing the wine that cheers both God and people, just to wave back and forth over the trees?’"
      }
    },
