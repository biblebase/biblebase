    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "每当他们撒种的时候，米甸人、亚玛力人及东方人就前来进犯，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "每逢以色列人撒種以後，米甸人、亞瑪力人和東方人都上來，欺壓他們；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人每逢撒種之後，米甸人、亞瑪力人，和東方人都上來攻打他們，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it was when Israel had sown, that the Midianites would come up with the Amalekites and the sons of the east and go against them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whenever the Israelites planted their crops, the Midianites, Amalekites and other eastern peoples invaded the country."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Whenever the Israelites planted their crops, marauders from Midian, Amalek, and the people of the east would attack Israel,"
      }
    },
