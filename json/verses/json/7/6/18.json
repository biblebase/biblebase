    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "请你在这里等一等，让我回去拿礼物献给你。”耶和华说：“我必等你回来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你不要離開這裡，等我回來，把我的禮物帶來，擺在你面前。”他回答：“我必等你回來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你不要離開這裏，等我歸回將禮物帶來供在你面前。」主說：「我必等你回來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Please do not depart from here, until I come back to You, and bring out my offering and lay it before You.” And He said, “I will remain until you return.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Please do not go away until I come back and bring my offering and set it before you.” And the Lord said, “I will wait until you return.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t go away until I come back and bring my offering to you.” He answered, “I will stay here until you return.”"
      }
    },
