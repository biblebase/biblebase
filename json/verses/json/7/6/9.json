    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "将你们从埃及人及一切欺压者的手中拯救出来，为你们赶走敌人，把他们的土地赐给你们。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我曾經把你們從埃及人的手拯救出來，拯救你們脫離所有欺壓你們的人的手，把他們從你們面前趕走，把他們的地賜給你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "救你們脫離埃及人的手，並脫離一切欺壓你們之人的手，把他們從你們面前趕出，將他們的地賜給你們』；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I delivered you from the hands of the Egyptians and from the hands of all your oppressors, and dispossessed them before you and gave you their land,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I rescued you from the hand of the Egyptians. And I delivered you from the hand of all your oppressors; I drove them out before you and gave you their land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I rescued you from the Egyptians and from all who oppressed you. I drove out your enemies and gave you their land."
      }
    },
