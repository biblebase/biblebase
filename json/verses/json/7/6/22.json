    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基甸发现他果然是耶和华的天使，便说：“唉！主耶和华啊，不好了，我面对面见了你的天使。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基甸看出他是耶和華的使者，就說：“哀哉，我主耶和華啊，因為我面對面看見了耶和華的使者。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基甸見他是耶和華的使者，就說：「哀哉！主耶和華啊，我不好了，因為我覿面看見耶和華的使者。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Gideon saw that he was the angel of the Lord, he said, “Alas, O Lord God! For now I have seen the angel of the Lord face to face.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Gideon realized that it was the angel of the Lord, he exclaimed, “Alas, Sovereign Lord! I have seen the angel of the Lord face to face!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Gideon realized that it was the angel of the Lord, he cried out, “Oh, Sovereign Lord, I’m doomed! I have seen the angel of the Lord face to face!”"
      }
    },
