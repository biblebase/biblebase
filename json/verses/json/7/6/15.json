    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基甸说：“主啊，我出身于玛拿西支派最贫穷的家族，在家里最微不足道，我怎能拯救以色列人呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基甸對他說：“唉，我主啊，我憑著甚麼拯救以色列人呢？看哪，我的家族在瑪拿西支派中是最卑微的，我在我的父家是最年輕的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基甸說：「主啊，我有何能拯救以色列人呢？我家在瑪拿西支派中是至貧窮的。我在我父家是至微小的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to Him, “O Lord, how shall I deliver Israel? Behold, my family is the least in Manasseh, and I am the youngest in my father’s house.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Pardon me, my lord,” Gideon replied, “but how can I save Israel? My clan is the weakest in Manasseh, and I am the least in my family.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But Lord,” Gideon replied, “how can I rescue Israel? My clan is the weakest in the whole tribe of Manasseh, and I am the least in my entire family!”"
      }
    },
