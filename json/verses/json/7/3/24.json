    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以笏走后，王的仆人过来，见房门紧锁，以为王在大解。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以笏出來以後，王的僕人才來到；他們看見涼樓上的門鎖著，就說：“王一定是在涼樓上大解。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以笏出來之後，王的僕人到了，看見樓門關鎖，就說：「他必是在樓上大解。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he had gone out, his servants came and looked, and behold, the doors of the roof chamber were locked; and they said, “He is only relieving himself in the cool room.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After he had gone, the servants came and found the doors of the upper room locked. They said, “He must be relieving himself in the inner room of the palace.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After Ehud was gone, the king’s servants returned and found the doors to the upstairs room locked. They thought he might be using the latrine in the room,"
      }
    },
