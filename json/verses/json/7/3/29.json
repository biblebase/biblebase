    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当时他们杀了约一万名强悍善战的摩押勇士，没有一人逃脱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時他們擊殺了約有一萬摩押人，都是強壯的、勇猛的人，沒有一人逃脫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時擊殺了摩押人約有一萬，都是強壯的勇士，沒有一人逃脫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They struck down at that time about ten thousand Moabites, all robust and valiant men; and no one escaped."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At that time they struck down about ten thousand Moabites, all vigorous and strong; not one escaped."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They attacked the Moabites and killed about 10,000 of their strongest and most able-bodied warriors. Not one of them escaped."
      }
    },
