    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人围攻便雅悯人，一路追杀他们直到基比亚的东边。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人包圍了便雅憫人，從挪哈追趕他們，踐踏他們，直到基比亞對面日出之處。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人圍繞便雅憫人，追趕他們，在他們歇腳之處、對着日出之地的基比亞踐踏他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They surrounded Benjamin, pursued them without rest and trod them down opposite Gibeah toward the east."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They surrounded the Benjamites, chased them and easily overran them in the vicinity of Gibeah on the east."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Israelites surrounded the men of Benjamin and chased them relentlessly, finally overtaking them east of Gibeah."
      }
    },
