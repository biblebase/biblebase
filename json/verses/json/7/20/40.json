    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这时基比亚城已浓烟滚滚，便雅悯人回头观望，只见全城浓烟四起。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當煙柱信號從城中上升的時候，便雅憫人就轉身觀看，只見全城煙火沖天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當煙氣如柱從城中上騰的時候，便雅憫人回頭觀看，見全城的煙氣沖天。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when the cloud began to rise from the city in a column of smoke, Benjamin looked behind them; and behold, the whole city was going up in smoke to heaven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But when the column of smoke began to rise from the city, the Benjamites turned and saw the whole city going up in smoke."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when the warriors of Benjamin looked behind them and saw the smoke rising into the sky from every part of the town,"
      }
    },
