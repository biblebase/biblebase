    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶宾王拥有九百辆铁战车，他残暴地压迫以色列人二十年。以色列人向耶和华呼求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶賓王有鐵車九百輛，他極力欺壓以色列人二十年，以色列人就哀求耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶賓王有鐵車九百輛。他大大欺壓以色列人二十年，以色列人就呼求耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sons of Israel cried to the Lord; for he had nine hundred iron chariots, and he oppressed the sons of Israel severely for twenty years."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because he had nine hundred chariots fitted with iron and had cruelly oppressed the Israelites for twenty years, they cried to the Lord for help."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Sisera, who had 900 iron chariots, ruthlessly oppressed the Israelites for twenty years. Then the people of Israel cried out to the Lord for help."
      }
    },
