    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "西西拉说：“我口渴，请给我一点水吧。”雅亿打开皮袋给他奶喝，然后又给他盖上毯子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "西西拉對雅億說：“請給我一點水喝，因為我渴了。”雅億就打開盛奶的皮袋，給他喝了，再把他蓋著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "西西拉對雅億說：「我渴了，求你給我一點水喝。」雅億就打開皮袋，給他奶子喝，仍舊把他遮蓋。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to her, “Please give me a little water to drink, for I am thirsty.” So she opened a bottle of milk and gave him a drink; then she covered him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I’m thirsty,” he said. “Please give me some water.” She opened a skin of milk, gave him a drink, and covered him up."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Please give me some water,” he said. “I’m thirsty.” So she gave him some milk from a leather bag and covered him again."
      }
    },
