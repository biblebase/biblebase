    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴拉追击他的战车和军队，直到夏罗设。西西拉全军都死于刀下，无一幸免。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴拉追趕西西拉的車輛和軍隊，直到夏羅設歌印；西西拉的全軍都倒在刀下，連一個也沒有留下。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴拉追趕車輛、軍隊，直到外邦人的夏羅設。西西拉的全軍都倒在刀下，沒有留下一人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Barak pursued the chariots and the army as far as Harosheth-hagoyim, and all the army of Sisera fell by the edge of the sword; not even one was left."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Barak pursued the chariots and army as far as Harosheth Haggoyim, and all Sisera’s troops fell by the sword; not a man was left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Barak chased the chariots and the enemy army all the way to Harosheth-haggoyim, killing all of Sisera’s warriors. Not a single one was left alive."
      }
    },
