    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时有三百人捧水舔着喝，其余的都跪着喝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "用手捧到嘴邊舔水的，總數共有三百人；其餘的人都屈膝跪下喝水。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是用手捧着舔水的有三百人，其餘的都跪下喝水。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the number of those who lapped, putting their hand to their mouth, was 300 men; but all the rest of the people kneeled to drink water."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Three hundred of them drank from cupped hands, lapping like dogs. All the rest got down on their knees to drink."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Only 300 of the men drank from their hands. All the others got down on their knees and drank with their mouths in the stream."
      }
    },
