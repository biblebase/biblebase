    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对他们说：“你们要看我的行动，当我们走到敌营旁边时，我怎么做，你们也要怎么做。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後對他們說：“你們要看著我，也要照樣行事。注意！我到了營的邊緣，我怎樣行事，你們也要怎樣行事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "吩咐他們說：「你們要看我行事：我到了營的旁邊怎樣行，你們也要怎樣行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to them, “Look at me and do likewise. And behold, when I come to the outskirts of the camp, do as I do."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Watch me,” he told them. “Follow my lead. When I get to the edge of the camp, do exactly as I do."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said to them, “Keep your eyes on me. When I come to the edge of the camp, do just as I do."
      }
    },
