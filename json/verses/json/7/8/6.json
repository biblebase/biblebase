    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "疏割的首领却说：“你还没有抓到西巴和撒慕拿，我们凭什么给你的军队饼吃？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "疏割人的首領回答：“西巴和撒慕拿現在不是已經在你的手裡嗎？我們應該把餅送給你的軍隊吃嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "疏割人的首領回答說：「西巴和撒慕拿已經在你手裏，你使我們將餅給你的軍兵嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The leaders of Succoth said, “Are the hands of Zebah and Zalmunna already in your hands, that we should give bread to your army?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the officials of Sukkoth said, “Do you already have the hands of Zebah and Zalmunna in your possession? Why should we give bread to your troops?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the officials of Succoth replied, “Catch Zebah and Zalmunna first, and then we will feed your army.”"
      }
    },
