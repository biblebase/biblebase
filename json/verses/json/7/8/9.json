    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基甸对那里的人说：“我凯旋归来时，必拆毁你们的这座城楼。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他亦對毘努伊勒人說：“我平平安安回來的時候，我必拆毀這座望樓。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他向毗努伊勒人說：「我平平安安回來的時候，我必拆毀這樓。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he spoke also to the men of Penuel, saying, “When I return safely, I will tear down this tower.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So he said to the men of Peniel, “When I return in triumph, I will tear down this tower.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So he said to the people of Peniel, “After I return in victory, I will tear down this tower.”"
      }
    },
