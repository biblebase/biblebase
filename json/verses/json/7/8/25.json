    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们欣然同意，便铺开一件外衣，各自把抢来的金耳环都放在上面，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們回答：“我們願意給你。”於是鋪開一件外衣，各人把奪得的耳環丟在上面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們說：「我們情願給你」，就鋪開一件外衣，各人將所奪的耳環丟在其上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said, “We will surely give them.” So they spread out a garment, and every one of them threw an earring there from his spoil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They answered, “We’ll be glad to give them.” So they spread out a garment, and each of them threw a ring from his plunder onto it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Gladly!” they replied. They spread out a cloak, and each one threw in a gold earring he had gathered from the plunder."
      }
    },
