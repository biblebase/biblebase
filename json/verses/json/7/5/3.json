    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "君王和首领啊，请留心细听！ 我要向耶和华歌唱， 我要歌颂以色列的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "君王啊，你們要聽；官長啊，你們要側耳傾聽； 至於我，我要向耶和華歌唱； 我要歌頌耶和華以色列的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "君王啊，要聽！ 王子啊，要側耳而聽！ 我要向耶和華歌唱； 我要歌頌耶和華－以色列的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hear, O kings; give ear, O rulers! I—to the Lord, I will sing, I will sing praise to the Lord, the God of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Hear this, you kings! Listen, you rulers! I, even I, will sing to the Lord; I will praise the Lord, the God of Israel, in song."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Listen, you kings! Pay attention, you mighty rulers! For I will sing to the Lord. I will make music to the Lord, the God of Israel."
      }
    },
