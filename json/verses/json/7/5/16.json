    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "吕便人为什么坐在羊圈中听悠悠笛声？ 他们举棋不定。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你為甚麼坐在羊圈之中， 聽呼喚群畜的笛聲呢？ 在流本的眾溪旁，有心懷大志的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你為何坐在羊圈內 聽羣中吹笛的聲音呢？ 在呂便的溪水旁有心中設大謀的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why did you sit among the sheepfolds, To hear the piping for the flocks? Among the divisions of Reuben There were great searchings of heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why did you stay among the sheep pens to hear the whistling for the flocks? In the districts of Reuben there was much searching of heart."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why did you sit at home among the sheepfolds— to hear the shepherds whistle for their flocks? Yes, in the tribe of Reuben there was great indecision."
      }
    },
