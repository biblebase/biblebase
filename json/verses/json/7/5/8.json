    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人选择新的神明， 便战祸临门， 四万以色列人中找不到一矛一盾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人選擇了新的神， 戰爭就臨近城門； 那時四萬以色列人中， 竟不見有一面盾牌、一枝長矛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人選擇新神， 爭戰的事就臨到城門。 那時，以色列四萬人中 豈能見盾牌槍矛呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "New gods were chosen; Then war was in the gates. Not a shield or a spear was seen Among forty thousand in Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God chose new leaders when war came to the city gates, but not a shield or spear was seen among forty thousand in Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Israel chose new gods, war erupted at the city gates. Yet not a shield or spear could be seen among forty thousand warriors in Israel!"
      }
    },
