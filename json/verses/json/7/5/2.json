    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以色列的首领身先士卒， 民众前仆后继， 你们要赞美耶和华！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們要稱頌耶和華， 因為以色列中有勇士， 因為有人民自願從軍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為以色列中有軍長率領， 百姓也甘心犧牲自己， 你們應當頌讚耶和華！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“That the leaders led in Israel, That the people volunteered, Bless the Lord!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When the princes in Israel take the lead, when the people willingly offer themselves— praise the Lord!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Israel’s leaders took charge, and the people gladly followed. Praise the Lord!"
      }
    },
