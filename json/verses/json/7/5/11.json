    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "歌乐手聚集在井旁， 传扬耶和华和祂以色列子民的胜利。 耶和华的子民都来到城门口。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在打水的地方分開羊群的人發聲， 在那裡人必歌頌耶和華公義的作為， 就是他在以色列中的農村所行公義的作為； 那時，耶和華的子民下到城門口。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在遠離弓箭響聲打水之處， 人必述說耶和華公義的作為， 就是他治理以色列公義的作為。 那時耶和華的民下到城門。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "At the sound of those who divide flocks among the watering places, There they shall recount the righteous deeds of the Lord, The righteous deeds for His peasantry in Israel. Then the people of the Lord went down to the gates."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the voice of the singers at the watering places. They recite the victories of the Lord, the victories of his villagers in Israel. “Then the people of the Lord went down to the city gates."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen to the village musicians gathered at the watering holes. They recount the righteous victories of the Lord and the victories of his villagers in Israel. Then the people of the Lord marched down to the city gates."
      }
    },
