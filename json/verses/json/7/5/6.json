    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在亚拿的儿子珊迦与雅亿的时代， 大路行人绝迹， 旅客绕道而行。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在亞拿的兒子珊迦的時候， 在雅億的日子，大道無人行走， 行路的人繞道而行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在亞拿之子珊迦的時候， 又在雅億的日子， 大道無人行走， 都是繞道而行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“In the days of Shamgar the son of Anath, In the days of Jael, the highways were deserted, And travelers went by roundabout ways."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“In the days of Shamgar son of Anath, in the days of Jael, the highways were abandoned; travelers took to winding paths."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“In the days of Shamgar son of Anath, and in the days of Jael, people avoided the main roads, and travelers stayed on winding pathways."
      }
    },
