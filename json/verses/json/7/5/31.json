    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 愿你的敌人都像西西拉一样灭亡！ 愿爱你的人如升起的太阳， 光辉熠熠！” 此后，以色列安享太平四十年。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊，願你所有的仇敵都這樣滅亡； 願愛他的人像太陽出現，大有能力。” 於是國中太平了四十年。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊， 願你的仇敵都這樣滅亡！ 願愛你的人如日頭出現，光輝烈烈！ 這樣，國中太平四十年。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus let all Your enemies perish, O Lord; But let those who love Him be like the rising of the sun in its might.” And the land was undisturbed for forty years."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“So may all your enemies perish, Lord! But may all who love you be like the sun when it rises in its strength.” Then the land had peace forty years."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Lord, may all your enemies die like Sisera! But may those who love you rise like the sun in all its power!” Then there was peace in the land for forty years."
      }
    },
