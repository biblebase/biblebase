    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "幸存的以色列人响应他们的首领， 耶和华的子民为我攻击勇士。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時餘剩的貴胄下來； 耶和華帶著勇士下到我這裡來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時有餘剩的貴冑和百姓一同下來； 耶和華降臨，為我攻擊勇士。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then survivors came down to the nobles; The people of the Lord came down to me as warriors."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The remnant of the nobles came down; the people of the Lord came down to me against the mighty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Down from Tabor marched the few against the nobles. The people of the Lord marched down against mighty warriors."
      }
    },
