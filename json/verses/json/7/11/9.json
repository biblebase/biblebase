    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶弗他说：“如果我跟你们回去与亚扪人作战，耶和华让我打败他们，我就真的可以做你们的首领吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶弗他對基列的長老說：“如果你們帶我回去，攻打亞捫人，耶和華把他們交在我面前，那麼我就可以作你們的首領嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶弗他對基列的長老說：「你們叫我回去，與亞捫人爭戰，耶和華把他交給我，我可以作你們的領袖嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Jephthah said to the elders of Gilead, “If you take me back to fight against the sons of Ammon and the Lord gives them up to me, will I become your head?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jephthah answered, “Suppose you take me back to fight the Ammonites and the Lord gives them to me—will I really be your head?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jephthah said to the elders, “Let me get this straight. If I come with you and if the Lord gives me victory over the Ammonites, will you really make me ruler over all the people?”"
      }
    },
