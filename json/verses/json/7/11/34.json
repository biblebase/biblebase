    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶弗他返回自己在米斯巴的家，他的独生女儿敲着鼓、跳着舞出来迎接他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶弗他回米斯巴去，到了自己的家，不料，他的女兒拿著鼓、跳著舞，出來迎接他；她是耶弗他的獨生女，除她以外，耶弗他沒有其他的兒女。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶弗他回米斯巴到了自己的家，不料，他女兒拿着鼓跳舞出來迎接他，是他獨生的，此外無兒無女。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Jephthah came to his house at Mizpah, behold, his daughter was coming out to meet him with tambourines and with dancing. Now she was his one and only child; besides her he had no son or daughter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jephthah returned to his home in Mizpah, who should come out to meet him but his daughter, dancing to the sound of timbrels! She was an only child. Except for her he had neither son nor daughter."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jephthah returned home to Mizpah, his daughter came out to meet him, playing on a tambourine and dancing for joy. She was his one and only child; he had no other sons or daughters."
      }
    },
