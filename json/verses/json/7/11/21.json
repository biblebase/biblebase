    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但以色列的上帝耶和华帮助以色列人战胜西宏和他的军队，占领了他们所有的土地，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華以色列的　神，把西宏和他所有的人民都交在以色列人手中，以色列人擊殺了他們，奪取了住在那地的亞摩利人的全地，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－以色列的神將西宏和他的眾民都交在以色列人手中，以色列人就擊殺他們，得了亞摩利人的全地："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord, the God of Israel, gave Sihon and all his people into the hand of Israel, and they defeated them; so Israel possessed all the land of the Amorites, the inhabitants of that country."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Then the Lord, the God of Israel, gave Sihon and his whole army into Israel’s hands, and they defeated them. Israel took over all the land of the Amorites who lived in that country,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Lord, the God of Israel, gave his people victory over King Sihon. So Israel took control of all the land of the Amorites, who lived in that region,"
      }
    },
