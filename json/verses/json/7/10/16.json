    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人除掉了他们的外邦神像，转而事奉耶和华。耶和华不忍再看以色列人受苦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人就從他們中間除去外族人的神，事奉耶和華；耶和華的心因以色列人所受的患難，就很著急。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人就除掉他們中間的外邦神，事奉耶和華。耶和華因以色列人受的苦難，就心中擔憂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they put away the foreign gods from among them and served the Lord; and He could bear the misery of Israel no longer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they got rid of the foreign gods among them and served the Lord. And he could bear Israel’s misery no longer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Israelites put aside their foreign gods and served the Lord. And he was grieved by their misery."
      }
    },
