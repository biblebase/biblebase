    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基列的首领们彼此商议说：“谁先攻打亚扪人，谁就做所有基列人的首领。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基列的人民和眾領袖彼此說：“誰先去攻打亞捫人，誰就可以作基列所有居民的首領。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基列的民和眾首領彼此商議說：「誰能先去攻打亞捫人，誰必作基列一切居民的領袖。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The people, the leaders of Gilead, said to one another, “Who is the man who will begin to fight against the sons of Ammon? He shall become head over all the inhabitants of Gilead.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The leaders of the people of Gilead said to each other, “Whoever will take the lead in attacking the Ammonites will be head over all who live in Gilead.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The leaders of Gilead said to each other, “Whoever attacks the Ammonites first will become ruler over all the people of Gilead.”"
      }
    },
