    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当时，亚扪人整装备战，在基列安营；以色列人也集合起来，在米斯巴安营。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當時亞捫人聚集起來，在基列安營；以色列人也聚集，在米斯巴安營。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當時亞捫人聚集，安營在基列。以色列人也聚集，安營在米斯巴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the sons of Ammon were summoned and they camped in Gilead. And the sons of Israel gathered together and camped in Mizpah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the Ammonites were called to arms and camped in Gilead, the Israelites assembled and camped at Mizpah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At that time the armies of Ammon had gathered for war and were camped in Gilead, and the people of Israel assembled and camped at Mizpah."
      }
