    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们对耶和华说：“我们犯了罪，任你处置我们，只求你今天拯救我们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人對耶和華說：“我們犯了罪了，你任意對待我們吧；只求你今日拯救我們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人對耶和華說：「我們犯罪了，任憑你隨意待我們吧！只求你今日拯救我們。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sons of Israel said to the Lord, “We have sinned, do to us whatever seems good to You; only please deliver us this day.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the Israelites said to the Lord, “We have sinned. Do with us whatever you think best, but please rescue us now.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Israelites pleaded with the Lord and said, “We have sinned. Punish us as you see fit, only rescue us today from our enemies.”"
      }
    },
