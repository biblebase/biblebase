      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们离开米迦的家一段路后，米迦召集邻居赶了上来，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們離開米迦的家很遠的時候，在米迦的家附近各家的人，都被召集出來，去追趕但人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "離米迦的住宅已遠，米迦的近鄰都聚集來，追趕但人，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they had gone some distance from the house of Micah, the men who were in the houses near Micah’s house assembled and overtook the sons of Dan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they had gone some distance from Micah’s house, the men who lived near Micah were called together and overtook the Danites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the people from the tribe of Dan were quite a distance from Micah’s house, the people who lived near Micah came chasing after them."
      }
