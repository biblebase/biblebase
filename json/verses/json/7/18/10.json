    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们到了那里，会看见当地的居民毫无防范，那里地大物博，一无所缺。上帝已经把那地方赐给我们了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們到了那裡的時候，你們必遇見安然居住的人民；那地的兩邊寬闊，　神已經把那地交在你們手裡；那地百物俱全，一無所缺。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們到了那裏，必看見安居無慮的民，地也寬闊。神已將那地交在你們手中；那地百物俱全，一無所缺。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you enter, you will come to a secure people with a spacious land; for God has given it into your hand, a place where there is no lack of anything that is on the earth.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you get there, you will find an unsuspecting people and a spacious land that God has put into your hands, a land that lacks nothing whatever.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When you get there, you will find the people living carefree lives. God has given us a spacious and fertile land, lacking in nothing!”"
      }
    },
