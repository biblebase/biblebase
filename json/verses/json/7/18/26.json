    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但支派的人继续赶路。米迦见对方人多势众，就回家去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但人繼續走他們的路；米迦見他們比自己強，就轉身回自己的家去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但人還是走他們的路。米迦見他們的勢力比自己強盛，就轉身回家去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the sons of Dan went on their way; and when Micah saw that they were too strong for him, he turned and went back to his house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the Danites went their way, and Micah, seeing that they were too strong for him, turned around and went back home."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the men of Dan continued on their way. When Micah saw that there were too many of them for him to attack, he turned around and went home."
      }
    },
