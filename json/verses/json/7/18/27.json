    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但支派的人带着米迦的神像和他的祭司来到拉亿，那里的人生活安宁，毫无防范。他们杀掉当地的居民，放火烧了城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但人把米迦所做的神像和他的祭司都帶走，他們來到拉億，去到平靜安居的人民那裡，就用刀劍擊殺了他們，又用火燒了那城。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但人將米迦所做的神像和他的祭司都帶到拉億，見安居無慮的民，就用刀殺了那民，又放火燒了那城，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they took what Micah had made and the priest who had belonged to him, and came to Laish, to a people quiet and secure, and struck them with the edge of the sword; and they burned the city with fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they took what Micah had made, and his priest, and went on to Laish, against a people at peace and secure. They attacked them with the sword and burned down their city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then, with Micah’s idols and his priest, the men of Dan came to the town of Laish, whose people were peaceful and secure. They attacked with swords and burned the town to the ground."
      }
    },
