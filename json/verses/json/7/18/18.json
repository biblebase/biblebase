    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司见那五人进入米迦家拿走雕刻的偶像、以弗得、家庭神像及金属铸像，就问他们：“你们在做什么？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當那五個人進了米迦的家，拿走雕像、以弗得、神像和鑄像的時候，祭司就問他們：“你們作甚麼呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那五個人進入米迦的住宅，拿出雕刻的像、以弗得、家中的神像，並鑄成的像，祭司就問他們說：「你們做甚麼呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When these went into Micah’s house and took the graven image, the ephod and household idols and the molten image, the priest said to them, “What are you doing?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the five men went into Micah’s house and took the idol, the ephod and the household gods, the priest said to them, “What are you doing?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the priest saw the men carrying all the sacred objects out of Micah’s shrine, he said, “What are you doing?”"
      }
    },
