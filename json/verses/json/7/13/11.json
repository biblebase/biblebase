    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛挪亚立刻跟随妻子来到那人面前，问他：“那天跟我妻子说话的就是你吗？”他答道：“是我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪挪亞起來，跟著他的妻子出去，來到那人跟前，問他：“你就是向這個婦人說過話的人嗎？”他回答：“我就是。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瑪挪亞起來跟隨他的妻來到那人面前，對他說：「與這婦人說話的就是你嗎？」他說：「是我。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Manoah arose and followed his wife, and when he came to the man he said to him, “Are you the man who spoke to the woman?” And he said, “I am.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Manoah got up and followed his wife. When he came to the man, he said, “Are you the man who talked to my wife?” “I am,” he said."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Manoah ran back with his wife and asked, “Are you the man who spoke to my wife the other day?” “Yes,” he replied, “I am.”"
      }
    },
