    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的天使向那妇人显现，对她说：“你虽然多年不育，但不久你必怀孕生一个儿子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的使者向那婦人顯現，對她說：“你向來不能生育，沒有孩子，現在你必要懷孕，生一個兒子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的使者向那婦人顯現，對她說：「向來你不懷孕，不生育，如今你必懷孕生一個兒子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the angel of the Lord appeared to the woman and said to her, “Behold now, you are barren and have borne no children, but you shall conceive and give birth to a son."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The angel of the Lord appeared to her and said, “You are barren and childless, but you are going to become pregnant and give birth to a son."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The angel of the Lord appeared to Manoah’s wife and said, “Even though you have been unable to have children, you will soon become pregnant and give birth to a son."
      }
    },
