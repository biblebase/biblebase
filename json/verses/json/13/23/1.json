    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫年纪老迈，寿数将尽，就立他儿子所罗门为以色列王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛年老，壽數將盡的時候，就立他的兒子所羅門作王統治以色列。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛年紀老邁，日子滿足，就立他兒子所羅門作以色列的王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when David reached old age, he made his son Solomon king over Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When David was old and full of years, he made his son Solomon king over Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When David was an old man, he appointed his son Solomon to be king over Israel."
      }
    },
