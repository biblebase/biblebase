    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡三十岁以上的利未人都在统计之列，共有三万八千男子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "利未人自三十歲及以上的都被數點；按統計，他們男丁的數目，共有三萬八千人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利未人從三十歲以外的都被數點，他們男丁的數目共有三萬八千；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Levites were numbered from thirty years old and upward, and their number by census of men was 38,000."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Levites thirty years old or more were counted, and the total number of men was thirty-eight thousand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the Levites who were thirty years old or older were counted, and the total came to 38,000."
      }
    },
