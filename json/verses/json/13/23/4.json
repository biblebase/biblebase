    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其中有两万四千人在耶和华的殿里司职，六千人做官长和审判官，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "其中有二萬四千人，監管耶和華殿宇的工作；有六千人作官長和審判官；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其中有二萬四千人管理耶和華殿的事，有六千人作官長和士師，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Of these, 24,000 were to oversee the work of the house of the Lord; and 6,000 were officers and judges,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "David said, “Of these, twenty-four thousand are to be in charge of the work of the temple of the Lord and six thousand are to be officials and judges."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then David said, “From all the Levites, 24,000 will supervise the work at the Temple of the Lord. Another 6,000 will serve as officials and judges."
      }
    },
