    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希斯仑的儿子迦勒和妻子阿苏巴生了儿子，他也和耶略生了儿子。阿苏巴的儿子是耶设、朔罢和押墩。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希斯崙的兒子迦勒與妻子阿蘇巴生了兒子，也和耶略生了兒子；她的兒子是耶設、朔罷和押墩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希斯崙的兒子迦勒娶阿蘇巴和耶略為妻，阿蘇巴的兒子是耶設、朔罷、押墩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Caleb the son of Hezron had sons by Azubah his wife, and by Jerioth; and these were her sons: Jesher, Shobab, and Ardon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Caleb son of Hezron had children by his wife Azubah (and by Jerioth). These were her sons: Jesher, Shobab and Ardon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Hezron’s son Caleb had sons from his wife Azubah and from Jerioth. Her sons were named Jesher, Shobab, and Ardon."
      }
    },
