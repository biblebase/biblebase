    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "迦米的儿子是亚干，这亚干私藏本该毁灭的东西，连累了以色列人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "迦米的兒子是亞干；這亞干偷取了當毀滅之物，連累了以色列人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "迦米的兒子是亞干，這亞干在當滅的物上犯了罪，連累了以色列人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The son of Carmi was Achar, the troubler of Israel, who violated the ban."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The son of Karmi: Achar, who brought trouble on Israel by violating the ban on taking devoted things."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The son of Carmi (a descendant of Zimri) was Achan, who brought disaster on Israel by taking plunder that had been set apart for the Lord."
      }
    },
