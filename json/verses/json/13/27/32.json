    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫的叔叔约拿单很精明，任谋士和书记。哈摩尼的儿子耶歇做众王子的老师。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛的叔父約拿單是一個謀士；這人很有智慧，又是一個經學家；哈摩尼的兒子耶歇是王眾子的老師。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛的叔叔約拿單作謀士；這人有智慧，又作書記。哈摩尼的兒子耶歇作王眾子的師傅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Also Jonathan, David’s uncle, was a counselor, a man of understanding, and a scribe; and Jehiel the son of Hachmoni tutored the king’s sons."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jonathan, David’s uncle, was a counselor, a man of insight and a scribe. Jehiel son of Hakmoni took care of the king’s sons."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jonathan, David’s uncle, was a wise counselor to the king, a man of great insight, and a scribe. Jehiel the Hacmonite was responsible for teaching the king’s sons."
      }
    },
