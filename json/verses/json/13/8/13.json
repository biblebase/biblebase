    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利巴力另外的两个儿子比利亚和示玛是亚雅仑居民的族长，他们赶走了迦特人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "還有比利亞和示瑪，他們二人是亞雅崙居民各家族的首領，曾把迦特的居民趕走。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又有比利亞和示瑪是亞雅崙居民的族長，是驅逐迦特人的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and Beriah and Shema, who were heads of fathers’ households of the inhabitants of Aijalon, who put to flight the inhabitants of Gath;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and Beriah and Shema, who were heads of families of those living in Aijalon and who drove out the inhabitants of Gath."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Beriah, and Shema. They were the leaders of the clans living in Aijalon, and they drove out the inhabitants of Gath."
      }
    },
