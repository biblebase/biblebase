    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以下這些人是以忽的子孫，他們是迦巴居民各家族的首領，曾被擄到瑪拿轄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以忽的兒子作迦巴居民的族長，被擄到瑪拿轄；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These are the sons of Ehud: these are the heads of fathers’ households of the inhabitants of Geba, and they carried them into exile to Manahath,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These were the descendants of Ehud, who were heads of families of those living in Geba and were deported to Manahath:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The sons of Ehud, leaders of the clans living at Geba, were exiled to Manahath."
      }
    },
