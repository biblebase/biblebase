      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫又吩咐以色列的众首领帮助他儿子所罗门，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“耶和華你們的　神不是與你們同在嗎？不是使你們四境都安寧嗎？因為他已經把這地的居民交在我手中；這地在耶和華和他的子民面前被治服了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛又吩咐以色列的眾首領幫助他兒子所羅門，說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David also commanded all the leaders of Israel to help his son Solomon, saying,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then David ordered all the leaders of Israel to help his son Solomon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then David ordered all the leaders of Israel to assist Solomon in this project."
      }
