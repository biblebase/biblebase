    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "还有不计其数的金匠、银匠、铜匠和铁匠。现在动工吧，愿耶和华与你同在。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛又吩咐以色列的眾領袖幫助他的兒子所羅門，說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並有無數的金銀銅鐵。你當起來辦事，願耶和華與你同在。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Of the gold, the silver and the bronze and the iron there is no limit. Arise and work, and may the Lord be with you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "in gold and silver, bronze and iron—craftsmen beyond number. Now begin the work, and the Lord be with you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have expert goldsmiths and silversmiths and workers of bronze and iron. Now begin the work, and may the Lord be with you!”"
      }
    },
