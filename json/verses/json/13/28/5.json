    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华赐给我许多儿子，祂在我众子中拣选了所罗门，让他坐以色列的王位，治理耶和华的国。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在我的眾子中，他揀選了我的兒子所羅門，坐耶和華的國位，統治以色列。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華賜我許多兒子，在我兒子中揀選所羅門坐耶和華的國位，治理以色列人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Of all my sons (for the Lord has given me many sons), He has chosen my son Solomon to sit on the throne of the kingdom of the Lord over Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Of all my sons—and the Lord has given me many—he has chosen my son Solomon to sit on the throne of the kingdom of the Lord over Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And from among my sons—for the Lord has given me many—he chose Solomon to succeed me on the throne of Israel and to rule over the Lord’s kingdom."
      }
    },
