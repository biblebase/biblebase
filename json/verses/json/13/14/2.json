    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时，大卫知道耶和华已立他做以色列的王，并因自己以色列子民的缘故而使他国家兴旺。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，大衛知道耶和華已經堅立他作王統治以色列，又為了以色列民的緣故，使他的國高舉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛就知道耶和華堅立他作以色列王，又為自己的民以色列，使他的國興旺。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And David realized that the Lord had established him as king over Israel, and that his kingdom was highly exalted, for the sake of His people Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And David knew that the Lord had established him as king over Israel and that his kingdom had been highly exalted for the sake of his people Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And David realized that the Lord had confirmed him as king over Israel and had greatly blessed his kingdom for the sake of his people Israel."
      }
    },
