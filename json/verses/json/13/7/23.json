    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来，以法莲与妻子同房，妻子怀孕，生了一个儿子。以法莲因家里遭遇不幸，就给孩子取名叫比利亚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮與妻子同房，他的妻子就懷孕，生了一個兒子；以法蓮就給他起名叫比利亞，因為他的家遭遇禍患。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮與妻同房，他妻就懷孕生了一子，以法蓮因為家裏遭禍，就給這兒子起名叫比利亞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he went in to his wife, and she conceived and bore a son, and he named him Beriah, because misfortune had come upon his house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he made love to his wife again, and she became pregnant and gave birth to a son. He named him Beriah, because there had been misfortune in his family."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Afterward Ephraim slept with his wife, and she became pregnant and gave birth to a son. Ephraim named him Beriah because of the tragedy his family had suffered."
      }
    },
