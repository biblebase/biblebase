    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们要刚强，为我们的人民和我们上帝的城邑而奋勇作战。愿耶和华成全祂自己的旨意！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你當剛強；為了我們的人民和我們　神的眾城鎮，我們要剛強；願耶和華成全他看為美的行事。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們都當剛強，為本國的民和神的城邑作大丈夫，願耶和華憑他的意旨而行。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be strong, and let us show ourselves courageous for the sake of our people and for the cities of our God; and may the Lord do what is good in His sight.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be strong, and let us fight bravely for our people and the cities of our God. The Lord will do what is good in his sight.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Be courageous! Let us fight bravely for our people and the cities of our God. May the Lord’s will be done.”"
      }
