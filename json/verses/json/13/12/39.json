    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们在那里与大卫一起吃喝了三天，因为他们的亲族为他们预备了食物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在那裡三天，與大衛在一起吃喝，因為他們的親族給他們預備好了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們在那裏三日，與大衛一同吃喝，因為他們的族弟兄給他們預備了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They were there with David three days, eating and drinking, for their kinsmen had prepared for them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The men spent three days there with David, eating and drinking, for their families had supplied provisions for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They feasted and drank with David for three days, for preparations had been made by their relatives for their arrival."
      }
    },
