    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "来自以萨迦支派、洞悉时事且深明以色列当行之道、统领族人的二百位族长；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以薩迦支派的子孫中，通曉時勢和知道以色列當怎樣行的，共有二百個首領；他們所有的親族都聽從他們的命令。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以薩迦支派，有二百族長都通達時務，知道以色列人所當行的；他們族弟兄都聽從他們的命令。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Of the sons of Issachar, men who understood the times, with knowledge of what Israel should do, their chiefs were two hundred; and all their kinsmen were at their command."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "from Issachar, men who understood the times and knew what Israel should do—200 chiefs, with all their relatives under their command;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From the tribe of Issachar, there were 200 leaders of the tribe with their relatives. All these men understood the signs of the times and knew the best course for Israel to take."
      }
    },
