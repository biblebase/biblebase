    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希幔和耶杜顿负责用号、钹及其他乐器伴奏歌颂上帝。耶杜顿的子孙负责守门。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希幔、耶杜頓和他們一起吹號敲鈸，以及演奏各種樂器，大聲歌頌　神。耶杜頓的子孫負責守門。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希幔、耶杜頓同着他們吹號、敲鈸，大發響聲，並用別的樂器隨着歌頌神。耶杜頓的子孫作守門的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And with them were Heman and Jeduthun with trumpets and cymbals for those who should sound aloud, and with instruments for the songs of God, and the sons of Jeduthun for the gate."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Heman and Jeduthun were responsible for the sounding of the trumpets and cymbals and for the playing of the other instruments for sacred song. The sons of Jeduthun were stationed at the gate."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They used their trumpets, cymbals, and other instruments to accompany their songs of praise to God. And the sons of Jeduthun were appointed as gatekeepers."
      }
    },
