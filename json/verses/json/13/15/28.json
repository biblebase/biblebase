    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人把耶和华的约柜抬了上来，一路上欢呼吹角、鸣号、敲钹、鼓瑟、弹琴，大声奏乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，以色列人歡呼、吹角、吹號、敲鈸、鼓瑟、彈琴，大聲奏樂，把耶和華的約櫃抬了上來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，以色列眾人歡呼吹角、吹號、敲鈸、鼓瑟、彈琴，大發響聲，將耶和華的約櫃擡上來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus all Israel brought up the ark of the covenant of the Lord with shouting, and with sound of the horn, with trumpets, with loud-sounding cymbals, with harps and lyres."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So all Israel brought up the ark of the covenant of the Lord with shouts, with the sounding of rams’ horns and trumpets, and of cymbals, and the playing of lyres and harps."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So all Israel brought up the Ark of the Lord’s Covenant with shouts of joy, the blowing of rams’ horns and trumpets, the crashing of cymbals, and loud playing on harps and lyres."
      }
    },
