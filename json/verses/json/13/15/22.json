    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "利未人的族长基拿尼雅善于歌唱，是歌乐手的领班。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "利未支派的領袖基拿尼雅專責音樂，又教人音樂，因為他精通音樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利未人的族長基拿尼雅是歌唱人的首領，又教訓人歌唱，因為他精通此事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Chenaniah, chief of the Levites, was in charge of the singing; he gave instruction in singing because he was skillful."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Kenaniah the head Levite was in charge of the singing; that was his responsibility because he was skillful at it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Kenaniah, the head Levite, was chosen as the choir leader because of his skill."
      }
    },
