    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "回来的祭司共一千七百六十人，全是族长，善于办理上帝殿的事务。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以及他們的眾親族，他們家族的首領，共有一千七百六十人，都是有才幹，很會作神殿裡工作的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們和眾弟兄都是族長，共有一千七百六十人，是善於做神殿使用之工的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and their relatives, heads of their fathers’ households, 1,760 very able men for the work of the service of the house of God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priests, who were heads of families, numbered 1,760. They were able men, responsible for ministering in the house of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In all, 1,760 priests returned. They were heads of clans and very able men. They were responsible for ministering at the house of God."
      }
    },
