    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫的勇士有哈革摩尼人雅朔班，他是三勇士之首，曾在一次交战中挥矛刺死了三百人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這是大衛眾勇士的數目：哈革摩尼的兒子雅朔班，是軍官”；《七十士譯本》的一些抄本作“三”）的首領；他揮舞矛槍，只一次就殺死了三百人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛勇士的數目記在下面：哈革摩尼的兒子雅朔班，他是軍長的統領，一時舉槍殺了三百人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These constitute the list of the mighty men whom David had: Jashobeam, the son of a Hachmonite, the chief of the thirty; he lifted up his spear against three hundred whom he killed at one time."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "this is the list of David’s mighty warriors: Jashobeam, a Hakmonite, was chief of the officers; he raised his spear against three hundred men, whom he killed in one encounter."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Here is the record of David’s mightiest warriors: The first was Jashobeam the Hacmonite, who was leader of the Three—the mightiest warriors among David’s men. He once used his spear to kill 300 enemy warriors in a single battle."
      }
    },
