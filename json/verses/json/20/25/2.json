    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "将事隐藏是上帝的荣耀， 把事查明是君王的光荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "把事隱藏，是　神的榮耀； 把事察清，是君王的光榮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將事隱祕乃神的榮耀； 將事察清乃君王的榮耀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is the glory of God to conceal a matter, But the glory of kings is to search out a matter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is the glory of God to conceal a matter; to search out a matter is the glory of kings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is God’s privilege to conceal things and the king’s privilege to discover them."
      }
