        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "富人自以为有智慧， 却被明智的穷人看透。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "財主自以為有智慧， 聰明的窮人卻能看透他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "富足人自以為有智慧， 但聰明的貧窮人能將他查透。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The rich man is wise in his own eyes, But the poor who has understanding sees through him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The rich are wise in their own eyes; one who is poor and discerning sees how deluded they are."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rich people may think they are wise, but a poor person with discernment can see right through them."
      }
