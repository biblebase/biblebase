    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "水蛭有两个女儿， 她们不停地叫：“给我！给我！” 三样东西从不知足， 永不言“够”的共有四样："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "水蛭有兩個女兒，常說：‘給我！給我！’ 不知足的東西有三樣， 連總不說‘夠了’的共有四樣，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "螞蟥有兩個女兒， 常說：給呀，給呀！ 有三樣不知足的， 連不說「夠的」共有四樣："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The leech has two daughters, “Give,” “Give.” There are three things that will not be satisfied, Four that will not say, “Enough”:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The leech has two daughters. ‘Give! Give!’ they cry. “There are three things that are never satisfied, four that never say, ‘Enough!’:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The leech has two suckers that cry out, “More, more!” There are three things that are never satisfied— no, four that never say, “Enough!”:"
      }
    },
