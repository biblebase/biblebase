      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "鹰在空中飞翔之道， 蛇在石上爬行之道， 船在海中航行之道， 男女相爱之道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就是鷹在空中飛翔之道， 蛇在磐石上爬行之道， 船在海中航行之道， 男與女交合之道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是鷹在空中飛的道； 蛇在磐石上爬的道； 船在海中行的道； 男與女交合的道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The way of an eagle in the sky, The way of a serpent on a rock, The way of a ship in the middle of the sea, And the way of a man with a maid."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the way of an eagle in the sky, the way of a snake on a rock, the way of a ship on the high seas, and the way of a man with a young woman."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "how an eagle glides through the sky, how a snake slithers on a rock, how a ship navigates the ocean, how a man loves a woman."
      }
