    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人有智慧，自得益处； 人好嘲讽，自食恶果。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你有智慧，你的智慧必使你得益； 如果你譏笑人，你就必獨自擔當一切後果。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你若有智慧，是與自己有益； 你若褻慢，就必獨自擔當。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you are wise, you are wise for yourself, And if you scoff, you alone will bear it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you are wise, your wisdom will reward you; if you are a mocker, you alone will suffer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you become wise, you will be the one to benefit. If you scorn wisdom, you will be the one to suffer."
      }
    },
