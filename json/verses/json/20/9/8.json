    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要责备嘲讽者，免得他恨你； 要责备智者，他必爱你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不要責備好譏笑人的， 免得他恨你； 要責備智慧人，他必愛你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要責備褻慢人，恐怕他恨你； 要責備智慧人，他必愛你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not reprove a scoffer, or he will hate you, Reprove a wise man and he will love you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not rebuke mockers or they will hate you; rebuke the wise and they will love you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So don’t bother correcting mockers; they will only hate you. But correct the wise, and they will love you."
      }
