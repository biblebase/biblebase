    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "敬畏耶和华使人得享生命， 安然满足，免遭祸患。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "敬畏耶和華的，得著生命； 他必安居知足，不會遭受禍患。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "敬畏耶和華的，得着生命； 他必恆久知足，不遭禍患。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The fear of the Lord  leads to life, So that one may sleep satisfied, untouched by evil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The fear of the Lord leads to life; then one rests content, untouched by trouble."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fear of the Lord leads to life, bringing security and protection from harm."
      }
