        "text": "謹慎口舌的，可保性命； 口沒遮攔的，自取滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "謹守口的，得保生命； 大張嘴的，必致敗亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The one who guards his mouth preserves his life; The one who opens wide his lips comes to ruin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who guard their lips preserve their lives, but those who speak rashly will come to ruin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who control their tongue will have a long life; opening your mouth can ruin everything."
      }
