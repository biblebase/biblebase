    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "公义守卫正直的人， 邪恶倾覆犯罪之徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正義護衛行為完全的人； 邪惡卻使犯罪的人傾覆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行為正直的，有公義保守； 犯罪的，被邪惡傾覆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Righteousness guards the one whose way is blameless, But wickedness subverts the sinner."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Righteousness guards the person of integrity, but wickedness overthrows the sinner."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Godliness guards the path of the blameless, but the evil are misled by sin."
      }
