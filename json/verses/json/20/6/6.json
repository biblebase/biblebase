      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "懒惰的人啊，去看看蚂蚁， 观察它的生活，就会有智慧。"
      },
      "cnv": {
        "version_id": "CNV",
