      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要救自己，如鹿脫離獵戶的手， 如鳥脫離捕鳥人的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Deliver yourself like a gazelle from the hunter’s hand And like a bird from the hand of the fowler."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Free yourself, like a gazelle from the hand of the hunter, like a bird from the snare of the fowler."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Save yourself like a gazelle escaping from a hunter, like a bird fleeing from a net."
      }
