    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂铺设诸天的时候，我已在场。 祂在深渊上画出地平线，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他立定諸天的時候，我在那裡； 他在淵面上界劃地平線的時候，我在那裡；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他立高天，我在那裏； 他在淵面的周圍，劃出圓圈。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He established the heavens, I was there, When He inscribed a circle on the face of the deep,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I was there when he set the heavens in place, when he marked out the horizon on the face of the deep,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I was there when he established the heavens, when he drew the horizon on the oceans."
      }
