        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她说的话带着智慧， 她的训言充满慈爱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她開口就說出智慧的話； 她的舌頭上充滿慈愛的訓誨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她開口就發智慧； 她舌上有仁慈的法則。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She opens her mouth in wisdom, And the teaching of kindness is on her tongue."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She speaks with wisdom, and faithful instruction is on her tongue."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When she speaks, her words are wise, and she gives instructions with kindness."
      }
