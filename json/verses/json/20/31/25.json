        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她充满力量和尊荣， 她以笑颜迎接未来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她以能力和威儀為衣服， 想到日後的幸福就歡笑。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "能力和威儀是她的衣服； 她想到日後的景況就喜笑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Strength and dignity are her clothing, And she smiles at the future."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She is clothed with strength and dignity; she can laugh at the days to come."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She is clothed with strength and dignity, and she laughs without fear of the future."
      }
