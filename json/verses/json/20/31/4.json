    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "利慕伊勒啊， 君王不可喝酒， 不可喝酒， 首领不宜喝烈酒，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "利慕伊勒啊！君王不可喝酒； 君王千萬不可喝酒； 掌權的不可貪飲烈酒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利慕伊勒啊，君王喝酒，君王喝酒不相宜； 王子說濃酒在那裏也不相宜；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is not for kings, O Lemuel, It is not for kings to drink wine, Or for rulers to desire strong drink,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is not for kings, Lemuel— it is not for kings to drink wine, not for rulers to crave beer,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is not for kings, O Lemuel, to guzzle wine. Rulers should not crave alcohol."
      }
    },
