    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "背弃正道，自食恶果； 善人行善，必得善报。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "心中背道的，必飽嘗自己行為的惡果； 善人也因自己所行的，得到善報。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心中背道的，必滿得自己的結果； 善人必從自己的行為得以知足。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The backslider in heart will have his fill of his own ways, But a good man will be satisfied with his."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The faithless will be fully repaid for their ways, and the good rewarded for theirs."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Backsliders get what they deserve; good people receive their reward."
      }
    },
