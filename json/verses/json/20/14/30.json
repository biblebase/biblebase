    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "心平气和，滋润生命； 妒火中烧，啃蚀骨头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "心裡平靜，可使身體健康； 但嫉妒能使骨頭朽爛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心中安靜是肉體的生命； 嫉妒是骨中的朽爛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A tranquil heart is life to the body, But passion is rottenness to the bones."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A heart at peace gives life to the body, but envy rots the bones."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A peaceful heart leads to a healthy body; jealousy is like cancer in the bones."
      }
    },
