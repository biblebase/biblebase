    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "百姓无神谕便任意妄为， 但遵守律法的人必蒙福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有啟示，人民就沒有法紀； 遵守律法的，就為有福。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "沒有異象，民就放肆； 惟遵守律法的，便為有福。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Where there is no vision, the people are unrestrained, But happy is he who keeps the law."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Where there is no revelation, people cast off restraint; but blessed is the one who heeds wisdom’s instruction."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When people do not accept divine guidance, they run wild. But whoever obeys the law is joyful."
      }
    },
