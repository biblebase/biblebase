      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雇愚昧人的，與雇過路人的， 就像射傷眾人的弓箭手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like an archer who wounds everyone, So is he who hires a fool or who hires those who pass by."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like an archer who wounds at random is one who hires a fool or any passer-by."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "An employer who hires a fool or a bystander is like an archer who shoots at random."
      }
    },
