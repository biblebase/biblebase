    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "懒惰人赖在床上滚来滚去， 就像门在门轴上转来转去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "懶惰人在床上翻動， 就像門繞著門鉸轉動一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門在樞紐轉動， 懶惰人在牀上也是如此。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As the door turns on its hinges, So does the sluggard on his bed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As a door turns on its hinges, so a sluggard turns on his bed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As a door swings back and forth on its hinges, so the lazy person turns over in bed."
      }
    },
