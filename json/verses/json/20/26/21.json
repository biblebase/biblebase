      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "好斗之人煽动争端， 如同余火加炭、火上加柴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "好爭競的人，煽動爭端， 就像餘燼上加炭，火上加柴一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "好爭競的人煽惑爭端， 就如餘火加炭，火上加柴一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like charcoal to hot embers and wood to fire, So is a contentious man to kindle strife."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As charcoal to embers and as wood to fire, so is a quarrelsome person for kindling strife."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A quarrelsome person starts fights as easily as hot embers light charcoal or fire lights wood."
      }
    },
