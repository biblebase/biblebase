      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "明哲人遇祸躲避， 愚昧人前去受害。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "精明人看見災禍，就躲藏起來； 愚蒙人反往前走，自取禍害。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "通達人見禍藏躲； 愚蒙人前往受害。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A prudent man sees evil and hides himself, The naive proceed and pay the penalty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The prudent see danger and take refuge, but the simple keep going and pay the penalty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A prudent person foresees danger and takes precautions. The simpleton goes blindly on and suffers the consequences."
      }
    },
