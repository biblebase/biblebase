    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可抛弃你的朋友和父亲的至交。 患难时别投靠兄弟， 近邻胜过远亲。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的朋友和你父親的朋友，你都不可離棄； 在你遭遇患難的日子，不可進你兄弟的家。 在附近的鄰舍勝過在遠方的兄弟。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的朋友和父親的朋友， 你都不可離棄。 你遭難的日子，不要上弟兄的家去； 相近的鄰舍強如遠方的弟兄。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not forsake your own friend or your father’s friend, And do not go to your brother’s house in the day of your calamity; Better is a neighbor who is near than a brother far away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not forsake your friend or a friend of your family, and do not go to your relative’s house when disaster strikes you— better a neighbor nearby than a relative far away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Never abandon a friend— either yours or your father’s. When disaster strikes, you won’t have to ask your brother for assistance. It’s better to go to a neighbor than to a brother who lives far away."
      }
    },
