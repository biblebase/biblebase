    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "膏油和香料使人畅快， 朋友的劝诫更加甘甜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "膏油和香料使人心暢快， 朋友真誠的勸勉也使人覺得甘甜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "膏油與香料使人心喜悅； 朋友誠實的勸教也是如此甘美。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Oil and perfume make the heart glad, So a man’s counsel is sweet to his friend."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Perfume and incense bring joy to the heart, and the pleasantness of a friend springs from their heartfelt advice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The heartfelt counsel of a friend is as sweet as perfume and incense."
      }
    },
