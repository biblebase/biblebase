      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人赚来的转眼成空， 播种公义的必获奖赏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人賺得的工價是虛假的， 播種公義的得著實在的賞賜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人經營，得虛浮的工價； 撒義種的，得實在的果效。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wicked earns deceptive wages, But he who sows righteousness gets a true reward."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A wicked person earns deceptive wages, but the one who sows righteousness reaps a sure reward."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Evil people get rich for the moment, but the reward of the godly will last."
      }
    },
