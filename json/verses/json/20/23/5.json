    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "钱财眨眼之间消逝无踪， 它必长出翅膀如鹰飞去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的眼睛注視在錢財上，錢財卻不見了， 因為錢財必長起翅膀， 如鷹飛往天上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你豈要定睛在虛無的錢財上嗎？ 因錢財必長翅膀，如鷹向天飛去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you set your eyes on it, it is gone. For wealth certainly makes itself wings Like an eagle that flies toward the heavens."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Cast but a glance at riches, and they are gone, for they will surely sprout wings and fly off to the sky like an eagle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In the blink of an eye wealth disappears, for it will sprout wings and fly away like an eagle."
      }
