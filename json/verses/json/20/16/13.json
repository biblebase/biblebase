      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Righteous lips are the delight of kings, And he who speaks right is loved."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Kings take pleasure in honest lips; they value the one who speaks what is right."
      },
      "nlt": {
