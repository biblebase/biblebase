    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人眯起眼睛图谋不轨， 歹徒咬着嘴唇策划恶事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瞇著眼睛的，圖謀乖謬的事； 緊抿著嘴唇的，作成惡事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眼目緊合的，圖謀乖僻； 嘴唇緊閉的，成就邪惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who winks his eyes does so to devise perverse things; He who compresses his lips brings evil to pass."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever winks with their eye is plotting perversity; whoever purses their lips is bent on evil."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "With narrowed eyes, people plot evil; with a smirk, they plan their mischief."
      }
    },
