    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可说：“别人怎样待我， 我就怎样待他； 我要照他所做的报复他。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不可說：“人怎樣待我，我也怎樣待他； 我要照他所行的報復他。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可說：人怎樣待我，我也怎樣待他； 我必照他所行的報復他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not say, “Thus I shall do to him as he has done to me; I will render to the man according to his work.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not say, “I’ll do to them as they have done to me; I’ll pay them back for what they did.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And don’t say, “Now I can pay them back for what they’ve done to me! I’ll get even with them!”"
      }
    },
