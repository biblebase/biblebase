    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智慧对愚人高不可及， 他在城门口沉默不语。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "智慧對愚妄人來說是太高超， 因此他在城門口啞口無言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧極高，非愚昧人所能及， 所以在城門內不敢開口。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Wisdom is too exalted for a fool, He does not open his mouth in the gate."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Wisdom is too high for fools; in the assembly at the gate they must not open their mouths."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wisdom is too lofty for fools. Among leaders at the city gate, they have nothing to say."
      }
    },
