      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她就保你遠離淫婦， 遠離說諂媚話的外女。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "That they may keep you from an adulteress, From the foreigner who flatters with her words."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will keep you from the adulterous woman, from the wayward woman with her seductive words."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let them protect you from an affair with an immoral woman, from listening to the flattery of a promiscuous woman."
      }
