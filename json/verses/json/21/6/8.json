    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那么，智者比愚人有什么优势呢？贫穷人即使懂得如何处世，又有什么益处呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "智慧人比愚昧人有甚麼長處呢？窮人在眾人面前知道怎樣行事為人，又有甚麼長處呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣看來，智慧人比愚昧人有甚麼長處呢？窮人在眾人面前知道如何行，有甚麼長處呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For what advantage does the wise man have over the fool? What advantage does the poor man have, knowing how to walk before the living?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What advantage have the wise over fools? What do the poor gain by knowing how to conduct themselves before others?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So are wise people really better off than fools? Do poor people gain anything by being wise and knowing how to act in front of others?"
      }
    },
