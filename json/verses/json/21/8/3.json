    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要在王面前鲁莽行事，也不要参与恶行，因为王可以随意行事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要輕率離開王的面前，也不要參與惡事，因為王可以隨己意作任何事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要急躁離開王的面前，不要固執行惡，因為他凡事都隨自己的心意而行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not be in a hurry to leave him. Do not join in an evil matter, for he will do whatever he pleases.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not be in a hurry to leave the king’s presence. Do not stand up for a bad cause, for he will do whatever he pleases."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t try to avoid doing your duty, and don’t stand with those who plot evil, for the king can do whatever he wants."
      }
    },
