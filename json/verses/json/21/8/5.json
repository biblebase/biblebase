    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "遵从王命的必免遭祸患，因为智者心里懂得做事的时机和方法。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "遵守命令的，必不遭受災禍；智慧人的心，曉得時機，懂得判斷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡遵守命令的，必不經歷禍患；智慧人的心能辨明時候和定理。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who keeps a royal command experiences no trouble, for a wise heart knows the proper time and procedure."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever obeys his command will come to no harm, and the wise heart will know the proper time and procedure."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who obey him will not be punished. Those who are wise will find a time and a way to do what is right,"
      }
    },
