    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然罪人作恶多端仍得长寿，我却认为敬畏上帝的人必亨通。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "罪人既然作惡百次，還享長壽，那麼我知道敬畏　神的人，就是在他面前存敬畏的心的，必享福樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "罪人雖然作惡百次，倒享長久的年日；然而我準知道，敬畏神的，就是在他面前敬畏的人，終久必得福樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Although a sinner does evil a hundred times and may lengthen his life, still I know that it will be well for those who fear God, who fear Him openly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Although a wicked person who commits a hundred crimes may live a long time, I know that it will go better with those who fear God, who are reverent before him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But even though a person sins a hundred times and still lives a long time, I know that those who fear God will be better off."
      }
    },
