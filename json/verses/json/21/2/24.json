    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对人而言，没有什么比吃喝并享受劳碌之乐更好，我看这也是出自上帝的手。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人最好是吃喝，在自己的勞碌中自得其樂；我看這也是出於　神的手。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人莫強如吃喝，且在勞碌中享福，我看這也是出於神的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is nothing better for a man than to eat and drink and tell himself that his labor is good. This also I have seen that it is from the hand of God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A person can do nothing better than to eat and drink and find satisfaction in their own toil. This too, I see, is from the hand of God,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I decided there is nothing better than to enjoy food and drink and to find satisfaction in work. Then I realized that these pleasures are from the hand of God."
      }
    },
