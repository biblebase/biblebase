    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我说：“欢笑只不过是一阵狂妄，享乐又有什么用！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對嬉笑，我說：“那是狂妄”；對享樂，我說：“這有甚麼作用呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我指嬉笑說：「這是狂妄。」論喜樂說：「有何功效呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I said of laughter, “It is madness,” and of pleasure, “What does it accomplish?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Laughter,” I said, “is madness. And what does pleasure accomplish?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I said, “Laughter is silly. What good does it do to seek pleasure?”"
      }
    },
