    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，我便财势日增，享誉盛名，超过耶路撒冷历来所有的人。然而，我仍然保持智慧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是我就日漸昌大，超過任何比我先在耶路撒冷的人；我的智慧仍然與我同在。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，我就日見昌盛，勝過以前在耶路撒冷的眾人。我的智慧仍然存留。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I became great and increased more than all who preceded me in Jerusalem. My wisdom also stood by me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I became greater by far than anyone in Jerusalem before me. In all this my wisdom stayed with me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I became greater than all who had lived in Jerusalem before me, and my wisdom never failed me."
      }
    },
