    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我心里想：“不论义人或恶人，上帝必审判，因为万事万务都有受审之时。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我自己心裡說：“　神必審判義人和惡人，因為各樣事務、各樣工作都有定時。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我心裏說，神必審判義人和惡人；因為在那裏，各樣事務，一切工作，都有定時。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I said to myself, “God will judge both the righteous man and the wicked man,” for a time for every matter and for every deed is there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I said to myself, “God will bring into judgment both the righteous and the wicked, for there will be a time for every activity, a time to judge every deed.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I said to myself, “In due season God will judge everyone, both good and bad, for all their deeds.”"
      }
    },
