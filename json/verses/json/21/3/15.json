    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在的事并不是新事，未来的事也早已发生过。上帝使过去的事再次出现。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在有的，先前就有；將來有的，早已有了；因為　神使已過的事重新出現。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現今的事早先就有了，將來的事早已也有了，並且神使已過的事重新再來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "That which is has been already and that which will be has already been, for God seeks what has passed by."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whatever is has already been, and what will be has been before; and God will call the past to account."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What is happening now has happened before, and what will happen in the future has happened before, because God makes the same things happen over and over again."
      }
