    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁知道人的气息会向上升，兽的气息会降到地下呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有誰知道人的靈是往上升，牲畜的魂是下降而入地呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誰知道人的靈是往上升，獸的魂是下入地呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who knows that the breath of man ascends upward and the breath of the beast descends downward to the earth?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who knows if the human spirit rises upward and if the spirit of the animal goes down into the earth?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For who can prove that the human spirit goes up and the spirit of animals goes down into the earth?"
      }
    },
