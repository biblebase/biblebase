    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "美好的名声胜过珍贵的膏油， 人死之日胜过出生之时。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "美名優於美好的膏油， 死亡的日子勝過誕生的日子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "名譽強如美好的膏油；人死的日子勝過人生的日子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A good name is better than a good ointment, And the day of one’s death is better than the day of one’s birth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A good name is better than fine perfume, and the day of death better than the day of birth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A good reputation is more valuable than costly perfume. And the day you die is better than the day you are born."
      }
