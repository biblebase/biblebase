    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在我虚空的一生中，我见过义人行义，反而灭亡；恶人行恶，却享长寿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在我虛空的日子裡，我見過這兩件事：有義人行義反而滅亡，有惡人行惡倒享長壽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有義人行義，反致滅亡；有惡人行惡，倒享長壽。這都是我在虛度之日中所見過的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have seen everything during my lifetime of futility; there is a righteous man who perishes in his righteousness and there is a wicked man who prolongs his life in his wickedness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In this meaningless life of mine I have seen both of these: the righteous perishing in their righteousness, and the wicked living long in their wickedness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have seen everything in this meaningless life, including the death of good young people and the long life of wicked people."
