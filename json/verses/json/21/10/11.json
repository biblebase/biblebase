    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弄蛇人行法术之前， 若先被蛇咬， 行法术还有什么用呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "未行法術先被蛇咬，那麼，法術對行法術的人就毫無用處。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "未行法術以先，蛇若咬人， 後行法術也是無益。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If the serpent bites before being charmed, there is no profit for the charmer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If a snake bites before it is charmed, the charmer receives no fee."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If a snake bites before you charm it, what’s the use of being a snake charmer?"
      }
    },
