    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不过，未经你同意，我不愿意这样做，因为我盼望你的善行是出于甘心，而非勉强。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但還沒有得到你的同意，我就不願意這樣作，好叫你的善行不是出於勉強，而是出於甘心。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但不知道你的意思，我就不願意這樣行，叫你的善行不是出於勉強，乃是出於甘心。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but without your consent I did not want to do anything, so that your goodness would not be, in effect, by compulsion but of your own free will."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I did not want to do anything without your consent, so that any favor you do would not seem forced but would be voluntary."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I didn’t want to do anything without your consent. I wanted you to help because you were willing, not because you were forced."
      }
    },
