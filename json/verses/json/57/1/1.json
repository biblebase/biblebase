    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是为了传扬基督耶稣而被囚禁的保罗，与提摩太弟兄写信给我们亲爱的同工腓利门弟兄、"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為基督耶穌被囚禁的保羅，和提摩太弟兄，寫信給我們所愛的，又一同作工的腓利門，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為基督耶穌被囚的保羅，同兄弟提摩太寫信給我們所親愛的同工腓利門，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Paul, a prisoner of Christ Jesus, and Timothy our brother, To Philemon our beloved brother and fellow worker,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Paul, a prisoner of Christ Jesus, and Timothy our brother, To Philemon our dear friend and fellow worker—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This letter is from Paul, a prisoner for preaching the Good News about Christ Jesus, and from our brother Timothy. I am writing to Philemon, our beloved co-worker,"
      }
    },
