    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我保罗在此亲笔保证，我必偿还。其实我不说你也知道，连你自己也欠我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我必償還”，這是我保羅親手寫的。用不著我說，甚至你的生命，你也是欠我的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必償還，這是我－保羅親筆寫的。我並不用對你說，連你自己也是虧欠於我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I, Paul, am writing this with my own hand, I will repay it (not to mention to you that you owe to me even your own self as well)."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I, Paul, am writing this with my own hand. I will pay it back—not to mention that you owe me your very self."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I, Paul, write this with my own hand: I will repay it. And I won’t mention that you owe me your very soul!"
      }
    },
