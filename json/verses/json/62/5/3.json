    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为我们遵行上帝的命令就是爱上帝，祂的命令并不难遵守。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們遵守　神的命令，就是愛他了；而且他的命令是不難遵守的，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們遵守神的誡命，這就是愛他了，並且他的誡命不是難守的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For this is the love of God, that we keep His commandments; and His commandments are not burdensome."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In fact, this is love for God: to keep his commands. And his commands are not burdensome,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Loving God means keeping his commandments, and his commandments are not burdensome."
      }
