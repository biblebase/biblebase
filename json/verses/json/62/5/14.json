    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们若按着上帝的旨意祈求，祂必垂听，这是我们对上帝的信心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我們照著　神的旨意祈求，他必聽我們；這就是我們對　神所存的坦然無懼的心。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們若照他的旨意求甚麼，他就聽我們，這是我們向他所存坦然無懼的心。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is the confidence which we have before Him, that, if we ask anything according to His will, He hears us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is the confidence we have in approaching God: that if we ask anything according to his will, he hears us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And we are confident that he hears us whenever we ask for anything that pleases him."
      }
    },
