    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们既然知道上帝垂听我们一切的祈求，就知道我们能得到所求的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "既然我們知道他聽我們的祈求，我們就知道，我們無論求甚麼，他必賜給我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "既然知道他聽我們一切所求的，就知道我們所求於他的，無不得着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And if we know that He hears us in whatever we ask, we know that we have the requests which we have asked from Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And if we know that he hears us—whatever we ask—we know that we have what we asked of him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And since we know he hears us when we make our requests, we also know that he will give us what we ask for."
      }
    },
