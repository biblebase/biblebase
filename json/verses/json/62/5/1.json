    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "每一个信耶稣是基督的人，都是从上帝生的。凡爱生他之上帝的，也爱上帝所生的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡信耶穌是基督的，都是從　神生的，凡愛那生他的，也必愛那從他而生的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡信耶穌是基督的，都是從神而生，凡愛生他之神的，也必愛從神生的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whoever believes that Jesus is the Christ is born of God, and whoever loves the Father loves the child born of Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Everyone who believes that Jesus is the Christ is born of God, and everyone who loves the father loves his child as well."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone who believes that Jesus is the Christ has become a child of God. And everyone who loves the Father loves his children, too."
      }
    },
