    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们知道从上帝生的不会继续犯罪，因为上帝的儿子保护他，那恶者无法害他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們知道凡從　神生的就不犯罪，而且從　神生的那一位也必保守他，連那惡者也不能碰他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們知道凡從神生的，必不犯罪，從神生的，必保守自己，那惡者也就無法害他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We know that no one who is born of God sins; but He who was born of God keeps him, and the evil one does not touch him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We know that anyone born of God does not continue to sin; the One who was born of God keeps them safe, and the evil one cannot harm them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We know that God’s children do not make a practice of sinning, for God’s Son holds them securely, and the evil one cannot touch them."
      }
