    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当我们爱上帝，遵行祂的命令时，便知道自己也爱祂的儿女。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們若愛　神，並且遵行他的命令，就知道我們是愛　神的兒女了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們若愛神，又遵守他的誡命，從此就知道我們愛神的兒女。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By this we know that we love the children of God, when we love God and observe His commandments."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is how we know that we love the children of God: by loving God and carrying out his commands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We know we love God’s children if we love God and obey his commandments."
      }
    },
