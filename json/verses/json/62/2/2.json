    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂为我们的罪作了赎罪祭，不只是为我们的罪，也是为全人类的罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他為我們的罪作了贖罪祭，不僅為我們的罪，也為全人類的罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他為我們的罪作了挽回祭，不是單為我們的罪，也是為普天下人的罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and He Himself is the propitiation for our sins; and not for ours only, but also for those of the whole world."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He is the atoning sacrifice for our sins, and not only for ours but also for the sins of the whole world."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He himself is the sacrifice that atones for our sins—and not only our sins but the sins of all the world."
      }
    },
