    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若有人说自己在光明中，却恨他的弟兄姊妹，他就仍然活在黑暗里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是說自己在光明中，卻恨他的弟兄的，他到現在還在黑暗裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人若說自己在光明中，卻恨他的弟兄，他到如今還是在黑暗裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The one who says he is in the Light and yet hates his brother is in the darkness until now."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who claims to be in the light but hates a brother or sister is still in the darkness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If anyone claims, “I am living in the light,” but hates a fellow believer, that person is still living in darkness."
      }
    },
