    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "父老们，我写信给你们，因为你们认识了从太初就存在的那位。年轻人，我写信给你们，因为你们已经战胜了那恶者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "父老們，我寫信給你們， 因為你們認識太初就存在的那一位。 青年們，我寫信給你們， 因為你們已經勝過那惡者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "父老啊，我寫信給你們， 因為你們認識那從起初原有的。 少年人哪，我寫信給你們， 因為你們勝了那惡者。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am writing to you, fathers, because you know Him who has been from the beginning. I am writing to you, young men, because you have overcome the evil one. I have written to you, children, because you know the Father."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am writing to you, fathers, because you know him who is from the beginning. I am writing to you, young men, because you have overcome the evil one."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am writing to you who are mature in the faith because you know Christ, who existed from the beginning. I am writing to you who are young in the faith because you have won your battle with the evil one."
      }
    },
