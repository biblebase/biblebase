    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要爱世界和世上的事，因为人若爱世界，就不会再有爱父的心了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要愛世界和世上的東西。人若愛世界，愛父的心就不在他裡面了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要愛世界和世界上的事。人若愛世界，愛父的心就不在他裏面了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not love the world nor the things in the world. If anyone loves the world, the love of the Father is not in him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not love the world or anything in the world. If anyone loves the world, love for the Father is not in them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not love this world nor the things it offers you, for when you love the world, you do not have the love of the Father in you."
      }
    },
