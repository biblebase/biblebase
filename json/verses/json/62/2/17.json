    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这世界和其中一切的私欲都要过去，但遵守上帝旨意的人永远长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這世界和世上的私慾都要漸漸過去，但那遵行　神旨意的卻存到永遠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這世界和其上的情慾都要過去，惟獨遵行神旨意的，是永遠常存。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The world is passing away, and also its lusts; but the one who does the will of God lives forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The world and its desires pass away, but whoever does the will of God lives forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And this world is fading away, along with everything that people crave. But anyone who does what pleases God will live forever."
      }
    },
