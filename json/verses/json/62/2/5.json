    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "遵行主话语的人是真正全心爱上帝的人，我们借此知道自己是在主里面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然而凡是遵守他的道的，他愛　神的心就的確在他裡面完全了。這樣，我們就知道我們是在　神裡面了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡遵守主道的，愛神的心在他裏面實在是完全的。從此，我們知道我們是在主裏面。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but whoever keeps His word, in him the love of God has truly been perfected. By this we know that we are in Him:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if anyone obeys his word, love for God is truly made complete in them. This is how we know we are in him:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But those who obey God’s word truly show how completely they love him. That is how we know we are living in him."
      }
    },
