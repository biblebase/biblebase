    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "任何灵若承认耶稣基督曾降世为人，就是从上帝来的，你们可以凭这一点认出上帝的灵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們可以這樣認出　神的靈：凡是承認耶穌基督是成了肉身來的，那靈就是出於　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡靈認耶穌基督是成了肉身來的，就是出於神的；從此你們可以認出神的靈來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By this you know the Spirit of God: every spirit that confesses that Jesus Christ has come in the flesh is from God;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is how you can recognize the Spirit of God: Every spirit that acknowledges that Jesus Christ has come in the flesh is from God,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is how we know if they have the Spirit of God: If a person claiming to be a prophet acknowledges that Jesus Christ came in a real body, that person has the Spirit of God."
      }
    },
