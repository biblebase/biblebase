    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亲爱的弟兄姊妹，我们应当彼此相爱，因为爱是从上帝来的。凡有爱心的人都是从上帝生的，并且认识上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "親愛的，我們應當彼此相愛，因為愛是從　神那裡來的。凡是愛人的，都是從　神生的，並且認識　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "親愛的弟兄啊，我們應當彼此相愛，因為愛是從神來的。凡有愛心的，都是由神而生，並且認識神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Beloved, let us love one another, for love is from God; and everyone who loves is born of God and knows God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Dear friends, let us love one another, for love comes from God. Everyone who loves has been born of God and knows God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Dear friends, let us continue to love one another, for love comes from God. Anyone who loves is a child of God and knows God."
      }
    },
