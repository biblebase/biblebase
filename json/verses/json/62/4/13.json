    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝将祂的灵赐给了我们，所以我们知道自己住在祂里面，祂也住在我们里面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神把他的靈賜給我們，我們就知道我們是住在他裡面，他也住在我們裡面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神將他的靈賜給我們，從此就知道我們是住在他裏面，他也住在我們裏面。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By this we know that we abide in Him and He in us, because He has given us of His Spirit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is how we know that we live in him and he in us: He has given us of his Spirit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And God has given us his Spirit as proof that we live in him and he in us."
      }
    },
