    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不是我们爱上帝，而是上帝爱我们，并且差遣祂儿子为我们的罪作了赎罪祭，这就是爱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不是我們愛　神，而是　神愛我們，差遣他的兒子為我們的罪作了贖罪祭，這就是愛了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不是我們愛神，乃是神愛我們，差他的兒子為我們的罪作了挽回祭，這就是愛了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In this is love, not that we loved God, but that He loved us and sent His Son to be the propitiation for our sins."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is love: not that we loved God, but that he loved us and sent his Son as an atoning sacrifice for our sins."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is real love—not that we loved God, but that he loved us and sent his Son as a sacrifice to take away our sins."
      }
