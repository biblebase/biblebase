    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恨弟兄姊妹的就是杀人的，你们知道杀人的没有永生。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡恨弟兄的，就是殺人的。你們知道，殺人的在他裡面沒有永生。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡恨他弟兄的，就是殺人的；你們曉得凡殺人的，沒有永生存在他裏面。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Everyone who hates his brother is a murderer; and you know that no murderer has eternal life abiding in him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who hates a brother or sister is a murderer, and you know that no murderer has eternal life residing in him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who hates another brother or sister is really a murderer at heart. And you know that murderers don’t have eternal life within them."
      }
    },
