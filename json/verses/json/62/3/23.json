    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其实上帝的命令是：我们要信祂儿子耶稣基督的名，遵照祂的命令彼此相爱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神的命令，就是要我們信他的兒子耶穌基督的名，並且照著他的吩咐彼此相愛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神的命令就是叫我們信他兒子耶穌基督的名，且照他所賜給我們的命令彼此相愛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is His commandment, that we believe in the name of His Son Jesus Christ, and love one another, just as He commanded us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And this is his command: to believe in the name of his Son, Jesus Christ, and to love one another as he commanded us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And this is his commandment: We must believe in the name of his Son, Jesus Christ, and love one another, just as he commanded us."
      }
    },
