    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "遵行上帝命令的人就住在上帝里面，上帝也住在他里面。我们借着上帝赐给我们的圣灵知道：祂住在我们里面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是遵守　神命令的，就住在　神裡面，　神也住在他裡面。憑著　神所賜給我們的聖靈，我們就可以知道　神住在我們裡面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "遵守神命令的，就住在神裏面，神也住在他裏面。我們所以知道神住在我們裏面是因他所賜給我們的聖靈。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The one who keeps His commandments abides in Him, and He in him. We know by this that He abides in us, by the Spirit whom He has given us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The one who keeps God’s commands lives in him, and he in them. And this is how we know that he lives in us: We know it by the Spirit he gave us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who obey God’s commandments remain in fellowship with him, and he with them. And we know he lives in us because the Spirit he gave us lives in us."
      }
    },
