    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶稣为我们舍命，我们由此便知道什么是爱，我们也应当为弟兄姊妹舍命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主為我們捨命，這樣，我們就知道甚麼是愛；我們也應當為弟兄捨命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主為我們捨命，我們從此就知道何為愛；我們也當為弟兄捨命。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We know love by this, that He laid down His life for us; and we ought to lay down our lives for the brethren."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is how we know what love is: Jesus Christ laid down his life for us. And we ought to lay down our lives for our brothers and sisters."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We know what real love is because Jesus gave up his life for us. So we also ought to give up our lives for our brothers and sisters."
      }
    },
