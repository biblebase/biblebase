    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从上帝生的人不会继续犯罪，因为他里面有上帝的生命。他不能犯罪，因为他是从上帝生的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是從　神生的，就不犯罪，因為　神的生命在他裡面；他也不能犯罪，因為他是從　神生的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡從神生的，就不犯罪，因神的道存在他心裏；他也不能犯罪，因為他是由神生的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No one who is born of God practices sin, because His seed abides in him; and he cannot sin, because he is born of God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one who is born of God will continue to sin, because God’s seed remains in them; they cannot go on sinning, because they have been born of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who have been born into God’s family do not make a practice of sinning, because God’s life is in them. So they can’t keep on sinning, because they are children of God."
      }
    },
