    "sermons": [
      {
        "id": "home_sermons_retreat-2019-06-21",
        "title": "愛—生命的驗証",
        "date": "2019-06-21",
        "preacher": "陳愛光牧師",
        "verses": "約翰一書 2:7-11,15-17, 3:11-18, 4:7-21",
        "audio": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-21.mp3",
        "slides": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-21.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果一个丰衣足食的人看见贫穷的弟兄姊妹，却毫无怜悯之心，怎能说他爱上帝呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡有世上財物的，看見弟兄窮乏，卻硬著心腸不理，他怎能說他心裡有　神的愛呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡有世上財物的，看見弟兄窮乏，卻塞住憐恤的心，愛神的心怎能存在他裏面呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But whoever has the world’s goods, and sees his brother in need and closes his heart against him, how does the love of God abide in him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If anyone has material possessions and sees a brother or sister in need but has no pity on them, how can the love of God be in that person?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If someone has enough money to live well and sees a brother or sister in need but shows no compassion—how can God’s love be in that person?"
      }
    },
