    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "住在祂里面的人不会一直犯罪；一直犯罪的人既没见过祂也不认识祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是住在他裡面的，就不犯罪；凡犯罪的，是未曾見過他，也不認識他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡住在他裏面的，就不犯罪；凡犯罪的，是未曾看見他，也未曾認識他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No one who abides in Him sins; no one who sins has seen Him or knows Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one who lives in him keeps on sinning. No one who continues to sin has either seen him or known him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who continues to live in him will not sin. But anyone who keeps on sinning does not know him or understand who he is."
      }
    },
