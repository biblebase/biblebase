    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，我们无论向上帝祈求什么，都必得到，因为我们遵守祂的命令，做祂喜悦的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡我們所求的，就必從他得著，因為我們遵守他的命令，作他所喜悅的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且我們一切所求的，就從他得着；因為我們遵守他的命令，行他所喜悅的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and whatever we ask we receive from Him, because we keep His commandments and do the things that are pleasing in His sight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and receive from him anything we ask, because we keep his commands and do what pleases him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And we will receive from him whatever we ask because we obey him and do the things that please him."
      }
    },
