    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果我们说与祂相交，却仍过着黑暗的生活，就是撒谎，没有遵行真理。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們若說自己與他彼此相通，卻行在黑暗裡，就是說謊話，不實行真理了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們若說是與神相交，卻仍在黑暗裏行，就是說謊話，不行真理了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If we say that we have fellowship with Him and yet walk in the darkness, we lie and do not practice the truth;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If we claim to have fellowship with him and yet walk in the darkness, we lie and do not live out the truth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So we are lying if we say we have fellowship with God but go on living in spiritual darkness; we are not practicing the truth."
      }
    },
