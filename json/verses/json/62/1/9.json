    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果我们承认自己的罪，上帝是信实公义的，必赦免我们的罪，洗净我们一切的不义。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們若承認自己的罪，　神是信實的、公義的，必定赦免我們的罪，潔淨我們脫離一切不義。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們若認自己的罪，神是信實的，是公義的，必要赦免我們的罪，洗淨我們一切的不義。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If we confess our sins, He is faithful and righteous to forgive us our sins and to cleanse us from all unrighteousness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If we confess our sins, he is faithful and just and will forgive us our sins and purify us from all unrighteousness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if we confess our sins to him, he is faithful and just to forgive us our sins and to cleanse us from all wickedness."
      }
