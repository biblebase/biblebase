    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们从耶稣那里听见、现在传给你们的信息就是：上帝是光，在祂里面毫无黑暗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神是光，在他裡面毫無黑暗；這就是我們從他那裡聽見，現在傳給你們的信息。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神就是光，在他毫無黑暗。這是我們從主所聽見、又報給你們的信息。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is the message we have heard from Him and announce to you, that God is Light, and in Him there is no darkness at all."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is the message we have heard from him and declare to you: God is light; in him there is no darkness at all."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is the message we heard from Jesus and now declare to you: God is light, and there is no darkness in him at all."
      }
    },
