    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们恳切地求我们准许他们与有需要的圣徒分享上帝的恩典。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "再三懇求我們，准許他們在供應聖徒的事上有分。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "再三地求我們，准他們在這供給聖徒的恩情上有分；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "begging us with much urging for the favor of participation in the support of the saints,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "they urgently pleaded with us for the privilege of sharing in this service to the Lord’s people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They begged us again and again for the privilege of sharing in the gift for the believers in Jerusalem."
      }
    },
