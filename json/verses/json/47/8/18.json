    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们还要派一位弟兄和他同去，这位弟兄在传福音的事上得到各地教会的赞许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們還派了一位弟兄和他同去，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們還打發一位兄弟和他同去，這人在福音上得了眾教會的稱讚。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We have sent along with him the brother whose fame in the things of the gospel has spread through all the churches;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And we are sending along with him the brother who is praised by all the churches for his service to the gospel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We are also sending another brother with Titus. All the churches praise him as a preacher of the Good News."
      }
    },
