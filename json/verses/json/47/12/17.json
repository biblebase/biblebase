    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "请问，在我派到你们那里的人当中，我借着谁占过你们的便宜呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我派到你們那裡去的人，我藉著誰佔過你們的便宜？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我所差到你們那裏去的人，我藉着他們一個人佔過你們的便宜嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Certainly I have not taken advantage of you through any of those whom I have sent to you, have I?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Did I exploit you through any of the men I sent to you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But how? Did any of the men I sent to you take advantage of you?"
      }
    },
