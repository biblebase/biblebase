    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为了你们的灵魂，我甘愿付出，包括自己的生命。难道我越爱你们，就越得不到你们的爱吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於我，我甘心樂意為你們的靈魂付上一切，鞠躬盡瘁。難道我越愛你們，就越得不到你們的愛嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我也甘心樂意為你們的靈魂費財費力。難道我越發愛你們，就越發少得你們的愛嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will most gladly spend and be expended for your souls. If I love you more, am I to be loved less?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I will very gladly spend for you everything I have and expend myself as well. If I love you more, will you love me less?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will gladly spend myself and all I have for you, even though it seems that the more I love you, the less you love me."
      }
    },
