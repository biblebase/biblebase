    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我和西拉、提摩太在你们当中所传扬的那位上帝的儿子耶稣基督绝不会忽是忽非，在祂只有“是”。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為我、西拉和提摩太，在你們中間所宣揚的耶穌基督，　神的兒子，並不是“是”而又“非”的，在他總是“是”的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為我和西拉並提摩太，在你們中間所傳神的兒子耶穌基督，總沒有是而又非的，在他只有一是。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Son of God, Christ Jesus, who was preached among you by us—by me and Silvanus and Timothy—was not yes and no, but is yes in Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the Son of God, Jesus Christ, who was preached among you by us—by me and Silas and Timothy—was not “Yes” and “No,” but in him it has always been “Yes.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For Jesus Christ, the Son of God, does not waver between “Yes” and “No.” He is the one whom Silas, Timothy, and I preached to you, and as God’s ultimate “Yes,” he always does what he says."
      }
    },
