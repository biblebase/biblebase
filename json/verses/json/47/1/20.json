    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为上帝的一切应许在基督里都是确实的，所以我们也是借着基督说“阿们”，将荣耀归于上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為　神的一切應許，在基督裡都是“是”的，為此我們藉著他說“阿們”，使榮耀歸於　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神的應許，不論有多少，在基督都是是的。所以藉着他也都是實在的，叫神因我們得榮耀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For as many as are the promises of God, in Him they are yes; therefore also through Him is our Amen to the glory of God through us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For no matter how many promises God has made, they are “Yes” in Christ. And so through him the “Amen” is spoken by us to the glory of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For all of God’s promises have been fulfilled in Christ with a resounding “Yes!” And through Christ, our “Amen” (which means “Yes”) ascends to God for his glory."
      }
    },
