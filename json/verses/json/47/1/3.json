    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们主耶稣基督的父上帝当受赞美！祂是仁慈的父和赐一切安慰的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們主耶穌基督的父　神是應當稱頌的。他是滿有憐憫的父，賜各樣安慰的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願頌讚歸與我們的主耶穌基督的父神，就是發慈悲的父，賜各樣安慰的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Blessed be the God and Father of our Lord Jesus Christ, the Father of mercies and God of all comfort,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Praise be to the God and Father of our Lord Jesus Christ, the Father of compassion and the God of all comfort,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All praise to God, the Father of our Lord Jesus Christ. God is our merciful Father and the source of all comfort."
      }
    },
