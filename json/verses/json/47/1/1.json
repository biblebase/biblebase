    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是奉上帝旨意做基督耶稣使徒的保罗，与提摩太弟兄写信给在哥林多的上帝的教会以及亚该亚境内所有的圣徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "奉　神旨意作基督耶穌使徒的保羅，和提摩太弟兄，寫信給在哥林多　神的教會，和全亞該亞所有的聖徒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "奉神旨意作基督耶穌使徒的保羅和兄弟提摩太，寫信給在哥林多神的教會，並亞該亞遍處的眾聖徒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Paul, an apostle of Christ Jesus by the will of God, and Timothy our brother, To the church of God which is at Corinth with all the saints who are throughout Achaia:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Paul, an apostle of Christ Jesus by the will of God, and Timothy our brother, To the church of God in Corinth, together with all his holy people throughout Achaia:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This letter is from Paul, chosen by the will of God to be an apostle of Christ Jesus, and from our brother Timothy. I am writing to God’s church in Corinth and to all of his holy people throughout Greece."
      }
    },
