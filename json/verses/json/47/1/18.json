    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我在信实的上帝面前保证：我们对你们说的话绝不会忽是忽非！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神是信實的，我們向你們所傳的道並不是“是”而又“非”的，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我指着信實的神說，我們向你們所傳的道，並沒有是而又非的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as God is faithful, our word to you is not yes and no."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But as surely as God is faithful, our message to you is not “Yes” and “No.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As surely as God is faithful, our word to you does not waver between “Yes” and “No.”"
      }
    },
