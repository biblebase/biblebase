    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "是上帝使我们和你们一同在基督里信心坚固。祂差遣了我们，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那在基督裡堅定我們和你們，又膏抹我們的，就是　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那在基督裏堅固我們和你們，並且膏我們的就是神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now He who establishes us with you in Christ and anointed us is God,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now it is God who makes both us and you stand firm in Christ. He anointed us,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is God who enables us, along with you, to stand firm for Christ. He has commissioned us,"
      }
    },
