    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那渐渐消逝的尚且有荣耀，这永远长存的更是荣耀无比。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果那短暫的有榮光，那長存的就更有榮光了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若那廢掉的有榮光，這長存的就更有榮光了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if that which fades away was with glory, much more that which remains is in glory."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And if what was transitory came with glory, how much greater is the glory of that which lasts!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So if the old way, which has been replaced, was glorious, how much more glorious is the new, which remains forever!"
      }
    },
