    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那定人罪的事工尚且有荣耀，这使人被称为义人的事工岂不更有荣耀吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果那定人有罪的工作尚且有榮光，那使人稱義的工作就更有榮光了！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若是定罪的職事有榮光，那稱義的職事榮光就越發大了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if the ministry of condemnation has glory, much more does the ministry of righteousness abound in glory."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If the ministry that brought condemnation was glorious, how much more glorious is the ministry that brings righteousness!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If the old way, which brings condemnation, was glorious, how much more glorious is the new way, which makes us right with God!"
      }
    },
