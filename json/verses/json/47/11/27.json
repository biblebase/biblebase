    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我劳碌困苦，不得安眠，又饥又渴，挨饿受冻，赤身露体。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "勞碌辛苦，多次不得睡覺，又飢又渴，多次缺糧，赤身挨冷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "受勞碌、受困苦，多次不得睡，又飢又渴，多次不得食，受寒冷，赤身露體。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have been in labor and hardship, through many sleepless nights, in hunger and thirst, often without food, in cold and exposure."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have labored and toiled and have often gone without sleep; I have known hunger and thirst and have often gone without food; I have been cold and naked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have worked hard and long, enduring many sleepless nights. I have been hungry and thirsty and have often gone without food. I have shivered in the cold, without enough clothing to keep me warm."
      }
    },
