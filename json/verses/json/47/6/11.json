    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "哥林多人啊！我们对你们推心置腹，开诚布公，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "哥林多人哪！我們對你們，口是坦率的，心是寬宏的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "哥林多人哪，我們向你們，口是張開的，心是寬宏的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Our mouth has spoken freely to you, O Corinthians, our heart is opened wide."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We have spoken freely to you, Corinthians, and opened wide our hearts to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Oh, dear Corinthian friends! We have spoken honestly with you, and our hearts are open to you."
      }
