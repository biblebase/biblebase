    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为了避免有人毁谤我们的职分，我们凡事尽量不妨碍别人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們凡事都沒有妨礙人，不讓這職分受到毀謗，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們凡事都不叫人有妨礙，免得這職分被人毀謗；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "giving no cause for offense in anything, so that the ministry will not be discredited,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We put no stumbling block in anyone’s path, so that our ministry will not be discredited."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We live in such a way that no one will stumble because of us, and no one will find fault with our ministry."
      }
    },
