    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然而，即使我们所传的福音被遮蔽，也是在那些灭亡的人当中被遮蔽。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我們的福音被蒙蔽了，那是對滅亡的人才蒙蔽的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "如果我們的福音蒙蔽，就是蒙蔽在滅亡的人身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And even if our gospel is veiled, it is veiled to those who are perishing,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And even if our gospel is veiled, it is veiled to those who are perishing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If the Good News we preach is hidden behind a veil, it is hidden only from people who are perishing."
      }
