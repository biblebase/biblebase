    "sermons": [
      {
        "id": "home_sermons_sermon-2012-08-12",
        "title": "得勝患難不喪膽!",
        "date": "2012-08-12",
        "preacher": "馬寧姐妹",
        "verses": "哥林多後書 4:7-18",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-08-12.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-08-12.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，我们不气馁。身体虽然日渐衰残，内在的生命却日日更新。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，我們並不沮喪，我們外面的人雖然漸漸朽壞，但裡面的人卻日日更新，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，我們不喪膽。外體雖然毀壞，內心卻一天新似一天。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore we do not lose heart, but though our outer man is decaying, yet our inner man is being renewed day by day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore we do not lose heart. Though outwardly we are wasting away, yet inwardly we are being renewed day by day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That is why we never give up. Though our bodies are dying, our spirits are being renewed every day."
      }
    },
