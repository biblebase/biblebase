    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝使无罪的基督担当我们的罪，使我们可以借着基督成为被上帝接纳的义人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神使那無罪的替我們成為有罪的，使我們在他裡面成為　神的義。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神使那無罪的，替我們成為罪，好叫我們在他裏面成為神的義。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He made Him who knew no sin to be sin on our behalf, so that we might become the righteousness of God in Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God made him who had no sin to be sin for us, so that in him we might become the righteousness of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For God made Christ, who never sinned, to be the offering for our sin, so that we could be made right with God through Christ."
      }
    },
