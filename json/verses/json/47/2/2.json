    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果我使你们忧伤，那么除了因我而忧伤的你们，还有谁能使我喜乐呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我使你們憂愁，除了那因我而憂愁的人以外，誰能使我快樂呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "倘若我叫你們憂愁，除了我叫那憂愁的人以外，誰能叫我快樂呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if I cause you sorrow, who then makes me glad but the one whom I made sorrowful?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For if I grieve you, who is left to make me glad but you whom I have grieved?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For if I cause you grief, who will make me glad? Certainly not someone I have grieved."
      }
    },
