    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，我劝你们现在要像我一样，因为我已经像你们一样。你们向来没有亏待过我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，我請求你們要像我一樣，因為我也像你們一樣。你們並沒有虧負過我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，我勸你們要像我一樣，因為我也像你們一樣。你們一點沒有虧負我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I beg of you, brethren, become as I am, for I also have become as you are. You have done me no wrong;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I plead with you, brothers and sisters, become like me, for I became like you. You did me no wrong."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Dear brothers and sisters, I plead with you to live as I do in freedom from these things, for I have become like you Gentiles—free from those laws. You did not mistreat me when I first preached to you."
      }
    },
