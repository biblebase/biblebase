    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们这些甘受律法约束的人啊！告诉我，你们没听过律法怎么说吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們這願意在律法之下的人哪！請告訴我，你們沒有聽過律法嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們這願意在律法以下的人，請告訴我，你們豈沒有聽見律法嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Tell me, you who want to be under law, do you not listen to the law?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Tell me, you who want to be under the law, are you not aware of what the law says?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Tell me, you who want to live under the law, do you know what the law actually says?"
      }
