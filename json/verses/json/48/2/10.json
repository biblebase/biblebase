    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们只要求我们照顾那些贫困的人，这正是我一向热衷的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只是要我們記念窮人，這本來也是我一向熱心作的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是願意我們記念窮人；這也是我本來熱心去行的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They only asked us to remember the poor—the very thing I also was eager to do."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All they asked was that we should continue to remember the poor, the very thing I had been eager to do all along."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their only suggestion was that we keep on helping the poor, which I have always been eager to do."
      }
    },
