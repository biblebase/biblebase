    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "事实上，我因无法满足律法的要求而向律法死了，不再受它的束缚，使我可以为上帝而活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我藉著律法已經向律法死了，使我可以向　神活著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因律法，就向律法死了，叫我可以向神活着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For through the Law I died to the Law, so that I might live to God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“For through the law I died to the law so that I might live for God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For when I tried to keep the law, it condemned me. So I died to the law—I stopped trying to meet all its requirements—so that I might live for God."
      }
