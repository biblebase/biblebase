    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，我告诉你们，我传的福音不是出于人的意思，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，我要你們知道，我所傳的福音，並不是照著人的意思，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，我告訴你們，我素來所傳的福音不是出於人的意思。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I would have you know, brethren, that the gospel which was preached by me is not according to man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I want you to know, brothers and sisters, that the gospel I preached is not of human origin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Dear brothers and sisters, I want you to understand that the gospel message I preach is not based on mere human reasoning."
      }
    },
