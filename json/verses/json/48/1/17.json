    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "也没有上耶路撒冷去见那些比我先做使徒的人，而是立刻去了阿拉伯，然后回到大马士革。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也沒有上耶路撒冷見那些比我先作使徒的，卻去了阿拉伯，然後再回到大馬士革。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也沒有上耶路撒冷去見那些比我先作使徒的，惟獨往阿拉伯去，後又回到大馬士革。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "nor did I go up to Jerusalem to those who were apostles before me; but I went away to Arabia, and returned once more to Damascus."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I did not go up to Jerusalem to see those who were apostles before I was, but I went into Arabia. Later I returned to Damascus."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Nor did I go up to Jerusalem to consult with those who were apostles before I was. Instead, I went away into Arabia, and later I returned to the city of Damascus."
      }
    },
