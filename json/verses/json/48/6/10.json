    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们要把握机会为众人做善事，对待信徒更要如此。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，我們一有機會，就應該對眾人行善，對信徒更要這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，有了機會就當向眾人行善，向信徒一家的人更當這樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So then, while we have opportunity, let us do good to all people, and especially to those who are of the household of the faith."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore, as we have opportunity, let us do good to all people, especially to those who belong to the family of believers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Therefore, whenever we have the opportunity, we should do good to everyone—especially to those in the family of faith."
      }
