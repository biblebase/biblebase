    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "律法却不是以信心为本，而是说：“遵行的人必存活。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "律法本來不是出於信，而是說：“遵行這些事的人，就必因這些事而活。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "律法原不本乎信，只說：「行這些事的，就必因此活着。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "However, the Law is not of faith; on the contrary, “He who practices them shall live by them.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The law is not based on faith; on the contrary, it says, “The person who does these things will live by them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This way of faith is very different from the way of law, which says, “It is through obeying the law that a person has life.”"
      }
    },
