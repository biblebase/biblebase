      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們既靠聖靈入門，如今還靠肉身成全嗎？你們是這樣的無知嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Are you so foolish? Having begun by the Spirit, are you now being perfected by the flesh?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Are you so foolish? After beginning by means of the Spirit, are you now trying to finish by means of the flesh?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How foolish can you be? After starting your new lives in the Spirit, why are you now trying to become perfect by your own human effort?"
      }
    },
