    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无知的加拉太人啊！谁又迷惑了你们呢？耶稣基督被钉在十字架上的事早就像一幅画一样展现在你们眼前了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "無知的加拉太人哪！耶穌基督釘十字架，已經活現在你們眼前，誰又迷惑了你們呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無知的加拉太人哪，耶穌基督釘十字架，已經活畫在你們眼前，誰又迷惑了你們呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You foolish Galatians, who has bewitched you, before whose eyes Jesus Christ was publicly portrayed as crucified?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You foolish Galatians! Who has bewitched you? Before your very eyes Jesus Christ was clearly portrayed as crucified."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Oh, foolish Galatians! Who has cast an evil spell on you? For the meaning of Jesus Christ’s death was made as clear to you as if you had seen a picture of his death on the cross."
      }
    },
