    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亲爱的弟兄姊妹，要谨记主耶稣基督的使徒从前给你们的警告。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但你們呢，親愛的，你們要記住我們主耶穌基督的使徒從前所說的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "親愛的弟兄啊，你們要記念我們主耶穌基督之使徒從前所說的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But you, beloved, ought to remember the words that were spoken beforehand by the apostles of our Lord Jesus Christ,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But, dear friends, remember what the apostles of our Lord Jesus Christ foretold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you, my dear friends, must remember what the apostles of our Lord Jesus Christ predicted."
      }
