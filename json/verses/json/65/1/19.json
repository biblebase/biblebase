    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这些人制造分裂，血气用事，心中没有圣灵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些人分黨結派，是屬血氣的，沒有聖靈。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這就是那些引人結黨、屬乎血氣、沒有聖靈的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These are the ones who cause divisions, worldly-minded, devoid of the Spirit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These are the people who divide you, who follow mere natural instincts and do not have the Spirit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These people are the ones who are creating divisions among you. They follow their natural instincts because they do not have God’s Spirit in them."
      }
    },
