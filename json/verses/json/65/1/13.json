    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们是海中的狂涛，翻动着自己可耻的泡沫，是流荡的星星，有幽幽黑暗永远留给他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "是海中的狂浪，濺起了自己可恥的泡沫；是流蕩的星，有漆黑的幽暗永遠為他們存留。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "是海裏的狂浪，湧出自己可恥的沫子來；是流蕩的星，有墨黑的幽暗為他們永遠存留。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "wild waves of the sea, casting up their own shame like foam; wandering stars, for whom the black darkness has been reserved forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are wild waves of the sea, foaming up their shame; wandering stars, for whom blackest darkness has been reserved forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They are like wild waves of the sea, churning up the foam of their shameful deeds. They are like wandering stars, doomed forever to blackest darkness."
      }
    },
