    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有些人，你们要将他们从火中抢救出来；还有些人，你们要怀着畏惧的心怜悯他们，甚至要厌恶被他们的邪情私欲玷污的衣服。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有些人你們要拯救，把他們從火中搶救出來；又有些人你們要戰戰兢兢地憐憫他們，連染上情慾污漬的衣服也應當憎惡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有些人你們要從火中搶出來，搭救他們；有些人你們要存懼怕的心憐憫他們，連那被情慾沾染的衣服也當厭惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "save others, snatching them out of the fire; and on some have mercy with fear, hating even the garment polluted by the flesh."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "save others by snatching them from the fire; to others show mercy, mixed with fear—hating even the clothing stained by corrupted flesh."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rescue others by snatching them from the flames of judgment. Show mercy to still others, but do so with great caution, hating the sins that contaminate their lives."
      }
    },
