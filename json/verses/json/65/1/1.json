    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是耶稣基督的奴仆、雅各的兄弟犹大，现在写信给蒙父上帝呼召、眷爱、被耶稣基督看顾的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌基督的僕人，雅各的兄弟猶大，寫信給那些被召的人，就是在父　神裡蒙愛，並且為了耶穌基督而蒙保守的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌基督的僕人，雅各的弟兄猶大，寫信給那被召、在父神裏蒙愛、為耶穌基督保守的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jude, a bond-servant of Jesus Christ, and brother of James, To those who are the called, beloved in God the Father, and kept for Jesus Christ:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jude, a servant of Jesus Christ and a brother of James, To those who have been called, who are loved in God the Father and kept for Jesus Christ:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This letter is from Jude, a slave of Jesus Christ and a brother of James. I am writing to all who have been called by God the Father, who loves you and keeps you safe in the care of Jesus Christ."
      }
    },
