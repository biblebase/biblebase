    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人只烧毁了夏琐，没有烧建在山丘上的其他城池。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於那些立在土丘上的城，除了夏瑣以外，以色列人都沒有焚燒；約書亞只燒了夏瑣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於造在山岡上的城，除了夏瑣以外，以色列人都沒有焚燒。約書亞只將夏瑣焚燒了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "However, Israel did not burn any cities that stood on their mounds, except Hazor alone, which Joshua burned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet Israel did not burn any of the cities built on their mounds—except Hazor, which Joshua burned."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Israelites did not burn any of the towns built on mounds except Hazor, which Joshua burned."
      }
    },
