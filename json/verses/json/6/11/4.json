    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这些王便率领自己的军队，带着许多马匹和战车倾巢出动，人数多如海边的沙。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些王與他們所有的軍隊都一同出來，人數眾多，好像海邊的沙，還有很多馬匹和車輛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些王和他們的眾軍都出來，人數多如海邊的沙，並有許多馬匹車輛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They came out, they and all their armies with them, as many people as the sand that is on the seashore, with very many horses and chariots."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They came out with all their troops and a large number of horses and chariots—a huge army, as numerous as the sand on the seashore."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All these kings came out to fight. Their combined armies formed a vast horde. And with all their horses and chariots, they covered the landscape like the sand on the seashore."
      }
    },
