    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从那里经路斯，即伯特利南面山坡到下伯·和仑南面山区的亚他绿·亚达。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又從那裡向南經過路斯，路斯就是伯特利，再下到亞他綠．亞達，靠近下伯．和崙南邊的山地；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從那裏往南接連到路斯，貼近路斯（路斯就是伯特利），又下到亞他綠‧亞達，靠近下伯‧和崙南邊的山；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From there the border continued to Luz, to the side of Luz (that is, Bethel) southward; and the border went down to Ataroth-addar, near the hill which lies on the south of lower Beth-horon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From there it crossed to the south slope of Luz (that is, Bethel) and went down to Ataroth Addar on the hill south of Lower Beth Horon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From there the boundary went south to Luz (that is, Bethel) and proceeded down to Ataroth-addar on the hill that lies south of Lower Beth-horon."
      }
    },
