    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们把这七份土地绘成地图交给我，我要在我们的上帝耶和华面前为你们抽签。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要把地劃分七份，然後把圖樣拿給我；我要在這裡，在耶和華我們的　神面前，為你們抽籤。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要將地分做七分，寫明了拿到我這裏來。我要在耶和華－我們神面前，為你們拈鬮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall describe the land in seven divisions, and bring the description here to me. I will cast lots for you here before the Lord our God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After you have written descriptions of the seven parts of the land, bring them here to me and I will cast lots for you in the presence of the Lord our God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when you record the seven divisions of the land and bring them to me, I will cast sacred lots in the presence of the Lord our God to assign land to each tribe."
      }
    },
