    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人都安然无恙地回到约书亚驻扎的玛基大营。再也没有人敢威胁以色列人了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾以色列人都平平安安回到瑪基大營中約書亞那裡。沒有人敢說話攻擊以色列人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾百姓就安然回瑪基大營中，到約書亞那裏。沒有一人敢向以色列人饒舌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that all the people returned to the camp to Joshua at Makkedah in peace. No one uttered a word against any of the sons of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The whole army then returned safely to Joshua in the camp at Makkedah, and no one uttered a word against the Israelites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Israelites returned safely to Joshua in the camp at Makkedah. After that, no one dared to speak even a word against Israel."
      }
    },
