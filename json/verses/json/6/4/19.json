    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那天是一月十日。以色列人从约旦河上来之后，就在耶利哥东边的吉甲扎营。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正月初十，人民從約旦河上來以後，就在吉甲，在耶利哥的東邊安營。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正月初十日，百姓從約旦河裏上來，就在吉甲，在耶利哥的東邊安營。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the people came up from the Jordan on the tenth of the first month and camped at Gilgal on the eastern edge of Jericho."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the tenth day of the first month the people went up from the Jordan and camped at Gilgal on the eastern border of Jericho."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people crossed the Jordan on the tenth day of the first month. Then they camped at Gilgal, just east of Jericho."
      }
    },
