    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那一天，耶和华使约书亚在以色列众人面前受尊重，他像摩西一样终生受民众敬畏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那一天耶和華使約書亞在以色列眾人眼中成為尊大；在他一生的日子，人民敬畏他，好像敬畏摩西一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當那日，耶和華使約書亞在以色列眾人眼前尊大。在他平生的日子，百姓敬畏他，像從前敬畏摩西一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On that day the Lord exalted Joshua in the sight of all Israel; so that they revered him, just as they had revered Moses all the days of his life."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "That day the Lord exalted Joshua in the sight of all Israel; and they stood in awe of him all the days of his life, just as they had stood in awe of Moses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That day the Lord made Joshua a great leader in the eyes of all the Israelites, and for the rest of his life they revered him as much as they had revered Moses."
      }
    },
