    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我四十岁那年，耶和华的仆人摩西从加低斯·巴尼亚派我来侦察这地方，我如实禀告他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的僕人摩西從加低斯．巴尼亞派我去窺探這地的時候，我正四十歲；我照著我心裡所想的向他報告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的僕人摩西從加低斯‧巴尼亞打發我窺探這地，那時我正四十歲；我按着心意回報他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I was forty years old when Moses the servant of the Lord sent me from Kadesh-barnea to spy out the land, and I brought word back to him as it was in my heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I was forty years old when Moses the servant of the Lord sent me from Kadesh Barnea to explore the land. And I brought him back a report according to my convictions,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I was forty years old when Moses, the servant of the Lord, sent me from Kadesh-barnea to explore the land of Canaan. I returned and gave an honest report,"
      }
    },
