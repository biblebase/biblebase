    "sermons": [
      {
        "id": "home_sermons_sermon-2012-06-03",
        "title": "一生成功的信徒",
        "date": "2012-06-03",
        "preacher": "鍾定基牧師",
        "verses": "約書亞記 14:6-15",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-06-03.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-06-03.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "仍像当年摩西派我出去时那样身强力壮，跟当年一样能征善战。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "今日我還是強壯，像摩西派我去的那天一樣；無論是作戰，或是出入，那時我的力量怎樣，現在我的力量還是怎樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我還是強壯，像摩西打發我去的那天一樣；無論是爭戰，是出入，我的力量那時如何，現在還是如何。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am still as strong today as I was in the day Moses sent me; as my strength was then, so my strength is now, for war and for going out and coming in."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am still as strong today as the day Moses sent me out; I’m just as vigorous to go out to battle now as I was then."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am as strong now as I was when Moses sent me on that journey, and I can still travel and fight as well as I could then."
      }
    },
