    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人过河的时候，抬耶和华约柜的祭司站立在河中间的干地上，直到所有人都从干地上过了约旦河。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "抬耶和華約櫃的祭司在約旦河中間的乾地上站住，等以色列眾人都在乾地上走過去，直到全國的人都完全走過了約旦河為止。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "擡耶和華約櫃的祭司在約旦河中的乾地上站定，以色列眾人都從乾地上過去，直到國民盡都過了約旦河。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the priests who carried the ark of the covenant of the Lord stood firm on dry ground in the middle of the Jordan while all Israel crossed on dry ground, until all the nation had finished crossing the Jordan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priests who carried the ark of the covenant of the Lord stopped in the middle of the Jordan and stood on dry ground, while all Israel passed by until the whole nation had completed the crossing on dry ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Meanwhile, the priests who were carrying the Ark of the Lord’s Covenant stood on dry ground in the middle of the riverbed as the people passed by. They waited there until the whole nation of Israel had crossed the Jordan on dry ground."
      }
    },
