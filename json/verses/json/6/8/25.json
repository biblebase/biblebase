      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当天被杀的艾城人男男女女共有一万二千人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那一天被殺的人，連男帶女，共有一萬二千人，就是艾城所有的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當日殺斃的人，連男帶女共有一萬二千，就是艾城所有的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All who fell that day, both men and women, were 12,000—all the people of Ai."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Twelve thousand men and women fell that day—all the people of Ai."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the entire population of Ai, including men and women, was wiped out that day—12,000 in all."
      }
    },
