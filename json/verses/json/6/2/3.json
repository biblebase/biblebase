    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利哥王便派人到喇合那里，对她说：“把来到你家里的人交出来，因为他们是来刺探我们的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利哥王派人到喇合那裡，說：“把那些來到你這裡，進了你家中的人帶出來，因為他們來是要窺探全地的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶利哥王打發人去見喇合說：「那來到你這裏、進了你家的人要交出來，因為他們來窺探全地。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the king of Jericho sent word to Rahab, saying, “Bring out the men who have come to you, who have entered your house, for they have come to search out all the land.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the king of Jericho sent this message to Rahab: “Bring out the men who came to you and entered your house, because they have come to spy out the whole land.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the king of Jericho sent orders to Rahab: “Bring out the men who have come into your house, for they have come here to spy out the whole land.”"
      }
    },
