    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "喇合答道：“一言为定！”于是她送走他们，把朱红色的绳子系在窗户上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那女人回答：“照你們的話，就這樣吧。”於是打發他們二人走了。他們走了以後，她就把朱紅色線繫在窗戶上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "女人說：「照你們的話行吧！」於是打發他們去了，又把朱紅線繩繫在窗戶上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She said, “According to your words, so be it.” So she sent them away, and they departed; and she tied the scarlet cord in the window."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Agreed,” she replied. “Let it be as you say.” So she sent them away, and they departed. And she tied the scarlet cord in the window."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I accept your terms,” she replied. And she sent them on their way, leaving the scarlet rope hanging from the window."
      }
    },
