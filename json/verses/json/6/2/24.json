    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又说：“耶和华确实将那整片土地交给我们了，那里的居民听到我们的消息，都吓得胆战心惊。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們又對約書亞說：“耶和華真的把那地全交在我們手裡了；那地所有的居民都在我們面前膽戰心驚。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又對約書亞說：「耶和華果然將那全地交在我們手中；那地的一切居民在我們面前心都消化了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said to Joshua, “Surely the Lord has given all the land into our hands; moreover, all the inhabitants of the land have melted away before us.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They said to Joshua, “The Lord has surely given the whole land into our hands; all the people are melting in fear because of us.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The Lord has given us the whole land,” they said, “for all the people in the land are terrified of us.”"
      }
    },
