    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并对他们说：“你们往山上去，免得被追捕的人发现。你们要在那里躲三天，等追捕的人回城以后才可以走。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她對他們二人說：“你們要到山上去，免得追趕的人遇上你們；你們要在那裡躲藏三天，等追趕的人回城，然後你們才可以走你們的路。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她對他們說：「你們且往山上去，恐怕追趕的人碰見你們；要在那裏隱藏三天，等追趕的人回來，然後才可以走你們的路。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She said to them, “Go to the hill country, so that the pursuers will not happen upon you, and hide yourselves there for three days until the pursuers return. Then afterward you may go on your way.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She said to them, “Go to the hills so the pursuers will not find you. Hide yourselves there three days until they return, and then go on your way.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Escape to the hill country,” she told them. “Hide there for three days from the men searching for you. Then, when they have returned, you can go on your way.”"
      }
    },
