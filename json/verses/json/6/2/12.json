    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我既然恩待了你们，现在请你们凭耶和华向我起誓，你们也照样恩待我家，并且给我一个可靠的凭据，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在求你們指著耶和華向我起誓：我既然恩待了你們，你們也要恩待我的父家，並且給我一個確實的憑據，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在我既是恩待你們，求你們指着耶和華向我起誓，也要恩待我父家，並給我一個實在的證據，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now therefore, please swear to me by the Lord, since I have dealt kindly with you, that you also will deal kindly with my father’s household, and give me a pledge of truth,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Now then, please swear to me by the Lord that you will show kindness to my family, because I have shown kindness to you. Give me a sure sign"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Now swear to me by the Lord that you will be kind to me and my family since I have helped you. Give me some guarantee that"
      }
    },
