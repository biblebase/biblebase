    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "二人到山上躲藏了三天，等待追捕的人回去。追捕的人沿途搜索，毫无所获，便回去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "二人離去，上到山上，在那裡住了三天，等追趕的人回城去了；追趕的人一路搜尋他們，卻找不著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "二人到山上，在那裏住了三天，等着追趕的人回去了。追趕的人一路找他們，卻找不着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They departed and came to the hill country, and remained there for three days until the pursuers returned. Now the pursuers had sought them all along the road, but had not found them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they left, they went into the hills and stayed there three days, until the pursuers had searched all along the road and returned without finding them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The spies went up into the hill country and stayed there three days. The men who were chasing them searched everywhere along the road, but they finally returned without success."
      }
    },
