    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "押撒答道：“请你赐福给我，你既然把南地给了我，就求你也把水泉赐给我。”迦勒就把上泉和下泉都给了她。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她回答：“求你給我祝福；你既然把南地給了我，求你也把水泉給我。”她父親就把上泉和下泉都給了她。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她說：「求你賜福給我，你既將我安置在南地，求你也給我水泉。」她父親就把上泉下泉賜給她。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then she said, “Give me a blessing; since you have given me the land of the Negev, give me also springs of water.” So he gave her the upper springs and the lower springs."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She replied, “Do me a special favor. Since you have given me land in the Negev, give me also springs of water.” So Caleb gave her the upper and lower springs."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She said, “Give me another gift. You have already given me land in the Negev; now please give me springs of water, too.” So Caleb gave her the upper and lower springs."
      }
    },
