    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我引领你们的祖先离开埃及来到红海的时候，埃及人带领战车骑兵追了上来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我領你們的列祖離開埃及，他們去到海邊；埃及人帶著戰車和馬兵，追趕你們的列祖直到紅海。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我領你們列祖出埃及，他們就到了紅海；埃及人帶領車輛馬兵追趕你們列祖到紅海。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I brought your fathers out of Egypt, and you came to the sea; and Egypt pursued your fathers with chariots and horsemen to the Red Sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When I brought your people out of Egypt, you came to the sea, and the Egyptians pursued them with chariots and horsemen as far as the Red Sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when your ancestors arrived at the Red Sea, the Egyptians chased after you with chariots and charioteers."
      }
    },
