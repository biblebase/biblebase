    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "任何人违抗你的命令，不照你的吩咐去做，都要被处死。你要刚强勇敢！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "無論誰違背你的話，不聽從你吩咐他的一切命令，那人必要處死。你只管堅強勇敢。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無論甚麼人違背你的命令，不聽從你所吩咐他的一切話，就必治死他。你只要剛強壯膽！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Anyone who rebels against your command and does not obey your words in all that you command him, shall be put to death; only be strong and courageous.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever rebels against your word and does not obey it, whatever you may command them, will be put to death. Only be strong and courageous!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who rebels against your orders and does not obey your words and everything you command will be put to death. So be strong and courageous!”"
      }
    },
