    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要刚强勇敢，因为你要带领这些人占领我起誓赐给他们祖先的土地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要堅強勇敢，因為你必使這人民得著我曾經向他們列祖起誓，要賜給他們的那地作產業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你當剛強壯膽！因為你必使這百姓承受那地為業，就是我向他們列祖起誓應許賜給他們的地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be strong and courageous, for you shall give this people possession of the land which I swore to their fathers to give them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be strong and courageous, because you will lead these people to inherit the land I swore to their ancestors to give them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Be strong and courageous, for you are the one who will lead these people to possess all the land I swore to their ancestors I would give them."
      }
