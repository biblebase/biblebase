    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从埃及出来的男子都受过割礼，而后来在旷野出生的以色列人都没有受过割礼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為所有從埃及出來的人民都受過割禮；唯有從埃及出來以後，所有在曠野的路上出生的人民，都沒有受過割禮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為出來的眾民都受過割禮；惟獨出埃及以後、在曠野的路上所生的眾民都沒有受過割禮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For all the people who came out were circumcised, but all the people who were born in the wilderness along the way as they came out of Egypt had not been circumcised."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the people that came out had been circumcised, but all the people born in the wilderness during the journey from Egypt had not."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who left Egypt had all been circumcised, but none of those born after the Exodus, during the years in the wilderness, had been circumcised."
      }
    },
