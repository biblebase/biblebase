      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要把所有的金银和铜铁器皿分别出来献给耶和华，放进耶和华的库房里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可是，所有的金銀和銅鐵的器皿，都要歸耶和華為聖，存入耶和華的庫房中。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟有金子、銀子，和銅鐵的器皿都要歸耶和華為聖，必入耶和華的庫中。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But all the silver and gold and articles of bronze and iron are holy to the Lord; they shall go into the treasury of the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the silver and gold and the articles of bronze and iron are sacred to the Lord and must go into his treasury.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everything made from silver, gold, bronze, or iron is sacred to the Lord and must be brought into his treasury.”"
      }
    },
