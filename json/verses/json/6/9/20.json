    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们要容他们存活，免得我们因违背誓言而惹耶和华发怒。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們要這樣待他們，就是容他們存活，免得　神的忿怒因我們向他們所起的誓，臨到我們身上。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們要如此待他們，容他們活着，免得有忿怒因我們所起的誓臨到我們身上。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This we will do to them, even let them live, so that wrath will not be upon us for the oath which we swore to them.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what we will do to them: We will let them live, so that God’s wrath will not fall on us for breaking the oath we swore to them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what we must do. We must let them live, for divine anger would come upon us if we broke our oath."
      }
