    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“看啊，在耶和华那伟大而可畏的日子来临以前，我必差遣以利亚先知到你们那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪！在耶和華大而可畏的日子來到以先，我必差派以利亞先知到你們那裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「看哪，耶和華大而可畏之日未到以前，我必差遣先知以利亞到你們那裏去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, I am going to send you Elijah the prophet before the coming of the great and terrible day of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“See, I will send the prophet Elijah to you before that great and dreadful day of the Lord comes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Look, I am sending you the prophet Elijah before the great and dreadful day of the Lord arrives."
      }
    },
