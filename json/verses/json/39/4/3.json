    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到我所定的日子，你们必践踏恶人，他们必成为你们脚下的尘土。’这是万军之耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華說：“你們必踐踏惡人；在我施行作為的日子，他們必在你們腳掌之下成為塵土。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們必踐踏惡人；在我所定的日子，他們必如灰塵在你們腳掌之下。這是萬軍之耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will tread down the wicked, for they will be ashes under the soles of your feet on the day which I am preparing,” says the Lord of hosts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then you will trample on the wicked; they will be ashes under the soles of your feet on the day when I act,” says the Lord Almighty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On the day when I act, you will tread upon the wicked as if they were dust under your feet,” says the Lord of Heaven’s Armies."
      }
    },
