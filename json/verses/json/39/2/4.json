    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们便知道我这样告诫你们，是为了维持我与你们祖先利未所立的约。这是万军之耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華說：“這樣，你們就知道是我把這命令給你們，使我與利未所立的約得以存留。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們就知道我傳這誡命給你們，使我與利未所立的約可以常存。這是萬軍之耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then you will know that I have sent this commandment to you, that My covenant may continue with Levi,” says the Lord of hosts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And you will know that I have sent you this warning so that my covenant with Levi may continue,” says the Lord Almighty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then at last you will know it was I who sent you this warning so that my covenant with the Levites can continue,” says the Lord of Heaven’s Armies."
      }
    },
