    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然而，你们却偏离正道，你们的教导使许多人跌倒，破坏了我和利未所立的约。”这是万军之耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華說：“你們卻偏離了正路，使許多人在律法上跌倒；你們也破壞了我與利未所立的約。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們卻偏離正道，使許多人在律法上跌倒。你們廢棄我與利未所立的約。這是萬軍之耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for you, you have turned aside from the way; you have caused many to stumble by the instruction; you have corrupted the covenant of Levi,” says the Lord of hosts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you have turned from the way and by your teaching have caused many to stumble; you have violated the covenant with Levi,” says the Lord Almighty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you priests have left God’s paths. Your instructions have caused many to stumble into sin. You have corrupted the covenant I made with the Levites,” says the Lord of Heaven’s Armies."
      }
    },
