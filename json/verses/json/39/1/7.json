    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们把污秽的食物献在我的祭坛上，还说，‘我们在何事上玷污了你呢？’你们说耶和华的桌子是可藐视的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們把污穢的食物獻在我的壇上，你們竟說：‘我們怎樣污穢了你的壇呢？’因你們以為耶和華的桌子是可以藐視的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們將污穢的食物獻在我的壇上，且說：『我們在何事上污穢你呢？』因你們說，耶和華的桌子是可藐視的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You are presenting defiled food upon My altar. But you say, ‘How have we defiled You?’ In that you say, ‘The table of the Lord is to be despised.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“By offering defiled food on my altar. “But you ask, ‘How have we defiled you?’ “By saying that the Lord’s table is contemptible."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You have shown contempt by offering defiled sacrifices on my altar. “Then you ask, ‘How have we defiled the sacrifices?’ “You defile them by saying the altar of the Lord deserves no respect."
      }
    },
