    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华说：“从你们祖先开始，你们就常常偏离我的律例，不肯遵从。现在你们要转向我，我就转向你们。可是你们竟然说，‘我们怎么转向你呢？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“從你們列祖的日子以來，你們就偏離了我的律例而不遵守。現在你們要轉向我，我就必轉向你們。你們還問：‘我們要怎樣回轉呢？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華說：從你們列祖的日子以來，你們常常偏離我的典章而不遵守。現在你們要轉向我，我就轉向你們。你們卻問說：『我們如何才是轉向呢？』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“From the days of your fathers you have turned aside from My statutes and have not kept them. Return to Me, and I will return to you,” says the Lord of hosts. “But you say, ‘How shall we return?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ever since the time of your ancestors you have turned away from my decrees and have not kept them. Return to me, and I will return to you,” says the Lord Almighty. “But you ask, ‘How are we to return?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ever since the days of your ancestors, you have scorned my decrees and failed to obey them. Now return to me, and I will return to you,” says the Lord of Heaven’s Armies. “But you ask, ‘How can we return when we have never gone away?’"
      }
