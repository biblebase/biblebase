      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基督已经升到天上，坐在上帝的右边，众天使、掌权的、有能力的都服从了祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基督已進到天上，在　神右邊，眾天使、有權勢的、有能力的，都服從了他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌已經進入天堂，在神的右邊；眾天使和有權柄的，並有能力的，都服從了他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "who is at the right hand of God, having gone into heaven, after angels and authorities and powers had been subjected to Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who has gone into heaven and is at God’s right hand—with angels, authorities and powers in submission to him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now Christ has gone to heaven. He is seated in the place of honor next to God, and all the angels and authorities and powers accept his authority."
      }
    },
