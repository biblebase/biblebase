    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从前那些仰赖上帝的圣洁妇女，都是以这些来妆饰自己，并顺服自己的丈夫，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為古時仰望　神的聖潔婦女，正是這樣裝飾自己，順服丈夫，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為古時仰賴神的聖潔婦人正是以此為妝飾，順服自己的丈夫，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For in this way in former times the holy women also, who hoped in God, used to adorn themselves, being submissive to their own husbands;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this is the way the holy women of the past who put their hope in God used to adorn themselves. They submitted themselves to their own husbands,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is how the holy women of old made themselves beautiful. They put their trust in God and accepted the authority of their husbands."
      }
    },
