    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你们为基督的缘故受辱骂，就有福了！因为上帝荣耀的灵常住在你们身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要是為基督的名受辱罵，就有福了！因為　神榮耀的靈，住在你們身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若為基督的名受辱罵，便是有福的；因為神榮耀的靈常住在你們身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you are reviled for the name of Christ, you are blessed, because the Spirit of glory and of God rests on you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you are insulted because of the name of Christ, you are blessed, for the Spirit of glory and of God rests on you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you are insulted because you bear the name of Christ, you will be blessed, for the glorious Spirit of God rests upon you."
      }
    },
