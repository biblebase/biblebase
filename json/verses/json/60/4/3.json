    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们从前长期沉溺于异教徒的嗜好——淫乱、纵欲、醉酒、狂欢、宴乐和可憎的偶像崇拜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你們過去隨從教外人的心意，行邪淫、私慾、醉酒、荒宴、狂飲和可憎拜偶像的事，時候已經夠了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為往日隨從外邦人的心意行邪淫、惡慾、醉酒、荒宴、羣飲，並可惡拜偶像的事，時候已經夠了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the time already past is sufficient for you to have carried out the desire of the Gentiles, having pursued a course of sensuality, lusts, drunkenness, carousing, drinking parties and abominable idolatries."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For you have spent enough time in the past doing what pagans choose to do—living in debauchery, lust, drunkenness, orgies, carousing and detestable idolatry."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have had enough in the past of the evil things that godless people enjoy—their immorality and lust, their feasting and drunkenness and wild parties, and their terrible worship of idols."
      }
    },
