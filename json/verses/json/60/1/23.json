    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们获得重生，不是借着会腐烂的种子，而是借着不会腐烂的种子——上帝活泼永存的道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們得了重生，並不是由於能壞的種子，卻是由於不能朽壞的，就是藉著　神永活長存的道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們蒙了重生，不是由於能壞的種子，乃是由於不能壞的種子，是藉着神活潑常存的道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "for you have been born again not of seed which is perishable but imperishable, that is, through the living and enduring word of God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For you have been born again, not of perishable seed, but of imperishable, through the living and enduring word of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you have been born again, but not to a life that will quickly end. Your new life will last forever because it comes from the eternal, living word of God."
      }
