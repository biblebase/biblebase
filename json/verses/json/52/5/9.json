    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为上帝不是预定我们受惩罚，而是预定我们靠着主耶稣基督得救。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為　神不是定意要我們受刑罰，而是要我們藉著我們的主耶穌基督得著救恩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為神不是預定我們受刑，乃是預定我們藉着我們主耶穌基督得救。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For God has not destined us for wrath, but for obtaining salvation through our Lord Jesus Christ,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For God did not appoint us to suffer wrath but to receive salvation through our Lord Jesus Christ."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For God chose to save us through our Lord Jesus Christ, not to pour out his anger on us."
      }
    },
