    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿主使你们彼此之间的爱和对他人的爱一齐增长，甚至满溢出来，就像我们爱你们一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又願主叫你們彼此相愛的心，和愛眾人的心，都充充足足，多而又多，好像我們愛你們一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又願主叫你們彼此相愛的心，並愛眾人的心都能增長，充足，如同我們愛你們一樣，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and may the Lord cause you to increase and abound in love for one another, and for all people, just as we also do for you;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the Lord make your love increase and overflow for each other and for everyone else, just as ours does for you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And may the Lord make your love for one another and for all people grow and overflow, just as our love for you overflows."
      }
    },
