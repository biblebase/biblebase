    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们使我们在上帝面前感到非常喜乐，我们该怎样为你们感谢上帝呢！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們因你們的緣故，在我們的　神面前滿有喜樂；為這一切喜樂，我們可以為你們向　神獻上怎麼樣的感謝呢！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們在神面前，因着你們甚是喜樂；為這一切喜樂，可用何等的感謝為你們報答神呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For what thanks can we render to God for you in return for all the joy with which we rejoice before our God on your account,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How can we thank God enough for you in return for all the joy we have in the presence of our God because of you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How we thank God for you! Because of you we have great joy as we enter God’s presence."
      }
    },
