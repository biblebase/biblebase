    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我保罗同西拉和提摩太写信给帖撒罗尼迦属于父上帝和主耶稣基督的教会。 愿恩典和平安归给你们！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "保羅、西拉和提摩太，寫信給帖撒羅尼迦、在父　神和主耶穌基督裡的教會。願恩惠平安臨到你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "保羅、西拉、提摩太寫信給帖撒羅尼迦在父神和主耶穌基督裏的教會。願恩惠、平安歸與你們！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Paul and Silvanus and Timothy, To the church of the Thessalonians in God the Father and the Lord Jesus Christ: Grace to you and peace."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Paul, Silas and Timothy, To the church of the Thessalonians in God the Father and the Lord Jesus Christ: Grace and peace to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This letter is from Paul, Silas, and Timothy. We are writing to the church in Thessalonica, to you who belong to God the Father and the Lord Jesus Christ. May God give you grace and peace."
      }
    },
