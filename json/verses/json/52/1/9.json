    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为人们都在传讲你们如何接待我们，如何离弃偶像归向上帝，事奉又真又活的上帝，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們都述說你們是怎樣接待我們，而且怎樣離棄偶像歸向　神，要服事這位又真又活的　神，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他們自己已經報明我們是怎樣進到你們那裏，你們是怎樣離棄偶像，歸向神，要服事那又真又活的神，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they themselves report about us what kind of a reception we had with you, and how you turned to God from idols to serve a living and true God,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for they themselves report what kind of reception you gave us. They tell how you turned to God from idols to serve the living and true God,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "for they keep talking about the wonderful welcome you gave us and how you turned away from idols to serve the living and true God."
      }
    },
