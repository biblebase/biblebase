    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "等候祂的儿子从天降临，就是祂使之从死里复活、救我们脱离将来可怕审判的耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "並且等候他的兒子從天降臨。這就是　神使他從死人中復活，救我們脫離將來的忿怒的那位耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "等候他兒子從天降臨，就是他從死裏復活的－那位救我們脫離將來忿怒的耶穌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and to wait for His Son from heaven, whom He raised from the dead, that is Jesus, who rescues us from the wrath to come."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and to wait for his Son from heaven, whom he raised from the dead—Jesus, who rescues us from the coming wrath."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And they speak of how you are looking forward to the coming of God’s Son from heaven—Jesus, whom God raised from the dead. He is the one who has rescued us from the terrors of the coming judgment."
      }
    },
