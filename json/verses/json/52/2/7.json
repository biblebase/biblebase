    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "身为基督的使徒，我们理当受到你们的尊重，然而我们像母亲抚育婴儿一样温柔地对待你们。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只在你們中間存心溫柔，如同母親乳養自己的孩子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But we proved to be gentle among you, as a nursing mother tenderly cares for her own children."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Instead, we were like young children among you. Just as a nursing mother cares for her children,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As apostles of Christ we certainly had a right to make some demands of you, but instead we were like children among you. Or we were like a mother feeding and caring for her own children."
      }
    },
