    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，你们一定记得我们的劳苦和艰难。我们一面向你们传福音，一面昼夜辛勤工作，免得成为你们任何人的负担。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，你們應該記得我們的辛苦和勞碌；我們把　神的福音傳給你們的時候，怎樣晝夜作工，免得你們有人受累。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，你們記念我們的辛苦勞碌，晝夜做工，傳神的福音給你們，免得叫你們一人受累。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For you recall, brethren, our labor and hardship, how working night and day so as not to be a burden to any of you, we proclaimed to you the gospel of God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely you remember, brothers and sisters, our toil and hardship; we worked night and day in order not to be a burden to anyone while we preached the gospel of God to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t you remember, dear brothers and sisters, how hard we worked among you? Night and day we toiled to earn a living so that we would not be a burden to any of you as we preached God’s Good News to you."
      }
    },
