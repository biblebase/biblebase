    "sermons": [
      {
        "id": "home_sermons_retreat-2019-06-23",
        "title": "更加相愛",
        "date": "2019-06-23",
        "preacher": "陳愛光牧師",
        "verses": "帖撒羅尼迦前書 4:9-12",
        "audio": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-23.mp3",
        "slides": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-23.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们对马其顿全境的弟兄姊妹已经做到了这一点。不过，我劝各位要再接再厉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "其實，你們向全馬其頓所有的弟兄，已經這樣行了。但是，弟兄們，我們勸你們要更加彼此相愛；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們向馬其頓全地的眾弟兄固然是這樣行，但我勸弟兄們要更加勉勵。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "for indeed you do practice it toward all the brethren who are in all Macedonia. But we urge you, brethren, to excel still more,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And in fact, you do love all of God’s family throughout Macedonia. Yet we urge you, brothers and sisters, to do so more and more,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Indeed, you already show your love for all the believers throughout Macedonia. Even so, dear brothers and sisters, we urge you to love them even more."
      }
    },
