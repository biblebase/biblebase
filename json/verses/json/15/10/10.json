    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以斯拉祭司站起来对他们说：“你们对上帝不忠，娶外族的女子为妻，加重了以色列的罪恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以斯拉祭司站起來，對他們說：“你們對　神不忠，娶了外族的女子為妻，增添了以色列的罪過。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司以斯拉站起來，對他們說：「你們有罪了；因你們娶了外邦的女子為妻，增添以色列人的罪惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Ezra the priest stood up and said to them, “You have been unfaithful and have married foreign wives adding to the guilt of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Ezra the priest stood up and said to them, “You have been unfaithful; you have married foreign women, adding to Israel’s guilt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Ezra the priest stood and said to them: “You have committed a terrible sin. By marrying pagan women, you have increased Israel’s guilt."
      }
    },
