    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡不遵行你上帝的律法和王法的，要对他严加治罪，或处死，或发配，或抄家，或监禁。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不遵守你　神的律法和王的律法的，就要審慎地判決處分，或是處死、或是放逐、或是沒收家產、或是囚禁。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡不遵行你神律法和王命令的人就當速速定他的罪，或治死，或充軍，或抄家，或囚禁。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whoever will not observe the law of your God and the law of the king, let judgment be executed upon him strictly, whether for death or for banishment or for confiscation of goods or for imprisonment.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever does not obey the law of your God and the law of the king must surely be punished by death, banishment, confiscation of property, or imprisonment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who refuses to obey the law of your God and the law of the king will be punished immediately, either by death, banishment, confiscation of goods, or imprisonment.”"
      }
    },
