    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司、利未人、一些百姓、歌乐手、殿门守卫和殿役住在自己的城里，其余的以色列人也都住在自己的城里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是祭司、利未人、歌唱者、守門的、作殿役的，和一部分人民住在城裡；以色列眾人都住在自己的城裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是祭司、利未人、民中的一些人、歌唱的、守門的、尼提寧，並以色列眾人，各住在自己的城裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the priests and the Levites, some of the people, the singers, the gatekeepers and the temple servants lived in their cities, and all Israel in their cities."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priests, the Levites, the musicians, the gatekeepers and the temple servants settled in their own towns, along with some of the other people, and the rest of the Israelites settled in their towns."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the priests, the Levites, the singers, the gatekeepers, the Temple servants, and some of the common people settled in villages near Jerusalem. The rest of the people returned to their own towns throughout Israel."
      }
    },
