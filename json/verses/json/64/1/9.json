    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我曾写信给你那里的教会，但喜欢做领袖的丢特腓不理会我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我曾經略略寫信給你那裡的教會，但他們中間那好作領袖的丟特腓不接待我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我曾略略地寫信給教會，但那在教會中好為首的丟特腓不接待我們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I wrote something to the church; but Diotrephes, who loves to be first among them, does not accept what we say."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I wrote to the church, but Diotrephes, who loves to be first, will not welcome us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I wrote to the church about this, but Diotrephes, who loves to be the leader, refuses to have anything to do with us."
      }
