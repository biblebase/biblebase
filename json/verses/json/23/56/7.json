    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必领他们到我的圣山， 到向我祷告的殿内， 使他们欢欣。 他们献上的燔祭和其他祭物必蒙悦纳， 因为我的殿必被称为万民祷告的殿。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必領他們到我的聖山， 使他們在屬於我的禱告的殿中喜樂； 他們的燔祭和祭品，在我的祭壇上必蒙悅納； 因為我的殿必稱為萬族禱告的殿。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必領他們到我的聖山， 使他們在禱告我的殿中喜樂。 他們的燔祭和平安祭， 在我壇上必蒙悅納， 因我的殿必稱為萬民禱告的殿。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even those I will bring to My holy mountain And make them joyful in My house of prayer. Their burnt offerings and their sacrifices will be acceptable on My altar; For My house will be called a house of prayer for all the peoples.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "these I will bring to my holy mountain and give them joy in my house of prayer. Their burnt offerings and sacrifices will be accepted on my altar; for my house will be called a house of prayer for all nations.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will bring them to my holy mountain of Jerusalem and will fill them with joy in my house of prayer. I will accept their burnt offerings and sacrifices, because my Temple will be called a house of prayer for all nations."
      }
    },
