    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谨守而不亵渎安息日、 不做恶事的人有福了！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "謹守安息日，不褻瀆這日， 保守自己的手不作任何惡事， 這樣行的人和堅持這樣作的人， 是有福的！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "謹守安息日而不干犯， 禁止己手而不作惡； 如此行、如此持守的人便為有福。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How blessed is the man who does this, And the son of man who takes hold of it; Who keeps from profaning the sabbath, And keeps his hand from doing any evil.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Blessed is the one who does this— the person who holds it fast, who keeps the Sabbath without desecrating it, and keeps their hands from doing any evil.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Blessed are all those who are careful to do this. Blessed are those who honor my Sabbath days of rest and keep themselves from doing wrong."
      }
    },
