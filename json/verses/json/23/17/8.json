    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时他们不再仰望自己制造的祭坛，也不再供奉自己指头所造的亚舍拉神像和香坛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們必不仰望自己雙手所做的祭壇， 也不再看著自己指頭所做的， 無論是亞舍拉，或是香壇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必不仰望祭壇，就是自己手所築的，也不重看自己指頭所做的，無論是木偶是日像。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will not have regard for the altars, the work of his hands, Nor will he look to that which his fingers have made, Even the Asherim and incense stands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will not look to the altars, the work of their hands, and they will have no regard for the Asherah poles and the incense altars their fingers have made."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will no longer look to their idols for help or worship what their own hands have made. They will never again bow down to their Asherah poles or worship at the pagan shrines they have built."
      }
    },
