    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们必因在橡树下， 在你们选择的园子里祭拜偶像而蒙羞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們必因你們所喜愛的橡樹抱愧， 必因你們所選擇的園子蒙羞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那等人必因你們所喜愛的橡樹抱愧； 你們必因所選擇的園子蒙羞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Surely you will be ashamed of the oaks which you have desired, And you will be embarrassed at the gardens which you have chosen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You will be ashamed because of the sacred oaks in which you have delighted; you will be disgraced because of the gardens that you have chosen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will be ashamed of your idol worship in groves of sacred oaks. You will blush because you worshiped in gardens dedicated to idols."
      }
    },
