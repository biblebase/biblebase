    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的土地荒凉， 城邑化为灰烬。 你们亲眼目睹自己的田园被外族人侵吞、毁坏、变成不毛之地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們的土地荒涼， 你們的城鎮被火燒毀， 你們的田地，在你們面前給外族人侵吞； 被外族人傾覆之後，就荒涼了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們的地土已經荒涼； 你們的城邑被火焚毀。 你們的田地在你們眼前為外邦人所侵吞， 既被外邦人傾覆就成為荒涼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your land is desolate, Your cities are burned with fire, Your fields—strangers are devouring them in your presence; It is desolation, as overthrown by strangers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your country is desolate, your cities burned with fire; your fields are being stripped by foreigners right before you, laid waste as when overthrown by strangers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your country lies in ruins, and your towns are burned. Foreigners plunder your fields before your eyes and destroy everything they see."
      }
