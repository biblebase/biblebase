      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要洗濯、自潔， 從我眼前除掉你們的惡行， 要止住作惡，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Wash yourselves, make yourselves clean; Remove the evil of your deeds from My sight. Cease to do evil,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Wash and make yourselves clean. Take your evil deeds out of my sight; stop doing wrong."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wash yourselves and be clean! Get your sins out of my sight. Give up your evil ways."
      }
