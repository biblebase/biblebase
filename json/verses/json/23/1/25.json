    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必攻击你， 用碱炼净你的渣滓， 除尽你的杂质。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必把手轉過來攻擊你； 我要煉除你的渣滓，如同用鹼來煉一樣， 我要除去你的一切雜質。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必反手加在你身上， 煉盡你的渣滓， 除淨你的雜質。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will also turn My hand against you, And will smelt away your dross as with lye And will remove all your alloy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will turn my hand against you; I will thoroughly purge away your dross and remove all your impurities."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will raise my fist against you. I will melt you down and skim off your slag. I will remove all your impurities."
      }
    },
