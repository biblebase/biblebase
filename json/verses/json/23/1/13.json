    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要再带毫无意义的祭物了。 我憎恶你们烧的香， 厌恶你们的朔日、安息日和大会。 你们又作恶又举行庄严的聚会， 令我无法容忍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要再帶沒有意義的供物來了， 燒獻祭物的香氣也是我厭惡的。 我厭煩月朔、安息日和集會； 作罪孽又守嚴肅會，是我不能容忍的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們不要再獻虛浮的供物。 香品是我所憎惡的； 月朔和安息日，並宣召的大會， 也是我所憎惡的； 作罪孽，又守嚴肅會， 我也不能容忍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Bring your worthless offerings no longer, Incense is an abomination to Me. New moon and sabbath, the calling of assemblies— I cannot endure iniquity and the solemn assembly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Stop bringing meaningless offerings! Your incense is detestable to me. New Moons, Sabbaths and convocations— I cannot bear your worthless assemblies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Stop bringing me your meaningless gifts; the incense of your offerings disgusts me! As for your celebrations of the new moon and the Sabbath and your special days for fasting— they are all sinful and false. I want no more of your pious meetings."
      }
    },
