    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要学习行善， 追求正义， 帮助受欺压的， 替孤儿辩护， 为寡妇申冤。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "學習行善， 尋求公平， 指責殘暴的人， 替孤兒伸冤， 為寡婦辨屈。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "學習行善， 尋求公平， 解救受欺壓的； 給孤兒伸冤， 為寡婦辨屈。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Learn to do good; Seek justice, Reprove the ruthless, Defend the orphan, Plead for the widow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Learn to do right; seek justice. Defend the oppressed. Take up the cause of the fatherless; plead the case of the widow."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Learn to do good. Seek justice. Help the oppressed. Defend the cause of orphans. Fight for the rights of widows."
      }
    },
