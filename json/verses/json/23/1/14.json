    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我憎恶你们的朔日及其他节期， 它们成了我的重担， 令我厌倦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們的月朔和制定的節期，我心裡恨惡； 它們都成了我的重擔， 我已承擔得不耐煩了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們的月朔和節期，我心裏恨惡， 我都以為麻煩； 我擔當，便不耐煩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I hate your new moon festivals and your appointed feasts, They have become a burden to Me; I am weary of bearing them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your New Moon feasts and your appointed festivals I hate with all my being. They have become a burden to me; I am weary of bearing them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I hate your new moon celebrations and your annual festivals. They are a burden to me. I cannot stand them!"
      }
