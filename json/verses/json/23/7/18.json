    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时，耶和华的哨声一响，埃及的大军必像苍蝇一样从遥远的尼罗河飞扑而来，亚述的人马必如黄蜂一样蜂拥而至，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到那時，耶和華必發出哨聲，把埃及河流盡頭的蒼蠅和亞述地的蜂子召來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「那時，耶和華要發嘶聲，使埃及江河源頭的蒼蠅和亞述地的蜂子飛來；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In that day the Lord will whistle for the fly that is in the remotest part of the rivers of Egypt and for the bee that is in the land of Assyria."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In that day the Lord will whistle for flies from the Nile delta in Egypt and for bees from the land of Assyria."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In that day the Lord will whistle for the army of southern Egypt and for the army of Assyria. They will swarm around you like flies and bees."
      }
    },
