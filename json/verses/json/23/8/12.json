    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“他们认为是阴谋的，你们不要认为是阴谋。他们所怕的，你们不要怕，也不要畏惧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“這子民所稱為陰謀的，你們都不要稱為陰謀； 他們所怕的，你們不要怕，也不要畏懼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「這百姓說同謀背叛，你們不要說同謀背叛。他們所怕的，你們不要怕，也不要畏懼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You are not to say, ‘It is a conspiracy!’ In regard to all that this people call a conspiracy, And you are not to fear what they fear or be in dread of it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do not call conspiracy everything this people calls a conspiracy; do not fear what they fear, and do not dread it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Don’t call everything a conspiracy, like they do, and don’t live in dread of what frightens them."
      }
    },
