    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要在旷野栽香柏树、 皂荚树、番石榴树和橄榄树。 我要在沙漠种松树、杉树和黄杨树。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要在曠野種上香柏樹、 皂莢樹、番石榴樹和野橄欖樹； 我要在沙漠中栽種松樹、 杉樹和黃楊樹，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要在曠野種上香柏樹、 皂莢樹、番石榴樹，和野橄欖樹。 我在沙漠要把松樹、杉樹， 並黃楊樹一同栽植；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will put the cedar in the wilderness, The acacia and the myrtle and the olive tree; I will place the juniper in the desert Together with the box tree and the cypress,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will put in the desert the cedar and the acacia, the myrtle and the olive. I will set junipers in the wasteland, the fir and the cypress together,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will plant trees in the barren desert— cedar, acacia, myrtle, olive, cypress, fir, and pine."
      }
