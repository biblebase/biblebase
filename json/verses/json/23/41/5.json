    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“众海岛看见我的作为就害怕， 地极也颤抖， 他们都上前来，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾海島看見了，就都懼怕； 地極也都戰兢； 他們都近前來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "海島看見就都害怕； 地極也都戰兢， 就近前來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The coastlands have seen and are afraid; The ends of the earth tremble; They have drawn near and have come."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The islands have seen it and fear; the ends of the earth tremble. They approach and come forward;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The lands beyond the sea watch in fear. Remote lands tremble and mobilize for war."
      }
    },
