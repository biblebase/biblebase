    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，你们实在算不得什么， 你们所做的毫无意义， 拜你们的人真是可憎！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！你們屬於無有， 你們的作為都屬於虛空， 那選擇你們的是可厭惡的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，你們屬乎虛無； 你們的作為也屬乎虛空。 那選擇你們的是可憎惡的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, you are of no account, And your work amounts to nothing; He who chooses you is an abomination."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you are less than nothing and your works are utterly worthless; whoever chooses you is detestable."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But no! You are less than nothing and can do nothing at all. Those who choose you pollute themselves."
      }
    },
