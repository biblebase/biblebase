    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使光秃的山岭上江河奔流， 山谷中泉水涌流； 我要使沙漠变成水塘， 使干地冒出泉水。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要在光禿的高處開闢江河， 在山谷之間開闢泉源； 我要使曠野變為水池， 使乾地變成水泉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要在淨光的高處開江河， 在谷中開泉源； 我要使沙漠變為水池， 使乾地變為湧泉。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will open rivers on the bare heights And springs in the midst of the valleys; I will make the wilderness a pool of water And the dry land fountains of water."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will make rivers flow on barren heights, and springs within the valleys. I will turn the desert into pools of water, and the parched ground into springs."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will open up rivers for them on the high plateaus. I will give them fountains of water in the valleys. I will fill the desert with pools of water. Rivers fed by springs will flow across the parched ground."
      }
    },
