    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝说：“众海岛啊， 要在我面前安静！ 让众民重新拿出勇力， 让他们前来说话， 让我们一起来理论。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾海島啊！你們要在我面前靜默， 願萬民重新得力。 讓他們近前來，然後說話， 讓我們聚在一起辯論吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾海島啊，當在我面前靜默； 眾民當重新得力， 都要近前來才可以說話， 我們可以彼此辯論。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Coastlands, listen to Me in silence, And let the peoples gain new strength; Let them come forward, then let them speak; Let us come together for judgment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Be silent before me, you islands! Let the nations renew their strength! Let them come forward and speak; let us meet together at the place of judgment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Listen in silence before me, you lands beyond the sea. Bring your strongest arguments. Come now and speak. The court is ready for your case."
      }
    },
