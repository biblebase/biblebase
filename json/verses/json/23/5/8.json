    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些不断建房置田、占光土地、 独居其中的人有祸了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些使房屋連接房屋，使田地連接田地， 以致不留餘地的人，有禍了！ 你們只可以獨居在境內。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "禍哉！那些以房接房， 以地連地， 以致不留餘地的， 只顧自己獨居境內。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe to those who add house to house and join field to field, Until there is no more room, So that you have to live alone in the midst of the land!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Woe to you who add house to house and join field to field till no space is left and you live alone in the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What sorrow for you who buy up house after house and field after field, until everyone is evicted and you live alone in the land."
      }
    },
