    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些用虚假作绳子扯来罪恶， 用套绳拉来邪恶的人有祸了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些人虛假的繩索牽引罪孽， 又像用車繩拉罪惡的人，有禍了！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "禍哉！那些以虛假之細繩牽罪孽的人！ 他們又像以套繩拉罪惡，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe to those who drag iniquity with the cords of falsehood, And sin as if with cart ropes;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Woe to those who draw sin along with cords of deceit, and wickedness as with cart ropes,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What sorrow for those who drag their sins behind them with ropes made of lies, who drag wickedness behind them like a cart!"
      }
    },
