    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂必竖起旗帜召集远方的国家， 吹哨叫来地极的人。 看啊，他们必飞速而来！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必豎立旗幟，招一國的人從遠方而來， 發出哨聲把他們從地極召來； 看！他們必急速快奔而來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必豎立大旗，招遠方的國民， 發嘶聲叫他們從地極而來； 看哪，他們必急速奔來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will also lift up a standard to the distant nation, And will whistle for it from the ends of the earth; And behold, it will come with speed swiftly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He lifts up a banner for the distant nations, he whistles for those at the ends of the earth. Here they come, swiftly and speedily!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will send a signal to distant nations far away and whistle to those at the ends of the earth. They will come racing toward Jerusalem."
      }
    },
