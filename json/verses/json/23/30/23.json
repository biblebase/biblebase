    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们撒种，主必降雨，并使你们五谷丰登。那时，你们的牲畜必在宽阔的草场上吃草；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你撒在地裡的種子，主必降雨在其上，並且使你地裡所出產的糧食，既豐盛又肥美。到那日，你的牲畜必在寬闊的草場上吃草。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你將種子撒在地裏，主必降雨在其上，並使地所出的糧肥美豐盛。到那時，你的牲畜必在寬闊的草場吃草。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He will give you rain for the seed which you will sow in the ground, and bread from the yield of the ground, and it will be rich and plenteous; on that day your livestock will graze in a roomy pasture."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will also send you rain for the seed you sow in the ground, and the food that comes from the land will be rich and plentiful. In that day your cattle will graze in broad meadows."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord will bless you with rain at planting time. There will be wonderful harvests and plenty of pastureland for your livestock."
      }
    },
