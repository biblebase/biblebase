    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "埃及的帮助是徒然的， 所以我称她为“没用的海怪”。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "埃及的幫助是徒然的，是虛幻的， 因此我稱它為“坐而不動的拉哈伯”。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "埃及的幫助是徒然無益的； 所以我稱她為「坐而不動的拉哈伯」。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even Egypt, whose help is vain and empty. Therefore, I have called her “Rahab who has been exterminated.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "to Egypt, whose help is utterly useless. Therefore I call her Rahab the Do-Nothing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Egypt’s promises are worthless! Therefore, I call her Rahab— the Harmless Dragon."
      }
    },
