    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "让开！不要挡我们的路！ 不要再给我们讲以色列圣者的事。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要離開正道，轉離義路， 不要在我們面前再提以色列的聖者了！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要離棄正道，偏離直路， 不要在我們面前再提說以色列的聖者。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Get out of the way, turn aside from the path, Let us hear no more about the Holy One of Israel.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Leave this way, get off this path, and stop confronting us with the Holy One of Israel!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Forget all this gloom. Get off your narrow path. Stop telling us about your ‘Holy One of Israel.’”"
      }
    },
