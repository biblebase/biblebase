    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华——以色列的圣者说： “你们回转、安息便可得到拯救； 你们安静、信靠便可得到力量。 但你们却不肯。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為主耶和華以色列的聖者這樣說： “你們得救在於悔改和安息， 你們得力在於平靜和信靠。” 但你們竟不願意。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華－以色列的聖者曾如此說： 你們得救在乎歸回安息； 你們得力在乎平靜安穩； 你們竟自不肯。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus the Lord God, the Holy One of Israel, has said, “In repentance and rest you will be saved, In quietness and trust is your strength.” But you were not willing,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Sovereign Lord, the Holy One of Israel, says: “In repentance and rest is your salvation, in quietness and trust is your strength, but you would have none of it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Sovereign Lord, the Holy One of Israel, says: “Only in returning to me and resting in me will you be saved. In quietness and confidence is your strength. But you would have none of it."
      }
    },
