    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的刑杖每责打他们一下，祂的子民必击鼓弹琴。祂必在战场上挥手击打他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必把刑罰的杖加在他們身上， 每打一下， 都有擊鼓和彈琴的聲音； 耶和華在爭戰的時候，必揮動著手與他們爭戰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華必將命定的杖加在他身上；每打一下，人必擊鼓彈琴。打仗的時候，耶和華必掄起手來，與他交戰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And every blow of the rod of punishment, Which the Lord will lay on him, Will be with the music of tambourines and lyres; And in battles, brandishing weapons, He will fight them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every stroke the Lord lays on them with his punishing club will be to the music of timbrels and harps, as he fights them in battle with the blows of his arm."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And as the Lord strikes them with his rod of punishment, his people will celebrate with tambourines and harps. Lifting his mighty arm, he will fight the Assyrians."
      }
    },
