    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你们必欢唱，好像晚上庆祝节期；你们必充满喜乐，好像人们吹着笛子登耶和华的山、到以色列磐石那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們卻必歌唱，好像守聖節的夜間一樣； 你們必心裡喜樂，好像吹著笛行走的人， 必上耶和華的山，到以色列的磐石那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們必唱歌，像守聖節的夜間一樣，並且心中喜樂，像人吹笛，上耶和華的山，到以色列的磐石那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will have songs as in the night when you keep the festival, And gladness of heart as when one marches to the sound of the flute, To go to the mountain of the Lord, to the Rock of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And you will sing as on the night you celebrate a holy festival; your hearts will rejoice as when people playing pipes go up to the mountain of the Lord, to the Rock of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the people of God will sing a song of joy, like the songs at the holy festivals. You will be filled with joy, as when a flutist leads a group of pilgrims to Jerusalem, the mountain of the Lord— to the Rock of Israel."
      }
    },
