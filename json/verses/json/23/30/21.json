    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不管你们向左还是向右，都会听见背后有声音说：“这是正路，踏上去吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "每當你偏左或偏右的時候，你必聽見後面有聲音說：“這是正路，要行在其中！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你或向左或向右，你必聽見後邊有聲音說：「這是正路，要行在其間。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your ears will hear a word behind you, “This is the way, walk in it,” whenever you turn to the right or to the left."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whether you turn to the right or to the left, your ears will hear a voice behind you, saying, “This is the way; walk in it.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your own ears will hear him. Right behind you a voice will say, “This is the way you should go,” whether to the right or to the left."
      }
