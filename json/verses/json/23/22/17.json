    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，你这有权有势的人，耶和华必紧紧抓住你，把你猛力抛开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪，勇士啊！耶和華將要猛烈地拋擲你； 他必把你緊緊抓住，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，耶和華必像大有力的人，將你緊緊纏裹，竭力拋去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, the Lord is about to hurl you headlong, O man. And He is about to grasp you firmly"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Beware, the Lord is about to take firm hold of you and hurl you away, you mighty man."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Lord is about to hurl you away, mighty man. He is going to grab you,"
      }
