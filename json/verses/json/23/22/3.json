    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的官长一起逃跑， 一箭未发便束手就缚， 逃到远方的也一同被擒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們所有的官長都一起逃跑； 他們因沒有弓箭而被俘擄， 你們中間所有被找到的都一起被俘擄， 雖然他們已逃到遠方去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你所有的官長一同逃跑， 都為弓箭手所捆綁。 你中間一切被找到的都一同被捆綁； 他們本是逃往遠方的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All your rulers have fled together, And have been captured without the bow; All of you who were found were taken captive together, Though they had fled far away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All your leaders have fled together; they have been captured without using the bow. All you who were caught were taken prisoner together, having fled while the enemy was still far away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All your leaders have fled. They surrendered without resistance. The people tried to slip away, but they were captured, too."
      }
    },
