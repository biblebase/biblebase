    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们这座充满喧闹、骚动和欢乐的城啊， 你们中间被杀的人既非丧身刀下， 也非死于战场。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你這充滿喧嘩，繁囂、 歡樂的城市啊！ 你們中間被殺的，並不是被刀劍所殺， 也不是在戰場上陣亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你這滿處吶喊、大有喧嘩的城， 歡樂的邑啊， 你中間被殺的並不是被刀殺， 也不是因打仗死亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You who were full of noise, You boisterous town, you exultant city; Your slain were not slain with the sword, Nor did they die in battle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "you town so full of commotion, you city of tumult and revelry? Your slain were not killed by the sword, nor did they die in battle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The whole city is in a terrible uproar. What do I see in this reveling city? Bodies are lying everywhere, killed not in battle but by famine and disease."
      }
    },
