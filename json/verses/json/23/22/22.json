    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必把大卫家的钥匙交给他。他开启的，没有人能关；他关闭的，没有人能开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必把大衛家的鑰匙放在他肩頭上； 他開了，就沒有人能關； 他關了，就沒有人能開。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必將大衛家的鑰匙放在他肩頭上。他開，無人能關；他關，無人能開。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I will set the key of the house of David on his shoulder, When he opens no one will shut, When he shuts no one will open."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will place on his shoulder the key to the house of David; what he opens no one can shut, and what he shuts no one can open."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will give him the key to the house of David—the highest position in the royal court. When he opens doors, no one will be able to close them; when he closes doors, no one will be able to open them."
      }
    },
