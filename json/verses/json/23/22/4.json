    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，我说：“你们不要管我， 让我痛哭一场吧！ 不要安慰我， 我的同胞都被毁灭了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此我說：“要轉開你們的眼，不要望著我， 讓我痛哭吧！ 不要因我人民的毀滅，而急急安慰我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以我說：你們轉眼不看我， 我要痛哭。 不要因我眾民的毀滅， 就竭力安慰我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore I say, “Turn your eyes away from me, Let me weep bitterly, Do not try to comfort me concerning the destruction of the daughter of my people.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore I said, “Turn away from me; let me weep bitterly. Do not try to console me over the destruction of my people.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That’s why I said, “Leave me alone to weep; do not try to comfort me. Let me cry for my people as I watch them being destroyed.”"
      }
    },
