    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华启示我说： “你们这罪到死也得不到赦免。 这是主——万军之耶和华说的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華親自啟示我說： “直到你們死的時候， 這罪孽也不得赦免。”這是主萬軍之耶和華說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華親自默示我說： 這罪孽直到你們死，斷不得赦免！ 這是主－萬軍之耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the Lord of hosts revealed Himself to me, “Surely this iniquity shall not be forgiven you Until you die,” says the Lord God of hosts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord Almighty has revealed this in my hearing: “Till your dying day this sin will not be atoned for,” says the Lord, the Lord Almighty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord of Heaven’s Armies has revealed this to me: “Till the day you die, you will never be forgiven for this sin.” That is the judgment of the Lord, the Lord of Heaven’s Armies."
      }
    },
