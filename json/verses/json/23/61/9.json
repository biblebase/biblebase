    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的后代必享誉列国， 子孙必名闻万邦， 看见的人都承认他们是蒙上帝赐福之民。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的後裔必在列國中被人認識， 他們的子孫在萬民中也必這樣； 看見他們的， 都必承認他們是耶和華賜福的後裔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的後裔必在列國中被人認識； 他們的子孫在眾民中也是如此。 凡看見他們的必認他們是耶和華賜福的後裔。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then their offspring will be known among the nations, And their descendants in the midst of the peoples. All who see them will recognize them Because they are the offspring whom the Lord has blessed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their descendants will be known among the nations and their offspring among the peoples. All who see them will acknowledge that they are a people the Lord has blessed.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their descendants will be recognized and honored among the nations. Everyone will realize that they are a people the Lord has blessed.”"
      }
    },
