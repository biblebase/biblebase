    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“因为我耶和华喜爱公正， 憎恶抢劫之罪。 我必凭信实赏赐我的子民， 与他们立永远的约。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為我耶和華喜愛公平， 恨惡不義的搶奪； 我要憑真理賞賜他們， 與他們立永遠的約。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為我－耶和華喜愛公平， 恨惡搶奪和罪孽； 我要憑誠實施行報應， 並要與我的百姓立永約。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I, the Lord, love justice, I hate robbery in the burnt offering; And I will faithfully give them their recompense And make an everlasting covenant with them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“For I, the Lord, love justice; I hate robbery and wrongdoing. In my faithfulness I will reward my people and make an everlasting covenant with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“For I, the Lord, love justice. I hate robbery and wrongdoing. I will faithfully reward my people for their suffering and make an everlasting covenant with them."
      }
    },
