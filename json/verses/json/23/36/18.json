    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要让希西迦欺骗你们，以为耶和华必拯救你们。列国的神明中有哪个曾经从亚述王手中救他的国家呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要小心，免得希西家慫恿你們說：耶和華必拯救我們。列國的神有哪一位曾拯救自己的國土脫離亞述王的手呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要謹防，恐怕希西家勸導你們說：耶和華必拯救我們。列國的神有哪一個救他本國脫離亞述王的手呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Beware that Hezekiah does not mislead you, saying, “The Lord will deliver us.” Has any one of the gods of the nations delivered his land from the hand of the king of Assyria?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do not let Hezekiah mislead you when he says, ‘The Lord will deliver us.’ Have the gods of any nations ever delivered their lands from the hand of the king of Assyria?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Don’t let Hezekiah mislead you by saying, ‘The Lord will rescue us!’ Have the gods of any other nations ever saved their people from the king of Assyria?"
      }
    },
