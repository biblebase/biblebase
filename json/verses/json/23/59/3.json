    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的手染满了血污， 手指沾满了罪恶。 你们的嘴唇吐出谎言， 舌头嘀咕恶事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你們的手沾滿了血， 你們的指頭被罪孽玷污， 你們的嘴唇說虛謊的話， 你們的舌頭講出兇言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因你們的手被血沾染， 你們的指頭被罪孽沾污， 你們的嘴唇說謊言， 你們的舌頭出惡語。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For your hands are defiled with blood And your fingers with iniquity; Your lips have spoken falsehood, Your tongue mutters wickedness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For your hands are stained with blood, your fingers with guilt. Your lips have spoken falsely, and your tongue mutters wicked things."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your hands are the hands of murderers, and your fingers are filthy with sin. Your lips are full of lies, and your mouth spews corruption."
      }
    },
