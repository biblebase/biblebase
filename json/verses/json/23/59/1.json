    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，耶和华的臂膀并非没有力量拯救， 祂的耳朵并非聋得听不见。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！耶和華的手不是縮短了， 以致不能拯救； 他的耳朵不是不靈， 不能聽見；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的膀臂並非縮短，不能拯救， 耳朵並非發沉，不能聽見，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, the Lord’s hand is not so short That it cannot save; Nor is His ear so dull That it cannot hear."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely the arm of the Lord is not too short to save, nor his ear too dull to hear."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen! The Lord’s arm is not too weak to save you, nor is his ear too deaf to hear you call."
      }
    },
