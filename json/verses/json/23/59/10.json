    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们像盲人一样沿墙摸索， 像无眼睛的人一样探路。 我们在中午也跌跌撞撞， 如同走在晚上。 我们在强壮人当中如同死人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們摸索牆壁，好像瞎子一樣； 我們摸索，如同沒有眼睛的人一般； 我們在中午跌倒，有如在黃昏跌倒一樣； 我們在肥壯的人中，好像死人一般。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們摸索牆壁，好像瞎子； 我們摸索，如同無目之人。 我們晌午絆腳，如在黃昏一樣； 我們在肥壯人中，像死人一般。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We grope along the wall like blind men, We grope like those who have no eyes; We stumble at midday as in the twilight, Among those who are vigorous we are like dead men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like the blind we grope along the wall, feeling our way like people without eyes. At midday we stumble as if it were twilight; among the strong, we are like the dead."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We grope like the blind along a wall, feeling our way like people without eyes. Even at brightest noontime, we stumble as though it were dark. Among the living, we are like the dead."
      }
    },
