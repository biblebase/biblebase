    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "正义被赶逐， 公义不能靠近， 真理倒在街上， 正直被拒之门外。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "公平被迫後退， 公義站在遠處； 誠實在街上跌倒， 正直不能進入。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且公平轉而退後， 公義站在遠處； 誠實在街上仆倒， 正直也不得進入。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Justice is turned back, And righteousness stands far away; For truth has stumbled in the street, And uprightness cannot enter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So justice is driven back, and righteousness stands at a distance; truth has stumbled in the streets, honesty cannot enter."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Our courts oppose the righteous, and justice is nowhere to be found. Truth stumbles in the streets, and honesty has been outlawed."
      }
    },
