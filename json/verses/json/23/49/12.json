    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，他们必从远方来， 有的来自北方，有的来自西方， 还有的来自希尼。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！有些人從遠方而來； 看哪！有些從北方，從西方而來， 還有些從色尼姆地而來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，這些從遠方來； 這些從北方、從西方來； 這些從秦國來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, these will come from afar; And lo, these will come from the north and from the west, And these from the land of Sinim.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See, they will come from afar— some from the north, some from the west, some from the region of Aswan.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "See, my people will return from far away, from lands to the north and west, and from as far south as Egypt.”"
      }
