      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "诸天啊，要欢呼！ 大地啊，要快乐！ 群山啊，要歌唱！ 因为耶和华必安慰祂的子民， 怜悯祂受苦的百姓。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "諸天哪！你們要歡呼。大地啊！你當快樂。 眾山啊！你們要發聲歡呼。 因為耶和華已經安慰了他的子民， 也必憐憫他受困苦的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "諸天哪，應當歡呼！ 大地啊，應當快樂！ 眾山哪，應當發聲歌唱！ 因為耶和華已經安慰他的百姓， 也要憐恤他困苦之民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Shout for joy, O heavens! And rejoice, O earth! Break forth into joyful shouting, O mountains! For the Lord has comforted His people And will have compassion on His afflicted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Shout for joy, you heavens; rejoice, you earth; burst into song, you mountains! For the Lord comforts his people and will have compassion on his afflicted ones."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Sing for joy, O heavens! Rejoice, O earth! Burst into song, O mountains! For the Lord has comforted his people and will have compassion on them in their suffering."
      }
    },
