    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂使我的口像利剑， 用祂的手护庇我； 祂使我成为磨亮的箭， 把我藏在祂的箭囊中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他使我的口如快刀， 把我藏在他手的陰影之下； 他又使我成為磨亮的箭， 把我藏在他的箭袋裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他使我的口如快刀， 將我藏在他手蔭之下； 又使我成為磨亮的箭， 將我藏在他箭袋之中；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has made My mouth like a sharp sword, In the shadow of His hand He has concealed Me; And He has also made Me a select arrow, He has hidden Me in His quiver."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He made my mouth like a sharpened sword, in the shadow of his hand he hid me; he made me into a polished arrow and concealed me in his quiver."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He made my words of judgment as sharp as a sword. He has hidden me in the shadow of his hand. I am like a sharp arrow in his quiver."
      }
    },
