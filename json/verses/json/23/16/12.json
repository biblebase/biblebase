    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押人上丘坛祭拜， 却落得筋疲力尽； 在庙宇里祷告， 却毫无用处。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到那時，摩押人雖朝見他們的神， 雖在高處弄得疲乏不堪， 又到他們的聖所去禱告， 卻總不得亨通。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩押人朝見的時候，在高處疲乏，又到他聖所祈禱，也不蒙應允。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So it will come about when Moab presents himself, When he wearies himself upon his high place And comes to his sanctuary to pray, That he will not prevail."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Moab appears at her high place, she only wears herself out; when she goes to her shrine to pray, it is to no avail."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people of Moab will worship at their pagan shrines, but it will do them no good. They will cry to the gods in their temples, but no one will be able to save them."
      }
    },
