    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们听见从地极传来歌唱声： “荣耀归给公义者。” 我说：“我完了！ 我完了！我有祸了！ 因为诡诈的仍然在行诡诈， 极为猖獗。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們聽見從地極有歌聲唱著： “榮耀歸於義人。” 我卻說：“我消瘦了，我消瘦了！我有禍了！ 詭詐的仍在行詭詐， 詭詐的仍在大行詭詐。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們聽見從地極有人歌唱，說： 榮耀歸於義人。 我卻說：我消滅了！ 我消滅了，我有禍了！ 詭詐的行詭詐； 詭詐的大行詭詐。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the ends of the earth we hear songs, “Glory to the Righteous One,” But I say, “Woe to me! Woe to me! Alas for me! The treacherous deal treacherously, And the treacherous deal very treacherously.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the ends of the earth we hear singing: “Glory to the Righteous One.” But I said, “I waste away, I waste away! Woe to me! The treacherous betray! With treachery the treacherous betray!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We hear songs of praise from the ends of the earth, songs that give glory to the Righteous One! But my heart is heavy with grief. Weep for me, for I wither away. Deceit still prevails, and treachery is everywhere."
      }
    },
