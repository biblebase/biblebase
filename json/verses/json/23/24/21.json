    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到那日，耶和华必在天上惩罚天上的众权能者， 在地上惩罚地上的君王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到那日， 耶和華必在高天之上懲罰天上的眾軍， 又在地上懲罰地上的列王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到那日，耶和華在高處必懲罰高處的眾軍， 在地上必懲罰地上的列王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So it will happen in that day, That the Lord will punish the host of heaven on high, And the kings of the earth on earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In that day the Lord will punish the powers in the heavens above and the kings on the earth below."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In that day the Lord will punish the gods in the heavens and the proud rulers of the nations on earth."
      }
