    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人们因无酒而在街上喊叫； 一切欢乐变作哀愁， 地上喜乐不复存在。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有人在街上因無酒呼喊； 一切喜樂都已過去， 地上的喜樂都消逝了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在街上因酒有悲歎的聲音； 一切喜樂變為昏暗； 地上的歡樂歸於無有。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is an outcry in the streets concerning the wine; All joy turns to gloom. The gaiety of the earth is banished."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the streets they cry out for wine; all joy turns to gloom, all joyful sounds are banished from the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Mobs gather in the streets, crying out for wine. Joy has turned to gloom. Gladness has been banished from the land."
      }
    },
