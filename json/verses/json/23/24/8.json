    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "欢快的鼓声止息， 悦耳的琴音消失， 宴乐的喧闹不复听闻。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "擊鼓的喜樂止息， 宴樂者的喧嘩停止， 彈琴的喜樂也止息了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "擊鼓之樂止息； 宴樂人的聲音完畢， 彈琴之樂也止息了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The gaiety of tambourines ceases, The noise of revelers stops, The gaiety of the harp ceases."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The joyful timbrels are stilled, the noise of the revelers has stopped, the joyful harp is silent."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The cheerful sound of tambourines is stilled; the happy cries of celebration are heard no more. The melodious chords of the harp are silent."
      }
    },
