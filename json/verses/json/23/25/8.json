    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂必永远吞灭死亡。主耶和华必擦去各人脸上的眼泪，从世上除掉祂子民的羞辱。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他要吞滅死亡，直到永遠；主耶和華必擦去各人臉上的眼淚，又從地上除掉他子民的羞辱；這是耶和華說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他已經吞滅死亡直到永遠。主耶和華必擦去各人臉上的眼淚，又除掉普天下他百姓的羞辱，因為這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will swallow up death for all time, And the Lord God will wipe tears away from all faces, And He will remove the reproach of His people from all the earth; For the Lord has spoken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "he will swallow up death forever. The Sovereign Lord will wipe away the tears from all faces; he will remove his people’s disgrace from all the earth. The Lord has spoken."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will swallow up death forever! The Sovereign Lord will wipe away all tears. He will remove forever all insults and mockery against his land and people. The Lord has spoken!"
      }
    },
