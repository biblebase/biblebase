    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在这山上，祂必除去遮蔽万民的面纱和笼罩万国的幔子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又必在這山上除滅那遮蓋萬民的面巾，和那遮蓋列國的帕子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又必在這山上除滅遮蓋萬民之物和遮蔽萬國蒙臉的帕子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And on this mountain He will swallow up the covering which is over all peoples, Even the veil which is stretched over all nations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On this mountain he will destroy the shroud that enfolds all peoples, the sheet that covers all nations;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There he will remove the cloud of gloom, the shadow of death that hangs over the earth."
      }
    },
