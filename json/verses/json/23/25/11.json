    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们必在里面张开手臂，好像张开手臂游泳的人一样。耶和华必摧毁他们的骄傲，挫败他们手中的诡计。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們必在其中伸開手來，好像游泳者伸開手來游泳一樣；但耶和華要使他們的驕傲和他們手所行的詭計，一同敗落。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必在其中伸開手，好像洑水的伸開手洑水一樣；但耶和華必使他的驕傲和他手所行的詭計一併敗落。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he will spread out his hands in the middle of it As a swimmer spreads out his hands to swim, But the Lord will lay low his pride together with the trickery of his hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will stretch out their hands in it, as swimmers stretch out their hands to swim. God will bring down their pride despite the cleverness of their hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God will push down Moab’s people as a swimmer pushes down water with his hands. He will end their pride and all their evil works."
      }
    },
