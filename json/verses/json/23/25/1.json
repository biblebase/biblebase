    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊！你是我的上帝，我要尊崇你，赞美你的名。你信实无比， 按你古时定下的计划行了奇妙的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你是我的　神， 我要尊崇你，稱謝你的名， 因為你以信實真誠作成了奇妙的事， 就是你遠古以前所計劃的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你是我的神； 我要尊崇你，我要稱讚你的名。 因為你以忠信誠實行過奇妙的事， 成就你古時所定的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, You are my God; I will exalt You, I will give thanks to Your name; For You have worked wonders, Plans formed long ago, with perfect faithfulness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, you are my God; I will exalt you and praise your name, for in perfect faithfulness you have done wonderful things, things planned long ago."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, I will honor and praise your name, for you are my God. You do such wonderful things! You planned them long ago, and now you have accomplished them."
      }
    },
