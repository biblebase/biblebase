    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我使假先知的预兆无法应验， 使占卜者丑态百出； 我颠覆智者的知识， 使之愚不可及。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我使說假話者的兆頭失效， 使占卜者都變成愚昧人； 我使有智慧的人後退， 使他們的知識變為愚拙。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使說假話的兆頭失效， 使占卜的癲狂， 使智慧人退後， 使他的知識變為愚拙；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Causing the omens of boasters to fail, Making fools out of diviners, Causing wise men to draw back And turning their knowledge into foolishness,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who foils the signs of false prophets and makes fools of diviners, who overthrows the learning of the wise and turns it into nonsense,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I expose the false prophets as liars and make fools of fortune-tellers. I cause the wise to give bad advice, thus proving them to be fools."
      }
    },
