    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们愚昧无知，因为眼睛被封住以致看不见，心窍被塞住以致不明白。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們並不知道，也不明白； 因為耶和華閉住他們的眼睛，使他們不能看見， 塞住他們的心，使他們不能明白。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們不知道，也不思想；因為耶和華閉住他們的眼，不能看見，塞住他們的心，不能明白。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They do not know, nor do they understand, for He has smeared over their eyes so that they cannot see and their hearts so that they cannot comprehend."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They know nothing, they understand nothing; their eyes are plastered over so they cannot see, and their minds closed so they cannot understand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Such stupidity and ignorance! Their eyes are closed, and they cannot see. Their minds are shut, and they cannot think."
      }
    },
