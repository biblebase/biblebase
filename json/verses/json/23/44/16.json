    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他用一半木头烤肉，饱餐一顿，并用火取暖，说：“啊！我这里有火，真暖和。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把木頭的一半放在火裡燃燒； 在上面烤肉； 吃了烤肉，並且吃飽了， 就自己烤火取暖，說： “哈哈！我暖和了，我看見火了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他把一分燒在火中，把一分烤肉吃飽。自己烤火說：「啊哈，我暖和了，我見火了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Half of it he burns in the fire; over this half he eats meat as he roasts a roast and is satisfied. He also warms himself and says, “Aha! I am warm, I have seen the fire.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Half of the wood he burns in the fire; over it he prepares his meal, he roasts his meat and eats his fill. He also warms himself and says, “Ah! I am warm; I see the fire.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He burns part of the tree to roast his meat and to keep himself warm. He says, “Ah, that fire feels good.”"
      }
    },
