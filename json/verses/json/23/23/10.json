    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他施的人民啊， 要像尼罗河一样穿行在自己的土地上， 因为再没有人压制你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他施的居民哪！要像尼羅河一般流遍你的地， 再沒有限制了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他施的民哪， 可以流行你的地，好像尼羅河； 不再有腰帶拘緊你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Overflow your land like the Nile, O daughter of Tarshish, There is no more restraint."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Till your land as they do along the Nile, Daughter Tarshish, for you no longer have a harbor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Come, people of Tarshish, sweep over the land like the flooding Nile, for Tyre is defenseless."
      }
    },
