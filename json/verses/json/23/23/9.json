        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "是万军之耶和华定的， 为要摧毁因荣耀而生的骄傲， 羞辱世上的尊贵者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這是萬軍之耶和華所定的旨意， 要凌辱那些因榮美而有狂傲， 使地上所有的尊貴人被藐視。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "是萬軍之耶和華所定的！ 為要污辱一切高傲的榮耀， 使地上一切的尊貴人被藐視。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord of hosts has planned it, to defile the pride of all beauty, To despise all the honored of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord Almighty planned it, to bring down her pride in all her splendor and to humble all who are renowned on the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord of Heaven’s Armies has done it to destroy your pride and bring low all earth’s nobility."
      }
    },
