    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为耶和华是我们的审判官， 是我们的立法者， 是我们的君王， 是我们的拯救者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華是我們的審判者， 耶和華是我們的立法者， 耶和華是我們的君王， 他必拯救我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，耶和華是審判我們的； 耶和華是給我們設律法的； 耶和華是我們的王； 他必拯救我們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord is our judge, The Lord is our lawgiver, The Lord is our king; He will save us—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the Lord is our judge, the Lord is our lawgiver, the Lord is our king; it is he who will save us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Lord is our judge, our lawgiver, and our king. He will care for us and save us."
      }
