    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的战利品必被拿走， 就像谷物被蝗虫吃掉一样； 人们像蝗虫一样扑向他们的战利品。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "列國啊！你們所擄掠的必被收取， 像螞蚱收取禾稼一樣； 蝗蟲怎樣為食物忙碌走動，人也怎樣在擄物上忙亂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們所擄的必被斂盡， 好像螞蚱吃盡禾稼。 人要蹦在其上，好像蝗蟲一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your spoil is gathered as the caterpillar gathers; As locusts rushing about men rush about on it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your plunder, O nations, is harvested as by young locusts; like a swarm of locusts people pounce on it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Just as caterpillars and locusts strip the fields and vines, so the fallen army of Assyria will be stripped!"
