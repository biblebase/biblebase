      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，他們的豪傑在外頭哀號； 求和的使臣痛痛哭泣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, their brave men cry in the streets, The ambassadors of peace weep bitterly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Look, their brave men cry aloud in the streets; the envoys of peace weep bitterly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But now your brave warriors weep in public. Your ambassadors of peace cry in bitter disappointment."
      }
