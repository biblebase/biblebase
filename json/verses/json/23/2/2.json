    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在末后的日子，耶和华殿所在的山必耸立， 高过群山，超过万岭， 万族都要涌向它。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在末後的日子， 耶和華殿的山， 必被堅立，超乎眾山， 必被高舉，過於萬嶺； 萬國都要流歸這山。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "末後的日子，耶和華殿的山必堅立， 超乎諸山，高舉過於萬嶺； 萬民都要流歸這山。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now it will come about that In the last days The mountain of the house of the Lord Will be established as the chief of the mountains, And will be raised above the hills; And all the nations will stream to it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the last days the mountain of the Lord’s temple will be established as the highest of the mountains; it will be exalted above the hills, and all nations will stream to it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In the last days, the mountain of the Lord’s house will be the highest of all— the most important place on earth. It will be raised above the other hills, and people from all over the world will stream there to worship."
      }
    },
