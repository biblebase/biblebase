    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到那日，狂妄的人必被挫败， 骄傲的人必遭贬抑， 唯独耶和华受尊崇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人的高傲必變為謙虛， 人的狂妄都必降為卑微， 在那日，唯獨耶和華被高舉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "驕傲的必屈膝； 狂妄的必降卑。 在那日，惟獨耶和華被尊崇；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The pride of man will be humbled And the loftiness of men will be abased; And the Lord alone will be exalted in that day,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The arrogance of man will be brought low and human pride humbled; the Lord alone will be exalted in that day,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Human pride will be humbled, and human arrogance will be brought down. Only the Lord will be exalted on that day of judgment."
      }
    },
