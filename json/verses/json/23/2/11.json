    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到那日，狂妄的人将被挫败， 骄傲的人将遭贬抑； 唯独耶和华将受尊崇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到那日，眼目高傲的人必降卑， 性情驕傲的人也必俯首， 唯獨耶和華被高舉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到那日，眼目高傲的必降為卑； 性情狂傲的都必屈膝； 惟獨耶和華被尊崇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The proud look of man will be abased And the loftiness of man will be humbled, And the Lord alone will be exalted in that day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The eyes of the arrogant will be humbled and human pride brought low; the Lord alone will be exalted in that day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Human pride will be brought down, and human arrogance will be humbled. Only the Lord will be exalted on that day of judgment."
      }
    },
