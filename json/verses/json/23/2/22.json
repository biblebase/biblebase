    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要再倚靠世人， 他们的生命不过是一口气， 他们有什么值得称道的呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不要倚靠世人， 他的鼻孔裡只有一口氣息， 他實在算得甚麼呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們休要倚靠世人。 他鼻孔裏不過有氣息； 他在一切事上可算甚麼呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Stop regarding man, whose breath of life is in his nostrils; For why should he be esteemed?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Stop trusting in mere humans, who have but a breath in their nostrils. Why hold them in esteem?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t put your trust in mere humans. They are as frail as breath. What good are they?"
      }
    },
