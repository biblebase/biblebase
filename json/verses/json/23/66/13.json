    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "母亲怎样抚慰孩子， 我也怎样抚慰你们， 使你们在耶路撒冷得到安慰。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人怎樣受他母親的安慰，我也怎樣安慰你們； 你們要在耶路撒冷得安慰。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "母親怎樣安慰兒子，我就照樣安慰你們； 你們也必因耶路撒冷得安慰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As one whom his mother comforts, so I will comfort you; And you will be comforted in Jerusalem.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As a mother comforts her child, so will I comfort you; and you will be comforted over Jerusalem.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will comfort you there in Jerusalem as a mother comforts her child.”"
      }
