    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为耶和华要用火和剑审判世人，杀死许多人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華必用火和刀劍， 在所有的人身上施行審判， 被耶和華所殺的人必定很多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為耶和華在一切有血氣的人身上， 必以火與刀施行審判； 被耶和華所殺的必多。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord will execute judgment by fire And by His sword on all flesh, And those slain by the Lord will be many."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For with fire and with his sword the Lord will execute judgment on all people, and many will be those slain by the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord will punish the world by fire and by his sword. He will judge the earth, and many will be killed by him."
      }
    },
