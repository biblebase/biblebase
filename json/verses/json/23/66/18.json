    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我知道他们的所作所为和所思所想。我要召集万邦万民，使他们看见我的荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我知道他們的行為和意念；我要來，聚集萬國萬族；他們都必來看我的榮耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我知道他們的行為和他們的意念。時候將到，我必將萬民萬族聚來，看見我的榮耀，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“For I know their works and their thoughts; the time is coming to gather all nations and tongues. And they shall come and see My glory."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“And I, because of what they have planned and done, am about to come and gather the people of all nations and languages, and they will come and see my glory."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I can see what they are doing, and I know what they are thinking. So I will gather all nations and peoples together, and they will see my glory."
      }
