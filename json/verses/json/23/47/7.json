    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你声称要永远做王后， 不把这些事放在心上， 也不思想以后的结局。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你曾說：‘我必永遠作王后！’ 所以你不把這些事放在心上， 也不思想這事的結局。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你自己說：我必永為主母， 所以你不將這事放在心上， 也不思想這事的結局。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet you said, ‘I will be a queen forever.’ These things you did not consider Nor remember the outcome of them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You said, ‘I am forever— the eternal queen!’ But you did not consider these things or reflect on what might happen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You said, ‘I will reign forever as queen of the world!’ You did not reflect on your actions or think about their consequences."
      }
