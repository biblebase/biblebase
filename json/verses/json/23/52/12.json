    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们不必匆忙离开， 也不用奔逃， 因为耶和华必走在你们前面， 以色列的上帝必作你们的后盾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們出來，不必著急； 你們行走，也不必奔逃， 因為耶和華要走在你們前面， 以色列的　神必作你們的後盾。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們出來必不致急忙， 也不致奔逃。 因為，耶和華必在你們前頭行； 以色列的神必作你們的後盾。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But you will not go out in haste, Nor will you go as fugitives; For the Lord will go before you, And the God of Israel will be your rear guard."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you will not leave in haste or go in flight; for the Lord will go before you, the God of Israel will be your rear guard."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will not leave in a hurry, running for your lives. For the Lord will go ahead of you; yes, the God of Israel will protect you from behind."
      }
    },
