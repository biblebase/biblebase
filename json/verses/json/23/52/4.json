    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华说：“起先我的子民到埃及寄居，后来亚述人无缘无故地欺压他们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶和華這樣說： “我的子民先前曾下到埃及，在那裡寄居； 後來有亞述人無緣無故地欺壓了他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華如此說：起先我的百姓下到埃及，在那裏寄居，又有亞述人無故欺壓他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus says the Lord God, “My people went down at the first into Egypt to reside there; then the Assyrian oppressed them without cause."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this is what the Sovereign Lord says: “At first my people went down to Egypt to live; lately, Assyria has oppressed them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Sovereign Lord says: “Long ago my people chose to live in Egypt. Now they are oppressed by Assyria."
      }
    },
