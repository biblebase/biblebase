    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂必审问祂子民中的长老和首领，对他们说： “你们毁坏了我的葡萄园——我的子民， 家里堆满了从穷人那里掠夺的东西。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必審問他子民中的長老和領袖，說： “那吞盡葡萄園的，就是你們； 從貧窮人那裡掠奪的，都在你們家中。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華必審問他民中的長老和首領，說： 吃盡葡萄園果子的就是你們； 向貧窮人所奪的都在你們家中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord enters into judgment with the elders and princes of His people, “It is you who have devoured the vineyard; The plunder of the poor is in your houses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord enters into judgment against the elders and leaders of his people: “It is you who have ruined my vineyard; the plunder from the poor is in your houses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord comes forward to pronounce judgment on the elders and rulers of his people: “You have ruined Israel, my vineyard. Your houses are filled with things stolen from the poor."
      }
    },
