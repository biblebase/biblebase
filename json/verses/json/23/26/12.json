    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你必赐我们平安， 因为我们所做的事都是你为我们成就的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你必使我們得平安， 因為我們所作的，都是你為我們成就的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你必派定我們得平安， 因為我們所做的事都是你給我們成就的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Lord, You will establish peace for us, Since You have also performed for us all our works."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, you establish peace for us; all that we have accomplished you have done for us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, you will grant us peace; all we have accomplished is really from you."
      }
    },
