    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，我们等候你， 遵从你的法令， 我们心里渴慕尊崇你的名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！我們等候你那審判的路徑； 你的名字和稱號，都是我們心裡所愛慕的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我們在你行審判的路上等候你； 我們心裏所羨慕的是你的名， 就是你那可記念的名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Indeed, while following the way of Your judgments, O Lord, We have waited for You eagerly; Your name, even Your memory, is the desire of our souls."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yes, Lord, walking in the way of your laws, we wait for you; your name and renown are the desire of our hearts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, we show our trust in you by obeying your laws; our heart’s desire is to glorify your name."
      }
    },
