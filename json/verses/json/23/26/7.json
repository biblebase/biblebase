    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人的道路是平坦的， 公正的主啊，你必修平义人所走的道路。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的路是正直的， 正直的主啊！你必修平義人的路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人的道是正直的； 你為正直的主，必修平義人的路。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The way of the righteous is smooth; O Upright One, make the path of the righteous level."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The path of the righteous is level; you, the Upright One, make the way of the righteous smooth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But for those who are righteous, the way is not steep and rough. You are a God who does what is right, and you smooth out the path ahead of them."
      }
