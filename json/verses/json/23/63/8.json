    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂说：“他们的确是我的子民， 不会背叛我。” 因此，祂做他们的救主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他曾說：“他們真是我的子民， 不行虛假的兒子”， 於是，在他們的一切苦難中，他就作了他們的拯救者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：他們誠然是我的百姓， 不行虛假的子民； 這樣，他就作了他們的救主。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He said, “Surely, they are My people, Sons who will not deal falsely.” So He became their Savior."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said, “Surely they are my people, children who will be true to me”; and so he became their Savior."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He said, “They are my very own people. Surely they will not betray me again.” And he became their Savior."
      }
    },
