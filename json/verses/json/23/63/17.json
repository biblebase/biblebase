    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊！你为何使我们偏离你的路， 使我们硬着心不敬畏你呢？ 求你为了你的仆人， 你的产业——以色列各支派而改变心意！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你為甚麼使我們走錯， 離開你的道路？ 使我們的心剛硬，不敬畏你呢？ 求你為你眾僕人的緣故，為你產業的各支派的緣故，回心轉意吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你為何使我們走差離開你的道， 使我們心裏剛硬、不敬畏你呢？ 求你為你僕人， 為你產業支派的緣故，轉回來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why, O Lord, do You cause us to stray from Your ways And harden our heart from fearing You? Return for the sake of Your servants, the tribes of Your heritage."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why, Lord, do you make us wander from your ways and harden our hearts so we do not revere you? Return for the sake of your servants, the tribes that are your inheritance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, why have you allowed us to turn from your path? Why have you given us stubborn hearts so we no longer fear you? Return and help us, for we are your servants, the tribes that are your special possession."
      }
    },
