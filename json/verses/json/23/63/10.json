    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们却叛逆，使祂的圣灵忧伤。 于是，祂与他们为敌， 亲自攻击他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可是，他們竟然悖逆， 使他的聖靈憂傷； 所以他轉作他們的仇敵， 親自攻擊他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們竟悖逆，使主的聖靈擔憂。 他就轉作他們的仇敵， 親自攻擊他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But they rebelled And grieved His Holy Spirit; Therefore He turned Himself to become their enemy, He fought against them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet they rebelled and grieved his Holy Spirit. So he turned and became their enemy and he himself fought against them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But they rebelled against him and grieved his Holy Spirit. So he became their enemy and fought against them."
      }
