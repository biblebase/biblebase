    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂说：“你去告诉以色列人， “‘你们听了又听，却不明白； 看了又看，却不领悟。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“你去告訴這子民，說： ‘你們聽了又聽，卻不明白； 看了又看，卻不曉得。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：「你去告訴這百姓說： 你們聽是要聽見，卻不明白； 看是要看見，卻不曉得。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said, “Go, and tell this people: ‘Keep on listening, but do not perceive; Keep on looking, but do not understand.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said, “Go and tell this people: “ ‘Be ever hearing, but never understanding; be ever seeing, but never perceiving.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And he said, “Yes, go, and say to this people, ‘Listen carefully, but do not understand. Watch closely, but learn nothing.’"
      }
