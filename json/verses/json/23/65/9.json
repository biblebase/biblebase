    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我必使雅各的后裔兴起， 使犹大人兴起， 继承我的群山。 我拣选的子民必拥有那片土地， 我的仆人必在那里安居。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要從雅各中領出後裔， 從猶大中領出承受我眾山的； 我揀選的人必承受那地為業， 我的眾僕人必在那裡居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必從雅各中領出後裔， 從猶大中領出承受我眾山的。 我的選民必承受； 我的僕人要在那裏居住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will bring forth offspring from Jacob, And an heir of My mountains from Judah; Even My chosen ones shall inherit it, And My servants will dwell there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will bring forth descendants from Jacob, and from Judah those who will possess my mountains; my chosen people will inherit them, and there will my servants live."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will preserve a remnant of the people of Israel and of Judah to possess my land. Those I choose will inherit it, and my servants will live there."
      }
    },
