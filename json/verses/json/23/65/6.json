    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，这些都记录在我面前， 我不会再保持缄默， 我要报应他们，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！這都寫在我面前， 我必不緘默不言，我非報應不可， 我必報應在他們的懷中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, it is written before Me, I will not keep silent, but I will repay; I will even repay into their bosom,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“See, it stands written before me: I will not keep silent but will pay back in full; I will pay it back into their laps—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Look, my decree is written out in front of me: I will not stand silent; I will repay them in full! Yes, I will repay them—"
      }
    },
