    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们拿谁与我相比， 使之与我同等呢？ 你们把谁比作我， 使之与我相同呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要把誰與我相比，誰與我相似呢？ 你們把誰與我比較，好使我們同等呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們將誰與我相比，與我同等， 可以與我比較，使我們相同呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“To whom would you liken Me And make Me equal and compare Me, That we would be alike?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“With whom will you compare me or count me equal? To whom will you liken me that we may be compared?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“To whom will you compare me? Who is my equal?"
      }
