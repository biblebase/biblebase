    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它们一同屈膝降服， 不但救不了被掳的巴比伦人， 自己也被掳去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "它們一同彎腰、俯伏， 不能保護重負， 它們自己反倒被擄去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "都一同彎腰屈身， 不能保全重馱， 自己倒被擄去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They stooped over, they have bowed down together; They could not rescue the burden, But have themselves gone into captivity."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They stoop and bow down together; unable to rescue the burden, they themselves go off into captivity."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Both the idols and their owners are bowed down. The gods cannot protect the people, and the people cannot protect the gods. They go off into captivity together."
      }
    },
