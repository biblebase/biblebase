    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我不会永远指责， 也不会一直发怒， 以免世人——我所造的生灵昏倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必不永遠爭辯， 也不長久發怒， 因為這樣，人的靈，就是我所造的人，在我面前就要發昏。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必不永遠相爭，也不長久發怒， 恐怕我所造的人與靈性都必發昏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I will not contend forever, Nor will I always be angry; For the spirit would grow faint before Me, And the breath of those whom I have made."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will not accuse them forever, nor will I always be angry, for then they would faint away because of me— the very people I have created."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I will not fight against you forever; I will not always be angry. If I were, all people would pass away— all the souls I have made."
      }
    },
