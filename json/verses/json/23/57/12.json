    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要揭露你们的所谓公义行为， 它们对你们毫无益处。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
