    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然遥远的路途使你们疲倦， 你们却从不放弃。 你们找到了新的力量， 所以没有晕倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你因路途遙遠而困乏， 卻不說：“沒有希望了！” 因為你找著了生命的力量， 所以你不覺得疲倦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你因路遠疲倦， 卻不說這是枉然； 你以為有復興之力， 所以不覺疲憊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You were tired out by the length of your road, Yet you did not say, ‘It is hopeless.’ You found renewed strength, Therefore you did not faint."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You wearied yourself by such going about, but you would not say, ‘It is hopeless.’ You found renewal of your strength, and so you did not faint."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You grew weary in your search, but you never gave up. Desire gave you renewed strength, and you did not grow weary."
      }
    },
