    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们带着橄榄油和许多香料去拜假神， 差遣使者到远方， 甚至去阴间寻找神明。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你帶著油走到君王那裡， 並且加多了你的香料， 差派使者到遠方去， 你甚至自己下到陰間去！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你把油帶到王那裏， 又多加香料， 打發使者往遠方去， 自卑自賤直到陰間，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have journeyed to the king with oil And increased your perfumes; You have sent your envoys a great distance And made them go down to Sheol."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You went to Molek with olive oil and increased your perfumes. You sent your ambassadors far away; you descended to the very realm of the dead!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have gone to Molech with olive oil and many perfumes, sending your agents far and wide, even to the world of the dead."
      }
    },
