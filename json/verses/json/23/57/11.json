    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们惧怕谁， 以致对我说谎， 不把我放在心上，忘记我？ 你们不再敬畏我， 是因为我长期沉默吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你怕誰？你因誰而懼怕， 以致你對我說謊呢？你不記念我， 也不把我放在心上， 是不是因為我長久緘默， 你就不懼怕我呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你怕誰？因誰恐懼？ 竟說謊，不記念我， 又不將這事放在心上。 我不是許久閉口不言， 你仍不怕我嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Of whom were you worried and fearful When you lied, and did not remember Me Nor give Me a thought? Was I not silent even for a long time So you do not fear Me?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Whom have you so dreaded and feared that you have not been true to me, and have neither remembered me nor taken this to heart? Is it not because I have long been silent that you do not fear me?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Are you afraid of these idols? Do they terrify you? Is that why you have lied to me and forgotten me and my words? Is it because of my long silence that you no longer fear me?"
      }
    },
