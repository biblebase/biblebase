    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列啊，你的人民虽多如海沙，将只有剩余的人归回。充满公义的毁灭之事已定。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列啊！你的人民雖然像海沙那樣多，卻只有餘剩的人可以歸回， 因為滅絕的事已經定了，公義必如洪水氾濫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列啊，你的百姓雖多如海沙，惟有剩下的歸回。原來滅絕的事已定，必有公義施行，如水漲溢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For though your people, O Israel, may be like the sand of the sea, Only a remnant within them will return; A destruction is determined, overflowing with righteousness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Though your people be like the sand by the sea, Israel, only a remnant will return. Destruction has been decreed, overwhelming and righteous."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But though the people of Israel are as numerous as the sand of the seashore, only a remnant of them will return. The Lord has rightly decided to destroy his people."
      }
    },
