    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“亚述王有祸了！ 他是我的愤怒之棍， 他手中拿着我发烈怒的杖。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞述啊！我怒氣的棍子，有禍了。 他們手中拿著我忿怒的杖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞述是我怒氣的棍， 手中拿我惱恨的杖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe to Assyria, the rod of My anger And the staff in whose hands is My indignation,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Woe to the Assyrian, the rod of my anger, in whose hand is the club of my wrath!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What sorrow awaits Assyria, the rod of my anger. I use it as a club to express my anger."
      }
    },
