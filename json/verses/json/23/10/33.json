    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，主——万军之耶和华要以大能削去树枝。 高大的树必被斩断， 挺拔的大树必被砍倒，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！主萬軍之耶和華在可怕的震撼聲中，把樹枝削去； 身量高的必被砍下， 高大的必被伐倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，主－萬軍之耶和華 以驚嚇削去樹枝； 長高的必被砍下， 高大的必被伐倒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, the Lord, the God of hosts, will lop off the boughs with a terrible crash; Those also who are tall in stature will be cut down And those who are lofty will be abased."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See, the Lord, the Lord Almighty, will lop off the boughs with great power. The lofty trees will be felled, the tall ones will be brought low."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But look! The Lord, the Lord of Heaven’s Armies, will chop down the mighty tree of Assyria with great power! He will cut down the proud. That lofty tree will be brought down."
      }
    },
