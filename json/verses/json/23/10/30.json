      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "迦琳人啊，高声喊叫吧！ 莱煞人啊，可怜的亚拿突人啊， 留心听吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "迦琳的居民哪，高聲叫喊吧！ 萊煞人和可憐的亞拿突啊，留心聽吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "迦琳的居民哪，要高聲呼喊！ 萊煞人哪，須聽！ 哀哉！困苦的亞拿突啊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Cry aloud with your voice, O daughter of Gallim! Pay attention, Laishah and wretched Anathoth!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Cry out, Daughter Gallim! Listen, Laishah! Poor Anathoth!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Scream in terror, you people of Gallim! Shout out a warning to Laishah. Oh, poor Anathoth!"
      }
    },
