    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到那日，祂必除去亚述人加在你们肩头的重担和颈上的轭；那轭必因你们肥壮而折断。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到那日，亞述人的重壓必從你們的肩頭上挪開，他們的軛必離開你們的頸項；那軛必因肥壯的緣故折斷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到那日，亞述王的重擔必離開你的肩頭；他的軛必離開你的頸項；那軛也必因肥壯的緣故撐斷。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So it will be in that day, that his burden will be removed from your shoulders and his yoke from your neck, and the yoke will be broken because of fatness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In that day their burden will be lifted from your shoulders, their yoke from your neck; the yoke will be broken because you have grown so fat."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In that day the Lord will end the bondage of his people. He will break the yoke of slavery and lift it from their shoulders."
      }
    },
