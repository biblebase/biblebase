    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我心慌意乱，惊惧不堪， 我期盼的黄昏却令我恐惧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的心迷糊，驚恐威嚇著我； 我所愛慕的黃昏，竟使我戰兢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我心慌張，驚恐威嚇我。 我所羨慕的黃昏，變為我的戰兢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My mind reels, horror overwhelms me; The twilight I longed for has been turned for me into trembling."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My heart falters, fear makes me tremble; the twilight I longed for has become a horror to me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My mind reels and my heart races. I longed for evening to come, but now I am terrified of the dark."
      }
    },
