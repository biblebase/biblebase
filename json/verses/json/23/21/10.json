    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，我说：“我的百姓啊！ 你们像场上被碾、被筛的谷物， 现在我把从以色列的上帝——万军之耶和华那里听见的都告诉你们了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我那被打的禾稼啊！我禾場上的穀啊！ 我從萬軍之耶和華，以色列的　神那裡聽見的， 都告訴你們了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我被打的禾稼，我場上的穀啊， 我從萬軍之耶和華－ 以色列的神那裏所聽見的，都告訴你們了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O my threshed people, and my afflicted of the threshing floor! What I have heard from the Lord of hosts, The God of Israel, I make known to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My people who are crushed on the threshing floor, I tell you what I have heard from the Lord Almighty, from the God of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O my people, threshed and winnowed, I have told you everything the Lord of Heaven’s Armies has said, everything the God of Israel has told me."
      }
    },
