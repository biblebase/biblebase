    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们摆设宴席， 坐在地毯上又吃又喝。 突然有人大喊： “官长啊，起来擦亮盾牌，备战吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們擺設筵席，鋪張地氈，又吃又喝。 領袖們，起來吧！用油抹淨盾牌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們擺設筵席， 派人守望，又吃又喝。 首領啊，你們起來， 用油抹盾牌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They set the table, they spread out the cloth, they eat, they drink; “Rise up, captains, oil the shields,”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They set the tables, they spread the rugs, they eat, they drink! Get up, you officers, oil the shields!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Look! They are preparing a great feast. They are spreading rugs for people to sit on. Everyone is eating and drinking. But quick! Grab your shields and prepare for battle. You are being attacked!"
      }
    },
