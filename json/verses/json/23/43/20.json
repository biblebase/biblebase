    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "旷野的走兽必尊崇我， 其中有豺狼和鸵鸟， 因为我使旷野有甘泉， 使沙漠有江河， 供我拣选的子民饮用。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "野地的走獸必尊重我， 野狗和鴕鳥也必這樣， 因為我使曠野有水， 使荒地有江河， 好使我揀選的子民有水喝，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "野地的走獸必尊重我； 野狗和鴕鳥也必如此。 因我使曠野有水， 使沙漠有河， 好賜給我的百姓、我的選民喝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The beasts of the field will glorify Me, The jackals and the ostriches, Because I have given waters in the wilderness And rivers in the desert, To give drink to My chosen people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wild animals honor me, the jackals and the owls, because I provide water in the wilderness and streams in the wasteland, to give drink to my people, my chosen,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wild animals in the fields will thank me, the jackals and owls, too, for giving them water in the desert. Yes, I will make rivers in the dry wasteland so my chosen people can be refreshed."
      }
    },
