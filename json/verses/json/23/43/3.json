    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为我是你的上帝耶和华， 是以色列的圣者，是你的救主。 我用埃及作你的赎价， 使古实和西巴代替你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為我是耶和華你的　神， 是以色列的聖者，你的拯救者； 我使埃及作你的贖價， 使古實和示巴作你的替身。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為我是耶和華－你的神， 是以色列的聖者－你的救主； 我已經使埃及作你的贖價， 使古實和西巴代替你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I am the Lord your God, The Holy One of Israel, your Savior; I have given Egypt as your ransom, Cush and Seba in your place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
