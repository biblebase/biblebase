    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你并没有花钱买菖蒲献给我， 也没有用祭物的脂肪满足我， 反而使我因你的罪恶而受累， 因你的过犯而烦扰。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你沒有用銀子為我買菖蒲， 也沒有用祭物的脂肪使我飽足； 相反地，你用你的罪使我勞累， 你用你的罪孽使我厭煩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你沒有用銀子為我買菖蒲， 也沒有用祭物的脂油使我飽足； 倒使我因你的罪惡服勞， 使我因你的罪孽厭煩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have bought Me not sweet cane with money, Nor have you filled Me with the fat of your sacrifices; Rather you have burdened Me with your sins, You have wearied Me with your iniquities."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have not bought any fragrant calamus for me, or lavished on me the fat of your sacrifices. But you have burdened me with your sins and wearied me with your offenses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have not brought me fragrant calamus or pleased me with the fat from sacrifices. Instead, you have burdened me with your sins and wearied me with your faults."
      }
    },
