    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘我从太初就是上帝。 无人能逃脱我的手。 谁能阻挠我行事呢？’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "自頭一天以來，我就是‘那位’； 誰也不能救人脫離我的手； 我要行事，誰能攔阻呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "自從有日子以來，我就是神； 誰也不能救人脫離我手。 我要行事誰能阻止呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even from eternity I am He, And there is none who can deliver out of My hand; I act and who can reverse it?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yes, and from ancient days I am he. No one can deliver out of my hand. When I act, who can reverse it?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“From eternity to eternity I am God. No one can snatch anyone out of my hand. No one can undo what I have done.”"
      }
    },
