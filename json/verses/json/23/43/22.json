    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“雅各啊，你却没有求告我； 以色列啊，你竟然厌烦我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然而，雅各啊！你並沒有呼求我。 以色列啊！你竟厭煩我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雅各啊，你並沒有求告我； 以色列啊，你倒厭煩我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Yet you have not called on Me, O Jacob; But you have become weary of Me, O Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Yet you have not called on me, Jacob, you have not wearied yourselves for me, Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But, dear family of Jacob, you refuse to ask for my help. You have grown tired of me, O Israel!"
      }
    },
