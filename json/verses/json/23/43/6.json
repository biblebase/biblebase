    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要对北方说，‘交出来！’ 对南方说，‘不要扣留！’ 要从远方把我的儿子们带来， 从地极把我的女儿们领回。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要對北方說：‘把他們交出來！’ 又對南方說：‘不要拘留他們！’ 要把我的眾子從遠方帶回來， 把我的女兒從地極領回來，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要對北方說，交出來！ 對南方說，不要拘留！ 將我的眾子從遠方帶來， 將我的眾女從地極領回，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will say to the north, ‘Give them up!’ And to the south, ‘Do not hold them back.’ Bring My sons from afar And My daughters from the ends of the earth,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will say to the north, ‘Give them up!’ and to the south, ‘Do not hold them back.’ Bring my sons from afar and my daughters from the ends of the earth—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will say to the north and south, ‘Bring my sons and daughters back to Israel from the distant corners of the earth."
      }
    },
