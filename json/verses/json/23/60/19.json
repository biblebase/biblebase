    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“太阳不再作你白昼的光， 月亮也不再给你光辉， 因为耶和华要作你永远的光， 你的上帝要成为你的荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "白天太陽必不再作你的光， 晚上月亮也必不再發光照耀你， 耶和華卻要作你永遠的光， 你的　神要作你的榮耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "日頭不再作你白晝的光； 月亮也不再發光照耀你。 耶和華卻要作你永遠的光； 你神要為你的榮耀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No longer will you have the sun for light by day, Nor for brightness will the moon give you light; But you will have the Lord for an everlasting light, And your God for your glory."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The sun will no more be your light by day, nor will the brightness of the moon shine on you, for the Lord will be your everlasting light, and your God will be your glory."
      },
      "nlt": {
        "version_id": "NLT",
