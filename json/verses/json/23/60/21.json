    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的人民都必成为义人， 永远拥有这土地。 他们是我亲手栽种的树苗， 以彰显我的荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的人民都必成為義人， 永遠得地為業； 他們是我栽種的嫩芽， 我手所作的工作， 使我得榮耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的居民都成為義人， 永遠得地為業； 是我種的栽子，我手的工作， 使我得榮耀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then all your people will be righteous; They will possess the land forever, The branch of My planting, The work of My hands, That I may be glorified."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then all your people will be righteous and they will possess the land forever. They are the shoot I have planted, the work of my hands, for the display of my splendor."
      },
