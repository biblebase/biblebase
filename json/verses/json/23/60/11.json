    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的城门要一直敞开， 昼夜不关， 好让各国的君王带着他们的人民和财物列队前来向你朝贡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的城門必常常開， 日夜都不關閉， 使人把列國的財富帶來歸你， 他們的君王也都被引導而來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的城門必時常開放， 晝夜不關； 使人把列國的財物帶來歸你， 並將他們的君王牽引而來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your gates will be open continually; They will not be closed day or night, So that men may bring to you the wealth of the nations, With their kings led in procession."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your gates will always stand open, they will never be shut, day or night, so that people may bring you the wealth of the nations— their kings led in triumphal procession."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your gates will stay open day and night to receive the wealth of many lands. The kings of the world will be led as captives in a victory procession."
      }
    },
