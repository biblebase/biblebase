    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "米甸和以法的骆驼必成群结队而来， 布满你的地面； 示巴人必带着黄金和乳香来颂赞耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "成群的駱駝必遮蓋你的地， 米甸和以法的小駱駝要蓋滿你的國境； 示巴的人都必帶著黃金和乳香來到， 並且宣揚讚美耶和華的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "成羣的駱駝， 並米甸和以法的獨峯駝必遮滿你； 示巴的眾人都必來到； 要奉上黃金乳香， 又要傳說耶和華的讚美。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A multitude of camels will cover you, The young camels of Midian and Ephah; All those from Sheba will come; They will bring gold and frankincense, And will bear good news of the praises of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Herds of camels will cover your land, young camels of Midian and Ephah. And all from Sheba will come, bearing gold and incense and proclaiming the praise of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Vast caravans of camels will converge on you, the camels of Midian and Ephah. The people of Sheba will bring gold and frankincense and will come worshiping the Lord."
      }
    },
