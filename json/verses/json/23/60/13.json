    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "黎巴嫩引以为荣的松树、杉树和黄杨树都要运到你这里， 用来装饰我的圣所； 我要使我脚踏之处充满荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "黎巴嫩的榮耀必來歸你， 就是松樹、杉樹和黃楊樹，都一起歸你， 為要裝飾我聖所之地， 我也要使我踏足之處得榮耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "黎巴嫩的榮耀， 就是松樹、杉樹、黃楊樹， 都必一同歸你， 為要修飾我聖所之地； 我也要使我腳踏之處得榮耀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The glory of Lebanon will come to you, The juniper, the box tree and the cypress together, To beautify the place of My sanctuary; And I shall make the place of My feet glorious."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The glory of Lebanon will come to you, the juniper, the fir and the cypress together, to adorn my sanctuary; and I will glorify the place for my feet."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The glory of Lebanon will be yours— the forests of cypress, fir, and pine— to beautify my sanctuary. My Temple will be glorious!"
      }
    },
