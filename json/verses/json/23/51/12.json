    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“安慰你们的是我，是我。 你为什么要害怕难逃一死、 生命如草芥的世人呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我，我耶和華是安慰你們的； 你是誰，竟怕那會死的人， 怕那被造如草的世人呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟有我，是安慰你們的。 你是誰，竟怕那必死的人？ 怕那要變如草的世人？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I, even I, am He who comforts you. Who are you that you are afraid of man who dies And of the son of man who is made like grass,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I, even I, am he who comforts you. Who are you that you fear mere mortals, human beings who are but grass,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I, yes I, am the one who comforts you. So why are you afraid of mere humans, who wither like the grass and disappear?"
      }
    },
