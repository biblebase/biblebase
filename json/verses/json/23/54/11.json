    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“困苦不堪、饱经风雨、 得不着安慰的城啊， 我要以彩石作你的地基， 用蓝宝石来建造你，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "受困苦、被風飄蕩、不得安慰的啊！ 你看，我要用彩色的石頭安置你的基石， 以藍寶石奠定你的根基。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你這受困苦、被風飄蕩不得安慰的人哪， 我必以彩色安置你的石頭， 以藍寶石立定你的根基；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“O afflicted one, storm-tossed, and not comforted, Behold, I will set your stones in antimony, And your foundations I will lay in sapphires."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Afflicted city, lashed by storms and not comforted, I will rebuild you with stones of turquoise, your foundations with lapis lazuli."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O storm-battered city, troubled and desolate! I will rebuild you with precious jewels and make your foundations from lapis lazuli."
      }
    },
