    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我盛怒之下暂时掩面不理你， 但我要以永远不变的慈爱怜悯你。 这是你的救赎主耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在怒氣漲溢的時候， 我暫時向你掩面， 卻要以永遠的慈愛憐憫你； 這是耶和華你的救贖主說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的怒氣漲溢， 頃刻之間向你掩面， 卻要以永遠的慈愛憐恤你。 這是耶和華－你的救贖主說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In an outburst of anger I hid My face from you for a moment, But with everlasting lovingkindness I will have compassion on you,” Says the Lord your Redeemer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In a surge of anger I hid my face from you for a moment, but with everlasting kindness I will have compassion on you,” says the Lord your Redeemer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In a burst of anger I turned my face away for a little while. But with everlasting love I will have compassion on you,” says the Lord, your Redeemer."
      }
    },
