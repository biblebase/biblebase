    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，是我造了扇旺炭火、 铸造合用兵器的铁匠， 是我造了毁灭者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！那吹炭火、 打造出合用武器的鐵匠，是我創造的； 那殘害人、行毀滅的，也是我創造的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "吹噓炭火、打造合用器械的鐵匠是我所造； 殘害人、行毀滅的也是我所造。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I Myself have created the smith who blows the fire of coals And brings out a weapon for its work; And I have created the destroyer to ruin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“See, it is I who created the blacksmith who fans the coals into flame and forges a weapon fit for its work. And it is I who have created the destroyer to wreak havoc;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I have created the blacksmith who fans the coals beneath the forge and makes the weapons of destruction. And I have created the armies that destroy."
      }
    },
