    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“明亮的晨星、黎明之子啊， 你怎么从天上坠落下来？ 你这打败列国的怎么被砍倒在地上？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "明亮之星、清晨之子啊！ 你怎麼從天上墜落？ 你這傾覆列國的， 怎麼被砍倒在地上？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "明亮之星，早晨之子啊， 你何竟從天墜落？ 你這攻敗列國的何竟被砍倒在地上？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How you have fallen from heaven, O star of the morning, son of the dawn! You have been cut down to the earth, You who have weakened the nations!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How you have fallen from heaven, morning star, son of the dawn! You have been cast down to the earth, you who once laid low the nations!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“How you are fallen from heaven, O shining star, son of the morning! You have been thrown down to the earth, you who destroyed the nations of the world."
      }
    },
