    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的荣耀和琴声一同落入阴间， 虫成了你的床铺， 蛆成了你的被子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的威勢和你的琴瑟音樂， 都被帶下陰間了； 你下面鋪的是蟲， 上面蓋的是蛆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的威勢和你琴瑟的聲音都下到陰間。 你下鋪的是蟲，上蓋的是蛆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your pomp and the music of your harps Have been brought down to Sheol; Maggots are spread out as your bed beneath you And worms are your covering.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All your pomp has been brought down to the grave, along with the noise of your harps; maggots are spread out beneath you and worms cover you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your might and power were buried with you. The sound of the harp in your palace has ceased. Now maggots are your sheet, and worms your blanket.’"
      }
    },
