      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华起誓说： “我的计划必实现， 我的旨意必成就。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華起了誓，說： “我怎樣想，事就怎樣成就； 我怎樣計劃，事就怎樣立定，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華起誓說： 我怎樣思想，必照樣成就； 我怎樣定意，必照樣成立，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord of hosts has sworn saying, “Surely, just as I have intended so it has happened, and just as I have planned so it will stand,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord Almighty has sworn, “Surely, as I have planned, so it will be, and as I have purposed, so it will happen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord of Heaven’s Armies has sworn this oath: “It will all happen as I have planned. It will be as I have decided."
      }
