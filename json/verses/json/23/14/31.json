    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "城门啊，哀号吧！ 城邑啊，哭喊吧！ 非利士人啊，战抖吧！ 因为有阵容整齐的军队从北方如尘烟滚滚而来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "城門啊，要哀號！城鎮啊，要呼叫！ 非利士全地啊，要戰慄發軟！ 因為有煙從北方來， 在他的行伍中沒有離隊逃走的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門哪，應當哀號！ 城啊，應當呼喊！ 非利士全地啊，你都消化了！ 因為有煙從北方出來， 他行伍中並無亂隊的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Wail, O gate; cry, O city; Melt away, O Philistia, all of you; For smoke comes from the north, And there is no straggler in his ranks."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Wail, you gate! Howl, you city! Melt away, all you Philistines! A cloud of smoke comes from the north, and there is not a straggler in its ranks."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wail at the gates! Weep in the cities! Melt with fear, you Philistines! A powerful army comes like smoke from the north. Each soldier rushes forward eager to fight."
      }
    },
