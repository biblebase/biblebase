    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘我要走在你前面， 为你铲平高山， 打破铜门， 砍断铁闩。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必親自領導你，把高低不平的路修平； 銅門，我必打破；鐵門，我必砍斷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必在你前面行， 修平崎嶇之地。 我必打破銅門， 砍斷鐵閂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I will go before you and make the rough places smooth; I will shatter the doors of bronze and cut through their iron bars."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will go before you and will level the mountains; I will break down gates of bronze and cut through bars of iron."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord says: “I will go before you, Cyrus, and level the mountains. I will smash down gates of bronze and cut through bars of iron."
      }
    },
