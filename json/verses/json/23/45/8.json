    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘我使诸天降下甘露， 云彩降下公义， 使大地裂开， 萌生拯救和公义， 这是我耶和华的作为。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "諸天啊！你們要從上滴下公義， 讓雲層降下公義。 讓大地裂開，生出救恩， 使公義一同生長， 是我耶和華創造了這事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "諸天哪，自上而滴， 穹蒼降下公義； 地面開裂，產出救恩， 使公義一同發生； 這都是我－耶和華所造的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Drip down, O heavens, from above, And let the clouds pour down righteousness; Let the earth open up and salvation bear fruit, And righteousness spring up with it. I, the Lord, have created it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You heavens above, rain down my righteousness; let the clouds shower it down. Let the earth open wide, let salvation spring up, let righteousness flourish with it; I, the Lord, have created it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Open up, O heavens, and pour out your righteousness. Let the earth open wide so salvation and righteousness can sprout up together. I, the Lord, created them."
      }
    },
