    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你怎能坐视不理呢？ 你仍然保持缄默，使我们重重地受罚吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！對這些事你還忍得住嗎？ 你仍然緘默不言，使我們受苦到極點嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，有這些事，你還忍得住嗎？ 你仍靜默使我們深受苦難嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will You restrain Yourself at these things, O Lord? Will You keep silent and afflict us beyond measure?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After all this, Lord, will you hold yourself back? Will you keep silent and punish us beyond measure?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After all this, Lord, must you still refuse to help us? Will you continue to be silent and punish us?"
      }
    },
