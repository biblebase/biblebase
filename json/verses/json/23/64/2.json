    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你使敌人认识你的威名， 使列国在你面前颤抖， 如火烧干柴使水沸腾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "好像火燒著乾柴，又像火把水燒開， 使你的敵人認識你的名， 使列國在你面前發顫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "好像火燒乾柴， 又像火將水燒開， 使你敵人知道你的名， 使列國在你面前發顫！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As fire kindles the brushwood, as fire causes water to boil— To make Your name known to Your adversaries, That the nations may tremble at Your presence!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As when fire sets twigs ablaze and causes water to boil, come down to make your name known to your enemies and cause the nations to quake before you!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As fire causes wood to burn and water to boil, your coming would make the nations tremble. Then your enemies would learn the reason for your fame!"
      }
    },
