    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你眷顾乐于行义、遵行你旨意的人。 我们不断地犯罪惹你发怒， 我们怎能得救呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你善待那些喜歡行義， 在你的道路上記念你的人。 看哪！你曾發怒，因為我們犯了罪； 這樣的情形已經很久， 我們還能得救嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你迎接那歡喜行義、記念你道的人； 你曾發怒，我們仍犯罪； 這景況已久，我們還能得救嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You meet him who rejoices in doing righteousness, Who remembers You in Your ways. Behold, You were angry, for we sinned, We continued in them a long time; And shall we be saved?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You come to the help of those who gladly do right, who remember your ways. But when we continued to sin against them, you were angry. How then can we be saved?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You welcome those who gladly do good, who follow godly ways. But you have been very angry with us, for we are not godly. We are constant sinners; how can people like us be saved?"
      }
    },
