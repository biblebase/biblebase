    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "走在黑暗中的人必看见大光， 活在死亡阴影下的人必被光照亮。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "行在黑暗中的人民， 看見了大光； 住在死蔭之地的人， 有光照耀他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在黑暗中行走的百姓看見了大光； 住在死蔭之地的人有光照耀他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The people who walk in darkness Will see a great light; Those who live in a dark land, The light will shine on them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The people walking in darkness have seen a great light; on those living in the land of deep darkness a light has dawned."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people who walk in darkness will see a great light. For those who live in a land of deep darkness, a light will shine."
      }
    },
