    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的人民必娶你， 好像男子娶少女； 上帝必喜爱你， 如同新郎喜爱新娘。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為年輕人怎樣娶處女， 那建造你的也必怎樣娶你； 新郎怎樣因新娘歡樂， 你的　神也必怎樣因你歡樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "少年人怎樣娶處女， 你的眾民也要照樣娶你； 新郎怎樣喜悅新婦， 你的神也要照樣喜悅你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For as a young man marries a virgin, So your sons will marry you; And as the bridegroom rejoices over the bride, So your God will rejoice over you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As a young man marries a young woman, so will your Builder marry you; as a bridegroom rejoices over his bride, so will your God rejoice over you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your children will commit themselves to you, O Jerusalem, just as a young man commits himself to his bride. Then God will rejoice over you as a bridegroom rejoices over his bride."
      }
    },
