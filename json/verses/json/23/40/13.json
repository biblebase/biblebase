      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁曾测度耶和华的心？ 谁曾做祂的谋士指点祂？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "誰曾測度耶和華的靈， 或作過他的謀士教導他呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誰曾測度耶和華的心， 或作他的謀士指教他呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who has directed the Spirit of the Lord, Or as His counselor has informed Him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who can fathom the Spirit of the Lord, or instruct the Lord as his counselor?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Who is able to advise the Spirit of the Lord? Who knows enough to give him advice or teach him?"
      }
