    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁曾用手心量海水？ 谁曾用手掌度苍天？ 谁曾用升斗盛大地的尘土？ 谁曾用秤称高山，用天平称丘陵？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "誰曾用掌心量過海水， 用手掌測過蒼天呢？ 誰曾用升斗量過大地的塵土， 用秤稱山嶺， 用天平稱岡陵呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誰曾用手心量諸水， 用手虎口量蒼天， 用升斗盛大地的塵土， 用秤稱山嶺， 用天平平岡陵呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who has measured the waters in the hollow of His hand, And marked off the heavens by the span, And calculated the dust of the earth by the measure, And weighed the mountains in a balance And the hills in a pair of scales?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who has measured the waters in the hollow of his hand, or with the breadth of his hand marked off the heavens? Who has held the dust of the earth in a basket, or weighed the mountains on the scales and the hills in a balance?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Who else has held the oceans in his hand? Who has measured off the heavens with his fingers? Who else knows the weight of the earth or has weighed the mountains and hills on a scale?"
      }
    },
