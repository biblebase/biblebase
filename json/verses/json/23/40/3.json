    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听啊，有人高喊： “在旷野预备耶和华的道， 在沙漠修直我们上帝的路。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有聲音呼喊說： “你們要在曠野清理耶和華的路， 在沙漠修直我們　神的大道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人聲喊着說： 在曠野預備耶和華的路， 在沙漠地修平我們神的道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A voice is calling, “Clear the way for the Lord in the wilderness; Make smooth in the desert a highway for our God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A voice of one calling: “In the wilderness prepare the way for the Lord; make straight in the desert a highway for our God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen! It’s the voice of someone shouting, “Clear the way through the wilderness for the Lord! Make a straight highway through the wasteland for our God!"
      }
    },
