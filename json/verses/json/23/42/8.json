    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是耶和华，这就是我的名字。 我不会把我的荣耀给假神， 也不会把我当受的赞美给雕刻的偶像。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我是耶和華，這是我的名； 我必不把我的榮耀歸給別人， 也不把我當受的稱讚歸給雕刻的偶像。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我是耶和華，這是我的名； 我必不將我的榮耀歸給假神， 也不將我的稱讚歸給雕刻的偶像。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am the Lord, that is My name; I will not give My glory to another, Nor My praise to graven images."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I am the Lord; that is my name! I will not yield my glory to another or my praise to idols."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I am the Lord; that is my name! I will not give my glory to anyone else, nor share my praise with carved idols."
      }
    },
