    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但那些信靠雕刻的偶像、 供奉金属铸像的必遭唾弃， 羞愧难当。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些倚靠偶像， 對鑄像說： “你們是我們的神”的， 必被趕逐，全然蒙羞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "倚靠雕刻的偶像， 對鑄造的偶像說： 你是我們的神； 這等人要退後，全然蒙羞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will be turned back and be utterly put to shame, Who trust in idols, Who say to molten images, “You are our gods.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But those who trust in idols, who say to images, ‘You are our gods,’ will be turned back in utter shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But those who trust in idols, who say, ‘You are our gods,’ will be turned away in shame."
      }
    },
