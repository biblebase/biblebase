    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“看啊，我所扶持、所拣选、所喜悦的仆人， 我已将我的灵赐给祂， 祂必将正义带给万邦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！這是我的僕人，我扶持他； 我所揀選的，我的心喜悅他； 我已經把我的靈賜給他， 他必把公理帶給萬國。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，我的僕人－ 我所扶持所揀選、心裏所喜悅的！ 我已將我的靈賜給他； 他必將公理傳給外邦。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, My Servant, whom I uphold; My chosen one in whom My soul delights. I have put My Spirit upon Him; He will bring forth justice to the nations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Here is my servant, whom I uphold, my chosen one in whom I delight; I will put my Spirit on him, and he will bring justice to the nations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Look at my servant, whom I strengthen. He is my chosen one, who pleases me. I have put my Spirit upon him. He will bring justice to the nations."
      }
    },
