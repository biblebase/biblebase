    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我耶和华凭公义呼召你， 我必扶持你，保护你， 使你做我与众民立约的中保、 成为列国的光，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我耶和華憑著公義呼召了你； 我必緊拉著你的手，我必保護你， 立你作人民的約，作列國的光，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我－耶和華憑公義召你， 必攙扶你的手，保守你， 使你作眾民的中保， 作外邦人的光，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I am the Lord, I have called You in righteousness, I will also hold You by the hand and watch over You, And I will appoint You as a covenant to the people, As a light to the nations,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I, the Lord, have called you in righteousness; I will take hold of your hand. I will keep you and will make you to be a covenant for the people and a light for the Gentiles,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I, the Lord, have called you to demonstrate my righteousness. I will take you by the hand and guard you, and I will give you to my people, Israel, as a symbol of my covenant with them. And you will be a light to guide the nations."
      }
    },
