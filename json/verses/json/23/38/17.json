    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其实我受大苦对我有益。 你施慈爱，从死亡的深坑中拯救我的生命， 将我一切罪恶抛在你背后。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！我受盡苦楚，為要使我得平安； 你保全我的性命，脫離毀滅的坑， 因為你把我的一切罪都丟在你的背後。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，我受大苦，本為使我得平安； 你因愛我的靈魂便救我脫離敗壞的坑， 因為你將我一切的罪扔在你的背後。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Lo, for my own welfare I had great bitterness; It is You who has kept my soul from the pit of nothingness, For You have cast all my sins behind Your back."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely it was for my benefit that I suffered such anguish. In your love you kept me from the pit of destruction; you have put all my sins behind your back."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, this anguish was good for me, for you have rescued me from death and forgiven all my sins."
      }
    },
