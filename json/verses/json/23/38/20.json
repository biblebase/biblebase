    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华必救我， 我们要一生一世在祂殿中弹琴歌唱。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必拯救我， 因此我們要一生一世在耶和華的殿中， 用絲弦的樂器彈奏我的詩歌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華肯救我， 所以，我們要一生一世 在耶和華殿中 用絲弦的樂器唱我的詩歌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord will surely save me; So we will play my songs on stringed instruments All the days of our life at the house of the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will save me, and we will sing with stringed instruments all the days of our lives in the temple of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Think of it—the Lord is ready to heal me! I will sing his praises with instruments every day of my life in the Temple of the Lord."
      }
    },
