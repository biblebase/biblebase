    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚述王必把埃及和古实的老老少少裸身赤足地掳走，使埃及蒙受羞辱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞述王也必照樣把埃及的俘虜和古實被放逐的人帶走，無論老少，都露體赤足，露出下體，使埃及蒙羞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "照樣，亞述王也必擄去埃及人，掠去古實人，無論老少，都露身赤腳，現出下體，使埃及蒙羞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so the king of Assyria will lead away the captives of Egypt and the exiles of Cush, young and old, naked and barefoot with buttocks uncovered, to the shame of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "so the king of Assyria will lead away stripped and barefoot the Egyptian captives and Cushite exiles, young and old, with buttocks bared—to Egypt’s shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the king of Assyria will take away the Egyptians and Ethiopians as prisoners. He will make them walk naked and barefoot, both young and old, their buttocks bared, to the shame of Egypt."
      }
    },
