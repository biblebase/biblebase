    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "而是按公义审判贫穷人， 以公正为受苦者断案。 他必用口中的杖击打世界， 以嘴里的气杀戮恶人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "卻要以公義審判貧窮人， 以正直判斷地上的困苦人； 以口中的杖擊打世界， 以嘴裡的氣殺死惡人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "卻要以公義審判貧窮人， 以正直判斷世上的謙卑人， 以口中的杖擊打世界， 以嘴裏的氣殺戮惡人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But with righteousness He will judge the poor, And decide with fairness for the afflicted of the earth; And He will strike the earth with the rod of His mouth, And with the breath of His lips He will slay the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but with righteousness he will judge the needy, with justice he will give decisions for the poor of the earth. He will strike the earth with the rod of his mouth; with the breath of his lips he will slay the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will give justice to the poor and make fair decisions for the exploited. The earth will shake at the force of his word, and one breath from his mouth will destroy the wicked."
      }
    },
