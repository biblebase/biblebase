    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为将有余民从耶路撒冷出来，有幸存者从锡安山出来。万军之耶和华必热切地成就这事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為餘下的人必從耶路撒冷出來，逃脫的人必從錫安山出來。萬軍之耶和華的熱心必作成這事。’"
      },
      "cunp": {
        "version_id": "CUNP",
