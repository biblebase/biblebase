    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "宁林的河道干涸， 青草枯萎，植被消失，毫无绿色。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為寧林的眾水都已荒涼， 青草枯乾，嫩草滅沒， 完全沒有青綠之物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為寧林的水成為乾涸， 青草枯乾，嫩草滅沒， 青綠之物，一無所有。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the waters of Nimrim are desolate. Surely the grass is withered, the tender grass died out, There is no green thing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The waters of Nimrim are dried up and the grass is withered; the vegetation is gone and nothing green is left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even the waters of Nimrim are dried up! The grassy banks are scorched. The tender plants are gone; nothing green remains."
      }
    },
