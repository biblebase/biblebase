    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押境内哀声四起， 号啕声传到以基莲， 传到比珥·以琳。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "哀號的聲音傳遍了摩押四境， 哀聲直達以基蓮，又傳到比珥．以琳。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "哀聲遍聞摩押的四境； 哀號的聲音達到以基蓮； 哀號的聲音達到比珥‧以琳。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the cry of distress has gone around the territory of Moab, Its wail goes as far as Eglaim and its wailing even to Beer-elim."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their outcry echoes along the border of Moab; their wailing reaches as far as Eglaim, their lamentation as far as Beer Elim."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A cry of distress echoes through the land of Moab from one end to the other— from Eglaim to Beer-elim."
      }
    },
