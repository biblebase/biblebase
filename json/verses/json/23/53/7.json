    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂遭欺压、受痛苦， 却默然不语， 像被人牵去宰杀的羔羊， 又如在剪毛人手下一声不吭的绵羊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他被虐待，受痛苦的時候， 他並不開口； 他像羊羔被牽去屠宰， 又像羊在剪羊毛的人面前寂然無聲， 他也是這樣不開口。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他被欺壓， 在受苦的時候卻不開口； 他像羊羔被牽到宰殺之地， 又像羊在剪毛的人手下無聲， 他也是這樣不開口。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He was oppressed and He was afflicted, Yet He did not open His mouth; Like a lamb that is led to slaughter, And like a sheep that is silent before its shearers, So He did not open His mouth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He was oppressed and afflicted, yet he did not open his mouth; he was led like a lamb to the slaughter, and as a sheep before its shearers is silent, so he did not open his mouth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He was oppressed and treated harshly, yet he never said a word. He was led like a lamb to the slaughter. And as a sheep is silent before the shearers, he did not open his mouth."
      }
    },
