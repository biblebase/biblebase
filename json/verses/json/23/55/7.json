    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿邪恶的人停止作恶， 不义的人除掉恶念。 愿他们归向耶和华， 祂必怜悯他们； 愿他们归向我们的上帝， 祂必仁慈地赦免他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人要離棄自己的道路， 不義的人當除去自己的意念， 回轉過來歸向耶和華， 耶和華就必憐憫他。 你們當回轉過來歸向我們的　神， 因為他大大赦免人的罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人當離棄自己的道路； 不義的人當除掉自己的意念。 歸向耶和華，耶和華就必憐恤他； 當歸向我們的神，因為神必廣行赦免。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let the wicked forsake his way And the unrighteous man his thoughts; And let him return to the Lord, And He will have compassion on him, And to our God, For He will abundantly pardon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let the wicked forsake their ways and the unrighteous their thoughts. Let them turn to the Lord, and he will have mercy on them, and to our God, for he will freely pardon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let the wicked change their ways and banish the very thought of doing wrong. Let them turn to the Lord that he may have mercy on them. Yes, turn to our God, for he will forgive generously."
      }
    },
