    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我还是要叫你受苦。 你必悲伤哀号， 在我面前成为一座祭坛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我卻要使亞利伊勒受困苦， 成了一座充滿悲傷哀號的城， 我要以它為亞利伊勒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我終必使亞利伊勒困難； 她必悲傷哀號， 我卻仍以她為亞利伊勒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will bring distress to Ariel, And she will be a city of lamenting and mourning; And she will be like an Ariel to me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet I will besiege Ariel; she will mourn and lament, she will be to me like an altar hearth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet I will bring disaster upon you, and there will be much weeping and sorrow. For Jerusalem will become what her name Ariel means— an altar covered with blood."
      }
