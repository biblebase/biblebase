    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们把书卷交给不识字的人读，他会说：“我不识字。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人又把這書卷交給不識字的人，說：“請念吧！”他必回答：“我不識字。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又將這書卷交給不識字的人，說：「請念吧！」他說：「我不識字。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the book will be given to the one who is illiterate, saying, “Please read this.” And he will say, “I cannot read.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Or if you give the scroll to someone who cannot read, and say, “Read this, please,” they will answer, “I don’t know how to read.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When you give it to those who cannot read, they will say, “We don’t know how to read.”"
      }
    },
