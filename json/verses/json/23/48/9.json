    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我为了我的名，暂时忍怒； 我为了我的荣耀，压住怒气， 不消灭你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為了我的名的緣故，我暫時不發怒； 為了我的名譽的緣故，我向你忍耐； 不把你剪除。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我為我的名暫且忍怒， 為我的頌讚向你容忍， 不將你剪除。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the sake of My name I delay My wrath, And for My praise I restrain it for you, In order not to cut you off."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For my own name’s sake I delay my wrath; for the sake of my praise I hold it back from you, so as not to destroy you completely."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet for my own sake and for the honor of my name, I will hold back my anger and not wipe you out."
      }
    },
