    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雅各，我所拣选的以色列啊， 听我说！ 我是上帝，我是首先的， 也是末后的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“雅各，我所呼召的以色列啊！你要聽我的話。 我就是‘那位’，我是首先的，也是末後的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雅各－我所選召的以色列啊， 當聽我言： 我是耶和華， 我是首先的，也是末後的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Listen to Me, O Jacob, even Israel whom I called; I am He, I am the first, I am also the last."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Listen to me, Jacob, Israel, whom I have called: I am he; I am the first and I am the last."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Listen to me, O family of Jacob, Israel my chosen one! I alone am God, the First and the Last."
      }
    },
