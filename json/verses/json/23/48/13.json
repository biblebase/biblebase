    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我亲手奠立大地的根基， 我的右手铺展穹苍。 我一呼唤，它们都侍立一旁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的手奠定了大地的根基， 我的右手展開了諸天； 我一呼喚它們，它們就一同站著侍候。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我手立了地的根基； 我右手鋪張諸天； 我一招呼便都立住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Surely My hand founded the earth, And My right hand spread out the heavens; When I call to them, they stand together."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My own hand laid the foundations of the earth, and my right hand spread out the heavens; when I summon them, they all stand up together."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It was my hand that laid the foundations of the earth, my right hand that spread out the heavens above. When I call out the stars, they all appear in order.”"
      }
