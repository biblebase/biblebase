    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "唯有我说过这些事； 我召了他，领他出来； 他必一路亨通。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我親自說過，又呼召了他； 我帶領了他來，他所行的就必亨通。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟有我曾說過，我又選召他， 領他來，他的道路就必亨通。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I, even I, have spoken; indeed I have called him, I have brought him, and He will make his ways successful."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I, even I, have spoken; yes, I have called him. I will bring him, and he will succeed in his mission."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I have said it: I am calling Cyrus! I will send him on this errand and will help him succeed."
      }
