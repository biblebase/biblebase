    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们还自称为圣城的人， 声称倚靠名为万军之耶和华的以色列的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們被稱為聖城的人， 又倚靠以色列的　神； 他的名字是萬軍之耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們自稱為聖城的人， 所倚靠的是 名為萬軍之耶和華－以色列的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they call themselves after the holy city And lean on the God of Israel; The Lord of hosts is His name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "you who call yourselves citizens of the holy city and claim to rely on the God of Israel— the Lord Almighty is his name:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "even though you call yourself the holy city and talk about depending on the God of Israel, whose name is the Lord of Heaven’s Armies."
      }
    },
