    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，我熬炼你，但不像在火炉中炼银子， 我是在苦难的炉中熬炼你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！我熬煉了你，卻不像熬煉銀子； 你在苦難的爐中，我揀選了你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我熬煉你，卻不像熬煉銀子； 你在苦難的爐中，我揀選你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I have refined you, but not as silver; I have tested you in the furnace of affliction."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See, I have refined you, though not as silver; I have tested you in the furnace of affliction."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have refined you, but not as silver is refined. Rather, I have refined you in the furnace of suffering."
      }
    },
