    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我这样做是为了自己， 我岂能让自己的名受亵渎， 把自己的荣耀给别人？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為了我自己的緣故，為了我自己的緣故， 我必作這事。 我的名怎能被褻瀆呢？ 我必不把我的榮耀歸給別人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我為自己的緣故必行這事， 我焉能使我的名被褻瀆？ 我必不將我的榮耀歸給假神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For My own sake, for My own sake, I will act; For how can My name be profaned? And My glory I will not give to another."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For my own sake, for my own sake, I do this. How can I let myself be defamed? I will not yield my glory to another."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will rescue you for my sake— yes, for my own sake! I will not let my reputation be tarnished, and I will not share my glory with idols!"
      }
    },
