    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这是万军之耶和华在埃及的记号和凭据。埃及人因受到欺压就呼求耶和华，祂必差遣一位救主保护、拯救他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這都要在埃及地作萬軍之耶和華的記號和證據。埃及人因受人欺壓向耶和華哀求的時候，他就差派一位拯救者和維護者到他們那裡，他必拯救他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這都要在埃及地為萬軍之耶和華作記號和證據。埃及人因為受人的欺壓哀求耶和華，他就差遣一位救主作護衛者，拯救他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It will become a sign and a witness to the Lord of hosts in the land of Egypt; for they will cry to the Lord because of oppressors, and He will send them a Savior and a Champion, and He will deliver them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It will be a sign and witness to the Lord Almighty in the land of Egypt. When they cry out to the Lord because of their oppressors, he will send them a savior and defender, and he will rescue them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It will be a sign and a witness that the Lord of Heaven’s Armies is worshiped in the land of Egypt. When the people cry to the Lord for help against those who oppress them, he will send them a savior who will rescue them."
      }
    },
