    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "河流必发臭， 埃及的河流逐渐枯竭。 芦荻和灯心草必枯死，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "河道必發臭， 埃及眾河流的水必消退乾涸； 葦子和蘆荻都必枯萎。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "江河要變臭； 埃及的河水都必減少枯乾。 葦子和蘆荻都必衰殘；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The canals will emit a stench, The streams of Egypt will thin out and dry up; The reeds and rushes will rot away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The canals will stink; the streams of Egypt will dwindle and dry up. The reeds and rushes will wither,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The canals of the Nile will dry up, and the streams of Egypt will stink with rotting reeds and rushes."
      }
    },
