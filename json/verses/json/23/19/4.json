    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必将埃及人交给一位残忍的主人， 一位暴君必统治他们。” 这是主——万军之耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必把埃及人交在殘暴的主人手中； 兇猛的君王要管轄他們；這是主萬軍之耶和華說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必將埃及人交在殘忍主的手中； 強暴王必轄制他們。 這是主－萬軍之耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, I will deliver the Egyptians into the hand of a cruel master, And a mighty king will rule over them,” declares the Lord God of hosts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will hand the Egyptians over to the power of a cruel master, and a fierce king will rule over them,” declares the Lord, the Lord Almighty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will hand Egypt over to a hard, cruel master. A fierce king will rule them,” says the Lord, the Lord of Heaven’s Armies."
      }
    },
