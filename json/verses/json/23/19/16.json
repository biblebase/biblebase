    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到那日，当万军之耶和华挥拳惩罚的时候，埃及人必恐惧战抖，如柔弱的女子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到那日，埃及人必像婦人一樣。他們必因萬軍之耶和華在埃及之上揮手而戰兢恐懼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到那日，埃及人必像婦人一樣，他們必因萬軍之耶和華在埃及以上所掄的手，戰兢懼怕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In that day the Egyptians will become like women, and they will tremble and be in dread because of the waving of the hand of the Lord of hosts, which He is going to wave over them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In that day the Egyptians will become weaklings. They will shudder with fear at the uplifted hand that the Lord Almighty raises against them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In that day the Egyptians will be as weak as women. They will cower in fear beneath the upraised fist of the Lord of Heaven’s Armies."
      }
    },
