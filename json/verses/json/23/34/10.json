    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "昼夜燃烧，浓烟滚滚， 永不止息。 以东必世世代代荒废， 人踪绝迹。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "晝夜不熄， 煙氣永遠上騰， 並且必世世代代變為荒涼， 永永遠遠無人經過。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "晝夜總不熄滅， 煙氣永遠上騰， 必世世代代成為荒廢， 永永遠遠無人經過。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It will not be quenched night or day; Its smoke will go up forever. From generation to generation it will be desolate; None will pass through it forever and ever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It will not be quenched night or day; its smoke will rise forever. From generation to generation it will lie desolate; no one will ever pass through it again."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This judgment on Edom will never end; the smoke of its burning will rise forever. The land will lie deserted from generation to generation. No one will live there anymore."
      }
    },
