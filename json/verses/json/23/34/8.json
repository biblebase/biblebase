    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为这是耶和华报应的日子， 是祂为锡安报仇之年。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華有報仇的日子， 為錫安的案件，必有報應之年。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因耶和華有報仇之日， 為錫安的爭辯有報應之年。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord has a day of vengeance, A year of recompense for the cause of Zion."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the Lord has a day of vengeance, a year of retribution, to uphold Zion’s cause."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For it is the day of the Lord’s revenge, the year when Edom will be paid back for all it did to Israel."
      }
    },
