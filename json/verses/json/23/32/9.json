    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "生活安逸的妇女啊， 来听我的声音！ 无忧无虑的女子啊， 要侧耳听我的言语！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "安逸的婦女啊！你們要起來， 聽我的聲音； 無憂無慮的女子啊！ 你們要側耳聽我的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "安逸的婦女啊，起來聽我的聲音！ 無慮的女子啊，側耳聽我的言語！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Rise up, you women who are at ease, And hear my voice; Give ear to my word, You complacent daughters."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You women who are so complacent, rise up and listen to me; you daughters who feel secure, hear what I have to say!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen, you women who lie around in ease. Listen to me, you who are so smug."
      }
    },
