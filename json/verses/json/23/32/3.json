    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人们的眼睛必不再迷蒙， 耳朵必能听见。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，能看見事物者的眼睛，必不再昏暗； 能聽到事物者的耳朵，必然傾聽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那能看的人，眼不再昏迷； 能聽的人，耳必得聽聞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the eyes of those who see will not be blinded, And the ears of those who hear will listen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the eyes of those who see will no longer be closed, and the ears of those who hear will listen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then everyone who has eyes will be able to see the truth, and everyone who has ears will be able to hear it."
      }
