    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必惩罚这罪恶的世界， 惩治邪恶的世人， 制止骄横之人的狂妄， 压下残暴之徒的骄傲。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必因世界的邪惡施行刑罰， 也必因惡人的罪孽懲罰他們； 我要使狂妄人的驕傲止息， 使強暴人的狂傲降卑。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必因邪惡刑罰世界， 因罪孽刑罰惡人， 使驕傲人的狂妄止息， 制伏強暴人的狂傲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus I will punish the world for its evil And the wicked for their iniquity; I will also put an end to the arrogance of the proud And abase the haughtiness of the ruthless."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will punish the world for its evil, the wicked for their sins. I will put an end to the arrogance of the haughty and will humble the pride of the ruthless."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I, the Lord, will punish the world for its evil and the wicked for their sin. I will crush the arrogance of the proud and humble the pride of the mighty."
      }
    },
