    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我不再向它发怒。 若是发现荆棘和蒺藜， 我就对付它们， 把它们烧光。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我再沒有忿怒， 若荊棘和蒺藜與我交戰， 我就上前踐踏它們，把它們全部焚毀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我心中不存忿怒。 惟願荊棘蒺藜與我交戰， 我就勇往直前， 把它一同焚燒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have no wrath. Should someone give Me briars and thorns in battle, Then I would step on them, I would burn them completely."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am not angry. If only there were briers and thorns confronting me! I would march against them in battle; I would set them all on fire."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My anger will be gone. If I find briers and thorns growing, I will attack them; I will burn them up—"
      }
    },
