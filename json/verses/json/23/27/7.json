    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华不像击打以色列的敌人那样击打以色列人。 祂不像击杀以色列的敌人那样击杀以色列人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華擊打以色列人，豈像擊打那些擊打他們的呢？ 主殺戮以色列人，豈像殺戮那些殺戮他們的呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主擊打他們， 豈像擊打那些擊打他們的人嗎？ 他們被殺戮， 豈像被他們所殺戮的嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like the striking of Him who has struck them, has He struck them? Or like the slaughter of His slain, have they been slain?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Has the Lord struck her as he struck down those who struck her? Has she been killed as those were killed who killed her?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Has the Lord struck Israel as he struck her enemies? Has he punished her as he punished them?"
      }
    },
