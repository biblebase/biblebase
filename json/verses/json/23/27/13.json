    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到那日，号角吹响后，亚述地将要灭亡的以色列人和流散到埃及的以色列人，都必来到耶路撒冷的圣山敬拜耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到那日，必有大號角吹響，那些在亞述地快要滅亡的，以及那些被趕散在埃及地的，都要前來，在耶路撒冷的聖山上敬拜耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當那日，必大發角聲，在亞述地將要滅亡的，並在埃及地被趕散的，都要來，他們就在耶路撒冷聖山上敬拜耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It will come about also in that day that a great trumpet will be blown, and those who were perishing in the land of Assyria and who were scattered in the land of Egypt will come and worship the Lord in the holy mountain at Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And in that day a great trumpet will sound. Those who were perishing in Assyria and those who were exiled in Egypt will come and worship the Lord on the holy mountain in Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In that day the great trumpet will sound. Many who were dying in exile in Assyria and Egypt will return to Jerusalem to worship the Lord on his holy mountain."
      }
    },
