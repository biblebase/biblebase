    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若有人到你们那里不传基督的教导，不要请他到家里，也不要问候他，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果有人到你們那裡，不傳這教訓，你們就不要接待他到家裡，也不要問候他；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若有人到你們那裏，不是傳這教訓，不要接他到家裏，也不要問他的安；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If anyone comes to you and does not bring this teaching, do not receive him into your house, and do not give him a greeting;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If anyone comes to you and does not bring this teaching, do not take them into your house or welcome them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If anyone comes to your meeting and does not teach the truth about Christ, don’t invite that person into your home or give any kind of encouragement."
      }
    },
