    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "爱就是遵行上帝的命令，这是你们从起初就听过又当遵行的命令。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們要照著他的命令行事，這就是愛。你們從起初所聽見的那命令，就是要你們憑著愛心行事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們若照他的命令行，這就是愛。你們從起初所聽見當行的，就是這命令。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And this is love, that we walk according to His commandments. This is the commandment, just as you have heard from the beginning, that you should walk in it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And this is love: that we walk in obedience to his commands. As you have heard from the beginning, his command is that you walk in love."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Love means doing what God has commanded us, and he has commanded us to love one another, just as you heard from the beginning."
      }
    },
