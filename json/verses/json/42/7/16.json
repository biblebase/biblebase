    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在场的人惊惧万分，把荣耀归给上帝，说：“我们中间出了一位大先知！”又说：“上帝眷顾了祂的百姓！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人都驚懼，頌讚　神說：“有偉大的先知在我們中間興起來了！”又說：“　神眷顧他的子民了！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人都驚奇，歸榮耀與神，說：「有大先知在我們中間興起來了！」又說：「神眷顧了他的百姓！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Fear gripped them all, and they began glorifying God, saying, “A great prophet has arisen among us!” and, “God has visited His people!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They were all filled with awe and praised God. “A great prophet has appeared among us,” they said. “God has come to help his people.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Great fear swept the crowd, and they praised God, saying, “A mighty prophet has risen among us,” and “God has visited his people today.”"
      }
    },
