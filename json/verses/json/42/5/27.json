    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来耶稣外出时，看见一个名叫利未的税吏坐在收税站里，就对他说：“跟从我！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "事後，耶穌出去，看見一個稅吏，名叫利未，坐在稅關那裡，就對他說：“來跟從我！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這事以後，耶穌出去，看見一個稅吏，名叫利未，坐在稅關上，就對他說：「你跟從我來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After that He went out and noticed a tax collector named Levi sitting in the tax booth, and He said to him, “Follow Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After this, Jesus went out and saw a tax collector by the name of Levi sitting at his tax booth. “Follow me,” Jesus said to him,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Later, as Jesus left the town, he saw a tax collector named Levi sitting at his tax collector’s booth. “Follow me and be my disciple,” Jesus said to him."
      }
