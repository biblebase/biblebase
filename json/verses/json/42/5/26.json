    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人都十分惊奇，一同赞美上帝，心里充满了敬畏，说：“今天我们看到神迹了！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人都驚奇，頌讚　神，並且十分懼怕，說：“我們今天看見了不平常的事。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人都驚奇，也歸榮耀與神，並且滿心懼怕，說：「我們今日看見非常的事了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They were all struck with astonishment and began glorifying God; and they were filled with fear, saying, “We have seen remarkable things today.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Everyone was amazed and gave praise to God. They were filled with awe and said, “We have seen remarkable things today.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone was gripped with great wonder and awe, and they praised God, exclaiming, “We have seen amazing things today!”"
      }
    },
