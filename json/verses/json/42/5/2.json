    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂看见湖边停着两条船，渔夫离了船正在洗网，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他看見兩隻船停在湖邊，漁夫離開船洗網去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他見有兩隻船灣在湖邊；打魚的人卻離開船洗網去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and He saw two boats lying at the edge of the lake; but the fishermen had gotten out of them and were washing their nets."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He saw at the water’s edge two boats, left there by the fishermen, who were washing their nets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He noticed two empty boats at the water’s edge, for the fishermen had left them and were washing their nets."
      }
    },
