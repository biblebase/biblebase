    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "同样，没有人把新酒装进旧皮囊里，否则新酒会把旧皮囊胀破，酒也漏了，皮囊也毁了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也沒有人會把新酒裝在舊皮袋裡；如果這樣，新酒就會把皮袋脹破，不但酒漏掉，皮袋也損壞了；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也沒有人把新酒裝在舊皮袋裏；若是這樣，新酒必將皮袋裂開，酒便漏出來，皮袋也就壞了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And no one puts new wine into old wineskins; otherwise the new wine will burst the skins and it will be spilled out, and the skins will be ruined."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And no one pours new wine into old wineskins. Otherwise, the new wine will burst the skins; the wine will run out and the wineskins will be ruined."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And no one puts new wine into old wineskins. For the new wine would burst the wineskins, spilling the wine and ruining the skins."
      }
    },
