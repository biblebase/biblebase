    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法利赛人和律法教师就向耶稣的门徒抱怨说：“你们为什么跟税吏和罪人一起吃喝呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法利賽人和經學家埋怨他的門徒，說：“你們為甚麼跟稅吏和罪人一起吃喝呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法利賽人和文士就向耶穌的門徒發怨言說：「你們為甚麼和稅吏並罪人一同吃喝呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Pharisees and their scribes began grumbling at His disciples, saying, “Why do you eat and drink with the tax collectors and sinners?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the Pharisees and the teachers of the law who belonged to their sect complained to his disciples, “Why do you eat and drink with tax collectors and sinners?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Pharisees and their teachers of religious law complained bitterly to Jesus’ disciples, “Why do you eat and drink with such scum?”"
      }
    },
