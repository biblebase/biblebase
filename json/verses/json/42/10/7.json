    "sermons": [
      {
        "id": "home_sermons_sermon-2020-01-05",
        "title": "莊稼多，工人少",
        "date": "2020-01-05",
        "preacher": "蕭旻奇牧師",
        "verses": "路加福音 10:1-12; 馬太福音 9:35-38; 約翰福音 4:34-38",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2020/Sermon-2020-01-05.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2020/Sermon-2020-01-05.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要搬来搬去，要固定住在一家，接受那家的款待，因为工人理应得到工钱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要住在那家，吃喝他們所供給的，因為作工的理當得工資；不必從這家搬到那家。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要住在那家，吃喝他們所供給的，因為工人得工價是應當的；不要從這家搬到那家。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Stay in that house, eating and drinking what they give you; for the laborer is worthy of his wages. Do not keep moving from house to house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Stay there, eating and drinking whatever they give you, for the worker deserves his wages. Do not move around from house to house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t move around from home to home. Stay in one place, eating and drinking what they provide. Don’t hesitate to accept hospitality, because those who work deserve their pay."
      }
    },
