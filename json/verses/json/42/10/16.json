    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣继续对门徒说：“谁听从你们，就是听从我；谁拒绝你们，就是拒绝我；拒绝我的，就是拒绝差我来的那位。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "聽從你們的，就是聽從我；棄絕你們的，就是棄絕我；棄絕我的，就是棄絕那差我來的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又對門徒說：「聽從你們的就是聽從我；棄絕你們的就是棄絕我；棄絕我的就是棄絕那差我來的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The one who listens to you listens to Me, and the one who rejects you rejects Me; and he who rejects Me rejects the One who sent Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Whoever listens to you listens to me; whoever rejects you rejects me; but whoever rejects me rejects him who sent me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said to the disciples, “Anyone who accepts your message is also accepting me. And anyone who rejects you is rejecting me. And anyone who rejects me is rejecting God, who sent me.”"
      }
    },
