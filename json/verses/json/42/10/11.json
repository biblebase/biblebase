    "sermons": [
      {
        "id": "home_sermons_sermon-2020-01-05",
        "title": "莊稼多，工人少",
        "date": "2020-01-05",
        "preacher": "蕭旻奇牧師",
        "verses": "路加福音 10:1-12; 馬太福音 9:35-38; 約翰福音 4:34-38",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2020/Sermon-2020-01-05.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2020/Sermon-2020-01-05.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘就连你们城里的尘土粘在我们脚上，我们都要跺掉，但是要知道，上帝的国临近了。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘連你們城裡那黏在我們腳上的塵土，我們也要當著你們擦去。雖然這樣，你們應該知道：　神的國臨近了！’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：『就是你們城裏的塵土黏在我們的腳上，我們也當着你們擦去。雖然如此，你們該知道神的國臨近了。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Even the dust of your city which clings to our feet we wipe off in protest against you; yet be sure of this, that the kingdom of God has come near.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘Even the dust of your town we wipe from our feet as a warning to you. Yet be sure of this: The kingdom of God has come near.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘We wipe even the dust of your town from our feet to show that we have abandoned you to your fate. And know this—the Kingdom of God is near!’"
      }
    },
