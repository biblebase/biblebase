    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他立刻得见光明，并跟随耶稣，一路赞美上帝。目睹这事的人也都赞美上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他立刻看見了，就跟隨耶穌，頌讚　神。眾人看見這事，也讚美　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瞎子立刻看見了，就跟隨耶穌，一路歸榮耀與神。眾人看見這事，也讚美神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Immediately he regained his sight and began following Him, glorifying God; and when all the people saw it, they gave praise to God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Immediately he received his sight and followed Jesus, praising God. When all the people saw it, they also praised God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Instantly the man could see, and he followed Jesus, praising God. And all who saw it praised God, too."
      }
