    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们为人子的缘故而遭人憎恨、弃绝、侮辱、毁谤，就有福了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "世人為人子的緣故憎恨你們、排斥你們、辱罵你們，棄絕你們的名好像棄絕惡物，你們就有福了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人為人子恨惡你們，拒絕你們，辱罵你們，棄掉你們的名，以為是惡，你們就有福了！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Blessed are you when men hate you, and ostracize you, and insult you, and scorn your name as evil, for the sake of the Son of Man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Blessed are you when people hate you, when they exclude you and insult you and reject your name as evil, because of the Son of Man."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What blessings await you when people hate you and exclude you and mock you and curse you as evil because you follow the Son of Man."
      }
    },
