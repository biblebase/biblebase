    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人人都夸赞你们的时候， 你们就有祸了， 因为他们的祖先也是这样夸赞假先知！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人都說你們好的時候，你們就有禍了，因為你們的祖先對待假先知也是這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人都說你們好的時候，你們就有禍了！因為他們的祖宗待假先知也是這樣。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe to you when all men speak well of you, for their fathers used to treat the false prophets in the same way."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Woe to you when everyone speaks well of you, for that is how their ancestors treated the false prophets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What sorrow awaits you who are praised by the crowds, for their ancestors also praised false prophets."
      }
