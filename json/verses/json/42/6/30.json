    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人向你求什么，就给他；有人拿了你的东西，不要追讨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "向你求的，就給他；有人拿去你的東西，不用再要回來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡求你的，就給他。有人奪你的東西去，不用再要回來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Give to everyone who asks of you, and whoever takes away what is yours, do not demand it back."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Give to everyone who asks you, and if anyone takes what belongs to you, do not demand it back."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give to anyone who asks; and when things are taken away from you, don’t try to get them back."
      }
    },
