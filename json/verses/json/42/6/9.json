    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣问众人：“我问你们，在安息日应该行善呢，还是作恶呢？救人呢，还是害人呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對他們說：“我問你們：在安息日哪一樣是可以作的呢：作好事還是壞事？救命還是害命？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他們說：「我問你們，在安息日行善行惡，救命害命，哪樣是可以的呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus said to them, “I ask you, is it lawful to do good or to do harm on the Sabbath, to save a life or to destroy it?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jesus said to them, “I ask you, which is lawful on the Sabbath: to do good or to do evil, to save life or to destroy it?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus said to his critics, “I have a question for you. Does the law permit good deeds on the Sabbath, or is it a day for doing evil? Is this a day to save life or to destroy it?”"
      }
    },
