    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂环视众人，然后对那人说：“把手伸出来！”那人的手一伸就复原了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他環視周圍所有的人，就對那人說：“伸出你的手來！”他把手一伸，手就復原了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他就周圍看着他們眾人，對那人說：「伸出手來！」他把手一伸，手就復了原。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After looking around at them all, He said to him, “Stretch out your hand!” And he did so; and his hand was restored."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He looked around at them all, and then said to the man, “Stretch out your hand.” He did so, and his hand was completely restored."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He looked around at them one by one and then said to the man, “Hold out your hand.” So the man held out his hand, and it was restored!"
      }
    },
