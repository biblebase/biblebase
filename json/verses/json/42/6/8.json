    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣知道他们的心思，就对那个右手萎缩的人说：“起来，站在大家面前！”那人就起来站在那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌知道他們的意念，就對那一隻手枯乾了的人說：“起來，站在當中！”那人就起來站著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌卻知道他們的意念，就對那枯乾一隻手的人說：「起來！站在當中。」那人就起來，站着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But He knew what they were thinking, and He said to the man with the withered hand, “Get up and come forward!” And he got up and came forward."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Jesus knew what they were thinking and said to the man with the shriveled hand, “Get up and stand in front of everyone.” So he got up and stood there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jesus knew their thoughts. He said to the man with the deformed hand, “Come and stand in front of everyone.” So the man came forward."
      }
    },
