    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人都受了洗，耶稣也接受了洗礼。祂正在祷告的时候，天开了，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人受了洗，耶穌也受了洗。他正在禱告，天就開了，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾百姓都受了洗，耶穌也受了洗。正禱告的時候，天就開了，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when all the people were baptized, Jesus was also baptized, and while He was praying, heaven was opened,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When all the people were being baptized, Jesus was baptized too. And as he was praying, heaven was opened"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One day when the crowds were being baptized, Jesus himself was baptized. As he was praying, the heavens opened,"
      }
