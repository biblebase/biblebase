    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拿该是玛押的儿子， 玛押是玛他提亚的儿子， 玛他提亚是西美的儿子， 西美是约瑟的儿子， 约瑟是犹大的儿子， 犹大是约亚拿的儿子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪押、瑪他提亞、西美、約瑟、約大、"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拿該是瑪押的兒子；瑪押是瑪他提亞的兒子；瑪他提亞是西美的兒子；西美是約瑟的兒子；約瑟是猶大的兒子；猶大是約亞拿的兒子；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "the son of Maath, the son of Mattathias, the son of Semein, the son of Josech, the son of Joda,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the son of Maath, the son of Mattathias, the son of Semein, the son of Josek, the son of Joda,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Naggai was the son of Maath. Maath was the son of Mattathias. Mattathias was the son of Semein. Semein was the son of Josech. Josech was the son of Joda."
      }
