        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "麦基是亚底的儿子， 亚底是哥桑的儿子， 哥桑是以摩当的儿子， 以摩当是珥的儿子， 珥是约细的儿子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "麥基、亞底、哥桑、以摩當、珥、"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "麥基是亞底的兒子；亞底是哥桑的兒子；哥桑是以摩當的兒子；以摩當是珥的兒子；珥是約細的兒子；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "the son of Melchi, the son of Addi, the son of Cosam, the son of Elmadam, the son of Er,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the son of Melki, the son of Addi, the son of Cosam, the son of Elmadam, the son of Er,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Neri was the son of Melki. Melki was the son of Addi. Addi was the son of Cosam. Cosam was the son of Elmadam. Elmadam was the son of Er."
      }
