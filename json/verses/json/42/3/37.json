    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拉麦是玛土撒拉的儿子， 玛土撒拉是以诺的儿子， 以诺是雅列的儿子， 雅列是玛勒列的儿子， 玛勒列是该南的儿子， 该南是以挪士的儿子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪土撒拉、以諾、雅列、瑪勒列、該南、"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拉麥是瑪土撒拉的兒子；瑪土撒拉是以諾的兒子；以諾是雅列的兒子；雅列是瑪勒列的兒子；瑪勒列是該南的兒子；該南是以挪士的兒子；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "the son of Methuselah, the son of Enoch, the son of Jared, the son of Mahalaleel, the son of Cainan,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the son of Methuselah, the son of Enoch, the son of Jared, the son of Mahalalel, the son of Kenan,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lamech was the son of Methuselah. Methuselah was the son of Enoch. Enoch was the son of Jared. Jared was the son of Mahalalel. Mahalalel was the son of Kenan."
      }
    },
