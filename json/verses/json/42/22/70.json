    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们都问：“那么，你是上帝的儿子吗？”耶稣回答说：“你们说我是。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們說：“那麼你是　神的兒子嗎？”耶穌說：“你們說了，我是。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們都說：「這樣，你是神的兒子嗎？」耶穌說：「你們所說的是。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they all said, “Are You the Son of God, then?” And He said to them, “Yes, I am.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They all asked, “Are you then the Son of God?” He replied, “You say that I am.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They all shouted, “So, are you claiming to be the Son of God?” And he replied, “You say that I am.”"
      }
