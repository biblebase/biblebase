    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对祂说：“如果你是基督，就告诉我们。” 耶稣说：“即使我告诉你们，你们也不会相信。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你若是基督，就告訴我們吧。”耶穌說：“就算我告訴你們，你們也決不相信。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：「你若是基督，就告訴我們。」耶穌說：「我若告訴你們，你們也不信；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If You are the Christ, tell us.” But He said to them, “If I tell you, you will not believe;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If you are the Messiah,” they said, “tell us.” Jesus answered, “If I tell you, you will not believe me,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and they said, “Tell us, are you the Messiah?” But he replied, “If I tell you, you won’t believe me."
      }
