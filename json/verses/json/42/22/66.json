    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天亮后，民间的长老、祭司长和律法教师聚在一起，把耶稣押到他们的公会，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天一亮，民間的長老、祭司長和經學家就都聚集，把他帶到他們的公議會裡，說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天一亮，民間的眾長老連祭司長帶文士都聚會， 把耶穌帶到他們的公會裏，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When it was day, the Council of elders of the people assembled, both chief priests and scribes, and they led Him away to their council chamber, saying,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At daybreak the council of the elders of the people, both the chief priests and the teachers of the law, met together, and Jesus was led before them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At daybreak all the elders of the people assembled, including the leading priests and the teachers of religious law. Jesus was led before this high council,"
      }
    },
