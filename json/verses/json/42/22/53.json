    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我天天和你们一起在圣殿里，你们没有抓我。但现在正是黑暗当权、你们得势的时候了！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我天天跟你們在殿裡，你們不向我下手；但現在是你們的時候了，也是黑暗掌權的時候了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我天天同你們在殿裏，你們不下手拿我。現在卻是你們的時候，黑暗掌權了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While I was with you daily in the temple, you did not lay hands on Me; but this hour and the power of darkness are yours.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every day I was with you in the temple courts, and you did not lay a hand on me. But this is your hour—when darkness reigns.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why didn’t you arrest me in the Temple? I was there every day. But this is your moment, the time when the power of darkness reigns.”"
      }
    },
