    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂的门徒雅各和约翰见状，说：“主啊，你要我们叫天上的火降下来烧死他们吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的門徒雅各、約翰看見了，就說：“主啊，你要我們吩咐火從天降下來，燒滅他們嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的門徒雅各、約翰看見了，就說：「主啊，你要我們吩咐火從天上降下來燒滅他們，像以利亞所做的嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When His disciples James and John saw this, they said, “Lord, do You want us to command fire to come down from heaven and consume them?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the disciples James and John saw this, they asked, “Lord, do you want us to call fire down from heaven to destroy them?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When James and John saw this, they said to Jesus, “Lord, should we call down fire from heaven to burn them up?”"
      }
    },
