    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对他们说：“那么，你们说我是谁？” 彼得回答说：“你是上帝所立的基督。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又問他們：“你們說我是誰？”彼得回答：“是　神的基督！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「你們說我是誰？」彼得回答說：「是神所立的基督。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to them, “But who do you say that I am?” And Peter answered and said, “The Christ of God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But what about you?” he asked. “Who do you say I am?” Peter answered, “God’s Messiah.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he asked them, “But who do you say I am?” Peter replied, “You are the Messiah sent from God!”"
      }
