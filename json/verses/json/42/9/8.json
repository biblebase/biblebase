    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人说：“以利亚显现了”，还有人说：“古代的某个先知复活了”。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又有人說：“以利亞顯現了。”還有人說：“古時的一位先知復活了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又有人說：「是以利亞顯現」；還有人說：「是古時的一個先知又活了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and by some that Elijah had appeared, and by others that one of the prophets of old had risen again."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "others that Elijah had appeared, and still others that one of the prophets of long ago had come back to life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Others thought Jesus was Elijah or one of the other prophets risen from the dead."
      }
    },
