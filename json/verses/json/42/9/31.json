    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "二人在荣光中显现，谈论有关耶稣离世的事情，就是祂在耶路撒冷将要成就的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在榮光裡顯現，談論他去世的事，就是他在耶路撒冷將要完成的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們在榮光裏顯現，談論耶穌去世的事，就是他在耶路撒冷將要成的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "who, appearing in glory, were speaking of His departure which He was about to accomplish at Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They spoke about his departure, which he was about to bring to fulfillment at Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They were glorious to see. And they were speaking about his exodus from this world, which was about to be fulfilled in Jerusalem."
      }
    },
