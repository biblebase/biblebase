    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人群中有一个人高声喊叫：“老师，求求你看看我的儿子吧！他是我的独生子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在人群中有一個人喊叫說：“老師，求你憐憫我的兒子，因為他是我的獨生子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其中有一人喊叫說：「夫子！求你看顧我的兒子，因為他是我的獨生子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And a man from the crowd shouted, saying, “Teacher, I beg You to look at my son, for he is my only boy,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A man in the crowd called out, “Teacher, I beg you to look at my son, for he is my only child."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A man in the crowd called out to him, “Teacher, I beg you to look at my son, my only child."
      }
