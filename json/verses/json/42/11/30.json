    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "正如约拿成了尼尼微人的神迹，人子也要成为这世代的神迹。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約拿怎樣成了尼尼微人的神蹟，人子也照樣成為這個世代的神蹟。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約拿怎樣為尼尼微人成了神蹟，人子也要照樣為這世代的人成了神蹟。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For just as Jonah became a sign to the Ninevites, so will the Son of Man be to this generation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For as Jonah was a sign to the Ninevites, so also will the Son of Man be to this generation."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What happened to him was a sign to the people of Nineveh that God had sent him. What happens to the Son of Man will be a sign to these people that he was sent by God."
      }
    },
