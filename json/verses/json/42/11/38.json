      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这位法利赛人看见耶稣饭前没有行犹太人洗手的礼仪，十分诧异。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這個法利賽人見耶穌吃飯之前不洗手，覺得奇怪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這法利賽人看見耶穌飯前不洗手便詫異。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the Pharisee saw it, he was surprised that He had not first ceremonially washed before the meal."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the Pharisee was surprised when he noticed that Jesus did not first wash before the meal."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "His host was amazed to see that he sat down to eat without first performing the hand-washing ceremony required by Jewish custom."
      }
