    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这时聚集的人越来越多，耶稣说：“这是一个邪恶的世代，人们只想看神迹，但除了约拿的神迹以外，再没有神迹给他们看。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人聚集的時候，耶穌就說：“這世代是個邪惡的世代。這世代尋求神蹟，可是除了約拿的神蹟以外，再沒有神蹟給他們了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當眾人聚集的時候，耶穌開講說：「這世代是一個邪惡的世代。他們求看神蹟，除了約拿的神蹟以外，再沒有神蹟給他們看。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As the crowds were increasing, He began to say, “This generation is a wicked generation; it seeks for a sign, and yet no sign will be given to it but the sign of Jonah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As the crowds increased, Jesus said, “This is a wicked generation. It asks for a sign, but none will be given it except the sign of Jonah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As the crowd pressed in on Jesus, he said, “This evil generation keeps asking me to show them a miraculous sign. But the only sign I will give them is the sign of Jonah."
      }
    },
