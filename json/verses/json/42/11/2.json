    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对他们说：“你们应该这样祷告， “‘天父，愿人都尊崇你的圣名， 愿你的国度降临， 愿你的旨意在地上成就， 就像在天上成就一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說：“你們禱告的時候，要說： ‘父啊，願你的名被尊為聖， 願你的國降臨；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「你們禱告的時候，要說： 我們在天上的父： 願人都尊你的名為聖。 願你的國降臨； 願你的旨意行在地上， 如同行在天上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to them, “When you pray, say: ‘    Father, hallowed be Your name. Your kingdom come."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to them, “When you pray, say: “ ‘Father, hallowed be your name, your kingdom come."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus said, “This is how you should pray: “Father, may your name be kept holy. May your Kingdom come soon."
      }
    },
