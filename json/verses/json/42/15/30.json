    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你这个儿子在娼妓身上耗尽家财，一回来，你倒为他宰了肥牛犊！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但你這個兒子，常常跟娼妓在一起，花盡了你的財產，他一回來，你倒為他宰殺肥牛犢！’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你這個兒子和娼妓吞盡了你的產業，他一來了，你倒為他宰了肥牛犢。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but when this son of yours came, who has devoured your wealth with prostitutes, you killed the fattened calf for him.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But when this son of yours who has squandered your property with prostitutes comes home, you kill the fattened calf for him!’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet when this son of yours comes back after squandering your money on prostitutes, you celebrate by killing the fattened calf!’"
      }
