    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并邀来朋友邻居，说，‘我走失的羊找到了，来和我一同庆祝吧！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "回到家裡，請朋友鄰舍來，對他們說：‘請大家和我一同歡樂，因為我失去的羊已經找到了！’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就請朋友鄰舍來，對他們說：『我失去的羊已經找着了，你們和我一同歡喜吧！』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And when he comes home, he calls together his friends and his neighbors, saying to them, ‘Rejoice with me, for I have found my sheep which was lost!’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and goes home. Then he calls his friends and neighbors together and says, ‘Rejoice with me; I have found my lost sheep.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When he arrives, he will call together his friends and neighbors, saying, ‘Rejoice with me because I have found my lost sheep.’"
      }
