    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“奴仆回答说，‘你弟弟回来了！你父亲因他无灾无难地回来，特地宰了那只肥牛犊。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "僕人說：‘你弟弟回來了，你父親因為他平安無恙地回來，就宰了肥牛犢。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "僕人說：『你兄弟來了；你父親因為得他無災無病地回來，把肥牛犢宰了。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he said to him, ‘Your brother has come, and your father has killed the fattened calf because he has received him back safe and sound.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘Your brother has come,’ he replied, ‘and your father has killed the fattened calf because he has him back safe and sound.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘Your brother is back,’ he was told, ‘and your father has killed the fattened calf. We are celebrating because of his safe return.’"
      }
    },
