    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂伟大无比，将被称为至高者的儿子，主上帝要把祂祖先大卫的王位赐给祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他將要被尊為大，稱為至高者的兒子，主　神要把他祖大衛的王位賜給他，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他要為大，稱為至高者的兒子；主神要把他祖大衛的位給他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will be great and will be called the Son of the Most High; and the Lord God will give Him the throne of His father David;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will be great and will be called the Son of the Most High. The Lord God will give him the throne of his father David,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will be very great and will be called the Son of the Most High. The Lord God will give him the throne of his ancestor David."
      }
    },
