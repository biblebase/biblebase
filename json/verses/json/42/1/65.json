    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "住在周围的人都很惧怕，这消息很快就传遍了整个犹太山区。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "住在周圍的人都害怕，這一切事傳遍了整個猶太山地，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "周圍居住的人都懼怕；這一切的事就傳遍了猶太的山地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Fear came on all those living around them; and all these matters were being talked about in all the hill country of Judea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the neighbors were filled with awe, and throughout the hill country of Judea people were talking about all these things."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Awe fell upon the whole neighborhood, and the news of what had happened spread throughout the Judean hills."
      }
    },
