    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天使到了玛丽亚那里，说：“恭喜你，蒙大恩的女子，主与你同在！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天使進去，對她說：“恭喜！蒙大恩的女子，主與你同在！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天使進去，對她說：「蒙大恩的女子，我問你安，主和你同在了！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And coming in, he said to her, “Greetings, favored one! The Lord is with you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The angel went to her and said, “Greetings, you who are highly favored! The Lord is with you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Gabriel appeared to her and said, “Greetings, favored woman! The Lord is with you!”"
      }
    },
