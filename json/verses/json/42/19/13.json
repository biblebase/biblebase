    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "临行前，他召集了十个奴仆，发给每人一千个银币，吩咐他们，‘你们在我出门期间要用这些钱做生意。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他叫了自己的十個僕人來，給他們一千銀幣，說：‘你們拿去作生意，等到我回來。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "便叫了他的十個僕人來，交給他們十錠銀子，說：『你們去做生意，直等我回來。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he called ten of his slaves, and gave them ten minas and said to them, ‘Do business with this until I come back.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So he called ten of his servants and gave them ten minas.  ‘Put this money to work,’ he said, ‘until I come back.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Before he left, he called together ten of his servants and divided among them ten pounds of silver, saying, ‘Invest this for me while I am gone.’"
      }
