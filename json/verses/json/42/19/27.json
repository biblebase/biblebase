    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "至于那些反对我做王的仇敌，把他们捉回来，在我面前处决。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於我那些仇敵，就是不願意我作王統治他們的，把他們拉到這裡來，在我面前殺掉！’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於我那些仇敵，不要我作他們王的，把他們拉來，在我面前殺了吧！』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But these enemies of mine, who did not want me to reign over them, bring them here and slay them in my presence.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But those enemies of mine who did not want me to be king over them—bring them here and kill them in front of me.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And as for these enemies of mine who didn’t want me to be their king—bring them in and execute them right here in front of me.’”"
      }
