    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "黎明时，耶稣退到一处僻静的地方。众人四处找祂，找到后想留住祂，不让祂离开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天一亮，耶穌出來，到曠野地方去。眾人尋找他，一直找到他那裡，要留住他，不要他離開他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天亮的時候，耶穌出來，走到曠野地方。眾人去找他，到了他那裏，要留住他，不要他離開他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When day came, Jesus left and went to a secluded place; and the crowds were searching for Him, and came to Him and tried to keep Him from going away from them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At daybreak, Jesus went out to a solitary place. The people were looking for him and when they came to where he was, they tried to keep him from leaving them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Early the next morning Jesus went out to an isolated place. The crowds searched everywhere for him, and when they finally found him, they begged him not to leave them."
      }
    },
