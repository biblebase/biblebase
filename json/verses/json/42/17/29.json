    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在罗得离开所多玛那天，烈火和硫磺从天而降，把他们全毁灭了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "直到羅得離開所多瑪的那一天，火與硫磺從天上降下來，把他們全都滅掉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到羅得出所多瑪的那日，就有火與硫磺從天上降下來，把他們全都滅了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but on the day that Lot went out from Sodom it rained fire and brimstone from heaven and destroyed them all."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the day Lot left Sodom, fire and sulfur rained down from heaven and destroyed them all."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "until the morning Lot left Sodom. Then fire and burning sulfur rained down from heaven and destroyed them all."
      }
