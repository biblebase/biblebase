    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂又对门徒说：“时候将到，你们将渴望见到人子降临的日子，可是你们却见不到。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又對門徒說：“日子將到，你們渴望看見人子的一個日子，卻見不到。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又對門徒說：「日子將到，你們巴不得看見人子的一個日子，卻不得看見。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to the disciples, “The days will come when you will long to see one of the days of the Son of Man, and you will not see it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he said to his disciples, “The time is coming when you will long to see one of the days of the Son of Man, but you will not see it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said to his disciples, “The time is coming when you will long to see the day when the Son of Man returns, but you won’t see it."
      }
    },
