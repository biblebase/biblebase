    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "想保全生命的，必丧失生命；丧失生命的，必保全生命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是想保全生命的，必喪掉生命；凡是犧牲生命的，卻必保全生命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡想要保全生命的，必喪掉生命；凡喪掉生命的，必救活生命。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whoever seeks to keep his life will lose it, and whoever loses his life will preserve it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever tries to keep their life will lose it, and whoever loses their life will preserve it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you cling to your life, you will lose it, and if you let your life go, you will save it."
      }
