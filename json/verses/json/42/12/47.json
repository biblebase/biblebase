    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "明知主人的意思却不准备，也不遵行主人吩咐的仆人，必受更重的责打。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那僕人知道主人的意思，卻不預備，也不照他的意思行，必多受責打；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "僕人知道主人的意思，卻不預備，又不順他的意思行，那僕人必多受責打；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And that slave who knew his master’s will and did not get ready or act in accord with his will, will receive many lashes,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The servant who knows the master’s will and does not get ready or does not do what the master wants will be beaten with many blows."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And a servant who knows what the master wants, but isn’t prepared and doesn’t carry out those instructions, will be severely punished."
      }
    },
