    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当人押你们到会堂，或到官员和当权者面前时，不要顾虑如何辩解，或说什么话，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人把你們拉到會堂、官長和當權者的面前，你們不要思慮怎樣申辯或說甚麼話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人帶你們到會堂，並官府和有權柄的人面前，不要思慮怎麼分訴，說甚麼話；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they bring you before the synagogues and the rulers and the authorities, do not worry about how or what you are to speak in your defense, or what you are to say;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When you are brought before synagogues, rulers and authorities, do not worry about how you will defend yourselves or what you will say,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And when you are brought to trial in the synagogues and before rulers and authorities, don’t worry about how to defend yourself or what to say,"
      }
    },
