    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我有要受的‘洗礼’，我何等迫切地想完成这‘洗礼’啊！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我有應當受的洗，我是多麼迫切地期待這事完成。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我有當受的洗還沒有成就，我是何等地迫切呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I have a baptism to undergo, and how distressed I am until it is accomplished!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I have a baptism to undergo, and what constraint I am under until it is completed!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have a terrible baptism of suffering ahead of me, and I am under a heavy burden until it is accomplished."
      }
    },
