    "sermons": [
      {
        "id": "home_sermons_sermon-2015-02-15",
        "title": "事奉神使你的人生更美好",
        "date": "2015-02-15",
        "preacher": "王文堂牧師",
        "verses": "路加福音 12:35-48",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2015/Sermon-2015-02-15.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2015/Sermon-2015-02-15.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无论主人在深夜或黎明回家，若发现奴仆警醒等候，奴仆就有福了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主人也許半夜之前，或天亮之前回來，看見他們這樣，這些僕人就有福了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "或是二更天來，或是三更天來，看見僕人這樣，那僕人就有福了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whether he comes in the second watch, or even in the third, and finds them so, blessed are those slaves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It will be good for those servants whose master finds them ready, even if he comes in the middle of the night or toward daybreak."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He may come in the middle of the night or just before dawn. But whenever he comes, he will reward the servants who are ready."
      }
    },
