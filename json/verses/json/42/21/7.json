    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们问：“老师，这些事什么时候会发生呢？发生的时候有什么预兆呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些人問他：“老師，甚麼時候會有這些事呢？這些事要發生的時候，有甚麼預兆呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們問他說：「夫子！甚麼時候有這事呢？這事將到的時候有甚麼預兆呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They questioned Him, saying, “Teacher, when therefore will these things happen? And what will be the sign when these things are about to take place?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Teacher,” they asked, “when will these things happen? And what will be the sign that they are about to take place?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Teacher,” they asked, “when will all this happen? What sign will show us that these things are about to take place?”"
      }
    },
