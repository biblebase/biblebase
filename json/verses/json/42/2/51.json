    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，耶稣随父母回到拿撒勒，并顺从他们。玛丽亚把这一切事牢记在心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他就同他們下去，回到拿撒勒，並且順從他們。他母親把這一切事，都存在心裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他就同他們下去，回到拿撒勒，並且順從他們。他母親把這一切的事都存在心裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He went down with them and came to Nazareth, and He continued in subjection to them; and His mother treasured all these things in her heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he went down to Nazareth with them and was obedient to them. But his mother treasured all these things in her heart."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he returned to Nazareth with them and was obedient to them. And his mother stored all these things in her heart."
      }
