    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对他们说：“你们为什么找我呢？难道你们不知道我应该在我父的家吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“為甚麼找我呢？你們不知道我必須在我父的家裡嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「為甚麼找我呢？豈不知我應當以我父的事為念嗎 ？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to them, “Why is it that you were looking for Me? Did you not know that I had to be in My Father’s house?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Why were you searching for me?” he asked. “Didn’t you know I had to be in my Father’s house?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But why did you need to search?” he asked. “Didn’t you know that I must be in my Father’s house?”"
      }
    },
