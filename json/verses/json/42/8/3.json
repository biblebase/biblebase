    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "还有希律的管家苦撒的妻子约亚拿，此外还有苏撒拿等其他妇女。她们用自己的钱财资助耶稣和门徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有希律的管家古撒的妻子約亞拿，又有蘇珊娜，和許多別的婦女，她們都用自己的財物供給耶穌和門徒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又有希律的家宰苦撒的妻子約亞拿，並蘇撒拿，和好些別的婦女，都是用自己的財物供給耶穌和門徒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and Joanna the wife of Chuza, Herod’s steward, and Susanna, and many others who were contributing to their support out of their private means."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Joanna the wife of Chuza, the manager of Herod’s household; Susanna; and many others. These women were helping to support them out of their own means."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Joanna, the wife of Chuza, Herod’s business manager; Susanna; and many others who were contributing from their own resources to support Jesus and his disciples."
      }
    },
