    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“有一个农夫到田里撒种，有些种子落在路旁，被人践踏，又被飞鸟吃掉了；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“有一個撒種的出去撒種，撒的時候，有的落在路旁，被人踐踏，或給空中的小鳥吃掉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「有一個撒種的出去撒種。撒的時候，有落在路旁的，被人踐踏，天上的飛鳥又來吃盡了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The sower went out to sow his seed; and as he sowed, some fell beside the road, and it was trampled under foot and the birds of the air ate it up."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“A farmer went out to sow his seed. As he was scattering the seed, some fell along the path; it was trampled on, and the birds ate it up."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“A farmer went out to plant his seed. As he scattered it across his field, some seed fell on a footpath, where it was stepped on, and the birds ate it."
      }
    },
