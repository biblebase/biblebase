    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有些落在沃土里，发芽生长，结出百倍的果实。”耶稣讲完这番话后，高声说：“有耳可听的，就要留心听。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有的落在好土裡，就生長起來，結出百倍的果實。”他說了這些話，就大聲說：“有耳可聽的，就應當聽！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又有落在好土裏的，生長起來，結實百倍。」耶穌說了這些話，就大聲說：「有耳可聽的，就應當聽！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Other seed fell into the good soil, and grew up, and produced a crop a hundred times as great.” As He said these things, He would call out, “He who has ears to hear, let him hear.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Still other seed fell on good soil. It came up and yielded a crop, a hundred times more than was sown.” When he said this, he called out, “Whoever has ears to hear, let them hear.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Still other seed fell on fertile soil. This seed grew and produced a crop that was a hundred times as much as had been planted!” When he had said this, he called out, “Anyone with ears to hear should listen and understand.”"
      }
    },
