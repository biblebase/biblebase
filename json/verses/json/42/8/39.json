    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你回家吧，把上帝为你所行的奇事告诉别人。”他就回去，走遍全城，四处传扬耶稣为他所行的奇事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你回家去，述說　神為你作了怎樣的事。”他就走遍全城，傳講耶穌為他作了怎樣的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你回家去，傳說神為你做了何等大的事。」他就去，滿城裏傳揚耶穌為他做了何等大的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Return to your house and describe what great things God has done for you.” So he went away, proclaiming throughout the whole city what great things Jesus had done for him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Return home and tell how much God has done for you.” So the man went away and told all over town how much Jesus had done for him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“No, go back to your family, and tell them everything God has done for you.” So he went all through the town proclaiming the great things Jesus had done for him."
      }
    },
