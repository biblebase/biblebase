    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "掩盖的事终会暴露出来，隐藏的秘密终会被人知道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為沒有甚麼隱藏的事不被顯明，也沒有甚麼掩蓋的事不被人知道而暴露出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為掩藏的事沒有不顯出來的；隱瞞的事沒有不露出來被人知道的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For nothing is hidden that will not become evident, nor anything secret that will not be known and come to light."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For there is nothing hidden that will not be disclosed, and nothing concealed that will not be known or brought out into the open."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For all that is secret will eventually be brought into the open, and everything that is concealed will be brought to light and made known to all."
      }
    },
