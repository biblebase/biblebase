    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "门徒过来唤醒耶稣，说：“主啊，主啊！我们快淹死了！” 耶稣醒来，就斥责风浪，顿时风平浪静了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "門徒來叫醒耶穌，說：“主啊！主啊！我們沒命了！”他醒過來，斥責風浪，風浪就止息、平靜了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門徒來叫醒了他，說：「夫子！夫子！我們喪命啦！」耶穌醒了，斥責那狂風大浪；風浪就止住，平靜了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They came to Jesus and woke Him up, saying, “Master, Master, we are perishing!” And He got up and rebuked the wind and the surging waves, and they stopped, and it became calm."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The disciples went and woke him, saying, “Master, Master, we’re going to drown!” He got up and rebuked the wind and the raging waters; the storm subsided, and all was calm."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The disciples went and woke him up, shouting, “Master, Master, we’re going to drown!” When Jesus woke up, he rebuked the wind and the raging waves. Suddenly the storm stopped and all was calm."
      }
    },
