    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣问他们：“你们一路上在谈论什么？” 他们停下脚步，满面愁容，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說：“你們走路的時候，彼此討論的是甚麼事呢？”他們就站住，面帶愁容；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他們說：「你們走路彼此談論的是甚麼事呢？」他們就站住，臉上帶着愁容。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to them, “What are these words that you are exchanging with one another as you are walking?” And they stood still, looking sad."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He asked them, “What are you discussing together as you walk along?” They stood still, their faces downcast."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He asked them, “What are you discussing so intently as you walk along?” They stopped short, sadness written across their faces."
      }
    },
