    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“后来我们有几个人亲自去坟墓察看，果然像她们所说的，耶稣的遗体不见了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又有我們中間的幾個人到墳墓那裡去，所看見的正如婦女們所說的，只是沒有看見他。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又有我們的幾個人往墳墓那裏去，所遇見的正如婦女們所說的，只是沒有看見他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Some of those who were with us went to the tomb and found it just exactly as the women also had said; but Him they did not see.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then some of our companions went to the tomb and found it just as the women had said, but they did not see Jesus.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some of our men ran out to see, and sure enough, his body was gone, just as the women had said.”"
      }
    },
