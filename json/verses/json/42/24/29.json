    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们极力挽留祂，说：“天快黑了，时候不早了，跟我们一同住宿吧。”耶稣就和他们一起进村住下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們強留他說：“天晚了，太陽下山了，請與我們同住吧！”他就進去與他們同住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們卻強留他，說：「時候晚了，日頭已經平西了，請你同我們住下吧！」耶穌就進去，要同他們住下。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But they urged Him, saying, “Stay with us, for it is getting toward evening, and the day is now nearly over.” So He went in to stay with them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But they urged him strongly, “Stay with us, for it is nearly evening; the day is almost over.” So he went in to stay with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "but they begged him, “Stay the night with us, since it is getting late.” So he went home with them."
      }
    },
