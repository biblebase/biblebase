    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "忽然他们眼睛明亮了，认出是耶稣。耶稣很快从他们眼前消失了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的眼睛開了，才認出是耶穌來；他卻從他們面前不見了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的眼睛明亮了，這才認出他來。忽然耶穌不見了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then their eyes were opened and they recognized Him; and He vanished from their sight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then their eyes were opened and they recognized him, and he disappeared from their sight."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Suddenly, their eyes were opened, and they recognized him. And at that moment he disappeared!"
      }
    },
