    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼得却起身跑到墓地，屈身往墓里张望，只见细麻布在那里，他便离开了，对发生的事大惑不解。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼得卻起來跑到墳墓那裡，屈身往裡觀看，只見細麻布在那裡，就回去了，對所發生的事非常驚奇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼得起來，跑到墳墓前，低頭往裏看，見細麻布獨在一處，就回去了，心裏希奇所成的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Peter got up and ran to the tomb; stooping and looking in, he *saw the linen wrappings only; and he went away to his home, marveling at what had happened."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Peter, however, got up and ran to the tomb. Bending over, he saw the strips of linen lying by themselves, and he went away, wondering to himself what had happened."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "However, Peter jumped up and ran to the tomb to look. Stooping, he peered in and saw the empty linen wrappings; then he went home again, wondering what had happened."
      }
    },
