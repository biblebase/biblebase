    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我告诉你们，要善用今世的钱财，广结朋友。这样，当钱财不管用时，他们会接你们到永远的居所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我告訴你們，要用不義的錢財去結交朋友，好叫錢財無用的時候，他們可以接你們到永恆的帳棚裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我又告訴你們，要藉着那不義的錢財結交朋友，到了錢財無用的時候，他們可以接你們到永存的帳幕裏去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And I say to you, make friends for yourselves by means of the wealth of unrighteousness, so that when it fails, they will receive you into the eternal dwellings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I tell you, use worldly wealth to gain friends for yourselves, so that when it is gone, you will be welcomed into eternal dwellings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Here’s the lesson: Use your worldly resources to benefit others and make friends. Then, when your possessions are gone, they will welcome you to an eternal home."
      }
    },
