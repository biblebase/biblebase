    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“他回答说，‘一百桶油。’ “管家说，‘这是你的帐，快坐下，改成五十吧！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：‘四千公升油。’管家說：‘拿你的欠單去，快坐下寫二千。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：『一百簍油。』管家說：『拿你的帳，快坐下，寫五十。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he said, ‘A hundred measures of oil.’ And he said to him, ‘Take your bill, and sit down quickly and write fifty.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Nine hundred gallons    of olive oil,’ he replied. “The manager told him, ‘Take your bill, sit down quickly, and make it four hundred and fifty.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The man replied, ‘I owe him 800 gallons of olive oil.’ So the manager told him, ‘Take the bill and quickly change it to 400 gallons.’"
      }
