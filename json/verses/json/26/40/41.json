    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即里外各有四张桌子，共八张，供祭司屠宰祭牲之用。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在大門兩邊，這邊有四張桌子，那邊也有四張桌子，共有八張，供宰殺祭牲之用。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門這邊有四張桌子，那邊有四張桌子，共八張；在其上祭司宰殺犧牲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Four tables were on each side next to the gate; or, eight tables on which they slaughter sacrifices."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So there were four tables on one side of the gateway and four on the other—eight tables in all—on which the sacrifices were slaughtered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So there were eight tables in all—four inside and four outside—where the sacrifices were cut up and prepared."
      }
    },
