    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他从门口的墙量到外院的墙柱，是三十米。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又沿著裡面各牆柱量度，直到朝向院子的牆柱，共長三十公尺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又量廊子六十肘，牆柱外是院子，有廊為界，在門洞兩邊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He made the side pillars sixty cubits high; the gate extended round about to the side pillar of the courtyard."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He measured along the faces of the projecting walls all around the inside of the gateway—sixty cubits. The measurement was up to the portico facing the courtyard."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He measured the dividing walls all along the inside of the gateway up to the entry room of the gateway; this distance was 105 feet."
      }
