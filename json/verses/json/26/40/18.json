      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "铺石地在门口的两边，比内院低，宽度与入口的长度一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "鋪石地在各大門的旁邊，比門矮一點，寬度與門的長度相同。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "鋪石地，就是矮鋪石地在各門洞兩旁，以門洞的長短為度。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The pavement (that is, the lower pavement) was by the side of the gates, corresponding to the length of the gates."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It abutted the sides of the gateways and was as wide as they were long; this was the lower pavement."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This pavement flanked the gates and extended out from the walls into the courtyard the same distance as the gateway entrance. This was the lower pavement."
      }
