    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们是我草场上的羊，你们是我的子民，我是你们的上帝。这是主耶和华说的。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們是我的羊，是我草場上的羊；我是你們的　神。這是耶和華的宣告。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們作我的羊，我草場上的羊，乃是以色列人，我也是你們的神。這是主耶和華說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“As for you, My sheep, the sheep of My pasture, you are men, and I am your God,” declares the Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are my sheep, the sheep of my pasture, and I am your God, declares the Sovereign Lord.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You are my flock, the sheep of my pasture. You are my people, and I am your God. I, the Sovereign Lord, have spoken!”"
      }
    },
