    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要赐福他们，也要赐福我圣山的周围，我要按季节降雨给他们，使恩福如雨降下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必使他們和我的山四圍都蒙福；我必按時降雨，這就是賜福的雨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必使他們與我山的四圍成為福源，我也必叫時雨落下，必有福如甘霖而降。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will make them and the places around My hill a blessing. And I will cause showers to come down in their season; they will be showers of blessing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will make them and the places surrounding my hill a blessing. I will send down showers in season; there will be showers of blessing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will bless my people and their homes around my holy hill. And in the proper season I will send the showers they need. There will be showers of blessing."
      }
    },
