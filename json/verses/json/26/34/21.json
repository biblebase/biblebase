    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们肥羊用胁和肩拱，用角抵瘦弱的，使他们四散。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你們用腰用肩擁擠所有瘦弱的，又用角牴觸牠們，直到把牠們驅散到外面，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你們用脅用肩擁擠一切瘦弱的，又用角牴觸，以致使牠們四散。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because you push with side and with shoulder, and thrust at all the weak with your horns until you have scattered them abroad,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because you shove with flank and shoulder, butting all the weak sheep with your horns until you have driven them away,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you fat sheep pushed and butted and crowded my sick and hungry flock until you scattered them to distant lands."
      }
    },
