    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我进去后，看见四周的墙上画满了各种爬虫、可憎的兽类和以色列人拜的神像。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是我進去觀看，見四圍的牆上刻著各種形狀的爬行的動物，和可憎惡的走獸，以及以色列家所有的偶像。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我進去一看，誰知，在四面牆上畫着各樣爬物和可憎的走獸，並以色列家一切的偶像。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I entered and looked, and behold, every form of creeping things and beasts and detestable things, with all the idols of the house of Israel, were carved on the wall all around."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I went in and looked, and I saw portrayed all over the walls all kinds of crawling things and unclean animals and all the idols of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I went in and saw the walls covered with engravings of all kinds of crawling animals and detestable creatures. I also saw the various idols worshiped by the people of Israel."
      }
    },
