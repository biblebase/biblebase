    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第六年六月五日，我正在家中与犹大的众长老同坐，主耶和华的能力降在我身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "第六年六月五日，我坐在家中，猶大的眾長老也坐在我面前，在那裡主耶和華的靈降在我身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "第六年六月初五日，我坐在家中；猶大的眾長老坐在我面前。在那裏主耶和華的靈降在我身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It came about in the sixth year, on the fifth day of the sixth month, as I was sitting in my house with the elders of Judah sitting before me, that the hand of the Lord God fell on me there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the sixth year, in the sixth month on the fifth day, while I was sitting in my house and the elders of Judah were sitting before me, the hand of the Sovereign Lord came on me there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then on September 17, during the sixth year of King Jehoiachin’s captivity, while the leaders of Judah were in my home, the Sovereign Lord took hold of me."
      }
    },
