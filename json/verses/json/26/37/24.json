    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘我的仆人大卫要做王治理他们，成为他们唯一的牧人。他们必恪守我的典章，谨遵我的律例。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘我的僕人大衛必作他們的王；他們眾人只有一個牧者。他們必遵行我的典章，謹守實行我的律例。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我的僕人大衛必作他們的王；眾民必歸一個牧人。他們必順從我的典章，謹守遵行我的律例。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“My servant David will be king over them, and they will all have one shepherd; and they will walk in My ordinances and keep My statutes and observe them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘My servant David will be king over them, and they will all have one shepherd. They will follow my laws and be careful to keep my decrees."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“My servant David will be their king, and they will have only one shepherd. They will obey my regulations and be careful to keep my decrees."
      }
    },
