      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的子民啊，当我这样做的时候，你们就知道我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的子民哪！我打開你們的墳墓，把你們從墳墓中領上來的時候，你們就知道我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的民哪，我開你們的墳墓，使你們從墳墓中出來，你們就知道我是耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then you will know that I am the Lord, when I have opened your graves and caused you to come up out of your graves, My people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then you, my people, will know that I am the Lord, when I open your graves and bring you up from them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When this happens, O my people, you will know that I am the Lord."
      }
