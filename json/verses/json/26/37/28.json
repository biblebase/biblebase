    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "各国的人看见我的圣所永远设立在他们当中，便会知道是我耶和华使以色列圣洁了。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的聖所必永遠在他們中間，列國就知道我是耶和華，是我使以色列成聖的。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的聖所在以色列人中間直到永遠，外邦人就必知道我是叫以色列成為聖的耶和華。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the nations will know that I am the Lord who sanctifies Israel, when My sanctuary is in their midst forever.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the nations will know that I the Lord make Israel holy, when my sanctuary is among them forever.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when my Temple is among them forever, the nations will know that I am the Lord, who makes Israel holy.”"
      }
    },
