    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，我便按耶和华的吩咐说预言，气息就进入他们里面，他们便复活了，并且站立起来，成为一支大军。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是我遵命說預言，氣息就進入骸骨裡面，骸骨就活過來，並且站起，成了一支極大的軍隊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是我遵命說預言，氣息就進入骸骨，骸骨便活了，並且站起來，成為極大的軍隊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I prophesied as He commanded me, and the breath came into them, and they came to life and stood on their feet, an exceedingly great army."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I prophesied as he commanded me, and breath entered them; they came to life and stood up on their feet—a vast army."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I spoke the message as he commanded me, and breath came into their bodies. They all came to life and stood up on their feet—a great army."
      }
