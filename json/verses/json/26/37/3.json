    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂对我说：“人子啊，这些骸骨能复活吗？”我说：“主耶和华啊，只有你知道。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他問我：“人子啊！這些骸骨能活過來嗎？”我回答：“主耶和華啊！唯有你才知道。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對我說：「人子啊，這些骸骨能復活嗎？」我說：「主耶和華啊，你是知道的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to me, “Son of man, can these bones live?” And I answered, “O Lord God, You know.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He asked me, “Son of man, can these bones live?” I said, “Sovereign Lord, you alone know.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he asked me, “Son of man, can these bones become living people again?” “O Sovereign Lord,” I replied, “you alone know the answer to that.”"
      }
