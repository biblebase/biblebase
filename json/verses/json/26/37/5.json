    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华说，我要使气息进入你们里面，你们就必复活过来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶和華對這些骸骨這樣說：看哪！我必使氣息進入你們裡面，你們就活過來了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華對這些骸骨如此說：『我必使氣息進入你們裏面，你們就要活了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord God to these bones, ‘Behold, I will cause breath to enter you that you may come to life."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Sovereign Lord says to these bones: I will make breath enter you, and you will come to life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Sovereign Lord says: Look! I am going to put breath into you and make you live again!"
      }
    },
