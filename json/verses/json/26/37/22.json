    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使他们在以色列的土地和群山上成为一国，只有一位王统治他们，不再分为两个国家。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要使他們在那地上，在以色列的眾山上，成為一國；必有一王統治他們眾人；他們不再是兩個國，不再分為兩個邦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要使他們在那地，在以色列山上成為一國，有一王作他們眾民的王。他們不再為二國，決不再分為二國；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and I will make them one nation in the land, on the mountains of Israel; and one king will be king for all of them; and they will no longer be two nations and no longer be divided into two kingdoms."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will make them one nation in the land, on the mountains of Israel. There will be one king over all of them and they will never again be two nations or be divided into two kingdoms."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will unify them into one nation on the mountains of Israel. One king will rule them all; no longer will they be divided into two nations or into two kingdoms."
      }
    },
