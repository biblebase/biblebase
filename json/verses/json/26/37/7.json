    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，我便按耶和华的吩咐向枯骨发预言，我正说预言的时候，忽然有震动的声音，骨头便连在了一起。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是我遵命說預言；我正在說預言的時候，就聽見有響聲，骨頭與骨頭互相結合起來，格格作響。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，我遵命說預言。正說預言的時候，不料，有響聲，有地震；骨與骨互相聯絡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I prophesied as I was commanded; and as I prophesied, there was a noise, and behold, a rattling; and the bones came together, bone to its bone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I prophesied as I was commanded. And as I was prophesying, there was a noise, a rattling sound, and the bones came together, bone to bone."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I spoke this message, just as he told me. Suddenly as I spoke, there was a rattling noise all across the valley. The bones of each body came together and attached themselves as complete skeletons."
      }
    },
