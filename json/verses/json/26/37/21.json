    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "告诉他们，主耶和华这样说，‘我要把以色列的百姓从他们分散的各国招聚起来，领他们回归故土。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要對他們說：‘主耶和華這樣說：看哪！我要把以色列人從他們所到的列國中領出來，又從四圍聚集他們，把他們領回故土。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要對他們說，主耶和華如此說：我要將以色列人從他們所到的各國收取，又從四圍聚集他們，引導他們歸回本地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Say to them, ‘Thus says the Lord God, “Behold, I will take the sons of Israel from among the nations where they have gone, and I will gather them from every side and bring them into their own land;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and say to them, ‘This is what the Sovereign Lord says: I will take the Israelites out of the nations where they have gone. I will gather them from all around and bring them back into their own land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And give them this message from the Sovereign Lord: I will gather the people of Israel from among the nations. I will bring them home to their own land from the places where they have been scattered."
      }
    },
