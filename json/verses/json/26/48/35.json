    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "城的四周共长十公里，从那时起，这城必叫‘耶和华的居所’。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "城的周圍共長九千公尺。從此以後，這城的名字要稱為‘耶和華的所在’。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "城四圍共一萬八千肘。從此以後，這城的名字必稱為「耶和華的所在」。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The city shall be 18,000 cubits round about; and the name of the city from that day shall be, ‘The Lord is there.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The distance all around will be 18,000 cubits. “And the name of the city from that time on will be: the Lord is there.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The distance around the entire city will be 6 miles. And from that day the name of the city will be ‘The Lord Is There.’”"
      }
