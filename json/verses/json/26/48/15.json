    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“剩下的十二点五公里长、二点五公里宽的地方是公用的，用来建城邑、房屋和草场，城要建在地的中央。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“所剩下的地，寬兩公里半，長十二公里半，要作俗地，供城市、居民和郊野之用。城要在其中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這二萬五千肘前面所剩下五千肘寬之地要作俗用，作為造城蓋房郊野之地。城要在當中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The remainder, 5,000 cubits in width and 25,000 in length, shall be for common use for the city, for dwellings and for open spaces; and the city shall be in its midst."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The remaining area, 5,000 cubits wide and 25,000 cubits long, will be for the common use of the city, for houses and for pastureland. The city will be in the center of it"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“An additional strip of land 81/3 miles long by 12/3 miles wide, south of the sacred Temple area, will be allotted for public use—homes, pasturelands, and common lands, with a city at the center."
      }
    },
