    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人却说，‘主这样做不公平！’以色列人啊，是我的做法不公平呢？还是你们的做法不公平呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但以色列家還說：‘主所行的不公平。’以色列家啊，我所行的哪裡不公平呢？豈不是你們所行的不公平嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列家還說：『主的道不公平！』以色列家啊，我的道豈不公平嗎？你們的道豈不是不公平嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the house of Israel says, ‘The way of the Lord is not right.’ Are My ways not right, O house of Israel? Is it not your ways that are not right?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet the Israelites say, ‘The way of the Lord is not just.’ Are my ways unjust, people of Israel? Is it not your ways that are unjust?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And yet the people of Israel keep saying, ‘The Lord isn’t doing what’s right!’ O people of Israel, it is you who are not doing what’s right, not I."
      }
