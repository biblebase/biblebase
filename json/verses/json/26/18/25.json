    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们说，主这样做不公平。以色列人啊，你们听着，是我的做法不公平呢？还是你们的做法不公平呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們還說：‘主所行的不公平。’以色列家啊，你們要聽！我所行的哪裡不公平呢？豈不是你們所行的不公平嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們還說：『主的道不公平！』以色列家啊，你們當聽，我的道豈不公平嗎？你們的道豈不是不公平嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet you say, ‘The way of the Lord is not right.’ Hear now, O house of Israel! Is My way not right? Is it not your ways that are not right?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Yet you say, ‘The way of the Lord is not just.’ Hear, you Israelites: Is my way unjust? Is it not your ways that are unjust?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yet you say, ‘The Lord isn’t doing what’s right!’ Listen to me, O people of Israel. Am I the one not doing what’s right, or is it you?"
      }
    },
