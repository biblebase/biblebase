    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们以色列人为什么说‘父亲吃了酸葡萄，儿子的牙酸倒了’这句俗语呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“論到以色列地，你們為甚麼引用這俗語說‘父親吃了酸葡萄，兒子的牙酸倒了’呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們在以色列地怎麼用這俗語說『父親吃了酸葡萄，兒子的牙酸倒了』呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“What do you mean by using this proverb concerning the land of Israel, saying, ‘The fathers eat the sour grapes, But the children’s teeth are set on edge’?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“What do you people mean by quoting this proverb about the land of Israel: “ ‘The parents eat sour grapes, and the children’s teeth are set on edge’?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why do you quote this proverb concerning the land of Israel: ‘The parents have eaten sour grapes, but their children’s mouths pucker at the taste’?"
      }
    },
