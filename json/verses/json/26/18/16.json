    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不欺压他人，不扣留抵押品，不抢夺财物，反而给饥饿的人食物吃，给赤身露体的人衣服穿，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不欺壓任何人，不索取抵押品，不搶奪人的物件，卻把自己的食物給飢餓的人，把衣服給赤身的人穿著；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "未曾虧負人，未曾取人的當頭，未曾搶奪人的物件，卻將食物給飢餓的人吃，將衣服給赤身的人穿，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "or oppress anyone, or retain a pledge, or commit robbery, but he gives his bread to the hungry and covers the naked with clothing,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He does not oppress anyone or require a pledge for a loan. He does not commit robbery but gives his food to the hungry and provides clothing for the naked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He does not exploit the poor, but instead is fair to debtors and does not rob them. He gives food to the hungry and provides clothes for the needy."
      }
    },
