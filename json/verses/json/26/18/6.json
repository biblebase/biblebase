    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不在山上的庙宇里吃祭物，不拜以色列的偶像，不玷污别人的妻子，不亲近经期中的妇女；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他不在山上吃祭肉，眼目也不仰望以色列家的偶像，不玷污鄰舍的妻子，不親近在經期中的婦人；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "未曾在山上吃過祭偶像之物，未曾仰望以色列家的偶像，未曾玷污鄰舍的妻，未曾在婦人的經期內親近她，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and does not eat at the mountain shrines or lift up his eyes to the idols of the house of Israel, or defile his neighbor’s wife or approach a woman during her menstrual period—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He does not eat at the mountain shrines or look to the idols of Israel. He does not defile his neighbor’s wife or have sexual relations with a woman during her period."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He does not feast in the mountains before Israel’s idols or worship them. He does not commit adultery or have intercourse with a woman during her menstrual period."
      }
    },
