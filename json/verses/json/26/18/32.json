    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华说，‘我断不喜欢任何人死亡，你们要悔改，以便存活。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我不喜悅任何人滅亡，所以你們要回轉，得以存活。這是主耶和華的宣告。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華說：我不喜悅那死人之死，所以你們當回頭而存活。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I have no pleasure in the death of anyone who dies,” declares the Lord God. “Therefore, repent and live.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I take no pleasure in the death of anyone, declares the Sovereign Lord. Repent and live!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I don’t want you to die, says the Sovereign Lord. Turn back and live!"
      }
    },
