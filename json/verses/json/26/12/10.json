    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要对他们说，‘主耶和华说，这是关于耶路撒冷的首领和城中以色列人的预兆。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要對他們說：‘主耶和華這樣說：這默示是關乎耶路撒冷的君王，和在它中間的以色列全家。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要對他們說：『主耶和華如此說：這是關乎耶路撒冷的君王和他周圍以色列全家的預表。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Say to them, ‘Thus says the Lord God, “This burden concerns the prince in Jerusalem as well as all the house of Israel who are in it.” ’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Say to them, ‘This is what the Sovereign Lord says: This prophecy concerns the prince in Jerusalem and all the Israelites who are there.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Say to them, ‘This is what the Sovereign Lord says: These actions contain a message for King Zedekiah in Jerusalem and for all the people of Israel.’"
      }
    },
