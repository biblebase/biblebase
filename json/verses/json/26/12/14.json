    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要把他周围的随从和所有军队驱散到四方，并且拔刀追杀他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要把他周圍所有的人，就是所有幫助他的和他所有的軍隊，分散到四方去，也要拔出刀來追趕他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "周圍一切幫助他的和他所有的軍隊，我必分散四方，也要拔刀追趕他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will scatter to every wind all who are around him, his helpers and all his troops; and I will draw out a sword after them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will scatter to the winds all those around him—his staff and all his troops—and I will pursue them with drawn sword."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will scatter his servants and warriors to the four winds and send the sword after them."
      }
    },
