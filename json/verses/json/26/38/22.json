    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要用瘟疫、杀戮、暴雨、冰雹、烈火和硫磺惩罚他们和他们的联军。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必用瘟疫和血腥懲罰他；我必把暴雨、大冰雹、火和硫磺降在他和他的軍隊，以及與他在一起的許多民族身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必用瘟疫和流血的事刑罰他。我也必將暴雨、大雹與火，並硫磺降與他和他的軍隊，並他所率領的眾民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "With pestilence and with blood I will enter into judgment with him; and I will rain on him and on his troops, and on the many peoples who are with him, a torrential rain, with hailstones, fire and brimstone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will execute judgment on him with plague and bloodshed; I will pour down torrents of rain, hailstones and burning sulfur on him and on his troops and on the many nations with him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will punish you and your armies with disease and bloodshed; I will send torrential rain, hailstones, fire, and burning sulfur!"
      }
    },
