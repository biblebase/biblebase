    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘主耶和华说，“那时你必心起歹念，筹划恶谋。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘主耶和華這樣說：到那日你心中必起意念，圖謀惡計，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華如此說：「到那時，你心必起意念，圖謀惡計，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Thus says the Lord God, “It will come about on that day, that thoughts will come into your mind and you will devise an evil plan,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘This is what the Sovereign Lord says: On that day thoughts will come into your mind and you will devise an evil scheme."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Sovereign Lord says: At that time evil thoughts will come to your mind, and you will devise a wicked scheme."
      }
    },
