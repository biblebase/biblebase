      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘你和你的联军都要预备好，你要做他们的统帅。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘你要準備好，你和那聚集到你那裡的各隊都要準備好；你要作他們的大帥。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「那聚集到你這裏的各隊都當準備；你自己也要準備，作他們的大帥。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Be prepared, and prepare yourself, you and all your companies that are assembled about you, and be a guard for them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Get ready; be prepared, you and all the hordes gathered about you, and take command of them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Get ready; be prepared! Keep all the armies around you mobilized, and take command of them."
      }
    },
