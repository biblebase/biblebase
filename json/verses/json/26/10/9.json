    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我又看见四个基路伯天使旁各有一个闪耀如绿宝石的轮子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我觀看，見眾基路伯旁邊有四個輪子；每一個基路伯旁邊都有一個輪子；輪子的樣子好像閃耀的水蒼玉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我又觀看，見基路伯旁邊有四個輪子。這基路伯旁有一個輪子，那基路伯旁有一個輪子，每基路伯都是如此；輪子的顏色彷彿水蒼玉。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I looked, and behold, four wheels beside the cherubim, one wheel beside each cherub; and the appearance of the wheels was like the gleam of a Tarshish stone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I looked, and I saw beside the cherubim four wheels, one beside each of the cherubim; the wheels sparkled like topaz."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I looked, and each of the four cherubim had a wheel beside him, and the wheels sparkled like beryl."
      }
    },
