    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂对那穿细麻衣的人说：“你要从基路伯天使下面旋转的轮子中取火。”那人便进去，站在一个轮子旁边。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他吩咐那身穿細麻布衣服的人說：“你要從輪子之間，就是從基路伯之間取火。”那人就進去，站在一個輪子的旁邊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他吩咐那穿細麻衣的人說：「要從旋轉的輪內基路伯中間取火。」那人就進去站在一個輪子旁邊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It came about when He commanded the man clothed in linen, saying, “Take fire from between the whirling wheels, from between the cherubim,” he entered and stood beside a wheel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the Lord commanded the man in linen, “Take fire from among the wheels, from among the cherubim,” the man went in and stood beside a wheel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord said to the man in linen clothing, “Go between the cherubim and take some burning coals from between the wheels.” So the man went in and stood beside one of the wheels."
      }
    },
