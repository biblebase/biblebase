      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基路伯的全身、背、手和翅膀，以及那些輪子，都布滿了眼睛；四個基路伯的輪子都是這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們全身，連背帶手和翅膀，並輪周圍都滿了眼睛。這四個基路伯的輪子都是如此。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their whole body, their backs, their hands, their wings and the wheels were full of eyes all around, the wheels belonging to all four of them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their entire bodies, including their backs, their hands and their wings, were completely full of eyes, as were their four wheels."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Both the cherubim and the wheels were covered with eyes. The cherubim had eyes all over their bodies, including their hands, their backs, and their wings."
      }
