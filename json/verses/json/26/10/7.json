    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有一个基路伯天使伸手到他们中间的火里，取了些火放在那人手里，那人就拿了出去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一個基路伯從眾基路伯之間，伸手到他們中間的火那裡去，取了些火，放在那身穿細麻布衣服的人手中；那人拿了火，就出去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一個基路伯從基路伯中伸手到基路伯中間的火那裏，取些放在那穿細麻衣的人兩手中，那人就拿出去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the cherub stretched out his hand from between the cherubim to the fire which was between the cherubim, took some and put it into the hands of the one clothed in linen, who took it and went out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then one of the cherubim reached out his hand to the fire that was among them. He took up some of it and put it into the hands of the man in linen, who took it and went out."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then one of the cherubim reached out his hand and took some live coals from the fire burning among them. He put the coals into the hands of the man in linen clothing, and the man took them and went out."
      }
    },
