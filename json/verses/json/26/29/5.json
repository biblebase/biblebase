    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "丢弃在旷野。 你必暴尸野外，无人收殓。 我要让你成为野兽和飞鸟的食物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必把你和你河中所有的魚，都丟在曠野； 你必倒在田間，無人收殮，無人埋葬。 我已經把你給了地上的走獸和空中的飛鳥作食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "把你並江河中的魚都拋在曠野； 你必倒在田間， 不被收殮，不被掩埋。 我已將你給地上野獸、空中飛鳥作食物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will abandon you to the wilderness, you and all the fish of your rivers; You will fall on the open field; you will not be brought together or gathered. I have given you for food to the beasts of the earth and to the birds of the sky."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will leave you in the desert, you and all the fish of your streams. You will fall on the open field and not be gathered or picked up. I will give you as food to the beasts of the earth and the birds of the sky."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will leave you and all your fish stranded in the wilderness to die. You will lie unburied on the open ground, for I have given you as food to the wild animals and birds."
      }
    },
