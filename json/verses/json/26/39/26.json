    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当他们在故土安居，没有人来威胁他们的时候，他们就会忘记自己所有的羞辱和得罪我的恶事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在自己的土地上安然居住、無人驚嚇的時候，就要擔當自己的羞辱和向我所犯一切不忠的罪；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will forget their disgrace and all their treachery which they perpetrated against Me, when they live securely on their own land with no one to make them afraid."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will forget their shame and all the unfaithfulness they showed toward me when they lived in safety in their land with no one to make them afraid."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will accept responsibility for their past shame and unfaithfulness after they come home to live in peace in their own land, with no one to bother them."
      }
    },
