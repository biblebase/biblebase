    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它们要在我为它们设的祭宴中任意吃肉饮血，直到吃饱喝醉为止。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們吃我為你們所預備的祭，必吃飽脂油，喝醉了血。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們吃我為你們所獻的祭，必吃飽了脂油，喝醉了血。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So you will eat fat until you are glutted, and drink blood until you are drunk, from My sacrifice which I have sacrificed for you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At the sacrifice I am preparing for you, you will eat fat till you are glutted and drink blood till you are drunk."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Gorge yourselves with flesh until you are glutted; drink blood until you are drunk. This is the sacrificial feast I have prepared for you."
      }
    },
