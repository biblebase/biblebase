    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那个身穿细麻衣，腰带墨盒的人回来禀告说：“我已照你的吩咐做了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那身穿細麻布衣服，腰間帶著墨盒子的人回報這事，說：“我已經照著你所吩咐我的行了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那穿細麻衣、腰間帶着墨盒子的人將這事回覆說：「我已經照你所吩咐的行了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then behold, the man clothed in linen at whose loins was the writing case reported, saying, “I have done just as You have commanded me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the man in linen with the writing kit at his side brought back word, saying, “I have done as you commanded.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the man in linen clothing, who carried the writer’s case, reported back and said, “I have done as you commanded.”"
      }
    },
