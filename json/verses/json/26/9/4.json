    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对他说：“你去走遍耶路撒冷，在那些因城中可憎之事而叹息哀伤的人额上画个记号。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對他說：“你要走遍耶路撒冷全城，那些因城中所行的一切可憎的事而歎息悲哀的人，你要在他們的額上畫個記號。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對他說：「你去走遍耶路撒冷全城，那些因城中所行可憎之事歎息哀哭的人，畫記號在額上。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord said to him, “Go through the midst of the city, even through the midst of Jerusalem, and put a mark on the foreheads of the men who sigh and groan over all the abominations which are being committed in its midst.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and said to him, “Go throughout the city of Jerusalem and put a mark on the foreheads of those who grieve and lament over all the detestable things that are done in it.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He said to him, “Walk through the streets of Jerusalem and put a mark on the foreheads of all who weep and sigh because of the detestable sins being committed in their city.”"
      }
    },
