    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我听见祂高声呼喊说：“惩罚这城的人啊，拿起你们的兵器上前来吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我聽見他大聲呼叫說：“懲罰這城的啊！你們要近前來，各人手裡拿著滅命的武器。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他向我耳中大聲喊叫說：「要使那監管這城的人手中各拿滅命的兵器前來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He cried out in my hearing with a loud voice saying, “Draw near, O executioners of the city, each with his destroying weapon in his hand.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I heard him call out in a loud voice, “Bring near those who are appointed to execute judgment on the city, each with a weapon in his hand.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord thundered, “Bring on the men appointed to punish the city! Tell them to bring their weapons with them!”"
      }
    },
