    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在你们中间，父亲要吃自己的儿子，儿子要吃自己的父亲。我必惩罚你们，把你们余下的人分散到各地各方。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，在你中間，父親要吃兒子，兒子要吃父親。我要向你施行審判，把你餘剩的眾人分散到四方去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在你中間父親要吃兒子，兒子要吃父親。我必向你施行審判，我必將你所剩下的分散四方。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore, fathers will eat their sons among you, and sons will eat their fathers; for I will execute judgments on you and scatter all your remnant to every wind."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore in your midst parents will eat their children, and children will eat their parents. I will inflict punishment on you and will scatter all your survivors to the winds."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Parents will eat their own children, and children will eat their parents. I will punish you and scatter to the winds the few who survive."
      }
    },
