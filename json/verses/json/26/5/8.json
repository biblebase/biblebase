    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此主耶和华这样说：“看啊，我要对付你们，我必在列国面前审判你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，主耶和華這樣說：“看哪！我要敵對你。我要在你中間，在列國的眼前，施行審判。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以主耶和華如此說：看哪，我與你反對，必在列國的眼前，在你中間，施行審判；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "therefore, thus says the Lord God, ‘Behold, I, even I, am against you, and I will execute judgments among you in the sight of the nations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Therefore this is what the Sovereign Lord says: I myself am against you, Jerusalem, and I will inflict punishment on you in the sight of the nations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Therefore, I myself, the Sovereign Lord, am now your enemy. I will punish you publicly while all the nations watch."
      }
    },
