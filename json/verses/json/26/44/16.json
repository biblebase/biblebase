      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只有他们可以进入我的圣所，在我的桌前事奉我，遵行我的吩咐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們可以進入我的聖所，可以走近我的桌前來事奉我，守我所吩咐的職責。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必進入我的聖所，就近我的桌前事奉我，守我所吩咐的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“They shall enter My sanctuary; they shall come near to My table to minister to Me and keep My charge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They alone are to enter my sanctuary; they alone are to come near my table to minister before me and serve me as guards."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They alone will enter my sanctuary and approach my table to serve me. They will fulfill all my requirements."
      }
