      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要报应他们，发烈怒惩罚他们。我报应他们的时候，他们就知道我是耶和华。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必怒責他們，向他們大施報復；我向他們報復的時候，他們就知道我是耶和華。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我向他們大施報應，發怒斥責他們。我報復他們的時候，他們就知道我是耶和華。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will execute great vengeance on them with wrathful rebukes; and they will know that I am the Lord when I lay My vengeance on them.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will carry out great vengeance on them and punish them in my wrath. Then they will know that I am the Lord, when I take vengeance on them.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will execute terrible vengeance against them to punish them for what they have done. And when I have inflicted my revenge, they will know that I am the Lord.”"
      }
