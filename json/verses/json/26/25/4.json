    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以我要使东方人侵占你们的地方。他们必在你们的土地上安营扎寨，吃你们出产的果子，喝你们牛羊的奶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以我要把你交給東方人作產業；他們必在你那裡安營，在你那裡設立居所；他們必吃你的果子，喝你的奶。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以我必將你的地交給東方人為業；他們必在你的地上安營居住，吃你的果子，喝你的奶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "therefore, behold, I am going to give you to the sons of the east for a possession, and they will set their encampments among you and make their dwellings among you; they will eat your fruit and drink your milk."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "therefore I am going to give you to the people of the East as a possession. They will set up their camps and pitch their tents among you; they will eat your fruit and drink your milk."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will allow nomads from the eastern deserts to overrun your country. They will set up their camps among you and pitch their tents on your land. They will harvest all your fruit and drink the milk from your livestock."
      }
    },
