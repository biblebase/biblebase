    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我还要撕去你们的头巾，把我的子民从你们手中拯救出来，使他们不再做你们手中的猎物。这样，你们就知道我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必扯下你們的頭巾，救我的子民脫離你們的手，使他們不再被你們的手獵取；你們就知道我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我也必撕裂你們下垂的頭巾，救我百姓脫離你們的手，不再被獵取，落在你們手中。你們就知道我是耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will also tear off your veils and deliver My people from your hands, and they will no longer be in your hands to be hunted; and you will know that I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will tear off your veils and save my people from your hands, and they will no longer fall prey to your power. Then you will know that I am the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will tear off the magic veils and save my people from your grasp. They will no longer be your victims. Then you will know that I am the Lord."
      }
    },
