    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“人子啊，你要说预言斥责你同胞中私自发预言的女子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人子啊！你要面向民中那些隨著自己心意說預言的女子，說預言攻擊她們。你要說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人子啊，你要面向本民中、從己心發預言的女子說預言，攻擊她們，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Now you, son of man, set your face against the daughters of your people who are prophesying from their own inspiration. Prophesy against them"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Now, son of man, set your face against the daughters of your people who prophesy out of their own imagination. Prophesy against them"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Now, son of man, speak out against the women who prophesy from their own imaginations."
      }
