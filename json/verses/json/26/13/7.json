    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道你们不是见了虚假的异象，说了虚谎的预言吗？你们说这是耶和华说的，其实我并没有说。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不是見了虛假的異象，不是說了騙人的占卜嗎？你們還說：‘這是耶和華的宣告。’其實我並沒有說過。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們豈不是見了虛假的異象嗎？豈不是說了謊詐的占卜嗎？你們說，這是耶和華說的，其實我沒有說。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Did you not see a false vision and speak a lying divination when you said, ‘The Lord declares,’ but it is not I who have spoken?” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Have you not seen false visions and uttered lying divinations when you say, “The Lord declares,” though I have not spoken?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can your visions be anything but false if you claim, ‘This message is from the Lord,’ when I have not even spoken to you?"
      }
    },
