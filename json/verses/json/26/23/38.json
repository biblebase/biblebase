    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "同时，她们还在同一天玷污我的圣所，亵渎我的安息日，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "此外，同一天，她們還向我作了這事，就是玷污我的聖所，又褻瀆我的安息日。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "此外，她們還有向我所行的，就是同日玷污我的聖所，干犯我的安息日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Again, they have done this to Me: they have defiled My sanctuary on the same day and have profaned My sabbaths."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They have also done this to me: At that same time they defiled my sanctuary and desecrated my Sabbaths."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Furthermore, they have defiled my Temple and violated my Sabbath day!"
      }
    },
