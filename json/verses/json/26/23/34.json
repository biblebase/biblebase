    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你必一滴不剩把它喝光， 然后把杯摔成碎片， 抓破自己的胸脯。 这是主耶和华说的。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要喝這杯，並且喝盡； 然後把這杯摔成碎塊， 又抓傷自己的胸。 因為這是我曾說過的。’這是主耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必喝這杯，以致喝盡。 杯破又齦杯片， 撕裂自己的乳； 因為這事我曾說過。 這是主耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will drink it and drain it. Then you will gnaw its fragments And tear your breasts; for I have spoken,’ declares the Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will drink it and drain it dry and chew on its pieces— and you will tear your breasts. I have spoken, declares the Sovereign Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will drain that cup of terror to the very bottom. Then you will smash it to pieces and beat your breast in anguish. I, the Sovereign Lord, have spoken!"
      }
    },
