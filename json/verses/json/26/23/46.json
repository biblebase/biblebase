    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“主耶和华说，‘我要使许多人起来攻击、抢掠她们，叫她们胆战心惊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“因為主耶和華這樣說：我必使一大群人上來攻擊她們，使她們驚慌，被人搶掠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華如此說：「我必使多人來攻擊她們，使她們拋來拋去，被人搶奪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“For thus says the Lord God, ‘Bring up a company against them and give them over to terror and plunder."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Sovereign Lord says: Bring a mob against them and give them over to terror and plunder."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Now this is what the Sovereign Lord says: Bring an army against them and hand them over to be terrorized and plundered."
      }
    },
