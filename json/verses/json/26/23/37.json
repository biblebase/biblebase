    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她们淫荡，手上沾满血腥，与偶像淫乱，把为我所生的儿女烧死献给偶像。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為她們行了姦淫，手中染血，與自己的偶像行姦淫，他們甚至使自己給我所生的兒女經火，獻給偶像作食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她們行淫，手中有殺人的血，又與偶像行淫，並使她們為我所生的兒女經火燒給偶像。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they have committed adultery, and blood is on their hands. Thus they have committed adultery with their idols and even caused their sons, whom they bore to Me, to pass through the fire to them as food."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for they have committed adultery and blood is on their hands. They committed adultery with their idols; they even sacrificed their children, whom they bore to me, as food for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They have committed both adultery and murder—adultery by worshiping idols and murder by burning as sacrifices the children they bore to me."
      }
    },
