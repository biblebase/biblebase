    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“主耶和华说， “‘你必喝你姐姐那又大又深的杯， 成为人们耻笑和辱骂的对象。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶和華這樣說： 你必喝你姊姊所喝的杯， 那杯又深又闊， 容量甚大， 必使你成為嗤笑和譏諷的對象。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華如此說： 你必喝你姊姊所喝的杯； 那杯又深又廣，盛得甚多， 使你被人嗤笑譏刺。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord God, ‘You will drink your sister’s cup, Which is deep and wide. You will be laughed at and held in derision; It contains much."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Sovereign Lord says: “You will drink your sister’s cup, a cup large and deep; it will bring scorn and derision, for it holds so much."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yes, this is what the Sovereign Lord says: “You will drink from your sister’s cup of terror, a cup that is large and deep. It is filled to the brim with scorn and derision."
      }
    },
