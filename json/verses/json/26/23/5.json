    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“阿荷拉归属我后仍然淫乱，贪恋她的情人——亚述人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“阿荷拉嫁了我以後仍行邪淫，戀慕她的愛人，就是亞述的戰士。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「阿荷拉歸我之後行邪淫，貪戀所愛的人，就是她的鄰邦亞述人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Oholah played the harlot while she was Mine; and she lusted after her lovers, after the Assyrians, her neighbors,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Oholah engaged in prostitution while she was still mine; and she lusted after her lovers, the Assyrians—warriors"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then Oholah lusted after other lovers instead of me, and she gave her love to the Assyrian officers."
      }
