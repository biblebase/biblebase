    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我膏立你为看守约柜的基路伯天使。 你在上帝的圣山上， 在闪闪发光的宝石中行走。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你是護衛寶座的、受膏的基路伯； 我安置你在　神的聖山上， 你在閃耀如火的寶石中間行走。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你是那受膏遮掩約櫃的基路伯； 我將你安置在神的聖山上； 你在發光如火的寶石中間往來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You were the anointed cherub who covers, And I placed you there. You were on the holy mountain of God; You walked in the midst of the stones of fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You were anointed as a guardian cherub, for so I ordained you. You were on the holy mount of God; you walked among the fiery stones."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I ordained and anointed you as the mighty angelic guardian. You had access to the holy mountain of God and walked among the stones of fire."
      }
    },
