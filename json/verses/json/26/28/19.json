      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "各國民中，凡認識你的， 都必為你驚奇。 你令人驚恐， 不再存留於世，直到永遠。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All who know you among the peoples Are appalled at you; You have become terrified And you will cease to be forever.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the nations who knew you are appalled at you; you have come to a horrible end and will be no more.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All who knew you are appalled at your fate. You have come to a terrible end, and you will exist no more.”"
      }
