    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使瘟疫进入你境内， 刀剑从四面八方临到你， 被杀的人横尸城中，血流街头， 这样人们就知道我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必使瘟疫進入西頓， 使血流在它的街上； 必有刀劍四圍攻擊它， 使被殺的人倒在城中， 人就知道我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必使瘟疫進入西頓， 使血流在她街上。 被殺的必在其中仆倒， 四圍有刀劍臨到她， 人就知道我是耶和華。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I will send pestilence to her And blood to her streets, And the wounded will fall in her midst By the sword upon her on every side; Then they will know that I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will send a plague upon you and make blood flow in your streets. The slain will fall within you, with the sword against you on every side. Then you will know that I am the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will send a plague against you, and blood will be spilled in your streets. The attack will come from every direction, and your people will lie slaughtered within your walls. Then everyone will know that I am the Lord."
      }
    },
