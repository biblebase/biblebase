    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要将一头公牛犊作为赎罪祭给撒督后裔中做祭司的利未人，他们可以前来事奉我。这是主耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要把一頭公牛犢給那些親近我、事奉我的祭司利未人作贖罪祭，他們是撒督的後裔。這是主耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華說，你要將一隻公牛犢作為贖罪祭，給祭司利未人撒督的後裔，就是那親近我、事奉我的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall give to the Levitical priests who are from the offspring of Zadok, who draw near to Me to minister to Me,’ declares the Lord God, ‘a young bull for a sin offering."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are to give a young bull as a sin offering to the Levitical priests of the family of Zadok, who come near to minister before me, declares the Sovereign Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At that time, the Levitical priests of the family of Zadok, who minister before me, are to be given a young bull for a sin offering, says the Sovereign Lord."
      }
    },
