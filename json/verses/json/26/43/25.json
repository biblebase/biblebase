    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一连七天，每天都要献上一只公山羊、一头公牛犊和一只公绵羊作赎罪祭，都不可有残疾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "一連七天，每天你都要預備一頭公山羊為贖罪祭，也要預備一頭公牛犢和羊群中的一隻公綿羊，都要沒有殘疾的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "七日內，每日要預備一隻公山羊為贖罪祭，也要預備一隻公牛犢和羊羣中的一隻公綿羊，都要沒有殘疾的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For seven days you shall prepare daily a goat for a sin offering; also a young bull and a ram from the flock, without blemish, shall be prepared."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“For seven days you are to provide a male goat daily for a sin offering; you are also to provide a young bull and a ram from the flock, both without defect."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Every day for seven days a male goat, a young bull, and a ram from the flock will be sacrificed as a sin offering. None of these animals may have physical defects of any kind."
      }
    },
