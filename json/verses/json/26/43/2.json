      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只见以色列上帝的荣光从东方而来，祂的声音如澎湃的洪涛，大地也因祂的荣耀而发出光芒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！以色列　神的榮耀從東面而來；他的聲音好像洪水的聲音，大地因他的榮耀而發光。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列神的榮光從東而來。他的聲音如同多水的聲音；地就因他的榮耀發光。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and behold, the glory of the God of Israel was coming from the way of the east. And His voice was like the sound of many waters; and the earth shone with His glory."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and I saw the glory of the God of Israel coming from the east. His voice was like the roar of rushing waters, and the land was radiant with his glory."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Suddenly, the glory of the God of Israel appeared from the east. The sound of his coming was like the roar of rushing waters, and the whole landscape shone with his glory."
      }
