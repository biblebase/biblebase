    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "必有刀剑攻击埃及， 埃及必尸横遍野， 古实必痛苦不堪。 敌人要掳掠埃及的财富， 摧毁她的根基。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "必有刀劍臨到埃及， 被殺的人倒在埃及的時候， 在古實就有痛苦； 埃及的財富必被奪去， 它的根基必被拆毀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "必有刀劍臨到埃及； 在埃及被殺之人仆倒的時候， 古實人就有痛苦， 人民必被擄掠， 基址必被拆毀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A sword will come upon Egypt, And anguish will be in Ethiopia; When the slain fall in Egypt, They take away her wealth, And her foundations are torn down."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A sword will come against Egypt, and anguish will come upon Cush. When the slain fall in Egypt, her wealth will be carried away and her foundations torn down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A sword will come against Egypt, and those who are slaughtered will cover the ground. Its wealth will be carried away and its foundations destroyed. The land of Ethiopia will be ravished."
      }
    },
