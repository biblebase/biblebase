      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要放火烧埃及， 训要受痛苦的折磨， 底比斯要被攻破， 挪弗终日惶恐不已，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必在埃及點起火來； 訓必有大痛苦； 挪必被攻破； 挪弗必終日受難。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必在埃及中使火着起； 訓必大大痛苦； 挪必被攻破； 挪弗白日見仇敵。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will set a fire in Egypt; Sin will writhe in anguish, Thebes will be breached And Memphis will have distresses daily."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will set fire to Egypt; Pelusium will writhe in agony. Thebes will be taken by storm; Memphis will be in constant distress."
      },
      "nlt": {
