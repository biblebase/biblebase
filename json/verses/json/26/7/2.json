    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“人子啊，主耶和华对以色列这样说，‘结局到了，以色列全境的结局到了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人子啊！主耶和華對以色列地這樣說： ‘終局到了！終局臨到這地的四境了！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人子啊，主耶和華對以色列地如此說：結局到了，結局到了地的四境！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“And you, son of man, thus says the Lord God to the land of Israel, ‘An end! The end is coming on the four corners of the land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Son of man, this is what the Sovereign Lord says to the land of Israel: “ ‘The end! The end has come upon the four corners of the land!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Son of man, this is what the Sovereign Lord says to Israel: “The end is here! Wherever you look— east, west, north, or south— your land is finished."
      }
    },
