      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华说：“看啊，灾祸来了，一场空前的灾祸来了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶和華這樣說： ‘看哪，災禍，獨特的災禍，臨到了！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「主耶和華如此說：有一災，獨有一災；看哪，臨近了！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Thus says the Lord God, ‘A disaster, unique disaster, behold it is coming!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Sovereign Lord says: “ ‘Disaster! Unheard-of disaster! See, it comes!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Sovereign Lord says: Disaster after disaster is coming your way!"
      }
