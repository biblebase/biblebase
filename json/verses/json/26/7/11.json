    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "暴行成了惩罚邪恶的杖，以色列人必没有一个能够存留，他们的财富和尊荣必不复存在。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "強暴興起，成了懲罰邪惡的杖， 他們無一存留，他們的群眾都不在了。 他們的財富沒有了，他們中間也沒有尊榮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "強暴興起，成了罰惡的杖。以色列人，或是他們的羣眾，或是他們的財寶，無一存留；他們中間也沒有得尊榮的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Violence has grown into a rod of wickedness. None of them shall remain, none of their people, none of their wealth, nor anything eminent among them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Violence has arisen, a rod to punish the wicked. None of the people will be left, none of that crowd— none of their wealth, nothing of value."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their violence has grown into a rod that will beat them for their wickedness. None of these proud and wicked people will survive. All their wealth and prestige will be swept away."
      }
    },
