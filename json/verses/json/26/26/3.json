    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以主耶和华说，‘泰尔啊，我要跟你作对，我要激起多国来攻击你，就像大海掀起波浪一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以主耶和華這樣說：‘推羅啊！我要與你為敵；我必叫多國上來攻擊你，好像海使波浪湧上來一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，主耶和華如此說：泰爾啊，我必與你為敵，使許多國民上來攻擊你，如同海使波浪湧上來一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "therefore thus says the Lord God, ‘Behold, I am against you, O Tyre, and I will bring up many nations against you, as the sea brings up its waves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "therefore this is what the Sovereign Lord says: I am against you, Tyre, and I will bring many nations against you, like the sea casting up its waves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Therefore, this is what the Sovereign Lord says: I am your enemy, O Tyre, and I will bring many nations against you, like the waves of the sea crashing against your shoreline."
      }
    },
