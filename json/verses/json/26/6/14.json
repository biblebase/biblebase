    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必伸手攻击他们，使他们居住的地方，从旷野到第伯拉他，变得极其荒凉。这样，他们便知道我是耶和华。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必伸手攻擊他們，使他們一切居住的地方，從曠野到第伯拉他，都變成非常荒涼，他們就知道我是耶和華。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必伸手攻擊他們，使他們的地從曠野到第伯拉他一切住處極其荒涼，他們就知道我是耶和華。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So throughout all their habitations I will stretch out My hand against them and make the land more desolate and waste than the wilderness toward Diblah; thus they will know that I am the Lord.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And I will stretch out my hand against them and make the land a desolate waste from the desert to Diblah—wherever they live. Then they will know that I am the Lord.’ ”"
      },
