      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "使他们横尸在你们的偶像面前，骸骨散在祭坛的四周。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要把以色列人的屍體放在他們的偶像面前，又把你們的骸骨拋散在你們祭壇的周圍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我也要將以色列人的屍首放在他們的偶像面前，將你們的骸骨拋散在你們祭壇的四圍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will also lay the dead bodies of the sons of Israel in front of their idols; and I will scatter your bones around your altars."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will lay the dead bodies of the Israelites in front of their idols, and I will scatter your bones around your altars."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will lay your corpses in front of your idols and scatter your bones around your altars."
      }
    },
