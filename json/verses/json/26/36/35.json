    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们会说，以前的荒凉之地如今竟变得像伊甸园，被毁坏废弃的荒凉城邑已经设防，有人居住。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們必說：“先前這荒涼之地，現在竟像伊甸園一樣；這些廢棄、荒涼、毀壞了的城市，現在卻成了堅固城，有人居住了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必說：『這先前為荒廢之地，現在成如伊甸園；這荒廢淒涼、毀壞的城邑現在堅固有人居住。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will say, ‘This desolate land has become like the garden of Eden; and the waste, desolate and ruined cities are fortified and inhabited.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will say, “This land that was laid waste has become like the garden of Eden; the cities that were lying in ruins, desolate and destroyed, are now fortified and inhabited.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when I bring you back, people will say, ‘This former wasteland is now like the Garden of Eden! The abandoned and ruined cities now have strong walls and are filled with people!’"
      }
    },
