    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要拯救你们脱离各样的污秽之事。我要使你们五谷丰登，不让饥荒侵害你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必拯救你們脫離你們的一切污穢；我必叫五穀豐收，不使你們遇見饑荒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必救你們脫離一切的污穢，也必命五穀豐登，不使你們遭遇饑荒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, I will save you from all your uncleanness; and I will call for the grain and multiply it, and I will not bring a famine on you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will save you from all your uncleanness. I will call for the grain and make it plentiful and will not bring famine upon you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will cleanse you of your filthy behavior. I will give you good crops of grain, and I will send no more famines on the land."
      }
    },
