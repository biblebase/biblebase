    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以我誓言要让你们四围的外族人也受羞辱。这是主耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以主耶和華這樣說：我舉手起誓說：你們四圍的列國必親自擔當自己的羞辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以我起誓說：你們四圍的外邦人總要擔當自己的羞辱。這是主耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore thus says the Lord God, ‘I have sworn that surely the nations which are around you will themselves endure their insults."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore this is what the Sovereign Lord says: I swear with uplifted hand that the nations around you will also suffer scorn."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Therefore, this is what the Sovereign Lord says: I have taken a solemn oath that those nations will soon have their own shame to endure."
      }
    },
