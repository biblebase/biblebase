    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“主耶和华说，‘在我洗净你们一切罪恶的日子，我要使你们的城邑有人居住，废墟得以重建。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘主耶和華這樣說：我潔淨你們，使你們脫離一切罪孽的日子，我必使城市有人居住，廢墟可以重建。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「主耶和華如此說：我潔淨你們，使你們脫離一切罪孽的日子，必使城邑有人居住，荒場再被建造。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Thus says the Lord God, “On the day that I cleanse you from all your iniquities, I will cause the cities to be inhabited, and the waste places will be rebuilt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘This is what the Sovereign Lord says: On the day I cleanse you from all your sins, I will resettle your towns, and the ruins will be rebuilt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Sovereign Lord says: When I cleanse you from your sins, I will repopulate your cities, and the ruins will be rebuilt."
      }
    },
