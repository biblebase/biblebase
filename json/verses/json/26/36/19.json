    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把他们驱散到列国，分散到列邦，照他们的行为报应他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我使他們分散在列邦，四散在各地；我按著他們的所作所為審判他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我將他們分散在列國，四散在列邦，按他們的行動作為懲罰他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Also I scattered them among the nations and they were dispersed throughout the lands. According to their ways and their deeds I judged them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I dispersed them among the nations, and they were scattered through the countries; I judged them according to their conduct and their actions."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I scattered them to many lands to punish them for the evil way they had lived."
      }
    },
