    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要在你们身上洒清水洁净你们，洗净你们所有的污秽，除掉你们心中一切的偶像。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必用潔淨的水灑在你們身上，你們就潔淨了；我必潔淨你們的一切污穢，使你們遠離所有可憎的像。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必用清水灑在你們身上，你們就潔淨了。我要潔淨你們，使你們脫離一切的污穢，棄掉一切的偶像。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I will sprinkle clean water on you, and you will be clean; I will cleanse you from all your filthiness and from all your idols."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will sprinkle clean water on you, and you will be clean; I will cleanse you from all your impurities and from all your idols."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then I will sprinkle clean water on you, and you will be clean. Your filth will be washed away, and you will no longer worship idols."
      }
    },
