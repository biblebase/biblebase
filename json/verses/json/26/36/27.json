    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要把我的灵放在你们里面，使你们恪守我的律例，谨遵我的典章。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必把我的靈放在你們裡面，使你們遵行我的律例，謹守遵行我的典章。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必將我的靈放在你們裏面，使你們順從我的律例，謹守遵行我的典章。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will put My Spirit within you and cause you to walk in My statutes, and you will be careful to observe My ordinances."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And I will put my Spirit in you and move you to follow my decrees and be careful to keep my laws."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I will put my Spirit in you so that you will follow my decrees and be careful to obey my regulations."
      }
