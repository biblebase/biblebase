    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华说，你们应当知道，我这样做并不是为了你们。以色列人啊，你们应当为自己的行为抱愧蒙羞。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要知道我這樣行，不是為了你們的緣故；以色列家啊！你們要因自己的行為抱愧蒙羞。這是主耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華說：你們要知道，我這樣行不是為你們。以色列家啊，當為自己的行為抱愧蒙羞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am not doing this for your sake,” declares the Lord God, “let it be known to you. Be ashamed and confounded for your ways, O house of Israel!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I want you to know that I am not doing this for your sake, declares the Sovereign Lord. Be ashamed and disgraced for your conduct, people of Israel!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But remember, says the Sovereign Lord, I am not doing this because you deserve it. O my people of Israel, you should be utterly ashamed of all you have done!"
      }
    },
