    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们必安居在我赐给你们祖先的土地上。你们要做我的子民，我要做你们的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要住在我賜給你們列祖的地；你們要作我的子民，我要作你們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們必住在我所賜給你們列祖之地。你們要作我的子民，我要作你們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will live in the land that I gave to your forefathers; so you will be My people, and I will be your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then you will live in the land I gave your ancestors; you will be my people, and I will be your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And you will live in Israel, the land I gave your ancestors long ago. You will be my people, and I will be your God."
      }
    },
