    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使树木硕果累累，田间出产丰盛，免得你们因饥荒再被外族人讥笑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必使樹木的果子和田地的出產增多，好使你們在列國中不再因饑荒而遭受恥辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必使樹木多結果子，田地多出土產，好叫你們不再因饑荒受外邦人的譏誚。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will multiply the fruit of the tree and the produce of the field, so that you will not receive again the disgrace of famine among the nations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will increase the fruit of the trees and the crops of the field, so that you will no longer suffer disgrace among the nations because of famine."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will give you great harvests from your fruit trees and fields, and never again will the surrounding nations be able to scoff at your land for its famines."
      }
    },
