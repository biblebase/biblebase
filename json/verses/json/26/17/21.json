    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他所有的精兵都要丧身刀下，余下的也要四散逃窜。这样，你们就知道这是我耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他軍中所有的精兵，都必倒在刀下；剩下的也必分散到四方；你們就知道說這話的是我耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的一切軍隊，凡逃跑的，都必倒在刀下；所剩下的，也必分散四方。你們就知道說這話的是我－耶和華。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the choice men in all his troops will fall by the sword, and the survivors will be scattered to every wind; and you will know that I, the Lord, have spoken.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All his choice troops will fall by the sword, and the survivors will be scattered to the winds. Then you will know that I the Lord have spoken."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And all his best warriors will be killed in battle, and those who survive will be scattered to the four winds. Then you will know that I, the Lord, have spoken."
      }
    },
