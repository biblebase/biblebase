    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它又把以色列的树苗像柳树一样栽在水源丰沛的沃土里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠又把以色列地的種子取去，栽種在肥沃的地土、水流的旁邊，好像栽種柳樹一樣；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又將以色列地的枝子栽於肥田裏，插在大水旁，如插柳樹，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also took some of the seed of the land and planted it in fertile soil. He placed it beside abundant waters; he set it like a willow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘He took one of the seedlings of the land and put it in fertile soil. He planted it like a willow by abundant water,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He also took a seedling from the land and planted it in fertile soil. He placed it beside a broad river, where it could grow like a willow tree."
      }
    },
