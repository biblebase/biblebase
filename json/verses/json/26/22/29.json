      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "國內眾民一味地欺壓，慣行搶奪，虧負困苦窮乏的，背理欺壓寄居的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The people of the land have practiced oppression and committed robbery, and they have wronged the poor and needy and have oppressed the sojourner without justice."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The people of the land practice extortion and commit robbery; they oppress the poor and needy and mistreat the foreigner, denying them justice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even common people oppress the poor, rob the needy, and deprive foreigners of justice."
      }
