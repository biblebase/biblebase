    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在我惩罚你的日子，你还有勇气和力量吗？我耶和华言出必行，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了我懲治你的日子，你的心還能受得住嗎？你的手還有力嗎？我耶和華說了，就必實行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了我懲罰你的日子，你的心還能忍受嗎？你的手還能有力嗎？我－耶和華說了這話，就必照着行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Can your heart endure, or can your hands be strong in the days that I will deal with you? I, the Lord, have spoken and will act."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Will your courage endure or your hands be strong in the day I deal with you? I the Lord have spoken, and I will do it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How strong and courageous will you be in my day of reckoning? I, the Lord, have spoken, and I will do what I said."
      }
    },
