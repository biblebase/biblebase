    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人与邻居的妻子干出可憎之事，有人玷污自己的儿媳妇，有人奸污自己的同父姊妹。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在你裡面，這人與鄰舍的妻子行可憎的事；那人行淫亂，玷污自己的兒媳；還有人污辱自己的同胞姊妹。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這人與鄰舍的妻行可憎的事；那人貪淫玷污兒婦；還有玷辱同父之姊妹的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "One has committed abomination with his neighbor’s wife and another has lewdly defiled his daughter-in-law. And another in you has humbled his sister, his father’s daughter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In you one man commits a detestable offense with his neighbor’s wife, another shamefully defiles his daughter-in-law, and another violates his sister, his own father’s daughter."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Within your walls live men who commit adultery with their neighbors’ wives, who defile their daughters-in-law, or who rape their own sisters."
      }
    },
