    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘我见你谋财害命，就击掌叹息。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘看哪！因你所得的不義之財，又因在你中間所流的血，我就拍掌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「看哪，我因你所得不義之財和你中間所流的血，就拍掌歎息。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, then, I smite My hand at your dishonest gain which you have acquired and at the bloodshed which is among you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘I will surely strike my hands together at the unjust gain you have made and at the blood you have shed in your midst."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But now I clap my hands in indignation over your dishonest gain and bloodshed."
      }
    },
