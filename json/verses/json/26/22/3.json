    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要向她宣告主耶和华的话，说，‘唉，你这杀人流血、制造偶像玷污自己的城啊，你的末日到了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要說：‘主耶和華這樣說：這城中有流人血的事，它製造偶像，玷污自己，以致它受審判的時候來到。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要說，主耶和華如此說：哎！這城有流人血的事在其中，叫她受報的日期來到，又做偶像玷污自己，陷害自己。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall say, ‘Thus says the Lord God, “A city shedding blood in her midst, so that her time will come, and that makes idols, contrary to her interest, for defilement!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and say: ‘This is what the Sovereign Lord says: You city that brings on herself doom by shedding blood in her midst and defiles herself by making idols,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and give her this message from the Sovereign Lord: O city of murderers, doomed and damned—city of idols, filthy and foul—"
      }
    },
