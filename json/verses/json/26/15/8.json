    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必使他们的土地荒凉，因为他们对我不忠。这是主耶和华说的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必使這地荒涼，因為他們行了不忠的事。這是主耶和華的宣告。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必使地土荒涼，因為他們行事干犯我。這是主耶和華說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus I will make the land desolate, because they have acted unfaithfully,’ ” declares the Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will make the land desolate because they have been unfaithful, declares the Sovereign Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I will make the land desolate because my people have been unfaithful to me. I, the Sovereign Lord, have spoken!”"
      }
    },
