    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要告诉他们，主耶和华这样说，‘我拣选以色列那日，曾向雅各家的后裔起誓，又在埃及向他们显现，告诉他们我是他们的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對他們說：‘主耶和華這樣說：在我揀選以色列的日子，我向雅各家的後裔舉手起誓，又在埃及地向他們顯現。我向他們舉手起誓說：我是耶和華你們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對他們說，主耶和華如此說：當日我揀選以色列，向雅各家的後裔起誓，在埃及地將自己向他們顯現，說：我是耶和華－你們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and say to them, ‘Thus says the Lord God, “On the day when I chose Israel and swore to the descendants of the house of Jacob and made Myself known to them in the land of Egypt, when I swore to them, saying, I am the Lord your God,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and say to them: ‘This is what the Sovereign Lord says: On the day I chose Israel, I swore with uplifted hand to the descendants of Jacob and revealed myself to them in Egypt. With uplifted hand I said to them, “I am the Lord your God.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give them this message from the Sovereign Lord: When I chose Israel—when I revealed myself to the descendants of Jacob in Egypt—I took a solemn oath that I, the Lord, would be their God."
      }
    },
