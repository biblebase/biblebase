    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我怜悯他们，没有毁灭他们，没有在旷野把他们全部灭绝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然這樣，我的眼還是顧惜他們，不毀滅他們；我沒有在曠野把他們滅絕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雖然如此，我眼仍顧惜他們，不毀滅他們，不在曠野將他們滅絕淨盡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet My eye spared them rather than destroying them, and I did not cause their annihilation in the wilderness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet I looked on them with pity and did not destroy them or put an end to them in the wilderness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Nevertheless, I took pity on them and held back from destroying them in the wilderness."
      }
    },
