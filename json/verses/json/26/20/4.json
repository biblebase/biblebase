    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人子啊，你要审判他们？你要审判他们？要使他们知道他们祖先所做的可憎之事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人子啊，你要審問他們嗎？要審問他們嗎？你要使他們知道他們列祖那些可憎的事，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人子啊，你要審問審問他們嗎？你當使他們知道他們列祖那些可憎的事，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will you judge them, will you judge them, son of man? Make them know the abominations of their fathers;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Will you judge them? Will you judge them, son of man? Then confront them with the detestable practices of their ancestors"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Son of man, bring charges against them and condemn them. Make them realize how detestable the sins of their ancestors really were."
      }
