    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要把你们带回我起誓应许给你们祖先的以色列，这样你们就知道我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我領你們進入以色列地，就是進入我曾經舉手起誓要賜給你們祖先的地以後，你們就知道我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我領你們進入以色列地，就是我起誓應許賜給你們列祖之地，那時你們就知道我是耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And you will know that I am the Lord, when I bring you into the land of Israel, into the land which I swore to give to your forefathers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then you will know that I am the Lord, when I bring you into the land of Israel, the land I had sworn with uplifted hand to give to your ancestors."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then when I have brought you home to the land I promised with a solemn oath to give to your ancestors, you will know that I am the Lord."
      }
    },
