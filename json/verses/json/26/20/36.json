    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就像我从前在埃及的旷野审判你们的祖先一样。这是主耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我怎樣在埃及地的曠野審判你們的祖先，也必怎樣審判你們。這是主耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我怎樣在埃及地的曠野刑罰你們的列祖，也必照樣刑罰你們。這是主耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As I entered into judgment with your fathers in the wilderness of the land of Egypt, so I will enter into judgment with you,” declares the Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As I judged your ancestors in the wilderness of the land of Egypt, so I will judge you, declares the Sovereign Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will judge you there just as I did your ancestors in the wilderness after bringing them out of Egypt, says the Sovereign Lord."
      }
    },
