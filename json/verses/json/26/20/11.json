    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "向他们颁布我的律例和典章，若有人遵守，就必活着。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我把我的律例賜給他們，把我的典章指示他們；人如果遵行，就必因此活著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將我的律例賜給他們，將我的典章指示他們；人若遵行就必因此活着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I gave them My statutes and informed them of My ordinances, by which, if a man observes them, he will live."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I gave them my decrees and made known to them my laws, by which the person who obeys them will live."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There I gave them my decrees and regulations so they could find life by keeping them."
      }
    },
