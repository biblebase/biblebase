    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我任凭这城把受害者的血洒在磐石上，不加掩盖，好激起我的烈怒，使她受报应。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我為了發烈怒，好施行報應， 就把她的血倒在光滑的磐石上， 以致不得掩蓋。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這城中所流的血倒在淨光的磐石上，不得掩蓋，乃是出於我，為要發忿怒施行報應。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "That it may cause wrath to come up to take vengeance, I have put her blood on the bare rock, That it may not be covered.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To stir up wrath and take revenge I put her blood on the bare rock, so that it would not be covered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I will splash her blood on a rock for all to see, an expression of my anger and vengeance against her."
      }
    },
