    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "早晨我把这事告诉百姓，晚上我的妻子便死了。次日早晨，我便遵照耶和华的吩咐行。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是我在早上對眾人說了這事，晚上我的妻子就死了；次日早晨我就照著所吩咐我的行了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是我將這事早晨告訴百姓，晚上我的妻就死了。次日早晨我便遵命而行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I spoke to the people in the morning, and in the evening my wife died. And in the morning I did as I was commanded."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I spoke to the people in the morning, and in the evening my wife died. The next morning I did as I had been commanded."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I proclaimed this to the people the next morning, and in the evening my wife died. The next morning I did everything I had been told to do."
      }
    },
