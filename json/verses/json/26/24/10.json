    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "添在火上，燃起旺火，把肉煮烂，加入香料，烧焦骨头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要多加柴枝，點上火， 混和香料， 把肉煮爛， 使骨頭燒焦；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "添上木柴，使火着旺，將肉煮爛，把湯熬濃，使骨頭烤焦；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Heap on the wood, kindle the fire, Boil the flesh well And mix in the spices, And let the bones be burned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So heap on the wood and kindle the fire. Cook the meat well, mixing in the spices; and let the bones be charred."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, heap on the wood! Let the fire roar to make the pot boil. Cook the meat with many spices, and afterward burn the bones."
      }
    },
