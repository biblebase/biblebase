    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“人子啊，今天是巴比伦王围攻耶路撒冷的日子，你要记下这日子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人子啊！你要記下今天的日子，正正是今天，因為巴比倫王就是在這一天圍困耶路撒冷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人子啊，今日正是巴比倫王就近耶路撒冷的日子，你要將這日記下，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Son of man, write the name of the day, this very day. The king of Babylon has laid siege to Jerusalem this very day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Son of man, record this date, this very date, because the king of Babylon has laid siege to Jerusalem this very day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Son of man, write down today’s date, because on this very day the king of Babylon is beginning his attack against Jerusalem."
      }
    },
