    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“人子啊，有一天我要夺去他们所喜悦、所夸耀、眼中所爱、心里所慕的堡垒，也要夺去他们的儿女。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人子啊！我從他們中間奪去他們的堡壘、他們所喜歡的榮耀、他們眼所喜愛的、心所崇尚的和他們的兒女的時候，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人子啊，我除掉他們所倚靠、所歡喜的榮耀，並眼中所喜愛、心裏所重看的兒女。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘As for you, son of man, will it not be on the day when I take from them their stronghold, the joy of their pride, the desire of their eyes and their heart’s delight, their sons and their daughters,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“And you, son of man, on the day I take away their stronghold, their joy and glory, the delight of their eyes, their heart’s desire, and their sons and daughters as well—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord said to me, “Son of man, on the day I take away their stronghold—their joy and glory, their heart’s desire, their dearest treasure—I will also take away their sons and daughters."
