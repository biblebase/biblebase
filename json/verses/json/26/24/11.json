      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后，把倒空的锅放在炭火上烧热，把铜烧红，好熔化它的杂质，除净锈垢，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後把空鍋放在炭火上， 使鍋燒熱， 使銅燒紅， 使其中的污穢熔化， 使它的鏽得以除淨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "把鍋倒空坐在炭火上，使鍋燒熱，使銅燒紅，鎔化其中的污穢，除淨其上的銹。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then set it empty on its coals So that it may be hot And its bronze may glow And its filthiness may be melted in it, Its rust consumed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then set the empty pot on the coals till it becomes hot and its copper glows, so that its impurities may be melted and its deposit burned away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now set the empty pot on the coals. Heat it red hot! Burn away the filth and corruption."
      }
