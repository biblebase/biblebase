    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“人子啊，我要突然夺去你心爱的人，你不可悲伤，也不可流泪哭泣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人子啊！我要打擊你，把你眼中所喜愛的奪去，你卻不可悲哀，不可哭泣，也不可流淚。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人子啊，我要將你眼目所喜愛的忽然取去，你卻不可悲哀哭泣，也不可流淚，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Son of man, behold, I am about to take from you the desire of your eyes with a blow; but you shall not mourn and you shall not weep, and your tears shall not come."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Son of man, with one blow I am about to take away from you the delight of your eyes. Yet do not lament or weep or shed any tears."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Son of man, with one blow I will take away your dearest treasure. Yet you must not show any sorrow at her death. Do not weep; let there be no tears."
      }
    },
