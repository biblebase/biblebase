    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大人必认为这是虚假的征兆。他们曾向巴比伦王发誓效忠，但巴比伦王要使他们想起自己的罪，并掳走他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在那些曾經向巴比倫起誓效忠的猶大人看來，這是虛假的占卜，但巴比倫王要使他們想起自己的罪孽，把他們擄去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "據那些曾起誓的猶大人看來，這是虛假的占卜；但巴比倫王要使他們想起罪孽，以致將他們捉住。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And it will be to them like a false divination in their eyes; they have sworn solemn oaths. But he brings iniquity to remembrance, that they may be seized."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It will seem like a false omen to those who have sworn allegiance to him, but he will remind them of their guilt and take them captive."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people of Jerusalem will think it is a false omen, because of their treaty with the Babylonians. But the king of Babylon will remind the people of their rebellion. Then he will attack and capture them."
      }
    },
