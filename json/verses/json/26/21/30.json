    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要收刀入鞘，我要在你们的家乡，在你们的出生之地审判你们，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "收刀入鞘吧！我必在你被造之地，根源之處，審判你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你將刀收入鞘吧！在你受造之處、生長之地，我必刑罰你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Return it to its sheath. In the place where you were created, in the land of your origin, I will judge you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Let the sword return to its sheath. In the place where you were created, in the land of your ancestry, I will judge you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Now return the sword to its sheath, for in your own country, the land of your birth, I will pass judgment upon you."
      }
