      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这刀已交给人去擦亮，好握在手中，这刀已磨快擦亮，预备交给屠杀的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這刀已經交給人擦亮，可以握在手中使用；這刀已經磨利擦亮了，可以交在行殺戮的人手中。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這刀已經交給人擦亮，為要應手使用。這刀已經磨快擦亮，好交在行殺戮的人手中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is given to be polished, that it may be handled; the sword is sharpened and polished, to give it into the hand of the slayer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘The sword is appointed to be polished, to be grasped with the hand; it is sharpened and polished, made ready for the hand of the slayer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, the sword is now being sharpened and polished; it is being prepared for the executioner."
      }
