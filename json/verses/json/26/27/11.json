    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚发人和你的军队都守卫在你的城墙上，歌玛底人把守你的城楼，他们都在墙上挂满盾牌，使你完美无瑕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在你四圍的城牆上有亞發人和赫勒克人；在你的城樓上有歌瑪底人。他們把自己的盾牌掛在你四圍的城牆上，使你全然美麗。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞發人和你的軍隊都在你四圍的牆上，你的望樓也有勇士；他們懸掛盾牌，成全你的美麗。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sons of Arvad and your army were on your walls, all around, and the Gammadim were in your towers. They hung their shields on your walls all around; they perfected your beauty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Men of Arvad and Helek guarded your walls on every side; men of Gammad were in your towers. They hung their shields around your walls; they brought your beauty to perfection."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Men from Arvad and Helech stood on your walls. Your towers were manned by men from Gammad. Their shields hung on your walls, completing your beauty."
      }
    },
