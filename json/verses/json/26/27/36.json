    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "各国商人都嗤笑你， 你那可怕的末日来临了， 你将永远不复存在。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "各族的商人都嗤笑你； 你必遭遇可怕的災禍， 你就永遠不再存在了。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "各國民中的客商都向你發嘶聲； 你令人驚恐， 不再存留於世，直到永遠。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The merchants among the peoples hiss at you; You have become terrified And you will cease to be forever.’ ” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The merchants among the nations scoff at you; you have come to a horrible end and will be no more.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The merchants among the nations shake their heads at the sight of you, for you have come to a horrible end and will exist no more.’”"
      }
    },
