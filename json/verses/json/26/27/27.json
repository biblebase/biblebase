    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的财富、货物、商品、 水手、舵手、缝匠、客商、 战士和人民在你倾覆的日子都要沉到深海里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的財富、貨物、商品、水手、舵手、 修補裂縫的、與你進行貿易的， 在你那裡所有的戰士， 並在你中間所有聚集的人、 在你傾覆的日子， 都必在海的中心沉下去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的資財、物件、貨物、 水手、掌舵的、 補縫的、經營交易的， 並你中間的戰士和人民， 在你破壞的日子必都沉在海中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your wealth, your wares, your merchandise, Your sailors and your pilots, Your repairers of seams, your dealers in merchandise And all your men of war who are in you, With all your company that is in your midst, Will fall into the heart of the seas On the day of your overthrow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your wealth, merchandise and wares, your mariners, sailors and shipwrights, your merchants and all your soldiers, and everyone else on board will sink into the heart of the sea on the day of your shipwreck."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everything is lost— your riches and wares, your sailors and pilots, your ship builders, merchants, and warriors. On the day of your ruin, everyone on board sinks into the depths of the sea."
      }
    },
