    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘他施的船只替你运货， 你在海上载满沉甸甸的货物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他施的船隻運載你的商品， 你在海的中心，滿載貨物， 極其沉重。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他施的船隻接連成幫為你運貨， 你便在海中豐富極其榮華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The ships of Tarshish were the carriers for your merchandise. And you were filled and were very glorious In the heart of the seas."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘The ships of Tarshish serve as carriers for your wares. You are filled with heavy cargo as you sail the sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The ships of Tarshish were your ocean caravans. Your island warehouse was filled to the brim!"
      }
    },
