    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司要取些赎罪祭牲的血抹在圣殿的门柱、祭台的四角和内院门口的柱子上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司要取些贖罪祭牲的血，抹在殿的門框、祭壇臺上的四角，以及內院的門框上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司要取些贖罪祭牲的血，抹在殿的門柱上和壇磴臺的四角上，並內院的門框上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The priest shall take some of the blood from the sin offering and put it on the door posts of the house, on the four corners of the ledge of the altar and on the posts of the gate of the inner court."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priest is to take some of the blood of the sin offering and put it on the doorposts of the temple, on the four corners of the upper ledge of the altar and on the gateposts of the inner court."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The priest will take blood from this sin offering and put it on the doorposts of the Temple, the four corners of the upper ledge of the altar, and the gateposts at the entrance to the inner courtyard."
      }
    },
