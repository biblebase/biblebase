    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "另一块十二点五公里长、五公里宽的土地给那些在圣殿工作的利未人居住，作他们的产业。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "其餘的一塊地，長十二公里半，寬五公里，要歸給在聖殿裡供職的利未人作為他們的基業，作居住的城市。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又有一分，長二萬五千肘，寬一萬肘，要歸與在殿中供職的利未人，作為二十間房屋之業。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "An area 25,000 cubits in length and 10,000 in width shall be for the Levites, the ministers of the house, and for their possession cities to dwell in."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "An area 25,000 cubits long and 10,000 cubits wide will belong to the Levites, who serve in the temple, as their possession for towns to live in."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The strip of sacred land next to it, also 81/3 miles long and 31/3 miles wide, will be a living area for the Levites who work at the Temple. It will be their possession and a place for their towns."
      }
    },
