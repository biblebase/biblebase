      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當日，王要為自己和國內的眾民預備一隻公牛作贖罪祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On that day the prince shall provide for himself and all the people of the land a bull for a sin offering."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On that day the prince is to provide a bull as a sin offering for himself and for all the people of the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On the day of Passover the prince will provide a young bull as a sin offering for himself and the people of Israel."
      }
