    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘以下是你们应当献的供物，每一贺梅珥小麦要献上六分之一伊法，每一贺梅珥大麦也要献上六分之一伊法。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們所當獻的供物是這樣：一‘賀梅珥’小麥，要獻一‘伊法’的六分之一；一‘賀梅珥’大麥，要獻一‘伊法’的六分之一。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們當獻的供物乃是這樣：一賀梅珥麥子要獻伊法六分之一；一賀梅珥大麥要獻伊法六分之一。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“This is the offering that you shall offer: a sixth of an ephah from a homer of wheat; a sixth of an ephah from a homer of barley;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘This is the special gift you are to offer: a sixth of an ephah from each homer of wheat and a sixth of an ephah from each homer of barley."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must give this tax to the prince: one bushel of wheat or barley for every 60 you harvest,"
      }
    },
