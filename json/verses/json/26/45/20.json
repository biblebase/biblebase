    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到了一月七日，要同样为一切在无意和无知中犯罪的人赎罪，使圣殿保持洁净。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正月初七日，你也要為犯了誤殺罪的或因愚蒙而犯罪的人這樣行，為聖殿贖罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "本月初七日也要為誤犯罪的和愚蒙犯罪的如此行，為殿贖罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus you shall do on the seventh day of the month for everyone who goes astray or is naive; so you shall make atonement for the house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are to do the same on the seventh day of the month for anyone who sins unintentionally or through ignorance; so you are to make atonement for the temple."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do this also on the seventh day of the new year for anyone who has sinned through error or ignorance. In this way, you will purify the Temple."
      }
    },
