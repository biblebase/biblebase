    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那人手拿量绳向东量了五百米，然后领我涉水前行，水深及踝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那人手裡拿著一根準繩向東走出去的時候，量了五百公尺的距離，然後領我從水中走過去，那裡的水到了腳踝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他手拿準繩往東出去的時候，量了一千肘，使我蹚過水，水到踝子骨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the man went out toward the east with a line in his hand, he measured a thousand cubits, and he led me through the water, water reaching the ankles."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As the man went eastward with a measuring line in his hand, he measured off a thousand cubits and then led me through water that was ankle-deep."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Measuring as he went, he took me along the stream for 1,750 feet and then led me across. The water was up to my ankles."
      }
    },
