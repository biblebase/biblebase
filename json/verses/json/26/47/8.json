    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他对我说：“这水向东流，经过亚拉巴流到盐海，使那里的咸水变为淡水。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又對我說：“這水要向東方的地區流出去，下到亞拉巴，流入死海。這水流入死海，就使海裡的水變淡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對我說：「這水往東方流去，必下到亞拉巴，直到海。所發出來的水必流入鹽海，使水變甜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he said to me, “These waters go out toward the eastern region and go down into the Arabah; then they go toward the sea, being made to flow into the sea, and the waters of the sea become fresh."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to me, “This water flows toward the eastern region and goes down into the Arabah, where it enters the Dead Sea. When it empties into the sea, the salty water there becomes fresh."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said to me, “This river flows east through the desert into the valley of the Dead Sea. The waters of this stream will make the salty waters of the Dead Sea fresh and pure."
      }
    },
