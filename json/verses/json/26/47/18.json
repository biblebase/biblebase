    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“东界是从北界南下经浩兰、大马士革、沿着基列和以色列中间的约旦河为界，一直伸展到东部的海。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "東面的地界是在浩蘭和大馬士革之間，基列和以色列地之間的約旦河。你們要從北面的地界量到東海，這是東面的地界。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「東界在浩蘭、大馬士革、基列，和以色列地的中間，就是約旦河。你們要從北界量到東海。這是東界。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The east side, from between Hauran, Damascus, Gilead and the land of Israel, shall be the Jordan; from the north border to the eastern sea you shall measure. This is the east side."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“On the east side the boundary will run between Hauran and Damascus, along the Jordan between Gilead and the land of Israel, to the Dead Sea and as far as Tamar. This will be the eastern boundary."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The eastern border starts at a point between Hauran and Damascus and runs south along the Jordan River between Israel and Gilead, past the Dead Sea and as far south as Tamar. This will be the eastern border."
      }
    },
