    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "随后他领我由北门出来，从外面绕到外院的东门，我见水从东门的南面涓涓流出。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又帶我從北門出來，再領我從外面繞到朝東的外門。有水正從右邊流出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他帶我出北門，又領我從外邊轉到朝東的外門，見水從右邊流出。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He brought me out by way of the north gate and led me around on the outside to the outer gate by way of the gate that faces east. And behold, water was trickling from the south side."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He then brought me out through the north gate and led me around the outside to the outer gate facing east, and the water was trickling from the south side."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The man brought me outside the wall through the north gateway and led me around to the eastern entrance. There I could see the water flowing out through the south side of the east gateway."
      }
    },
