      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“西界从南界沿地中海的海岸到北界哈马口对面的地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "西面的地界是大海，從南面的地界到哈馬口的對面。這是西面的地界。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「西界就是大海，從南界直到哈馬口對面之地。這是西界。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The west side shall be the Great Sea, from the south border to a point opposite Lebo-hamath. This is the west side."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“On the west side, the Mediterranean Sea will be the boundary to a point opposite Lebo Hamath. This will be the western boundary."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“On the west side, the Mediterranean itself will be your border from the southern border to the point where the northern border begins, opposite Lebo-hamath."
      }
    },
