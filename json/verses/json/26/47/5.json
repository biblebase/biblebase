    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他又量了五百米，水已高涨成河，足可游泳，无法再涉水而行。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他再量了五百公尺，水已成了一條河，以致我不能走過去，因為水勢高漲，成為只可供游泳的水，而不能走過去的河。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又量了一千肘，水便成了河，使我不能蹚過。因為水勢漲起，成為可洑的水，不可蹚的河。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Again he measured a thousand; and it was a river that I could not ford, for the water had risen, enough water to swim in, a river that could not be forded."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He measured off another thousand, but now it was a river that I could not cross, because the water had risen and was deep enough to swim in—a river that no one could cross."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he measured another 1,750 feet, and the river was too deep to walk across. It was deep enough to swim in, but too deep to walk through."
      }
    },
