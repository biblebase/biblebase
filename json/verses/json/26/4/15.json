    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是祂对我说：“好吧，我容许你用牛粪代替人粪来烤饼。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他對我說：“你看，我准你用牛糞代替人糞，你可以在牛糞上烤你的餅。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是他對我說：「看哪，我給你牛糞代替人糞，你要將你的餅烤在其上。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He said to me, “See, I will give you cow’s dung in place of human dung over which you will prepare your bread.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Very well,” he said, “I will let you bake your bread over cow dung instead of human excrement.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“All right,” the Lord said. “You may bake your bread with cow dung instead of human dung.”"
      }
    },
