    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后围困这城，建垒筑台，扎营围攻，四面架起撞城锤。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後安放圍城設施，建造圍城高牆，築起攻城土壘，擺列兵營，四周架起攻城鎚。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又圍困這城，造臺築壘，安營攻擊，在四圍安設撞錘攻城，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then lay siege against it, build a siege wall, raise up a ramp, pitch camps and place battering rams against it all around."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then lay siege to it: Erect siege works against it, build a ramp up to it, set up camps against it and put battering rams around it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Show the city under siege. Build a wall around it so no one can escape. Set up the enemy camp, and surround the city with siege ramps and battering rams."
      }
    },
