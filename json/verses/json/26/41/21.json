    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "圣所的门框是方形的，至圣所前的门框也一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "外殿的門框是正方形的；在至聖所前面的門框，也是一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "殿的門柱是方的。至聖所的前面，形狀和殿的形狀一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The doorposts of the nave were square; as for the front of the sanctuary, the appearance of one doorpost was like that of the other."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The main hall had a rectangular doorframe, and the one at the front of the Most Holy Place was similar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There were square columns at the entrance to the sanctuary, and the ones at the entrance of the Most Holy Place were similar."
      }
    },
