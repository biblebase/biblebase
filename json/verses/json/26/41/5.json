    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他又量殿墙，厚三米，殿的外围有厢房，每间宽二米。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又量了聖殿的牆，厚三公尺；殿的周圍有多間廂房，各寬二公尺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又量殿牆，厚六肘；圍着殿有旁屋，各寬四肘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he measured the wall of the temple, six cubits; and the width of the side chambers, four cubits, all around about the house on every side."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he measured the wall of the temple; it was six cubits thick, and each side room around the temple was four cubits wide."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he measured the wall of the Temple, and it was 101/2 feet thick. There was a row of rooms along the outside wall; each room was 7 feet wide."
      }
    },
