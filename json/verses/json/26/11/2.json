    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对我说：“人子啊，这些人在这城里专给人出毒计，设恶谋。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對我說：“人子啊！這些就是圖謀罪孽，在這城裡設惡謀的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對我說：「人子啊，這就是圖謀罪孽的人，在這城中給人設惡謀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to me, “Son of man, these are the men who devise iniquity and give evil advice in this city,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said to me, “Son of man, these are the men who are plotting evil and giving wicked advice in this city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Spirit said to me, “Son of man, these are the men who are planning evil and giving wicked counsel in this city."
      }
