    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你要说，‘主耶和华这样说，我必从各国召集你们，从你们被驱散到的列邦中聚集你们，将以色列再赐给你们。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此你要說：‘主耶和華這樣說：我要從萬族中招聚你們，從你們被分散到的各地聚集你們，把以色列地賜給你們。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你當說：『主耶和華如此說：我必從萬民中招聚你們，從分散的列國內聚集你們，又要將以色列地賜給你們。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore say, ‘Thus says the Lord God, “I will gather you from the peoples and assemble you out of the countries among which you have been scattered, and I will give you the land of Israel.” ’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Therefore say: ‘This is what the Sovereign Lord says: I will gather you from the nations and bring you back from the countries where you have been scattered, and I will give you back the land of Israel again.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I, the Sovereign Lord, will gather you back from the nations where you have been scattered, and I will give you the land of Israel once again.’"
      }
    },
