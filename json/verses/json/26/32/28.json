    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“法老啊，你要被毁灭，跟那些未受割礼、丧身刀下的人躺在一起。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老啊！至於你，你必在沒有受割禮的人中被毀滅，與那些被刀所殺的人一起長眠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「法老啊，你必在未受割禮的人中敗壞，與那些被殺的人一同躺臥。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But in the midst of the uncircumcised you will be broken and lie with those slain by the sword."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You too, Pharaoh, will be broken and will lie among the uncircumcised, with those killed by the sword."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You too, Egypt, will lie crushed and broken among the outcasts, all slaughtered by the sword."
      }
    },
