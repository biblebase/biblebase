    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“亚述和她的大军都在那里，她周围是她大军的坟墓。他们都是丧身刀下的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“亞述和它的軍隊都在那裡；他們的墳墓在它周圍；他們都是被殺死的，是倒在刀下的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「亞述和她的眾民都在那裏，她民的墳墓在她四圍；他們都是被殺倒在刀下的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Assyria is there and all her company; her graves are round about her. All of them are slain, fallen by the sword,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Assyria is there with her whole army; she is surrounded by the graves of all her slain, all who have fallen by the sword."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Assyria lies there surrounded by the graves of its army, those who were slaughtered by the sword."
      }
    },
