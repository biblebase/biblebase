    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“埃及啊，你比谁更美呢？ 你下去跟未受割礼的人躺在一起吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要對埃及說： ‘你比誰更優越呢？ 你下去與那些沒有受割禮的人一起長眠吧！’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你埃及的美麗勝過誰呢？ 你下去與未受割禮的人一同躺臥吧！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Whom do you surpass in beauty? Go down and make your bed with the uncircumcised.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Say to them, ‘Are you more favored than others? Go down and be laid among the uncircumcised.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Say to them, ‘O Egypt, are you lovelier than the other nations? No! So go down to the pit and lie there among the outcasts.’"
      }
    },
