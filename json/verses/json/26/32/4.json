    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "抛在地上，丢在田野， 给天上的飞鸟吃， 让地上的走兽饱餐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必把你丟棄在地上， 拋擲在田野， 使空中的飛鳥都住在你身上， 使地上所有的野獸都吃你而得飽足。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必將你丟在地上， 拋在田野， 使空中的飛鳥都落在你身上， 使遍地的野獸吃你得飽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will leave you on the land; I will cast you on the open field. And I will cause all the birds of the heavens to dwell on you, And I will satisfy the beasts of the whole earth with you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will throw you on the land and hurl you on the open field. I will let all the birds of the sky settle on you and all the animals of the wild gorge themselves on you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will leave you stranded on the land to die. All the birds of the heavens will land on you, and the wild animals of the whole earth will gorge themselves on you."
      }
    },
