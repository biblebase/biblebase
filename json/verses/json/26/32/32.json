    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我使法老生前给人带来恐惧，但他和他的百姓最后还是要躺卧在未受割礼、丧身刀下的人当中。这是主耶和华说的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然我任憑法老使驚恐散布在活人之地，他和他的眾民必在沒有受割禮的人中，與被刀所殺的人一起長眠。這是主耶和華的宣告。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我任憑法老在活人之地使人驚恐，法老和他的羣眾必放在未受割禮和被殺的人中。這是主耶和華說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Though I instilled a terror of him in the land of the living, yet he will be made to lie down among the uncircumcised along with those slain by the sword, even Pharaoh and all his hordes,” declares the Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Although I had him spread terror in the land of the living, Pharaoh and all his hordes will be laid among the uncircumcised, with those killed by the sword, declares the Sovereign Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Although I have caused his terror to fall upon all the living, Pharaoh and his hordes will lie there among the outcasts who were slaughtered by the sword. I, the Sovereign Lord, have spoken!”"
      }
    },
