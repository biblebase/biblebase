    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它高大美丽，枝干修长， 因为树根深入水源，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "它樹身高大，枝條長長而顯得秀美， 因為它的根扎在水多的地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "樹大條長，成為榮美， 因為根在眾水之旁。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So it was beautiful in its greatness, in the length of its branches; For its roots extended to many waters."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It was majestic in beauty, with its spreading boughs, for its roots went down to abundant waters."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It was strong and beautiful, with wide-spreading branches, for its roots went deep into abundant water."
      }
    },
