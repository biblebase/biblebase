    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "空中的飞鸟都在它的枝头上搭窝， 田间的野兽都在它的树荫下生产， 所有的大国都住在它的荫下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "空中的飛鳥都在枝上搭窩； 田野的走獸都在枝條下生產； 所有大國都在它的樹蔭下居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "空中的飛鳥都在枝子上搭窩； 田野的走獸都在枝條下生子； 所有大國的人民都在它蔭下居住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the birds of the heavens nested in its boughs, And under its branches all the beasts of the field gave birth, And all great nations lived under its shade."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the birds of the sky nested in its boughs, all the animals of the wild gave birth under its branches; all the great nations lived in its shade."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The birds nested in its branches, and in its shade all the wild animals gave birth. All the great nations of the world lived in its shadow."
      }
    },
