    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘我从你旁边经过，见你在血泊中挣扎，便对血泊中的你说，活下去！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘但我從你身旁經過，見你在血中掙扎。那時，你躺在血中，我對你說：要活下去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我從你旁邊經過，見你滾在血中，就對你說：你雖在血中，仍可存活；你雖在血中，仍可存活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“When I passed by you and saw you squirming in your blood, I said to you while you were in your blood, ‘Live!’ Yes, I said to you while you were in your blood, ‘Live!’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Then I passed by and saw you kicking about in your blood, and as you lay there in your blood I said to you, “Live!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But I came by and saw you there, helplessly kicking about in your own blood. As you lay there, I said, ‘Live!’"
      }
    },
