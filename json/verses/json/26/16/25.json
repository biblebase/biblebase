    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你在街头巷尾建高高的神庙，糟蹋自己的美貌。你向每一个过路的人投怀送抱，纵情淫乱，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你在各路口建造高壇，使你的美麗變為可憎的，又向每一個過路的人張開兩腿，多行淫亂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你在一切市口上建造高臺，使你的美貌變為可憎的，又與一切過路的多行淫亂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You built yourself a high place at the top of every street and made your beauty abominable, and you spread your legs to every passer-by to multiply your harlotry."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At every street corner you built your lofty shrines and degraded your beauty, spreading your legs with increasing promiscuity to anyone who passed by."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On every street corner you defiled your beauty, offering your body to every passerby in an endless stream of prostitution."
      }
    },
