    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "烧毁你的房屋，在众妇女面前审判你。我要阻止你的淫乱行为，使你不再倒贴钱给情人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們也要用火燒你的房屋，在許多婦人面前向你施行審判。我必止住你的淫行，也必使你不再把誘人行淫的禮物送給人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "用火焚燒你的房屋，在許多婦人眼前向你施行審判。我必使你不再行淫，也不再贈送與人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will burn your houses with fire and execute judgments on you in the sight of many women. Then I will stop you from playing the harlot, and you will also no longer pay your lovers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will burn down your houses and inflict punishment on you in the sight of many women. I will put a stop to your prostitution, and you will no longer pay your lovers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will burn your homes and punish you in front of many women. I will stop your prostitution and end your payments to your many lovers."
      }
    },
