    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是又跟那商贾之地的迦勒底人行淫，但仍不满足。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是你增多你的淫亂，與貿易之地迦勒底行淫，你還是不滿足。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且多行淫亂，直到那貿易之地，就是迦勒底，你仍不滿意。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You also multiplied your harlotry with the land of merchants, Chaldea, yet even with this you were not satisfied.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then you increased your promiscuity to include Babylonia, a land of merchants, but even with this you were not satisfied."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You added to your lovers by embracing Babylonia, the land of merchants, but you still weren’t satisfied."
      }
