    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你和你母亲及姊妹如出一辙，都厌弃自己的丈夫和儿女。你母亲是赫人，你父亲是亚摩利人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你實在是你母親的女兒，她厭棄自己的丈夫和兒女；你實在是你姊妹的姊妹，她們厭棄她們的丈夫和兒女；你們的母親是赫人，你們的父親是亞摩利人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你正是你母親的女兒，厭棄丈夫和兒女；你正是你姊妹的姊妹，厭棄丈夫和兒女。你母親是赫人，你父親是亞摩利人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You are the daughter of your mother, who loathed her husband and children. You are also the sister of your sisters, who loathed their husbands and children. Your mother was a Hittite and your father an Amorite."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are a true daughter of your mother, who despised her husband and her children; and you are a true sister of your sisters, who despised their husbands and their children. Your mother was a Hittite and your father an Amorite."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For your mother loathed her husband and her children, and so do you. And you are exactly like your sisters, for they despised their husbands and their children. Truly your mother was a Hittite and your father an Amorite."
      }
    },
