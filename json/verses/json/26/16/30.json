    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘主耶和华说，你的心败坏至极，简直就像不知羞耻的娼妓，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘你行的這一切事，簡直是任性無恥的妓女所為，可見你的意志是多麼的薄弱。這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「主耶和華說：你行這一切事，都是不知羞恥妓女所行的，可見你的心是何等懦弱！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“How languishing is your heart,” declares the Lord God, “while you do all these things, the actions of a bold-faced harlot."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘I am filled with fury against you, declares the Sovereign Lord, when you do all these things, acting like a brazen prostitute!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What a sick heart you have, says the Sovereign Lord, to do such things as these, acting like a shameless prostitute."
      }
    },
