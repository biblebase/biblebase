    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你用我给你的金银首饰为自己制造男人的像，与他们苟合。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你又拿了我送給你那些美麗的金器銀器，為自己鑄造男人的像，與那些像行淫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你又將我所給你那華美的金銀、寶器為自己製造人像，與他行邪淫；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You also took your beautiful jewels made of My gold and of My silver, which I had given you, and made for yourself male images that you might play the harlot with them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You also took the fine jewelry I gave you, the jewelry made of my gold and silver, and you made for yourself male idols and engaged in prostitution with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You took the very jewels and gold and silver ornaments I had given you and made statues of men and worshiped them. This is adultery against me!"
      }
    },
