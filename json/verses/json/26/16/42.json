    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，我的烈怒才会平息，我的义愤才会消除，我才会平静下来，不再发怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣我向你發的烈怒就可以平息，我的妒忿也可以離開你；我可以安靜，不再惱怒了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，我就止息向你發的忿怒，我的忌恨也要離開你，我要安靜不再惱怒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I will calm My fury against you and My jealousy will depart from you, and I will be pacified and angry no more."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then my wrath against you will subside and my jealous anger will turn away from you; I will be calm and no longer angry."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then at last my fury against you will be spent, and my jealous anger will subside. I will be calm and will not be angry with you anymore."
      }
    },
