    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "母狮盼着它回来， 眼见指望落空， 便又养育一头幼狮， 使它成为雄壮的狮子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "母獅見自己所等候的落空了， 所希望的幻滅了， 就從牠的小獅子中另取出一隻， 使牠成為少壯的獅子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "母獅見自己等候失了指望， 就從牠小獅子中又將一個養為少壯獅子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When she saw, as she waited, That her hope was lost, She took another of her cubs And made him a young lion."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘When she saw her hope unfulfilled, her expectation gone, she took another of her cubs and made him a strong lion."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When the lioness saw that her hopes for him were gone, she took another of her cubs and taught him to be a strong young lion."
      }
    },
