    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘你母亲像葡萄园中的一棵葡萄树， 栽在溪水旁， 枝繁叶茂，硕果累累， 因为水源充沛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘你的母親像葡萄園裡的一棵葡萄樹， 栽種在水邊； 因為水源充足， 結果纍纍、枝葉茂盛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的母親先前如葡萄樹， 極其茂盛，栽於水旁。 因為水多， 就多結果子，滿生枝子；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your mother was like a vine in your vineyard, Planted by the waters; It was fruitful and full of branches Because of abundant waters."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Your mother was like a vine in your vineyard planted by the water; it was fruitful and full of branches because of abundant water."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Your mother was like a vine planted by the water’s edge. It had lush, green foliage because of the abundant water."
      }
    },
