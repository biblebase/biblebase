    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若我使猛兽在那里横行、肆虐，以致那里荒凉、人踪绝迹，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我使猛獸經過那地，蹂躪那地，使它荒涼，以致因為這些獸的緣故，沒有人敢經過；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若使惡獸經過糟踐那地，使地荒涼，以致因這些獸，人都不得經過；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If I were to cause wild beasts to pass through the land and they depopulated it, and it became desolate so that no one would pass through it because of the beasts,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Or if I send wild beasts through that country and they leave it childless and it becomes desolate so that no one can pass through it because of the beasts,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Or suppose I were to send wild animals to invade the country, kill the people, and make the land too desolate and dangerous to pass through."
      }
    },
