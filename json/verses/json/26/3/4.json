    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂又对我说：“人子啊，现在你要到以色列人那里，把我的话传给他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他對我說：“人子啊，你去！到以色列家那裡，把我的話告訴他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對我說：「人子啊，你往以色列家那裏去，將我的話對他們講說。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He said to me, “Son of man, go to the house of Israel and speak with My words to them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He then said to me: “Son of man, go now to the people of Israel and speak my words to them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said, “Son of man, go to the people of Israel and give them my messages."
      }
