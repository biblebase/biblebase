    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不过，我要使你的脸像他们的脸一样硬，使你的额像他们的额一样硬。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！我要使你的臉像他們的臉一樣堅硬，使你的前額像他們的前額一樣堅硬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，我使你的臉硬過他們的臉，使你的額硬過他們的額。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I have made your face as hard as their faces and your forehead as hard as their foreheads."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I will make you as unyielding and hardened as they are."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But look, I have made you as obstinate and hard-hearted as they are."
      }
