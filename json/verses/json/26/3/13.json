    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我听见活物的翅膀相碰的声音，以及他们旁边的轮子发出的隆隆声。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我又聽見那些活物的翅膀彼此相碰的聲音和活物旁邊的輪子的聲音，以及轟轟的響聲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我又聽見那活物翅膀相碰，與活物旁邊輪子旋轉震動轟轟的響聲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And I heard the sound of the wings of the living beings touching one another and the sound of the wheels beside them, even a great rumbling sound."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It was the sound of the wings of the living creatures brushing against each other and the sound of the wheels beside them, a loud rumbling sound."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It was the sound of the wings of the living beings as they brushed against each other and the rumbling of their wheels beneath them."
      }
