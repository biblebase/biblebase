    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他犯的罪不会被追究，因为他行事公正仁义，所以必得存活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他所犯的一切罪，必不被記念；他行了公平和正直的事，就必存活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他所犯的一切罪必不被記念。他行了正直與合理的事，必定存活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "None of his sins that he has committed will be remembered against him. He has practiced justice and righteousness; he shall surely live."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "None of the sins that person has committed will be remembered against them. They have done what is just and right; they will surely live."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "None of their past sins will be brought up again, for they have done what is just and right, and they will surely live."
      }
