    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们随着人群来到你面前，坐着聆听你的话，俨然像我的子民。但他们不付诸实行，嘴里说的是甜言蜜语，心中却追求不义之财。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們成群來到你那裡，坐在你面前，像是我的子民一樣。其實他們聽了你的話，卻不去行；他們只是用口表示愛慕，他們的心卻是追隨不義之財。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們來到你這裏如同民來聚會，坐在你面前彷彿是我的民。他們聽你的話卻不去行；因為他們的口多顯愛情，心卻追隨財利。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They come to you as people come, and sit before you as My people and hear your words, but they do not do them, for they do the lustful desires expressed by their mouth, and their heart goes after their gain."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My people come to you, as they usually do, and sit before you to hear your words, but they do not put them into practice. Their mouths speak of love, but their hearts are greedy for unjust gain."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So my people come pretending to be sincere and sit before you. They listen to your words, but they have no intention of doing what you say. Their mouths are full of lustful words, and their hearts seek only after money."
      }
    },
