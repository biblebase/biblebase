    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人弃恶行义，必因义行而存活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人若離開他的惡，去行公平和正直的事，他就必因此存活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人轉離他的惡，行正直與合理的事，就必因此存活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when the wicked turns from his wickedness and practices justice and righteousness, he will live by them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And if a wicked person turns away from their wickedness and does what is just and right, they will live by doing so."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if wicked people turn from their wickedness and do what is just and right, they will live."
      }
