    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看吧，我所说的必定应验。当一切应验的时候，他们就知道有先知在他们当中。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！這事必定應驗，到那時候，他們就知道在他們中間有先知了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，所說的快要應驗；應驗了，他們就知道在他們中間有了先知。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So when it comes to pass—as surely it will—then they will know that a prophet has been in their midst.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When all this comes true—and it surely will—then they will know that a prophet has been among them.”"
      },
