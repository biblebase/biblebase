    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“人子啊，你要告诉你的同胞，‘我若使战争降临某地，那地方的百姓就应选一人做守望者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人子啊！你要告訴你本國的人，說：‘我使刀劍臨到一地，那地的人民從他們中間選出一個人，作他們的守望者；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人子啊，你要告訴本國的子民說：我使刀劍臨到哪一國，那一國的民從他們中間選立一人為守望的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Son of man, speak to the sons of your people and say to them, ‘If I bring a sword upon a land, and the people of the land take one man from among them and make him their watchman,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Son of man, speak to your people and say to them: ‘When I bring the sword against a land, and the people of the land choose one of their men and make him their watchman,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Son of man, give your people this message: ‘When I bring an army against a country, the people of that land choose one of their own to be a watchman."
      }
    },
