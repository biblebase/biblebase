    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要推翻我的公义， 归罪于我而自以为义吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你怎能廢棄我所審斷的？ 怎能定我為有罪，好顯出你自己為義呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你豈可廢棄我所擬定的？ 豈可定我有罪，好顯自己為義嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will you really annul My judgment? Will you condemn Me that you may be justified?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Would you discredit my justice? Would you condemn me to justify yourself?"
