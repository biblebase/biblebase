    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“强辩者岂可与全能者争论？ 与上帝辩驳的人请作出答复。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“挑剔是非的，怎能與全能者爭辯呢？ 責備　神的，回答這個問題吧。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "強辯的豈可與全能者爭論嗎？ 與神辯駁的可以回答這些吧！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Will the faultfinder contend with the Almighty? Let him who reproves God answer it.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Will the one who contends with the Almighty correct him? Let him who accuses God answer him!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do you still want to argue with the Almighty? You are God’s critic, but do you have the answers?”"
      }
    },
