    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你看河马， 它和你都是我造的， 它像牛一样吃草。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你看看河馬吧， 我造牠像造你一樣， 牠吃草如牛一般。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你且觀看河馬； 我造你也造牠。 牠吃草與牛一樣；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold now, Behemoth, which I made as well as you; He eats grass like an ox."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Look at Behemoth, which I made along with you and which feeds on grass like an ox."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Take a look at Behemoth, which I made, just as I made you. It eats grass like an ox."
      }
    },
