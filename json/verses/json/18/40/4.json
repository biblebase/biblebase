    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我这样卑微，怎能答复你？ 我只有用手掩口。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我是微小的，可以回答你甚麼呢？ 我只好用手掩口。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我是卑賤的！我用甚麼回答你呢？ 只好用手摀口。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, I am insignificant; what can I reply to You? I lay my hand on my mouth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I am unworthy—how can I reply to you? I put my hand over my mouth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I am nothing—how could I ever find the answers? I will cover my mouth with my hand."
      }
    },
