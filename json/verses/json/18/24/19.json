    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "阴间吞噬犯罪之人， 如同干热吞噬融雪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "乾旱與炎熱怎樣消除雪水， 陰間也這樣除去犯罪的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "乾旱炎熱消沒雪水； 陰間也如此消沒犯罪之輩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Drought and heat consume the snow waters, So does Sheol those who have sinned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As heat and drought snatch away the melted snow, so the grave snatches away those who have sinned."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The grave consumes sinners just as drought and heat consume snow."
      }
    },
