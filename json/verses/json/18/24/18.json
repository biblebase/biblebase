    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“恶人如水上的泡沫。 他们的产业遭咒诅， 他们的葡萄园荒芜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些惡人如水面上飄浮的東西迅速流逝， 他們在世上所得的分被咒詛， 他們不能再走葡萄園的路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些惡人猶如浮萍快快飄去。 他們所得的分在世上被咒詛； 他們不得再走葡萄園的路。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“They are insignificant on the surface of the water; Their portion is cursed on the earth. They do not turn toward the vineyards."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Yet they are foam on the surface of the water; their portion of the land is cursed, so that no one goes to the vineyards."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But they disappear like foam down a river. Everything they own is cursed, and they are afraid to enter their own vineyards."
      }
    },
