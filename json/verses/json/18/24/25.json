    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我说的都是实情，谁能否认？ 谁能证明我的话是空谈？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果不是這樣，誰能證明我是撒謊的， 指出我的言語為空虛的呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若不是這樣，誰能證實我是說謊的， 將我的言語駁為虛空呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now if it is not so, who can prove me a liar, And make my speech worthless?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If this is not so, who can prove me false and reduce my words to nothing?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can anyone claim otherwise? Who can prove me wrong?”"
      }
    },
