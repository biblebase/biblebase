    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你榨干了我， 使我骨瘦如柴， 这成了我的罪证。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又把我捆綁起來。’這就作為證據； 我身體的枯瘦也當面作證反對我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又抓住我，作見證攻擊我； 我身體的枯瘦也當面見證我的不是。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have shriveled me up, It has become a witness; And my leanness rises up against me, It testifies to my face."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have shriveled me up—and it has become a witness; my gauntness rises up and testifies against me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As if to prove I have sinned, you’ve reduced me to skin and bones. My gaunt flesh testifies against me."
      }
    },
