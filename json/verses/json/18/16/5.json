    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我会对你们讲鼓励的话， 用劝慰之言减轻你们的痛苦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我能用口鼓勵你們， 我嘴唇的安慰，能緩和你們的痛苦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我必用口堅固你們， 用嘴消解你們的憂愁。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I could strengthen you with my mouth, And the solace of my lips could lessen your pain."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But my mouth would encourage you; comfort from my lips would bring you relief."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if it were me, I would encourage you. I would try to take away your grief."
      }
    },
