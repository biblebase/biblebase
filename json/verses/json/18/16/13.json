    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂的弓箭手四面围住我， 祂毫不留情地刺透我的肾脏， 使我肝胆涂地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的弓箭手四面包圍我， 他剖開我的腰子，全不顧惜， 把我的膽傾倒在地上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的弓箭手四面圍繞我； 他破裂我的肺腑，並不留情， 把我的膽傾倒在地上，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His arrows surround me. Without mercy He splits my kidneys open; He pours out my gall on the ground."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "his archers surround me. Without pity, he pierces my kidneys and spills my gall on the ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and now his archers surround me. His arrows pierce me without mercy. The ground is wet with my blood."
      }
    },
