      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "临终的人为我祝福， 我使寡妇心里欢唱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "將要滅亡的，為我祝福； 我使寡婦的心歡呼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將要滅亡的為我祝福； 我也使寡婦心中歡樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The blessing of the one ready to perish came upon me, And I made the widow’s heart sing for joy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The one who was dying blessed me; I made the widow’s heart sing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I helped those without hope, and they blessed me. And I caused the widows’ hearts to sing for joy."
      }
