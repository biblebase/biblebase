    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们盼我如盼甘霖， 张着口如慕春雨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們等候我像等候雨水， 又大大張嘴如切慕春雨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們仰望我如仰望雨， 又張開口如切慕春雨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They waited for me as for the rain, And opened their mouth as for the spring rain."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They waited for me as for showers and drank in my words as the spring rain."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They longed for me to speak as people long for rain. They drank my words like a refreshing spring rain."
      }
    },
