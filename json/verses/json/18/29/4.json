    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时，我身强力壮， 上帝是我家的密友；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但願我仍然活在年輕力壯的日子， 那時　神在我的帳棚裡與我親密。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我願如壯年的時候： 那時我在帳棚中， 神待我有密友之情；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As I was in the prime of my days, When the friendship of God was over my tent;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Oh, for the days when I was in my prime, when God’s intimate friendship blessed my house,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When I was in my prime, God’s friendship was felt in my home."
      }
    },
