    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听见我的都祝福我， 看见我的都称赞我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耳朵聽見我的，就稱我有福。 眼睛看見我的，就作證稱讚我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耳朵聽我的，就稱我有福； 眼睛看我的，便稱讚我；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For when the ear heard, it called me blessed, And when the eye saw, it gave witness of me,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever heard me spoke well of me, and those who saw me commended me,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“All who heard me praised me. All who saw me spoke well of me."
      }
