    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要亲自见祂， 我要亲眼见祂。 我心中充满渴望！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必見他在我身邊， 我要親眼見他，並非外人， 我的心腸在我裡面渴想極了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我自己要見他， 親眼要看他，並不像外人。 我的心腸在我裏面消滅了！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whom I myself shall behold, And whom my eyes will see and not another. My heart faints within me!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I myself will see him with my own eyes—I, and not another. How my heart yearns within me!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will see him for myself. Yes, I will see him with my own eyes. I am overwhelmed at the thought!"
      }
