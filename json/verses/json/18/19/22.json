    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们为何像上帝一样逼我？ 为何吃了我的肉还不满足？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們為甚麼有如　神那樣逼迫我？ 還不因吃我的肉感到滿足嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們為甚麼彷彿神逼迫我， 吃我的肉還以為不足呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why do you persecute me as God does, And are not satisfied with my flesh?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why do you pursue me as God does? Will you never get enough of my flesh?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Must you also persecute me, like God does? Haven’t you chewed me up enough?"
      }
    },
