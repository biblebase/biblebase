      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝筑起墙垒，阻断我的路， 祂使黑暗笼罩我的路途。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神用籬笆攔阻我的道路，使我不能經過； 他又使黑暗籠罩我的路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神用籬笆攔住我的道路，使我不得經過； 又使我的路徑黑暗。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has walled up my way so that I cannot pass, And He has put darkness on my paths."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He has blocked my way so I cannot pass; he has shrouded my paths in darkness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God has blocked my way so I cannot move. He has plunged my path into darkness."
      }
