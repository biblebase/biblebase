      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我呼唤仆人，他却不回应； 我哀求他，他也不理睬。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我呼喚僕人，他不回答， 我得用口哀求他。"
      },
      "cunp": {
