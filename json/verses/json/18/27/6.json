        "text": "我堅守我的義決不放鬆， 我一生的日子我的心必不責備我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我持定我的義，必不放鬆； 在世的日子，我心必不責備我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I hold fast my righteousness and will not let it go. My heart does not reproach any of my days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will maintain my innocence and never let go of it; my conscience will not reproach me as long as I live."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will maintain my innocence without wavering. My conscience is clear for as long as I live."
      }
    },
