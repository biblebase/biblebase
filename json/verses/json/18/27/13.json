    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以下是上帝给恶人定的结局， 全能者赐给残暴之徒的归宿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這是惡人從　神所得的分， 強暴的人從全能者所得的業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神為惡人所定的分， 強暴人從全能者所得的報乃是這樣："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“This is the portion of a wicked man from God, And the inheritance which tyrants receive from the Almighty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Here is the fate God allots to the wicked, the heritage a ruthless man receives from the Almighty:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the wicked will receive from God; this is their inheritance from the Almighty."
      }
    },
