    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我要教导你们有关上帝的能力， 我不会隐瞒全能者的作为。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我藉　神的能力教導你們， 我不向你們隱瞞全能者的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神的作為，我要指教你們； 全能者所行的，我也不隱瞞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will instruct you in the power of God; What is with the Almighty I will not conceal."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will teach you about the power of God; the ways of the Almighty I will not conceal."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will teach you about God’s power. I will not conceal anything concerning the Almighty."
      }
    },
