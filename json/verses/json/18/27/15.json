    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他幸存的后人被瘟疫吞噬， 生还的寡妇也不哀哭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的遺族在死人中埋葬， 他的寡婦也都不哀哭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他所遺留的人必死而埋葬； 他的寡婦也不哀哭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His survivors will be buried because of the plague, And their widows will not be able to weep."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The plague will bury those who survive him, and their widows will not weep for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who survive will die of a plague, and not even their widows will mourn them."
      }
