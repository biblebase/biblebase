    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他留不住劳碌的成果， 无法享用所赚的财富。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他勞苦所得的必歸別人，自己卻不得吃用； 他交易得來的財利，自己卻不得享用。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他勞碌得來的要賠還，不得享用； 不能照所得的財貨歡樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He returns what he has attained And cannot swallow it; As to the riches of his trading, He cannot even enjoy them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What he toiled for he must give back uneaten; he will not enjoy the profit from his trading."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will give back everything they worked for. Their wealth will bring them no joy."
      }
