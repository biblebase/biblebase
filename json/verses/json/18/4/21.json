    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们帐篷的绳索被拔起， 他们毫无智慧地死去。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們帳棚的繩索不是從他們中間抽出來嗎？ 他們死去，不是無智慧而死嗎？’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他帳棚的繩索豈不從中抽出來呢？ 他死，且是無智慧而死。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Is not their tent-cord plucked up within them? They die, yet without wisdom.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Are not the cords of their tent pulled up, so that they die without wisdom?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their tent-cords are pulled and the tent collapses, and they die in ignorance."
      }
