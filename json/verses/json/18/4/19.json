    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "更何况源自尘土、 住在土造的躯壳里、 脆弱如蛾的世人呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "何況那些住在土屋裡， 根基在塵土中， 比蠹蟲還容易被壓碎的人呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "何況那住在土房、根基在塵土裏、 被蠹蟲所毀壞的人呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How much more those who dwell in houses of clay, Whose foundation is in the dust, Who are crushed before the moth!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "how much more those who live in houses of clay, whose foundations are in the dust, who are crushed more readily than a moth!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "how much less will he trust people made of clay! They are made of dust, crushed as easily as a moth."
      }
    },
