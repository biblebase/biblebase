      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠使自己行過的路發出白光， 令人把深淵當作白髮老人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牠行的路隨後發光， 令人想深淵如同白髮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behind him he makes a wake to shine; One would think the deep to be gray-haired."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It leaves a glistening wake behind it; one would think the deep had white hair."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The water glistens in its wake, making the sea look white."
      }
