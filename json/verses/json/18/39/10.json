    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你岂能用缰绳把野牛牵到犁沟？ 它岂肯跟着你在山谷耕地？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你怎能用套繩把野牛繫在犁溝呢？ 牠怎肯跟著你耙山谷之地呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你豈能用套繩將野牛籠在犂溝之間？ 牠豈肯隨你耙山谷之地？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Can you bind the wild ox in a furrow with ropes, Or will he harrow the valleys after you?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Can you hold it to the furrow with a harness? Will it till the valleys behind you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can you hitch a wild ox to a plow? Will it plow a field for you?"
      }
    },
