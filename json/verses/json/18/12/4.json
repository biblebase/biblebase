    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我这求告上帝并蒙祂应允的人竟成了朋友的笑柄， 我这公义纯全的人竟成了笑柄。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我這求告　神，又蒙他應允的， 竟成為朋友的笑柄， 公義完全的人竟成為笑柄，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我這求告神、蒙他應允的人 竟成了朋友所譏笑的； 公義完全人竟受了人的譏笑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am a joke to my friends, The one who called on God and He answered him; The just and blameless man is a joke."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I have become a laughingstock to my friends, though I called on God and he answered— a mere laughingstock, though righteous and blameless!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet my friends laugh at me, for I call on God and expect an answer. I am a just and blameless man, yet they laugh at me."
      }
    },
