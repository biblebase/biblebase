    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但祂若保持缄默，谁能定祂有罪？ 祂若掩起脸来，谁能看得见祂？ 个人和国家都靠祂垂顾，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他若安靜不動，誰能定他有罪呢？ 他若掩面，誰能看見他呢？ 無論對待一國或一人，都是這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他使人安靜，誰能擾亂呢？ 他掩面，誰能見他呢？ 無論待一國或一人都是如此－"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He keeps quiet, who then can condemn? And when He hides His face, who then can behold Him, That is, in regard to both nation and man?—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if he remains silent, who can condemn him? If he hides his face, who can see him? Yet he is over individual and nation alike,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if he chooses to remain quiet, who can criticize him? When he hides his face, no one can find him, whether an individual or a nation."
      }
    },
