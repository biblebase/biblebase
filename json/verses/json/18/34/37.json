    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他的叛逆使他罪上加罪， 他在我们中间拍手讥笑， 用许多话亵渎上帝。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他在自己的罪上又加過犯， 在我們中間鼓掌嘲笑　神， 用許多話敵對　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他在罪上又加悖逆； 在我們中間拍手， 用許多言語輕慢神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For he adds rebellion to his sin; He claps his hands among us, And multiplies his words against God.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To his sin he adds rebellion; scornfully he claps his hands among us and multiplies his words against God.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you have added rebellion to your sin; you show no respect, and you speak many angry words against God.”"
      }
