    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我虽然有理，却被认为是撒谎者； 我虽然无过，却受到致命的创伤。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我雖然有公理，卻被認為是說謊的； 我雖然毫無過犯，我所受的箭傷卻無法醫治。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我雖有理，還算為說謊言的； 我雖無過，受的傷還不能醫治。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Should I lie concerning my right? My wound is incurable, though I am without transgression.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Although I am right, I am considered a liar; although I am guiltless, his arrow inflicts an incurable wound.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am innocent, but they call me a liar. My suffering is incurable, though I have not sinned.’"
      }
    },
