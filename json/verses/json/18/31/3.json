    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "岂不是降灾难给不义之人， 降祸患给作恶的人？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "災難不是臨到不義的人嗎？ 災禍不是臨到作孽的人嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "豈不是禍患臨到不義的， 災害臨到作孽的呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Is it not calamity to the unjust And disaster to those who work iniquity?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Is it not ruin for the wicked, disaster for those who do wrong?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Isn’t it calamity for the wicked and misfortune for those who do evil?"
      }
