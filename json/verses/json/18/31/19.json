      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若見人因無衣死亡， 或見窮乏人身無遮蓋；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I have seen anyone perish for lack of clothing, Or that the needy had no covering,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "if I have seen anyone perishing for lack of clothing, or the needy without garments,"
      },
      "nlt": {
