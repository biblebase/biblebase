    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我何曾拒绝穷人的请求， 或使寡妇眼露失望之情，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若不給窮人所要的， 或使寡婦的眼所期待的落空；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若不容貧寒人得其所願， 或叫寡婦眼中失望，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If I have kept the poor from their desire, Or have caused the eyes of the widow to fail,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If I have denied the desires of the poor or let the eyes of the widow grow weary,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Have I refused to help the poor, or crushed the hopes of widows?"
      }
    },
