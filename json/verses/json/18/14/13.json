      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但願你把我藏在陰間， 把我隱藏，直到你的怒氣過去； 願你為我定一個期限，好記念我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟願你把我藏在陰間， 存於隱密處，等你的忿怒過去； 願你為我定了日期，記念我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Oh that You would hide me in Sheol, That You would conceal me until Your wrath returns to You, That You would set a limit for me and remember me!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If only you would hide me in the grave and conceal me till your anger has passed! If only you would set me a time and then remember me!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I wish you would hide me in the grave and forget me there until your anger has passed. But mark your calendar to think of me again!"
      }
