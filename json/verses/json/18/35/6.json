    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你若犯罪，与上帝何妨？ 你若罪恶累累，对祂有何影响？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你若犯罪，你能使　神受害嗎？ 你的過犯若增多，你又能使他受損嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你若犯罪，能使神受何害呢？ 你的過犯加增，能使神受何損呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you have sinned, what do you accomplish against Him? And if your transgressions are many, what do you do to Him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you sin, how does that affect him? If your sins are many, what does that do to him?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you sin, how does that affect God? Even if you sin again and again, what effect will it have on him?"
      }
    },
