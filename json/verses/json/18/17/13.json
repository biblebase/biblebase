    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我若期望阴间作我的家园， 在黑暗之地铺设我的床榻，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若等待陰間作我的家， 在黑暗中鋪張我的床榻；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若盼望陰間為我的房屋， 若下榻在黑暗中，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I look for Sheol as my home, I make my bed in the darkness;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If the only home I hope for is the grave, if I spread out my bed in the realm of darkness,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What if I go to the grave and make my bed in darkness?"
      }
    },
