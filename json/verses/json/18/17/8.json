    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这使正直的人震惊， 使无辜的人起来讨伐不敬虔的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正直人必因此驚訝， 要被激發起來， 攻擊不敬虔的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正直人因此必驚奇； 無辜的人要興起攻擊不敬虔之輩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The upright will be appalled at this, And the innocent will stir up himself against the godless."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The upright are appalled at this; the innocent are aroused against the ungodly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The virtuous are horrified when they see me. The innocent rise up against the ungodly."
      }
