    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "称坟墓为父亲， 称蛆虫为母亲、姊妹，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若對深坑說：‘你是我的父親’， 對蟲說：‘你是我的母親，我的姊妹’，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若對朽壞說：你是我的父； 對蟲說：你是我的母親姊妹；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I call to the pit, ‘You are my father’; To the worm, ‘my mother and my sister’;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "if I say to corruption, ‘You are my father,’ and to the worm, ‘My mother’ or ‘My sister,’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What if I call the grave my father, and the maggot my mother or my sister?"
      }
    },
