    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人为了分产业而控告朋友， 其子孙的眼睛要失明。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為分產業而控告朋友的， 他子孫的眼睛也要昏花。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "控告他的朋友、以朋友為可搶奪的， 連他兒女的眼睛也要失明。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who informs against friends for a share of the spoil, The eyes of his children also will languish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If anyone denounces their friends for reward, the eyes of their children will fail."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They betray their friends for their own advantage, so let their children faint with hunger."
      }
