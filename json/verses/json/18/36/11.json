    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们若听从、事奉祂， 就可一生幸福，安享天年。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們若聽從他、事奉他， 就必幸福地度盡他們的日子， 也必快樂地度過他們的歲月。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們若聽從事奉他， 就必度日亨通，歷年福樂；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If they hear and serve Him, They will end their days in prosperity And their years in pleasures."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If they obey and serve him, they will spend the rest of their days in prosperity and their years in contentment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If they listen and obey God, they will be blessed with prosperity throughout their lives. All their years will be pleasant."
      }
    },
