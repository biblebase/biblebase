    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂也要引领你脱离困境， 进入广阔自由之地， 使你享受满桌佳肴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他也誘導你脫離患難的虎口， 到寬闊不狹窄之地， 到你擺滿肥甘的席上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神也必引你出離患難， 進入寬闊不狹窄之地； 擺在你席上的必滿有肥甘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then indeed, He enticed you from the mouth of distress, Instead of it, a broad place with no constraint; And that which was set on your table was full of fatness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“He is wooing you from the jaws of distress to a spacious place free from restriction, to the comfort of your table laden with choice food."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“God is leading you away from danger, Job, to a place free from distress. He is setting your table with the best food."
      }
    },
