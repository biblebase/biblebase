    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我以为床铺是我的安慰， 卧榻可解除我的哀愁，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若說：‘我的床必安慰我， 我的榻必減輕我的苦情’，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若說：我的牀必安慰我， 我的榻必解釋我的苦情，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I say, ‘My bed will comfort me, My couch will ease my complaint,’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When I think my bed will comfort me and my couch will ease my complaint,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I think, ‘My bed will comfort me, and sleep will ease my misery,’"
      }
