    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为何不赦免我的过犯， 饶恕我的罪恶？ 我很快将归于尘土， 你将寻找我， 而我已不复存在。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你為甚麼不赦免我的過犯， 除去我的罪孽呢？ 現在我快要躺臥在塵土中， 那時你尋找我，我卻不在了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為何不赦免我的過犯， 除掉我的罪孽？ 我現今要躺臥在塵土中； 你要殷勤地尋找我，我卻不在了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why then do You not pardon my transgression And take away my iniquity? For now I will lie down in the dust; And You will seek me, but I will not be.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why do you not pardon my offenses and forgive my sins? For I will soon lie down in the dust; you will search for me, but I will be no more.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why not just forgive my sin and take away my guilt? For soon I will lie down in the dust and die. When you look for me, I will be gone.”"
      }
    },
