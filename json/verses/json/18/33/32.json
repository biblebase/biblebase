    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你若有话，就答复我； 你只管说，我愿看到你的清白。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你若有話說，就請回答我； 你只管說吧，因為我願意以你為義。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你若有話說，就可以回答我； 你只管說，因我願以你為是。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then if you have anything to say, answer me; Speak, for I desire to justify you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you have anything to say, answer me; speak up, for I want to vindicate you."
      },
