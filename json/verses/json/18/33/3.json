    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的话发自正直的心， 我的口如实陈明道理。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的話表明我心中的正直， 我所知道的，我的嘴唇就誠誠實實地說出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的言語要發明心中所存的正直； 我所知道的，我嘴唇要誠實地說出。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My words are from the uprightness of my heart, And my lips speak knowledge sincerely."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My words come from an upright heart; my lips sincerely speak what I know."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I speak with all sincerity; I speak the truth."
      }
