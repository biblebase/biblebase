    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就是恶人在灾难之日得到幸免， 在上帝发怒之日得以逃脱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就是惡人在災難的日子得存留， 在　神發怒的時候得逃脫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是惡人在禍患的日子得存留， 在發怒的日子得逃脫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the wicked is reserved for the day of calamity; They will be led forth at the day of fury."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "that the wicked are spared from the day of calamity, that they are delivered from the day of wrath?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Evil people are spared in times of calamity and are allowed to escape disaster."
      }
