    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，他们的幸福不在他们手中； 因此，我决不苟同他们的想法。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的福樂豈不是掌握在他們的手中？ 惡人的謀算離我很遠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，他們亨通不在乎自己； 惡人所謀定的離我好遠。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, their prosperity is not in their hand; The counsel of the wicked is far from me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But their prosperity is not in their own hands, so I stand aloof from the plans of the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "(They think their prosperity is of their own doing, but I will have nothing to do with that kind of thinking.)"
      }
