    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂洞悉诡诈之人， 看见罪恶， 岂会不理？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他認識虛謊的人， 他看見欺詐，怎能不鑒察？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他本知道虛妄的人； 人的罪孽，他雖不留意，還是無所不見。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He knows false men, And He sees iniquity without investigating."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely he recognizes deceivers; and when he sees evil, does he not take note?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For he knows those who are false, and he takes note of all their sins."
      }
    },
