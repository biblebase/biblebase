    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你喋喋不休，人们岂能缄默不言？ 你嘲笑讥讽，岂不该有人使你羞愧？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你胡言亂語可使人不作聲嗎？ 你嘲笑的時候，無人責備嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你誇大的話豈能使人不作聲嗎？ 你戲笑的時候豈沒有人叫你害羞嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Shall your boasts silence men? And shall you scoff and none rebuke?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Will your idle talk reduce others to silence? Will no one rebuke you when you mock?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Should I remain silent while you babble on? When you mock God, shouldn’t someone make you ashamed?"
      }
    },
