    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我盼望幸福，来的却是灾祸； 我期待光明，来的却是黑暗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我希望得福樂，災禍就來了； 我等候光明，幽暗就到了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我仰望得好處，災禍就到了； 我等待光明，黑暗便來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When I expected good, then evil came; When I waited for light, then darkness came."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet when I hoped for good, evil came; when I looked for light, then came darkness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I looked for good, but evil came instead. I waited for the light, but darkness fell."
      }
    },
