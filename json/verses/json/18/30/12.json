      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这些无赖从右边攻击我， 迫使我逃亡， 他们展开攻势要毁灭我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "一窩暴民在我右邊興起， 推開我的腳，築起災難的路攻擊我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這等下流人在我右邊起來， 推開我的腳，築成戰路來攻擊我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On the right hand their brood arises; They thrust aside my feet and build up against me their ways of destruction."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On my right the tribe attacks; they lay snares for my feet, they build their siege ramps against me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These outcasts oppose me to my face. They send me sprawling and lay traps in my path."
      }
