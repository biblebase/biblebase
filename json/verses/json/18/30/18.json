    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝猛力抓住我的衣服， 紧紧地揪住我的衣领。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的衣服因　神的大力變了形， 他的大力如外衣的領口把我束緊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因神的大力，我的外衣污穢不堪， 又如裏衣的領子將我纏住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By a great force my garment is distorted; It binds me about as the collar of my coat."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In his great power God becomes like clothing to me; he binds me like the neck of my garment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "With a strong hand, God grabs my shirt. He grips me by the collar of my coat."
      }
    },
