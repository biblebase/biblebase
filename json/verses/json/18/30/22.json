    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把我提到风中，让我被风吹走， 被暴风抛来抛去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你把我提到風中，使我被風颳去， 使我在暴風中消散。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "把我提在風中，使我駕風而行， 又使我消滅在烈風中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You lift me up to the wind and cause me to ride; And You dissolve me in a storm."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You snatch me up and drive me before the wind; you toss me about in the storm."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You throw me into the whirlwind and destroy me in the storm."
      }
