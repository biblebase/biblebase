    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "夜间我的骨头刺痛， 如被啃噬，无休无止。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "夜間痛苦刺透我的骨頭， 疼痛不止，不住地咀嚼我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "夜間，我裏面的骨頭刺我， 疼痛不止，好像齦我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "At night it pierces my bones within me, And my gnawing pains take no rest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Night pierces my bones; my gnawing pains never rest."
