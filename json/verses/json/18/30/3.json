    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们贫穷饥饿，瘦弱不堪， 夜间在干旱荒凉之地啃食。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們因窮乏與飢餓而消瘦， 在黑夜荒廢與荒涼之地咀嚼曠野的乾草；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們因窮乏飢餓，身體枯瘦， 在荒廢淒涼的幽暗中齦乾燥之地，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From want and famine they are gaunt Who gnaw the dry ground by night in waste and desolation,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Haggard from want and hunger, they roamed the parched land in desolate wastelands at night."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They are gaunt from poverty and hunger. They claw the dry ground in desolate wastelands."
      }
    },
