    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“诚然，不幸的人在困境中呼救， 无人会伸手加害他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然而人跌倒，怎會不伸手？ 人遇災禍，怎會不呼救呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "然而，人仆倒豈不伸手？ 遇災難豈不求救呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Yet does not one in a heap of ruins stretch out his hand, Or in his disaster therefore cry out for help?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Surely no one lays a hand on a broken man when he cries for help in his distress."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Surely no one would turn against the needy when they cry for help in their trouble."
      }
