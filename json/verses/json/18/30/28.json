    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我皮肤发黑，不是因为日晒， 我在会众中站起来呼救。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我四處行走，全身發黑， 並不是因為日曬； 我在會中站著呼求。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我沒有日光就哀哭行去； 我在會中站着求救。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I go about mourning without comfort; I stand up in the assembly and cry out for help."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I go about blackened, but not by the sun; I stand up in the assembly and cry for help."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I walk in gloom, without sunlight. I stand in the public square and cry for help."
      }
