      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我曾見愚妄人扎下根， 但我忽然咒詛他的住處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have seen the foolish taking root, And I cursed his abode immediately."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I myself have seen a fool taking root, but suddenly his house was cursed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have seen that fools may be successful for the moment, but then comes sudden disaster."
      }
    },
