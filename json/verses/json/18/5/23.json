    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你必与田间的石头立约， 野兽必与你和平相处。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你必與田間的石頭立約， 田間的野獸必與你和平相處。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你必與田間的石頭立約； 田裏的野獸也必與你和好。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For you will be in league with the stones of the field, And the beasts of the field will be at peace with you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For you will have a covenant with the stones of the field, and the wild animals will be at peace with you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will be at peace with the stones of the field, and its wild animals will be at peace with you."
      }
