    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "饥饿的人吞掉他的庄稼， 连荆棘里的也不放过； 干渴的人吞尽他的财富。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "飢餓的人吃盡他的莊稼， 連在荊棘裡的也搶去； 口渴的人吞盡他的財富。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的莊稼有飢餓的人吃盡了， 就是在荊棘裏的也搶去了； 他的財寶有網羅張口吞滅了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His harvest the hungry devour And take it to a place of thorns, And the schemer is eager for their wealth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The hungry consume his harvest, taking it even from among thorns, and the thirsty pant after his wealth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The hungry devour their harvest, even when it is guarded by brambles. The thirsty pant after their wealth."
      }
    },
