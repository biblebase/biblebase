      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿那日一片黑暗， 被天上的上帝遗忘， 没有阳光照耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願那日變成黑暗， 願　神不從上面眷顧， 願光明不照耀在其上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願那日變為黑暗； 願神不從上面尋找它； 願亮光不照於其上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May that day be darkness; Let not God above care for it, Nor light shine on it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "That day—may it turn to darkness; may God above not care about it; may no light shine on it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let that day be turned to darkness. Let it be lost even to God on high, and let no light shine on it."
      }
    },
