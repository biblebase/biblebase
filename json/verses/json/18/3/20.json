    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“为何赐光给受苦的人， 赐生命给心灵痛苦的人？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為甚麼有光明賜給勞碌的人， 有生命賜給苦命的人呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "受患難的人為何有光賜給他呢？ 心中愁苦的人為何有生命賜給他呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Why is light given to him who suffers, And life to the bitter of soul,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Why is light given to those in misery, and life to the bitter of soul,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Oh, why give light to those in misery, and life to those who are bitter?"
      }
    },
