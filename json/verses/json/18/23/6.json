    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂会用大能与我争辩吗？ 不会的，祂会垂听我的申诉，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他會以大能與我相爭嗎？ 必不會這樣，他必關懷我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他豈用大能與我爭辯嗎？ 必不這樣！他必理會我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Would He contend with me by the greatness of His power? No, surely He would pay attention to me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Would he vigorously oppose me? No, he would not press charges against me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Would he use his great power to argue with me? No, he would give me a fair hearing."
      }
    },
