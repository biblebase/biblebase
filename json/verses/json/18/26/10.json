      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂在海面上画出地平线， 作为光明与黑暗的交界。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在水面劃出界限， 直到光與暗的交界。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在水面的周圍劃出界限， 直到光明黑暗的交界。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has inscribed a circle on the surface of the waters At the boundary of light and darkness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He marks out the horizon on the face of the waters for a boundary between light and darkness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He created the horizon when he separated the waters; he set the boundary between day and night."
      }
    },
