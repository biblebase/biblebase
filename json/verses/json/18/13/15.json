    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂必杀我，我毫无指望， 但我仍要在祂面前申辩。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必殺我，我沒有指望了， 我必在他面前辯明我所行的；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必殺我；我雖無指望， 然而我在他面前還要辯明我所行的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Though He slay me, I will hope in Him. Nevertheless I will argue my ways before Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Though he slay me, yet will I hope in him; I will surely defend my ways to his face."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God might kill me, but I have no other hope. I am going to argue my case with him."
      }
    },
