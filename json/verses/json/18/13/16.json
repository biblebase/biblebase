    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样我才能得救， 因为不信上帝的人无法到祂面前。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這要成為我的拯救， 因為不敬虔的人不能到他面前來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這要成為我的拯救， 因為不虔誠的人不得到他面前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This also will be my salvation, For a godless man may not come before His presence."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Indeed, this will turn out for my deliverance, for no godless person would dare come before him!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But this is what will save me—I am not godless. If I were, I could not stand before him."
      }
