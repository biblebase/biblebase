    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "连上帝的圣者都无法令祂信任， 连诸天在祂的眼中都不洁净，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神不信任他的眾天使， 在他看來，諸天也不潔淨，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神不信靠他的眾聖者； 在他眼前，天也不潔淨，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, He puts no trust in His holy ones, And the heavens are not pure in His sight;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If God places no trust in his holy ones, if even the heavens are not pure in his eyes,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Look, God does not even trust the angels. Even the heavens are not absolutely pure in his sight."
      }
