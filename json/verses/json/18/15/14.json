      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人是甚麼，竟算為潔淨呢？ 婦人所生的是甚麼，竟算為義呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "What is man, that he should be pure, Or he who is born of a woman, that he should be righteous?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“What are mortals, that they could be pure, or those born of woman, that they could be righteous?"
