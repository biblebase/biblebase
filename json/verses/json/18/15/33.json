      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必像葡萄樹未熟的葡萄掉落一樣， 又如橄欖樹的花朵一開就謝一般。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必像葡萄樹的葡萄，未熟而落； 又像橄欖樹的花，一開而謝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will drop off his unripe grape like the vine, And will cast off his flower like the olive tree."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will be like a vine stripped of its unripe grapes, like an olive tree shedding its blossoms."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will be like a vine whose grapes are harvested too early, like an olive tree that loses its blossoms before the fruit can form."
      }
    },
