    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你生平可曾向晨曦发号施令， 为黎明的曙光指定岗位，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "自你有生以來，你何曾吩咐過晨光照耀， 使黎明的曙光知道自己的本位，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你自生以來，曾命定晨光， 使清晨的日光知道本位，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Have you ever in your life commanded the morning, And caused the dawn to know its place,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Have you ever given orders to the morning, or shown the dawn its place,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Have you ever commanded the morning to appear and caused the dawn to rise in the east?"
      }
    },
