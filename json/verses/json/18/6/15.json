    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的弟兄难以信赖，如同季节河， 又像变化无常的河道——"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的兄弟詭詐像乾涸的溪流， 又像流乾的溪水；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的弟兄詭詐，好像溪水， 又像溪水流乾的河道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My brothers have acted deceitfully like a wadi, Like the torrents of wadis which vanish,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But my brothers are as undependable as intermittent streams, as the streams that overflow"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My brothers, you have proved as unreliable as a seasonal brook that overflows its banks in the spring"
      }
    },
