    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "水流在干季时消失， 河床在烈日下干涸。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天氣一暖溪水就消失， 氣候一熱就乾涸。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天氣漸暖就隨時消化， 日頭炎熱便從原處乾涸。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they become waterless, they are silent, When it is hot, they vanish from their place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but that stop flowing in the dry season, and in the heat vanish from their channels."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when the hot weather arrives, the water disappears. The brook vanishes in the heat."
      }
    },
