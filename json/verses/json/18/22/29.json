      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人遭贬抑时，你说，‘使他们升高吧。’ 上帝就会拯救他们，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人謙卑的時候，你就說：‘升高吧！’ 眼中謙卑的他必拯救，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人使你降卑，你仍可說：必得高升； 謙卑的人，神必然拯救。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you are cast down, you will speak with confidence, And the humble person He will save."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When people are brought low and you say, ‘Lift them up!’ then he will save the downcast."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If people are in trouble and you say, ‘Help them,’ God will save them."
      }
