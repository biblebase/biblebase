    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你却说，‘上帝知道什么？ 祂怎能隔着幽暗施行审判？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你說：‘　神知道甚麼？ 他能透過幽暗施行審判嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你說：神知道甚麼？ 他豈能看透幽暗施行審判呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You say, ‘What does God know? Can He judge through the thick darkness?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet you say, ‘What does God know? Does he judge through such darkness?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you reply, ‘That’s why God can’t see what I am doing! How can he judge through the thick darkness?"
      }
