    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约伯为朋友们祷告后，耶和华恢复了他以前的昌盛，并且耶和华赐给他的比以前多一倍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約伯替他的朋友禱告，耶和華就恢復約伯原來的景況，還照約伯以前所有的一切加倍賜給他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約伯為他的朋友祈禱。耶和華就使約伯從苦境轉回，並且耶和華賜給他的比他從前所有的加倍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord restored the fortunes of Job when he prayed for his friends, and the Lord increased all that Job had twofold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After Job had prayed for his friends, the Lord restored his fortunes and gave him twice as much as he had before."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Job prayed for his friends, the Lord restored his fortunes. In fact, the Lord gave him twice as much as before!"
      }
    },
