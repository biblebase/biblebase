    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "古实的黄玉无法与她相比， 纯金不能与她等量齐观。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "古實的黃玉不能與它比較， 純金也不能與它較量。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "古實的紅璧璽不足與比較； 精金也不足與較量。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The topaz of Ethiopia cannot equal it, Nor can it be valued in pure gold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The topaz of Cush cannot compare with it; it cannot be bought with pure gold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Precious peridot from Ethiopia cannot be exchanged for it. It’s worth more than the purest gold."
      }
    },
