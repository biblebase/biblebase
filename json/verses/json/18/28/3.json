    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世人探寻黑暗的尽头， 搜索幽暗的深处， 要得隐藏的矿石。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人探求黑暗的盡頭， 查明在陰暗中的石礦，直到極處。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人為黑暗定界限， 查究幽暗陰翳的石頭，直到極處，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Man puts an end to darkness, And to the farthest limit he searches out The rock in gloom and deep shadow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Mortals put an end to the darkness; they search out the farthest recesses for ore in the blackest darkness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They know how to shine light in the darkness and explore the farthest regions of the earth as they search in the dark for ore."
      }
    },
