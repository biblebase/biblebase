    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂对世人说，‘敬畏主就是智慧， 远离恶便是聪明。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他對人說：‘敬畏主就是智慧， 遠離邪惡就是聰明。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對人說：敬畏主就是智慧； 遠離惡便是聰明。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And to man He said, ‘Behold, the fear of the Lord, that is wisdom; And to depart from evil is understanding.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he said to the human race, “The fear of the Lord—that is wisdom, and to shun evil is understanding.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And this is what he says to all humanity: ‘The fear of the Lord is true wisdom; to forsake evil is real understanding.’”"
      }
    },
