    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "黄金和水晶无法与她相比， 纯金的器皿不能与她兑换；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "黃金和玻璃不能與它比較， 純金的器皿不能與它交換。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "黃金和玻璃不足與比較； 精金的器皿不足與兌換。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Gold or glass cannot equal it, Nor can it be exchanged for articles of fine gold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Neither gold nor crystal can compare with it, nor can it be had for jewels of gold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wisdom is more valuable than gold and crystal. It cannot be purchased with jewels mounted in fine gold."
      }
