        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "People assault the flinty rock with their hands and lay bare the roots of the mountains."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People know how to tear apart flinty rocks and overturn the roots of mountains."
      }
