    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大地落入恶人手中， 蒙敝审判官眼睛的不是祂是谁？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "全世界交在惡人的手中， 他蒙蔽世上審判官的臉， 如果不是他，那麼是誰呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "世界交在惡人手中； 蒙蔽世界審判官的臉， 若不是他，是誰呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The earth is given into the hand of the wicked; He covers the faces of its judges. If it is not He, then who is it?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When a land falls into the hands of the wicked, he blindfolds its judges. If it is not he, then who is it?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The whole earth is in the hands of the wicked, and God blinds the eyes of the judges. If he’s not the one who does it, who is?"
      }
