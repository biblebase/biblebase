        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我的年日比信差还快， 匆匆而过，不见幸福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的日子過得比信差還快， 飛快逝去，不見福樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的日子比跑信的更快， 急速過去，不見福樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Now my days are swifter than a runner; They flee away, they see no good."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“My days are swifter than a runner; they fly away without a glimpse of joy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“My life passes more swiftly than a runner. It flees away without a glimpse of happiness."
      }
