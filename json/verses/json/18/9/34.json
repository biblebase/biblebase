    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若能拿开祂责打我的刑杖， 使我不再受祂的惊吓，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願他使他的刑杖離開我， 願他可畏的威嚴不驚嚇我，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願他把杖離開我， 不使驚惶威嚇我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let Him remove His rod from me, And let not dread of Him terrify me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "someone to remove God’s rod from me, so that his terror would frighten me no more."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The mediator could make God stop beating me, and I would no longer live in terror of his punishment."
      }
