    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“不错，我知道你所言不虚， 但人怎能在上帝面前算为义人？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我實在知道是這樣， 但是，人在　神面前怎能算為公義呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我真知道是這樣； 但人在神面前怎能成為義呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“In truth I know that this is so; But how can a man be in the right before God?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Indeed, I know that this is true. But how can mere mortals prove their innocence before God?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yes, I know all this is true in principle. But how can a person be declared innocent in God’s sight?"
      }
    },
