    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们因愚昧而无法陈诉， 请指教我们如何与祂对话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們因愚昧無知的緣故，不能陳明我們的案件， 請你告訴我們應該對他說甚麼話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們愚昧不能陳說； 請你指教我們該對他說甚麼話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Teach us what we shall say to Him; We cannot arrange our case because of darkness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Tell us what we should say to him; we cannot draw up our case because of our darkness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“So teach the rest of us what to say to God. We are too ignorant to make our own arguments."
      }
