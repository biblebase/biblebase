    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你为何让我出母腹？ 我不如当时就死去， 无人会看见我——"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你為甚麼使我出母胎呢？ 我不如死去，無人得見我，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你為何使我出母胎呢？ 不如我當時氣絕，無人得見我；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Why then have You brought me out of the womb? Would that I had died and no eye had seen me!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Why then did you bring me out of the womb? I wish I had died before any eye saw me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“‘Why, then, did you deliver me from my mother’s womb? Why didn’t you let me die at birth?"
      }
    },
