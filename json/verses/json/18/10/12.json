    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你赐我生命给我慈爱， 你的眷顾使我保全性命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你賜我生命，又向我施慈愛， 你的眷顧保守我的心靈。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你將生命和慈愛賜給我； 你也眷顧保全我的心靈。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have granted me life and lovingkindness; And Your care has preserved my spirit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You gave me life and showed me kindness, and in your providence watched over my spirit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You gave me life and showed me your unfailing love. My life was preserved by your care."
      }
    },
