    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我日夜不停地为你祷告，对我效法祖先用清洁的良心事奉的上帝充满了感恩。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我感謝　神，就是我像我祖先一樣，以清潔的良心所事奉的　神，在禱告中晝夜不斷地記念你；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我感謝神，就是我接續祖先用清潔的良心所事奉的神。祈禱的時候，不住的想念你，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I thank God, whom I serve with a clear conscience the way my forefathers did, as I constantly remember you in my prayers night and day,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I thank God, whom I serve, as my ancestors did, with a clear conscience, as night and day I constantly remember you in my prayers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Timothy, I thank God for you—the God I serve with a clear conscience, just as my ancestors did. Night and day I constantly remember you in my prayers."
      }
    },
