    "sermons": [
      {
        "id": "home_sermons_sermon-2009-01-11",
        "title": "福音的戰士",
        "date": "2009-01-11",
        "preacher": "鄭昌國牧師",
        "verses": "提摩太後書 1:8-12"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，你不要羞于为我们的主做见证，也不要以我这为主被囚的人为耻，要靠着上帝的能力和我一同为福音受苦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，不要以給我們的主作見證當作羞恥，也不要以我這為主被囚的當作羞恥，卻要為了福音的緣故，靠著　神的大能，與我同受磨難。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你不要以給我們的主作見證為恥，也不要以我這為主被囚的為恥；總要按神的能力，與我為福音同受苦難。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore do not be ashamed of the testimony of our Lord or of me His prisoner, but join with me in suffering for the gospel according to the power of God,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So do not be ashamed of the testimony about our Lord or of me his prisoner. Rather, join with me in suffering for the gospel, by the power of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So never be ashamed to tell others about our Lord. And don’t be ashamed of me, either, even though I’m in prison for him. With the strength God gives you, be ready to suffer with me for the sake of the Good News."
      }
    },
