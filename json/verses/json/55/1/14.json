    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要靠着住在我们里面的圣灵牢牢守住交托给你的美善之道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又要靠著那住在我們裡面的聖靈，保守所交託你的善道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從前所交託你的善道，你要靠着那住在我們裏面的聖靈牢牢地守着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Guard, through the Holy Spirit who dwells in us, the treasure which has been entrusted to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Guard the good deposit that was entrusted to you—guard it with the help of the Holy Spirit who lives in us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Through the power of the Holy Spirit who lives within us, carefully guard the precious truth that has been entrusted to you."
      }
    },
