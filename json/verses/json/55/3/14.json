    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你要把自己所学的、所笃信的记在心里，因为你知道是跟谁学的，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你應當遵守所學和確信的；你知道是跟誰學的，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你所學習的，所確信的，要存在心裏；因為你知道是跟誰學的，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You, however, continue in the things you have learned and become convinced of, knowing from whom you have learned them,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But as for you, continue in what you have learned and have become convinced of, because you know those from whom you learned it,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you must remain faithful to the things you have been taught. You know they are true, for you know you can trust those who taught you."
      }
    },
