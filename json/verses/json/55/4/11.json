    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只剩下路加一人在我身边。你要把马可一起带来，因为他在传道的工作上对我很有帮助。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只有路加在我這裡。你找到了馬可，就把他帶來，因為他在聖工上對我有益。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "獨有路加在我這裏。你來的時候，要把馬可帶來，因為他在傳道的事上於我有益處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Only Luke is with me. Pick up Mark and bring him with you, for he is useful to me for service."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Only Luke is with me. Get Mark and bring him with you, because he is helpful to me in my ministry."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Only Luke is with me. Bring Mark with you when you come, for he will be helpful to me in my ministry."
      }
    },
