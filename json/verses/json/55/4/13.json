      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你来的时候，要把我在特罗亚时留在加布的外衣带来，也要带来那些书卷，特别是那些羊皮卷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我在特羅亞留在加布那裡的外衣，你來的時候要把它帶來，還有那些書卷，特別是那些羊皮卷，也要帶來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我在特羅亞留於加布的那件外衣，你來的時候可以帶來，那些書也要帶來，更要緊的是那些皮卷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you come bring the cloak which I left at Troas with Carpus, and the books, especially the parchments."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you come, bring the cloak that I left with Carpus at Troas, and my scrolls, especially the parchments."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When you come, be sure to bring the coat I left with Carpus at Troas. Also bring my books, and especially my papers."
      }
