    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当兵的若让其他事务缠身，就不能取悦征召他的上司。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當兵的人不讓世務纏身，為要使那招兵的人歡喜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡在軍中當兵的，不將世務纏身，好叫那招他當兵的人喜悅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No soldier in active service entangles himself in the affairs of everyday life, so that he may please the one who enlisted him as a soldier."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one serving as a soldier gets entangled in civilian affairs, but rather tries to please his commanding officer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Soldiers don’t get tied up in the affairs of civilian life, for then they cannot please the officer who enlisted them."
      }
    },
