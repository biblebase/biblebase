      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要切记我所传的福音：耶稣基督是大卫的后裔，祂从死里复活了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要記得那從死人中復活的耶穌基督，他是大衛的後裔所生的，這就是我所傳的福音。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要記念耶穌基督乃是大衛的後裔，他從死裏復活，正合乎我所傳的福音。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Remember Jesus Christ, risen from the dead, descendant of David, according to my gospel,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember Jesus Christ, raised from the dead, descended from David. This is my gospel,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Always remember that Jesus Christ, a descendant of King David, was raised from the dead. This is the Good News I preach."
      }
