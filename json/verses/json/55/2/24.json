    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝的奴仆不可与人争执，总要温和待人，善于教导，心存忍耐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但主的僕人卻不可爭論，總要待人溫和，善於教導，存心忍耐，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "然而主的僕人不可爭競，只要溫溫和和地待眾人，善於教導，存心忍耐，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord’s bond-servant must not be quarrelsome, but be kind to all, able to teach, patient when wronged,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Lord’s servant must not be quarrelsome but must be kind to everyone, able to teach, not resentful."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A servant of the Lord must not quarrel but must be kind to everyone, be able to teach, and be patient with difficult people."
      }
    },
