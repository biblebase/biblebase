    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我为这福音受苦，甚至像犯人一样被囚禁，但上帝的道是囚禁不住的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我為了這福音受了磨難，甚至像犯人一樣被捆綁起來；可是，　神的話卻不被捆綁。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我為這福音受苦難，甚至被捆綁，像犯人一樣。然而神的道卻不被捆綁。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "for which I suffer hardship even to imprisonment as a criminal; but the word of God is not imprisoned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for which I am suffering even to the point of being chained like a criminal. But God’s word is not chained."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And because I preach this Good News, I am suffering and have been chained like a criminal. But the word of God cannot be chained."
      }
    },
