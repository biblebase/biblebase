    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚扪人拿辖率军上来围困基列·雅比，城内的居民对拿辖说：“你与我们立约吧！我们愿意服从你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞捫人拿轄上來，安營攻擊基列．雅比。所有的雅比人都對拿轄說：“你與我們立約，我們就服事你。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞捫人的王拿轄上來，對着基列‧雅比安營。雅比眾人對拿轄說：「你與我們立約，我們就服事你。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Nahash the Ammonite came up and besieged Jabesh-gilead; and all the men of Jabesh said to Nahash, “Make a covenant with us and we will serve you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nahash the Ammonite went up and besieged Jabesh Gilead. And all the men of Jabesh said to him, “Make a treaty with us, and we will be subject to you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "About a month later, King Nahash of Ammon led his army against the Israelite town of Jabesh-gilead. But all the citizens of Jabesh asked for peace. “Make a treaty with us, and we will be your servants,” they pleaded."
      }
    },
