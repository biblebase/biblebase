    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他就吩咐撒母耳说：“你回去睡吧，若再呼唤你，你就说，‘耶和华啊，请吩咐，仆人恭听。’”于是，撒母耳就回去睡下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此以利對撒母耳說：“去睡吧！如果他呼喚你，你就說：‘耶和華啊！請說，僕人敬聽。’”於是撒母耳回去，仍然睡在自己的地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此以利對撒母耳說：「你仍去睡吧；若再呼喚你，你就說：『耶和華啊，請說，僕人敬聽！』」撒母耳就去，仍睡在原處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Eli said to Samuel, “Go lie down, and it shall be if He calls you, that you shall say, ‘Speak, Lord, for Your servant is listening.’ ” So Samuel went and lay down in his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Eli told Samuel, “Go and lie down, and if he calls you, say, ‘Speak, Lord, for your servant is listening.’ ” So Samuel went and lay down in his place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So he said to Samuel, “Go and lie down again, and if someone calls again, say, ‘Speak, Lord, your servant is listening.’” So Samuel went back to bed."
      }
    },
