    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华继续在示罗显现，用自己的话启示撒母耳。撒母耳把耶和华的话传遍了以色列。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華又在示羅顯現，因為耶和華在示羅藉著他的話向撒母耳啟示自己。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華又在示羅顯現；因為耶和華將自己的話默示撒母耳，撒母耳就把這話傳遍以色列地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the Lord appeared again at Shiloh, because the Lord revealed Himself to Samuel at Shiloh by the word of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord continued to appear at Shiloh, and there he revealed himself to Samuel through his word."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord continued to appear at Shiloh and gave messages to Samuel there at the Tabernacle."
      }
    },
