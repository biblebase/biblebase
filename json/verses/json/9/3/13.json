    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我对以利说过我要永远惩罚他家，因为他知道自己的儿子亵渎我，却没有制止他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我曾告訴他：‘我必按著他所知道的罪孽永遠處罰他的家。這罪就是他的兒子自招咒詛，他也不責備他們。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我曾告訴他必永遠降罰與他的家，因他知道兒子作孽，自招咒詛，卻不禁止他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I have told him that I am about to judge his house forever for the iniquity which he knew, because his sons brought a curse on themselves and he did not rebuke them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I told him that I would judge his family forever because of the sin he knew about; his sons blasphemed God, and he failed to restrain them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have warned him that judgment is coming upon his family forever, because his sons are blaspheming God and he hasn’t disciplined them."
      }
    },
