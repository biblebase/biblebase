    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫再求问耶和华，耶和华答道：“到基伊拉去吧，我一定把非利士人交在你手里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是大衛再求問耶和華。耶和華回答他說：“你只管起來，下到基伊拉去，因為我必把非利士人交在你的手裡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛又求問耶和華。耶和華回答說：「你起身下基伊拉去，我必將非利士人交在你手裏。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David inquired of the Lord once more. And the Lord answered him and said, “Arise, go down to Keilah, for I will give the Philistines into your hand.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Once again David inquired of the Lord, and the Lord answered him, “Go down to Keilah, for I am going to give the Philistines into your hand.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So David asked the Lord again, and again the Lord replied, “Go down to Keilah, for I will help you conquer the Philistines.”"
      }
    },
