    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫又问耶和华：“基伊拉的人会不会把我和我的部下交给扫罗呢？”耶和华说：“他们一定会。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛又說：“基伊拉人會把我和跟隨我的人交在掃羅手裡嗎？”耶和華說：“他們必把你們交出來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛又說：「基伊拉人將我和跟隨我的人交在掃羅手裏不交？」耶和華說：「必交出來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David said, “Will the men of Keilah surrender me and my men into the hand of Saul?” And the Lord said, “They will surrender you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Again David asked, “Will the citizens of Keilah surrender me and my men to Saul?” And the Lord said, “They will.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Again David asked, “Will the leaders of Keilah betray me and my men to Saul?” And the Lord replied, “Yes, they will betray you.”"
      }
    },
