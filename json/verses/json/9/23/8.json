    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗召集全军准备攻打基伊拉，围困大卫和他的部下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是掃羅召集眾人去爭戰，要下到基伊拉去，圍困大衛和跟隨他的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是掃羅招聚眾民，要下去攻打基伊拉城，圍困大衛和跟隨他的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Saul summoned all the people for war, to go down to Keilah to besiege David and his men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And Saul called up all his forces for battle, to go down to Keilah to besiege David and his men."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Saul mobilized his entire army to march to Keilah and besiege David and his men."
      }
    },
