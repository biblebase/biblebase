    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到了婚礼的日子，扫罗却把米拉嫁给了米何拉人亚得列。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了掃羅的女兒米拉該嫁給大衛的時候，掃羅卻把她給了米何拉人亞得列作妻子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅的女兒米拉到了當給大衛的時候，掃羅卻給了米何拉人亞得列為妻。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So it came about at the time when Merab, Saul’s daughter, should have been given to David, that she was given to Adriel the Meholathite for a wife."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So when the time came for Merab, Saul’s daughter, to be given to David, she was given in marriage to Adriel of Meholah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So when the time came for Saul to give his daughter Merab in marriage to David, he gave her instead to Adriel, a man from Meholah."
      }
    },
