    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫说：“我算什么？我的家族在以色列那么卑微，我怎么配做王的女婿呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛回答掃羅：“我是誰？我是甚麼身分？我父家在以色列人中算得甚麼？我怎麼作王的女婿呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛對掃羅說：「我是誰，我是甚麼出身，我父家在以色列中是何等的家，豈敢作王的女婿呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But David said to Saul, “Who am I, and what is my life or my father’s family in Israel, that I should be the king’s son-in-law?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But David said to Saul, “Who am I, and what is my family or my clan in Israel, that I should become the king’s son-in-law?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Who am I, and what is my family in Israel that I should be the king’s son-in-law?” David exclaimed. “My father’s family is nothing!”"
      }
    },
