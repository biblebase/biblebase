    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就割下了扫罗的头颅，剥下他的盔甲，并派人走遍非利士全境，在他们偶像的庙里及民众中通告消息。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就砍下他的頭，解下他的兵器，又派人到非利士地的各處，去向他們的偶像的廟和人民報信。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就割下他的首級，剝了他的軍裝，打發人到   非利士地的四境，報信與他們廟裏的偶像和眾民；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They cut off his head and stripped off his weapons, and sent them throughout the land of the Philistines, to carry the good news to the house of their idols and to the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They cut off his head and stripped off his armor, and they sent messengers throughout the land of the Philistines to proclaim the news in the temple of their idols and among their people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So they cut off Saul’s head and stripped off his armor. Then they proclaimed the good news of Saul’s death in their pagan temple and to the people throughout the land of Philistia."
      }
    },
