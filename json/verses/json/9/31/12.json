    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们所有的勇士就连夜赶到伯·珊，从墙上取下扫罗和他儿子们的尸体，带到雅比火化，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有的勇士就動身，走了一整夜，把掃羅的屍體和他三個兒子的屍體都從伯．珊的城牆上取下來，帶回雅比，在那裡把他們燒了，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們中間所有的勇士就起身，走了一夜，將掃羅和他兒子的屍身從伯‧珊城牆上取下來，送到雅比那裏，用火燒了；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "all the valiant men rose and walked all night, and took the body of Saul and the bodies of his sons from the wall of Beth-shan, and they came to Jabesh and burned them there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "all their valiant men marched through the night to Beth Shan. They took down the bodies of Saul and his sons from the wall of Beth Shan and went to Jabesh, where they burned them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "all their mighty warriors traveled through the night to Beth-shan and took the bodies of Saul and his sons down from the wall. They brought them to Jabesh, where they burned the bodies."
      }
    },
