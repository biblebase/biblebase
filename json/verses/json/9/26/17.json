    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗听出是大卫的声音，就说：“我儿大卫！是你吗？”大卫答道：“我主我王啊，是我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅認出是大衛的聲音，就說：“我兒大衛啊！這是你的聲音嗎？”大衛說：“我主我王啊！是我的聲音。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅聽出是大衛的聲音，就說：「我兒大衛，這是你的聲音嗎？」大衛說：「主－我的王啊，是我的聲音」；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Saul recognized David’s voice and said, “Is this your voice, my son David?” And David said, “It is my voice, my lord the king.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Saul recognized David’s voice and said, “Is that your voice, David my son?” David replied, “Yes it is, my lord the king.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Saul recognized David’s voice and called out, “Is that you, my son David?” And David replied, “Yes, my lord the king."
      }
