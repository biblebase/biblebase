    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗对大卫说：“我儿大卫，愿你蒙福。你一定能建功立业，战胜仇敌。”于是，大卫起身离开，扫罗也回家去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅對大衛說：“我兒大衛啊，願你得福，你必作成你所要作的，也必得勝。”於是大衛離去了，掃羅也回自己的地方去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅對大衛說：「我兒大衛，願你得福！你必做大事，也必得勝。」於是大衛起行，掃羅回他的本處去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Saul said to David, “Blessed are you, my son David; you will both accomplish much and surely prevail.” So David went on his way, and Saul returned to his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Saul said to David, “May you be blessed, David my son; you will do great things and surely triumph.” So David went on his way, and Saul returned home."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And Saul said to David, “Blessings on you, my son David. You will do many heroic deeds, and you will surely succeed.” Then David went away, and Saul returned home."
      }
    },
