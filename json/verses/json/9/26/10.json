      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我凭永活的耶和华起誓，耶和华必亲手击杀他。他要么寿终而死，要么命丧沙场。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛又說：“我指著永活的耶和華起誓，他或被耶和華擊打，或死期到了，或下到戰場陣亡了，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛又說：「我指着永生的耶和華起誓，他或被耶和華擊打，或是死期到了，或是出戰陣亡；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David also said, “As the Lord lives, surely the Lord will strike him, or his day will come that he dies, or he will go down into battle and perish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As surely as the Lord lives,” he said, “the Lord himself will strike him, or his time will come and he will die, or he will go into battle and perish."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Surely the Lord will strike Saul down someday, or he will die of old age or in battle."
      }
