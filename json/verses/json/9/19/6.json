      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗听了约拿单的话，就凭永活的耶和华起誓说：“我不杀大卫。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅聽了約拿單的話，就起誓，說：“我指著永活的耶和華起誓，大衛必不會被殺死。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅聽了約拿單的話，就指着永生的耶和華起誓說：「我必不殺他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Saul listened to the voice of Jonathan, and Saul vowed, “As the Lord lives, he shall not be put to death.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Saul listened to Jonathan and took this oath: “As surely as the Lord lives, David will not be put to death.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Saul listened to Jonathan and vowed, “As surely as the Lord lives, David will not be killed.”"
      }
    },
