    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约拿单叫来大卫，把经过告诉他，带他去见扫罗，大卫又像以前一样伺候扫罗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約拿單叫大衛來，把這一切話都告訴他；約拿單把大衛帶到掃羅那裡，他就像以前一樣，侍候在掃羅面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約拿單叫大衛來，把這一切事告訴他，帶他去見掃羅。他就仍然侍立在掃羅面前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jonathan called David, and Jonathan told him all these words. And Jonathan brought David to Saul, and he was in his presence as formerly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Jonathan called David and told him the whole conversation. He brought him to Saul, and David was with Saul as before."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Afterward Jonathan called David and told him what had happened. Then he brought David to Saul, and David served in the court as before."
      }
    },
