      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從耶和華那裡來的惡鬼又臨到掃羅身上；那時掃羅坐在屋裡，手裡拿著槍。大衛正在彈琴，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從耶和華那裏來的惡魔又降在掃羅身上（掃羅手裏拿槍坐在屋裏），大衛就用手彈琴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now there was an evil spirit from the Lord on Saul as he was sitting in his house with his spear in his hand, and David was playing the harp with his hand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But an evil spirit from the Lord came on Saul as he was sitting in his house with his spear in his hand. While David was playing the lyre,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But one day when Saul was sitting at home, with spear in hand, the tormenting spirit from the Lord suddenly came upon him again. As David played his harp,"
      }
