    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "每年来到耶和华的殿时，毗尼娜总是惹她伤心哭泣，以致她吃不下饭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "年年都是這樣；每逢她上耶和華殿的時候，毘尼拿總是刺激哈拿，使她哭泣，吃不下飯。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "每年上到耶和華殿的時候，以利加拿都以雙分給哈拿；毗尼拿仍是激動她，以致她哭泣不吃飯。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It happened year after year, as often as she went up to the house of the Lord, she would provoke her; so she wept and would not eat."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This went on year after year. Whenever Hannah went up to the house of the Lord, her rival provoked her till she wept and would not eat."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Year after year it was the same—Peninnah would taunt Hannah as they went to the Tabernacle. Each time, Hannah would be reduced to tears and would not even eat."
      }
    },
