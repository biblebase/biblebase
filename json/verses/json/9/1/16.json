    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "请不要以为我是堕落的女人，我心中非常悲伤痛苦，所以一直在祷告。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要把你的使女看作不檢點的女子，我因為極度愁苦和激動，才一直傾訴到現在。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要將婢女看作不正經的女子。我因被人激動，愁苦太多，所以祈求到如今。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not consider your maidservant as a worthless woman, for I have spoken until now out of my great concern and provocation.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not take your servant for a wicked woman; I have been praying here out of my great anguish and grief.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t think I am a wicked woman! For I have been praying out of great anguish and sorrow.”"
      }
    },
