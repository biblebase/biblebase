    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚吉对臣仆说：“你们看，这是个疯子，为什么把他带到我这里来？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞吉就對他的臣僕說：“你們看，這個人瘋了，你們為甚麼把他帶到我這裡來呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞吉對臣僕說：「你們看，這人是瘋子。為甚麼帶他到我這裏來呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Achish said to his servants, “Behold, you see the man behaving as a madman. Why do you bring him to me?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Achish said to his servants, “Look at the man! He is insane! Why bring him to me?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Finally, King Achish said to his men, “Must you bring me a madman?"
      }
    },
