    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你父亲问起我，你就说我恳求你允许我回到故乡伯利恒，因为我全家正在那里献年祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你父親不見我在座，你就說：‘大衛懇求我讓他趕回他的本城伯利恆去，因為他全家在那裡有獻年祭的事。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你父親若見我不在席上，你就說：『大衛切求我許他回本城伯利恆去，因為他全家在那裏獻年祭。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If your father misses me at all, then say, ‘David earnestly asked leave of me to run to Bethlehem his city, because it is the yearly sacrifice there for the whole family.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If your father misses me at all, tell him, ‘David earnestly asked my permission to hurry to Bethlehem, his hometown, because an annual sacrifice is being made there for his whole clan.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If your father asks where I am, tell him I asked permission to go home to Bethlehem for an annual family sacrifice."
      }
    },
