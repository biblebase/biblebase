    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫从拉玛的拿约逃出来去见约拿单，问他：“我做了什么？犯了什么罪？我什么地方对不起你父亲，他竟追杀我？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛從拉瑪的拿約逃走，來見約拿單，說：“我作了甚麼？我有甚麼罪孽？我在你父親面前犯了甚麼罪，他竟尋索我的性命呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛從拉瑪的拿約逃跑，來到約拿單那裏，對他說：「我做了甚麼？有甚麼罪孽呢？在你父親面前犯了甚麼罪，他竟尋索我的性命呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David fled from Naioth in Ramah, and came and said to Jonathan, “What have I done? What is my iniquity? And what is my sin before your father, that he is seeking my life?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then David fled from Naioth at Ramah and went to Jonathan and asked, “What have I done? What is my crime? How have I wronged your father, that he is trying to kill me?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "David now fled from Naioth in Ramah and found Jonathan. “What have I done?” he exclaimed. “What is my crime? How have I offended your father that he is so determined to kill me?”"
      }
    },
