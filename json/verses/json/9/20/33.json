    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗气得把矛掷向约拿单，想要杀他。约拿单知道父亲决意要杀大卫。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅向著約拿單把槍擲過去，要刺殺他。約拿單就知道，他父親已經決意要殺死大衛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅向約拿單掄槍要刺他，約拿單就知道他父親決意要殺大衛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Saul hurled his spear at him to strike him down; so Jonathan knew that his father had decided to put David to death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Saul hurled his spear at him to kill him. Then Jonathan knew that his father intended to kill David."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Saul hurled his spear at Jonathan, intending to kill him. So at last Jonathan realized that his father was really determined to kill David."
      }
    },
