    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只要耶西的儿子活在世上，你和你的王位就不得安稳。你派人把他抓到我这里，他非死不可。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為只要耶西的兒子還活在這世上，你和你的國就不能穩固。現在，你要派人去把他抓到我這裡來，因為他是該死的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶西的兒子若在世間活着，你和你的國位必站立不住。現在你要打發人去，將他捉拿交給我；他是該死的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For as long as the son of Jesse lives on the earth, neither you nor your kingdom will be established. Therefore now, send and bring him to me, for he must surely die.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As long as the son of Jesse lives on this earth, neither you nor your kingdom will be established. Now send someone to bring him to me, for he must die!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As long as that son of Jesse is alive, you’ll never be king. Now go and get him so I can kill him!”"
      }
    },
