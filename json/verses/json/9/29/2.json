    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "非利士首领统率以百人和千人为单位的士兵向前推进，大卫和他的部下跟着亚吉走在队伍的后面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "非利士人的首領各率領百人或千人前進；大衛和跟隨他的人也跟在亞吉的後面前進。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "非利士人的首領各率軍隊，或百或千，挨次前進；大衛和跟隨他的人同着亞吉跟在後邊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the lords of the Philistines were proceeding on by hundreds and by thousands, and David and his men were proceeding on in the rear with Achish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As the Philistine rulers marched with their units of hundreds and thousands, David and his men were marching at the rear with Achish."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As the Philistine rulers were leading out their troops in groups of hundreds and thousands, David and his men marched at the rear with King Achish."
      }
    },
