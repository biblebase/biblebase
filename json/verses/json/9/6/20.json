    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "伯·示麦人说：“谁能站立在耶和华——这位圣洁的上帝面前呢？该把约柜送到谁那里呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "伯．示麥人說：“在耶和華這位聖潔的　神面前，誰能站立得住呢？他可以從我們這裡上到哪裡去呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "伯‧示麥人說：「誰能在耶和華這聖潔的神面前侍立呢？這約櫃可以從我們這裏送到誰那裏去呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The men of Beth-shemesh said, “Who is able to stand before the Lord, this holy God? And to whom shall He go up from us?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the people of Beth Shemesh asked, “Who can stand in the presence of the Lord, this holy God? To whom will the ark go up from here?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Who is able to stand in the presence of the Lord, this holy God?” they cried out. “Where can we send the Ark from here?”"
      }
    },
