    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又说：“你过来，我要把你的肉丢给天上的飞禽和地上的走兽。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那非利士人又對大衛說：“你到我這裡來吧！我要把你的肉給空中的飛鳥和田野的走獸吃。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "非利士人又對大衛說：「來吧！我將你的肉給空中的飛鳥、田野的走獸吃。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Philistine also said to David, “Come to me, and I will give your flesh to the birds of the sky and the beasts of the field.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Come here,” he said, “and I’ll give your flesh to the birds and the wild animals!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Come over here, and I’ll give your flesh to the birds and wild animals!” Goliath yelled."
      }
    },
