    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫问他：“你是谁的仆人？你从哪里来？”他答道：“我是埃及人，是亚玛力人的奴仆。三天前，主人因为我患病把我撇弃了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛問他：“你是屬誰的？你從哪裡來？”他回答：“我是個埃及的青年人，是亞瑪力人的奴僕。因為我三天前病了，我的主人就把我丟棄了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛問他說：「你是屬誰的？你是哪裏的人？」他回答說：「我是埃及的少年人，是亞瑪力人的奴僕；因我三日前患病，我主人就把我撇棄了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David said to him, “To whom do you belong? And where are you from?” And he said, “I am a young man of Egypt, a servant of an Amalekite; and my master left me behind when I fell sick three days ago."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "David asked him, “Who do you belong to? Where do you come from?” He said, “I am an Egyptian, the slave of an Amalekite. My master abandoned me when I became ill three days ago."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“To whom do you belong, and where do you come from?” David asked him. “I am an Egyptian—the slave of an Amalekite,” he replied. “My master abandoned me three days ago because I was sick."
      }
