        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贫穷富足在于祂， 卑微高贵也在于祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華使人貧窮，也使人富足； 使人降卑，也使人升高。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他使人貧窮，也使人富足， 使人卑微，也使人高貴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord makes poor and rich; He brings low, He also exalts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord sends poverty and wealth; he humbles and he exalts."
      },
