    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华圣洁无比， 独一无二， 没有磐石像我们的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有人像耶和華那麼聖潔， 因為除你以外沒有別的神， 也沒有磐石像我們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只有耶和華為聖； 除他以外沒有可比的， 也沒有磐石像我們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is no one holy like the Lord, Indeed, there is no one besides You, Nor is there any rock like our God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“There is no one holy like the Lord; there is no one besides you; there is no Rock like our God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No one is holy like the Lord! There is no one besides you; there is no Rock like our God."
      }
