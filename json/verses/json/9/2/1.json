    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "哈娜祷告说： “我的心因耶和华而欢喜， 我的力量因耶和华而倍增， 我的口向敌人夸耀， 我因耶和华的拯救而快乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "哈拿禱告說： “我的心因耶和華喜樂， 我的角因耶和華高舉， 我的口向仇敵大大誇耀， 因為我以你的救恩為樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "哈拿禱告說： 我的心因耶和華快樂； 我的角因耶和華高舉。 我的口向仇敵張開； 我因耶和華的救恩歡欣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Hannah prayed and said, “My heart exults in the Lord; My horn is exalted in the Lord, My mouth speaks boldly against my enemies, Because I rejoice in Your salvation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Hannah prayed and said: “My heart rejoices in the Lord; in the Lord my horn is lifted high. My mouth boasts over my enemies, for I delight in your deliverance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Hannah prayed: “My heart rejoices in the Lord! The Lord has made me strong. Now I have an answer for my enemies; I rejoice because you rescued me."
      }
    },
