    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂保护祂的圣民， 使恶人在黑暗中灭亡， 因为得胜不是靠人的勇力。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必保護虔誠人的腳步， 卻使惡人在黑暗中滅亡， 因為人得勝不是靠著力量。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必保護聖民的腳步， 使惡人在黑暗中寂然不動； 人都不能靠力量得勝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He keeps the feet of His godly ones, But the wicked ones are silenced in darkness; For not by might shall a man prevail."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will guard the feet of his faithful servants, but the wicked will be silenced in the place of darkness. “It is not by strength that one prevails;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“He will protect his faithful ones, but the wicked will disappear in darkness. No one will succeed by strength alone."
      }
    },
