      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摆在扫罗和他的随从面前，他们吃完后，当天晚上就离开了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "送到掃羅和他臣僕的面前；他們吃了以後，當夜就起身離去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "擺在掃羅和他僕人面前。他們吃完，當夜就起身走了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She brought it before Saul and his servants, and they ate. Then they arose and went away that night."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then she set it before Saul and his men, and they ate. That same night they got up and left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She brought the meal to Saul and his advisers, and they ate it. Then they went out into the night."
      }
    },
