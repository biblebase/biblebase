    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撒母耳一直到死没有再见扫罗，但他为扫罗哀伤。耶和华后悔立扫罗做以色列王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒母耳直到死的日子，再沒有看見掃羅。撒母耳為掃羅悲傷，因為耶和華後悔立了掃羅作以色列的王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "撒母耳直到死的日子，再沒有見掃羅；但撒母耳為掃羅悲傷，是因耶和華後悔立他為以色列的王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Samuel did not see Saul again until the day of his death; for Samuel grieved over Saul. And the Lord regretted that He had made Saul king over Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Until the day Samuel died, he did not go to see Saul again, though Samuel mourned for him. And the Lord regretted that he had made Saul king over Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Samuel never went to meet with Saul again, but he mourned constantly for him. And the Lord was sorry he had ever made Saul king of Israel."
      }
    },
