    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗说：“我犯了罪，但求你在以色列的长老和百姓面前尊重我，求你跟我回去，我好敬拜你的上帝耶和华。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅說：“我犯了罪了，但是求你在我人民的長老面前和以色列人面前給我面子，與我一同回去，我好敬拜耶和華你的　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅說：「我有罪了，雖然如此，求你在我百姓的長老和以色列人面前擡舉我，同我回去，我好敬拜耶和華－你的神。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he said, “I have sinned; but please honor me now before the elders of my people and before Israel, and go back with me, that I may worship the Lord your God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Saul replied, “I have sinned. But please honor me before the elders of my people and before Israel; come back with me, so that I may worship the Lord your God.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Saul pleaded again, “I know I have sinned. But please, at least honor me before the elders of my people and before Israel by coming back with me so that I may worship the Lord your God.”"
      }
    },
