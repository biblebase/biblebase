    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要单单敬畏耶和华，全心全意地事奉他，想想他为你们做了何等伟大的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們只要敬畏耶和華，誠實全心事奉他，因為你們已經看過他為你們所作的是何等大的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只要你們敬畏耶和華，誠誠實實地盡心事奉他，想念他向你們所行的事何等大。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Only fear the Lord and serve Him in truth with all your heart; for consider what great things He has done for you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But be sure to fear the Lord and serve him faithfully with all your heart; consider what great things he has done for you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But be sure to fear the Lord and faithfully serve him. Think of all the wonderful things he has done for you."
      }
    },
