    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们答道：“你没有欺骗、压迫过我们，也没有拿过任何人的东西。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們回答：“你沒有欺負過我們，沒有虐待過我們，也沒有從誰手裡取過甚麼東西。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人說：「你未曾欺負我們，虐待我們，也未曾從誰手裏受過甚麼。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said, “You have not defrauded us or oppressed us or taken anything from any man’s hand.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You have not cheated or oppressed us,” they replied. “You have not taken anything from anyone’s hand.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“No,” they replied, “you have never cheated or oppressed us, and you have never taken even a single bribe.”"
      }
    },
