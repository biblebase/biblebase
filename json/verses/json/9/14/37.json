    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，扫罗求问上帝，说：“耶和华啊，我们可以去追杀非利士人吗？你会把他们交在以色列人手里吗？”但耶和华那日没有回答他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅求問　神：“我可以下去追趕非利士人嗎？你把他們交在以色列人的手裡嗎？”那天　神沒有回答他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅求問神說：「我下去追趕非利士人可以不可以？你將他們交在以色列人手裏不交？」這日神沒有回答他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Saul inquired of God, “Shall I go down after the Philistines? Will You give them into the hand of Israel?” But He did not answer him on that day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Saul asked God, “Shall I go down and pursue the Philistines? Will you give them into Israel’s hand?” But God did not answer him that day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Saul asked God, “Should we go after the Philistines? Will you help us defeat them?” But God made no reply that day."
      }
    },
