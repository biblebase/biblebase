    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他的妻子名叫亚希暖，是亚希玛斯的女儿。扫罗军队的元帅叫押尼珥，是他叔叔尼珥的儿子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅的妻子名叫亞希暖，是亞希瑪斯的女兒。掃羅軍隊的元帥名叫押尼珥，是尼珥的兒子，尼珥是掃羅的叔叔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅的妻名叫亞希暖，是亞希瑪斯的女兒。掃羅的元帥名叫押尼珥，是尼珥的兒子；尼珥是掃羅的叔叔。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The name of Saul’s wife was Ahinoam the daughter of Ahimaaz. And the name of the captain of his army was Abner the son of Ner, Saul’s uncle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His wife’s name was Ahinoam daughter of Ahimaaz. The name of the commander of Saul’s army was Abner son of Ner, and Ner was Saul’s uncle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Saul’s wife was Ahinoam, the daughter of Ahimaaz. The commander of Saul’s army was Abner, the son of Saul’s uncle Ner."
      }
    },
