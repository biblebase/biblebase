    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿耶和华在你我之间判定是非，替我申冤，我却不会动手伤害你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願耶和華在你我之間判斷是非；願耶和華在你身上為我伸冤。我的手卻不敢傷害你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願耶和華在你我中間判斷是非，在你身上為我伸冤，我卻不親手加害於你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May the Lord judge between you and me, and may the Lord avenge me on you; but my hand shall not be against you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the Lord judge between you and me. And may the Lord avenge the wrongs you have done to me, but my hand will not touch you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“May the Lord judge between us. Perhaps the Lord will punish you for what you are trying to do to me, but I will never harm you."
      }
