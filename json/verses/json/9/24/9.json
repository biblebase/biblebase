    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫对扫罗说：“你为什么听信谗言，认为我要谋害你呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛對掃羅說：“你為甚麼聽信人的話，說：‘大衛想要害你’呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛對掃羅說：「你為何聽信人的讒言，說大衛想要害你呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David said to Saul, “Why do you listen to the words of men, saying, ‘Behold, David seeks to harm you’?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to Saul, “Why do you listen when men say, ‘David is bent on harming you’?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he shouted to Saul, “Why do you listen to the people who say I am trying to harm you?"
      }
