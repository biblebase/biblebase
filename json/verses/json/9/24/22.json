    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫便向扫罗起誓。之后，扫罗回家了，大卫和部下也回堡垒去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是大衛向掃羅起誓，掃羅就回自己的家去了，大衛和跟隨他的人也上山寨去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是大衛向掃羅起誓，掃羅就回家去；大衛和跟隨他的人上山寨去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David swore to Saul. And Saul went to his home, but David and his men went up to the stronghold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So David gave his oath to Saul. Then Saul returned home, but David and his men went up to the stronghold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So David promised this to Saul with an oath. Then Saul went home, but David and his men went back to their stronghold."
      }
    },
