    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘你们当中有谁见过这殿昔日的荣耀呢？现在你们看这殿如何？看上去岂不是一文不值吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘你們中間剩下的人，有誰見過這殿宇從前的榮耀呢？現在你們又看它怎樣？豈不是視如無物一樣嗎？’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們中間存留的，有誰見過這殿從前的榮耀呢？現在你們看着如何？豈不在眼中看如無有嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Who is left among you who saw this temple in its former glory? And how do you see it now? Does it not seem to you like nothing in comparison?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘Who of you is left who saw this house in its former glory? How does it look to you now? Does it not seem to you like nothing?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘Does anyone remember this house—this Temple—in its former splendor? How, in comparison, does it look to you now? It must seem like nothing at all!"
      }
    },
