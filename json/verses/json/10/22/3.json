    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的上帝是保护我的磐石， 是我的盾牌，是拯救我的力量， 是我的堡垒，我的避难所， 我的救主。 你救我脱离残暴之徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的　神、我的磐石、我所投靠的、 我的盾牌、我救恩的角、我的高臺、我的避難所、 我的救主；你是救我脫離強暴的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的神，我的磐石，我所投靠的。 他是我的盾牌，是拯救我的角， 是我的高臺，是我的避難所。 我的救主啊，你是救我脫離強暴的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My God, my rock, in whom I take refuge, My shield and the horn of my salvation, my stronghold and my refuge; My savior, You save me from violence."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "my God is my rock, in whom I take refuge, my shield and the horn of my salvation. He is my stronghold, my refuge and my savior— from violent people you save me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "my God is my rock, in whom I find protection. He is my shield, the power that saves me, and my place of safety. He is my refuge, my savior, the one who saves me from violence."
      }
    },
