    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华永远活着， 保护我的磐石当受颂赞， 拯救我的上帝当受尊崇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華是永活的，我的磐石是應當稱頌的； 拯救我的　神，我的磐石，是應當被尊為至高的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華是活神，願我的磐石被人稱頌！ 願神－那拯救我的磐石被人尊崇！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord lives, and blessed be my rock; And exalted be God, the rock of my salvation,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The Lord lives! Praise be to my Rock! Exalted be my God, the Rock, my Savior!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The Lord lives! Praise to my Rock! May God, the Rock of my salvation, be exalted!"
      }
