    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "也许耶和华会体察我的困苦，因我今天所受的咒骂而赐福给我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也許耶和華看見我的苦難，就施恩與我，以取代今天這人對我的咒罵。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "或者耶和華見我遭難，為我今日被這人咒罵，就施恩與我。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Perhaps the Lord will look on my affliction and return good to me instead of his cursing this day.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It may be that the Lord will look upon my misery and restore to me his covenant blessing instead of his curse today.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And perhaps the Lord will see that I am being wronged and will bless me because of these curses today.”"
      }
    },
