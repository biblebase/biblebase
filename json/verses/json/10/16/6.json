    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "还朝大卫和他的臣仆扔石头，无视大卫身边的众多部下和勇士。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又拿石頭投擲大衛和大衛王所有的臣僕，那時眾人和眾勇士都在王的左右。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又拿石頭砍大衛王和王的臣僕；眾民和勇士都在王的左右。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He threw stones at David and at all the servants of King David; and all the people and all the mighty men were at his right hand and at his left."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He pelted David and all the king’s officials with stones, though all the troops and the special guard were on David’s right and left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He threw stones at the king and the king’s officers and all the mighty warriors who surrounded him."
      }
    },
