    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人离开时，百姓都放声大哭。王带着所有的人过了汲沦溪，向旷野走去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人向前走的時候，遍地的人都放聲大哭；王過了汲淪溪，眾人就都走上通往曠野去的路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "本地的人都放聲大哭。眾民盡都過去，王也過了汲淪溪；眾民往曠野去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While all the country was weeping with a loud voice, all the people passed over. The king also passed over the brook Kidron, and all the people passed over toward the way of the wilderness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The whole countryside wept aloud as all the people passed by. The king also crossed the Kidron Valley, and all the people moved on toward the wilderness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone cried loudly as the king and his followers passed by. They crossed the Kidron Valley and then went out toward the wilderness."
      }
    },
