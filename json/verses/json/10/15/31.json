    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人告诉大卫，亚希多弗也叛变，投奔了押沙龙，大卫就祷告说：“耶和华啊，求你使亚希多弗的谋算都变得愚不可及。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有人告訴大衛說：“亞希多弗也在押沙龍的叛黨之中。”大衛禱告說：“耶和華啊，求你使亞希多弗的計謀變為愚拙。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人告訴大衛說：「亞希多弗也在叛黨之中，隨從押沙龍。」大衛禱告說：「耶和華啊，求你使亞希多弗的計謀變為愚拙！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now someone told David, saying, “Ahithophel is among the conspirators with Absalom.” And David said, “O Lord, I pray, make the counsel of Ahithophel foolishness.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now David had been told, “Ahithophel is among the conspirators with Absalom.” So David prayed, “Lord, turn Ahithophel’s counsel into foolishness.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When someone told David that his adviser Ahithophel was now backing Absalom, David prayed, “O Lord, let Ahithophel give Absalom foolish advice!”"
      }
    },
