    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那家的妇人用盖子盖上井口，然后在上面撒了一些麦子，没有人知道这件事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那家的主婦拿了一塊大布，蓋在井口上，又在上面鋪了一些麥子，所以沒有人知道這件事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那家的婦人用蓋蓋上井口，又在上頭鋪上碎麥，事就沒有洩漏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the woman took a covering and spread it over the well’s mouth and scattered grain on it, so that nothing was known."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His wife took a covering and spread it out over the opening of the well and scattered grain over it. No one knew anything about it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The man’s wife put a cloth over the top of the well and scattered grain on it to dry in the sun; so no one suspected they were there."
      }
    },
