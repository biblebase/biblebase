    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫王和部下就动身过约旦河，在黎明之前，所有的人都过去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是大衛和跟隨他的眾人都起來，渡過約旦河；到了天亮，沒有一人還沒有渡過約旦河的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是大衛和跟隨他的人都起來，過約旦河。到了天亮，無一人不過約旦河的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David and all the people who were with him arose and crossed the Jordan; and by dawn not even one remained who had not crossed the Jordan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So David and all the people with him set out and crossed the Jordan. By daybreak, no one was left who had not crossed the Jordan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So David and all the people with him went across the Jordan River during the night, and they were all on the other bank before dawn."
      }
    },
