    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫又求问耶和华，耶和华说：“你不要正面出击，要绕到他们后面，从桑林对面攻打他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛又求問耶和華，耶和華說：“你不要直接上去，要繞到他們後面，從桑林對面攻打他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛求問耶和華；耶和華說：「不要一直地上去，要轉到他們後頭，從桑林對面攻打他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When David inquired of the Lord, He said, “You shall not go directly up; circle around behind them and come at them in front of the balsam trees."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "so David inquired of the Lord, and he answered, “Do not go straight up, but circle around behind them and attack them in front of the poplar trees."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And again David asked the Lord what to do. “Do not attack them straight on,” the Lord replied. “Instead, circle around behind and attack them near the poplar trees."
      }
    },
