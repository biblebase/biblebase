    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列的长老都到希伯仑见大卫王，大卫与他们在耶和华面前立约，他们膏立大卫做以色列的王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列的眾長老都到希伯崙來見王。大衛王在希伯崙耶和華面前與他們立約，他們就膏立大衛作以色列的王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是以色列的長老都來到希伯崙見大衛王，大衛在希伯崙耶和華面前與他們立約，他們就膏大衛作以色列的王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So all the elders of Israel came to the king at Hebron, and King David made a covenant with them before the Lord at Hebron; then they anointed David king over Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When all the elders of Israel had come to King David at Hebron, the king made a covenant with them at Hebron before the Lord, and they anointed David king over Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So there at Hebron, King David made a covenant before the Lord with all the elders of Israel. And they anointed him king of Israel."
      }
    },
