      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗的儿子伊施波设听到押尼珥死在希伯仑，吓得手脚发软，整个以色列都震惊不已。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅的兒子伊施波設聽見押尼珥在希伯崙死了，就怕得手都發軟，以色列眾人也都驚惶失措。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅的兒子伊施波設聽見押尼珥死在希伯崙，手就發軟；以色列眾人也都驚惶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when Ish-bosheth, Saul’s son, heard that Abner had died in Hebron, he lost courage, and all Israel was disturbed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Ish-Bosheth son of Saul heard that Abner had died in Hebron, he lost courage, and all Israel became alarmed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Ishbosheth, Saul’s son, heard about Abner’s death at Hebron, he lost all courage, and all Israel became paralyzed with fear."
      }
