    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来心情恢复平静，开始想念押沙龙。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛的心不再懷恨押沙龍，對暗嫩的死也不再那麼難過了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "暗嫩死了以後，大衛王得了安慰，心裏切切想念押沙龍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The heart of King David longed to go out to Absalom; for he was comforted concerning Amnon, since he was dead."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And King David longed to go to Absalom, for he was consoled concerning Amnon’s death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And King David, now reconciled to Amnon’s death, longed to be reunited with his son Absalom."
      }
    },
