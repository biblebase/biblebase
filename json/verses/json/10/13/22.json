    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "押沙龙没有对暗嫩说一句话，只是心里暗暗地恨他，因为他玷污了妹妹她玛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "押沙龍甚麼話都沒有對暗嫩說。押沙龍恨暗嫩，因為暗嫩污辱了他的妹妹他瑪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "押沙龍並不和他哥哥暗嫩說好說歹；因為暗嫩玷辱他妹妹她瑪，所以押沙龍恨惡他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Absalom did not speak to Amnon either good or bad; for Absalom hated Amnon because he had violated his sister Tamar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And Absalom never said a word to Amnon, either good or bad; he hated Amnon because he had disgraced his sister Tamar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And though Absalom never spoke to Amnon about this, he hated Amnon deeply because of what he had done to his sister."
      }
    },
