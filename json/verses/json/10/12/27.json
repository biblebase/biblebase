    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他派人去向大卫禀告说：“我攻打拉巴，已经占领了城的水源。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約押派遣使者去見大衛，說：“我攻打拉巴，已經攻取了水城。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約押打發使者去見大衛，說：「我攻打拉巴，取其水城。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joab sent messengers to David and said, “I have fought against Rabbah, I have even captured the city of waters."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Joab then sent messengers to David, saying, “I have fought against Rabbah and taken its water supply."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Joab sent messengers to tell David, “I have fought against Rabbah and captured its water supply."
      }
