    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拿单说完，便回家去了。 耶和华击打乌利亚的妻子给大卫生的孩子，使他患重病。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是拿單回自己的家去了。 耶和華擊打烏利亞的妻子給大衛所生的孩子，他就害了重病。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拿單就回家去了。 耶和華擊打烏利亞妻給大衛所生的孩子，使他得重病。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Nathan went to his house. Then the Lord struck the child that Uriah’s widow bore to David, so that he was very sick."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After Nathan had gone home, the Lord struck the child that Uriah’s wife had borne to David, and he became ill."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After Nathan returned to his home, the Lord sent a deadly illness to the child of David and Uriah’s wife."
      }
    },
