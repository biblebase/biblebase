    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说，‘我要在你家中降下灾祸，我要当着你的面把你的妻妾交给你的近臣，他必在光天化日之下与她们行淫。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華這樣說：‘看哪！我必從你家中興起禍患攻擊你；我要在你的眼前把你的妃嬪拿去賜給你的同伴。他要在光天化日之下與她們同床。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華如此說：『我必從你家中興起禍患攻擊你；我必在你眼前把你的妃嬪賜給別人，他在日光之下就與她們同寢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord, ‘Behold, I will raise up evil against you from your own household; I will even take your wives before your eyes and give them to your companion, and he will lie with your wives in broad daylight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Lord says: ‘Out of your own household I am going to bring calamity on you. Before your very eyes I will take your wives and give them to one who is close to you, and he will sleep with your wives in broad daylight."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Lord says: Because of what you have done, I will cause your own household to rebel against you. I will give your wives to another man before your very eyes, and he will go to bed with them in public view."
      }
    },
