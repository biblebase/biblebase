    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你藐视我，把赫人乌利亚的妻子据为己有，从今以后，杀戮流血的事必永不离开你的家。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，從今以後，刀劍必不離開你的家，因為你藐視了我，娶了赫人烏利亞的妻子作你的妻子。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你既藐視我，娶了赫人烏利亞的妻為妻，所以刀劍必永不離開你的家。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now therefore, the sword shall never depart from your house, because you have despised Me and have taken the wife of Uriah the Hittite to be your wife.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now, therefore, the sword will never depart from your house, because you despised me and took the wife of Uriah the Hittite to be your own.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From this time on, your family will live by the sword because you have despised me by taking Uriah’s wife to be your own."
      }
    },
