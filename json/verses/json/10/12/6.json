    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他真是没有半点怜悯之心，他必须偿还那穷人四倍！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必須四倍賠償這羊羔，因為他作了這事，又因他沒有憐憫的心。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必償還羊羔四倍；因為他行這事，沒有憐恤的心。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He must make restitution for the lamb fourfold, because he did this thing and had no compassion.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He must pay for that lamb four times over, because he did such a thing and had no pity.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He must repay four lambs to the poor man for the one he stole and for having no pity.”"
      }
    },
