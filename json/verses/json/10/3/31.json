    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫对约押和部下说：“你们要撕裂衣服，腰束麻布，在押尼珥棺前哭丧！”大卫王也跟在棺后送葬。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛對約押和與他在一起的眾人說：“你們要撕裂衣服，腰束麻布，在押尼珥面前哀哭！”大衛王也走在棺木後面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛吩咐約押和跟隨他的眾人說：「你們當撕裂衣服，腰束麻布，在押尼珥棺前哀哭。」大衛王也跟在棺後。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David said to Joab and to all the people who were with him, “Tear your clothes and gird on sackcloth and lament before Abner.” And King David walked behind the bier."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then David said to Joab and all the people with him, “Tear your clothes and put on sackcloth and walk in mourning in front of Abner.” King David himself walked behind the bier."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then David said to Joab and all those who were with him, “Tear your clothes and put on burlap. Mourn for Abner.” And King David himself walked behind the procession to the grave."
      }
    },
