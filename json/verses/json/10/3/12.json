    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "押尼珥派使者送信给大卫，说：“这个国家难道不是你的吗？如果你与我立约，我必帮助你统一以色列。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "押尼珥差派使者代表他去見大衛，說：“這地是屬誰的呢？”又說：“你與我立約，我就必幫助你，使全以色列都歸順你。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "押尼珥打發人去見大衛，替他說：「這國歸誰呢？」又說：「你與我立約，我必幫助你，使以色列人都歸服你。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Abner sent messengers to David in his place, saying, “Whose is the land? Make your covenant with me, and behold, my hand shall be with you to bring all Israel over to you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Abner sent messengers on his behalf to say to David, “Whose land is it? Make an agreement with me, and I will help you bring all Israel over to you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Abner sent messengers to David, saying, “Doesn’t the entire land belong to you? Make a solemn pact with me, and I will help turn over all of Israel to you.”"
      }
    },
