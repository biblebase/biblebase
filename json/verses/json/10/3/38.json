      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫王对他的随从说：“你们难道不知道今天以色列损失了一位伟大的将领吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王對他的臣僕說：“你們不知道今天在以色列中，有一位將領和偉人倒斃了嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王對臣僕說：「你們豈不知今日以色列人中死了一個作元帥的大丈夫嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king said to his servants, “Do you not know that a prince and a great man has fallen this day in Israel?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the king said to his men, “Do you not realize that a commander and a great man has fallen in Israel this day?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then King David said to his officials, “Don’t you realize that a great commander has fallen today in Israel?"
      }
    },
