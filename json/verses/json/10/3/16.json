    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "帕铁一路跟着米甲，边走边哭，一直到巴户琳。押尼珥对他说：“你回去吧！”他就回去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "米甲的丈夫與她一起去，跟在她後面，一邊走、一邊哭，一直跟到巴戶琳。押尼珥對他說：“你去吧！回去吧！”他只好回去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "米甲的丈夫跟着她，一面走一面哭，直跟到巴戶琳。押尼珥說：「你回去吧！」帕鐵就回去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But her husband went with her, weeping as he went, and followed her as far as Bahurim. Then Abner said to him, “Go, return.” So he returned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Her husband, however, went with her, weeping behind her all the way to Bahurim. Then Abner said to him, “Go back home!” So he went back."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Palti followed along behind her as far as Bahurim, weeping as he went. Then Abner told him, “Go back home!” So Palti returned."
      }
    },
