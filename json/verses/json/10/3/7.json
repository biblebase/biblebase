    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他跟扫罗的一个妃嫔、艾押的女儿利斯巴通奸，伊施波设问他：“你为什么跟我父亲的妃嫔一起睡觉呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅有一個妃子，名叫利斯巴，是愛亞的女兒。伊施波設對押尼珥說：“你為甚麼親近我父親的妃子呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅有一妃嬪，名叫利斯巴，是愛亞的女兒。一日，伊施波設對押尼珥說：「你為甚麼與我父的妃嬪同房呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Saul had a concubine whose name was Rizpah, the daughter of Aiah; and Ish-bosheth said to Abner, “Why have you gone in to my father’s concubine?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now Saul had had a concubine named Rizpah daughter of Aiah. And Ish-Bosheth said to Abner, “Why did you sleep with my father’s concubine?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One day Ishbosheth, Saul’s son, accused Abner of sleeping with one of his father’s concubines, a woman named Rizpah, daughter of Aiah."
      }
    },
