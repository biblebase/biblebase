    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们就把押尼珥葬在希伯仑。大卫王和众人在墓旁大哭一场。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們把押尼珥埋葬在希伯崙。王在押尼珥的墓旁放聲大哭，眾人也都哭了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們將押尼珥葬在希伯崙。王在押尼珥的墓旁放聲而哭，眾民也都哭了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus they buried Abner in Hebron; and the king lifted up his voice and wept at the grave of Abner, and all the people wept."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They buried Abner in Hebron, and the king wept aloud at Abner’s tomb. All the people wept also."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They buried Abner in Hebron, and the king and all the people wept at his graveside."
      }
    },
