    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一天黄昏，大卫从床上起来在王宫顶上散步，看见一个长得非常漂亮的女人在沐浴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一天近黃昏的時候，大衛從床上起來，在王宮的平頂上散步。他從平頂上看見一個婦人在洗澡；這婦人容貌非常美麗。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "一日，太陽平西，大衛從牀上起來，在王宮的平頂上遊行，看見一個婦人沐浴，容貌甚美，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when evening came David arose from his bed and walked around on the roof of the king’s house, and from the roof he saw a woman bathing; and the woman was very beautiful in appearance."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One evening David got up from his bed and walked around on the roof of the palace. From the roof he saw a woman bathing. The woman was very beautiful,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Late one afternoon, after his midday rest, David got out of bed and was walking on the roof of the palace. As he looked out over the city, he noticed a woman of unusual beauty taking a bath."
      }
    },
