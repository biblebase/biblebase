    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约押吩咐一个古示人：“你去把所看见的禀报给王。”那人就拜别了约押，马上跑回去报信。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是約押對一個古實人說：“你去把你所看見的告訴王吧！”那古實人叩拜了約押，就跑去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約押對古示人說：「你去將你所看見的告訴王。」古示人在約押面前下拜，就跑去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Joab said to the Cushite, “Go, tell the king what you have seen.” So the Cushite bowed to Joab and ran."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Joab said to a Cushite, “Go, tell the king what you have seen.” The Cushite bowed down before Joab and ran off."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Joab said to a man from Ethiopia, “Go tell the king what you have seen.” The man bowed and ran off."
      }
