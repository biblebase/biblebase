    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "洗鲁雅的儿子、约押的兄弟亚比筛是这三位勇士的统领，他曾挥矛刺死三百人，在三勇士中出了名，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "洗魯雅的兒子約押的兄弟亞比篩是三十勇士的首領。他曾揮舞矛槍殺死了三百人，因此他與“三勇士”齊名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "洗魯雅的兒子、約押的兄弟亞比篩是這三個勇士的首領；他舉槍殺了三百人，就在三個勇士裏得了名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Abishai, the brother of Joab, the son of Zeruiah, was chief of the thirty. And he swung his spear against three hundred and killed them, and had a name as well as the three."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Abishai the brother of Joab son of Zeruiah was chief of the Three. He raised his spear against three hundred men, whom he killed, and so he became as famous as the Three."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Abishai son of Zeruiah, the brother of Joab, was the leader of the Thirty. He once used his spear to kill 300 enemy warriors in a single battle. It was by such feats that he became as famous as the Three."
      }
    },
