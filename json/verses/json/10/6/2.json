    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "率领他们到犹大的巴拉去接上帝的约柜。这约柜以坐在基路伯天使上面的万军之耶和华的名字命名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛和與他在一起的眾人起程前去，要從巴拉．猶大把　神的約櫃運來，這約櫃的名稱是以坐在二基路伯之上萬軍之耶和華的名字而起的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛起身，率領跟隨他的眾人前往，要從巴拉‧猶大將神的約櫃運來；這約櫃就是坐在二基路伯上萬軍之耶和華留名的約櫃。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And David arose and went with all the people who were with him to Baale-judah, to bring up from there the ark of God which is called by the Name, the very name of the Lord of hosts who is enthroned above the cherubim."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He and all his men went to Baalah in Judah to bring up from there the ark of God, which is called by the Name, the name of the Lord Almighty, who is enthroned between the cherubim on the ark."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He led them to Baalah of Judah to bring back the Ark of God, which bears the name of the Lord of Heaven’s Armies, who is enthroned between the cherubim."
      }
    },
