    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人要害我和我儿子的性命，叫我们不得承受上帝所赐的产业，也许王能从这人手中救我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人要把我和我的兒子從　神的產業上一同消滅，也許王會垂聽，會把婢女從那人的手下救出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人要將我和我兒子從神的地業上一同除滅，王必應允救我脫離他的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the king will hear and deliver his maidservant from the hand of the man who would destroy both me and my son from the inheritance of God.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Perhaps the king will agree to deliver his servant from the hand of the man who is trying to cut off both me and my son from God’s inheritance.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and rescue us from those who would cut us off from the inheritance God has given us."
      }
    },
