    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚劳拿看见王及他的臣仆来了，就上前俯伏在地，向王跪拜，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞勞拿往下觀看，看見王和他的臣僕正朝著他這邊走過來，就出去迎接，臉伏於地向王叩拜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞勞拿觀看，見王和他臣僕前來，就迎接出去，臉伏於地，向王下拜，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Araunah looked down and saw the king and his servants crossing over toward him; and Araunah went out and bowed his face to the ground before the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Araunah looked and saw the king and his officials coming toward him, he went out and bowed down before the king with his face to the ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Araunah saw the king and his men coming toward him, he came and bowed before the king with his face to the ground."
      }
    },
