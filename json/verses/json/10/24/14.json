    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫说：“我实在为难！不过我宁愿落在耶和华的手中，也不愿落在人的手中，因为耶和华有无限的怜悯。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛對迦得說：“我非常為難，我們情願落在耶和華的手裡，因為他有豐盛的憐憫。不要讓我落在人的手裡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛對迦得說：「我甚為難！我願落在耶和華的手裏，因為他有豐盛的憐憫。我不願落在人的手裏。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David said to Gad, “I am in great distress. Let us now fall into the hand of the Lord for His mercies are great, but do not let me fall into the hand of man.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "David said to Gad, “I am in deep distress. Let us fall into the hands of the Lord, for his mercy is great; but do not let me fall into human hands.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I’m in a desperate situation!” David replied to Gad. “But let us fall into the hands of the Lord, for his mercy is great. Do not let me fall into human hands.”"
      }
    },
