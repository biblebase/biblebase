    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从我把以色列人带出埃及那天起，直到今天，我从未住过殿宇，一路上都住在帐篷里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為自從我把以色列人從埃及帶上來的那天起，直到今日，我都沒有住過殿宇。我一直都住在帳棚和會幕裡，到處漂泊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "自從我領以色列人出埃及直到今日，我未曾住過殿宇，常在會幕和帳幕中行走。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I have not dwelt in a house since the day I brought up the sons of Israel from Egypt, even to this day; but I have been moving about in a tent, even in a tabernacle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have not dwelt in a house from the day I brought the Israelites up out of Egypt to this day. I have been moving from place to place with a tent as my dwelling."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have never lived in a house, from the day I brought the Israelites out of Egypt until this very day. I have always moved from one place to another with a tent and a Tabernacle as my dwelling."
      }
    },
