    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你去告诉我的仆人大卫，‘耶和华说，你岂是为我建造殿宇的人？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你去對我的僕人大衛說：耶和華這樣說：‘你要建造一座殿宇給我居住嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你去告訴我僕人大衛，說耶和華如此說：『你豈可建造殿宇給我居住呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Go and say to My servant David, ‘Thus says the Lord, “Are you the one who should build Me a house to dwell in?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Go and tell my servant David, ‘This is what the Lord says: Are you the one to build me a house to dwell in?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Go and tell my servant David, ‘This is what the Lord has declared: Are you the one to build a house for me to live in?"
      }
