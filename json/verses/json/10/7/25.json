    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华上帝啊，求你永远信守你对仆人和仆人家的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華　神啊！你所應許關於你僕人和他的家的話，現在求你永遠堅守，並且照著你所說的去行吧；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「耶和華神啊，你所應許僕人和僕人家的話，求你堅定，直到永遠；照你所說的而行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now therefore, O Lord God, the word that You have spoken concerning Your servant and his house, confirm it forever, and do as You have spoken,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“And now, Lord God, keep forever the promise you have made concerning your servant and his house. Do as you promised,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And now, O Lord God, I am your servant; do as you have promised concerning me and my family. Confirm it as a promise that will last forever."
      }
    },
