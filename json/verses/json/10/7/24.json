    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你让以色列人永远做你的子民，你耶和华做他们的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你為自己堅立了你的子民以色列，使他們永遠作你的子民；耶和華啊，你也作了他們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你曾堅立你的民以色列作你的子民，直到永遠；你－耶和華也作了他們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You have established for Yourself Your people Israel as Your own people forever, and You, O Lord, have become their God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have established your people Israel as your very own forever, and you, Lord, have become their God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You made Israel your very own people forever, and you, O Lord, became their God."
      }
    },
