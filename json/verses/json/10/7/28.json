    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华啊，只有你是上帝，你的话真实可靠，你给了仆人美好的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶和華啊！你是　神；你的話是真實可信的。你向你僕人應許了這美好的福分。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「主耶和華啊，惟有你是神。你的話是真實的；你也應許將這福氣賜給僕人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now, O Lord God, You are God, and Your words are truth, and You have promised this good thing to Your servant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Sovereign Lord, you are God! Your covenant is trustworthy, and you have promised these good things to your servant."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you are God, O Sovereign Lord. Your words are truth, and you have promised these good things to your servant."
      }
