    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华啊，你真伟大！我们从未听过有谁能与你相比，你是独一无二的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶和華啊，因此，你是偉大的！按照我們耳朵所聽見的，沒有像你的，除了你以外，再沒有別的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華啊，你本為大，照我們耳中聽見，沒有可比你的；除你以外再無神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For this reason You are great, O Lord God; for there is none like You, and there is no God besides You, according to all that we have heard with our ears."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“How great you are, Sovereign Lord! There is no one like you, and there is no God but you, as we have heard with our own ears."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“How great you are, O Sovereign Lord! There is no one like you. We have never even heard of another God like you!"
      }
    },
