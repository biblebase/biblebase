    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "押尼珥说：“你去追赶别人，夺取他的兵器吧。”但亚撒黑还是穷追不舍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "押尼珥對他說：“你或轉左或轉右，抓住一個年輕人，奪去他的武器吧！”可是亞撒黑不願意離開不追趕他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "押尼珥對他說：「你或轉向左轉向右，拿住一個少年人，剝去他的戰衣。」亞撒黑卻不肯轉開不追趕他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Abner said to him, “Turn to your right or to your left, and take hold of one of the young men for yourself, and take for yourself his spoil.” But Asahel was not willing to turn aside from following him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Abner said to him, “Turn aside to the right or to the left; take on one of the young men and strip him of his weapons.” But Asahel would not stop chasing him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Go fight someone else!” Abner warned. “Take on one of the younger men, and strip him of his weapons.” But Asahel kept right on chasing Abner."
      }
    },
