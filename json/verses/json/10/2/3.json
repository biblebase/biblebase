    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他还把部下和他们的家属一起带去，住在希伯仑地区的城邑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛也把與他在一起的人，和他們各人的家眷都帶上去。他們就在希伯崙的城鎮裡住下來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛也將跟隨他的人和他們各人的眷屬一同帶上去，住在希伯崙的城邑中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And David brought up his men who were with him, each with his household; and they lived in the cities of Hebron."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "David also took the men who were with him, each with his family, and they settled in Hebron and its towns."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and his men and their families all moved to Judah, and they settled in the villages near Hebron."
      }
    },
