    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他为了你们以及老底嘉和希拉波立的弟兄姊妹而不辞劳苦，这是我可以作证的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我可以為他作證，他為了你們和在老底嘉、希拉波立的人，多受勞苦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他為你們和老底嘉並希拉坡里的弟兄多多地勞苦，這是我可以給他作見證的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I testify for him that he has a deep concern for you and for those who are in Laodicea and Hierapolis."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I vouch for him that he is working hard for you and for those at Laodicea and Hierapolis."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I can assure you that he prays hard for you and also for the believers in Laodicea and Hierapolis."
      }
    },
