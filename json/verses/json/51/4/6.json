    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谈吐要温和、风趣，知道该怎样回答每个人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們的話要常常溫和，好像是用鹽調和的，使你們知道應當怎樣回答各人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們的言語要常常帶着和氣，好像用鹽調和，就可知道該怎樣回答各人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let your speech always be with grace, as though seasoned with salt, so that you will know how you should respond to each person."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let your conversation be always full of grace, seasoned with salt, so that you may know how to answer everyone."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let your conversation be gracious and attractive so that you will have the right response for everyone."
      }
    },
