    "sermons": [
      {
        "id": "home_sermons_grace-colossians-ch1",
        "title": "歌羅西書 1",
        "date": "2016-02-19",
        "preacher": "王文堂牧師",
        "verses": "歌羅西書 1:1-29",
        "slides": "http://cbcwla.org/media/ch/Special/GraceFellowship2016/Grace-Colossians-ch1.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只是你们必须恒心持守所信的道，根基稳固，坚定不移，不要失去你们听到福音后所得到的盼望。这福音传给了天下万民。我保罗也做了这福音的使者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只是你們要常存信心，根基穩固，不受動搖偏離福音的盼望。這福音你們聽過了，也傳給了天下萬民；我保羅也作了這福音的僕役。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只要你們在所信的道上恆心，根基穩固，堅定不移，不致被引動失去福音的盼望。這福音就是你們所聽過的，也是傳與普天下萬人聽的，我－保羅也作了這福音的執事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "if indeed you continue in the faith firmly established and steadfast, and not moved away from the hope of the gospel that you have heard, which was proclaimed in all creation under heaven, and of which I, Paul, was made a minister."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "if you continue in your faith, established and firm, and do not move from the hope held out in the gospel. This is the gospel that you heard and that has been proclaimed to every creature under heaven, and of which I, Paul, have become a servant."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you must continue to believe this truth and stand firmly in it. Don’t drift away from the assurance you received when you heard the Good News. The Good News has been preached all over the world, and I, Paul, have been appointed as God’s servant to proclaim it."
      }
    },
