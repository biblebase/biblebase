    "sermons": [
      {
        "id": "home_sermons_sermon-2009-01-18",
        "title": "新年，作新人",
        "date": "2009-01-18",
        "preacher": "張繼忠牧師",
        "verses": "歌羅西書 3:1-15",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2009/Sermon-2009-01-18.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从此，不再分希腊人、犹太人，受割礼的、未受割礼的，野蛮人、未开化的人，奴隶和自由人，基督就是一切，并且贯穿一切。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在這一方面，並不分希臘人和猶太人，受割禮的和未受割禮的，未開化的人和西古提人，奴隸和自由人，唯有基督是一切，也在一切之內。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在此並不分希臘人、猶太人，受割禮的、未受割禮的，化外人、西古提人，為奴的、自主的，惟有基督是包括一切，又住在各人之內。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "a renewal in which there is no distinction between Greek and Jew, circumcised and uncircumcised, barbarian, Scythian, slave and freeman, but Christ is all, and in all."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Here there is no Gentile or Jew, circumcised or uncircumcised, barbarian, Scythian, slave or free, but Christ is all, and is in all."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In this new life, it doesn’t matter if you are a Jew or a Gentile, circumcised or uncircumcised, barbaric, uncivilized, slave, or free. Christ is all that matters, and he lives in all of us."
      }
    },
