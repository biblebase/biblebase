    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们无论做什么事、说什么话，都要奉主耶稣的名而行，并借着祂感谢父上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡你們所作的，無論是言語或行為，都要奉主耶穌的名，藉著他感謝父　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無論做甚麼，或說話或行事，都要奉主耶穌的名，藉着他感謝父神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whatever you do in word or deed, do all in the name of the Lord Jesus, giving thanks through Him to God the Father."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And whatever you do, whether in word or deed, do it all in the name of the Lord Jesus, giving thanks to God the Father through him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And whatever you do or say, do it as a representative of the Lord Jesus, giving thanks through him to God the Father."
      }
    },
