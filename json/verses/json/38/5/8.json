    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天使说：“这是罪恶。”他把女子推回量器中，盖上铅盖。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天使說：“這是罪惡。”然後他把那婦人擲回量器中，又把那鉛蓋丟在量器的口上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天使說：「這是罪惡。」他就把婦人扔在量器中，將那片圓鉛扔在量器的口上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he said, “This is Wickedness!” And he threw her down into the middle of the ephah and cast the lead weight on its opening."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said, “This is wickedness,” and he pushed her back into the basket and pushed its lead cover down on it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The angel said, “The woman’s name is Wickedness,” and he pushed her back into the basket and closed the heavy lid again."
      }
    },
