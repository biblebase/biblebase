    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华又让我看见约书亚大祭司站在祂的天使面前，撒旦站在约书亚的右边要控诉他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華又使我看見大祭司約書亞站在耶和華的使者面前，撒但也站在約書亞右邊控告他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天使又指給我看：大祭司約書亞站在耶和華的使者面前；撒但也站在約書亞的右邊，與他作對。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he showed me Joshua the high priest standing before the angel of the Lord, and Satan standing at his right hand to accuse him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he showed me Joshua the high priest standing before the angel of the Lord, and Satan standing at his right side to accuse him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the angel showed me Jeshua the high priest standing before the angel of the Lord. The Accuser, Satan, was there at the angel’s right hand, making accusations against Jeshua."
      }
    },
