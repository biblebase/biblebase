    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“丢弃羊群的无用牧人有祸了！ 愿刀砍在他的臂膀和右眼上！ 愿他的臂膀彻底枯槁， 他的右眼完全失明！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“那丟棄羊群的無用牧人有禍了！ 願刀劍落在他的膀臂和他的右眼上； 願他的膀臂全然枯槁， 願他的右眼完全失明！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無用的牧人丟棄羊羣有禍了！ 刀必臨到他的膀臂和右眼上。 他的膀臂必全然枯乾； 他的右眼也必昏暗失明。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe to the worthless shepherd Who leaves the flock! A sword will be on his arm And on his right eye! His arm will be totally withered And his right eye will be blind.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Woe to the worthless shepherd, who deserts the flock! May the sword strike his arm and his right eye! May his arm be completely withered, his right eye totally blinded!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What sorrow awaits this worthless shepherd who abandons the flock! The sword will cut his arm and pierce his right eye. His arm will become useless, and his right eye completely blind.”"
      }
    },
