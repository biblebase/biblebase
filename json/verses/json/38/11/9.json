    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是我说：“我不再牧养你们了。要死的就死吧，要灭亡的就灭亡吧，让剩下的互相吞吃吧。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我就說：“我不再牧養你們了；那將要死的，讓他死吧！那將要被除滅的，讓他被除滅吧；那些餘剩的，讓他們彼此吞食對方的肉吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我就說：「我不牧養你們。要死的，由他死；要喪亡的，由他喪亡；餘剩的，由他們彼此相食。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I said, “I will not pasture you. What is to die, let it die, and what is to be annihilated, let it be annihilated; and let those who are left eat one another’s flesh.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and said, “I will not be your shepherd. Let the dying die, and the perishing perish. Let those who are left eat one another’s flesh.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I told them, “I won’t be your shepherd any longer. If you die, you die. If you are killed, you are killed. And let those who remain devour each other!”"
      }
    },
