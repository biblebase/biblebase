    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我必向牧人发怒， 我必惩罚这些首领。 因为我——万军之耶和华必眷顾我的羊群犹大家， 我必使他们像战场上的骏马。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我的怒氣要向這些牧人發作， 我要懲罰那些公山羊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的怒氣向牧人發作； 我必懲罰公山羊； 因我－萬軍之耶和華 眷顧自己的羊羣，就是猶大家， 必使他們如駿馬在陣上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“My anger is kindled against the shepherds, And I will punish the male goats; For the Lord of hosts has visited His flock, the house of Judah, And will make them like His majestic horse in battle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“My anger burns against the shepherds, and I will punish the leaders; for the Lord Almighty will care for his flock, the people of Judah, and make them like a proud horse in battle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“My anger burns against your shepherds, and I will punish these leaders. For the Lord of Heaven’s Armies has arrived to look after Judah, his flock. He will make them strong and glorious, like a proud warhorse in battle."
      }
    },
