    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要用这些金银制作冠冕，戴在约撒答的儿子约书亚大祭司头上，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要拿這金銀來做冠冕，戴在約撒答的兒子大祭司約書亞的頭上，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "取這金銀做冠冕，戴在約撒答的兒子大祭司約書亞的頭上，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Take silver and gold, make an ornate crown and set it on the head of Joshua the son of Jehozadak, the high priest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Take the silver and gold and make a crown, and set it on the head of the high priest, Joshua son of Jozadak."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Accept their gifts, and make a crown from the silver and gold. Then put the crown on the head of Jeshua son of Jehozadak, the high priest."
      }
    },
