    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们问万军之耶和华殿里的祭司和先知：“我们还要照多年的惯例在五月哀伤、禁食吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們詢問萬軍之耶和華殿裡的眾祭司，以及眾先知說：“我們還要在五月哭泣和禁食，像我們這許多年來所行的嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並問萬軍之耶和華殿中的祭司和先知說：「我歷年以來，在五月間哭泣齋戒，現在還當這樣行嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "speaking to the priests who belong to the house of the Lord of hosts, and to the prophets, saying, “Shall I weep in the fifth month and abstain, as I have done these many years?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "by asking the priests of the house of the Lord Almighty and the prophets, “Should I mourn and fast in the fifth month, as I have done for so many years?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They were to ask this question of the prophets and the priests at the Temple of the Lord of Heaven’s Armies: “Should we continue to mourn and fast each summer on the anniversary of the Temple’s destruction, as we have done for so many years?”"
      }
    },
