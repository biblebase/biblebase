    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“万军之耶和华曾对你们的祖先说，‘要秉公行义，彼此以慈爱和怜悯相待。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“萬軍之耶和華曾這樣告訴你們的列祖說：‘你們要執法公正，各人要以慈愛和憐憫待自己的兄弟。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「萬軍之耶和華曾對你們的列祖如此說：『要按至理判斷，各人以慈愛憐憫弟兄。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Thus has the Lord of hosts said, ‘Dispense true justice and practice kindness and compassion each to his brother;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Lord Almighty said: ‘Administer true justice; show mercy and compassion to one another."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Lord of Heaven’s Armies says: Judge fairly, and show mercy and kindness to one another."
      }
    },
