      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "与以色列各支派一样， 世人的眼目都仰望耶和华。 以下是耶和华的启示： 祂的惩罚必临到哈得拉， 临到大马士革，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以下是耶和華的默示： 耶和華的話臨到哈得拉地，落在大馬士革，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的默示應驗在哈得拉地大馬士革 －世人和以色列各支派的眼目都仰望耶和華－"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The burden of the word of the Lord is against the land of Hadrach, with Damascus as its resting place (for the eyes of men, especially of all the tribes of Israel, are toward the Lord),"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A prophecy: The word of the Lord is against the land of Hadrak and will come to rest on Damascus— for the eyes of all people and all the tribes of Israel are on the Lord—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is the message from the Lord against the land of Aram and the city of Damascus, for the eyes of humanity, including all the tribes of Israel, are on the Lord."
      }
