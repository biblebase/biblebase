    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "锡安啊，因为我用血跟你立了约， 我必将你中间被掳的人从无水之坑释放出来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "錫安哪！至於你，因著我與你立約的血，我把你中間被囚禁的人從無水的坑裡釋放出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "錫安哪，我因與你立約的血， 將你中間被擄而囚的人從無水的坑中釋放出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As for you also, because of the blood of My covenant with you, I have set your prisoners free from the waterless pit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As for you, because of the blood of my covenant with you, I will free your prisoners from the waterless pit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because of the covenant I made with you, sealed with blood, I will free your prisoners from death in a waterless dungeon."
      }
    },
