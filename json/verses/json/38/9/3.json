    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "泰尔为自己建造堡垒， 堆积的银子多如尘土， 金子多如街上的泥土。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "推羅為自己建造了堡壘， 堆積銀子，多如塵沙， 積聚精金，多如街上的泥土。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "泰爾為自己修築保障， 積蓄銀子如塵沙， 堆起精金如街上的泥土。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For Tyre built herself a fortress And piled up silver like dust, And gold like the mire of the streets."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Tyre has built herself a stronghold; she has heaped up silver like dust, and gold like the dirt of the streets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Tyre has built a strong fortress and has made silver and gold as plentiful as dust in the streets!"
      }
    },
