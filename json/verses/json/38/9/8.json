    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必在我家四周安营， 不容敌人入侵， 再也不容外人来欺压我的子民， 因为现在我亲自看顾他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必在我的家四圍安營， 不容敵軍出入往來； 欺壓人的不能再侵犯我的子民， 因我現在親眼看顧他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必在我家的四圍安營， 使敵軍不得任意往來， 暴虐的人也不再經過， 因為我親眼看顧我的家。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I will camp around My house because of an army, Because of him who passes by and returns; And no oppressor will pass over them anymore, For now I have seen with My eyes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I will encamp at my temple to guard it against marauding forces. Never again will an oppressor overrun my people, for now I am keeping watch."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will guard my Temple and protect it from invading armies. I am watching closely to ensure that no more foreign oppressors overrun my people’s land."
      }
    },
