      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然而，看啊，主必夺去她的一切， 摧毁她海上的权势， 使她被火吞噬。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！主必奪去她的一切， 摧毀她海上的勢力， 她必被火吞滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主必趕出她， 打敗她海上的權利； 她必被火燒滅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, the Lord will dispossess her And cast her wealth into the sea; And she will be consumed with fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the Lord will take away her possessions and destroy her power on the sea, and she will be consumed by fire."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But now the Lord will strip away Tyre’s possessions and hurl its fortifications into the sea, and it will be burned to the ground."
      }
