    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚实基伦见状必陷入恐惧， 迦萨见状必充满痛苦， 以革伦见状必希望破灭。 迦萨必失去君王， 亚实基伦必荒无人烟。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞實基倫看見了，就必害怕； 迦薩看見了也必非常痛苦； 以革倫因失去盼望而蒙羞。 迦薩的王必滅亡， 亞實基倫必不再有人居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞實基倫看見必懼怕； 迦薩看見甚痛苦； 以革倫因失了盼望蒙羞。 迦薩必不再有君王； 亞實基倫也不再有居民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ashkelon will see it and be afraid. Gaza too will writhe in great pain; Also Ekron, for her expectation has been confounded. Moreover, the king will perish from Gaza, And Ashkelon will not be inhabited."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ashkelon will see it and fear; Gaza will writhe in agony, and Ekron too, for her hope will wither. Gaza will lose her king and Ashkelon will be deserted."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The city of Ashkelon will see Tyre fall and will be filled with fear. Gaza will shake with terror, as will Ekron, for their hopes will be dashed. Gaza’s king will be killed, and Ashkelon will be deserted."
      }
    },
