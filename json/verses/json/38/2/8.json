    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华说，祂在彰显荣耀之后要差遣我去那些掳掠你们的国家，因为谁侵害你们，就是侵害祂的瞳仁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華這樣說，得了榮耀之後，他差遣我去攻擊那些劫掠你們的列國，因為凡觸摸你們的，就是觸摸他眼中的瞳人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華說，在顯出榮耀之後，差遣我去懲罰那擄掠你們的列國，摸你們的就是摸他眼中的瞳人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus says the Lord of hosts, “After glory He has sent me against the nations which plunder you, for he who touches you, touches the apple of His eye."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this is what the Lord Almighty says: “After the Glorious One has sent me against the nations that have plundered you—for whoever touches you touches the apple of his eye—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After a period of glory, the Lord of Heaven’s Armies sent me against the nations who plundered you. For he said, “Anyone who harms you harms my most precious possession."
      }
    },
