    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，祂要挥手攻击他们，使他们被自己的奴隶掳掠。那时你们便知道是万军之耶和华差遣了我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！我要向他們揮動我的手，他們就必成了自己奴僕的掠物。這樣，你們就知道萬軍之耶和華差遣了我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，我要向他們掄手，他們就必作服事他們之人的擄物，你們便知道萬軍之耶和華差遣我了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For behold, I will wave My hand over them so that they will be plunder for their slaves. Then you will know that the Lord of hosts has sent Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will surely raise my hand against them so that their slaves will plunder them. Then you will know that the Lord Almighty has sent me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will raise my fist to crush them, and their own slaves will plunder them.” Then you will know that the Lord of Heaven’s Armies has sent me."
      }
    },
