    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说，我要成为一堵火墙环绕耶路撒冷，作城中的荣耀。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：我要作耶路撒冷四圍的火牆和城中的榮耀。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：我要作耶路撒冷四圍的火城，並要作其中的榮耀。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I,’ declares the Lord, ‘will be a wall of fire around her, and I will be the glory in her midst.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And I myself will be a wall of fire around it,’ declares the Lord, ‘and I will be its glory within.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I, myself, will be a protective wall of fire around Jerusalem, says the Lord. And I will be the glory inside the city!’”"
      }
    },
