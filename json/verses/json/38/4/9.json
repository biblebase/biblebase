    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“所罗巴伯的手奠立这殿的根基，他的手也必完成这项工程。那时你便知道是万军之耶和华差遣我到你们这里来的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“所羅巴伯的手奠立了這殿的根基，他的手也必完成這工；這樣，你就知道萬軍之耶和華差遣了我到你們這裡來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「所羅巴伯的手立了這殿的根基，他的手也必完成這工，你就知道萬軍之耶和華差遣我到你們這裏來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The hands of Zerubbabel have laid the foundation of this house, and his hands will finish it. Then you will know that the Lord of hosts has sent me to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The hands of Zerubbabel have laid the foundation of this temple; his hands will also complete it. Then you will know that the Lord Almighty has sent me to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Zerubbabel is the one who laid the foundation of this Temple, and he will complete it. Then you will know that the Lord of Heaven’s Armies has sent me."
      }
    },
