    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当你们在自己的土地上跟敌人作战时，要吹号，你们的上帝耶和华必眷顾你们，救你们脱离敌人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們在本地與欺壓你們的仇敵作戰的時候，就要吹大聲，使你們在耶和華你們的　神面前得蒙記念，也得拯救脫離你們的仇敵。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們在自己的地，與欺壓你們的敵人打仗，就要用號吹出大聲，便在耶和華－你們的神面前得蒙紀念，也蒙拯救脫離仇敵。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you go to war in your land against the adversary who attacks you, then you shall sound an alarm with the trumpets, that you may be remembered before the Lord your God, and be saved from your enemies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you go into battle in your own land against an enemy who is oppressing you, sound a blast on the trumpets. Then you will be remembered by the Lord your God and rescued from your enemies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When you arrive in your own land and go to war against your enemies who attack you, sound the alarm with the trumpets. Then the Lord your God will remember you and rescue you from your enemies."
      }
    },
