    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第二次吹响拔营号令时，在南面扎营的就要启行。拔营号令是启行的信号。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們第二次吹大聲的時候，那在南邊的營，都要起行；他們要起行，必須吹大聲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "二次吹出大聲的時候，南邊安的營都要起行。他們將起行，必吹出大聲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you blow an alarm the second time, the camps that are pitched on the south side shall set out; an alarm is to be blown for them to set out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At the sounding of a second blast, the camps on the south are to set out. The blast will be the signal for setting out."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When you sound the signal a second time, the tribes camped on the south will follow. You must sound short blasts as the signal for moving on."
      }
    },
