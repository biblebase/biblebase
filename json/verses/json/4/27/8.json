    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要告诉以色列人，‘人死后如果没有儿子，就由女儿继承产业；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你也要告訴以色列人說：‘一個人死時，如果沒有兒子，就要使他的產業歸給他的女兒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你也要曉諭以色列人說：『人若死了沒有兒子，就要把他的產業歸給他的女兒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Further, you shall speak to the sons of Israel, saying, ‘If a man dies and has no son, then you shall transfer his inheritance to his daughter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Say to the Israelites, ‘If a man dies and leaves no son, give his inheritance to his daughter."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And give the following instructions to the people of Israel: If a man dies and has no son, then give his inheritance to his daughters."
      }
