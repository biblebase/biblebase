    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，米利暗被关在营外七天，民众等她回营后才继续前行。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是米利暗被關在營外七天；人民沒有啟程，直到把米利暗接回來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是米利暗關鎖在營外七天。百姓沒有行路，直等到把米利暗領進來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Miriam was shut up outside the camp for seven days, and the people did not move on until Miriam was received again."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Miriam was confined outside the camp for seven days, and the people did not move on till she was brought back."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Miriam was kept outside the camp for seven days, and the people waited until she was brought back before they traveled again."
      }
    },
