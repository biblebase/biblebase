    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一定要赶走当地的居民，毁掉他们一切的石像和铸像，拆毁他们所有的丘坛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就要把所有的居民從你們面前趕出去，毀壞他們的一切雕像，以及一切鑄像，又拆毀他們的一切邱壇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就要從你們面前趕出那裏所有的居民，毀滅他們一切鏨成的石像和他們一切鑄成的偶像，又拆毀他們一切的邱壇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then you shall drive out all the inhabitants of the land from before you, and destroy all their figured stones, and destroy all their molten images and demolish all their high places;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "drive out all the inhabitants of the land before you. Destroy all their carved images and their cast idols, and demolish all their high places."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "you must drive out all the people living there. You must destroy all their carved and molten images and demolish all their pagan shrines."
      }
    },
