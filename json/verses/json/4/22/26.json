    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的天使又走到前面，站在狭窄之处，两边都无侧身通过的余地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的使者又往前行，站在狹窄之處，左右都沒有轉身的地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的使者又往前去，站在狹窄之處，左右都沒有轉折的地方。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The angel of the Lord went further, and stood in a narrow place where there was no way to turn to the right hand or the left."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the angel of the Lord moved on ahead and stood in a narrow place where there was no room to turn, either to the right or to the left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the angel of the Lord moved farther down the road and stood in a place too narrow for the donkey to get by at all."
      }
    },
