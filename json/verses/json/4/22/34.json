    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴兰对耶和华的天使说：“我有罪了，我不知道是你拦在路上。如果你反对我去，我就回去。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴蘭對耶和華的使者說：“我有罪了，我不知道是你站在路上阻擋我；現在你若是不喜歡我去，我就回去。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴蘭對耶和華的使者說：「我有罪了。我不知道你站在路上阻擋我；你若不喜歡我去，我就轉回。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Balaam said to the angel of the Lord, “I have sinned, for I did not know that you were standing in the way against me. Now then, if it is displeasing to you, I will turn back.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Balaam said to the angel of the Lord, “I have sinned. I did not realize you were standing in the road to oppose me. Now if you are displeased, I will go back.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Balaam confessed to the angel of the Lord, “I have sinned. I didn’t realize you were standing in the road to block my way. I will return home if you are against my going.”"
      }
    },
