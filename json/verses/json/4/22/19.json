    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们可以在这里过夜，看看耶和华对我还有什么吩咐。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在請你們今夜也住在這裡，等我得知耶和華還要向我說甚麼。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在我請你們今夜在這裏住宿，等我得知耶和華還要對我說甚麼。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now please, you also stay here tonight, and I will find out what else the Lord will speak to me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now spend the night here so that I can find out what else the Lord will tell me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But stay here one more night, and I will see if the Lord has anything else to say to me.”"
      }
