    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "驴看见耶和华的天使，就靠着墙边走，将巴兰的脚挤伤了。巴兰又鞭打它。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "驢看見了耶和華的使者，就緊擠在牆上，把巴蘭的腳擠傷了，巴蘭又打驢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "驢看見耶和華的使者，就貼靠牆，將巴蘭的腳擠傷了；巴蘭又打驢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the donkey saw the angel of the Lord, she pressed herself to the wall and pressed Balaam’s foot against the wall, so he struck her again."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the donkey saw the angel of the Lord, it pressed close to the wall, crushing Balaam’s foot against it. So he beat the donkey again."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the donkey saw the angel of the Lord, it tried to squeeze by and crushed Balaam’s foot against the wall. So Balaam beat the donkey again."
      }
    },
