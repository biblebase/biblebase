    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴兰说：“因为你戏弄我。如果我手中有刀，早就把你宰了！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴蘭對驢說：“因為你作弄我。但願我手中有刀，現在就把你殺死。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴蘭對驢說：「因為你戲弄我，我恨不能手中有刀，把你殺了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Balaam said to the donkey, “Because you have made a mockery of me! If there had been a sword in my hand, I would have killed you by now.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Balaam answered the donkey, “You have made a fool of me! If only I had a sword in my hand, I would kill you right now.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You have made me look like a fool!” Balaam shouted. “If I had a sword with me, I would kill you!”"
      }
    },
