    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华使驴开口对巴兰说：“我做错了什么，你竟打我三次！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華開了驢的口，驢就對巴蘭說：“我向你作了甚麼，你竟打我這三次呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華叫驢開口，對巴蘭說：「我向你行了甚麼，你竟打我這三次呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the Lord opened the mouth of the donkey, and she said to Balaam, “What have I done to you, that you have struck me these three times?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the Lord opened the donkey’s mouth, and it said to Balaam, “What have I done to you to make you beat me these three times?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord gave the donkey the ability to speak. “What have I done to you that deserves your beating me three times?” it asked Balaam."
      }
    },
