    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对摩西说：“你要统计以色列人中年龄在一个月以上的长子，登记他们的姓名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對摩西說：“以色列人中所有頭生的男子，一個月及以上的，你都要數點；登記他們的姓名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對摩西說：「你要從以色列人中數點一個月以外、凡頭生的男子，把他們的名字記下。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord said to Moses, “Number every firstborn male of the sons of Israel from a month old and upward, and make a list of their names."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said to Moses, “Count all the firstborn Israelite males who are a month old or more and make a list of their names."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord said to Moses, “Now count all the firstborn sons in Israel who are one month old or older, and make a list of their names."
      }
    },
