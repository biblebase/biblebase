    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以上照宗族和队伍统计的以色列人共六十万三千五百五十名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以上是照著他們的父家被數點的以色列人；所有按著他們的隊伍在眾營中被數點的，共有六十萬零三千五百五十人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些以色列人，照他們的宗族，按他們的軍隊，在諸營中被數的，共有六十萬零三千五百五十名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These are the numbered men of the sons of Israel by their fathers’ households; the total of the numbered men of the camps by their armies, 603,550."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These are the Israelites, counted according to their families. All the men in the camps, by their divisions, number 603,550."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In summary, the troops of Israel listed by their families totaled 603,550."
      }
    },
