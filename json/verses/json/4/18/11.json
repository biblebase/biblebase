    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人献的举祭和摇祭都归你，我已把它们赐给你和你的儿女，这是永远的定例。你家中所有洁净的人都可以吃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人禮物中的舉祭，與他們的搖祭，都是你的；我已經給了你和你的兒女，作為永遠應得的分；在你家中潔淨的人都可以吃。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人所獻的舉祭並搖祭都是你的；我已賜給你和你的兒女，當作永得的分；凡在你家中的潔淨人都可以吃。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This also is yours, the offering of their gift, even all the wave offerings of the sons of Israel; I have given them to you and to your sons and daughters with you as a perpetual allotment. Everyone of your household who is clean may eat it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This also is yours: whatever is set aside from the gifts of all the wave offerings of the Israelites. I give this to you and your sons and daughters as your perpetual share. Everyone in your household who is ceremonially clean may eat it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“All the sacred offerings and special offerings presented to me when the Israelites lift them up before the altar also belong to you. I have given them to you and to your sons and daughters as your permanent share. Any member of your family who is ceremonially clean may eat of these offerings."
      }
    },
