      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要把所得之物中至圣至好的献给耶和华作举祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從你們得的一切禮物中，要把最好的，分別為聖，獻給耶和華為舉祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "奉給你們的一切禮物，要從其中將至好的，就是分別為聖的，獻給耶和華為舉祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Out of all your gifts you shall present every offering due to the Lord, from all the best of them, the sacred part from them.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You must present as the Lord’s portion the best and holiest part of everything given to you.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Be sure to give to the Lord the best portions of the gifts given to you."
      }
