    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我要把以色列人出产的十分之一赐给利未人作产业，作为他们在会幕司职的酬劳。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“至於利未的子孫，我已經把以色列中所有的十分之一，賜給了他們作產業，這是為酬報他們在會幕裡辦理的事務。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「凡以色列中出產的十分之一，我已賜給利未的子孫為業；因他們所辦的是會幕的事，所以賜給他們為酬他們的勞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“To the sons of Levi, behold, I have given all the tithe in Israel for an inheritance, in return for their service which they perform, the service of the tent of meeting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I give to the Levites all the tithes in Israel as their inheritance in return for the work they do while serving at the tent of meeting."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As for the tribe of Levi, your relatives, I will compensate them for their service in the Tabernacle. Instead of an allotment of land, I will give them the tithes from the entire land of Israel."
      }
    },
