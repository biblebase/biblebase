    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们口中的肉还没嚼烂，耶和华的怒气就已向他们发作，降下了大灾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "肉在他們牙齒之間，還沒有嚼爛，耶和華的怒氣就向人民發作，用極重的災禍擊打他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "肉在他們牙齒之間尚未嚼爛，耶和華的怒氣就向他們發作，用最重的災殃擊殺了他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While the meat was still between their teeth, before it was chewed, the anger of the Lord was kindled against the people, and the Lord struck the people with a very severe plague."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But while the meat was still between their teeth and before it could be consumed, the anger of the Lord burned against the people, and he struck them with a severe plague."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But while they were gorging themselves on the meat—while it was still in their mouths—the anger of the Lord blazed against the people, and he struck them with a severe plague."
      }
    },
