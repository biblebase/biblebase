    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可玷污你们所居住的土地，因为这是我居住的地方，我耶和华住在以色列人中间。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不可玷污你所住的地，就是我居住的地，因為我耶和華是居住在以色列人中間的。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們不可玷污所住之地，就是我住在其中之地，因為我－耶和華住在以色列人中間。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not defile the land in which you live, in the midst of which I dwell; for I the Lord am dwelling in the midst of the sons of Israel.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not defile the land where you live and where I dwell, for I, the Lord, dwell among the Israelites.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must not defile the land where you live, for I live there myself. I am the Lord, who lives among the people of Israel.”"
      }
    },
