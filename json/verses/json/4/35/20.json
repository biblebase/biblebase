    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若有人因怨恨把人推倒致死，或故意扔东西砸人致死，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果人因懷恨把人推倒，或是埋伏著向人扔東西，以致把人打死；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人若因怨恨把人推倒，或是埋伏往人身上扔物，以致於死，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If he pushed him of hatred, or threw something at him lying in wait and as a result he died,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If anyone with malice aforethought shoves another or throws something at them intentionally so that they die"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So if someone hates another person and waits in ambush, then pushes him or throws something at him and he dies, it is murder."
      }
    },
