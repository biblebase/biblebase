    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可收逃进避难城之人的赎金，让他在大祭司逝世前返回家园。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "逃到避難城的人，你們也不可收取他的贖價，使他在大祭司未死以前回到本地居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那逃到逃城的人，你們不可為他收贖價，使他在大祭司未死以先再來住在本地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not take ransom for him who has fled to his city of refuge, that he may return to live in the land before the death of the priest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Do not accept a ransom for anyone who has fled to a city of refuge and so allow them to go back and live on their own land before the death of the high priest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And never accept a ransom payment from someone who has fled to a city of refuge, allowing a slayer to return to his property before the death of the high priest."
      }
    },
