    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对摩西说：“你去造一条蛇挂在杆子上，凡被咬的一望这蛇，就可活命。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對摩西說：“你要做一條火蛇，掛在杆上；被咬的，一看這蛇，就必得存活。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對摩西說：「你製造一條火蛇，掛在杆子上；凡被咬的，一望這蛇，就必得活。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord said to Moses, “Make a fiery serpent, and set it on a standard; and it shall come about, that everyone who is bitten, when he looks at it, he will live.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said to Moses, “Make a snake and put it up on a pole; anyone who is bitten can look at it and live.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord told him, “Make a replica of a poisonous snake and attach it to a pole. All who are bitten will live if they simply look at it!”"
      }
    },
