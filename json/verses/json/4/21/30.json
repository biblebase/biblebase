    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我们击溃了亚摩利人， 从希实本一直到底本都被摧毁， 从挪法到米底巴都沦为废墟。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們射擊他們， 希實本就毀滅，直到底本。 我們使地荒涼，直到挪法， 挪法直伸到米底巴。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們射了他們； 希實本直到底本盡皆毀滅。 我們使地變成荒場，直到挪法； 這挪法直延到米底巴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But we have cast them down, Heshbon is ruined as far as Dibon, Then we have laid waste even to Nophah, Which reaches to Medeba.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But we have overthrown them; Heshbon’s dominion has been destroyed all the way to Dibon. We have demolished them as far as Nophah, which extends to Medeba.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We have utterly destroyed them, from Heshbon to Dibon. We have completely wiped them out as far away as Nophah and Medeba.”"
      }
    },
