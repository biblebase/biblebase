    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦就照耶和华对摩西的吩咐把灯放好，使灯照向前方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫就這樣行了；他把燈臺上的燈都點著了，使光照向燈臺的前面，正如耶和華吩咐摩西的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫便這樣行。他點燈臺上的燈，使燈向前發光，是照耶和華所吩咐摩西的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Aaron therefore did so; he mounted its lamps at the front of the lampstand, just as the Lord had commanded Moses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Aaron did so; he set up the lamps so that they faced forward on the lampstand, just as the Lord commanded Moses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Aaron did this. He set up the seven lamps so they reflected their light forward, just as the Lord had commanded Moses."
      }
    },
