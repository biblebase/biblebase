    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无论是以色列人或是寄居的外族人，凡是犯无心之过的都要按这规例办理。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對於犯了無心之失的人，無論是本地人或是寄居在他們中間的外人，都有一樣的法例。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列中的本地人和寄居在他們中間的外人，若誤行了甚麼事，必歸一樣的條例。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall have one law for him who does anything unintentionally, for him who is native among the sons of Israel and for the alien who sojourns among them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One and the same law applies to everyone who sins unintentionally, whether a native-born Israelite or a foreigner residing among you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These same instructions apply both to native-born Israelites and to the foreigners living among you."
      }
    },
