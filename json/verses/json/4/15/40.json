      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使你們記念遵行我一切的命令，成為聖潔，歸與你們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that you may remember to do all My commandments and be holy to your God."
      },
