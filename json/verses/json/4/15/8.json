      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果用公牛作燔祭、平安祭、还愿祭或别的祭献给耶和华，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你若是預備公牛犢作燔祭，或是別的祭，為要還特許的願，或是作平安祭，獻給耶和華，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你預備公牛作燔祭，或是作平安祭，為要還特許的願，或是作平安祭，獻給耶和華，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you prepare a bull as a burnt offering or a sacrifice, to fulfill a special vow, or for peace offerings to the Lord,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘When you prepare a young bull as a burnt offering or sacrifice, for a special vow or a fellowship offering to the Lord,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When you present a young bull as a burnt offering or as a sacrifice to fulfill a vow or as a peace offering to the Lord,"
      }
