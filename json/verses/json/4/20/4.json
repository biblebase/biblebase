      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們為何把耶和華的會眾領到這曠野、使我們和牲畜都死在這裏呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why then have you brought the Lord’s assembly into this wilderness, for us and our beasts to die here?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why did you bring the Lord’s community into this wilderness, that we and our livestock should die here?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why have you brought the congregation of the Lord’s people into this wilderness to die, along with all our livestock?"
      }
