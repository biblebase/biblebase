    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人说：“我们只走大路，倘若我们和牲畜喝了你的水，我们会付钱。我们别无他求，只求步行穿过贵国。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人對他說：“我們要走大路上去，我們和牲畜若是喝了你的水，我必照價付你；不求別的事，只求你容我們步行經過。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人說：「我們要走大道上去；我們和牲畜若喝你的水，必給你價值。不求別的，只求你容我們步行過去。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Again, the sons of Israel said to him, “We will go up by the highway, and if I and my livestock do drink any of your water, then I will pay its price. Let me only pass through on my feet, nothing else.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Israelites replied: “We will go along the main road, and if we or our livestock drink any of your water, we will pay for it. We only want to pass through on foot—nothing else.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Israelites answered, “We will stay on the main road. If our livestock drink your water, we will pay for it. Just let us pass through your country. That’s all we ask.”"
      }
    },
