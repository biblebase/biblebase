    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他领巴兰上到毗斯迦山顶的琐腓田，在那里筑了七座祭坛，每座坛上都献了一头公牛和一只公绵羊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是巴勒帶巴蘭到了瑣腓田，上了毘斯迦山頂，築了七座祭壇，在每座祭壇上獻了一頭公牛和一隻公綿羊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是領巴蘭到了瑣腓田，上了毗斯迦山頂，築了七座壇；每座壇上獻一隻公牛，一隻公羊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he took him to the field of Zophim, to the top of Pisgah, and built seven altars and offered a bull and a ram on each altar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So he took him to the field of Zophim on the top of Pisgah, and there he built seven altars and offered a bull and a ram on each altar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Balak took Balaam to the plateau of Zophim on Pisgah Peak. He built seven altars there and offered a young bull and a ram on each altar."
      }
    },
