      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人虏获了米甸的妇女、孩子、牛羊和财物，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人擄了米甸人的婦女、孩子；又擄掠了他們所有的牲畜、羊群和財物；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人擄了米甸人的婦女孩子，並將他們的牲畜、羊羣，和所有的財物都奪了來，當作擄物，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sons of Israel captured the women of Midian and their little ones; and all their cattle and all their flocks and all their goods they plundered."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Israelites captured the Midianite women and children and took all the Midianite herds, flocks and goods as plunder."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Israelite army captured the Midianite women and children and seized their cattle and flocks and all their wealth as plunder."
      }
    },
