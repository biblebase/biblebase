    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就是她们听了巴兰的指使在毗珥引诱以色列人背叛耶和华，以致瘟疫降在耶和华的会众身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪，這些婦女因巴蘭的計謀，使以色列人在毘珥的事上，得罪耶和華，以致瘟疫臨到耶和華的會眾身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些婦女因巴蘭的計謀，叫以色列人在毗珥的事上得罪耶和華，以致耶和華的會眾遭遇瘟疫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, these caused the sons of Israel, through the counsel of Balaam, to trespass against the Lord in the matter of Peor, so the plague was among the congregation of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“They were the ones who followed Balaam’s advice and enticed the Israelites to be unfaithful to the Lord in the Peor incident, so that a plague struck the Lord’s people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“These are the very ones who followed Balaam’s advice and caused the people of Israel to rebel against the Lord at Mount Peor. They are the ones who caused the plague to strike the Lord’s people."
      }
    },
