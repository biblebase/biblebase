    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果有人在他身边暴毙，使他的头发沾染不洁，他要等到第七天洁净的日子剃掉头发。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘如果在他身旁，突然有人死了，以致他離俗的頭被玷污了，他就要在得潔淨的日子剃頭，就是在第七日剃頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「若在他旁邊忽然有人死了，以致沾染了他離俗的頭，他要在第七日，得潔淨的時候，剃頭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘But if a man dies very suddenly beside him and he defiles his dedicated head of hair, then he shall shave his head on the day when he becomes clean; he shall shave it on the seventh day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘If someone dies suddenly in the Nazirite’s presence, thus defiling the hair that symbolizes their dedication, they must shave their head on the seventh day—the day of their cleansing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If someone falls dead beside them, the hair they have dedicated will be defiled. They must wait for seven days and then shave their heads. Then they will be cleansed from their defilement."
      }
    },
