    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“献给耶和华的日子期满后，拿细耳人要遵行以下条例。他要被带到会幕门口，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘拿細耳人的律例是這樣：離俗的人的日子滿了那天，人要把他帶到會幕的門口；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「拿細耳人滿了離俗的日子乃有這條例：人要領他到會幕門口，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Now this is the law of the Nazirite when the days of his separation are fulfilled, he shall bring the offering to the doorway of the tent of meeting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Now this is the law of the Nazirite when the period of their dedication is over. They are to be brought to the entrance to the tent of meeting."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is the ritual law for Nazirites. At the conclusion of their time of separation as Nazirites, they must each go to the entrance of the Tabernacle"
      }
    },
