    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若有人死在帐篷里，当时走进帐篷的或在场的人都七天不洁净。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果人在會幕裡死了，條例是這樣的：進這會幕的和一切在會幕裡的，都必不潔淨七天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人死在帳棚裏的條例乃是這樣：凡進那帳棚的，和一切在帳棚裏的，都必七天不潔淨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘This is the law when a man dies in a tent: everyone who comes into the tent and everyone who is in the tent shall be unclean for seven days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is the law that applies when a person dies in a tent: Anyone who enters the tent and anyone who is in it will be unclean for seven days,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is the ritual law that applies when someone dies inside a tent: All those who enter that tent and those who were inside when the death occurred will be ceremonially unclean for seven days."
      }
    },
