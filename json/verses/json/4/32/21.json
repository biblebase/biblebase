    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们全军就要在耶和华面前渡过约旦河，直到祂赶走祂面前的仇敌，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們所有武裝起來的人，都要在耶和華面前過約旦河，直到他把他的仇敵從自己面前趕出去，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所有帶兵器的人都要在耶和華面前過約旦河，等他趕出他的仇敵，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and all of you armed men cross over the Jordan before the Lord until He has driven His enemies out from before Him,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and if all of you who are armed cross over the Jordan before the Lord until he has driven his enemies out before him—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and if your troops cross the Jordan and keep fighting until the Lord has driven out his enemies,"
      }
