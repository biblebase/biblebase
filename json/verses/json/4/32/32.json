    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们一定带着兵器在耶和华面前过河，前往迦南，但我们要拥有约旦河东之地作产业。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們要武裝起來，在耶和華面前過河，到迦南地去；只是約旦河這邊的地方，要歸我們作產業。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們要帶兵器，在耶和華面前過去，進入迦南地，只是約旦河這邊、我們所得為業之地仍歸我們。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We ourselves will cross over armed in the presence of the Lord into the land of Canaan, and the possession of our inheritance shall remain with us across the Jordan.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We will cross over before the Lord into Canaan armed, but the property we inherit will be on this side of the Jordan.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We will cross the Jordan into Canaan fully armed to fight for the Lord, but our property will be here on this side of the Jordan.”"
      }
    },
