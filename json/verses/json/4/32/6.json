    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西对他们说：“难道你们要坐在这里，让其他同胞去打仗吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西對迦得子孫和流本子孫說：“難道你們的兄弟去打仗，你們卻坐在這裡嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西對迦得子孫和呂便子孫說：「難道你們的弟兄去打仗，你們竟坐在這裏嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Moses said to the sons of Gad and to the sons of Reuben, “Shall your brothers go to war while you yourselves sit here?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moses said to the Gadites and Reubenites, “Should your fellow Israelites go to war while you sit here?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do you intend to stay here while your brothers go across and do all the fighting?” Moses asked the men of Gad and Reuben."
      }
