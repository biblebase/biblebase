    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你们离弃祂，祂必再次把以色列人丢弃在旷野，这样你们就毁了整个民族。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們轉離，不跟從他，他再要把以色列人撇在曠野，這樣，你們就要把整個民族都毀滅了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若退後不跟從他，他還要把以色列人撇在曠野，便是你們使這眾民滅亡。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if you turn away from following Him, He will once more abandon them in the wilderness, and you will destroy all these people.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you turn away from following him, he will again leave all this people in the wilderness, and you will be the cause of their destruction.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you turn away from him like this and he abandons them again in the wilderness, you will be responsible for destroying this entire nation!”"
      }
    },
