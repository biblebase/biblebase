      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「惟獨利未支派你不可數點，也不可在以色列人中計算他們的總數。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Only the tribe of Levi you shall not number, nor shall you take their census among the sons of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You must not count the tribe of Levi or include them in the census of the other Israelites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do not include the tribe of Levi in the registration; do not count them with the rest of the Israelites."
      }
