    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果这些人像常人一样死去，遭遇跟世人无异，我就不是耶和华派来的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果這些人死亡像一般人死亡一樣，或是他們所遭遇的像一般人的遭遇一樣，那麼就不是耶和華派我來的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些人死若與世人無異，或是他們所遭的與世人相同，就不是耶和華打發我來的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If these men die the death of all men or if they suffer the fate of all men, then the Lord has not sent me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If these men die a natural death and suffer the fate of all mankind, then the Lord has not sent me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If these men die a natural death, or if nothing unusual happens, then the Lord has not sent me."
      }
    },
