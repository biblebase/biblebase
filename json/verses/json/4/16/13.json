    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你带我们离开奶蜜之乡，要让我们死在旷野，这还不够吗？你还想自立为王管辖我们吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你把我們從流奶與蜜的地帶上來，要在曠野殺死我們，這算小事嗎？你還要自立為王管轄我們嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你將我們從流奶與蜜之地領上來，要在曠野殺我們，這豈為小事？你還要自立為王轄管我們嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Is it not enough that you have brought us up out of a land flowing with milk and honey to have us die in the wilderness, but you would also lord it over us?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Isn’t it enough that you have brought us up out of a land flowing with milk and honey to kill us in the wilderness? And now you also want to lord it over us!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Isn’t it enough that you brought us out of Egypt, a land flowing with milk and honey, to kill us here in this wilderness, and that you now treat us like your subjects?"
      }
    },
