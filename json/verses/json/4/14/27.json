    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“这邪恶的会众向我发怨言要到何时呢？我已听见以色列人对我的埋怨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“這惡會眾向我發怨言要到幾時呢？以色列人向我所發的怨言，我都聽見了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「這惡會眾向我發怨言，我忍耐他們要到幾時呢？以色列人向我所發的怨言，我都聽見了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“How long shall I bear with this evil congregation who are grumbling against Me? I have heard the complaints of the sons of Israel, which they are making against Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“How long will this wicked community grumble against me? I have heard the complaints of these grumbling Israelites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“How long must I put up with this wicked community and its complaints about me? Yes, I have heard the complaints the Israelites are making against me."
      }
    },
