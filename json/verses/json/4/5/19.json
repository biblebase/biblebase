    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "叫她发誓。祭司要对她说，‘如果你没有出轨，没有背着丈夫与人通奸，就不会被这苦水所害。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司要叫那婦人起誓，對她說：‘如果沒有人與你同寢，作妻子時沒有背棄丈夫被人玷污，你就免受這使人招致咒詛的苦水。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要叫婦人起誓，對她說：『若沒有人與你行淫，也未曾背着丈夫做污穢的事，你就免受這致咒詛苦水的災。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The priest shall have her take an oath and shall say to the woman, “If no man has lain with you and if you have not gone astray into uncleanness, being under the authority of your husband, be immune to this water of bitterness that brings a curse;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the priest shall put the woman under oath and say to her, “If no other man has had sexual relations with you and you have not gone astray and become impure while married to your husband, may this bitter water that brings a curse not harm you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The priest will then put the woman under oath and say to her, ‘If no other man has had sex with you, and you have not gone astray and defiled yourself while under your husband’s authority, may you be immune from the effects of this bitter water that brings on the curse."
      }
    },
