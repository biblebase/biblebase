    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你吩咐以色列人把所有患麻风病、漏症和因碰过死尸而不洁净的人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要吩咐以色列人，把所有患痲風病的，患漏症的，以及因接觸死人而不潔的，都送出營外；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你吩咐以色列人，使一切長大痲瘋的，患漏症的，並因死屍不潔淨的，都出營外去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Command the sons of Israel that they send away from the camp every leper and everyone having a discharge and everyone who is unclean because of a dead person."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Command the Israelites to send away from the camp anyone who has a defiling skin disease or a discharge of any kind, or who is ceremonially unclean because of a dead body."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Command the people of Israel to remove from the camp anyone who has a skin disease or a discharge, or who has become ceremonially unclean by touching a dead person."
      }
    },
