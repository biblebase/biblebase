    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但如果她是贞洁的，没有与人通奸，就不会受伤害，仍能生育。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是，如果這婦人沒有被玷污，而是貞潔的，就不致受害，並且要懷孕生子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若婦人沒有被玷污，卻是清潔的，就要免受這災，且要懷孕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But if the woman has not defiled herself and is clean, she will then be free and conceive children."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If, however, the woman has not made herself impure, but is clean, she will be cleared of guilt and will be able to have children."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if she has not defiled herself and is pure, then she will be unharmed and will still be able to have children."
      }
    },
