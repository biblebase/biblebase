    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "没有人会厌恶自己的身体，人总是珍惜保养身体，正如基督对待教会一样，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從來沒有人恨惡自己的身體，總是保養顧惜，好像基督對教會一樣，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從來沒有人恨惡自己的身子，總是保養顧惜，正像基督待教會一樣，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "for no one ever hated his own flesh, but nourishes and cherishes it, just as Christ also does the church,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After all, no one ever hated their own body, but they feed and care for their body, just as Christ does the church—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No one hates his own body but feeds and cares for it, just as Christ cares for the church."
      }
