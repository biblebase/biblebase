    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "等所定的时候一到，叫天地万物一同归在基督的名下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了所計劃的時機成熟，就使天上地上的萬有，都在基督裡同歸於一。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要照所安排的，在日期滿足的時候，使天上、地上、一切所有的都在基督裏面同歸於一。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "with a view to an administration suitable to the fullness of the times, that is, the summing up of all things in Christ, things in the heavens and things on the earth. In Him"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "to be put into effect when the times reach their fulfillment—to bring unity to all things in heaven and on earth under Christ."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And this is the plan: At the right time he will bring everything together under the authority of Christ—everything in heaven and on earth."
      }
    },
