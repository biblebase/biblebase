    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我听到你们对主耶稣的信心和对众圣徒的爱心后，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，我聽到你們在主耶穌裡的信心，和對眾聖徒的愛心，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，我既聽見你們信從主耶穌，親愛眾聖徒，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For this reason I too, having heard of the faith in the Lord Jesus which exists among you and your love for all the saints,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this reason, ever since I heard about your faith in the Lord Jesus and your love for all God’s people,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ever since I first heard of your strong faith in the Lord Jesus and your love for God’s people everywhere,"
      }
