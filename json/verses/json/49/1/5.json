    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝因为爱我们，就按照祂自己美好的旨意，预定我们借着耶稣基督得到做祂儿女的名分。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又按著自己旨意所喜悅的，預定我們藉著耶穌基督得兒子的名分，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又因愛我們，就按着自己的意旨所喜悅的，預定我們藉着耶穌基督得兒子的名分，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He predestined us to adoption as sons through Jesus Christ to Himself, according to the kind intention of His will,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "he predestined us for adoption to sonship through Jesus Christ, in accordance with his pleasure and will—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God decided in advance to adopt us into his own family by bringing us to himself through Jesus Christ. This is what he wanted to do, and it gave him great pleasure."
      }
    },
