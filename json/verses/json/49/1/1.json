    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是奉上帝旨意做基督耶稣使徒的保罗，写信给以弗所忠于基督耶稣的众圣徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "奉　神旨意作基督耶穌使徒的保羅，寫信給住在以弗所，在基督耶穌裡忠心的聖徒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "奉神旨意，作基督耶穌使徒的保羅，寫信給在以弗所的聖徒，就是在基督耶穌裏有忠心的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Paul, an apostle of Christ Jesus by the will of God, To the saints who are at Ephesus and who are faithful in Christ Jesus:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Paul, an apostle of Christ Jesus by the will of God, To God’s holy people in Ephesus, the faithful in Christ Jesus:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This letter is from Paul, chosen by the will of God to be an apostle of Christ Jesus. I am writing to God’s holy people in Ephesus, who are faithful followers of Christ Jesus."
      }
    },
