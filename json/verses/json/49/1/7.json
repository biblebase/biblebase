    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们借着祂爱子的血蒙救赎，过犯得到赦免，这都是出于祂的洪恩。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們在他愛子裡，藉著他的血蒙了救贖，過犯得到赦免，都是按著他豐盛的恩典。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們藉這愛子的血得蒙救贖，過犯得以赦免，乃是照他豐富的恩典。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In Him we have redemption through His blood, the forgiveness of our trespasses, according to the riches of His grace"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In him we have redemption through his blood, the forgiveness of sins, in accordance with the riches of God’s grace"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He is so rich in kindness and grace that he purchased our freedom with the blood of his Son and forgave our sins."
      }
    },
