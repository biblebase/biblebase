    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂又使万物降服在基督脚下，使基督为教会做万物的元首。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神又使萬有都歸服在他的腳下，並且賜他給教會作萬有的元首。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又將萬有服在他的腳下，使他為教會作萬有之首。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He put all things in subjection under His feet, and gave Him as head over all things to the church,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And God placed all things under his feet and appointed him to be head over everything for the church,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God has put all things under the authority of Christ and has made him head over all things for the benefit of the church."
      }
    },
