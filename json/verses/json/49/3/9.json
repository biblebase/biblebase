    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "让全人类都明白世代隐藏在创造万物的上帝里面的奥秘，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "並且使眾人明白那奧祕的救世計劃是甚麼，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又使眾人都明白，這歷代以來隱藏在創造萬物之神裏的奧祕是如何安排的，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and to bring to light what is the administration of the mystery which for ages has been hidden in God who created all things;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and to make plain to everyone the administration of this mystery, which for ages past was kept hidden in God, who created all things."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I was chosen to explain to everyone this mysterious plan that God, the Creator of all things, had kept secret from the beginning."
      }
    },
