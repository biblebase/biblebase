    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "相信你们已经听说了，上帝委派我将祂的恩典传给你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "想必你們聽過　神恩惠的計劃，他為你們的緣故賜恩給我，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "諒必你們曾聽見神賜恩給我，將關切你們的職分託付我，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "if indeed you have heard of the stewardship of God’s grace which was given to me for you;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely you have heard about the administration of God’s grace that was given to me for you,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "assuming, by the way, that you know God gave me the special responsibility of extending his grace to you Gentiles."
      }
    },
