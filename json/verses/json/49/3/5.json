    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "自古以来，上帝没有让人知道这奥秘，如今祂借着圣灵将这奥秘启示给祂的圣使徒和先知。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這奧祕在以前的世代並沒有讓世人知道，不像現在藉著聖靈啟示了聖使徒和先知那樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這奧祕在以前的世代沒有叫人知道，像如今藉着聖靈啟示他的聖使徒和先知一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "which in other generations was not made known to the sons of men, as it has now been revealed to His holy apostles and prophets in the Spirit;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "which was not made known to people in other generations as it has now been revealed by the Spirit to God’s holy apostles and prophets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God did not reveal it to previous generations, but now by his Spirit he has revealed it to his holy apostles and prophets."
      }
    },
