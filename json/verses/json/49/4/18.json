    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们因为愚昧无知，顽固不化，理智受到蒙蔽，与上帝所赐的生命隔绝了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的心思昏昧，因為自己無知，心裡剛硬，就與　神所賜的生命隔絕了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們心地昏昧，與神所賜的生命隔絕了，都因自己無知，心裏剛硬；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "being darkened in their understanding, excluded from the life of God because of the ignorance that is in them, because of the hardness of their heart;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are darkened in their understanding and separated from the life of God because of the ignorance that is in them due to the hardening of their hearts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their minds are full of darkness; they wander far from the life God gives because they have closed their minds and hardened their hearts against him."
      }
    },
