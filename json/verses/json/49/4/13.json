    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "直到我们对上帝的儿子有一致的信仰和认识，逐渐成熟，达到基督那样完美的生命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "直到我們眾人對　神的兒子都有一致的信仰和認識，可以長大成人，達到基督豐盛長成的身量；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "直等到我們眾人在真道上同歸於一，認識神的兒子，得以長大成人，滿有基督長成的身量，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "until we all attain to the unity of the faith, and of the knowledge of the Son of God, to a mature man, to the measure of the stature which belongs to the fullness of Christ."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "until we all reach unity in the faith and in the knowledge of the Son of God and become mature, attaining to the whole measure of the fullness of Christ."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This will continue until we all come to such unity in our faith and knowledge of God’s Son that we will be mature in the Lord, measuring up to the full and complete standard of Christ."
      }
    },
