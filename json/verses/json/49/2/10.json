    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们是上帝的杰作，是在基督耶稣里创造的，为要叫我们做祂预先安排给我们的美善之事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們原是　神所作成的，是在基督耶穌裡創造的，為的是要我們行各樣的善事，就是　神預先所安排的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們原是他的工作，在基督耶穌裏造成的，為要叫我們行善，就是神所預備叫我們行的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For we are His workmanship, created in Christ Jesus for good works, which God prepared beforehand so that we would walk in them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For we are God’s handiwork, created in Christ Jesus to do good works, which God prepared in advance for us to do."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For we are God’s masterpiece. He has created us anew in Christ Jesus, so we can do the good things he planned for us long ago."
      }
    },
