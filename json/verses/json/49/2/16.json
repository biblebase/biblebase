    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并借着十字架消灭彼此间的敌意，使双方借着一个身体与上帝和好。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基督既然藉著十字架消滅了仇恨，就藉著十字架使雙方與　神和好，成為一體，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "既在十字架上滅了冤仇，便藉這十字架使兩下歸為一體，與神和好了，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and might reconcile them both in one body to God through the cross, by it having put to death the enmity."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and in one body to reconcile both of them to God through the cross, by which he put to death their hostility."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Together as one body, Christ reconciled both groups to God by means of his death on the cross, and our hostility toward each other was put to death."
      }
    },
