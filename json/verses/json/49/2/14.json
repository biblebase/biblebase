    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为基督耶稣亲自给我们带来了和平，以自己的身体拆毁了以色列人和外族人之间充满敌意的隔墙，使双方合而为一，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基督就是我們的和平：他使雙方合而為一，拆毀了隔在中間的牆，就是以自己的身體除掉雙方的仇恨，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他使我們和睦，將兩下合而為一，拆毀了中間隔斷的牆；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He Himself is our peace, who made both groups into one and broke down the barrier of the dividing wall,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For he himself is our peace, who has made the two groups one and has destroyed the barrier, the dividing wall of hostility,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For Christ himself has brought peace to us. He united Jews and Gentiles into one people when, in his own body on the cross, he broke down the wall of hostility that separated us."
      }
    },
