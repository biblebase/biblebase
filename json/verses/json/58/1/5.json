    "sermons": [
      {
        "id": "home_sermons_sermon-2018-02-25",
        "title": "無比的基督",
        "date": "2018-02-25",
        "preacher": "鄭昌國牧師",
        "verses": "希伯來書 1:1-9",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2018/Sermon-2018-02-25.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝从未对任何一个天使说： “你是我的儿子，我今日成为你父亲。” 或说： “我要做你的父亲，你要做我的儿子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神曾對哪一個天使說過： “你是我的兒子， 我今日生了你”呢？ 或者說： “我要作他的父親， 他要作我的兒子”呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所有的天使，神從來對哪一個說： 你是我的兒子， 我今日生你？ 又指着哪一個說： 我要作他的父， 他要作我的子？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For to which of the angels did He ever say, “You are My Son, Today I have begotten You”? And again, “I will be a Father to Him And He shall be a Son to Me”?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For to which of the angels did God ever say, “You are my Son; today I have become your Father”? Or again, “I will be his Father, and he will be my Son”?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For God never said to any angel what he said to Jesus: “You are my Son. Today I have become your Father.” God also said, “I will be his Father, and he will be my Son.”"
      }
