    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝提到天使的时候，也只是说： “上帝使祂的天使成为风，使祂的仆役成为火焰。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "論到天使，說： “　神以他的天使為風， 以他的僕役為火燄。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論到使者，又說： 神以風為使者， 以火焰為僕役；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And of the angels He says, “Who makes His angels winds, And His ministers a flame of fire.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In speaking of the angels he says, “He makes his angels spirits, and his servants flames of fire.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Regarding the angels, he says, “He sends his angels like the winds, his servants like flames of fire.”"
      }
    },
