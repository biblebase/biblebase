    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要像卷外衣一样把天地卷起来。 天地将如衣服一样被更换， 但你永远不变， 你的年日永无穷尽。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要把天地像外套一樣捲起來， 天地就像衣服一樣被更換； 只有你永不改變， 你的年數也沒有窮盡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要將天地捲起來，像一件外衣， 天地就都改變了。 惟有你永不改變； 你的年數沒有窮盡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And like a mantle You will roll them up; Like a garment they will also be changed. But You are the same, And Your years will not come to an end.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will roll them up like a robe; like a garment they will be changed. But you remain the same, and your years will never end.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will fold them up like a cloak and discard them like old clothing. But you are always the same; you will live forever.”"
      }
    },
