    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝从未对任何一个天使说： “你坐在我的右边， 等我使你的仇敌成为你的脚凳。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神可曾向哪一個天使說： “你坐在我的右邊， 等我使你的仇敵作你的腳凳”呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所有的天使，神從來對哪一個說： 你坐在我的右邊， 等我使你仇敵作你的腳凳？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But to which of the angels has He ever said, “Sit at My right hand, Until I make Your enemies A footstool for Your feet”?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To which of the angels did God ever say, “Sit at my right hand until I make your enemies a footstool for your feet”?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And God never said to any of the angels, “Sit in the place of honor at my right hand until I humble your enemies, making them a footstool under your feet.”"
      }
    },
