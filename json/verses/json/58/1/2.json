    "sermons": [
      {
        "id": "home_sermons_sermon-2018-02-25",
        "title": "無比的基督",
        "date": "2018-02-25",
        "preacher": "鄭昌國牧師",
        "verses": "希伯來書 1:1-9",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2018/Sermon-2018-02-25.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在这世界的末期，祂又借着自己的儿子亲自向我们说话。上帝早已立祂承受万物，并借着祂创造了宇宙万物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在這末後的日子，卻藉著他的兒子向我們說話。　神已經立他作萬有的承受者，並且藉著他創造了宇宙。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就在這末世藉着他兒子曉諭我們；又早已立他為承受萬有的，也曾藉着他創造諸世界。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "in these last days has spoken to us in His Son, whom He appointed heir of all things, through whom also He made the world."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but in these last days he has spoken to us by his Son, whom he appointed heir of all things, and through whom also he made the universe."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And now in these final days, he has spoken to us through his Son. God promised everything to the Son as an inheritance, and through the Son he created the universe."
      }
    },
