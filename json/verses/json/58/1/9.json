    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你喜爱公义，憎恶邪恶。 所以上帝，你的上帝， 用喜乐之油膏你，使你超过同伴。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你喜愛公義，恨惡不法， 所以，　神，就是你的　神， 用喜樂的油膏抹你，勝過膏抹你的同伴。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你喜愛公義，恨惡罪惡； 所以神，就是你的神，用喜樂油膏你， 勝過膏你的同伴；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have loved righteousness and hated lawlessness; Therefore God, Your God, has anointed You With the oil of gladness above Your companions.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have loved righteousness and hated wickedness; therefore God, your God, has set you above your companions by anointing you with the oil of joy.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You love justice and hate evil. Therefore, O God, your God has anointed you, pouring out the oil of joy on you more than on anyone else.”"
      }
    },
