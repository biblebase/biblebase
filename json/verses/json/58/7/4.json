    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "试想，连我们的祖先亚伯拉罕都将战利品的十分之一给了他，可见他是何等的尊贵！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們想一想這人是多麼偉大啊！祖先亞伯拉罕也要從上等的擄物中，拿出十分之一來給了他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們想一想，先祖亞伯拉罕將自己所擄來上等之物取十分之一給他，這人是何等尊貴呢！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now observe how great this man was to whom Abraham, the patriarch, gave a tenth of the choicest spoils."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Just think how great he was: Even the patriarch Abraham gave him a tenth of the plunder!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Consider then how great this Melchizedek was. Even Abraham, the great patriarch of Israel, recognized this by giving him a tenth of what he had taken in battle."
      }
    },
