    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们所需要的，正是这样一位圣洁无瑕、良善纯全、远离罪恶、超越诸天的大祭司。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣的一位大祭司，對我們本是合適的。他是聖潔、沒有邪惡、沒有玷污、從罪人中分別出來、高過眾天的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "像這樣聖潔、無邪惡、無玷污、遠離罪人、高過諸天的大祭司，原是與我們合宜的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it was fitting for us to have such a high priest, holy, innocent, undefiled, separated from sinners and exalted above the heavens;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Such a high priest truly meets our need—one who is holy, blameless, pure, set apart from sinners, exalted above the heavens."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He is the kind of high priest we need because he is holy and blameless, unstained by sin. He has been set apart from sinners and has been given the highest place of honor in heaven."
      }
    },
