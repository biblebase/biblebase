    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为有一处经文为祂做见证说：“你照麦基洗德的模式永远做祭司。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為有為他作證的說： “你永遠作祭司， 是照著麥基洗德的體系。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為有給他作見證的說：「你是照着麥基洗德的等次永遠為祭司。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it is attested of Him, “You are a priest forever According to the order of Melchizedek.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For it is declared: “You are a priest forever, in the order of Melchizedek.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the psalmist pointed this out when he prophesied, “You are a priest forever in the order of Melchizedek.”"
      }
    },
