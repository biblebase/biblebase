    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以祂能拯救那些靠着祂来到上帝面前的人，直到永远，因为祂永远活着，为他们祈求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，那些靠著他進到　神面前的人，他都能拯救到底；因為他長遠活著，為他們代求。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡靠着他進到神面前的人，他都能拯救到底；因為他是長遠活着，替他們祈求。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore He is able also to save forever those who draw near to God through Him, since He always lives to make intercession for them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore he is able to save completely those who come to God through him, because he always lives to intercede for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Therefore he is able, once and forever, to save those who come to God through him. He lives forever to intercede with God on their behalf."
      }
    },
