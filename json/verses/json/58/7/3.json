    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他的父母、族谱、生辰、寿数都不得而知，他跟上帝的儿子相似，永远担任祭司的职分。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他沒有父親，沒有母親，沒有族譜，也沒有生死的記錄，而是與　神的兒子相似，永遠作祭司。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他無父，無母，無族譜，無生之始，無命之終，乃是與神的兒子相似。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Without father, without mother, without genealogy, having neither beginning of days nor end of life, but made like the Son of God, he remains a priest perpetually."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Without father or mother, without genealogy, without beginning of days or end of life, resembling the Son of God, he remains a priest forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There is no record of his father or mother or any of his ancestors—no beginning or end to his life. He remains a priest forever, resembling the Son of God."
      }
    },
