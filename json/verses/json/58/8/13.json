    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝既然说要立新约，就是把以前的约当作旧的了，那渐渐过时、陈旧的东西很快就会消失。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神既然說到新的約，就是把前約當作舊的了；那變成陳舊衰老的，就快要消逝了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "既說新約，就以前約為舊了；但那漸舊漸衰的，就必快歸無有了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He said, “A new covenant,” He has made the first obsolete. But whatever is becoming obsolete and growing old is ready to disappear."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "By calling this covenant “new,” he has made the first one obsolete; and what is obsolete and outdated will soon disappear."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When God speaks of a “new” covenant, it means he has made the first one obsolete. It is now out of date and will soon disappear."
      }
    },
