    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂如果是在地上，根本不会做祭司，因为地上已经有照律法向上帝献礼物的祭司了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果他在地上，就不會作祭司，因為已經有按照律法獻禮物的祭司了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他若在地上，必不得為祭司，因為已經有照律法獻禮物的祭司。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now if He were on earth, He would not be a priest at all, since there are those who offer the gifts according to the Law;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If he were on earth, he would not be a priest, for there are already priests who offer the gifts prescribed by the law."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If he were here on earth, he would not even be a priest, since there already are priests who offer the gifts required by the law."
      }
    },
