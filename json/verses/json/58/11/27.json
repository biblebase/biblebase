    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他凭信心离开埃及，不怕王的愤怒。他坚忍不拔，好像看见了肉眼不能看见的主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因著信，他離開了埃及，不怕王的忿怒；因為他堅定不移，就像看見了人不能看見的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他因着信，就離開埃及，不怕王怒；因為他恆心忍耐，如同看見那不能看見的主。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By faith he left Egypt, not fearing the wrath of the king; for he endured, as seeing Him who is unseen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "By faith he left Egypt, not fearing the king’s anger; he persevered because he saw him who is invisible."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It was by faith that Moses left the land of Egypt, not fearing the king’s anger. He kept right on going because he kept his eyes on the one who is invisible."
      }
