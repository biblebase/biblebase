    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，我简短地写信给你们，希望你们听我劝勉的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，我勸你們耐心接受我這勸勉的話，因為我只是簡略地寫給你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，我略略寫信給你們，望你們聽我勸勉的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I urge you, brethren, bear with this word of exhortation, for I have written to you briefly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Brothers and sisters, I urge you to bear with my word of exhortation, for in fact I have written to you quite briefly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I urge you, dear brothers and sisters, to pay attention to what I have written in this brief exhortation."
      }
    },
