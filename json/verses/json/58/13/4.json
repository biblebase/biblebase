    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人人都要尊重婚姻，不可玷污夫妻关系，因为上帝必审判淫乱和通奸的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人人都應該尊重婚姻，婚床也不要玷污，因為　神一定審判淫亂的和姦淫的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "婚姻，人人都當尊重，牀也不可污穢；因為苟合行淫的人，神必要審判。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Marriage is to be held in honor among all, and the marriage bed is to be undefiled; for fornicators and adulterers God will judge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Marriage should be honored by all, and the marriage bed kept pure, for God will judge the adulterer and all the sexually immoral."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give honor to marriage, and remain faithful to one another in marriage. God will surely judge people who are immoral and those who commit adultery."
      }
    },
