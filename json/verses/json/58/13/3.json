    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要设身处地地顾念那些被囚禁的人，要感同身受地顾念那些遭遇苦难的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要記念那些被囚禁的人，好像跟他們一起被囚禁；也要記念那些受虐待的人，好像你們也親自受過。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要記念被捆綁的人，好像與他們同受捆綁；也要記念遭苦害的人，想到自己也在肉身之內。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Remember the prisoners, as though in prison with them, and those who are ill-treated, since you yourselves also are in the body."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Continue to remember those in prison as if you were together with them in prison, and those who are mistreated as if you yourselves were suffering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Remember those in prison, as if you were there yourself. Remember also those being mistreated, as if you felt their pain in your own bodies."
      }
    },
