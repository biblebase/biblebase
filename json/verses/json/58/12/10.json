    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的生身父亲是按自己以为好的标准暂时管教我们，但上帝是为了我们的益处而管教我们，使我们可以在祂的圣洁上有份。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "肉身的父親照著自己的意思管教我們，只有短暫的日子；唯有　神管教我們，是為著我們的好處，使我們在他的聖潔上有分。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "生身的父都是暫隨己意管教我們；惟有萬靈的父管教我們，是要我們得益處，使我們在他的聖潔上有分。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they disciplined us for a short time as seemed best to them, but He disciplines us for our good, so that we may share His holiness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They disciplined us for a little while as they thought best; but God disciplines us for our good, in order that we may share in his holiness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For our earthly fathers disciplined us for a few years, doing the best they knew how. But God’s discipline is always good for us, so that we might share in his holiness."
      }
    },
