    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这里有名字记录在天上的众长子的教会，有审判众人的上帝和已达到纯全的义人的灵魂，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有名字登記在天上眾長子的教會，有審判眾人的　神，有被成全的義人的靈魂，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有名錄在天上諸長子之會所共聚的總會，有審判眾人的神和被成全之義人的靈魂，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "to the general assembly and church of the firstborn who are enrolled in heaven, and to God, the Judge of all, and to the spirits of the righteous made perfect,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "to the church of the firstborn, whose names are written in heaven. You have come to God, the Judge of all, to the spirits of the righteous made perfect,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have come to the assembly of God’s firstborn children, whose names are written in heaven. You have come to God himself, who is the judge over all things. You have come to the spirits of the righteous ones in heaven who have now been made perfect."
      }
    },
