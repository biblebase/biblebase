    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣照麦基洗德的模式成了永远的大祭司，祂替我们先进入了圣幕内。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌已經為我們作先鋒進入了幔子裡面；他是照著麥基洗德的體系，成了永遠的大祭司。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "作先鋒的耶穌，既照着麥基洗德的等次成了永遠的大祭司，就為我們進入幔內。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "where Jesus has entered as a forerunner for us, having become a high priest forever according to the order of Melchizedek."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "where our forerunner, Jesus, has entered on our behalf. He has become a high priest forever, in the order of Melchizedek."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus has already gone in there for us. He has become our eternal High Priest in the order of Melchizedek."
      }
    },
