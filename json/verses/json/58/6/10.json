    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为上帝并非不公义，以致忘记你们为祂所做的工作以及你们过去和现在服侍圣徒所表现的爱心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為　神並不是不公義，以致忘記了你們的工作，和你們為他的名所表現的愛心，就是你們以前服事聖徒，現在還是服事他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為神並非不公義，竟忘記你們所做的工和你們為他名所顯的愛心，就是先前伺候聖徒，如今還是伺候。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For God is not unjust so as to forget your work and the love which you have shown toward His name, in having ministered and in still ministering to the saints."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God is not unjust; he will not forget your work and the love you have shown him as you have helped his people and continue to help them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For God is not unjust. He will not forget how hard you have worked for him and how you have shown your love to him by caring for other believers, as you still do."
      }
    },
