    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，你们就不致懒散，可以效法那些凭信心和忍耐承受应许的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "並且不要懶惰，卻要效法那些憑著信心和忍耐承受應許的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且不懈怠，總要效法那些憑信心和忍耐承受應許的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that you will not be sluggish, but imitators of those who through faith and patience inherit the promises."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We do not want you to become lazy, but to imitate those who through faith and patience inherit what has been promised."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then you will not become spiritually dull and indifferent. Instead, you will follow the example of those who are going to inherit God’s promises because of their faith and endurance."
      }
    },
