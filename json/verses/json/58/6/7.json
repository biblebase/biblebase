    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一块经常受到雨水滋润的田地若为耕种的人长出庄稼来，就蒙上帝赐福；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這就像一塊地，吸收了常常下在它上面的雨水，如果長出對種植的人有用的菜蔬，就從　神那裡得福。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就如一塊田地，吃過屢次下的雨水，生長菜蔬，合乎耕種的人用，就從神得福；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For ground that drinks the rain which often falls on it and brings forth vegetation useful to those for whose sake it is also tilled, receives a blessing from God;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Land that drinks in the rain often falling on it and that produces a crop useful to those for whom it is farmed receives the blessing of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the ground soaks up the falling rain and bears a good crop for the farmer, it has God’s blessing."
      }
    },
