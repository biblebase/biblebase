    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若长出来的尽是荆棘和蒺藜，就毫无价值，它会面临被咒诅的危险，最后必遭焚烧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但如果這塊地長出荊棘和蒺藜來，就被廢棄，近於咒詛，結局就是焚燒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若長荊棘和蒺藜，必被廢棄，近於咒詛，結局就是焚燒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but if it yields thorns and thistles, it is worthless and close to being cursed, and it ends up being burned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But land that produces thorns and thistles is worthless and is in danger of being cursed. In the end it will be burned."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if a field bears thorns and thistles, it is useless. The farmer will soon condemn that field and burn it."
      }
