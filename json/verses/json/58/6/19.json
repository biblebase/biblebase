    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们有这盼望作我们灵魂的锚，既安稳又可靠，一直通往圣幕里面的至圣所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們有這盼望，就像靈魂的錨，又穩當又堅固，通過幔子直進到裡面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們有這指望，如同靈魂的錨，又堅固又牢靠，且通入幔內。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This hope we have as an anchor of the soul, a hope both sure and steadfast and one which enters within the veil,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We have this hope as an anchor for the soul, firm and secure. It enters the inner sanctuary behind the curtain,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This hope is a strong and trustworthy anchor for our souls. It leads us through the curtain into God’s inner sanctuary."
      }
    },
