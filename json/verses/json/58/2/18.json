    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂经历过受试炼的痛苦，所以能帮助受试炼的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他自己既然經過試探，受了苦，就能夠幫助那些被試探的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他自己既然被試探而受苦，就能搭救被試探的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For since He Himself was tempted in that which He has suffered, He is able to come to the aid of those who are tempted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because he himself suffered when he was tempted, he is able to help those who are being tempted."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Since he himself has gone through suffering and testing, he is able to help us when we are being tested."
      }
    },
