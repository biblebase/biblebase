    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "同时，上帝按自己的旨意，用神迹、奇事、各样的异能、圣灵的恩赐和他们一同做见证。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神又照著自己的旨意，用神蹟、奇事和各樣異能，以及聖靈的恩賜，與他們一同作見證。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神又按自己的旨意，用神蹟、奇事和百般的異能，並聖靈的恩賜，同他們作見證。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God also testifying with them, both by signs and wonders and by various miracles and by gifts of the Holy Spirit according to His own will."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God also testified to it by signs, wonders and various miracles, and by gifts of the Holy Spirit distributed according to his will."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And God confirmed the message by giving signs and wonders and various miracles and gifts of the Holy Spirit whenever he chose."
      }
    },
