    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又说： “我要倚靠祂。” 还说： “看啊，我和上帝赐给我的儿女都在这里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又說： “我要信靠他。” 又說： “看哪，我和　神所賜給我的孩子們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又說： 我要倚賴他； 又說： 看哪，我與神所給我的兒女。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And again, “I will put My trust in Him.” And again, “Behold, I and the children whom God has given Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And again, “I will put my trust in him.” And again he says, “Here am I, and the children God has given me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He also said, “I will put my trust in him,” that is, “I and the children God has given me.”"
      }
    },
