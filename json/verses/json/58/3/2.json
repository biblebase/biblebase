    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂向委任祂的上帝尽忠，就好像摩西向上帝的全家尽忠一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他忠於那位委派他的，好像摩西在　神的全家盡忠一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他為那設立他的盡忠，如同摩西在神的全家盡忠一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He was faithful to Him who appointed Him, as Moses also was in all His house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He was faithful to the one who appointed him, just as Moses was faithful in all God’s house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For he was faithful to God, who appointed him, just as Moses served faithfully when he was entrusted with God’s entire house."
      }
    },
