    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，要谨慎，免得你们当中有人心存恶念，不肯相信，背弃了永活的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，你們要小心，免得你們中間有人存著邪惡、不信的心，以致離棄了永活的　神；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，你們要謹慎，免得你們中間或有人存着不信的惡心，把永生神離棄了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Take care, brethren, that there not be in any one of you an evil, unbelieving heart that falls away from the living God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See to it, brothers and sisters, that none of you has a sinful, unbelieving heart that turns away from the living God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Be careful then, dear brothers and sisters. Make sure that your own hearts are not evil and unbelieving, turning you away from the living God."
      }
    },
