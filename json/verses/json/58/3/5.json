    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西以仆人的身份向上帝的全家尽忠，为将来要宣告的事做见证。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西不過是個僕人，在　神的全家盡忠，為以後要傳講的事作證；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西為僕人，在神的全家誠然盡忠，為要證明將來必傳說的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Moses was faithful in all His house as a servant, for a testimony of those things which were to be spoken later;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Moses was faithful as a servant in all God’s house,” bearing witness to what would be spoken by God in the future."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses was certainly faithful in God’s house as a servant. His work was an illustration of the truths God would reveal later."
      }
    },
