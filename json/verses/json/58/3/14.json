      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For we have become partakers of Christ, if we hold fast the beginning of our assurance firm until the end,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We have come to share in Christ, if indeed we hold our original conviction firmly to the very end."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For if we are faithful to the end, trusting God just as firmly as when we first believed, we will share in all that belongs to Christ."
      }
