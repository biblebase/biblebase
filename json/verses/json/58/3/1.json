    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，同蒙天召的圣洁的弟兄姊妹，你们应当仔细思想那位我们公认为特使和大祭司的耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，同蒙天召的聖潔的弟兄啊！你們應該想想耶穌，就是作使徒、作我們所宣認的大祭司的那一位。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "同蒙天召的聖潔弟兄啊，你們應當思想我們所認為使者、為大祭司的耶穌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore, holy brethren, partakers of a heavenly calling, consider Jesus, the Apostle and High Priest of our confession;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore, holy brothers and sisters, who share in the heavenly calling, fix your thoughts on Jesus, whom we acknowledge as our apostle and high priest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And so, dear brothers and sisters who belong to God and are partners with those called to heaven, think carefully about this Jesus whom we declare to be God’s messenger and High Priest."
      }
    },
