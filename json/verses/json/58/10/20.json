    "sermons": [
      {
        "id": "home_sermons_sermon-2012-01-22",
        "title": "從相靠神做新的事",
        "date": "2012-01-22",
        "preacher": "陳榕添牧師",
        "verses": "希伯來書 10:19-25",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-01-22.mp3"
      }
    ],
    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這進入的路，是他給我們開闢的，是一條通過幔子、又新又活的路，這幔子就是他的身體。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "是藉着他給我們開了一條又新又活的路，從幔子經過，這幔子就是他的身體。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "by a new and living way which He inaugurated for us through the veil, that is, His flesh,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "by a new and living way opened for us through the curtain, that is, his body,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "By his death, Jesus opened a new and life-giving way through the curtain into the Most Holy Place."
      }
    },
