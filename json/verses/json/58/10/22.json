    "sermons": [
      {
        "id": "home_sermons_sermon-2012-01-22",
        "title": "從相靠神做新的事",
        "date": "2012-01-22",
        "preacher": "陳榕添牧師",
        "verses": "希伯來書 10:19-25",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-01-22.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂洁净了我们被罪困扰的良心，用清水洗净了我们的身体。因此，我们要信心十足、真诚地到上帝面前。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們良心的邪惡既然被灑淨，身體也用清水洗淨了，那麼，我們就應該懷著真誠的心和完備的信，進到　神面前；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並我們心中天良的虧欠已經灑去，身體用清水洗淨了，就當存着誠心和充足的信心來到神面前；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "let us draw near with a sincere heart in full assurance of faith, having our hearts sprinkled clean from an evil conscience and our bodies washed with pure water."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "let us draw near to God with a sincere heart and with the full assurance that faith brings, having our hearts sprinkled to cleanse us from a guilty conscience and having our bodies washed with pure water."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "let us go right into the presence of God with sincere hearts fully trusting him. For our guilty consciences have been sprinkled with Christ’s blood to make us clean, and our bodies have been washed with pure water."
      }
    },
