    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司都要天天站着供职，一次次地献上同样的祭物，只是这些祭物根本不能除罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有的祭司都是天天站著事奉，多次獻上同樣的祭品，那些祭品永遠不能把罪除去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡祭司天天站着事奉神，屢次獻上一樣的祭物，這祭物永不能除罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every priest stands daily ministering and offering time after time the same sacrifices, which can never take away sins;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Day after day every priest stands and performs his religious duties; again and again he offers the same sacrifices, which can never take away sins."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Under the old covenant, the priest stands and ministers before the altar day after day, offering the same sacrifices again and again, which can never take away sins."
      }
    },
