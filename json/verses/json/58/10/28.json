    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人违犯摩西的律法，经两三个人指证后，尚且被毫不留情地处死，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果有人干犯了摩西的律法，憑著兩三個證人，他尚且得不到憐憫而死；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人干犯摩西的律法，憑兩三個見證人，尚且不得憐恤而死，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Anyone who has set aside the Law of Moses dies without mercy on the testimony of two or three witnesses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who rejected the law of Moses died without mercy on the testimony of two or three witnesses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For anyone who refused to obey the law of Moses was put to death without mercy on the testimony of two or three witnesses."
      }
