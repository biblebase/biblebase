    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有时候，你们在众目睽睽之下被辱骂，受迫害；有时候，你们和遭受这样苦难的人同舟共济。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有時在眾人面前被辱罵，遭患難；有時卻成了遭遇同樣情形的人的同伴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "一面被毀謗，遭患難，成了戲景，叫眾人觀看；一面陪伴那些受這樣苦難的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "partly by being made a public spectacle through reproaches and tribulations, and partly by becoming sharers with those who were so treated."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Sometimes you were publicly exposed to insult and persecution; at other times you stood side by side with those who were so treated."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Sometimes you were exposed to public ridicule and were beaten, and sometimes you helped others who were suffering the same things."
      }
    },
