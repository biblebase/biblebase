    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要停止聚会，像那些停止惯了的人，要互相鼓励，特别是你们知道主再来的日子近了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們不可放棄聚會，好像有些人的習慣一樣；卻要互相勸勉。你們既然知道那日子臨近，就更應該這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們不可停止聚會，好像那些停止慣了的人，倒要彼此勸勉，既知道那日子臨近，就更當如此。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "not forsaking our own assembling together, as is the habit of some, but encouraging one another; and all the more as you see the day drawing near."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "not giving up meeting together, as some are in the habit of doing, but encouraging one another—and all the more as you see the Day approaching."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And let us not neglect our meeting together, as some people do, but encourage one another, especially now that the day of his return is drawing near."
      }
