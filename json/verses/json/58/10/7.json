    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是我说， ‘上帝啊，我来是要遵行你的旨意， 我的事都记在圣经上了。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時我說： ‘看哪！我來了， 經卷上已經記載我的事， 　神啊！我來是要遵行你的旨意。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時我說：神啊，我來了， 為要照你的旨意行； 我的事在經卷上已經記載了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I said, ‘Behold, I have come (In the scroll of the book it is written of Me) To do Your will, O God.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I said, ‘Here I am—it is written about me in the scroll— I have come to do your will, my God.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I said, ‘Look, I have come to do your will, O God— as is written about me in the Scriptures.’”"
      }
    },
