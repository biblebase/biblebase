    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从人间选出来的大祭司，都是受委任代表人办理与上帝有关的事，为人的罪向上帝献上礼物和赎罪祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "每一個大祭司都是從人間選出來，奉派替人辦理與　神有關的事，為的是要獻上禮物和贖罪的祭物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡從人間挑選的大祭司，是奉派替人辦理屬神的事，為要獻上禮物和贖罪祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For every high priest taken from among men is appointed on behalf of men in things pertaining to God, in order to offer both gifts and sacrifices for sins;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every high priest is selected from among the people and is appointed to represent the people in matters related to God, to offer gifts and sacrifices for sins."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Every high priest is a man chosen to represent other people in their dealings with God. He presents their gifts to God and offers sacrifices for their sins."
      }
    },
