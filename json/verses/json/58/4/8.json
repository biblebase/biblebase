      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果当初约书亚已经让他们得到了安息，上帝就不必另定一个日子了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果約書亞已經使他們享受了安息，　神後來就不會再提到別的日子了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若是約書亞已叫他們享了安息，後來神就不再提別的日子了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if Joshua had given them rest, He would not have spoken of another day after that."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For if Joshua had given them rest, God would not have spoken later about another day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now if Joshua had succeeded in giving them this rest, God would not have spoken about another day of rest still to come."
      }
