    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，我们要竭力进入那安息，免得重蹈他们的覆辙，因不顺服而倒毙。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，我們要竭力進入那安息，免得有人隨著那不順從的樣子就跌倒了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，我們務必竭力進入那安息，免得有人學那不信從的樣子跌倒了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore let us be diligent to enter that rest, so that no one will fall, through following the same example of disobedience."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let us, therefore, make every effort to enter that rest, so that no one will perish by following their example of disobedience."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So let us do our best to enter that rest. But if we disobey God, as the people of Israel did, we will fall."
      }
    },
