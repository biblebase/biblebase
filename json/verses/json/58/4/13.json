    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "受造物在上帝面前都无法隐藏，因为万物都是赤裸裸地暴露在上帝眼前，我们必须向祂交账。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "被造的在　神面前沒有一樣不是顯明的，萬有在他的眼前都是赤露敞開的；我們必須向他交帳。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且被造的沒有一樣在他面前不顯然的；原來萬物在那與我們有關係的主眼前，都是赤露敞開的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And there is no creature hidden from His sight, but all things are open and laid bare to the eyes of Him with whom we have to do."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nothing in all creation is hidden from God’s sight. Everything is uncovered and laid bare before the eyes of him to whom we must give account."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Nothing in all creation is hidden from God. Everything is naked and exposed before his eyes, and he is the one to whom we are accountable."
      }
    },
