    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的上帝必按照祂在基督耶稣里的荣耀，丰丰富富地赐给你们所需用的一切。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的　神必照他在基督耶穌裡榮耀的豐富，滿足你們的一切需要。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的神必照他榮耀的豐富，在基督耶穌裏，使你們一切所需用的都充足。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And my God will supply all your needs according to His riches in glory in Christ Jesus."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And my God will meet all your needs according to the riches of his glory in Christ Jesus."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And this same God who takes care of me will supply all your needs from his glorious riches, which have been given to us in Christ Jesus."
      }
    },
