    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我并非有缺乏才这样说，因为我已经学会在任何处境中都能知足。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我並不是因為缺乏才這樣說：我已經學會了，無論在甚麼情況之下都可以知足。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我並不是因缺乏說這話；我無論在甚麼景況都可以知足，這是我已經學會了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Not that I speak from want, for I have learned to be content in whatever circumstances I am."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am not saying this because I am in need, for I have learned to be content whatever the circumstances."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Not that I was ever in need, for I have learned how to be content with whatever I have."
      }
