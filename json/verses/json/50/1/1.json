    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我保罗和提摩太是基督耶稣的奴仆，写信给腓立比属于基督耶稣的众圣徒、诸位监督和执事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基督耶穌的僕人保羅和提摩太，寫信給所有住在腓立比，在基督耶穌裡的聖徒、監督和執事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基督耶穌的僕人保羅和提摩太寫信給凡住腓立比、在基督耶穌裏的眾聖徒，和諸位監督，諸位執事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Paul and Timothy, bond-servants of Christ Jesus, To all the saints in Christ Jesus who are in Philippi, including the overseers and deacons:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Paul and Timothy, servants of Christ Jesus, To all God’s holy people in Christ Jesus at Philippi, together with the overseers and deacons:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This letter is from Paul and Timothy, slaves of Christ Jesus. I am writing to all of God’s holy people in Philippi who belong to Christ Jesus, including the church leaders and deacons."
      }
    },
