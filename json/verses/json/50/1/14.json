    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大多数主内的弟兄姊妹看见我为主被囚，信心受到鼓舞，更加放胆无惧地传扬上帝的道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "而且大多數主內的弟兄，因我所受的捆鎖，就篤信不疑，毫無畏懼，更勇敢地傳講　神的道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且那在主裏的弟兄多半因我受的捆鎖就篤信不疑，越發放膽傳神的道，無所懼怕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and that most of the brethren, trusting in the Lord because of my imprisonment, have far more courage to speak the word of God without fear."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And because of my chains, most of the brothers and sisters have become confident in the Lord and dare all the more to proclaim the gospel without fear."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And because of my imprisonment, most of the believers here have gained confidence and boldly speak God’s message without fear."
      }
    },
