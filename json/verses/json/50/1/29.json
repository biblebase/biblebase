    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为上帝为了基督的缘故，赐恩使你们不但信基督，而且为祂受苦，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為　神為了基督的緣故賜恩給你們，使你們不單是信基督，也是要為他受苦；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你們蒙恩，不但得以信服基督，並要為他受苦。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For to you it has been granted for Christ’s sake, not only to believe in Him, but also to suffer for His sake,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For it has been granted to you on behalf of Christ not only to believe in him, but also to suffer for him,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you have been given not only the privilege of trusting in Christ but also the privilege of suffering for him."
      }
    },
