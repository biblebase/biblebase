    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我所祈求的就是：你们的爱心随着属灵知识的增加和辨别力的提高而日益增长，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我所禱告的，是要你們的愛心，在充足的知識和各樣的見識上，多而又多，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我所禱告的，就是要你們的愛心在知識和各樣見識上多而又多，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And this I pray, that your love may abound still more and more in real knowledge and all discernment,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And this is my prayer: that your love may abound more and more in knowledge and depth of insight,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I pray that your love will overflow more and more, and that you will keep on growing in knowledge and understanding."
      }
    },
