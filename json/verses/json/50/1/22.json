    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果活在世上，我可以做工，多结果子。我真不知道该怎么选择！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但如果我仍在世上活著，能夠使我的工作有成果，我就不知道應該怎樣選擇了！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我在肉身活着，若成就我工夫的果子，我就不知道該挑選甚麼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But if I am to live on in the flesh, this will mean fruitful labor for me; and I do not know which to choose."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If I am to go on living in the body, this will mean fruitful labor for me. Yet what shall I choose? I do not know!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if I live, I can do more fruitful work for Christ. So I really don’t know which is better."
      }
    },
