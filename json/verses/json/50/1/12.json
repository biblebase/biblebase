    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，我希望你们知道，我的遭遇反而会使福音传得更广，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，我願意你們知道，我所遭遇的反而使福音更加興旺了，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，我願意你們知道，我所遭遇的事更是叫福音興旺，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now I want you to know, brethren, that my circumstances have turned out for the greater progress of the gospel,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now I want you to know, brothers and sisters, that what has happened to me has actually served to advance the gospel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I want you to know, my dear brothers and sisters, that everything that has happened to me here has helped to spread the Good News."
      }
    },
