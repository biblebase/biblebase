    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，我再到你们那里的时候，你们可以因为我的缘故更加以基督耶稣为荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以致你們因為我要再到你們那裡去，就在基督耶穌裡更加以我為榮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "叫你們在基督耶穌裏的歡樂，因我再到你們那裏去，就越發加增。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that your proud confidence in me may abound in Christ Jesus through my coming to you again."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "so that through my being with you again your boasting in Christ Jesus will abound on account of me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when I come to you again, you will have even more reason to take pride in Christ Jesus because of what he is doing through me."
      }
    },
