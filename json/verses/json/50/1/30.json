    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "经历你们从前在我身上见过，现在又从我这里听到的同样争战。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們面對的爭戰，和你們在我身上所見過，現在又聽到的是一樣的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們的爭戰，就與你們在我身上從前所看見、現在所聽見的一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "experiencing the same conflict which you saw in me, and now hear to be in me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "since you are going through the same struggle you saw I had, and now hear that I still have."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We are in this struggle together. You have seen my struggle in the past, and you know that I am still in the midst of it."
      }
    },
