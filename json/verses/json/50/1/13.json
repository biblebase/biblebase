    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "连所有的皇家卫兵和其他众人都知道我是为了基督的缘故而受囚禁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以致整個宮裡的衛隊和其餘一切的人，都知道我是為了基督才受捆鎖的；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以致我受的捆鎖在御營全軍和其餘的人中，已經顯明是為基督的緣故。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that my imprisonment in the cause of Christ has become well known throughout the whole praetorian guard and to everyone else,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As a result, it has become clear throughout the whole palace guard and to everyone else that I am in chains for Christ."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For everyone here, including the whole palace guard, knows that I am in chains because of Christ."
      }
    },
