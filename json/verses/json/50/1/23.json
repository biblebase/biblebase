    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我处在两难之间。我情愿离开世界，与基督在一起，那实在是再好不过了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我處於兩難之間，情願離世與基督同在，因為那是好得無比的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我正在兩難之間，情願離世與基督同在，因為這是好得無比的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I am hard-pressed from both directions, having the desire to depart and be with Christ, for that is very much better;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am torn between the two: I desire to depart and be with Christ, which is better by far;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I’m torn between two desires: I long to go and be with Christ, which would be far better for me."
      }
