    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我深信，上帝既然在你们心里开始了这美好的工作，祂必在耶稣基督再来的时候完成这工作。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我深信那在你們中間開始了美好工作的，到了基督耶穌的日子，必成全這工作。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我深信那在你們心裏動了善工的，必成全這工，直到耶穌基督的日子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I am confident of this very thing, that He who began a good work in you will perfect it until the day of Christ Jesus."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "being confident of this, that he who began a good work in you will carry it on to completion until the day of Christ Jesus."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I am certain that God, who began the good work within you, will continue his work until it is finally finished on the day when Christ Jesus returns."
      }
    },
