    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你们知道提摩太的为人，我与他情同父子，一起事奉和传扬福音。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不過，你們知道提摩太已證明了他的為人，他和我一同為了福音服事主，就像兒子跟父親一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你們知道提摩太的明證；他興旺福音，與我同勞，待我像兒子待父親一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But you know of his proven worth, that he served with me in the furtherance of the gospel like a child serving his father."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you know that Timothy has proved himself, because as a son with his father he has served with me in the work of the gospel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you know how Timothy has proved himself. Like a son with his father, he has served with me in preaching the Good News."
      }
    },
