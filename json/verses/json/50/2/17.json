    "sermons": [
      {
        "id": "home_sermons_sermon-2020-02-23",
        "title": "活出福音（在一個彎曲悖謬的世代跟從耶穌）",
        "date": "2020-02-23",
        "preacher": "王文堂牧師",
        "verses": "腓利比書 2:1-18",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2020/Sermon-2020-02-23.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2020/Sermon-2020-02-23.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的信心就是献给上帝的事奉和祭物，即使在上面浇奠我的生命，我也很喜乐，并且和你们大家一同喜乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "即使把我澆奠在你們信心的祭物和供奉上，我也喜樂，並且和你們大家一同喜樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我以你們的信心為供獻的祭物，我若被澆奠在其上，也是喜樂，並且與你們眾人一同喜樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But even if I am being poured out as a drink offering upon the sacrifice and service of your faith, I rejoice and share my joy with you all."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But even if I am being poured out like a drink offering on the sacrifice and service coming from your faith, I am glad and rejoice with all of you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I will rejoice even if I lose my life, pouring it out like a liquid offering to God, just like your faithful service is an offering to God. And I want all of you to share that joy."
      }
    },
