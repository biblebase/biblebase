    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果主耶稣许可，我希望尽快派提摩太去你们那里，我好知道你们的近况，心里得到安慰。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我靠著主耶穌，希望不久就會差提摩太到你們那裡去，使我們知道你們的情況，可以得到鼓勵。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我靠主耶穌指望快打發提摩太去見你們，叫我知道你們的事，心裏就得着安慰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I hope in the Lord Jesus to send Timothy to you shortly, so that I also may be encouraged when I learn of your condition."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I hope in the Lord Jesus to send Timothy to you soon, that I also may be cheered when I receive news about you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If the Lord Jesus is willing, I hope to send Timothy to you soon for a visit. Then he can cheer me up by telling me how you are getting along."
      }
    },
