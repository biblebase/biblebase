      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do nothing out of selfish ambition or vain conceit. Rather, in humility value others above yourselves,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t be selfish; don’t try to impress others. Be humble, thinking of others as better than yourselves."
      }
