    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，我想尽快派他回去与你们相聚，好让你们喜乐，也可以减少我的挂虑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，我更急著差他去，讓你們再見到他，就可以喜樂，也可以減少我的掛慮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以我越發急速打發他去，叫你們再見他，就可以喜樂，我也可以少些憂愁。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore I have sent him all the more eagerly so that when you see him again you may rejoice and I may be less concerned about you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore I am all the more eager to send him, so that when you see him again you may be glad and I may have less anxiety."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I am all the more anxious to send him back to you, for I know you will be glad to see him, and then I will not be so worried about you."
      }
