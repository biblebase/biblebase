    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我渴望认识基督和祂复活的大能，与祂一同受苦，甚至到死的地步，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "使我認識基督和他復活的大能，並且在他所受的苦上有分，受他所受的死；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使我認識基督，曉得他復活的大能，並且曉得和他一同受苦，效法他的死，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that I may know Him and the power of His resurrection and the fellowship of His sufferings, being conformed to His death;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I want to know Christ—yes, to know the power of his resurrection and participation in his sufferings, becoming like him in his death,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I want to know Christ and experience the mighty power that raised him from the dead. I want to suffer with him, sharing in his death,"
      }
    },
