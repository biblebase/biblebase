    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们却是天上的国民，殷切等候从天降临的救主，就是主耶稣基督。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但我們是天上的公民，切望救主，就是主耶穌基督，從天上降臨；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們卻是天上的國民，並且等候救主，就是主耶穌基督從天上降臨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For our citizenship is in heaven, from which also we eagerly wait for a Savior, the Lord Jesus Christ;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But our citizenship is in heaven. And we eagerly await a Savior from there, the Lord Jesus Christ,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But we are citizens of heaven, where the Lord Jesus Christ lives. And we are eagerly waiting for him to return as our Savior."
      }
    },
