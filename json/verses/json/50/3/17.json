    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，你们要一同效法我，也要向那些以我们为榜样的人学习。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，你們要一同效法我，也要留意那些照著我們給你們的榜樣而行的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，你們要一同效法我，也當留意看那些照我們榜樣行的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Brethren, join in following my example, and observe those who walk according to the pattern you have in us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Join together in following my example, brothers and sisters, and just as you have us as a model, keep your eyes on those who live as we do."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Dear brothers and sisters, pattern your lives after mine, and learn from those who follow our example."
      }
    },
