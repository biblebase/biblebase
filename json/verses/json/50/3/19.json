    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的神就是自己的肚腹，他们以自己的羞耻为荣，只想着地上的事，他们的结局是灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的結局是滅亡，他們的神是自己的肚腹，他們以自己的羞辱為榮耀，所思想的都是地上的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的結局就是沉淪；他們的神就是自己的肚腹。他們以自己的羞辱為榮耀，專以地上的事為念。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "whose end is destruction, whose god is their appetite, and whose glory is in their shame, who set their minds on earthly things."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their destiny is destruction, their god is their stomach, and their glory is in their shame. Their mind is set on earthly things."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They are headed for destruction. Their god is their appetite, they brag about shameful things, and they think only about this life here on earth."
      }
    },
