    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂的怒气尽发， 使他们被瘟疫吞噬，难逃一死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他為自己的怒氣修平了路， 不惜使他們死亡， 把他們的性命交給瘟疫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他為自己的怒氣修平了路， 將他們交給瘟疫， 使他們死亡，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He leveled a path for His anger; He did not spare their soul from death, But gave over their life to the plague,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He prepared a path for his anger; he did not spare them from death but gave them over to the plague."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He turned his anger against them; he did not spare the Egyptians’ lives but ravaged them with the plague."
      }
