    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们跟祖先一样背信弃义， 像断弓一样毫不可靠。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們背信不忠，像他們的列祖一樣； 他們改變了，如同不可靠的弓。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "反倒退後，行詭詐，像他們的祖宗一樣； 他們改變，如同翻背的弓。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But turned back and acted treacherously like their fathers; They turned aside like a treacherous bow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like their ancestors they were disloyal and faithless, as unreliable as a faulty bow."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They turned back and were as faithless as their parents. They were as undependable as a crooked bow."
      }
    },
