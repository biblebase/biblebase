    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，他们就会信靠上帝， 遵行祂的命令， 不忘记祂的作为；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "使他們信靠　神， 不忘記　神的作為， 謹守他的誡命；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "好叫他們仰望神， 不忘記神的作為， 惟要守他的命令。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "That they should put their confidence in God And not forget the works of God, But keep His commandments,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they would put their trust in God and would not forget his deeds but would keep his commands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So each generation should set its hope anew on God, not forgetting his glorious miracles and obeying his commands."
      }
