    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 我要永远赞美你的作为。 我要在你忠心的子民面前仰望你美善的名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要永遠稱謝你，因為你行了這事； 我必在你聖民的面前，宣揚你的名， 因你的名是美好的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要稱謝你，直到永遠， 因為你行了這事。 我也要在你聖民面前仰望你的名； 這名本為美好。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will give You thanks forever, because You have done it, And I will wait on Your name, for it is good, in the presence of Your godly ones."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For what you have done I will always praise you in the presence of your faithful people. And I will hope in your name, for your name is good."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will praise you forever, O God, for what you have done. I will trust in your good name in the presence of your faithful people."
      }
    },
