    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的声音击倒橡树， 使树木凋零。 众人在祂殿中高呼： “荣耀归于耶和华！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的聲音驚動母鹿生產， 使林中的樹木光禿凋零； 凡是在他殿中的都說：“榮耀啊！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的聲音驚動母鹿落胎， 樹木也脫落淨光。 凡在他殿中的，都稱說他的榮耀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The voice of the Lord makes the deer to calve And strips the forests bare; And in His temple everything says, “Glory!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The voice of the Lord twists the oaks and strips the forests bare. And in his temple all cry, “Glory!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The voice of the Lord twists mighty oaks and strips the forests bare. In his Temple everyone shouts, “Glory!”"
      }
    },
