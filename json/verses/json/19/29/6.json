      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他也使之跳躍如牛犢， 使黎巴嫩和西連跳躍如野牛犢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He makes Lebanon skip like a calf, And Sirion like a young wild ox."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He makes Lebanon leap like a calf, Sirion like a young wild ox."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He makes Lebanon’s mountains skip like a calf; he makes Mount Hermon leap like a young wild ox."
      }
    },
