    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "却不能救赎自己的生命， 也无法向上帝付生命的赎价。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
