      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为他们死后什么也带不走， 再不能拥有荣华富贵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他死的時候，甚麼也不能帶走； 他的榮華也不能隨他下去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，他死的時候甚麼也不能帶去； 他的榮耀不能隨他下去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For when he dies he will carry nothing away; His glory will not descend after him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for they will take nothing with them when they die, their splendor will not descend with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For when they die, they take nothing with them. Their wealth will not follow them into the grave."
      }
