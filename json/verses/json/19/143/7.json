    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你快快应允我！ 我已绝望，求你不要掩面不顾我， 不然我必和下坟墓的人无异。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你快快應允我， 我的心靈已經衰竭， 求你不要向我掩面， 免得我像那些下坑的人一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你速速應允我！ 我心神耗盡！ 不要向我掩面， 免得我像那些下坑的人一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Answer me quickly, O Lord, my spirit fails; Do not hide Your face from me, Or I will become like those who go down to the pit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Answer me quickly, Lord; my spirit fails. Do not hide your face from me or I will be like those who go down to the pit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Come quickly, Lord, and answer me, for my depression deepens. Don’t turn away from me, or I will die."
      }
    },
