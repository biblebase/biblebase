    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我举手向你呼求， 我的心渴慕你如同干旱之地渴慕甘霖。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我向你伸出雙手， 我的心渴想你，好像乾旱的地盼望雨水一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我向你舉手； 我的心渴想你， 如乾旱之地盼雨一樣。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I stretch out my hands to You; My soul longs for You, as a parched land. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I spread out my hands to you; I thirst for you like a parched land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I lift my hands to you in prayer. I thirst for you as parched land thirsts for rain. Interlude"
      }
    },
