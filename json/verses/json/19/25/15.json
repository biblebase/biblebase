    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我常常仰望耶和华， 因为唯有祂能救我脱离网罗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的眼睛時常仰望耶和華， 因為他必使我的腳脫離網羅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的眼目時常仰望耶和華， 因為他必將我的腳從網裏拉出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My eyes are continually toward the Lord, For He will pluck my feet out of the net."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My eyes are ever on the Lord, for only he will release my feet from the snare."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My eyes are always on the Lord, for he rescues me from the traps of my enemies."
      }
    },
