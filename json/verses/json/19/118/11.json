    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们四面围住我， 但我靠着耶和华消灭了他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們圍繞著我，把我圍困， 我靠著耶和華的名必除滅他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們環繞我，圍困我， 我靠耶和華的名必剿滅他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They surrounded me, yes, they surrounded me; In the name of the Lord I will surely cut them off."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They surrounded me on every side, but in the name of the Lord I cut them down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, they surrounded and attacked me, but I destroyed them all with the authority of the Lord."
      }
