    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你必赐福给义人， 你的恩惠像盾牌四面保护他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！因為你必賜福給義人， 你要以恩惠像盾牌四面護衛他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你必賜福與義人； 耶和華啊，你必用恩惠如同盾牌四面護衛他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it is You who blesses the righteous man, O Lord, You surround him with favor as with a shield."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely, Lord, you bless the righteous; you surround them with your favor as with a shield."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you bless the godly, O Lord; you surround them with your shield of love."
      }
