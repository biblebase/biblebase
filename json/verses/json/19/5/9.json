    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们口中毫无实话， 心里充满恶毒， 喉咙是敞开的坟墓， 舌头上尽是诡诈。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們的口裡沒有真話， 他們的內心充滿毀滅人的意圖， 他們的喉嚨是敞開的墳墓， 他們用舌頭弄詭詐。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，他們的口中沒有誠實； 他們的心裏滿有邪惡； 他們的喉嚨是敞開的墳墓； 他們用舌頭諂媚人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is nothing reliable in what they say; Their inward part is destruction itself. Their throat is an open grave; They flatter with their tongue."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Not a word from their mouth can be trusted; their heart is filled with malice. Their throat is an open grave; with their tongues they tell lies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My enemies cannot speak a truthful word. Their deepest desire is to destroy others. Their talk is foul, like the stench from an open grave. Their tongues are filled with flattery."
      }
    },
