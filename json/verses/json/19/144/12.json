    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿我们的儿子年轻时像茁壮的树木， 我们的女儿像宫殿中精雕的柱子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願我們的兒子，在幼年時都像旺盛的樹木； 願我們的女兒如同殿四角的柱子，為建造殿宇而鑿成的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們的兒子從幼年好像樹栽子長大； 我們的女兒如同殿角石， 是按建宮的樣式鑿成的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let our sons in their youth be as grown-up plants, And our daughters as corner pillars fashioned as for a palace;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then our sons in their youth will be like well-nurtured plants, and our daughters will be like pillars carved to adorn a palace."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May our sons flourish in their youth like well-nurtured plants. May our daughters be like graceful pillars, carved to beautify a palace."
      }
    },
