    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，我向你呼求， 你是我的避难所， 在世上你是我的福分。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！ 我向你呼求； 我說：“你是我的避難所， 是我在活人之地的業分。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我曾向你哀求。 我說：你是我的避難所； 在活人之地，你是我的福分。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I cried out to You, O Lord; I said, “You are my refuge, My portion in the land of the living."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I cry to you, Lord; I say, “You are my refuge, my portion in the land of the living.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I pray to you, O Lord. I say, “You are my place of refuge. You are all I really want in life."
      }
    },
