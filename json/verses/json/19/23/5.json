    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在我敌人面前你为我摆设宴席， 又用膏油浇我的头， 使我恩福满溢。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在我敵人面前，你為我擺設筵席； 你用油膏了我的頭， 使我的杯滿溢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在我敵人面前，你為我擺設筵席； 你用油膏了我的頭，使我的福杯滿溢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You prepare a table before me in the presence of my enemies; You have anointed my head with oil; My cup overflows."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You prepare a table before me in the presence of my enemies. You anoint my head with oil; my cup overflows."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You prepare a feast for me in the presence of my enemies. You honor me by anointing my head with oil. My cup overflows with blessings."
      }
    },
