    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 你向我施恩，我便稳固如山； 你掩面不理我，我就惊慌失措。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你的恩寵，使我堅立，如同大山； 你一掩面，我就驚惶。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你曾施恩，叫我的江山穩固； 你掩了面，我就驚惶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, by Your favor You have made my mountain to stand strong; You hid Your face, I was dismayed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, when you favored me, you made my royal mountain stand firm; but when you hid your face, I was dismayed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your favor, O Lord, made me as secure as a mountain. Then you turned away from me, and I was shattered."
      }
