        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人必如生机勃勃的棕榈树， 像茁壮生长的黎巴嫩香柏树。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人必像棕樹一樣繁茂， 像黎巴嫩的香柏樹一樣高聳。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人要發旺如棕樹， 生長如黎巴嫩的香柏樹。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The righteous man will flourish like the palm tree, He will grow like a cedar in Lebanon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The righteous will flourish like a palm tree, they will grow like a cedar of Lebanon;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the godly will flourish like palm trees and grow strong like the cedars of Lebanon."
      }
    },
