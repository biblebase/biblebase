    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你的作为使我快乐， 我要颂扬你手所做的工。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你藉著你的作為使我歡喜， 我要因你手的工作歡呼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因你－耶和華藉着你的作為叫我高興， 我要因你手的工作歡呼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You, O Lord, have made me glad by what You have done, I will sing for joy at the works of Your hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For you make me glad by your deeds, Lord; I sing for joy at what your hands have done."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You thrill me, Lord, with all you have done for me! I sing for joy because of what you have done."
      }
