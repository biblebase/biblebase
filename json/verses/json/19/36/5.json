    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你的慈爱广及诸天， 你的信实高达穹苍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你的慈愛上及諸天， 你的信實高達雲霄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你的慈愛上及諸天； 你的信實達到穹蒼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your lovingkindness, O Lord, extends to the heavens, Your faithfulness reaches to the skies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your love, Lord, reaches to the heavens, your faithfulness to the skies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your unfailing love, O Lord, is as vast as the heavens; your faithfulness reaches beyond the clouds."
      }
