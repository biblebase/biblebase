    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们自以为是，浑然不知自己的罪， 也不憎恶自己的罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "罪過媚惑他， 因此在他眼中看來， 自己的罪孽不會揭發，也不會被恨惡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他自誇自媚， 以為他的罪孽終不顯露，不被恨惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it flatters him in his own eyes Concerning the discovery of his iniquity and the hatred of it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In their own eyes they flatter themselves too much to detect or hate their sin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In their blind conceit, they cannot see how wicked they really are."
      }
