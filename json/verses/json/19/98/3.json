    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂没有忘记以慈爱和信实对待以色列人， 普天下都看见我们的上帝拯救了我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他記念他向以色列家所應許的慈愛和信實， 地的四極都看見我們　神的救恩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "記念他向以色列家所發的慈愛，所憑的信實。 地的四極都看見我們神的救恩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has remembered His lovingkindness and His faithfulness to the house of Israel; All the ends of the earth have seen the salvation of our God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He has remembered his love and his faithfulness to Israel; all the ends of the earth have seen the salvation of our God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He has remembered his promise to love and be faithful to Israel. The ends of the earth have seen the victory of our God."
      }
    },
