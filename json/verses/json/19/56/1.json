    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，求你怜悯我， 因为仇敌攻击我，整日迫害我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！求你恩待我，因為人要踐踏我； 他們終日攻擊我，迫害我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，求你憐憫我，因為人要把我吞了， 終日攻擊欺壓我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be gracious to me, O God, for man has trampled upon me; Fighting all day long he oppresses me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be merciful to me, my God, for my enemies are in hot pursuit; all day long they press their attack."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O God, have mercy on me, for people are hounding me. My foes attack me all day long."
      }
    },
