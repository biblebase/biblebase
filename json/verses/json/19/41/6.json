    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们来看我时， 心怀恶意，满口谎言， 出去后散布流言。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "即使他來看我，說的也是假話； 他把奸詐積存在心裡， 走到外面才說出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他來看我就說假話； 他心存奸惡，走到外邊才說出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And when he comes to see me, he speaks falsehood; His heart gathers wickedness to itself; When he goes outside, he tells it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When one of them comes to see me, he speaks falsely, while his heart gathers slander; then he goes out and spreads it around."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They visit me as if they were my friends, but all the while they gather gossip, and when they leave, they spread it everywhere."
      }
    },
