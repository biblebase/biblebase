    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我知道你喜悦我， 因为你没有让仇敌胜过我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此我就知道你喜愛我， 因為我的仇敵不能向我歡呼誇勝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因我的仇敵不得向我誇勝， 我從此便知道你喜愛我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By this I know that You are pleased with me, Because my enemy does not shout in triumph over me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I know that you are pleased with me, for my enemy does not triumph over me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I know you are pleased with me, for you have not let my enemies triumph over me."
      }
    },
