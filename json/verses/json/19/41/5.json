    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的仇敌恶狠狠地说： “他何时才会死， 并且被人遗忘呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的仇敵用惡毒的話中傷我，說： “他甚麼時候死呢？他的名字甚麼時候消滅呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的仇敵用惡言議論我說： 他幾時死，他的名才滅亡呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My enemies speak evil against me, “When will he die, and his name perish?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My enemies say of me in malice, “When will he die and his name perish?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But my enemies say nothing but evil about me. “How soon will he die and be forgotten?” they ask."
      }
