    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊！ 愿你的慈爱临到我， 愿你的拯救临到我， 正如你的应许，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！願你照著你的應許， 使你的慈愛、你的救恩臨到我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，願你照你的話，使你的慈愛， 就是你的救恩，臨到我身上，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May Your lovingkindnesses also come to me, O Lord, Your salvation according to Your word;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May your unfailing love come to me, Lord, your salvation, according to your promise;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, give me your unfailing love, the salvation that you promised me."
      }
    },
