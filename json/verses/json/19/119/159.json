    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 你知道我多么爱你的法则， 求你施慈爱保护我的生命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你看我多麼愛你的訓詞； 耶和華啊！求你按著你的慈愛使我存活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你看我怎樣愛你的訓詞！ 耶和華啊，求你照你的慈愛將我救活！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Consider how I love Your precepts; Revive me, O Lord, according to Your lovingkindness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See how I love your precepts; preserve my life, Lord, in accordance with your love."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "See how I love your commandments, Lord. Give back my life because of your unfailing love."
      }
    },
