    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的嘴巴充满罪恶， 口中尽是咒诅和谎言， 愿他们陷在狂傲中不能自拔。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因他們口中的罪， 因他們嘴裡所說的話， 願他們在自己的驕傲中被纏住。 因他們所說的是咒罵和欺騙的話，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他們口中的罪和嘴裏的言語， 並咒罵虛謊的話， 願他們在驕傲之中被纏住了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On account of the sin of their mouth and the words of their lips, Let them even be caught in their pride, And on account of curses and lies which they utter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the sins of their mouths, for the words of their lips, let them be caught in their pride. For the curses and lies they utter,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because of the sinful things they say, because of the evil that is on their lips, let them be captured by their pride, their curses, and their lies."
      }
