    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又使我们成为邻国争夺的对象， 仇敌都嘲笑我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使我們的鄰國因我們紛爭， 我們的仇敵也譏笑我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你使鄰邦因我們紛爭； 我們的仇敵彼此戲笑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You make us an object of contention to our neighbors, And our enemies laugh among themselves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have made us an object of derision to our neighbors, and our enemies mock us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have made us the scorn of neighboring nations. Our enemies treat us as a joke."
      }
