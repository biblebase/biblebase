    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你是保护我的上帝， 你为何丢弃我？ 为何让我受仇敌的攻击， 使我哀伤不已呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你是賜我力量的　神。 你為甚麼棄絕我呢？ 我為甚麼因仇敵的壓迫徘徊悲哀呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你是賜我力量的神，為何丟棄我呢？ 我為何因仇敵的欺壓時常哀痛呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You are the God of my strength; why have You rejected me? Why do I go mourning because of the oppression of the enemy?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are God my stronghold. Why have you rejected me? Why must I go about mourning, oppressed by the enemy?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you are God, my only safe haven. Why have you tossed me aside? Why must I wander around in grief, oppressed by my enemies?"
      }
    },
