    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的心啊！ 你为何沮丧？为何烦躁？ 要仰望上帝， 因为我还要赞美祂——我的救主，我的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的心哪！你為甚麼沮喪呢？ 為甚麼在我裡面不安呢？ 應當等候　神； 因為我還要稱讚他， 他是我面前的救助、我的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的心哪，你為何憂悶？ 為何在我裏面煩躁？ 應當仰望神，因我還要稱讚他。 他是我臉上的光榮，是我的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why are you in despair, O my soul? And why are you disturbed within me? Hope in God, for I shall again praise Him, The help of my countenance and my God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why, my soul, are you downcast? Why so disturbed within me? Put your hope in God, for I will yet praise him, my Savior and my God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why am I discouraged? Why is my heart so sad? I will put my hope in God! I will praise him again— my Savior and my God!"
      }
    },
