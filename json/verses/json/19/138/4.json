    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，世上所有的君王听见你的话都要称谢你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！世上的君王都要稱謝你， 因為他們聽見了你口中的言語。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，地上的君王都要稱謝你， 因他們聽見了你口中的言語。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the kings of the earth will give thanks to You, O Lord, When they have heard the words of Your mouth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May all the kings of the earth praise you, Lord, when they hear what you have decreed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Every king in all the earth will thank you, Lord, for all of them will hear your words."
      }
    },
