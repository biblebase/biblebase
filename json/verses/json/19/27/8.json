    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你说：“来寻求我！” 我心中响应： “耶和华啊，我要寻求你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你說：“你們要尋求我的面！” 那時我的心對你說： “耶和華啊！你的面我正要尋求。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你說：你們當尋求我的面。 那時我心向你說：耶和華啊， 你的面我正要尋求。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When You said, “Seek My face,” my heart said to You, “Your face, O Lord, I shall seek.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My heart says of you, “Seek his face!” Your face, Lord, I will seek."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My heart has heard you say, “Come and talk with me.” And my heart responds, “Lord, I am coming.”"
      }
