    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你指教我行你的道， 引导我走正路，远离仇敌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你指示我你的道路； 為我仇敵的緣故引導我走平坦的路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你將你的道指教我， 因我仇敵的緣故引導我走平坦的路。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Teach me Your way, O Lord, And lead me in a level path Because of my foes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Teach me your way, Lord; lead me in a straight path because of my oppressors."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Teach me how to live, O Lord. Lead me along the right path, for my enemies are waiting for me."
      }
    },
