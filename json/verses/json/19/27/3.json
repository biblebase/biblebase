    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然大军围攻我， 我心中却一无所惧； 虽然战争来临，我仍满怀信心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖有軍兵紮營攻擊我， 我的心也不害怕； 雖然發動戰爭攻擊我， 我仍舊安穩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雖有軍兵安營攻擊我，我的心也不害怕； 雖然興起刀兵攻擊我，我必仍舊安穩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Though a host encamp against me, My heart will not fear; Though war arise against me, In spite of this I shall be confident."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Though an army besiege me, my heart will not fear; though war break out against me, even then I will be confident."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Though a mighty army surrounds me, my heart will not be afraid. Even if I am attacked, I will remain confident."
      }
