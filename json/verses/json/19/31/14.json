    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然而，耶和华啊， 我依然信靠你； 我说：“你是我的上帝。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是，耶和華啊！我還是倚靠你； 我說：“你是我的　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我仍舊倚靠你； 我說：你是我的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, I trust in You, O Lord, I say, “You are my God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I trust in you, Lord; I say, “You are my God.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
