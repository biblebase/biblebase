    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你是我的避难所， 求你救我脱离人们为我设下的陷阱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你救我脫離人為我暗設的網羅， 因為你是我的避難所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你救我脫離人為我暗設的網羅， 因為你是我的保障。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will pull me out of the net which they have secretly laid for me, For You are my strength."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Keep me free from the trap that is set for me, for you are my refuge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Pull me from the trap my enemies set for me, for I find protection in you alone."
      }
    },
