    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你侧耳听我的呼求， 快来救我，作我坚固的避难所， 作拯救我的堡垒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你留心聽我，趕快拯救我； 求你作我堅固的磐石， 作拯救我的堅壘。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你側耳而聽， 快快救我！ 作我堅固的磐石， 拯救我的保障！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Incline Your ear to me, rescue me quickly; Be to me a rock of strength, A stronghold to save me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Turn your ear to me, come quickly to my rescue; be my rock of refuge, a strong fortress to save me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Turn your ear to listen to me; rescue me quickly. Be my rock of protection, a fortress where I will be safe."
      }
    },
