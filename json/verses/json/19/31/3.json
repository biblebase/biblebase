    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你是我的磐石，我的堡垒， 求你为了自己的名而引导我，带领我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你是我的巖石、我的堅壘； 為你名的緣故，求你帶領我，引導我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你是我的巖石，我的山寨； 所以，求你為你名的緣故引導我，指點我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You are my rock and my fortress; For Your name’s sake You will lead me and guide me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Since you are my rock and my fortress, for the sake of your name lead and guide me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You are my rock and my fortress. For the honor of your name, lead me out of this danger."
      }
    },
