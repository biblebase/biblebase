      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华当受称颂！ 因为我被困城中时， 祂以奇妙的爱待我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華是應當稱頌的， 因為我在被圍困的城裡， 他就向我顯出他奇妙的慈愛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華是應當稱頌的， 因為他在堅固城裏向我施展奇妙的慈愛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Blessed be the Lord, For He has made marvelous His lovingkindness to me in a besieged city."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Praise be to the Lord, for he showed me the wonders of his love when I was in a city under siege."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Praise the Lord, for he has shown me the wonders of his unfailing love. He kept me safe when my city was under attack."
      }
    },
