    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华忠心的子民啊， 你们要爱祂。 祂保护忠心的人， 严惩骄傲的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的眾聖民哪！你們都要愛耶和華； 耶和華保護誠實的人， 卻嚴厲地報應行事驕傲的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的聖民哪，你們都要愛他！ 耶和華保護誠實人， 足足報應行事驕傲的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O love the Lord, all you His godly ones! The Lord preserves the faithful And fully recompenses the proud doer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Love the Lord, all his faithful people! The Lord preserves those who are true to him, but the proud he pays back in full."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Love the Lord, all you godly ones! For the Lord protects those who are loyal to him, but he harshly punishes the arrogant."
      }
    },
