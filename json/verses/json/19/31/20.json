    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你把他们藏在你那里， 使他们得到庇护， 免遭世人暗算。 你使他们在你的居所安然无恙， 免受恶言恶语的攻击。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你把他們藏在你面前的隱密處， 免得他們陷在世人的陰謀裡； 又把他們保護在帳棚內，以免落在口舌的爭競中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必把他們藏在你面前的隱密處， 免得遇見人的計謀； 你必暗暗地保守他們在亭子裏， 免受口舌的爭鬧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You hide them in the secret place of Your presence from the conspiracies of man; You keep them secretly in a shelter from the strife of tongues."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the shelter of your presence you hide them from all human intrigues; you keep them safe in your dwelling from accusing tongues."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You hide them in the shelter of your presence, safe from those who conspire against them. You shelter them in your presence, far from accusing tongues."
      }
    },
