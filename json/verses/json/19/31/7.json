      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will rejoice and be glad in Your lovingkindness, Because You have seen my affliction; You have known the troubles of my soul,"
      },
