    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你昼夜管教我， 我的精力耗尽， 如水在盛夏枯竭。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你的手晝夜重壓在我身上， 我的精力耗盡，好像盛暑的乾旱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "黑夜白日，你的手在我身上沉重； 我的精液耗盡，如同夏天的乾旱。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For day and night Your hand was heavy upon me; My vitality was drained away as with the fever heat of summer. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For day and night your hand was heavy on me; my strength was sapped as in the heat of summer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Day and night your hand of discipline was heavy on me. My strength evaporated like water in the summer heat. Interlude"
      }
    },
