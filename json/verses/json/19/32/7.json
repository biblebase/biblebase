    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你是我的藏身之所， 你保护我免遭危难， 用得胜的凯歌四面环绕我。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你是我藏身之處， 你必保護我脫離患難， 以得救的歡呼四面環繞我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你是我藏身之處； 你必保佑我脫離苦難， 以得救的樂歌四面環繞我。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You are my hiding place; You preserve me from trouble; You surround me with songs of deliverance. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are my hiding place; you will protect me from trouble and surround me with songs of deliverance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you are my hiding place; you protect me from trouble. You surround me with songs of victory. Interlude"
      }
    },
