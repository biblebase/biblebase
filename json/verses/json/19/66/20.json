    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝当受称颂！ 祂没有对我的祷告拒而不听， 也没有收回祂对我的慈爱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神是應當稱頌的， 他沒有拒絕我的禱告， 也沒有使他的慈愛離開我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神是應當稱頌的！ 他並沒有推卻我的禱告， 也沒有叫他的慈愛離開我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Blessed be God, Who has not turned away my prayer Nor His lovingkindness from me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Praise be to God, who has not rejected my prayer or withheld his love from me!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Praise God, who did not ignore my prayer or withdraw his unfailing love from me."
      }
