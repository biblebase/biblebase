      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你保存我的性命，因我是虔誠人。 我的神啊，求你拯救這倚靠你的僕人！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Preserve my soul, for I am a godly man; O You my God, save Your servant who trusts in You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Guard my life, for I am faithful to you; save your servant who trusts in you. You are my God;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Protect me, for I am devoted to you. Save me, for I serve you and trust you. You are my God."
      }
