    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你教我行你的道， 我要行在你的真理中， 求你使我一心敬畏你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你把你的道路指教我， 使我行在你的真理中， 專心敬畏你的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你將你的道指教我； 我要照你的真理行； 求你使我專心敬畏你的名！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Teach me Your way, O Lord; I will walk in Your truth; Unite my heart to fear Your name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Teach me your way, Lord, that I may rely on your faithfulness; give me an undivided heart, that I may fear your name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Teach me your ways, O Lord, that I may live according to your truth! Grant me purity of heart, so that I may honor you."
      }
    },
