    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们说： “来吧，让我们铲除以色列， 让他们亡国，被人遗忘。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們說：“來吧！我們來把他們除滅，使他們不再成國， 使以色列的名不再被人記念。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們說：來吧，我們將他們剪滅， 使他們不再成國！ 使以色列的名不再被人記念！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have said, “Come, and let us wipe them out as a nation, That the name of Israel be remembered no more.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Come,” they say, “let us destroy them as a nation, so that Israel’s name is remembered no more.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Come,” they say, “let us wipe out Israel as a nation. We will destroy the very memory of its existence.”"
      }
