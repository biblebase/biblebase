    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "唯有你当受敬畏。 你发怒的时候， 谁能在你面前站立呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "唯有你是可畏的， 你的烈怒一發出，誰能在你面前站立得住呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟獨你是可畏的！ 你怒氣一發，誰能在你面前站得住呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You, even You, are to be feared; And who may stand in Your presence when once You are angry?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is you alone who are to be feared. Who can stand before you when you are angry?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No wonder you are greatly feared! Who can stand before you when your anger explodes?"
      }
