    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂说：“把我忠心的子民——那些借着献祭和我立约的人招聚到我这里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們把我的聖民聚集到我這裡來， 就是那些用祭物與我立約的人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：招聚我的聖民到我這裏來， 就是那些用祭物與我立約的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Gather My godly ones to Me, Those who have made a covenant with Me by sacrifice.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Gather to me this consecrated people, who made a covenant with me by sacrifice.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Bring my faithful people to me— those who made a covenant with me by giving sacrifices.”"
      }
    },
