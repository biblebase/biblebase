    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们生病时， 我披上麻衣， 谦卑地为他们禁食祷告， 但上帝没有垂听。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於我，他們有病的時候， 我就穿上麻衣， 禁食刻苦己心； 我心裡也不住地禱告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於我，當他們有病的時候， 我便穿麻衣，禁食，刻苦己心； 我所求的都歸到自己的懷中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, when they were sick, my clothing was sackcloth; I humbled my soul with fasting, And my prayer kept returning to my bosom."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet when they were ill, I put on sackcloth and humbled myself with fasting. When my prayers returned to me unanswered,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet when they were ill, I grieved for them. I denied myself by fasting for them, but my prayers returned unanswered."
      }
    },
