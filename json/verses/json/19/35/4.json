      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let those be ashamed and dishonored who seek my life; Let those be turned back and humiliated who devise evil against me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May those who seek my life be disgraced and put to shame; may those who plot my ruin be turned back in dismay."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Bring shame and disgrace on those trying to kill me; turn them back and humiliate those who want to harm me."
      }
    },
