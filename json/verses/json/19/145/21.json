      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要开口赞美耶和华， 万物都要称颂祂的圣名， 直到永永远远。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的口要說讚美耶和華的話； 願所有的人都永永遠遠稱頌他的聖名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的口要說出讚美耶和華的話； 惟願凡有血氣的都永永遠遠稱頌他的聖名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My mouth will speak the praise of the Lord, And all flesh will bless His holy name forever and ever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My mouth will speak in praise of the Lord. Let every creature praise his holy name for ever and ever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will praise the Lord, and may everyone on earth bless his holy name forever and ever."
      }
    },
