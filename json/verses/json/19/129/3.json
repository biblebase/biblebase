    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们鞭打我的背， 伤痕如同农夫耕出的长长犁沟。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們好像犁田的人犁在我的背上， 所犁的溝甚長。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "如同扶犂的在我背上扶犂而耕， 耕的犂溝甚長。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The plowers plowed upon my back; They lengthened their furrows.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Plowmen have plowed my back and made their furrows long."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My back is covered with cuts, as if a farmer had plowed long furrows."
      }
    },
