    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你把生命之路指示我， 你右手有永远的福乐， 我在你面前充满喜乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你必把生命的路指示我， 在你面前有滿足的喜樂， 在你的右手中有永遠的福樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必將生命的道路指示我。 在你面前有滿足的喜樂； 在你右手中有永遠的福樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will make known to me the path of life; In Your presence is fullness of joy; In Your right hand there are pleasures forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You make known to me the path of life; you will fill me with joy in your presence, with eternal pleasures at your right hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will show me the way of life, granting me the joy of your presence and the pleasures of living with you forever."
      }
    },
