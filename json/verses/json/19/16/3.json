    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世上敬畏你的人极其尊贵， 是我所喜爱的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於世上的聖民， 他們都是尊貴榮美的人，全是我所喜愛的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論到世上的聖民，他們又美又善， 是我最喜悅的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As for the saints who are in the earth, They are the majestic ones in whom is all my delight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I say of the holy people who are in the land, “They are the noble ones in whom is all my delight.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The godly people in the land are my true heroes! I take pleasure in them!"
      }
