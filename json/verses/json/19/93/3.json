    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 大海澎湃怒吼，波浪滔天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！大水揚起了， 大水揚起了聲音， 大水揚起了澎湃的波浪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，大水揚起， 大水發聲，波浪澎湃。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The floods have lifted up, O Lord, The floods have lifted up their voice, The floods lift up their pounding waves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The seas have lifted up, Lord, the seas have lifted up their voice; the seas have lifted up their pounding waves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The floods have risen up, O Lord. The floods have roared like thunder; the floods have lifted their pounding waves."
      }
