      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道创造耳朵的上帝听不见吗？ 难道创造眼睛的上帝看不见吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那造耳朵的，自己不能聽見嗎？ 那造眼睛的，自己不能看見嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "造耳朵的，難道自己不聽見嗎？ 造眼睛的，難道自己不看見嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who planted the ear, does He not hear? He who formed the eye, does He not see?"
      },
