    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使他们在患难中有平安， 恶人终必灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在患難的日子，你使他得享平安， 直到為惡人所預備的坑挖好了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你使他在遭難的日子得享平安； 惟有惡人陷在所挖的坑中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "That You may grant him relief from the days of adversity, Until a pit is dug for the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "you grant them relief from days of trouble, till a pit is dug for the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You give them relief from troubled times until a pit is dug to capture the wicked."
      }
    },
