    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "借律例制造不幸的首领， 岂能与你联盟？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "憑著律例製造奸惡， 藉著權位行毀滅的，怎能和你相交呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那藉着律例架弄殘害、 在位上行奸惡的，豈能與你相交嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Can a throne of destruction be allied with You, One which devises mischief by decree?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Can a corrupt throne be allied with you— a throne that brings on misery by its decrees?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can unjust leaders claim that God is on their side— leaders whose decrees permit injustice?"
      }
    },
