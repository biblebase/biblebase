      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "施恩與人、借貸與人的，這人事情順利； 他被審判的時候要訴明自己的冤。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is well with the man who is gracious and lends; He will maintain his cause in judgment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Good will come to those who are generous and lend freely, who conduct their affairs with justice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Good comes to those who lend money generously and conduct their business fairly."
      }
    },
