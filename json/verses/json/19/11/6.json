    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂要把烈焰熊熊的火炭和硫磺降在恶人身上， 用炙热的风惩罚他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必使火炭落在惡人身上， 烈火、硫磺和旱風是他們杯中的分。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他要向惡人密布網羅； 有烈火、硫磺、熱風，作他們杯中的分。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Upon the wicked He will rain snares; Fire and brimstone and burning wind will be the portion of their cup."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the wicked he will rain fiery coals and burning sulfur; a scorching wind will be their lot."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will rain down blazing coals and burning sulfur on the wicked, punishing them with scorching winds."
      }
    },
