        "lang": "cht",
        "text": "耶和華啊，你的名存到永遠！ 耶和華啊，你可記念的名存到萬代！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your name, O Lord, is everlasting, Your remembrance, O Lord, throughout all generations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your name, Lord, endures forever, your renown, Lord, through all generations."
      },
