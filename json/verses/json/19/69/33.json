    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华垂听穷苦人的祈求， 不轻看祂被囚的子民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華垂聽了貧窮人的禱告， 也不藐視屬他自己、正被囚禁的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為耶和華聽了窮乏人， 不藐視被囚的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord hears the needy And does not despise His who are prisoners."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord hears the needy and does not despise his captive people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Lord hears the cries of the needy; he does not despise his imprisoned people."
      }
    },
