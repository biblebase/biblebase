    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列各支派，耶和华的子民， 都遵照祂赐的法度去那里称谢祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾支派，就是耶和華的支派， 都上那裡去； 照著以色列的定例， 稱頌耶和華的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾支派，就是耶和華的支派，上那裏去， 按以色列的常例稱讚耶和華的名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To which the tribes go up, even the tribes of the Lord— An ordinance for Israel— To give thanks to the name of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "That is where the tribes go up— the tribes of the Lord— to praise the name of the Lord according to the statute given to Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the tribes of Israel—the Lord’s people— make their pilgrimage here. They come to give thanks to the name of the Lord, as the law requires of Israel."
      }
