    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我精疲力尽，彻底崩溃； 我心中悲伤，呻吟不止。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我已經疲乏無力，被壓得粉碎了； 我因心裡痛苦而唉哼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我被壓傷，身體疲倦； 因心裏不安，我就唉哼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am benumbed and badly crushed; I groan because of the agitation of my heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am feeble and utterly crushed; I groan in anguish of heart."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am exhausted and completely crushed. My groans come from an anguished heart."
      }
    },
