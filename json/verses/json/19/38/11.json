        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因我的疾病，朋友同伴回避我， 亲人远离我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的良朋密友因我的災禍，都站到一旁去； 我的親人也都站得遠遠的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的良朋密友因我的災病都躲在旁邊站着； 我的親戚本家也遠遠地站立。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My loved ones and my friends stand aloof from my plague; And my kinsmen stand afar off."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My friends and companions avoid me because of my wounds; my neighbors stay far away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My loved ones and friends stay away, fearing my disease. Even my own family stands at a distance."
      }
    },
