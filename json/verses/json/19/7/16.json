      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的恶行临到自己头上， 他们的暴力落到自己脑壳上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的惡毒必回到自己的頭上， 他的強暴必落在自己的頭頂上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的毒害必臨到他自己的頭上； 他的強暴必落到他自己的腦袋上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His mischief will return upon his own head, And his violence will descend upon his own pate."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The trouble they cause recoils on them; their violence comes down on their own heads."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The trouble they make for others backfires on them. The violence they plan falls on their own heads."
      }
