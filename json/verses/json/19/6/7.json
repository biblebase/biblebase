    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的眼睛因忧愁而模糊， 因敌人的攻击而昏花。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我因愁煩眼目昏花， 因眾多的仇敵視力衰退。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因憂愁眼睛乾癟， 又因我一切的敵人眼睛昏花。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My eye has wasted away with grief; It has become old because of all my adversaries."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My eyes grow weak with sorrow; they fail because of all my foes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My vision is blurred by grief; my eyes are worn out because of all my enemies."
      }
    },
