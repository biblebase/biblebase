        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂向自己的子民彰显大能， 把列国的土地赐给他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他向自己的子民彰顯大能的作為， 把列國賜給他們為業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他向百姓顯出大能的作為， 把外邦的地賜給他們為業。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has made known to His people the power of His works, In giving them the heritage of the nations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He has shown his people the power of his works, giving them the lands of other nations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He has shown his great power to his people by giving them the lands of other nations."
      }
