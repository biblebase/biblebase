    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "仇敌向我咆哮， 恶人迫害我。 他们带给我苦难， 怒气冲冲地辱骂我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這都是由於仇敵的聲音， 和惡人的欺壓； 因為他們使禍患臨到我的身上， 怒氣沖沖地迫害我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "都因仇敵的聲音，惡人的欺壓； 因為他們將罪孽加在我身上，發怒氣逼迫我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because of the voice of the enemy, Because of the pressure of the wicked; For they bring down trouble upon me And in anger they bear a grudge against me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "because of what my enemy is saying, because of the threats of the wicked; for they bring down suffering on me and assail me in their anger."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My enemies shout at me, making loud and wicked threats. They bring trouble on me and angrily hunt me down."
      }
    },
