    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁明白你愤怒的威力？ 有谁因为明白你的烈怒而对你心存敬畏呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "誰曉得你怒氣的威力， 誰按著你該受的敬畏曉得你的忿怒呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誰曉得你怒氣的權勢？ 誰按着你該受的敬畏曉得你的忿怒呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who understands the power of Your anger And Your fury, according to the fear that is due You?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If only we knew the power of your anger! Your wrath is as great as the fear that is your due."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Who can comprehend the power of your anger? Your wrath is as awesome as the fear you deserve."
      }
