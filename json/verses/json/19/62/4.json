    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们千方百计把我从高位拉下。 你们善于说谎，嘴上祝福， 心却咒诅。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們彼此商議， 務要把他從尊位上拉下來； 他們喜愛謊話； 他們口雖祝福，心卻咒詛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們彼此商議，專要從他的尊位上把他推下； 他們喜愛謊話，口雖祝福，心卻咒詛。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have counseled only to thrust him down from his high position; They delight in falsehood; They bless with their mouth, But inwardly they curse. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely they intend to topple me from my lofty place; they take delight in lies. With their mouths they bless, but in their hearts they curse."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They plan to topple me from my high position. They delight in telling lies about me. They praise me to my face but curse me in their hearts. Interlude"
      }
    },
