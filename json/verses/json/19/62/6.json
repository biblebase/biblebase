    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "唯有祂才是我的磐石， 我的拯救，我的堡垒， 我必不致动摇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "唯獨他是我的磐石，我的拯救； 他是我的保障，我必不至動搖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟獨他是我的磐石，我的拯救； 他是我的高臺，我必不動搖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He only is my rock and my salvation, My stronghold; I shall not be shaken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Truly he is my rock and my salvation; he is my fortress, I will not be shaken."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He alone is my rock and my salvation, my fortress where I will not be shaken."
      }
