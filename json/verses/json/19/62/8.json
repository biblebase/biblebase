    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众百姓啊， 要时刻信靠上帝， 向祂倾心吐意， 因为祂是我们的避难所。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾民哪！你們要常常倚靠他， 在他面前傾心吐意； 因為　神是我們的避難所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們眾民當時時倚靠他， 在他面前傾心吐意； 神是我們的避難所。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Trust in Him at all times, O people; Pour out your heart before Him; God is a refuge for us. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Trust in him at all times, you people; pour out your hearts to him, for God is our refuge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O my people, trust in him at all times. Pour out your heart to him, for God is our refuge. Interlude"
      }
    },
