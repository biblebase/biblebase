    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要称谢那位带领其子民走过旷野的， 因为祂的慈爱永远长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要稱謝那引導自己的子民走過曠野的， 因為他的慈愛永遠長存。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "稱謝那引導自己的民行走曠野的， 因他的慈愛永遠長存。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To Him who led His people through the wilderness, For His lovingkindness is everlasting;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "to him who led his people through the wilderness; His love endures forever."
