    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂把他们的土地赐给祂的仆人以色列人作产业， 因为祂的慈爱永远长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把他們的地賜給他的僕人以色列作產業， 因為他的慈愛永遠長存。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是賜他的僕人以色列為業， 因他的慈愛永遠長存。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even a heritage to Israel His servant, For His lovingkindness is everlasting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "an inheritance to his servant Israel. His love endures forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "a special possession to his servant Israel. His faithful love endures forever."
      }
