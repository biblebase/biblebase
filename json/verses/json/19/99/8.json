    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华，我们的上帝啊， 你应允了他们， 向他们显明你是赦罪的上帝， 但也惩罚他们的罪恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華我們的　神啊！你應允了他們； 你雖然按照他們所行的報應他們， 但對他們來說，你是一位赦免人的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－我們的神啊，你應允他們； 你是赦免他們的神， 卻按他們所行的報應他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord our God, You answered them; You were a forgiving God to them, And yet an avenger of their evil deeds."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord our God, you answered them; you were to Israel a forgiving God, though you punished their misdeeds."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord our God, you answered them. You were a forgiving God to them, but you punished them when they went wrong."
      }
    },
