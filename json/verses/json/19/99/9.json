    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要尊崇我们的上帝耶和华， 在祂的圣山上敬拜祂， 因为我们的上帝耶和华是圣洁的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要尊崇耶和華我們的　神， 在他的聖山上敬拜， 因為耶和華我們的　神是聖潔的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要尊崇耶和華－我們的神， 在他的聖山下拜， 因為耶和華－我們的神本為聖！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Exalt the Lord our God And worship at His holy hill, For holy is the Lord our God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Exalt the Lord our God and worship at his holy mountain, for the Lord our God is holy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Exalt the Lord our God, and worship at his holy mountain in Jerusalem, for the Lord our God is holy!"
      }
