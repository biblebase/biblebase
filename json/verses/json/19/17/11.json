    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们穷追围堵， 伺机把我打倒在地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們追蹤我，現在把我圍困了； 他們瞪著眼，要把我推倒在地上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們圍困了我們的腳步； 他們瞪着眼，要把我們推倒在地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have now surrounded us in our steps; They set their eyes to cast us down to the ground."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They have tracked me down, they now surround me, with eyes alert, to throw me to the ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They track me down and surround me, watching for the chance to throw me to the ground."
      }
