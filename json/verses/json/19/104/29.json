    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你若对它们弃而不顾， 它们会惊慌失措。 你一收回它们的气息， 它们便死亡，归于尘土。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你向牠們掩面，牠們就驚慌； 你收回牠們的氣息，牠們就死亡，歸回塵土。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你掩面，牠們便驚惶； 你收回牠們的氣，牠們就死亡，歸於塵土。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You hide Your face, they are dismayed; You take away their spirit, they expire And return to their dust."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you hide your face, they are terrified; when you take away their breath, they die and return to the dust."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if you turn away from them, they panic. When you take away their breath, they die and turn again to dust."
      }
    },
