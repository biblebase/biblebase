    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世人啊！你们把我的荣耀变为羞辱要到何时呢？ 你们追求虚谎之事要到何时呢？（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "尊貴的人啊！你們把我的榮耀變為羞辱，要到幾時呢？ 你們喜愛虛妄，追求虛謊，要到幾時呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們這上流人哪，你們將我的尊榮變為羞辱要到幾時呢？ 你們喜愛虛妄，尋找虛假，要到幾時呢？（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O sons of men, how long will my honor become a reproach? How long will you love what is worthless and aim at deception? Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How long will you people turn my glory into shame? How long will you love delusions and seek false gods?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How long will you people ruin my reputation? How long will you make groundless accusations? How long will you continue your lies? Interlude"
      }
    },
