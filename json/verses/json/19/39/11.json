    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为人犯罪，你管教他们， 使他们所爱的被吞噬，像被虫蛀。 世人不过是一丝气息。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你因人的罪孽，藉著責罰管教他們， 叫他們所寶貴的消失，像被蟲蛀蝕； 世人都不過是一口氣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你因人的罪惡懲罰他的時候， 叫他的笑容消滅，如衣被蟲所咬。 世人真是虛幻！（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "With reproofs You chasten a man for iniquity; You consume as a moth what is precious to him; Surely every man is a mere breath. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you rebuke and discipline anyone for their sin, you consume their wealth like a moth— surely everyone is but a breath."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When you discipline us for our sins, you consume like a moth what is precious to us. Each of us is but a breath. Interlude"
      }
    },
