    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 凡认识你名的人都必信靠你， 因为你从来不丢弃寻求你的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "認識你名的人必倚靠你； 耶和華啊！你從未撇棄尋求你的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，認識你名的人要倚靠你， 因你沒有離棄尋求你的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And those who know Your name will put their trust in You, For You, O Lord, have not forsaken those who seek You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who know your name trust in you, for you, Lord, have never forsaken those who seek you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who know your name trust in you, for you, O Lord, do not abandon those who search for you."
      }
