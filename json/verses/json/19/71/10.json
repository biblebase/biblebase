    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的仇敌议论我， 那些想杀害我的一起策划阴谋，说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為我的仇敵議論我； 那些窺探著要害我性命的，一同商議，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的仇敵議論我； 那些窺探要害我命的彼此商議，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For my enemies have spoken against me; And those who watch for my life have consulted together,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For my enemies speak against me; those who wait to kill me conspire together."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For my enemies are whispering against me. They are plotting together to kill me."
      }
    },
