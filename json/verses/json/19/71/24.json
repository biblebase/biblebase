    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要终日向人述说你的公义， 因为那些想害我的人已经蒙羞受辱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的舌頭也要終日講述你的公義， 因為那些謀求害我的人已經蒙羞受辱了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且我的舌頭必終日講論你的公義， 因為那些謀害我的人已經蒙羞受辱了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My tongue also will utter Your righteousness all day long; For they are ashamed, for they are humiliated who seek my hurt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My tongue will tell of your righteous acts all day long, for those who wanted to harm me have been put to shame and confusion."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will tell about your righteous deeds all day long, for everyone who tried to hurt me has been shamed and humiliated."
      }
    },
