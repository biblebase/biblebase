    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你拯救我脱离一切艰难， 让我傲视仇敌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他救我脫離了一切患難； 我親眼看見了我的仇敵滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他從一切的急難中把我救出來； 我的眼睛也看見了我仇敵遭報。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He has delivered me from all trouble, And my eye has looked with satisfaction upon my enemies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have delivered me from all my troubles, and my eyes have looked in triumph on my foes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you have rescued me from my troubles and helped me to triumph over my enemies."
      }
