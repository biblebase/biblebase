    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的身体还未成形， 你早已看见了。 你为我一生所定的年日， 我还没有出生就已经记在你的册子上了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我未成形的身體，你的眼睛早已看見； 為我所定的日子，我還未度過一日， 都完全記在你的冊上了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我未成形的體質，你的眼早已看見了； 你所定的日子，我尚未度一日， 你都寫在你的冊上了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your eyes have seen my unformed substance; And in Your book were all written The days that were ordained for me, When as yet there was not one of them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your eyes saw my unformed body; all the days ordained for me were written in your book before one of them came to be."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You saw me before I was born. Every day of my life was recorded in your book. Every moment was laid out before a single day had passed."
      }
    },
