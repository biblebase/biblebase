    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们高声求救，却无人搭救； 他们呼求耶和华，祂也不应允。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們呼叫，卻沒有人拯救； 就算向耶和華呼求，他也不答應他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們呼求，卻無人拯救； 就是呼求耶和華，他也不應允。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They cried for help, but there was none to save, Even to the Lord, but He did not answer them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They cried for help, but there was no one to save them— to the Lord, but he did not answer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They called for help, but no one came to their rescue. They even cried to the Lord, but he refused to answer."
      }
