    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你救我脱离我百姓的攻击， 立我做列国的元首。 素不认识的民族也归顺我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你救我脫離了人民的爭競， 你立我作列國的元首； 我不認識的人民要服事我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你救我脫離百姓的爭競， 立我作列國的元首； 我素不認識的民必事奉我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have delivered me from the contentions of the people; You have placed me as head of the nations; A people whom I have not known serve me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have delivered me from the attacks of the people; you have made me the head of nations. People I did not know now serve me,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You gave me victory over my accusers. You appointed me ruler over nations; people I don’t even know now serve me."
      }
    },
