    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂射出利箭驱散仇敌， 发出闪电击溃他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他射出箭來，使它們四散； 他連連發出閃電，使它們混亂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他射出箭來，使仇敵四散； 多多發出閃電，使他們擾亂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He sent out His arrows, and scattered them, And lightning flashes in abundance, and routed them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He shot his arrows and scattered the enemy, with great bolts of lightning he routed them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He shot his arrows and scattered his enemies; great bolts of lightning flashed, and they were confused."
      }
    },
