    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭物和供品非你所悦， 你开通了我的耳朵， 燔祭和赎罪祭非你所要。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭品和禮物不是你喜悅的。 你開通了我的耳朵； 燔祭和贖罪祭，不是你要求的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭物和禮物，你不喜悅； 你已經開通我的耳朵。 燔祭和贖罪祭非你所要。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Sacrifice and meal offering You have not desired; My ears You have opened; Burnt offering and sin offering You have not required."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Sacrifice and offering you did not desire— but my ears you have opened— burnt offerings and sin offerings you did not require."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You take no delight in sacrifices or offerings. Now that you have made me listen, I finally understand— you don’t require burnt offerings or sin offerings."
      }
    },
