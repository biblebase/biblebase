    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我患难重重，罪恶缠身， 看不到出路。 我的罪过比我的头发还多， 我心惊胆战。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因有無數的禍患圍繞著我； 我的罪孽追上了我，使我不能看見； 它們比我的頭髮還多， 以致我心驚膽戰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因有無數的禍患圍困我， 我的罪孽追上了我，使我不能昂首； 這罪孽比我的頭髮還多， 我就心寒膽戰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For evils beyond number have surrounded me; My iniquities have overtaken me, so that I am not able to see; They are more numerous than the hairs of my head, And my heart has failed me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For troubles without number surround me; my sins have overtaken me, and I cannot see. They are more than the hairs of my head, and my heart fails within me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For troubles surround me— too many to count! My sins pile up so high I can’t see my way out. They outnumber the hairs on my head. I have lost all courage."
      }
    },
