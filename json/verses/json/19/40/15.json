        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願那些對我說：“啊哈！啊哈！”的， 都因羞愧而驚惶。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願那些對我說阿哈、阿哈的， 因羞愧而敗亡！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let those be appalled because of their shame Who say to me, “Aha, aha!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May those who say to me, “Aha! Aha!” be appalled at their own shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let them be horrified by their shame, for they said, “Aha! We’ve got him now!”"
      }
