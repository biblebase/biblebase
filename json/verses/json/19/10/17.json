    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 你知道困苦人的愿望， 你必垂听他们的呼求， 安慰他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！困苦人的心願你已經聽見， 你必堅固他們的心，也必留心聽他們的呼求；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，謙卑人的心願， 你早已知道。 你必預備他們的心， 也必側耳聽他們的祈求，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, You have heard the desire of the humble; You will strengthen their heart, You will incline Your ear"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You, Lord, hear the desire of the afflicted; you encourage them, and you listen to their cry,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, you know the hopes of the helpless. Surely you will hear their cries and comfort them."
      }
    },
