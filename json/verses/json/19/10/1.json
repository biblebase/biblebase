    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你为何远远站着？ 为何在我遭难时隐藏起来？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你為甚麼遠遠地站著？在患難的時候，你為甚麼隱藏起來呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你為甚麼站在遠處？ 在患難的時候為甚麼隱藏？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why do You stand afar off, O Lord? Why do You hide Yourself in times of trouble?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why, Lord, do you stand far off? Why do you hide yourself in times of trouble?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, why do you stand so far away? Why do you hide when I am in trouble?"
      }
    },
