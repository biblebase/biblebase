    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们凡事顺利， 对你的审判不屑一顾， 对所有的仇敌嗤之以鼻。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的道路時常穩妥， 你的判斷高超，他卻不放在眼內； 他對所有的仇敵都嗤之以鼻。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡他所做的，時常穩固； 你的審判超過他的眼界。 至於他一切的敵人， 他都向他們噴氣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His ways prosper at all times; Your judgments are on high, out of his sight; As for all his adversaries, he snorts at them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His ways are always prosperous; your laws are rejected by him; he sneers at all his enemies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet they succeed in everything they do. They do not see your punishment awaiting them. They sneer at all their enemies."
      }
    },
