    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他们都赞美耶和华， 因为唯有祂的名当受尊崇， 祂的荣耀在天地之上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願這一切都讚美耶和華的名， 因為獨有他的名被尊崇， 他的榮耀超越天地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願這些都讚美耶和華的名！ 因為獨有他的名被尊崇； 他的榮耀在天地之上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let them praise the name of the Lord, For His name alone is exalted; His glory is above earth and heaven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let them praise the name of the Lord, for his name alone is exalted; his splendor is above the earth and the heavens."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let them all praise the name of the Lord. For his name is very great; his glory towers over the earth and heaven!"
      }
    },
