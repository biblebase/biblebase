    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要让人知道： 你的慈爱存到永远， 你的信实与天同存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要說：“你的慈愛必永遠建立； 你的信實，你必堅立在天上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因我曾說：你的慈悲必建立到永遠； 你的信實必堅立在天上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I have said, “Lovingkindness will be built up forever; In the heavens You will establish Your faithfulness.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will declare that your love stands firm forever, that you have established your faithfulness in heaven itself."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your unfailing love will last forever. Your faithfulness is as enduring as the heavens."
      }
    },
