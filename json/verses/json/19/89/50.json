    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主啊，求你顾念你仆人——我所受的羞辱， 我心中如何忍受列国的嘲笑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主啊！求你記念你僕人所受的羞辱， 記念我心裡承擔著萬民的羞辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主啊，求你記念僕人們所受的羞辱， 記念我怎樣將一切強盛民的羞辱存在我懷裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Remember, O Lord, the reproach of Your servants; How I bear in my bosom the reproach of all the many peoples,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember, Lord, how your servant has been mocked, how I bear in my heart the taunts of all the nations,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Consider, Lord, how your servants are disgraced! I carry in my heart the insults of so many people."
      }
