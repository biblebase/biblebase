    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要等候耶和华， 坚守祂的道， 祂必赐你土地，使你有尊荣。 你必亲眼看见恶人被铲除。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要等候耶和華，謹守他的道； 他必高舉你，使你承受地土； 惡人被剪除的時候，你必看見。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你當等候耶和華，遵守他的道， 他就擡舉你，使你承受地土； 惡人被剪除的時候，你必看見。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Wait for the Lord and keep His way, And He will exalt you to inherit the land; When the wicked are cut off, you will see it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hope in the Lord and keep his way. He will exalt you to inherit the land; when the wicked are destroyed, you will see it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Put your hope in the Lord. Travel steadily along his path. He will honor you by giving you the land. You will see the wicked destroyed."
      }
    },
