    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要在耶和华面前安静， 耐心等候祂， 不要因恶人道路通达、阴谋得逞而愤愤不平。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要在耶和華面前靜默無聲， 耐心地等候他； 不要因那凡事順利的， 和那惡謀得逞的，心懷不平。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你當默然倚靠耶和華，耐性等候他； 不要因那道路通達的和那惡謀成就的心懷不平。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Rest in the Lord and wait patiently for Him; Do not fret because of him who prospers in his way, Because of the man who carries out wicked schemes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be still before the Lord and wait patiently for him; do not fret when people succeed in their ways, when they carry out their wicked schemes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Be still in the presence of the Lord, and wait patiently for him to act. Don’t worry about evil people who prosper or fret about their wicked schemes."
      }
    },
