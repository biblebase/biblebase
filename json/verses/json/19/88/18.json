    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使我的亲朋好友离我而去， 只有黑暗与我相伴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使我摯愛的和同伴都遠離我， 黑暗成了我的知己。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你把我的良朋密友隔在遠處， 使我所認識的人進入黑暗裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have removed lover and friend far from me; My acquaintances are in darkness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have taken from me friend and neighbor— darkness is my closest friend."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have taken away my companions and loved ones. Darkness is my closest friend."
      }
    },
