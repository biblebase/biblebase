    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的眼睛因哭泣而视力模糊。 耶和华啊，我天天呼求你， 举手向你呼求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的眼睛因困苦而昏花； 耶和華啊，我天天向你呼求， 向你舉手禱告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的眼睛因困苦而乾癟。 耶和華啊，我天天求告你，向你舉手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My eye has wasted away because of affliction; I have called upon You every day, O Lord; I have spread out my hands to You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "my eyes are dim with grief. I call to you, Lord, every day; I spread out my hands to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My eyes are blinded by my tears. Each day I beg for your help, O Lord; I lift my hands to you for mercy."
      }
    },
