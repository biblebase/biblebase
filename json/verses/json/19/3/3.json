    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是，耶和华啊！ 你是四面保护我的盾牌， 你赐我荣耀，使我昂首而立。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你卻是我周圍的盾牌， 是我的榮耀，是使我抬起頭來的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你－耶和華是我四圍的盾牌， 是我的榮耀，又是叫我擡起頭來的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But You, O Lord, are a shield about me, My glory, and the One who lifts my head."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you, Lord, are a shield around me, my glory, the One who lifts my head high."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you, O Lord, are a shield around me; you are my glory, the one who holds my head high."
      }
