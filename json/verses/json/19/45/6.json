    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，你的宝座永远长存， 你以公义的杖执掌王权。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！你的寶座是永永遠遠的， 你國的權杖是公平的權杖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你的寶座是永永遠遠的； 你的國權是正直的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your throne, O God, is forever and ever; A scepter of uprightness is the scepter of Your kingdom."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your throne, O God, will last for ever and ever; a scepter of justice will be the scepter of your kingdom."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your throne, O God, endures forever and ever. You rule with a scepter of justice."
      }
