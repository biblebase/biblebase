    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的衣袍散发出没药、沉香和肉桂的芬芳， 你陶醉在象牙宫的弦乐中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的衣服都有沒藥、沉香和肉桂的香氣； 從象牙宮裡有絲弦的樂聲，使你歡喜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的衣服都有沒藥、沉香、肉桂的香氣； 象牙宮中有絲弦樂器的聲音使你歡喜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All Your garments are fragrant with myrrh and aloes and cassia; Out of ivory palaces stringed instruments have made You glad."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All your robes are fragrant with myrrh and aloes and cassia; from palaces adorned with ivory the music of the strings makes you glad."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Myrrh, aloes, and cassia perfume your robes. In ivory palaces the music of strings entertains you."
      }
    },
