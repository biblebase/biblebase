    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王喜欢你的美貌， 你要敬重他，因他是你的主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王就愛慕你的美麗； 因為他是你的主， 你要向他俯伏。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王就羨慕你的美貌； 因為他是你的主，你當敬拜他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the King will desire your beauty. Because He is your Lord, bow down to Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let the king be enthralled by your beauty; honor him, for he is your lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For your royal husband delights in your beauty; honor him, for he is your lord."
      }
    },
