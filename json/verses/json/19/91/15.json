    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他求告我，我就答应他； 他遭遇患难，我必与他同在。 我必拯救他，赐他尊贵的地位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他求告我，我必應允他； 他在患難中，我必與他同在； 我必拯救他，使他得尊榮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他若求告我，我就應允他； 他在急難中，我要與他同在； 我要搭救他，使他尊貴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will call upon Me, and I will answer him; I will be with him in trouble; I will rescue him and honor him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will call on me, and I will answer him; I will be with him in trouble, I will deliver him and honor him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they call on me, I will answer; I will be with them in trouble. I will rescue and honor them."
      }
