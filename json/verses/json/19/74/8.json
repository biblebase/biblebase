    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们心里说：“我们要彻底毁灭一切。” 于是他们烧毁了境内所有敬拜上帝的地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們心裡說：“我們要把它們完全毀滅！” 他們燒毀了地上一切敬拜　神的聚會地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們心裏說：我們要盡行毀滅； 他們就在遍地把神的會所都燒毀了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said in their heart, “Let us completely subdue them.” They have burned all the meeting places of God in the land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They said in their hearts, “We will crush them completely!” They burned every place where God was worshiped in the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they thought, “Let’s destroy everything!” So they burned down all the places where God was worshiped."
      }
    },
