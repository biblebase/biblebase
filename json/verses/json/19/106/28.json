    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们在毗珥祭拜巴力， 吃献给假神的祭物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們又歸附巴力．毘珥， 吃了祭過死神的祭物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們又與巴力‧毗珥連合， 且吃了祭死神的物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They joined themselves also to Baal-peor, And ate sacrifices offered to the dead."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They yoked themselves to the Baal of Peor and ate sacrifices offered to lifeless gods;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then our ancestors joined in the worship of Baal at Peor; they even ate sacrifices offered to the dead!"
      }
