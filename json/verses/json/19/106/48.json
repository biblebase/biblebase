      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Blessed be the Lord, the God of Israel, From everlasting even to everlasting. And let all the people say, “Amen.” Praise the Lord!"
      },
