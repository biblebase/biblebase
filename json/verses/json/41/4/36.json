    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们就离开众人，上了耶稣乘坐的船，一起渡到湖的对岸，还有其他船只跟着去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "門徒離開群眾，耶穌已經在船上，他們就載他過去，也有別的船和他同去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門徒離開眾人，耶穌仍在船上，他們就把他一同帶去；也有別的船和他同行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Leaving the crowd, they *took Him along with them in the boat, just as He was; and other boats were with Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Leaving the crowd behind, they took him along, just as he was, in the boat. There were also other boats with him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So they took Jesus in the boat and started out, leaving the crowds behind (although other boats followed)."
      }
    },
