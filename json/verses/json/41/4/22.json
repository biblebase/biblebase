    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "隐藏的事是不能掩盖的，终会显露出来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為沒有甚麼隱藏的事不被顯明出來，沒有甚麼掩蓋的事不被揭露的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為掩藏的事，沒有不顯出來的；隱瞞的事，沒有不露出來的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For nothing is hidden, except to be revealed; nor has anything been secret, but that it would come to light."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For whatever is hidden is meant to be disclosed, and whatever is concealed is meant to be brought out into the open."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For everything that is hidden will eventually be brought into the open, and every secret will be brought to light."
      }
    },
