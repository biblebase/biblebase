    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "鬼常常将他扔进火里或水里，要害他的命。如果你能，求你怜悯我们，帮助我们吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "鬼常常把他扔在火裡水中，要毀滅他。如果你能作甚麼，求你可憐我們，幫助我們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "鬼屢次把他扔在火裏、水裏，要滅他。你若能做甚麼，求你憐憫我們，幫助我們。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It has often thrown him both into the fire and into the water to destroy him. But if You can do anything, take pity on us and help us!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“It has often thrown him into fire or water to kill him. But if you can do anything, take pity on us and help us.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The spirit often throws him into the fire or into water, trying to kill him. Have mercy on us and help us, if you can.”"
      }
    },
