    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其实我告诉你们，以利亚已经来了，人们却任意对待他，正如圣经的记载。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "其實我告訴你們，以利亞已經來了，他們卻任意待他，正如經上指著他所說的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我告訴你們，以利亞已經來了，他們也任意待他，正如經上所指着他的話。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I say to you that Elijah has indeed come, and they did to him whatever they wished, just as it is written of him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I tell you, Elijah has come, and they have done to him everything they wished, just as it is written about him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I tell you, Elijah has already come, and they chose to abuse him, just as the Scriptures predicted.”"
      }
