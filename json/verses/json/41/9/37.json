    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“任何人为我的缘故接待这样一个小孩子，就是接待我；凡接待我的，不单是接待我，也是接待差我来的那位。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“凡因我的名接待一個這樣的小孩子的，就是接待我；凡接待我的，不是接待我，而是接待那差我來的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「凡為我名接待一個像這小孩子的，就是接待我；凡接待我的，不是接待我，乃是接待那差我來的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Whoever receives one child like this in My name receives Me; and whoever receives Me does not receive Me, but Him who sent Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Whoever welcomes one of these little children in my name welcomes me; and whoever welcomes me does not welcome me but the one who sent me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Anyone who welcomes a little child like this on my behalf welcomes me, and anyone who welcomes me welcomes not only me but also my Father who sent me.”"
      }
    },
