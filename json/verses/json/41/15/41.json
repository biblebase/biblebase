    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣在加利利时，她们已经跟随祂、服侍祂了。此外还有好些跟着祂上耶路撒冷的妇女。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些婦女，當耶穌在加利利的時候，就一直跟隨他、服事他。此外，還有許多和他一同上耶路撒冷的婦女。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是耶穌在加利利的時候，跟隨他、服事他的那些人，還有同耶穌上耶路撒冷的好些婦女在那裏觀看。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He was in Galilee, they used to follow Him and minister to Him; and there were many other women who came up with Him to Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In Galilee these women had followed him and cared for his needs. Many other women who had come up with him to Jerusalem were also there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They had been followers of Jesus and had cared for him while he was in Galilee. Many other women who had come with him to Jerusalem were also there."
      }
    },
