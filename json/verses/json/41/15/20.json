    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "戏弄完了，就脱去祂的紫袍，给祂穿上原来的衣服，押祂出去钉十字架。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們戲弄完了，就把他的紫色的外袍脫下，給他穿回自己的衣服，帶他出去，要釘十字架。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "戲弄完了，就給他脫了紫袍，仍穿上他自己的衣服，帶他出去，要釘十字架。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After they had mocked Him, they took the purple robe off Him and put His own garments on Him. And they *led Him out to crucify Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And when they had mocked him, they took off the purple robe and put his own clothes on him. Then they led him out to crucify him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they were finally tired of mocking him, they took off the purple robe and put his own clothes on him again. Then they led him away to be crucified."
      }
    },
