    "sermons": [
      {
        "id": "home_sermons_sermon-2019-07-28",
        "title": "歡迎對號入座",
        "date": "2019-07-28",
        "preacher": "蕭旻奇牧師",
        "verses": "馬可福音 10:32-45",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2019/Sermon-2019-07-28.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2019/Sermon-2019-07-28.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在前往耶路撒冷的路上，耶稣走在众人前面，门徒感到惊奇，其他跟随的人也很害怕。 耶稣把十二个门徒叫到一旁，再把自己将要遭遇的事告诉他们，说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在上耶路撒冷的路途中，耶穌走在前面，門徒希奇，跟從的人也害怕。耶穌又把十二門徒帶到一邊，把自己將要遭遇的事告訴他們，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們行路上耶路撒冷去。耶穌在前頭走，門徒就希奇，跟從的人也害怕。耶穌又叫過十二個門徒來，把自己將要遭遇的事告訴他們說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They were on the road going up to Jerusalem, and Jesus was walking on ahead of them; and they were amazed, and those who followed were fearful. And again He took the twelve aside and began to tell them what was going to happen to Him,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They were on their way up to Jerusalem, with Jesus leading the way, and the disciples were astonished, while those who followed were afraid. Again he took the Twelve aside and told them what was going to happen to him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They were now on the way up to Jerusalem, and Jesus was walking ahead of them. The disciples were filled with awe, and the people following behind were overwhelmed with fear. Taking the twelve disciples aside, Jesus once more began to describe everything that was about to happen to him."
      }
    },
