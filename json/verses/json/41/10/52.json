    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“回去吧！你的信心救了你。”那人立刻得见光明，在路上跟从了耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說：“去吧，你的信使你痊愈了。”他立刻看見了，就在路上跟隨耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「你去吧！你的信救了你了。」瞎子立刻看見了，就在路上跟隨耶穌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus said to him, “Go; your faith has made you well.” Immediately he regained his sight and began following Him on the road."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Go,” said Jesus, “your faith has healed you.” Immediately he received his sight and followed Jesus along the road."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And Jesus said to him, “Go, for your faith has healed you.” Instantly the man could see, and he followed Jesus down the road."
      }
    },
