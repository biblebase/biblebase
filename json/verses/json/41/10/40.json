    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不过谁坐在我的左右不是我来定，而是为谁预备的，就让谁坐。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只是坐在我的左右，不是我可以賜的，而是　神預備賜給誰，就賜給誰。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是坐在我的左右，不是我可以賜的，乃是為誰預備的，就賜給誰。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But to sit on My right or on My left, this is not Mine to give; but it is for those for whom it has been prepared.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but to sit at my right or left is not for me to grant. These places belong to those for whom they have been prepared.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I have no right to say who will sit on my right or my left. God has prepared those places for the ones he has chosen.”"
      }
    },
