    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣听见了，就说：“健康的人不需要医生，有病的人才需要。我来不是要召义人，乃是要召罪人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌聽見了，就對他們說：“健康的人不需要醫生，有病的人才需要；我來不是要召義人，而是要召罪人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌聽見，就對他們說：「康健的人用不着醫生，有病的人才用得着。我來本不是召義人，乃是召罪人。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And hearing this, Jesus *said to them, “It is not those who are healthy who need a physician, but those who are sick; I did not come to call the righteous, but sinners.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On hearing this, Jesus said to them, “It is not the healthy who need a doctor, but the sick. I have not come to call the righteous, but sinners.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jesus heard this, he told them, “Healthy people don’t need a doctor—sick people do. I have come to call not those who think they are righteous, but those who know they are sinners.”"
      }
    },
