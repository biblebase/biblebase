    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂对那棵树说：“愿无人再吃你的果子！”祂的门徒都听见了这句话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對樹說：“永遠再沒有人吃你的果子了！”他的門徒也聽見了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌就對樹說：「從今以後，永沒有人吃你的果子。」他的門徒也聽見了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to it, “May no one ever eat fruit from you again!” And His disciples were listening."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he said to the tree, “May no one ever eat fruit from you again.” And his disciples heard him say it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus said to the tree, “May no one ever eat your fruit again!” And the disciples heard him say it."
      }
    },
