    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣在周日清晨复活后，首先向抹大拉的玛丽亚显现，耶稣曾在她身上赶出七个鬼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "禮拜日的清早，耶穌復活了，先向抹大拉的馬利亞顯現，耶穌曾經從她身上趕出七個鬼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在七日的第一日清早，耶穌復活了，就先向抹大拉的馬利亞顯現（耶穌從她身上曾趕出七個鬼）。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "[Now after He had risen early on the first day of the week, He first appeared to Mary Magdalene, from whom He had cast out seven demons."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jesus rose early on the first day of the week, he appeared first to Mary Magdalene, out of whom he had driven seven demons."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After Jesus rose from the dead early on Sunday morning, the first person who saw him was Mary Magdalene, the woman from whom he had cast out seven demons."
      }
    },
