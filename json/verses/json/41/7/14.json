    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣又召集众人，教导他们说：“我的话，你们要听明白，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是耶穌又把群眾叫過來，對他們說：“你們大家都要聽我說，也要明白："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌又叫眾人來，對他們說：「你們都要聽我的話，也要明白。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After He called the crowd to Him again, He began saying to them, “Listen to Me, all of you, and understand:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Again Jesus called the crowd to him and said, “Listen to me, everyone, and understand this."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus called to the crowd to come and hear. “All of you listen,” he said, “and try to understand."
      }
