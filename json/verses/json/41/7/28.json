    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "妇人说：“主啊，你说的对，但桌子下的狗也吃孩子们掉下来的碎渣呀！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那女人回答他：“主啊，是的，不過小狗在桌子底下，也可以吃孩子們掉下來的碎渣。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "婦人回答說：「主啊，不錯；但是狗在桌子底下也吃孩子們的碎渣兒。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But she answered and *said to Him, “Yes, Lord, but even the dogs under the table feed on the children’s crumbs.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Lord,” she replied, “even the dogs under the table eat the children’s crumbs.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She replied, “That’s true, Lord, but even the dogs under the table are allowed to eat the scraps from the children’s plates.”"
      }
