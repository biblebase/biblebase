    "sermons": [
      {
        "id": "home_sermons_sermon-2019-01-27",
        "title": "心口不一的警告",
        "date": "2019-01-27",
        "preacher": "蕭旻奇牧師",
        "verses": "馬可福音 7:1-23",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2019/Sermon-2019-01-27.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2019/Sermon-2019-01-27.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们却认为人若对父母说，‘我把供养你们的钱财已经全部奉献给上帝了’，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們倒說：‘人對父母說，我應該給你的供奉，已經作了各耳板’，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們倒說：『人若對父母說：我所當奉給你的，已經作了各耳板』（各耳板就是供獻的意思），"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but you say, ‘If a man says to his father or his mother, whatever I have that would help you is Corban (that is to say, given to God),’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you say that if anyone declares that what might have been used to help their father or mother is Corban (that is, devoted to God)—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you say it is all right for people to say to their parents, ‘Sorry, I can’t help you. For I have vowed to give to God what I would have given to you.’"
      }
    },
