    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“你看见这宏伟的建筑了吗？将来它要被完全拆毁，找不到两块叠在一起的石头。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對他說：“你看見這些偉大的建築嗎？將來必沒有一塊石頭留在另一塊石頭上面，每一塊都要拆下來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他說：「你看見這大殿宇嗎？將來在這裏沒有一塊石頭留在石頭上，不被拆毀了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus said to him, “Do you see these great buildings? Not one stone will be left upon another which will not be torn down.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do you see all these great buildings?” replied Jesus. “Not one stone here will be left on another; every one will be thrown down.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “Yes, look at these great buildings. But they will be completely demolished. Not one stone will be left on top of another!”"
      }
    },
