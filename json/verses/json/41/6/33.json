    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是有许多人看见他们离开，认出了他们，便从各城镇步行赶往那里，比他们先到达。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "群眾看見他們走了，有許多人認出了他們，就從各城出來，跑到那裡，比他們先趕到。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人看見他們去，有許多認識他們的，就從各城步行，一同跑到那裏，比他們先趕到了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The people saw them going, and many recognized them and ran there together on foot from all the cities, and got there ahead of them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But many who saw them leaving recognized them and ran on foot from all the towns and got there ahead of them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But many people recognized them and saw them leaving, and people from many towns ran ahead along the shore and got there ahead of them."
      }
    },
