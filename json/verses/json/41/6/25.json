    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她马上回去对王说：“愿王立刻把施洗者约翰的头放在盘子里送给我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她急忙到王面前，向王要求說：“願王立刻把施洗的約翰的頭放在盤子上給我！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她就急忙進去見王，求他說：「我願王立時把施洗約翰的頭放在盤子裏給我。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Immediately she came in a hurry to the king and asked, saying, “I want you to give me at once the head of John the Baptist on a platter.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At once the girl hurried in to the king with the request: “I want you to give me right now the head of John the Baptist on a platter.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the girl hurried back to the king and told him, “I want the head of John the Baptist, right now, on a tray!”"
      }
    },
