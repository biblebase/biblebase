    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约翰的门徒听到这个消息，就来把约翰的尸体领回去，安葬在坟墓里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約翰的門徒聽見了，就來把他的屍體領去，安放在墳墓裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約翰的門徒聽見了，就來把他的屍首領去，葬在墳墓裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When his disciples heard about this, they came and took away his body and laid it in a tomb."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On hearing of this, John’s disciples came and took his body and laid it in a tomb."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When John’s disciples heard what had happened, they came to get his body and buried it in a tomb."
      }
    },
