    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以赛亚先知书上说： “看啊， 我要差遣我的使者在你前面为你预备道路。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正如以賽亞先知的書上寫著： “看哪，我差遣我的使者在你面前， 預備你的道路；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正如先知以賽亞書上記着說： 看哪，我要差遣我的使者在你前面， 預備道路。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As it is written in Isaiah the prophet: “Behold, I send My messenger ahead of You, Who will prepare Your way;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "as it is written in Isaiah the prophet: “I will send my messenger ahead of you, who will prepare your way”—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "just as the prophet Isaiah had written: “Look, I am sending my messenger ahead of you, and he will prepare your way."
      }
    },
