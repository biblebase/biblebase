    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣从水中一上来，就看见天开了，圣灵好像鸽子一样降在祂身上，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他從水裡一上來，就看見天裂開了，聖靈仿佛鴿子降在他身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他從水裏一上來，就看見天裂開了，聖靈彷彿鴿子，降在他身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Immediately coming up out of the water, He saw the heavens opening, and the Spirit like a dove descending upon Him;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Just as Jesus was coming up out of the water, he saw heaven being torn open and the Spirit descending on him like a dove."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As Jesus came up out of the water, he saw the heavens splitting apart and the Holy Spirit descending on him like a dove."
      }
    },
