    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣却回答说：“我们到附近的乡镇去吧，我也好在那里传道，因为我就是为这事来的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對他們說：“我們到鄰近的鄉鎮去吧，我也好在那裡傳道，因為我就是為這事而來的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他們說：「我們可以往別處去，到鄰近的鄉村，我也好在那裏傳道，因為我是為這事出來的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He *said to them, “Let us go somewhere else to the towns nearby, so that I may preach there also; for that is what I came for.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus replied, “Let us go somewhere else—to the nearby villages—so I can preach there also. That is why I have come.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jesus replied, “We must go on to other towns as well, and I will preach to them, too. That is why I came.”"
      }
    },
