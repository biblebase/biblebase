    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司长和全公会的人正在寻找证据控告耶稣，要定祂死罪，只是找不到。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司長和公議會全體都尋找證據控告耶穌，要把他處死，卻沒有找著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司長和全公會尋找見證控告耶穌，要治死他，卻尋不着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the chief priests and the whole Council kept trying to obtain testimony against Jesus to put Him to death, and they were not finding any."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The chief priests and the whole Sanhedrin were looking for evidence against Jesus so that they could put him to death, but they did not find any."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Inside, the leading priests and the entire high council were trying to find evidence against Jesus, so they could put him to death. But they couldn’t find any."
      }
    },
