    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "用餐的时候，耶稣说：“我实在告诉你们，你们中间有一个人要出卖我，他现在正和我同桌吃饭。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們坐著吃飯的時候，耶穌說：“我實在告訴你們，你們中間有一個跟我一起吃飯的人要出賣我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們坐席正吃的時候，耶穌說：「我實在告訴你們，你們中間有一個與我同吃的人要賣我了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As they were reclining at the table and eating, Jesus said, “Truly I say to you that one of you will betray Me—one who is eating with Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While they were reclining at the table eating, he said, “Truly I tell you, one of you will betray me—one who is eating with me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As they were at the table eating, Jesus said, “I tell you the truth, one of you eating with me here will betray me.”"
      }
    },
