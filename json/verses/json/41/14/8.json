    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她做了自己能做的。她是为我的安葬做准备，提前用香膏抹了我的身体。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她已經盡她所能的作了。她預先用香膏膏了我的身體，是為了我的安葬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她所做的，是盡她所能的；她是為我安葬的事把香膏預先澆在我身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She has done what she could; she has anointed My body beforehand for the burial."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She did what she could. She poured perfume on my body beforehand to prepare for my burial."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She has done what she could and has anointed my body for burial ahead of time."
      }
    },
