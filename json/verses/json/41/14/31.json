    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼得郑重地说：“就算要我跟你一起死，我也不会不认你。”其他门徒也都这样说。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼得更堅決地說：“就算必須與你一同死，我也決不會不認你！”眾人也都這樣說。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼得卻極力地說：「我就是必須和你同死，也總不能不認你。」眾門徒都是這樣說。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Peter kept saying insistently, “Even if I have to die with You, I will not deny You!” And they all were saying the same thing also."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Peter insisted emphatically, “Even if I have to die with you, I will never disown you.” And all the others said the same."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“No!” Peter declared emphatically. “Even if I have to die with you, I will never deny you!” And all the others vowed the same."
      }
    },
