    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我实在告诉你们，无论福音传到世界哪个角落，人们都会传扬这女人的事迹，纪念她。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我實在告訴你們，福音無論傳到世界上甚麼地方，都要傳講這女人所作的來記念她。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我實在告訴你們，普天之下，無論在甚麼地方傳這福音，也要述說這女人所做的，以為記念。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Truly I say to you, wherever the gospel is preached in the whole world, what this woman has done will also be spoken of in memory of her.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Truly I tell you, wherever the gospel is preached throughout the world, what she has done will also be told, in memory of her.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I tell you the truth, wherever the Good News is preached throughout the world, this woman’s deed will be remembered and discussed.”"
      }
    },
