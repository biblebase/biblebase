    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣马上知道有能力从自己身上发出，便在人群中转过身来问：“谁摸了我的衣服？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌自己立刻覺得有能力從他裡面出去，就轉過身來對群眾說：“誰摸了我的衣服？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌頓時心裏覺得有能力從自己身上出去，就在眾人中間轉過來，說：「誰摸我的衣裳？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Immediately Jesus, perceiving in Himself that the power proceeding from Him had gone forth, turned around in the crowd and said, “Who touched My garments?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At once Jesus realized that power had gone out from him. He turned around in the crowd and asked, “Who touched my clothes?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus realized at once that healing power had gone out from him, so he turned around in the crowd and asked, “Who touched my robe?”"
      }
    },
