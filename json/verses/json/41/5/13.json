    "sermons": [
      {
        "id": "home_sermons_sermon-2018-08-26",
        "title": "人、污鬼與耶穌之間",
        "date": "2018-08-26",
        "preacher": "蕭旻奇牧師",
        "verses": "馬可福音 5:1-20",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2018/Sermon-2018-08-26.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2018/Sermon-2018-08-26.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣允许了，污鬼就离开那人，进入猪群。那群猪就一路奔下陡坡，冲进湖里淹死了，约有两千头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌准了他們。污靈就出來，進到豬群裡去，於是那群豬闖下山崖，掉在海裡淹死了，豬的數目約有兩千。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌准了他們，污鬼就出來，進入豬裏去。於是那羣豬闖下山崖，投在海裏，淹死了。豬的數目約有二千。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus gave them permission. And coming out, the unclean spirits entered the swine; and the herd rushed down the steep bank into the sea, about two thousand of them; and they were drowned in the sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He gave them permission, and the impure spirits came out and went into the pigs. The herd, about two thousand in number, rushed down the steep bank into the lake and were drowned."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Jesus gave them permission. The evil spirits came out of the man and entered the pigs, and the entire herd of about 2,000 pigs plunged down the steep hillside into the lake and drowned in the water."
      }
    },
