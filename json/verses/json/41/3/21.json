    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂的亲属听见这个消息，就出来要拉住祂，因为人们说祂疯了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些和他在一起的人聽見了，就出來抓住他，因為他們說他癲狂了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌的親屬聽見，就出來要拉住他，因為他們說他癲狂了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When His own people heard of this, they went out to take custody of Him; for they were saying, “He has lost His senses.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When his family heard about this, they went to take charge of him, for they said, “He is out of his mind.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When his family heard what was happening, they tried to take him away. “He’s out of his mind,” they said."
      }
    },
