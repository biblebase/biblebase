    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后，耶稣走到圣殿的奉献箱对面坐下，看大家怎样奉献。很多财主奉献了大量的钱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌面對銀庫坐著，看著大家怎樣把錢投入庫中。許多有錢的人投入很多的錢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對銀庫坐着，看眾人怎樣投錢入庫。有好些財主往裏投了若干的錢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He sat down opposite the treasury, and began observing how the people were putting money into the treasury; and many rich people were putting in large sums."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus sat down opposite the place where the offerings were put and watched the crowd putting their money into the temple treasury. Many rich people threw in large amounts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus sat down near the collection box in the Temple and watched as the crowds dropped in their money. Many rich people put in large amounts."
      }
