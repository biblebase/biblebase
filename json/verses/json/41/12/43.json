    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣叫门徒来，说：“我实在告诉你们，这位穷寡妇比其他人奉献的都多，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌把門徒叫過來，對他們說：“我實在告訴你們，這窮寡婦投入庫裡的，比眾人投的更多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌叫門徒來，說：「我實在告訴你們，這窮寡婦投入庫裏的，比眾人所投的更多。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Calling His disciples to Him, He said to them, “Truly I say to you, this poor widow put in more than all the contributors to the treasury;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Calling his disciples to him, Jesus said, “Truly I tell you, this poor widow has put more into the treasury than all the others."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus called his disciples to him and said, “I tell you the truth, this poor widow has given more than all the others who are making contributions."
      }
    },
