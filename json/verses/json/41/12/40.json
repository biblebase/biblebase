    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们侵吞寡妇的财产，还假意做冗长的祷告。这种人必受到更严厉的惩罚。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們吞沒了寡婦的房產，又假裝作冗長的禱告。這些人必受更重的刑罰。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們侵吞寡婦的家產，假意作很長的禱告。這些人要受更重的刑罰！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "who devour widows’ houses, and for appearance’s sake offer long prayers; these will receive greater condemnation.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They devour widows’ houses and for a show make lengthy prayers. These men will be punished most severely.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet they shamelessly cheat widows out of their property and then pretend to be pious by making long prayers in public. Because of this, they will be more severely punished.”"
      }
    },
