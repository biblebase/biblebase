    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他该知道：使一个迷途中的罪人归回正路就是救一个灵魂脱离死亡，并且会遮盖许多罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們應該知道，那使罪人從歧途上轉回的，就會拯救他的靈魂脫離死亡，也會遮蓋許多罪惡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這人該知道：叫一個罪人從迷路上轉回便是救一個靈魂不死，並且遮蓋許多的罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "let him know that he who turns a sinner from the error of his way will save his soul from death and will cover a multitude of sins."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "remember this: Whoever turns a sinner from the error of their way will save them from death and cover over a multitude of sins."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "you can be sure that whoever brings the sinner back from wandering will save that person from death and bring about the forgiveness of many sins."
      }
    },
