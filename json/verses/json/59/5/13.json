    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们中间正在受苦的人应该祷告，喜乐的人应该唱歌赞美，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們中間有人受苦嗎？他就應該禱告。有人心情愉快嗎？他就應該歌頌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們中間有受苦的呢，他就該禱告；有喜樂的呢，他就該歌頌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Is anyone among you suffering? Then he must pray. Is anyone cheerful? He is to sing praises."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Is anyone among you in trouble? Let them pray. Is anyone happy? Let them sing songs of praise."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Are any of you suffering hardships? You should pray. Are any of you happy? You should sing praises."
      }
