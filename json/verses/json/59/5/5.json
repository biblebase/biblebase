    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们在世上奢侈享乐，在被宰杀的日子里，把自己养得肥肥胖胖。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們在世上窮奢極侈，養肥了自己，竟不知屠宰的日子到了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們在世上享美福，好宴樂，當宰殺的日子竟嬌養你們的心。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have lived luxuriously on the earth and led a life of wanton pleasure; you have fattened your hearts in a day of slaughter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have lived on earth in luxury and self-indulgence. You have fattened yourselves in the day of slaughter."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have spent your years on earth in luxury, satisfying your every desire. You have fattened yourselves for the day of slaughter."
      }
    },
