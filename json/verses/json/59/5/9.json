    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，不要互相埋怨，免得受审判。你们看，审判的主已经站在门外了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，不要彼此抱怨，免得你們受審判。看哪，審判的主已經站在門前了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，你們不要彼此埋怨，免得受審判。看哪，審判的主站在門前了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not complain, brethren, against one another, so that you yourselves may not be judged; behold, the Judge is standing right at the door."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Don’t grumble against one another, brothers and sisters, or you will be judged. The Judge is standing at the door!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t grumble about each other, brothers and sisters, or you will be judged. For look—the Judge is standing at the door!"
      }
    },
