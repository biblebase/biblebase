    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人只要触犯律法中的一条，就等于违犯了全部，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為凡是遵守全部律法的，只要在一條上失足，就違犯所有的了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為凡遵守全律法的，只在一條上跌倒，他就是犯了眾條。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For whoever keeps the whole law and yet stumbles in one point, he has become guilty of all."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For whoever keeps the whole law and yet stumbles at just one point is guilty of breaking all of it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the person who keeps all of the laws except one is as guilty as a person who has broken all of God’s laws."
      }
    },
