    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们反倒侮辱穷人。其实欺压你们、抓你们上法庭的不是那些富人吗？"
      },
      "cnv": {
        "version_id": "CNV",
