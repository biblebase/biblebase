    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我亲爱的弟兄姊妹，请听我说，上帝不是拣选了穷人，使他们在信心上富足，并承受祂应许赐给那些爱祂之人的国度吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我親愛的弟兄們，請聽：　神不是揀選了在世上被認為貧窮的人嗎？這些人卻在信心上富足，而且是承受　神的國的人。這國是　神應許賜給愛他的人的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我親愛的弟兄們，請聽，神豈不是揀選了世上的貧窮人，叫他們在信上富足，並承受他所應許給那些愛他之人的國嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Listen, my beloved brethren: did not God choose the poor of this world to be rich in faith and heirs of the kingdom which He promised to those who love Him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Listen, my dear brothers and sisters: Has not God chosen those who are poor in the eyes of the world to be rich in faith and to inherit the kingdom he promised those who love him?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen to me, dear brothers and sisters. Hasn’t God chosen the poor in this world to be rich in faith? Aren’t they the ones who will inherit the Kingdom he promised to those who love him?"
      }
    },
