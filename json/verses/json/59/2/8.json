    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你们能切实行出圣经中“要爱邻如己”这条至尊的律法，就好了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們若照著聖經所說“要愛人如己”這話，去完成這至尊的律法，你們就作對了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "經上記着說：「要愛人如己。」你們若全守這至尊的律法才是好的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If, however, you are fulfilling the royal law according to the Scripture, “You shall love your neighbor as yourself,” you are doing well."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you really keep the royal law found in Scripture, “Love your neighbor as yourself,” you are doing right."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes indeed, it is good when you obey the royal law as found in the Scriptures: “Love your neighbor as yourself.”"
      }
    },
