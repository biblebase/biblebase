    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "既然你们要按使人自由的律法受审判，就应该按律法行事为人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們既然按著使人自由的律法受審判，就應照著這律法說話行事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們既然要按使人自由的律法受審判，就該照這律法說話行事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So speak and so act as those who are to be judged by the law of liberty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Speak and act as those who are going to be judged by the law that gives freedom,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So whatever you say or whatever you do, remember that you will be judged by the law that sets you free."
      }
    },
