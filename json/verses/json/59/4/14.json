    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其实你们根本不知道明天会发生什么事。你们的生命是什么呢？你们只不过像一阵雾气，出现片刻，就无影无踪了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "其實明天怎樣，你們並不知道。你們的生命是甚麼呢？你們本來是過眼雲煙，轉瞬之間就消逝了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其實明天如何，你們還不知道。你們的生命是甚麼呢？你們原來是一片雲霧，出現少時就不見了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet you do not know what your life will be like tomorrow. You are just a vapor that appears for a little while and then vanishes away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why, you do not even know what will happen tomorrow. What is your life? You are a mist that appears for a little while and then vanishes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How do you know what your life will be like tomorrow? Your life is like the morning fog—it’s here a little while, then it’s gone."
      }
    },
