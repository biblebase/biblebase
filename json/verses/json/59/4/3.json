    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们求也得不到是因为你们动机不纯，只求享乐和满足自己的欲望。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們求也得不到，因為你們的動機不良，要把所得的耗費在你們的私慾上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們求也得不着，是因為你們妄求，要浪費在你們的宴樂中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You ask and do not receive, because you ask with wrong motives, so that you may spend it on your pleasures."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you ask, you do not receive, because you ask with wrong motives, that you may spend what you get on your pleasures."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And even when you ask, you don’t get it because your motives are all wrong—you want only what will give you pleasure."
      }
    },
