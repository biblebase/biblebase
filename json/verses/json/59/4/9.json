    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要悲伤、忧愁、痛哭，转欢笑为悲哀，变欢乐为愁苦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要愁苦、悲哀、哭泣，把歡笑變為傷痛，把快樂變為憂愁。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要愁苦、悲哀、哭泣，將喜笑變作悲哀，歡樂變作愁悶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be miserable and mourn and weep; let your laughter be turned into mourning and your joy to gloom."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Grieve, mourn and wail. Change your laughter to mourning and your joy to gloom."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let there be tears for what you have done. Let there be sorrow and deep grief. Let there be sadness instead of laughter, and gloom instead of joy."
      }
    },
