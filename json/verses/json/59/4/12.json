    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "设立律法和审判人的只有一位，就是能拯救人也能毁灭人的上帝。你是谁，竟然论断别人呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "立法的，審判的，只有一位，就是那能拯救人，也能毀滅人的　神；你這判斷鄰舍的，你是誰呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "設立律法和判斷人的，只有一位，就是那能救人也能滅人的。你是誰，竟敢論斷別人呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is only one Lawgiver and Judge, the One who is able to save and to destroy; but who are you who judge your neighbor?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There is only one Lawgiver and Judge, the one who is able to save and destroy. But you—who are you to judge your neighbor?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God alone, who gave the law, is the Judge. He alone has the power to save or to destroy. So what right do you have to judge your neighbor?"
      }
    },
