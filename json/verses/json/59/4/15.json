    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们只当说：“如果主许可，我们就可以活着，可以做这事或做那事。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們倒不如說：“主若願意，我們就可以活著，作這事或作那事。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們只當說：「主若願意，我們就可以活着，也可以做這事，或做那事。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Instead, you ought to say, “If the Lord wills, we will live and also do this or that.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Instead, you ought to say, “If it is the Lord’s will, we will live and do this or that.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What you ought to say is, “If the Lord wants us to, we will live and do this or that.”"
      }
