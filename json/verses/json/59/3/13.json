    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们当中谁是有智慧、有见识的呢？请他带着来自智慧的谦和，在善行上表现出来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們中間誰是有智慧、有見識的呢？他就應當有美好的生活，用明智的溫柔，把自己的行為表現出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們中間誰是有智慧有見識的呢？他就當在智慧的溫柔上顯出他的善行來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who among you is wise and understanding? Let him show by his good behavior his deeds in the gentleness of wisdom."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who is wise and understanding among you? Let them show it by their good life, by deeds done in the humility that comes from wisdom."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you are wise and understand God’s ways, prove it by living an honorable life, doing good works with the humility that comes from wisdom."
      }
    },
