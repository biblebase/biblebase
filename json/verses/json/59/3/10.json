    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "赞美和咒诅竟然从同一张嘴里发出来。我的弟兄姊妹，这是不应该的！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "同一張嘴竟然又稱頌主，又咒詛人；我的弟兄們，這是不應該的！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "頌讚和咒詛從一個口裏出來！我的弟兄們，這是不應當的！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "from the same mouth come both blessing and cursing. My brethren, these things ought not to be this way."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Out of the same mouth come praise and cursing. My brothers and sisters, this should not be."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And so blessing and cursing come pouring out of the same mouth. Surely, my brothers and sisters, this is not right!"
      }
    },
