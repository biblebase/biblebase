    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的弟兄姊妹，不要个个当教师，因为你们知道，我们为人师表的将受到更严格的评判，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的弟兄們，你們不應該有太多人作教師，因為知道我們作教師的將受更嚴厲的審判。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的弟兄們，不要多人作師傅，因為曉得我們要受更重的判斷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let not many of you become teachers, my brethren, knowing that as such we will incur a stricter judgment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Not many of you should become teachers, my fellow believers, because you know that we who teach will be judged more strictly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Dear brothers and sisters, not many of you should become teachers in the church, for we who teach will be judged more strictly."
      }
