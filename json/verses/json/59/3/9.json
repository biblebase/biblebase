    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们用舌头来赞美主和天父，也用舌头来咒诅照着上帝的形象被造的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們用它來稱頌我們的主和天父，又用它來咒詛照　神的形象被造的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們用舌頭頌讚那為主、為父的，又用舌頭咒詛那照着神形像被造的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "With it we bless our Lord and Father, and with it we curse men, who have been made in the likeness of God;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "With the tongue we praise our Lord and Father, and with it we curse human beings, who have been made in God’s likeness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Sometimes it praises our Lord and Father, and sometimes it curses those who have been made in the image of God."
      }
    },
