    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们如果把嚼环放在马嘴里，便可以使它顺服，驾驭它。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們若把嚼環扣入馬嘴，使牠們馴服，就能駕馭牠們的全身。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們若把嚼環放在馬嘴裏，叫牠順服，就能調動牠的全身。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now if we put the bits into the horses’ mouths so that they will obey us, we direct their entire body as well."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When we put bits into the mouths of horses to make them obey us, we can turn the whole animal."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We can make a large horse go wherever we want by means of a small bit in its mouth."
      }
    },
