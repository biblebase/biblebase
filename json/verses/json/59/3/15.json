    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这种智慧并非来自天上，而是从世俗、私欲和魔鬼来的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這種智慧不是從天上來的，而是屬地的、屬血氣的和屬鬼魔的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣的智慧不是從上頭來的，乃是屬地的，屬情慾的，屬鬼魔的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This wisdom is not that which comes down from above, but is earthly, natural, demonic."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Such “wisdom” does not come down from heaven but is earthly, unspiritual, demonic."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For jealousy and selfishness are not God’s kind of wisdom. Such things are earthly, unspiritual, and demonic."
      }
    },
