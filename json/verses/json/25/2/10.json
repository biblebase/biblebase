    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "锡安城的长老腰束麻布， 头蒙灰尘，坐在地上， 默然不语； 耶路撒冷的少女垂头至地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "錫安居民的長老都坐在地上，默默無聲； 他們把塵灰撒在頭上，腰束麻布。 耶路撒冷的處女都垂頭至地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "錫安城的長老坐在地上默默無聲； 他們揚起塵土落在頭上，腰束麻布； 耶路撒冷的處女垂頭至地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The elders of the daughter of Zion Sit on the ground, they are silent. They have thrown dust on their heads; They have girded themselves with sackcloth. The virgins of Jerusalem Have bowed their heads to the ground."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The elders of Daughter Zion sit on the ground in silence; they have sprinkled dust on their heads and put on sackcloth. The young women of Jerusalem have bowed their heads to the ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The leaders of beautiful Jerusalem sit on the ground in silence. They are clothed in burlap and throw dust on their heads. The young women of Jerusalem hang their heads in shame."
      }
    },
