    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我哭得眼睛失明， 心如刀割，肝胆欲碎， 因为人民惨遭毁灭， 儿童和婴孩昏倒在街头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的眼睛因流淚而失明，我的心腸激動， 我的肝膽傾倒在地，都因我的子民遭毀滅， 孩童和嬰兒在城裡的街上昏倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我眼中流淚，以致失明； 我的心腸擾亂，肝膽塗地， 都因我眾民遭毀滅， 又因孩童和吃奶的在城內街上發昏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My eyes fail because of tears, My spirit is greatly troubled; My heart is poured out on the earth Because of the destruction of the daughter of my people, When little ones and infants faint In the streets of the city."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My eyes fail from weeping, I am in torment within; my heart is poured out on the ground because my people are destroyed, because children and infants faint in the streets of the city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have cried until the tears no longer come; my heart is broken. My spirit is poured out in agony as I see the desperate plight of my people. Little children and tiny babies are fainting and dying in the streets."
      }
    },
