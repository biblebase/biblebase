    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你使我们回到你身边， 让我们归向你， 恢复我们昔日的繁荣！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你使我們向你回轉， 我們便得回轉。 求你復新我們的日子，像古時一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Restore us to You, O Lord, that we may be restored; Renew our days as of old,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Restore us to yourself, Lord, that we may return; renew our days as of old"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Restore us, O Lord, and bring us back to you again! Give us back the joys we once had!"
      }
    },
