    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在困苦流离中， 耶路撒冷想起昔日的荣华。 如今，她的人民落在敌人手中， 无人援救。 仇敌看见她，都嘲笑她的灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶路撒冷在困苦飄流的日子，就追念她昔日的一切珍寶。 她的人民落在敵人手裡的時候，沒有人幫助她。 敵人看見她，就譏笑她的滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷在困苦窘迫之時， 就追想古時一切的樂境。 她百姓落在敵人手中，無人救濟； 敵人看見，就因她的荒涼嗤笑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In the days of her affliction and homelessness Jerusalem remembers all her precious things That were from the days of old, When her people fell into the hand of the adversary And no one helped her. The adversaries saw her, They mocked at her ruin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the days of her affliction and wandering Jerusalem remembers all the treasures that were hers in days of old. When her people fell into enemy hands, there was no one to help her. Her enemies looked at her and laughed at her destruction."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In the midst of her sadness and wandering, Jerusalem remembers her ancient splendor. But now she has fallen to her enemy, and there is no one to help her. Her enemy struck her down and laughed as she fell."
      }
    },
