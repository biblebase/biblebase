    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我为此哭泣， 泪流满面， 却无人安慰、鼓励我。 我的儿女凄凉孤苦， 因为仇敌已经得胜。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為了這些事我不住哭泣，我的眼不住流下淚水； 因為那安慰我、使我重新振作的，都遠離了我。 我的兒女孤單淒涼，因為仇敵得勝了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因這些事哭泣； 我眼淚汪汪； 因為那當安慰我、救我性命的， 離我甚遠。 我的兒女孤苦， 因為仇敵得了勝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For these things I weep; My eyes run down with water; Because far from me is a comforter, One who restores my soul. My children are desolate Because the enemy has prevailed.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is why I weep and my eyes overflow with tears. No one is near to comfort me, no one to restore my spirit. My children are destitute because the enemy has prevailed.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“For all these things I weep; tears flow down my cheeks. No one is here to comfort me; any who might encourage me are far away. My children have no future, for the enemy has conquered us.”"
      }
