    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“祂从高天降下火来， 焚烧我的骨头； 祂在我脚前设下网罗， 使我返回； 祂使我凄凉孤寂，终日痛苦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他從高天降火，使火進入我的骨頭裡。 他設置網羅絆我的腳，使我退去。 他使我孤單淒涼，終日愁煩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他從高天使火進入我的骨頭， 剋制了我； 他鋪下網羅，絆我的腳， 使我轉回； 他使我終日淒涼發昏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From on high He sent fire into my bones, And it prevailed over them. He has spread a net for my feet; He has turned me back; He has made me desolate, Faint all day long."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“From on high he sent fire, sent it down into my bones. He spread a net for my feet and turned me back. He made me desolate, faint all the day long."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“He has sent fire from heaven that burns in my bones. He has placed a trap in my path and turned me back. He has left me devastated, racked with sickness all day long."
      }
    },
