    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "锡安伸手求助，却无人理睬。 耶和华已命四周邻国与雅各为敌。 耶路撒冷在他们当中成了污秽之物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "錫安伸出雙手，但沒有人安慰她。 論到雅各，耶和華已經下令，使他四圍的人都與他為敵； 耶路撒冷在他們中間，已經成了不潔的東西。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "錫安舉手，無人安慰。 耶和華論雅各已經出令， 使四圍的人作他仇敵； 耶路撒冷在他們中間像不潔之物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Zion stretches out her hands; There is no one to comfort her; The Lord has commanded concerning Jacob That the ones round about him should be his adversaries; Jerusalem has become an unclean thing among them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Zion stretches out her hands, but there is no one to comfort her. The Lord has decreed for Jacob that his neighbors become his foes; Jerusalem has become an unclean thing among them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jerusalem reaches out for help, but no one comforts her. Regarding his people Israel, the Lord has said, “Let their neighbors be their enemies! Let them be thrown away like a filthy rag!”"
      }
    },
