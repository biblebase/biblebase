    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我向情人们求助， 他们却欺骗我。 我的祭司和长老在觅食求生时， 倒毙在城中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我向我所愛的人呼求，他們卻欺騙我。 我的祭司和長老為了延續生命，尋找糧食的時候， 就在城中氣絕身亡了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我招呼我所親愛的， 他們卻愚弄我。 我的祭司和長老正尋求食物、救性命的時候， 就在城中絕氣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I called to my lovers, but they deceived me; My priests and my elders perished in the city While they sought food to restore their strength themselves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I called to my allies but they betrayed me. My priests and my elders perished in the city while they searched for food to keep themselves alive."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I begged my allies for help, but they betrayed me. My priests and leaders starved to death in the city, even as they searched for food to save their lives."
      }
    },
