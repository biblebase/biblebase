    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂围困我，使我无法逃脱， 祂给我戴上沉重的铜链。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他築牆圍住我，使我不能逃出去； 他又加重我的鋼鍊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他用籬笆圍住我，使我不能出去； 他使我的銅鍊沉重。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has walled me in so that I cannot go out; He has made my chain heavy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He has walled me in so I cannot escape; he has weighed me down with chains."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He has walled me in, and I cannot escape. He has bound me in heavy chains."
      }
