    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世上的君王和居民都不相信敌人能闯进耶路撒冷的城门。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "地上的君王，以及世上所有的居民都不相信， 會有任何敵人和仇敵攻進耶路撒冷的城門。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "地上的君王和世上的居民都不信 敵人和仇敵能進耶路撒冷的城門。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The kings of the earth did not believe, Nor did any of the inhabitants of the world, That the adversary and the enemy Could enter the gates of Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The kings of the earth did not believe, nor did any of the peoples of the world, that enemies and foes could enter the gates of Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Not a king in all the earth— no one in all the world— would have believed that an enemy could march through the gates of Jerusalem."
      }
    },
