    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "锡安的宝贝子民本来贵如黄金， 现在竟被视为陶匠制作的瓦器！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "錫安尊貴的眾民，本來和精金一樣貴重； 現在怎麼竟被看為瓦器，好像陶匠手中所作的呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "錫安寶貴的眾子好比精金， 現在何竟算為窰匠手所做的瓦瓶？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The precious sons of Zion, Weighed against fine gold, How they are regarded as earthen jars, The work of a potter’s hands!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How the precious children of Zion, once worth their weight in gold, are now considered as pots of clay, the work of a potter’s hands!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "See how the precious children of Jerusalem, worth their weight in fine gold, are now treated like pots of clay made by a common potter."
      }
    },
