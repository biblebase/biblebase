    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "追赶我们的人比飞鹰更快， 他们在山上追捕我们， 在旷野伏击我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "追趕我們的比空中的鷹還快； 他們在山上追趕我們，在曠野埋伏攻擊我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "追趕我們的比空中的鷹更快； 他們在山上追逼我們， 在曠野埋伏，等候我們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Our pursuers were swifter Than the eagles of the sky; They chased us on the mountains, They waited in ambush for us in the wilderness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Our pursuers were swifter than eagles in the sky; they chased us over the mountains and lay in wait for us in the desert."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Our enemies were swifter than eagles in flight. If we fled to the mountains, they found us. If we hid in the wilderness, they were waiting for us there."
      }
