    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在，他们的面目比煤炭还黑， 走在街上无人认得。 他们骨瘦如柴，干如枯木。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在他們的面貌比煤炭還黑，在街上再沒有人認得他們； 他們皮包骨頭，枯瘦如柴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在他們的面貌比煤炭更黑， 以致在街上無人認識； 他們的皮膚緊貼骨頭， 枯乾如同槁木。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their appearance is blacker than soot, They are not recognized in the streets; Their skin is shriveled on their bones, It is withered, it has become like wood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But now they are blacker than soot; they are not recognized in the streets. Their skin has shriveled on their bones; it has become as dry as a stick."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But now their faces are blacker than soot. No one recognizes them in the streets. Their skin sticks to their bones; it is as dry and hard as wood."
      }
    },
