    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "死于刀下的胜过死于饥饿的， 后者因田间缺粮而活活饿死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些死於刀下的比餓死的還好； 餓死的因缺乏田間的出產，就日漸消瘦而死亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "餓死的不如被刀殺的， 因為這是缺了田間的土產， 就身體衰弱，漸漸消滅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Better are those slain with the sword Than those slain with hunger; For they pine away, being stricken For lack of the fruits of the field."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those killed by the sword are better off than those who die of famine; racked with hunger, they waste away for lack of food from the field."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those killed by the sword are better off than those who die of hunger. Starving, they waste away for lack of food from the fields."
      }
    },
