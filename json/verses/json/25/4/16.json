    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华亲自驱散他们， 不再眷顾他们； 人们不再尊重祭司， 也不再敬重长老。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華親臨分散他們，不再看顧他們； 人不再尊重祭司，也不厚待長老。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華發怒，將他們分散， 不再眷顧他們； 人不重看祭司，也不厚待長老。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The presence of the Lord has scattered them, He will not continue to regard them; They did not honor the priests, They did not favor the elders."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord himself has scattered them; he no longer watches over them. The priests are shown no honor, the elders no favor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord himself has scattered them, and he no longer helps them. People show no respect for the priests and no longer honor the leaders."
      }
    },
