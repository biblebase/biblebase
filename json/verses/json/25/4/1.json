    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "黄金竟失去光泽， 纯金竟变色， 宝石竟被弃之街头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "黃金怎麼竟然失去光澤，純金怎麼竟然變色？ 聖殿的石頭都被拋棄在各街頭上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "黃金何其失光！ 純金何其變色！ 聖所的石頭倒在各市口上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How dark the gold has become, How the pure gold has changed! The sacred stones are poured out At the corner of every street."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How the gold has lost its luster, the fine gold become dull! The sacred gems are scattered at every street corner."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How the gold has lost its luster! Even the finest gold has become dull. The sacred gemstones lie scattered in the streets!"
      }
    },
