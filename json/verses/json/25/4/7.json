    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "锡安的首领曾比雪纯净， 比奶更白， 身体比红宝石更红润， 相貌美如蓝宝石。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "錫安的貴族素來比雪更明亮，比奶更潔白； 他們的身體比紅寶石更紅潤，他們的樣貌像藍寶石一樣美麗。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "錫安的貴冑素來比雪純淨， 比奶更白； 他們的身體比紅寶玉更紅， 像光潤的藍寶石一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Her consecrated ones were purer than snow, They were whiter than milk; They were more ruddy in body than corals, Their polishing was like lapis lazuli."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their princes were brighter than snow and whiter than milk, their bodies more ruddy than rubies, their appearance like lapis lazuli."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Our princes once glowed with health— brighter than snow, whiter than milk. Their faces were as ruddy as rubies, their appearance like fine jewels."
      }
    },
