    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "婴儿干渴难忍，舌头紧贴上膛； 孩童乞求食物，却无人给予。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "嬰兒的舌頭因乾渴而緊貼上膛； 孩童求餅，卻沒有人分給他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "吃奶孩子的舌頭因乾渴貼住上膛； 孩童求餅，無人擘給他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The tongue of the infant cleaves To the roof of its mouth because of thirst; The little ones ask for bread, But no one breaks it for them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because of thirst the infant’s tongue sticks to the roof of its mouth; the children beg for bread, but no one gives it to them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The parched tongues of their little ones stick to the roofs of their mouths in thirst. The children cry for bread, but no one has any to give them."
      }
