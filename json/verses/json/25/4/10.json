    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的百姓遭毁灭时， 慈母亲手煮自己的儿女充饥。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "慈心的婦人親手烹煮自己所生的孩子； 在我眾民遭毀滅的時候，孩子竟成了母親的食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "慈心的婦人，當我眾民被毀滅的時候， 親手煮自己的兒女作為食物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The hands of compassionate women Boiled their own children; They became food for them Because of the destruction of the daughter of my people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "With their own hands compassionate women have cooked their own children, who became their food when my people were destroyed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Tenderhearted women have cooked their own children. They have eaten them to survive the siege."
      }
    },
