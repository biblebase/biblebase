      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们如盲人在街上游荡， 沾满血污， 无人敢碰他们的衣服。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，他們像瞎子在街上流蕩，他們被血玷污， 沒有人可以觸摸他們的衣服。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們在街上如瞎子亂走， 又被血玷污， 以致人不能摸他們的衣服。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They wandered, blind, in the streets; They were defiled with blood So that no one could touch their garments."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now they grope through the streets as if they were blind. They are so defiled with blood that no one dares to touch their garments."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They wandered blindly through the streets, so defiled by blood that no one dared touch them."
      }
    },
