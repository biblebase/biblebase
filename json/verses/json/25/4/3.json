    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "豺狼尚且哺养自己的幼儿， 我的子民却像荒野的鸵鸟一样残忍无情。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "野狗尚且用乳房哺養牠們的幼兒； 但我的眾民變得多麼殘忍，像曠野的鴕鳥一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "野狗尚且把奶乳哺其子， 我民的婦人倒成為殘忍， 好像曠野的鴕鳥一般。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even jackals offer the breast, They nurse their young; But the daughter of my people has become cruel Like ostriches in the wilderness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even jackals offer their breasts to nurse their young, but my people have become heartless like ostriches in the desert."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even the jackals feed their young, but not my people Israel. They ignore their children’s cries, like ostriches in the desert."
      }
    },
