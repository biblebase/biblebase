    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“用酒灌醉邻居以观看他们祼体的人啊， 你们有祸了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你這請鄰舍喝酒，卻把毒物混入， 使他醉倒， 為要見他赤裸的，有禍了！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "給人酒喝、又加上毒物、 使他喝醉、好看見他下體的有禍了！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Woe to you who make your neighbors drink, Who mix in your venom even to make them drunk So as to look on their nakedness!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Woe to him who gives drink to his neighbors, pouring it from the wineskin till they are drunk, so that he can gaze on their naked bodies!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What sorrow awaits you who make your neighbors drunk! You force your cup on them so you can gloat over their shameful nakedness."
      }
    },
