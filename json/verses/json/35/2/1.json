    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要登上瞭望塔， 站在望楼上等候， 看耶和华要对我说什么， 要怎样答复我的抱怨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要站在哨崗，立在城樓， 留心看耶和華在我裡面說甚麼， 怎樣使我答覆自己的怨訴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要站在守望所， 立在望樓上觀看， 看耶和華對我說甚麼話， 我可用甚麼話向他訴冤。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will stand on my guard post And station myself on the rampart; And I will keep watch to see what He will speak to me, And how I may reply when I am reproved."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will stand at my watch and station myself on the ramparts; I will look to see what he will say to me, and what answer I am to give to this complaint."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will climb up to my watchtower and stand at my guardpost. There I will wait to see what the Lord says and how he will answer my complaint."
      }
    },
