    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“为自己的家积蓄不义之财、 在高处搭窝躲避灾祸的人啊， 你们有祸了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你這為自己的家積聚不義之財， 在高處搭窩，以逃避災害的， 有禍了！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為本家積蓄不義之財、 在高處搭窩、指望免災的有禍了！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Woe to him who gets evil gain for his house To put his nest on high, To be delivered from the hand of calamity!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Woe to him who builds his house by unjust gain, setting his nest on high to escape the clutches of ruin!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What sorrow awaits you who build big houses with money gained dishonestly! You believe your wealth will buy security, putting your family’s nest beyond the reach of danger."
      }
    },
