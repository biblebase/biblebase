    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，我听过你的威名； 耶和华啊，我对你的作为充满敬畏。 求你如今再次彰显你的作为， 求你发怒的时候仍以怜悯为念。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！我聽見你的聲音，懼怕你的作為； 求你在這些年間復興，在這些年間彰顯， 發怒的時候以憐憫為懷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我聽見你的名聲就懼怕。 耶和華啊，求你在這些年間復興你的作為， 在這些年間顯明出來； 在發怒的時候以憐憫為念。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Lord, I have heard the report about You and I fear. O Lord, revive Your work in the midst of the years, In the midst of the years make it known; In wrath remember mercy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, I have heard of your fame; I stand in awe of your deeds, Lord. Repeat them in our day, in our time make them known; in wrath remember mercy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have heard all about you, Lord. I am filled with awe by your amazing works. In this time of our deep need, help us again as you did in years gone by. And in your anger, remember your mercy."
      }
    },
