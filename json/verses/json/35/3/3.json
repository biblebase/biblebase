    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝从提幔来， 圣者从巴兰山来。（细拉） 祂的荣耀遮蔽诸天， 颂赞祂的声音响彻大地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神由提幔而來，聖者從巴蘭山臨到。 他的榮光瀰漫穹蒼，讚美他的聲音響徹大地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神從提幔而來； 聖者從巴蘭山臨到。（細拉） 他的榮光遮蔽諸天； 頌讚充滿大地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God comes from Teman, And the Holy One from Mount Paran. Selah. His splendor covers the heavens, And the earth is full of His praise."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God came from Teman, the Holy One from Mount Paran. His glory covered the heavens and his praise filled the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I see God moving across the deserts from Edom, the Holy One coming from Mount Paran. His brilliant splendor fills the heavens, and the earth is filled with his praise."
      }
    },
