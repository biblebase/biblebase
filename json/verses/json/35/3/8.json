    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你是向江河发怒吗？ 你乘着得胜的战车而来， 是向江河生气吗？ 是向海洋发怒吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你騎上你的馬，乘駕得勝的戰車， 難道是惱怒江河、向江河發怒、向海洋洩憤？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你乘在馬上， 坐在得勝的車上， 豈是不喜悅江河、 向江河發怒氣、 向洋海發憤恨嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Did the Lord rage against the rivers, Or was Your anger against the rivers, Or was Your wrath against the sea, That You rode on Your horses, On Your chariots of salvation?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Were you angry with the rivers, Lord? Was your wrath against the streams? Did you rage against the sea when you rode your horses and your chariots to victory?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Was it in anger, Lord, that you struck the rivers and parted the sea? Were you displeased with them? No, you were sending your chariots of salvation!"
      }
    },
