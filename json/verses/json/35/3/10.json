    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "群山看见你就战栗， 大雨倾盆， 深渊翻腾，波浪滔天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "諸山見你而顫抖， 眾水暴流而過； 深淵發聲，舉手高揚。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "山嶺見你，無不戰懼； 大水氾濫過去， 深淵發聲，洶湧翻騰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The mountains saw You and quaked; The downpour of waters swept by. The deep uttered forth its voice, It lifted high its hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the mountains saw you and writhed. Torrents of water swept by; the deep roared and lifted its waves on high."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The mountains watched and trembled. Onward swept the raging waters. The mighty deep cried out, lifting its hands in submission."
      }
    },
