    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "迦勒底人用钩子钩住他们， 用渔网网住他们， 用拖网把他们聚在一起， 并因此而欢喜快乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "迦勒底人既用鈎把他們釣起來，用網拖走， 用魚網收聚在一處，就歡喜快樂，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他用鈎鈎住，用網捕獲， 用拉網聚集他們； 因此，他歡喜快樂，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Chaldeans bring all of them up with a hook, Drag them away with their net, And gather them together in their fishing net. Therefore they rejoice and are glad."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wicked foe pulls all of them up with hooks, he catches them in his net, he gathers them up in his dragnet; and so he rejoices and is glad."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Must we be strung up on their hooks and caught in their nets while they rejoice and celebrate?"
      }
    },
