    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“他们的马比豹子还快， 比夜狼还猛。 他们的骑兵从远方飞驰而来， 如鹰扑食。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的馬比豹更快，比晚上的豺狼更猛。 他們的騎兵奔馳，自遠而來； 他們如鷹飛翔，迅速吞噬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的馬比豹更快， 比晚上的豺狼更猛。 馬兵踴躍爭先， 都從遠方而來； 他們飛跑如鷹抓食，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their horses are swifter than leopards And keener than wolves in the evening. Their horsemen come galloping, Their horsemen come from afar; They fly like an eagle swooping down to devour."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their horses are swifter than leopards, fiercer than wolves at dusk. Their cavalry gallops headlong; their horsemen come from afar. They fly like an eagle swooping to devour;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their horses are swifter than cheetahs and fiercer than wolves at dusk. Their charioteers charge from far away. Like eagles, they swoop down to devour their prey."
      }
    },
