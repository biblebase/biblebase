    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们快来看啊！有一个人把我过去的事全部说了出来。莫非祂就是基督？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們來，看看一個人，他把我所作的一切都說出來，難道這人就是基督嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們來看！有一個人將我素來所行的一切事都給我說出來了，莫非這就是基督嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Come, see a man who told me all the things that I have done; this is not the Christ, is it?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Come, see a man who told me everything I ever did. Could this be the Messiah?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Come and see a man who told me everything I ever did! Could he possibly be the Messiah?”"
      }
