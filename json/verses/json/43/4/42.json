    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们对那妇人说：“现在我们信，不再是因为你的话，而是我们亲耳听见了，知道祂真的是救世主。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們就對那婦人說：“現在我們信，不再是因為你的話，而是因為我們親自聽見了，知道這位真是世人的救主。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "便對婦人說：「現在我們信，不是因為你的話，是我們親自聽見了，知道這真是救世主。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they were saying to the woman, “It is no longer because of what you said that we believe, for we have heard for ourselves and know that this One is indeed the Savior of the world.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They said to the woman, “We no longer believe just because of what you said; now we have heard for ourselves, and we know that this man really is the Savior of the world.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they said to the woman, “Now we believe, not just because of what you told us, but because we have heard him ourselves. Now we know that he is indeed the Savior of the world.”"
      }
