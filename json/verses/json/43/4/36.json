    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "收割的人不但可以得到报酬，而且可以为永生收获果实，叫撒种的和收割的一同快乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "收割的人得到工資，也積儲五穀直到永生，使撒種的和收割的一同快樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "收割的人得工價，積蓄五穀到永生，叫撒種的和收割的一同快樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Already he who reaps is receiving wages and is gathering fruit for life eternal; so that he who sows and he who reaps may rejoice together."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even now the one who reaps draws a wage and harvests a crop for eternal life, so that the sower and the reaper may be glad together."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The harvesters are paid good wages, and the fruit they harvest is people brought to eternal life. What joy awaits both the planter and the harvester alike!"
      }
    },
