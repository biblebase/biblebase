    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那天正是逾越节的预备日，大约在中午十二时，彼拉多对犹太人说：“看啊！你们的王。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那天是逾越節的預備日，約在正午的時候。彼拉多對猶太人說：“看，你們的王！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那日是預備逾越節的日子，約有午正。彼拉多對猶太人說：「看哪，這是你們的王！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now it was the day of preparation for the Passover; it was about the sixth hour. And he *said to the Jews, “Behold, your King!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It was the day of Preparation of the Passover; it was about noon. “Here is your king,” Pilate said to the Jews."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It was now about noon on the day of preparation for the Passover. And Pilate said to the people, “Look, here is your king!”"
      }
