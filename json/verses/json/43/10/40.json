    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来，耶稣到了约旦河东岸约翰从前施洗的地方，在那里住下来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌又往約旦河東去，到約翰從前施洗的地方，住在那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌又往約旦河外去，到了約翰起初施洗的地方，就住在那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He went away again beyond the Jordan to the place where John was first baptizing, and He was staying there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jesus went back across the Jordan to the place where John had been baptizing in the early days. There he stayed,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He went beyond the Jordan River near the place where John was first baptizing and stayed there awhile."
      }
    },
