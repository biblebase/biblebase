    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要赐他们永生，他们永不灭亡。没有人能从我的手里把他们抢走。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我賜給他們永生，他們永不滅亡，誰也不能把他們從我手裡奪去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我又賜給他們永生；他們永不滅亡，誰也不能從我手裏把他們奪去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and I give eternal life to them, and they will never perish; and no one will snatch them out of My hand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I give them eternal life, and they shall never perish; no one will snatch them out of my hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I give them eternal life, and they will never perish. No one can snatch them away from me,"
      }
