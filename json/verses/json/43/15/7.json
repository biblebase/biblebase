    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你们常在我里面，我的话也常在你们里面，无论你们求什么，都会得到应允。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們若住在我裡面，我的話也留在你們裡面；無論你們想要甚麼，祈求，就給你們成就。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若常在我裏面，我的話也常在你們裏面，凡你們所願意的，祈求，就給你們成就。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you abide in Me, and My words abide in you, ask whatever you wish, and it will be done for you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you remain in me and my words remain in you, ask whatever you wish, and it will be done for you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if you remain in me and my words remain in you, you may ask for anything you want, and it will be granted!"
      }
    },
