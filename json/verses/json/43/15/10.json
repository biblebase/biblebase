        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们若遵守我的命令，就必常在我的爱中，正如我遵守了父的命令，常在父的爱中一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們遵守我的命令，就必定住在我的愛裡，正像我遵守了我父的命令，住在他的愛裡一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若遵守我的命令，就常在我的愛裏，正如我遵守了我父的命令，常在他的愛裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you keep My commandments, you will abide in My love; just as I have kept My Father’s commandments and abide in His love."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you keep my commands, you will remain in my love, just as I have kept my Father’s commands and remain in his love."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When you obey my commandments, you remain in my love, just as I obey my Father’s commandments and remain in his love."
      }
