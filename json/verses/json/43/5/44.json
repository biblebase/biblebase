    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们喜欢互相恭维，却不追求从独一上帝来的荣耀，怎能信我呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們彼此接受稱讚，卻不尋求從獨一的　神而來的稱讚，怎麼能信呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們互相受榮耀，卻不求從獨一之神來的榮耀，怎能信我呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How can you believe, when you receive glory from one another and you do not seek the glory that is from the one and only God?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How can you believe since you accept glory from one another but do not seek the glory that comes from the only God?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No wonder you can’t believe! For you gladly honor each other, but you don’t care about the honor that comes from the one who alone is God."
      }
    },
