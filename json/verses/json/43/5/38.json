    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "心里也没有祂的道，因为你们不信祂所差来的那位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的道，你們也不放在心裡，因為你們不信他所差來的那一位。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們並沒有他的道存在心裏；因為他所差來的，你們不信。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You do not have His word abiding in you, for you do not believe Him whom He sent."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "nor does his word dwell in you, for you do not believe the one he sent."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and you do not have his message in your hearts, because you do not believe me—the one he sent to you."
      }
    },
