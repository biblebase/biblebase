    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "叫人尊敬子如同尊敬父。不尊敬子的，就是不尊敬差子来的父。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "使所有的人尊敬子好像尊敬父一樣。不尊敬子的，就是不尊敬那差他來的父。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "叫人都尊敬子如同尊敬父一樣。不尊敬子的，就是不尊敬差子來的父。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that all will honor the Son even as they honor the Father. He who does not honor the Son does not honor the Father who sent Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "that all may honor the Son just as they honor the Father. Whoever does not honor the Son does not honor the Father, who sent him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "so that everyone will honor the Son, just as they honor the Father. Anyone who does not honor the Son is certainly not honoring the Father who sent him."
      }
    },
