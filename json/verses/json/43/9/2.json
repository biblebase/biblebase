    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "门徒问耶稣：“老师，这个人生下来便双目失明，是因为他犯了罪呢，还是他父母犯了罪呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的門徒問他：“拉比，這人生下來就瞎眼，是誰犯了罪？是他呢，還是他的父母呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門徒問耶穌說：「拉比，這人生來是瞎眼的，是誰犯了罪？是這人呢？是他父母呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And His disciples asked Him, “Rabbi, who sinned, this man or his parents, that he would be born blind?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His disciples asked him, “Rabbi, who sinned, this man or his parents, that he was born blind?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Rabbi,” his disciples asked him, “why was this man born blind? Was it because of his own sins or his parents’ sins?”"
      }
    },
