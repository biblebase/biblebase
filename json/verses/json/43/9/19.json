    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "问他们：“这是你们的儿子吗？你们不是说他生来就瞎眼吗？怎么现在能看见了？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "問他們：“這是你們所說那生下來就瞎眼的兒子嗎？現在他怎麼又能看見呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "問他們說：「這是你們的兒子嗎？你們說他生來是瞎眼的，如今怎麼能看見了呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and questioned them, saying, “Is this your son, who you say was born blind? Then how does he now see?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Is this your son?” they asked. “Is this the one you say was born blind? How is it that now he can see?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They asked them, “Is this your son? Was he born blind? If so, how can he now see?”"
      }
    },
