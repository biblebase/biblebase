    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世人不会恨你们，只会恨我，因为我指证他们行为邪恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "世人不能恨你們，卻憎恨我，因為我指證他們的行為是邪惡的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "世人不能恨你們，卻是恨我，因為我指證他們所做的事是惡的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The world cannot hate you, but it hates Me because I testify of it, that its deeds are evil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The world cannot hate you, but it hates me because I testify that its works are evil."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The world can’t hate you, but it does hate me because I accuse it of doing evil."
      }
    },
