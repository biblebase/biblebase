    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "绰号“双胞胎”的多马对其他门徒说：“我们也去，好跟祂一块儿死吧。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那稱為“雙生子”的多馬，對其他的門徒說：“我們也去跟他一同死吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "多馬，又稱為低土馬，就對那同作門徒的說：「我們也去和他同死吧。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore Thomas, who is called Didymus, said to his fellow disciples, “Let us also go, so that we may die with Him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Thomas (also known as Didymus) said to the rest of the disciples, “Let us also go, that we may die with him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Thomas, nicknamed the Twin, said to his fellow disciples, “Let’s go, too—and die with Jesus.”"
      }
    },
