    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣看见她和陪她来的犹太人都在哭，心中感动，十分难过，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌看見她在哭，和她一同來的猶太人也在哭，就心裡激動，難過起來，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌看見她哭，並看見與她同來的猶太人也哭，就心裏悲歎，又甚憂愁，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Jesus therefore saw her weeping, and the Jews who came with her also weeping, He was deeply moved in spirit and was troubled,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jesus saw her weeping, and the Jews who had come along with her also weeping, he was deeply moved in spirit and troubled."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jesus saw her weeping and saw the other people wailing with her, a deep anger welled up within him, and he was deeply troubled."
      }
