    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，他们把石头挪开，耶稣望着天说：“父啊，我感谢你，因为你已垂听了我的祷告，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他們把石頭挪開。耶穌舉目向天，說：“父啊，我感謝你，因為你垂聽了我，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們就把石頭挪開。耶穌舉目望天，說：「父啊，我感謝你，因為你已經聽我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they removed the stone. Then Jesus raised His eyes, and said, “Father, I thank You that You have heard Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So they took away the stone. Then Jesus looked up and said, “Father, I thank you that you have heard me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So they rolled the stone aside. Then Jesus looked up to heaven and said, “Father, thank you for hearing me."
      }
