    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其中也有人说：“祂既然能医好瞎眼的人，难道不能叫这个人不死吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們中間有人說：“他既然開了瞎子的眼睛，難道不能使這個人不死嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其中有人說：「他既然開了瞎子的眼睛，豈不能叫這人不死嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But some of them said, “Could not this man, who opened the eyes of the blind man, have kept this man also from dying?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But some of them said, “Could not he who opened the eyes of the blind man have kept this man from dying?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But some said, “This man healed a blind man. Couldn’t he have kept Lazarus from dying?”"
      }
    },
