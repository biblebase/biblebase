      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「我已經告訴你們，我就是。你們若找我，就讓這些人去吧。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus answered, “I told you that I am He; so if you seek Me, let these go their way,”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus answered, “I told you that I am he. If you are looking for me, then let these men go.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I told you that I Am he,” Jesus said. “And since I am the one you want, let these others go.”"
      }
