    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这个该亚法以前曾对犹太人建议说：“让祂一个人替众人死对你们更好。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "該亞法就是從前向猶太人提議說“一個人代替人民死，這是有益的”那個人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這該亞法就是從前向猶太人發議論說「一個人替百姓死是有益的」那位。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Caiaphas was the one who had advised the Jews that it was expedient for one man to die on behalf of the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Caiaphas was the one who had advised the Jewish leaders that it would be good if one man died for the people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Caiaphas was the one who had told the other Jewish leaders, “It’s better that one man should die for the people.”"
      }
    },
