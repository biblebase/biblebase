    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你从世上赐给我的人，我已将你显明给他们。他们本来属于你，你将他们赐给了我，他们遵守你的道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你從世上分別出來賜給我的人，我已經把你的名顯明給他們了。他們是你的，你把他們賜給了我，他們也遵守了你的道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你從世上賜給我的人，我已將你的名顯明與他們。他們本是你的，你將他們賜給我，他們也遵守了你的道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I have manifested Your name to the men whom You gave Me out of the world; they were Yours and You gave them to Me, and they have kept Your word."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I have revealed you    to those whom you gave me out of the world. They were yours; you gave them to me and they have obeyed your word."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I have revealed you to the ones you gave me from this world. They were always yours. You gave them to me, and they have kept your word."
      }
    },
