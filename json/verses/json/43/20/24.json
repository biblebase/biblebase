    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣显现的时候，十二个门徒中绰号“双胞胎”的多马不在场。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "十二個門徒中，有一個稱為“雙生子”的多馬。耶穌來的時候，他沒有和門徒在一起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那十二個門徒中，有稱為低土馬的多馬；耶穌來的時候，他沒有和他們同在。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Thomas, one of the twelve, called Didymus, was not with them when Jesus came."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now Thomas (also known as Didymus), one of the Twelve, was not with the disciples when Jesus came."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One of the twelve disciples, Thomas (nicknamed the Twin), was not with the others when Jesus came."
      }
    },
