    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "周日清晨，天还未亮，抹大拉的玛丽亚就来到坟墓前，发现封住墓口的石头已经挪开了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "禮拜日清早，天還沒有亮的時候，抹大拉的馬利亞來到墓旁，看見石頭已經從墳墓移開了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "七日的第一日清早，天還黑的時候，抹大拉的馬利亞來到墳墓那裏，看見石頭從墳墓挪開了，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now on the first day of the week Mary Magdalene *came early to the tomb, while it *was still dark, and *saw the stone already taken away from the tomb."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Early on the first day of the week, while it was still dark, Mary Magdalene went to the tomb and saw that the stone had been removed from the entrance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Early on Sunday morning, while it was still dark, Mary Magdalene came to the tomb and found that the stone had been rolled away from the entrance."
      }
    },
