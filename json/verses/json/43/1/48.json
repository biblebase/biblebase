    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拿但业问耶稣：“你怎么会认识我？” 耶稣答道：“腓力还没有去找你之前，我就看见你在无花果树下了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "拿但業問他：“你怎麼認識我呢？”耶穌回答：“腓力還沒有招呼你，你在無花果樹下的時候，我就看見你了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拿但業對耶穌說：「你從哪裏知道我呢？」耶穌回答說：「腓力還沒有招呼你，你在無花果樹底下，我就看見你了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nathanael *said to Him, “How do You know me?” Jesus answered and said to him, “Before Philip called you, when you were under the fig tree, I saw you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“How do you know me?” Nathanael asked. Jesus answered, “I saw you while you were still under the fig tree before Philip called you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“How do you know about me?” Nathanael asked. Jesus replied, “I could see you under the fig tree before Philip found you.”"
      }
    },
