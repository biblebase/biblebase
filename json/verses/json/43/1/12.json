    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但所有接纳祂的，就是那些信祂的人，祂就赐给他们权利成为上帝的儿女。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡接受他的，就是信他名的人，他就賜給他們權利，成為　神的兒女。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡接待他的，就是信他名的人，他就賜他們權柄作神的兒女。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as many as received Him, to them He gave the right to become children of God, even to those who believe in His name,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet to all who did receive him, to those who believed in his name, he gave the right to become children of God—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But to all who believed him and accepted him, he gave the right to become children of God."
      }
    },
