    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们又追问：“你到底是谁？我们好回复差我们来的人。你自己说你是谁？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他們再問：“你是誰？好讓我們回覆派我們來的人。你說你自己是誰？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是他們說：「你到底是誰，叫我們好回覆差我們來的人。你自己說，你是誰？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they said to him, “Who are you, so that we may give an answer to those who sent us? What do you say about yourself?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Finally they said, “Who are you? Give us an answer to take back to those who sent us. What do you say about yourself?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then who are you? We need an answer for those who sent us. What do you have to say about yourself?”"
      }
    },
