    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我在事情还没有发生之前，预先告诉你们，等事情发生以后，你们就会相信我是基督。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在，事情還沒有發生，我就告訴你們，使你們到時可以信我就是‘那一位’。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "如今事情還沒有成就，我要先告訴你們，叫你們到事情成就的時候可以信我是基督。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From now on I am telling you before it comes to pass, so that when it does occur, you may believe that I am He."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I am telling you now before it happens, so that when it does happen you will believe that I am who I am."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I tell you this beforehand, so that when it happens you will believe that I Am the Messiah."
      }
    },
