    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼得说：“你绝对不可以洗我的脚。” 耶稣说：“如果我不洗你的脚，你就和我没有关系了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼得說：“不行，你千萬不可洗我的腳！”耶穌說：“如果我不洗你，你就與我沒有關係了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼得說：「你永不可洗我的腳！」耶穌說：「我若不洗你，你就與我無分了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Peter *said to Him, “Never shall You wash my feet!” Jesus answered him, “If I do not wash you, you have no part with Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“No,” said Peter, “you shall never wash my feet.” Jesus answered, “Unless I wash you, you have no part with me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“No,” Peter protested, “you will never ever wash my feet!” Jesus replied, “Unless I wash you, you won’t belong to me.”"
      }
    },
