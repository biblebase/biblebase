    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但这是为了让世人知道我爱父，父怎么吩咐我，我就怎么做。起来，我们走吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但這些事是要使世人知道我愛父，並且知道父怎樣吩咐了我，我就怎樣作。起來，我們走吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但要叫世人知道我愛父，並且父怎樣吩咐我，我就怎樣行。起來，我們走吧！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but so that the world may know that I love the Father, I do exactly as the Father commanded Me. Get up, let us go from here."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but he comes so that the world may learn that I love the Father and do exactly what my Father has commanded me. “Come now; let us leave."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "but I will do what the Father requires of me, so that the world will know that I love the Father. Come, let’s be going."
      }
