    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝差祂的儿子到世上来，不是要定世人的罪，而是要借着祂的儿子拯救世人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為　神差他的兒子到世上來，不是要定世人的罪，而是要使世人藉著他得救。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為神差他的兒子降世，不是要定世人的罪，乃是要叫世人因他得救。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For God did not send the Son into the world to judge the world, but that the world might be saved through Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For God did not send his Son into the world to condemn the world, but to save the world through him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God sent his Son into the world not to judge the world, but to save the world through him."
      }
    },
