    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "作恶的人恨光，不肯接近光，恐怕他们的罪行暴露出来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡作惡的都恨光，不來接近光，免得他的惡行暴露出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡作惡的便恨光，並不來就光，恐怕他的行為受責備。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For everyone who does evil hates the Light, and does not come to the Light for fear that his deeds will be exposed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Everyone who does evil hates the light, and will not come into the light for fear that their deeds will be exposed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All who do evil hate the light and refuse to go near it for fear their sins will be exposed."
      }
    },
