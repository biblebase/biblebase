    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“我实实在在地告诉你，人若不重生，就不能看见上帝的国。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌回答：“我實實在在告訴你，人若不重生，就不能見　神的國。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌回答說：「我實實在在地告訴你，人若不重生，就不能見神的國。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus answered and said to him, “Truly, truly, I say to you, unless one is born again he cannot see the kingdom of God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus replied, “Very truly I tell you, no one can see the kingdom of God unless they are born again.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “I tell you the truth, unless you are born again, you cannot see the Kingdom of God.”"
      }
    },
