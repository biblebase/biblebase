    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我知道祂的命令能带来永生。所以祂怎么告诉我，我就怎么说。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我知道他的命令就是永生。所以，我所講的，正是父吩咐我要我講的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我也知道他的命令就是永生。故此，我所講的話正是照着父對我所說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I know that His commandment is eternal life; therefore the things I speak, I speak just as the Father has told Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I know that his command leads to eternal life. So whatever I say is just what the Father has told me to say.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I know his commands lead to eternal life; so I say whatever the Father tells me to say.”"
      }
    },
