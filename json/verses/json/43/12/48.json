    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弃绝我、不接受我话的人将受到审判，我讲过的道在末日要审判他，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "棄絕我又不接受我的話的人，自有審判他的。我所講的道在末日要定他的罪；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "棄絕我、不領受我話的人，有審判他的－就是我所講的道在末日要審判他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who rejects Me and does not receive My sayings, has one who judges him; the word I spoke is what will judge him at the last day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There is a judge for the one who rejects me and does not accept my words; the very words I have spoken will condemn them at the last day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But all who reject me and my message will be judged on the day of judgment by the truth I have spoken."
      }
    },
