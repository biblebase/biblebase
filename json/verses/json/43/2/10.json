    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对他说：“人们都是先拿好酒款待客人，等客人喝够了，才把次等的拿出来，你却把好酒留到现在！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對他說：“人人都是先擺上好酒，等到親友喝夠了，才擺上次等的，你倒把好酒留到現在。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對他說：「人都是先擺上好酒，等客喝足了，才擺上次的，你倒把好酒留到如今！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and *said to him, “Every man serves the good wine first, and when the people have drunk freely, then he serves the poorer wine; but you have kept the good wine until now.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and said, “Everyone brings out the choice wine first and then the cheaper wine after the guests have had too much to drink; but you have saved the best till now.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“A host always serves the best wine first,” he said. “Then, when everyone has had a lot to drink, he brings out the less expensive wine. But you have kept the best until now!”"
      }
