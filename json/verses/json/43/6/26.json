    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣回答说：“我实实在在地告诉你们，你们找我，并不是因为看见了神迹，而是因为你们有饼吃，并且吃饱了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌回答：“我實實在在告訴你們，你們找我，不是因為看見了神蹟，而是因為吃了餅又吃飽了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌回答說：「我實實在在地告訴你們，你們找我，並不是因見了神蹟，乃是因吃餅得飽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus answered them and said, “Truly, truly, I say to you, you seek Me, not because you saw signs, but because you ate of the loaves and were filled."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus answered, “Very truly I tell you, you are looking for me, not because you saw the signs I performed but because you ate the loaves and had your fill."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “I tell you the truth, you want to be with me because I fed you, not because you understood the miraculous signs."
      }
    },
