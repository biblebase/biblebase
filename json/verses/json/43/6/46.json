      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Not that anyone has seen the Father, except the One who is from God; He has seen the Father."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
