    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "带你们进入那奶蜜之乡。但我不会跟你们一起去，免得我在途中就把你们灭绝了，因为你们是顽固不化的百姓。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要上那流奶與蜜的地方去；我不與你們一同上去，因為你們是硬著頸項的人民，免得我在路上把你們消滅。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "領你到那流奶與蜜之地。我自己不同你們上去；因為你們是硬着頸項的百姓，恐怕我在路上把你們滅絕。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Go up to a land flowing with milk and honey; for I will not go up in your midst, because you are an obstinate people, and I might destroy you on the way.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Go up to the land flowing with milk and honey. But I will not go with you, because you are a stiff-necked people and I might destroy you on the way.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Go up to this land that flows with milk and honey. But I will not travel among you, for you are a stubborn and rebellious people. If I did, I would surely destroy you along the way.”"
      }
    },
