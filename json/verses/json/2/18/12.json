    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西的岳父叶忒罗说完，便向上帝献燔祭及其他祭物。亚伦和以色列的长老都来与他一起在上帝面前吃饭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西的岳父葉忒羅把燔祭和幾樣祭品拿來獻給　神；亞倫和所有以色列的長老都來到　神面前，與摩西的岳父一同吃飯。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西的岳父葉忒羅把燔祭和平安祭獻給神。亞倫和以色列的眾長老都來了，與摩西的岳父在神面前吃飯。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jethro, Moses’ father-in-law, took a burnt offering and sacrifices for God, and Aaron came with all the elders of Israel to eat a meal with Moses’ father-in-law before God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jethro, Moses’ father-in-law, brought a burnt offering and other sacrifices to God, and Aaron came with all the elders of Israel to eat a meal with Moses’ father-in-law in the presence of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jethro, Moses’ father-in-law, brought a burnt offering and sacrifices to God. Aaron and all the elders of Israel came out and joined him in a sacrificial meal in God’s presence."
      }
    },
