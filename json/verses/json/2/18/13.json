    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第二天，摩西坐着审理百姓的纠纷，百姓从早到晚都站在摩西周围。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "次日，摩西坐著審判人民的案件；人民從早到晚都站在摩西的身旁。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "第二天，摩西坐着審判百姓，百姓從早到晚都站在摩西的左右。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It came about the next day that Moses sat to judge the people, and the people stood about Moses from the morning until the evening."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The next day Moses took his seat to serve as judge for the people, and they stood around him from morning till evening."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The next day, Moses took his seat to hear the people’s disputes against each other. They waited before him from morning till evening."
      }
    },
