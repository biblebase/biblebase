    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西下山把耶和华的话和法令都告诉百姓，百姓齐声回答：“我们愿意遵行耶和华的一切吩咐。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西下來，把耶和華的一切話和一切典章都向人民講述；人民都同聲回答，說：“耶和華吩咐的一切話，我們都必遵行。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西下山，將耶和華的命令典章都述說與百姓聽。眾百姓齊聲說：「耶和華所吩咐的，我們都必遵行。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Moses came and recounted to the people all the words of the Lord and all the ordinances; and all the people answered with one voice and said, “All the words which the Lord has spoken we will do!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Moses went and told the people all the Lord’s words and laws, they responded with one voice, “Everything the Lord has said we will do.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Moses went down to the people and repeated all the instructions and regulations the Lord had given him. All the people answered with one voice, “We will do everything the Lord has commanded.”"
      }
    },
