    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只有你可以靠近我，其他人，包括全体百姓，都不得走近。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只有摩西一人可以走近耶和華，他們卻不可以親近；人民也不可和摩西一同上來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟獨你可以親近耶和華；他們卻不可親近；百姓也不可和你一同上來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moses alone, however, shall come near to the Lord, but they shall not come near, nor shall the people come up with him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but Moses alone is to approach the Lord; the others must not come near. And the people may not come up with him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Only Moses is allowed to come near to the Lord. The others must not come near, and none of the other people are allowed to climb up the mountain with him.”"
      }
