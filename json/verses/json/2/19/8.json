    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "百姓都齐声回答说：“凡耶和华所吩咐的，我们都愿意遵从。” 摩西便把他们的话回报耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人民都一致地回答，說：“凡是耶和華吩咐的，我們都要遵行。”摩西就去把人民的話回覆耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "百姓都同聲回答說：「凡耶和華所說的，我們都要遵行。」摩西就將百姓的話回覆耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the people answered together and said, “All that the Lord has spoken we will do!” And Moses brought back the words of the people to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The people all responded together, “We will do everything the Lord has said.” So Moses brought their answer back to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And all the people responded together, “We will do everything the Lord has commanded.” So Moses brought the people’s answer back to the Lord."
      }
    },
