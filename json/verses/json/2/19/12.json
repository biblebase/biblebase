    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "此外，你要在山的四围划定界线，吩咐百姓不得上山或碰到界线，违例者死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要在山的周圍給人民定界限，說：‘你們要謹慎，不可上山來，也不可觸摸山腳；凡是觸摸著那山的，必要把他處死。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要在山的四圍給百姓定界限，說：『你們當謹慎，不可上山去，也不可摸山的邊界；凡摸這山的，必要治死他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall set bounds for the people all around, saying, ‘Beware that you do not go up on the mountain or touch the border of it; whoever touches the mountain shall surely be put to death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Put limits for the people around the mountain and tell them, ‘Be careful that you do not approach the mountain or touch the foot of it. Whoever touches the mountain is to be put to death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Mark off a boundary all around the mountain. Warn the people, ‘Be careful! Do not go up on the mountain or even touch its boundaries. Anyone who touches the mountain will certainly be put to death."
      }
    },
