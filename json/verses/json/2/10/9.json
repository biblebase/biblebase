    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西回答说：“我们男女老幼都要去，还有我们的牛群和羊群，因为我们要去守耶和华的节期，向祂献祭。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西回答：“我們要和我們年少的與年老的同去，要和我們的兒女同去，也要和我們的羊群與牛群同去，因為我們必須向耶和華守節。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西說：「我們要和我們老的少的、兒子女兒同去，且把羊羣牛羣一同帶去，因為我們務要向耶和華守節。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moses said, “We shall go with our young and our old; with our sons and our daughters, with our flocks and our herds we shall go, for we must hold a feast to the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moses answered, “We will go with our young and our old, with our sons and our daughters, and with our flocks and herds, because we are to celebrate a festival to the Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses replied, “We will all go—young and old, our sons and daughters, and our flocks and herds. We must all join together in celebrating a festival to the Lord.”"
      }
    },
