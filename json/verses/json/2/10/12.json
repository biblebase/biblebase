    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对摩西说：“你向埃及伸杖，使蝗虫飞来吃尽冰雹过后剩下的植物。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對摩西說：“你要向埃及地伸手，使蝗蟲上埃及地來，吃盡地上所有的蔬菜，就是受冰雹擊打後留下的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對摩西說：「你向埃及地伸杖，使蝗蟲到埃及地上來，吃地上一切的菜蔬，就是冰雹所剩的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord said to Moses, “Stretch out your hand over the land of Egypt for the locusts, that they may come up on the land of Egypt and eat every plant of the land, even all that the hail has left.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Lord said to Moses, “Stretch out your hand over Egypt so that locusts swarm over the land and devour everything growing in the fields, everything left by the hail.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord said to Moses, “Raise your hand over the land of Egypt to bring on the locusts. Let them cover the land and devour every plant that survived the hailstorm.”"
      }
    },
