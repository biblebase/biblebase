    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“将来耶和华成就祂起誓给你们祖先的应许，领你们进入迦南人的土地，把那里赐给你们以后，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“日後，耶和華照著他向你和你的列祖所起的誓，把你領進迦南人之地，把這地賜給你之後，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「將來，耶和華照他向你和你祖宗所起的誓將你領進迦南人之地，把這地賜給你，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Now when the Lord brings you to the land of the Canaanite, as He swore to you and to your fathers, and gives it to you,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“After the Lord brings you into the land of the Canaanites and gives it to you, as he promised on oath to you and your ancestors,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what you must do when the Lord fulfills the promise he swore to you and to your ancestors. When he gives you the land where the Canaanites now live,"
      }
    },
