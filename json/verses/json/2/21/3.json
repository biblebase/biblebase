    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若买他的时候，他是单身，那么期满的时候，他可以单身离开。如果他跟妻子一同被买，就可以带妻子一起离开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他若是單身來，就可以單身出去；他若是有婦之夫，他的妻子就可以和他同去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他若孤身來就可以孤身去；他若有妻，他的妻就可以同他出去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If he comes alone, he shall go out alone; if he is the husband of a wife, then his wife shall go out with him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If he comes alone, he is to go free alone; but if he has a wife when he comes, she is to go with him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If he was single when he became your slave, he shall leave single. But if he was married before he became a slave, then his wife must be freed with him."
      }
    },
