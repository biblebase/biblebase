    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但牛皮、肉和粪要拿到营外焚烧。这是赎罪祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於公牛的肉、皮和糞，你都要在營外用火焚化。因為這是贖罪祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是公牛的皮、肉、糞都要用火燒在營外。這牛是贖罪祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the flesh of the bull and its hide and its refuse, you shall burn with fire outside the camp; it is a sin offering."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But burn the bull’s flesh and its hide and its intestines outside the camp. It is a sin offering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then take the rest of the bull, including its hide, meat, and dung, and burn it outside the camp as a sin offering."
      }
    },
