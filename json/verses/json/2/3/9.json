    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在，以色列人的呼求传到我耳中，我也看到了埃及人怎样压迫他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在以色列人的呼聲已經達到我的面前，我也看見了埃及人對他們所施的壓迫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在以色列人的哀聲達到我耳中，我也看見埃及人怎樣欺壓他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now, behold, the cry of the sons of Israel has come to Me; furthermore, I have seen the oppression with which the Egyptians are oppressing them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And now the cry of the Israelites has reached me, and I have seen the way the Egyptians are oppressing them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Look! The cry of the people of Israel has reached me, and I have seen how harshly the Egyptians abuse them."
      }
    },
