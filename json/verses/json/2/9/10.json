    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西和亚伦便取了一些窑里的灰，站在法老面前，摩西把灰抛向空中，埃及人和牲畜身上就开始起疱长疮。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西和亞倫取了爐灰，站在法老面前；摩西把爐灰向天上揚起來，人和牲畜身上就生出了疹病和膿瘡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西、亞倫取了爐灰，站在法老面前。摩西向天揚起來，就在人身上和牲畜身上成了起泡的瘡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they took soot from a kiln, and stood before Pharaoh; and Moses threw it toward the sky, and it became boils breaking out with sores on man and beast."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So they took soot from a furnace and stood before Pharaoh. Moses tossed it into the air, and festering boils broke out on people and animals."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So they took soot from a brick kiln and went and stood before Pharaoh. As Pharaoh watched, Moses threw the soot into the air, and boils broke out on people and animals alike."
      }
    },
