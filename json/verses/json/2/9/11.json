      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "埃及的巫师无法与摩西对抗，因为他们及所有埃及人身上都长了脓疮。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾術士因為生了瘡的緣故，不能站在摩西的面前；因為眾術士和所有埃及人身上都生了瘡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行法術的在摩西面前站立不住，因為在他們身上和一切埃及人身上都有這瘡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The magicians could not stand before Moses because of the boils, for the boils were on the magicians as well as on all the Egyptians."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The magicians could not stand before Moses because of the boils that were on them and on all the Egyptians."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even the magicians were unable to stand before Moses, because the boils had broken out on them and all the Egyptians."
      }
    },
