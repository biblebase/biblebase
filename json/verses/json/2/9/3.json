      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我就会使你田间所有的牲畜，包括马、驴、骆驼和牛羊，都染上严重的瘟疫。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪，耶和華的手必用嚴重的瘟疫加在你田間的牲畜身上，加在馬、驢、駱駝、牛群、羊群身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的手加在你田間的牲畜上，就是在馬、驢、駱駝、牛羣、羊羣上，必有重重的瘟疫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "behold, the hand of the Lord will come with a very severe pestilence on your livestock which are in the field, on the horses, on the donkeys, on the camels, on the herds, and on the flocks."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the hand of the Lord will bring a terrible plague on your livestock in the field—on your horses, donkeys and camels and on your cattle, sheep and goats."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "the hand of the Lord will strike all your livestock—your horses, donkeys, camels, cattle, sheep, and goats—with a deadly plague."
      }
