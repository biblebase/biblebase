    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西离开法老，走出城外向耶和华举手祷告，雷和冰雹便停止了，豪雨也不下了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西離開法老出城去了，就向耶和華伸手禱告，雷聲和冰雹就止住了，雨也不再傾倒在地上了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西離了法老出城，向耶和華舉手禱告；雷和雹就止住，雨也不再澆在地上了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Moses went out of the city from Pharaoh, and spread out his hands to the Lord; and the thunder and the hail ceased, and rain no longer poured on the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Moses left Pharaoh and went out of the city. He spread out his hands toward the Lord; the thunder and hail stopped, and the rain no longer poured down on the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Moses left Pharaoh’s court and went out of the city. When he lifted his hands to the Lord, the thunder and hail stopped, and the downpour ceased."
      }
