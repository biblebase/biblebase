      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“竊賊挖窟窿入屋的時候，如果被人發現，把他打了，以致打死，打死人的就沒有流人血的罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人若遇見賊挖窟窿，把賊打了，以至於死，就不能為他有流血的罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If the thief is caught while breaking in and is struck so that he dies, there will be no bloodguiltiness on his account."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If a thief is caught breaking in at night and is struck a fatal blow, the defender is not guilty of bloodshed;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If a thief is caught in the act of breaking into a house and is struck and killed in the process, the person who killed the thief is not guilty of murder."
