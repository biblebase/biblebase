    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要向我献上五谷和新酒，不可迟疑耽延。要把你们的长子献给我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要把你豐收的五穀和初榨的新酒獻上，不可遲延；要把你頭生的兒子獻給我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你要從你莊稼中的穀和酒醡中滴出來的酒拿來獻上，不可遲延。 「你要將頭生的兒子歸給我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall not delay the offering from your harvest and your vintage. The firstborn of your sons you shall give to Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do not hold back offerings from your granaries or your vats. “You must give me the firstborn of your sons."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must not hold anything back when you give me offerings from your crops and your wine. “You must give me your firstborn sons."
      }
    },
