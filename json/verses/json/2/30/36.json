    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要把一些香捣成极细的粉末，放在会幕内的约柜前面，就是我与你会面的地方，你们要视这香为至圣之物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要取一部分香，把它研成碎末，放在會幕裡法櫃前，就是我要與你相會的地方。你們要以這香為至聖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這香要取點搗得極細，放在會幕內、法櫃前，我要在那裏與你相會。你們要以這香為至聖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall beat some of it very fine, and put part of it before the testimony in the tent of meeting where I will meet with you; it shall be most holy to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Grind some of it to powder and place it in front of the ark of the covenant law in the tent of meeting, where I will meet with you. It shall be most holy to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Grind some of the mixture into a very fine powder and put it in front of the Ark of the Covenant, where I will meet with you in the Tabernacle. You must treat this incense as most holy."
      }
    },
