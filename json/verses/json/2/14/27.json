    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天刚亮的时候，摩西向海伸出手，海水便复原。埃及人纷纷向岸上逃命，耶和华却使大水把他们卷回海中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西就向海伸手，到了天亮的時候，海水流回原處；埃及人逆流逃跑的時候，耶和華就把他們投在海中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西就向海伸杖，到了天一亮，海水仍舊復原。埃及人避水逃跑的時候，耶和華把他們推翻在海中，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Moses stretched out his hand over the sea, and the sea returned to its normal state at daybreak, while the Egyptians were fleeing right into it; then the Lord overthrew the Egyptians in the midst of the sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moses stretched out his hand over the sea, and at daybreak the sea went back to its place. The Egyptians were fleeing toward it, and the Lord swept them into the sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So as the sun began to rise, Moses raised his hand over the sea, and the water rushed back into its usual place. The Egyptians tried to escape, but the Lord swept them into the sea."
      }
    },
