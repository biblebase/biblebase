    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到了破晓时分，耶和华从云柱火柱中俯视埃及军队，扰乱他们的队伍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了晨更的時候，耶和華透過雲柱火柱向下觀看埃及人的軍兵，並且使埃及人的軍兵大起混亂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了晨更的時候，耶和華從雲火柱中向埃及的軍兵觀看，使埃及的軍兵混亂了；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "At the morning watch, the Lord looked down on the army of the Egyptians through the pillar of fire and cloud and brought the army of the Egyptians into confusion."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "During the last watch of the night the Lord looked down from the pillar of fire and cloud at the Egyptian army and threw it into confusion."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But just before dawn the Lord looked down on the Egyptian army from the pillar of fire and cloud, and he threw their forces into total confusion."
      }
    },
