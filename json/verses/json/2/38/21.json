    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以上是安放约柜的圣幕中所用的物品，是照摩西的吩咐，由亚伦祭司的儿子以他玛指挥利未人清点的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這是帳幕內利未人使用的物件的總數，是照著摩西的吩咐，經亞倫祭司的兒子以他瑪的手計算的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這是法櫃的帳幕中利未人所用物件的總數，是照摩西的吩咐，經祭司亞倫的兒子以他瑪的手數點的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is the number of the things for the tabernacle, the tabernacle of the testimony, as they were numbered according to the command of Moses, for the service of the Levites, by the hand of Ithamar the son of Aaron the priest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These are the amounts of the materials used for the tabernacle, the tabernacle of the covenant law, which were recorded at Moses’ command by the Levites under the direction of Ithamar son of Aaron, the priest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is an inventory of the materials used in building the Tabernacle of the Covenant. The Levites compiled the figures, as Moses directed, and Ithamar son of Aaron the priest served as recorder."
      }
    },
