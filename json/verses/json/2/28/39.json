      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要用細麻線編織雜色內袍，用細麻布做禮冠，又用刺繡的手工做腰帶。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要用雜色細麻線織內袍，用細麻布做冠冕，又用繡花的手工做腰帶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall weave the tunic of checkered work of fine linen, and shall make a turban of fine linen, and you shall make a sash, the work of a weaver."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Weave the tunic of fine linen and make the turban of fine linen. The sash is to be the work of an embroiderer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Weave Aaron’s patterned tunic from fine linen cloth. Fashion the turban from this linen as well. Also make a sash, and decorate it with colorful embroidery."
      }
