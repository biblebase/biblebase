    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可叩拜他们的神明，不可供奉它们，也不可效法他们的行为，要彻底拆毁神像，打碎他们的神柱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不可跪拜他們的神像，也不可事奉它們，不可作他們所作的，不可模仿他們的行為，卻要把神像完全拆毀，打碎他們的柱像。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你不可跪拜他們的神，不可事奉他，也不可效法他們的行為，卻要把神像盡行拆毀，打碎他們的柱像。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not worship their gods, nor serve them, nor do according to their deeds; but you shall utterly overthrow them and break their sacred pillars in pieces."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not bow down before their gods or worship them or follow their practices. You must demolish them and break their sacred stones to pieces."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must not worship the gods of these nations or serve them in any way or imitate their evil practices. Instead, you must utterly destroy them and smash their sacred pillars."
      }
    },
