    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要事奉你们的上帝耶和华，我必赐给你日用的饮食，使你们身体健康，不受疾病的侵害，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要事奉耶和華你們的　神，他就必賜福你的糧和你的水，我也必從你們中間除去疾病。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要事奉耶和華－你們的神，他必賜福與你的糧與你的水，也必從你們中間除去疾病。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But you shall serve the Lord your God, and He will bless your bread and your water; and I will remove sickness from your midst."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Worship the Lord your God, and his blessing will be on your food and water. I will take away sickness from among you,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must serve only the Lord your God. If you do, I will bless you with food and water, and I will protect you from illness."
      }
    },
