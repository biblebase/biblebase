    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要派黄蜂做你的先锋，赶走那些希未人、迦南人和赫人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必在你面前打發大黃蜂，把希未人、迦南人、赫人，都從你面前驅逐出去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要打發黃蜂飛在你前面，把希未人、迦南人、赫人攆出去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will send hornets ahead of you so that they will drive out the Hivites, the Canaanites, and the Hittites before you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will send the hornet ahead of you to drive the Hivites, Canaanites and Hittites out of your way."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will send terror ahead of you to drive out the Hivites, Canaanites, and Hittites."
      }
    },
