    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "埃及人看见我伸手攻击他们、把以色列人带出埃及，就会知道我是耶和华。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我向埃及伸手，把以色列人從他們中間領出來的時候，埃及人就必知道我是耶和華。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我伸手攻擊埃及，將以色列人從他們中間領出來的時候，埃及人就要知道我是耶和華。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Egyptians shall know that I am the Lord, when I stretch out My hand on Egypt and bring out the sons of Israel from their midst.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Egyptians will know that I am the Lord when I stretch out my hand against Egypt and bring the Israelites out of it.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When I raise my powerful hand and bring out the Israelites, the Egyptians will know that I am the Lord.”"
      }
    },
