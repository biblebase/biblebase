      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要记住这一天，守为耶和华的节期，作为世世代代永远的定例。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這一日必作你們的記念日；你們要守這日為耶和華的節；你們要世世代代守這節日作永遠的定例。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們要記念這日，守為耶和華的節，作為你們世世代代永遠的定例。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Now this day will be a memorial to you, and you shall celebrate it as a feast to the Lord; throughout your generations you are to celebrate it as a permanent ordinance."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is a day you are to commemorate; for the generations to come you shall celebrate it as a festival to the Lord—a lasting ordinance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is a day to remember. Each year, from generation to generation, you must celebrate it as a special festival to the Lord. This is a law for all time."
      }
