    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他的妻子西坡拉拿起锋利的火石，割下儿子的包皮，放在摩西脚前，说：“你真是我的血郎。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "西坡拉就拿了一把石刀，把她兒子的包皮割下來，把它丟到摩西的腳上，說：“你真是我的血郎了！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "西坡拉就拿一塊火石，割下他兒子的陽皮，丟在摩西腳前，說：「你真是我的血郎了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Zipporah took a flint and cut off her son’s foreskin and threw it at Moses’ feet, and she said, “You are indeed a bridegroom of blood to me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Zipporah took a flint knife, cut off her son’s foreskin and touched Moses’ feet with it. “Surely you are a bridegroom of blood to me,” she said."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Moses’ wife, Zipporah, took a flint knife and circumcised her son. She touched his feet with the foreskin and said, “Now you are a bridegroom of blood to me.”"
      }
    },
