    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对亚伦说：“你到旷野去迎接摩西。”他就在上帝的山上遇见摩西，并亲吻他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對亞倫說：“你到曠野去迎接摩西。”他就去了；他在　神的山遇見了摩西，就和他親吻。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對亞倫說：「你往曠野去迎接摩西。」他就去，在神的山遇見摩西，和他親嘴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the Lord said to Aaron, “Go to meet Moses in the wilderness.” So he went and met him at the mountain of God and kissed him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said to Aaron, “Go into the wilderness to meet Moses.” So he met Moses at the mountain of God and kissed him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now the Lord had said to Aaron, “Go out into the wilderness to meet Moses.” So Aaron went and met Moses at the mountain of God, and he embraced him."
      }
    },
