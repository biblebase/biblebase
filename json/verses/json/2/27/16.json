    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "院子的入口要有门帘，长九米，用细麻线和蓝色、紫色、朱红色的线织成，上面有精致的刺绣，还有四根柱子和四个带凹槽的底座。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "院子的大門要有門簾，長九公尺，是用藍色紫色朱紅色線和撚的細麻，用刺繡的手工織成，柱子四根，插座四個。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "院子的門當有簾子，長二十肘，要拿藍色、紫色、朱紅色線，和撚的細麻，用繡花的手工織成，柱子四根，帶卯的座四個。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the gate of the court there shall be a screen of twenty cubits, of blue and purple and scarlet material and fine twisted linen, the work of a weaver, with their four pillars and their four sockets."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“For the entrance to the courtyard, provide a curtain twenty cubits long, of blue, purple and scarlet yarn and finely twisted linen—the work of an embroiderer—with four posts and four bases."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“For the entrance to the courtyard, make a curtain that is 30 feet long. Make it from finely woven linen, and decorate it with beautiful embroidery in blue, purple, and scarlet thread. Support it with four posts, each securely set in its own base."
      }
    },
