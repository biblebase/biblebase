    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“要为圣幕围出一个院子。院子的南面要用细麻线做帷幔，长四十六米，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要做會幕的院子。在南面，即向南的一面，要用撚的細麻為院子做帷幔，每邊長四十四公尺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你要做帳幕的院子。院子的南面要用撚的細麻做帷子，長一百肘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall make the court of the tabernacle. On the south side there shall be hangings for the court of fine twisted linen one hundred cubits long for one side;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Make a courtyard for the tabernacle. The south side shall be a hundred cubits long and is to have curtains of finely twisted linen,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then make the courtyard for the Tabernacle, enclosed with curtains made of finely woven linen. On the south side, make the curtains 150 feet long."
      }
    },
