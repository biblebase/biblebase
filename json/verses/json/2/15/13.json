    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你以不变的爱引领你所救赎的子民， 以大能带他们进入你的圣所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你憑著慈愛，帶領了你買贖的人民；你憑著能力，引導了他們到你的聖所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你憑慈愛領了你所贖的百姓； 你憑能力引他們到了你的聖所。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In Your lovingkindness You have led the people whom You have redeemed; In Your strength You have guided them to Your holy habitation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In your unfailing love you will lead the people you have redeemed. In your strength you will guide them to your holy dwelling."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“With your unfailing love you lead the people you have redeemed. In your might, you guide them to your sacred home."
      }
    },
