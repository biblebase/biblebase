    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦的姐姐米利暗是个先知，她手拿小鼓，带领妇女击鼓跳舞，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫的姊姊女先知米利暗，手裡拿著鼓；所有的婦女都跟著她出來，擊鼓跳舞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫的姊姊，女先知米利暗，手裏拿着鼓；眾婦女也跟她出去拿鼓跳舞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Miriam the prophetess, Aaron’s sister, took the timbrel in her hand, and all the women went out after her with timbrels and with dancing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Miriam the prophet, Aaron’s sister, took a timbrel in her hand, and all the women followed her, with timbrels and dancing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Miriam the prophet, Aaron’s sister, took a tambourine and led all the women as they played their tambourines and danced."
      }
