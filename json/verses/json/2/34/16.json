    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "给儿子娶他们的女儿为妻，以致她们与自己的神明苟合时诱使你的儿子犯同样的罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又恐怕你給你的兒子娶他們的女兒為妻，他們的女兒隨從自己的神行邪淫的時候，使你的兒子也隨從她們的神行邪淫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又為你的兒子娶他們的女兒為妻，他們的女兒隨從她們的神，就行邪淫，使你的兒子也隨從她們的神行邪淫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and you might take some of his daughters for your sons, and his daughters might play the harlot with their gods and cause your sons also to play the harlot with their gods."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And when you choose some of their daughters as wives for your sons and those daughters prostitute themselves to their gods, they will lead your sons to do the same."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then you will accept their daughters, who sacrifice to other gods, as wives for your sons. And they will seduce your sons to commit adultery against me by worshiping other gods."
      }
    },
