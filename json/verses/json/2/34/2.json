      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要预备好，明天清早你要上西奈山顶来见我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "早晨之前，你要預備好了；到早晨的時候，你要上到西奈山來，在山頂上站在我面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "明日早晨，你要預備好了，上西奈山，在山頂上站在我面前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So be ready by morning, and come up in the morning to Mount Sinai, and present yourself there to Me on the top of the mountain."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be ready in the morning, and then come up on Mount Sinai. Present yourself to me there on top of the mountain."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Be ready in the morning to climb up Mount Sinai and present yourself to me on the top of the mountain."
      }
