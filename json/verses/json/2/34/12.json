    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要谨慎，到了目的地后，不可与当地的居民立约，免得因此而陷入网罗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要小心，不可與你所去的那地的居民立約，恐怕這事在你中間成為陷阱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要謹慎，不可與你所去那地的居民立約，恐怕成為你們中間的網羅；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Watch yourself that you make no covenant with the inhabitants of the land into which you are going, or it will become a snare in your midst."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be careful not to make a treaty with those who live in the land where you are going, or they will be a snare among you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Be very careful never to make a treaty with the people who live in the land where you are going. If you do, you will follow their evil ways and be trapped."
      }
    },
