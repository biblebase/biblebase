    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“不可与当地的居民立约，免得他们与自己的神明苟合时邀请你，你就吃他们的祭物，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "恐怕你與那地的居民立約，他們隨從自己的神行邪淫，給自己的神獻祭的時候，有人叫你，你就吃他的祭物；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只怕你與那地的居民立約，百姓隨從他們的神，就行邪淫，祭祀他們的神，有人叫你，你便吃他的祭物，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "otherwise you might make a covenant with the inhabitants of the land and they would play the harlot with their gods and sacrifice to their gods, and someone might invite you to eat of his sacrifice,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Be careful not to make a treaty with those who live in the land; for when they prostitute themselves to their gods and sacrifice to them, they will invite you and you will eat their sacrifices."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must not make a treaty of any kind with the people living in the land. They lust after their gods, offering sacrifices to them. They will invite you to join them in their sacrificial meals, and you will go with them."
      }
    },
