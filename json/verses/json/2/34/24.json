    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要从你面前赶走外族人，扩张你的领土。你一年三次来朝见你的上帝耶和华时，必没有人贪恋你的土地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要把列國從你面前趕出去，擴張你的境界；你一年三次上去面見耶和華你的　神的時候，必沒有人貪圖你的地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要從你面前趕出外邦人，擴張你的境界。你一年三次上去朝見耶和華－你神的時候，必沒有人貪慕你的地土。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I will drive out nations before you and enlarge your borders, and no man shall covet your land when you go up three times a year to appear before the Lord your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will drive out nations before you and enlarge your territory, and no one will covet your land when you go up three times each year to appear before the Lord your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will drive out the other nations ahead of you and expand your territory, so no one will covet and conquer your land while you appear before the Lord your God three times each year."
      }
    },
