    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他选了两块红玛瑙，用刻图章的方法刻上以色列十二个儿子的名字，镶在两个金框里，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們預備了兩塊紅瑪瑙寶石，鑲嵌在金槽裡，又用刻圖章的方法刻上以色列眾子的名字，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又琢出兩塊紅瑪瑙，鑲在金槽上，彷彿刻圖書，按着以色列兒子的名字雕刻；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They made the onyx stones, set in gold filigree settings; they were engraved like the engravings of a signet, according to the names of the sons of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They mounted the onyx stones in gold filigree settings and engraved them like a seal with the names of the sons of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They mounted the two onyx stones in settings of gold filigree. The stones were engraved with the names of the tribes of Israel, just as a seal is engraved."
      }
