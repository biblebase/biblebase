    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我赞赏你们，因为你们凡事都想到我，并坚守我传给你们的各种教导。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我稱讚你們，因為你們在一切事上都記念我，又持守我傳交給你們的教訓。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我稱讚你們，因你們凡事記念我，又堅守我所傳給你們的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now I praise you because you remember me in everything and hold firmly to the traditions, just as I delivered them to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I praise you for remembering me in everything and for holding to the traditions just as I passed them on to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am so glad that you always keep me in your thoughts, and that you are following the teachings I passed on to you."
      }
    },
