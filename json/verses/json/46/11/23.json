    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我把从主领受的传给了你们，就是：主耶稣被出卖的那天晚上，祂拿起一个饼来，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我當日傳交給你們的，原是從主領受的，就是主耶穌被出賣的那一夜，他拿起餅來，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我當日傳給你們的，原是從主領受的，就是主耶穌被賣的那一夜，拿起餅來，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I received from the Lord that which I also delivered to you, that the Lord Jesus in the night in which He was betrayed took bread;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I received from the Lord what I also passed on to you: The Lord Jesus, on the night he was betrayed, took bread,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I pass on to you what I received from the Lord himself. On the night when he was betrayed, the Lord Jesus took some bread"
      }
    },
