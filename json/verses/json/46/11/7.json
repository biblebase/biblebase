    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然而，男人不该蒙头，因为男人是上帝的形象和荣耀，而女人是男人的荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "男人不應蒙著頭，因為他是　神的形象和榮耀，而女人是男人的榮耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "男人本不該蒙着頭，因為他是神的形像和榮耀；但女人是男人的榮耀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For a man ought not to have his head covered, since he is the image and glory of God; but the woman is the glory of man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A man ought not to cover his head, since he is the image and glory of God; but woman is the glory of man."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A man should not wear anything on his head when worshiping, for man is made in God’s image and reflects God’s glory. And woman reflects man’s glory."
      }
    },
