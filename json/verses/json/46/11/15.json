    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "女人留长发是她的荣耀吗？因为头发是用来给女人盖头的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果女人有長頭髮，不就是她的榮耀嗎？因為頭髮是給她作蓋頭的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但女人有長頭髮，乃是她的榮耀，因為這頭髮是給她作蓋頭的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but if a woman has long hair, it is a glory to her? For her hair is given to her for a covering."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but that if a woman has long hair, it is her glory? For long hair is given to her as a covering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And isn’t long hair a woman’s pride and joy? For it has been given to her as a covering."
      }
    },
