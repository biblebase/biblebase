    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要把这人交给撒旦去毁坏他的肉体，好使这人的灵魂在主耶稣再来的日子可以得救。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要把這樣的人交給撒但，敗壞他的身體，使他的靈魂在主的日子可以得救。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要把這樣的人交給撒但，敗壞他的肉體，使他的靈魂在主耶穌的日子可以得救。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have decided to deliver such a one to Satan for the destruction of his flesh, so that his spirit may be saved in the day of the Lord Jesus."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "hand this man over to Satan for the destruction of the flesh, so that his spirit may be saved on the day of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then you must throw this man out and hand him over to Satan so that his sinful nature will be destroyed and he himself will be saved on the day the Lord returns."
      }
    },
