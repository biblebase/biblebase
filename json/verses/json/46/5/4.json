    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们奉我们主耶稣的名聚会时，我的心并我们主耶稣的权能也与你们同在。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就是當你們奉我們主耶穌的名聚集在一起，我的靈在那裡，我們主耶穌的權能也同在的時候，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是你們聚會的時候，我的心也同在。奉我們主耶穌的名，並用我們主耶穌的權能，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In the name of our Lord Jesus, when you are assembled, and I with you in spirit, with the power of our Lord Jesus,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So when you are assembled and I am with you in spirit, and the power of our Lord Jesus is present,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "in the name of the Lord Jesus. You must call a meeting of the church. I will be present with you in spirit, and so will the power of our Lord Jesus."
      }
    },
