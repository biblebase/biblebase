    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们所讲的是上帝隐藏在奥秘中的智慧，是祂为了使我们得荣耀而在万世以前定好的智慧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們所講的，是從前隱藏的、　神奧祕的智慧，就是　神在萬世以前，為我們的榮耀所預定的；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們講的，乃是從前所隱藏、神奧祕的智慧，就是神在萬世以前預定使我們得榮耀的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but we speak God’s wisdom in a mystery, the hidden wisdom which God predestined before the ages to our glory;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No, we declare God’s wisdom, a mystery that has been hidden and that God destined for our glory before time began."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No, the wisdom we speak of is the mystery of God—his plan that was previously hidden, even though he made it for our ultimate glory before the world began."
      }
