    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，我当初到你们那里传扬上帝的奥秘，并没有用高深的言论或哲理。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，我從前到你們那裡去，並沒有用高言大智向你們傳講　神的奧祕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，從前我到你們那裏去，並沒有用高言大智對你們宣傳神的奧祕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And when I came to you, brethren, I did not come with superiority of speech or of wisdom, proclaiming to you the testimony of God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And so it was with me, brothers and sisters. When I came to you, I did not come with eloquence or human wisdom as I proclaimed to you the testimony about God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When I first came to you, dear brothers and sisters, I didn’t use lofty words and impressive wisdom to tell you God’s secret plan."
      }
    },
