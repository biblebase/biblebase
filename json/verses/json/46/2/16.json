    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“谁曾知道主的心意， 能够指教祂呢？” 但我们明白基督的心意。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如經上所記： “誰曾知道主的心意， 能夠指教他呢？” 但我們已經得著基督的心意了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誰曾知道主的心去教導他呢？但我們是有基督的心了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For who has known the mind of the Lord, that he will instruct Him? But we have the mind of Christ."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for, “Who has known the mind of the Lord so as to instruct him?” But we have the mind of Christ."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For, “Who can know the Lord’s thoughts? Who knows enough to teach him?” But we understand these things, for we have the mind of Christ."
      }
    },
