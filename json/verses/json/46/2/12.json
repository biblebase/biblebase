    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们接受的不是这世界的灵，而是上帝的圣灵，使我们可以领会上帝开恩启示给我们的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們所領受的，不是這世界的靈，而是從　神來的靈，使我們能知道　神開恩賜給我們的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們所領受的，並不是世上的靈，乃是從神來的靈，叫我們能知道神開恩賜給我們的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now we have received, not the spirit of the world, but the Spirit who is from God, so that we may know the things freely given to us by God,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What we have received is not the spirit of the world, but the Spirit who is from God, so that we may understand what God has freely given us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And we have received God’s Spirit (not the world’s spirit), so we can know the wonderful things God has freely given us."
      }
    },
