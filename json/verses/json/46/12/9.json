    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "圣灵赐这人信心，赐那人医病的恩赐；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又有人因著同一位聖靈領受了信心，還有人因著這位聖靈領受了醫病的恩賜，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又有一人蒙這位聖靈賜他信心，還有一人蒙這位聖靈賜他醫病的恩賜，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "to another faith by the same Spirit, and to another gifts of healing by the one Spirit,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "to another faith by the same Spirit, to another gifts of healing by that one Spirit,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The same Spirit gives great faith to another, and to someone else the one Spirit gives the gift of healing."
      }
    },
