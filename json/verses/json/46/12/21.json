    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "眼睛不能对手说：“我不需要你。”头也不能对脚说：“我不需要你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眼睛不能對手說：“我不需要你。”頭也不能對腳說：“我不需要你們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眼不能對手說：「我用不着你。」頭也不能對腳說：「我用不着你。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the eye cannot say to the hand, “I have no need of you”; or again the head to the feet, “I have no need of you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The eye cannot say to the hand, “I don’t need you!” And the head cannot say to the feet, “I don’t need you!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The eye can never say to the hand, “I don’t need you.” The head can’t say to the feet, “I don’t need you.”"
      }
    },
