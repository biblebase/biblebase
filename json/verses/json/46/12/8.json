      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這人蒙聖靈賜他智慧的言語，那人也蒙這位聖靈賜他知識的言語，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For to one is given the word of wisdom through the Spirit, and to another the word of knowledge according to the same Spirit;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To one there is given through the Spirit a message of wisdom, to another a message of knowledge by means of the same Spirit,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "To one person the Spirit gives the ability to give wise advice; to another the same Spirit gives a message of special knowledge."
      }
    },
