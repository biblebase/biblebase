    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们应该热心追求更大的恩赐。 现在，我要将一条奇妙无比的道路指示你们！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們卻要熱切地追求那些更大的恩賜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要切切地求那更大的恩賜。 我現今把最妙的道指示你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But earnestly desire the greater gifts. And I show you a still more excellent way."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now eagerly desire the greater gifts. And yet I will show you the most excellent way."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So you should earnestly desire the most helpful gifts. But now let me show you a way of life that is best of all."
      }
    },
