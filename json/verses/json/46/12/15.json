    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果脚说：“我不是手，所以我不属于身体。”难道脚就因此不属于身体吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "假如腳說：“我不是手，所以我不屬於身體。”它不能因此就不屬於身體。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "設若腳說：「我不是手，所以不屬乎身子，」它不能因此就不屬乎身子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If the foot says, “Because I am not a hand, I am not a part of the body,” it is not for this reason any the less a part of the body."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now if the foot should say, “Because I am not a hand, I do not belong to the body,” it would not for that reason stop being part of the body."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If the foot says, “I am not a part of the body because I am not a hand,” that does not make it any less a part of the body."
      }
    },
