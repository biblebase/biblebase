    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "各人应当依照上帝的呼召和安排生活，这是我对各教会的吩咐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不過，主怎樣分給各人，　神怎樣呼召各人，各人就要照著去行事為人。我也這樣吩咐各教會。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只要照主所分給各人的，和神所召各人的而行。我吩咐各教會都是這樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Only, as the Lord has assigned to each one, as God has called each, in this manner let him walk. And so I direct in all the churches."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nevertheless, each person should live as a believer in whatever situation the Lord has assigned to them, just as God has called them. This is the rule I lay down in all the churches."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each of you should continue to live in whatever situation the Lord has placed you, and remain as you were when God first called you. This is my rule for all the churches."
      }
    },
