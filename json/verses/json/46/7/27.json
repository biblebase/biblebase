    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "已经有妻子的，就不要设法摆脱她；还没有妻子的，就不要想着结婚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你已經有了妻子嗎？就不要想擺脫。你還沒有妻子嗎？就不要去找妻子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你有妻子纏着呢，就不要求脫離；你沒有妻子纏着呢，就不要求妻子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Are you bound to a wife? Do not seek to be released. Are you released from a wife? Do not seek a wife."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Are you pledged to a woman? Do not seek to be released. Are you free from such a commitment? Do not look for a wife."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you have a wife, do not seek to end the marriage. If you do not have a wife, do not seek to get married."
      }
    },
