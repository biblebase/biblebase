    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "关于独身的问题，主并没有给我任何命令，但我既然深受主恩，成为祂忠心的仆人，就向你们提供一些意见。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "關於未婚的，我沒有主的命令，但我既然蒙了主的憐憫，成為可信靠的人，就把我的意見提出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論到童身的人，我沒有主的命令，但我既蒙主憐恤能作忠心的人，就把自己的意見告訴你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now concerning virgins I have no command of the Lord, but I give an opinion as one who by the mercy of the Lord is trustworthy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now about virgins: I have no command from the Lord, but I give a judgment as one who by the Lord’s mercy is trustworthy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now regarding your question about the young women who are not yet married. I do not have a command from the Lord for them. But the Lord in his mercy has given me wisdom that can be trusted, and I will share it with you."
      }
    },
