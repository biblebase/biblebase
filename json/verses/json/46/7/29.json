    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄们，我告诉你们，时日不多了，从今以后，那些有妻子的，要像没有妻子的；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，我是說時候不多了。從今以後，有妻子的要像沒有妻子的，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，我對你們說：時候減少了。從此以後，那有妻子的，要像沒有妻子；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But this I say, brethren, the time has been shortened, so that from now on those who have wives should be as though they had none;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What I mean, brothers and sisters, is that the time is short. From now on those who have wives should live as if they do not;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But let me say this, dear brothers and sisters: The time that remains is very short. So from now on, those with wives should not focus only on their marriage."
      }
    },
