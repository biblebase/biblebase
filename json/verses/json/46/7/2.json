    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不过，为了避免发生淫乱的事，男婚女嫁也合情合理。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但為了避免淫亂的事，男人應當各有自己的妻子，女人也應當各有自己的丈夫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但要免淫亂的事，男子當各有自己的妻子；女子也當各有自己的丈夫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But because of immoralities, each man is to have his own wife, and each woman is to have her own husband."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But since sexual immorality is occurring, each man should have sexual relations with his own wife, and each woman with her own husband."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But because there is so much sexual immorality, each man should have his own wife, and each woman should have her own husband."
      }
    },
