    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我希望你们无牵无挂。未婚的男子可以专心于主的事，想着怎样讨主的喜悦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我願你們無所掛慮。沒有娶妻子的人，掛念的是主的事，想怎樣去得主喜悅；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我願你們無所掛慮。沒有娶妻的，是為主的事掛慮，想怎樣叫主喜悅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I want you to be free from concern. One who is unmarried is concerned about the things of the Lord, how he may please the Lord;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I would like you to be free from concern. An unmarried man is concerned about the Lord’s affairs—how he can please the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I want you to be free from the concerns of this life. An unmarried man can spend his time doing the Lord’s work and thinking how to please him."
      }
    },
