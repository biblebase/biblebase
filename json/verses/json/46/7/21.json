    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你蒙召时是奴隶，不必因此而烦恼。不过如果你可以获得自由，也不要放过机会。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你蒙召的時候是作奴僕的嗎？不要為此煩惱。但如果你能夠得到自由，就要把握這機會。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你是作奴隸蒙召的嗎？不要因此憂慮；若能以自由，就求自由更好。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Were you called while a slave? Do not worry about it; but if you are able also to become free, rather do that."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Were you a slave when you were called? Don’t let it trouble you—although if you can gain your freedom, do so."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Are you a slave? Don’t let that worry you—but if you get a chance to be free, take it."
      }
