    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若是离开了，妻子不可再嫁别人，只能与丈夫复合。丈夫也不可离弃妻子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果離開了，就不可再嫁，不然，就要跟丈夫復合。丈夫也不可離棄妻子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若是離開了，不可再嫁，或是仍同丈夫和好。丈夫也不可離棄妻子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "(but if she does leave, she must remain unmarried, or else be reconciled to her husband), and that the husband should not divorce his wife."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if she does, she must remain unmarried or else be reconciled to her husband. And a husband must not divorce his wife."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if she does leave him, let her remain single or else be reconciled to him. And the husband must not leave his wife."
      }
    },
