    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "关于你们信上所写的事，我认为男人不亲近女人是好的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "關於你們信上所提的事:“男人不親近女人倒好。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論到你們信上所提的事，我說男不近女倒好。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now concerning the things about which you wrote, it is good for a man not to touch a woman."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now for the matters you wrote about: “It is good for a man not to have sexual relations with a woman.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now regarding the questions you asked in your letter. Yes, it is good to abstain from sexual relations."
      }
