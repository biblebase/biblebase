    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，你们要在上帝面前保持自己蒙召时的身份。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，你們各人蒙召的時候怎樣，就應當在　神面前保持這原來的情況。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，你們各人蒙召的時候是甚麼身分，仍要在神面前守住這身分。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Brethren, each one is to remain with God in that condition in which he was called."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Brothers and sisters, each person, as responsible to God, should remain in the situation they were in when God called them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each of you, dear brothers and sisters, should remain as you were when God first called you."
      }
    },
