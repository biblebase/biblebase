    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果蒙召时已经受了割礼，不必消除割礼；如果蒙召时没有受割礼，也不必去受割礼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有人受了割禮而蒙召的嗎？他就不要遮掩割禮的記號。有人未受割禮而蒙召的嗎？他就不要受割禮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人已受割禮蒙召呢，就不要廢割禮；有人未受割禮蒙召呢，就不要受割禮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Was any man called when he was already circumcised? He is not to become uncircumcised. Has anyone been called in uncircumcision? He is not to be circumcised."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Was a man already circumcised when he was called? He should not become uncircumcised. Was a man uncircumcised when he was called? He should not be circumcised."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For instance, a man who was circumcised before he became a believer should not try to reverse it. And the man who was uncircumcised when he became a believer should not be circumcised now."
      }
    },
