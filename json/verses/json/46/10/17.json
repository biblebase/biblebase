    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们人数虽多，却同属一个身体，因为我们同享一个饼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為事實上只有一個餅，我們人數雖多，還是一個身體，因為我們都是分享同一個餅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們雖多，仍是一個餅，一個身體，因為我們都是分受這一個餅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Since there is one bread, we who are many are one body; for we all partake of the one bread."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because there is one loaf, we, who are many, are one body, for we all share the one loaf."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And though we are many, we all eat from one loaf of bread, showing that we are one body."
      }
    },
