    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们想惹主嫉妒吗？难道我们比祂更有能力吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "難道我們要激起主的忿怒嗎？我們比他更強嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們可惹主的憤恨嗎？我們比他還有能力嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Or do we provoke the Lord to jealousy? We are not stronger than He, are we?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Are we trying to arouse the Lord’s jealousy? Are we stronger than he?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What? Do we dare to rouse the Lord’s jealousy? Do you think we are stronger than he is?"
      }
    },
