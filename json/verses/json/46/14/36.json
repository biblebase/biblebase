    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道上帝的道是出自你们哥林多人吗？难道上帝的道单单传给了你们吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "難道　神的道是從你們出來的嗎？是單單臨到你們的嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神的道理豈是從你們出來嗎？豈是單臨到你們嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Was it from you that the word of God first went forth? Or has it come to you only?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Or did the word of God originate with you? Or are you the only people it has reached?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Or do you think God’s word originated with you Corinthians? Are you the only ones to whom it was given?"
      }
