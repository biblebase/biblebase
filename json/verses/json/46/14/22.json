    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "由此可见，讲方言不是显给信徒的标记，而是显给非信徒的标记；但先知讲道是显给信徒的标记，不是显给非信徒的标记。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可見說方言不是要給信主的人作記號，而是要給未信的人；講道不是要給未信的人，而是要給信主的人作記號。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣看來，說方言不是為信的人作證據，乃是為不信的人；作先知講道不是為不信的人作證據，乃是為信的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So then tongues are for a sign, not to those who believe but to unbelievers; but prophecy is for a sign, not to unbelievers but to those who believe."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Tongues, then, are a sign, not for believers but for unbelievers; prophecy, however, is not for unbelievers but for believers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So you see that speaking in tongues is a sign, not for believers, but for unbelievers. Prophecy, however, is for the benefit of believers, not unbelievers."
      }
    },
