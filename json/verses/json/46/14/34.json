    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "正如圣徒的各教会一样，妇女在聚会中要保持安静，因为她们不可以发言，总要顺服，正如律法书所说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "婦女在聚會中應當閉口，好像在聖徒的眾教會中一樣，因為她們是不准講話的；就如律法所說的，她們應該順服。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "婦女在會中要閉口不言，像在聖徒的眾教會一樣，因為不准她們說話。她們總要順服，正如律法所說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The women are to keep silent in the churches; for they are not permitted to speak, but are to subject themselves, just as the Law also says."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Women should remain silent in the churches. They are not allowed to speak, but must be in submission, as the law says."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Women should be silent during the church meetings. It is not proper for them to speak. They should be submissive, just as the law says."
      }
    },
