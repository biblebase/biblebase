    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到了万物都归服基督以后，圣子基督也要归服那使万物归服祂的上帝，使上帝做万物的主宰。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "等到萬物都服了他，那時子自己也要服那位叫萬物服他的，好使　神在萬有之上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬物既服了他，那時子也要自己服那叫萬物服他的，叫神在萬物之上，為萬物之主。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When all things are subjected to Him, then the Son Himself also will be subjected to the One who subjected all things to Him, so that God may be all in all."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he has done this, then the Son himself will be made subject to him who put everything under him, so that God may be all in all."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then, when all things are under his authority, the Son will put himself under God’s authority, so that God, who gave his Son authority over all things, will be utterly supreme over everything everywhere."
      }
    },
