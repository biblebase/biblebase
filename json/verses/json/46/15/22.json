    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为正如在亚当里众人都死了，同样，在基督里众人也都要复活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在亞當裡眾人都死了，照樣，在基督裡眾人也都要復活；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在亞當裏眾人都死了；照樣，在基督裏眾人也都要復活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For as in Adam all die, so also in Christ all will be made alive."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For as in Adam all die, so in Christ all will be made alive."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Just as everyone dies because we all belong to Adam, everyone who belongs to Christ will be given new life."
      }
    },
