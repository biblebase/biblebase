    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来有一次向五百多位弟兄姊妹显现，其中有许多至今依然健在，也有些已经安息了；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以後又有一次向五百多個弟兄顯現。他們中間大多數到現今還在，也有些已經睡了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "後來一時顯給五百多弟兄看，其中一大半到如今還在，卻也有已經睡了的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After that He appeared to more than five hundred brethren at one time, most of whom remain until now, but some have fallen asleep;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After that, he appeared to more than five hundred of the brothers and sisters at the same time, most of whom are still living, though some have fallen asleep."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After that, he was seen by more than 500 of his followers at one time, most of whom are still alive, though some have died."
      }
