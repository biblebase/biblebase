    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "日有日的荣光，月有月的荣光，星有星的荣光，这颗星和那颗星的荣光也有分别。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "太陽有太陽的榮光，月亮有月亮的榮光，星星有星星的榮光，而且每一顆星的榮光也都不同。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "日有日的榮光，月有月的榮光，星有星的榮光；這星和那星的榮光也有分別。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is one glory of the sun, and another glory of the moon, and another glory of the stars; for star differs from star in glory."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The sun has one kind of splendor, the moon another and the stars another; and star differs from star in splendor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The sun has one kind of glory, while the moon and stars each have another kind. And even the stars differ from each other in their glory."
      }
    },
