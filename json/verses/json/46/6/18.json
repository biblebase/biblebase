    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们务要远离淫乱的行为。人无论犯什么样的罪，都是在身体以外，唯独淫乱的，是得罪自己的身体。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要逃避淫亂的事。人所犯的，無論是甚麼罪，都是在身體以外，唯有行淫亂的，是觸犯自己的身體。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要逃避淫行。人所犯的，無論甚麼罪，都在身子以外，惟有行淫的，是得罪自己的身子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Flee immorality. Every other sin that a man commits is outside the body, but the immoral man sins against his own body."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Flee from sexual immorality. All other sins a person commits are outside the body, but whoever sins sexually, sins against their own body."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Run from sexual sin! No other sin so clearly affects the body as this one does. For sexual immorality is a sin against your own body."
      }
    },
