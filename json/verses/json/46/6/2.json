    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道你们不知道圣徒将来要审判这世界吗？既然这世界也要由你们审判，难道你们不能审理这些小事吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不知道聖徒要審判世界嗎？既然世界要由你們來審判，難道你們不配審判這些最小的事嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "豈不知聖徒要審判世界嗎？若世界為你們所審，難道你們不配審判這最小的事嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Or do you not know that the saints will judge the world? If the world is judged by you, are you not competent to constitute the smallest law courts?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Or do you not know that the Lord’s people will judge the world? And if you are to judge the world, are you not competent to judge trivial cases?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t you realize that someday we believers will judge the world? And since you are going to judge the world, can’t you decide even these little things among yourselves?"
      }
    },
