    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人的工程若被烧毁了，他必遭受损失，自己虽然可以得救，却像火里逃生一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果有人的工程被燒毀，他就要受虧損；自己卻要得救，只是要像從火裡經過一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人的工程若被燒了，他就要受虧損，自己卻要得救；雖然得救，乃像從火裏經過的一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If any man’s work is burned up, he will suffer loss; but he himself will be saved, yet so as through fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If it is burned up, the builder will suffer loss but yet will be saved—even though only as one escaping through the flames."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if the work is burned up, the builder will suffer great loss. The builder will be saved, but like someone barely escaping through a wall of flames."
      }
    },
