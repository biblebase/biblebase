    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "每逢周日，各人应按照自己的收入抽出一部分留起来，免得我到的时候才现凑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "每逢七日的第一日，你們各人要按著自己的收入抽一些出來留著，免得我來的時候才現湊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "每逢七日的第一日，各人要照自己的進項抽出來留着，免得我來的時候現湊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On the first day of every week each one of you is to put aside and save, as he may prosper, so that no collections be made when I come."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the first day of every week, each one of you should set aside a sum of money in keeping with your income, saving it up, so that when I come no collections will have to be made."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On the first day of each week, you should each put aside a portion of the money you have earned. Don’t wait until I get there and then try to collect it all at once."
      }
    },
