    "sermons": [
      {
        "id": "home_sermons_retreat-2019-06-22-1",
        "title": "愛—最高的原則",
        "date": "2019-06-22",
        "preacher": "陳愛光牧師",
        "verses": "哥林多前書 13:1-13",
        "audio": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-22-1.mp3",
        "slides": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-22-1.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即使我倾家荡产周济穷人，甚至舍己捐躯任人焚烧，如果没有爱，对我也毫无益处。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若把一切所有的分給人，又捨己身被人焚燒，卻沒有愛，對我仍然毫無益處。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若將所有的賙濟窮人，又捨己身叫人焚燒，卻沒有愛，仍然與我無益。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And if I give all my possessions to feed the poor, and if I surrender my body to be burned, but do not have love, it profits me nothing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If I give all I possess to the poor and give over my body to hardship that I may boast, but do not have love, I gain nothing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If I gave everything I have to the poor and even sacrificed my body, I could boast about it; but if I didn’t love others, I would have gained nothing."
      }
    },
