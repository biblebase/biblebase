    "sermons": [
      {
        "id": "home_sermons_retreat-2019-06-22-1",
        "title": "愛—最高的原則",
        "date": "2019-06-22",
        "preacher": "陳愛光牧師",
        "verses": "哥林多前書 13:1-13",
        "audio": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-22-1.mp3",
        "slides": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-22-1.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即使我能做先知讲道，又明白各样的奥秘，而且学问渊博，甚至有移开山岭的信心，如果没有爱，我仍然算不了什么。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若有先知講道的恩賜，也明白各樣的奧祕，各樣的知識；並且有全備的信，叫我能夠移山，卻沒有愛，我就算不得甚麼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若有先知講道之能，也明白各樣的奧祕，各樣的知識，而且有全備的信，叫我能夠移山，卻沒有愛，我就算不得甚麼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I have the gift of prophecy, and know all mysteries and all knowledge; and if I have all faith, so as to remove mountains, but do not have love, I am nothing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If I have the gift of prophecy and can fathom all mysteries and all knowledge, and if I have a faith that can move mountains, but do not have love, I am nothing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If I had the gift of prophecy, and if I understood all of God’s secret plans and possessed all knowledge, and if I had such faith that I could move mountains, but didn’t love others, I would be nothing."
      }
    },
