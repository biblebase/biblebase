    "sermons": [
      {
        "id": "home_sermons_retreat-2019-06-22-1",
        "title": "愛—最高的原則",
        "date": "2019-06-22",
        "preacher": "陳愛光牧師",
        "verses": "哥林多前書 13:1-13",
        "audio": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-22-1.mp3",
        "slides": "http://cbcwla.org/media/ch/Special/Retreat2019/Retreat-2019-06-22-1.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如今我们好像对着镜子观看影像，模糊不清，但将来会看得真真切切。现在我所知道的有限，但将来会完全知道，如同主知道我一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們現在是對著鏡子觀看，模糊不清，到那時就要面對面了。我現在所知道的只是一部分，到那時就完全知道了，好像主完全知道我一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們如今彷彿對着鏡子觀看，模糊不清，到那時就要面對面了。我如今所知道的有限，到那時就全知道，如同主知道我一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For now we see in a mirror dimly, but then face to face; now I know in part, but then I will know fully just as I also have been fully known."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For now we see only a reflection as in a mirror; then we shall see face to face. Now I know in part; then I shall know fully, even as I am fully known."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now we see things imperfectly, like puzzling reflections in a mirror, but then we will see everything with perfect clarity. All that I know now is partial and incomplete, but then I will know everything completely, just as God now knows me completely."
      }
    },
