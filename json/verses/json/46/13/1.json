    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即使我能说人类和天使的各种语言，如果没有爱，我不过像咣咣作响的锣和钹。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若能說世人和天使的方言，卻沒有愛，我就成了鳴的鑼、響的鈸一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若能說萬人的方言，並天使的話語，卻沒有愛，我就成了鳴的鑼，響的鈸一般。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I speak with the tongues of men and of angels, but do not have love, I have become a noisy gong or a clanging cymbal."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If I speak in the tongues of men or of angels, but do not have love, I am only a resounding gong or a clanging cymbal."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If I could speak all the languages of earth and of angels, but didn’t love others, I would only be a noisy gong or a clanging cymbal."
      }
    },
