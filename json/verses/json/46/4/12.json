      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and we toil, working with our own hands; when we are reviled, we bless; when we are persecuted, we endure;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We work hard with our own hands. When we are cursed, we bless; when we are persecuted, we endure it;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We work wearily with our own hands to earn our living. We bless those who curse us. We are patient with those who abuse us."
      }
