    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我对你们或别人给我的评价毫不介意，我也不评价我自己。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我被你們論斷，或被人間法庭審判，我都認為是極小的事，連我自己也不論斷自己。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我被你們論斷，或被別人論斷，我都以為極小的事；連我自己也不論斷自己。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But to me it is a very small thing that I may be examined by you, or by any human court; in fact, I do not even examine myself."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I care very little if I am judged by you or by any human court; indeed, I do not even judge myself."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As for me, it matters very little how I might be evaluated by you or by any human authority. I don’t even trust my own judgment on this point."
      }
    },
