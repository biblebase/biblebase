    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "正如圣经上说：“我要摧毁智者的智慧，废弃明哲的聪明。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為經上記著說： “我要滅絕智慧人的智慧， 廢棄聰明人的聰明。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就如經上所記： 我要滅絕智慧人的智慧， 廢棄聰明人的聰明。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it is written, “I will destroy the wisdom of the wise, And the cleverness of the clever I will set aside.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For it is written: “I will destroy the wisdom of the wise; the intelligence of the intelligent I will frustrate.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As the Scriptures say, “I will destroy the wisdom of the wise and discard the intelligence of the intelligent.”"
      }
    },
