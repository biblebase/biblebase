    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为革来家的人把各位弟兄姊妹的事情告诉了我，说你们中间有纷争。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的弟兄們，革來氏家裡的人向我提到你們，說你們中間有紛爭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為革來氏家裏的人曾對我提起弟兄們來，說你們中間有紛爭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I have been informed concerning you, my brethren, by Chloe’s people, that there are quarrels among you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My brothers and sisters, some from Chloe’s household have informed me that there are quarrels among you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For some members of Chloe’s household have told me about your quarrels, my dear brothers and sisters."
      }
    },
