    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝是信实的，祂呼召你们是要你们与祂的儿子——我们的主耶稣基督相交。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神是信實的，他呼召了你們，是要你們與他的兒子我們主耶穌基督連合在一起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神是信實的，你們原是被他所召，好與他兒子－我們的主耶穌基督一同得分。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God is faithful, through whom you were called into fellowship with His Son, Jesus Christ our Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God is faithful, who has called you into fellowship with his Son, Jesus Christ our Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God will do this, for he is faithful to do what he says, and he has invited you into partnership with his Son, Jesus Christ our Lord."
      }
    },
