    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，你们在殷切等候我们主耶稣基督再来的期间，并不缺少任何属灵的恩赐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以致你們在恩賜上一無所缺，殷切盼望著我們主耶穌基督的顯現；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以致你們在恩賜上沒有一樣不及人的，等候我們的主耶穌基督顯現。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that you are not lacking in any gift, awaiting eagerly the revelation of our Lord Jesus Christ,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore you do not lack any spiritual gift as you eagerly wait for our Lord Jesus Christ to be revealed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now you have every spiritual gift you need as you eagerly wait for the return of our Lord Jesus Christ."
      }
