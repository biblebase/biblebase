    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝运用自己的智慧不让世人凭自己的智慧去认识祂，祂乐意采用世人看为愚昧的道理去拯救那些相信的人，这就是上帝的智慧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為在　神的智慧裡，世人憑自己的智慧，既然不能認識　神，　神就樂意藉著所傳的愚笨的道理，去拯救那些信的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "世人憑自己的智慧，既不認識神，神就樂意用人所當作愚拙的道理拯救那些信的人；這就是神的智慧了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For since in the wisdom of God the world through its wisdom did not come to know God, God was well-pleased through the foolishness of the message preached to save those who believe."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For since in the wisdom of God the world through its wisdom did not know him, God was pleased through the foolishness of what was preached to save those who believe."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Since God in his wisdom saw to it that the world would never know him through human wisdom, he has used our foolish preaching to save those who believe."
      }
    },
