    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的意思是你们有人说：“我是跟随保罗的”，有人说：“我是跟随亚波罗的”，有人说：“我是跟随彼得的”，有人说：“我是跟随基督的”。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的意思就是，你們各人說，我是保羅派的，我是亞波羅派的，我是磯法派的，我是基督派的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的意思就是你們各人說：「我是屬保羅的」；「我是屬亞波羅的」；「我是屬磯法的」；「我是屬基督的」。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now I mean this, that each one of you is saying, “I am of Paul,” and “I of Apollos,” and “I of Cephas,” and “I of Christ.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What I mean is this: One of you says, “I follow Paul”; another, “I follow Apollos”; another, “I follow Cephas”; still another, “I follow Christ.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some of you are saying, “I am a follower of Paul.” Others are saying, “I follow Apollos,” or “I follow Peter,” or “I follow only Christ.”"
      }
    },
