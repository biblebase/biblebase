    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其实我传福音并没有什么可夸的，因为这是我的任务，我不传福音就有祸了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我傳福音原是沒有可誇的，因我不能不傳。如果不傳福音，我就有禍了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我傳福音原沒有可誇的，因為我是不得已的。若不傳福音，我便有禍了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if I preach the gospel, I have nothing to boast of, for I am under compulsion; for woe is me if I do not preach the gospel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For when I preach the gospel, I cannot boast, since I am compelled to preach. Woe to me if I do not preach the gospel!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet preaching the Good News is not something I can boast about. I am compelled by God to do it. How terrible for me if I didn’t preach the Good News!"
      }
    },
