    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她把捡来的麦子带回城里给婆婆拿俄米看，又把剩下来的食物给婆婆。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後帶回城裡去。路得把撿來的麥子拿給婆婆看，把吃剩的給了婆婆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她就把所拾取的帶進城去給婆婆看，又把她吃飽了所剩的給了婆婆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She took it up and went into the city, and her mother-in-law saw what she had gleaned. She also took it out and gave Naomi what she had left after she was satisfied."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She carried it back to town, and her mother-in-law saw how much she had gathered. Ruth also brought out and gave her what she had left over after she had eaten enough."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She carried it back into town and showed it to her mother-in-law. Ruth also gave her the roasted grain that was left over from her meal."
      }
    },
