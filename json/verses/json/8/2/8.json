    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "波阿斯对路得说：“姑娘，听我说，你不用到别人的田里去拾麦穗了，也不必离开这里，只管留下来跟我的女工在一起。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "波阿斯對路得說：“女兒啊，聽我說，不要到別人田裡去撿麥穗，也不要離開這裡，要常常與我的女傭人在一起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "波阿斯對路得說：「女兒啊，聽我說，不要往別人田裏拾取麥穗，也不要離開這裏，要常與我使女們在一處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Boaz said to Ruth, “Listen carefully, my daughter. Do not go to glean in another field; furthermore, do not go on from this one, but stay here with my maids."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Boaz said to Ruth, “My daughter, listen to me. Don’t go and glean in another field and don’t go away from here. Stay here with the women who work for me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Boaz went over and said to Ruth, “Listen, my daughter. Stay right here with us when you gather grain; don’t go to any other fields. Stay right behind the young women working in my field."
      }
    },
