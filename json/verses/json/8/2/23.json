    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，路得便常跟波阿斯的女工在一起捡麦穗，直到大麦小麦都收割完毕。路得就这样陪伴婆婆过日子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她就緊跟著波阿斯的女傭人撿麥穗，直到大麥和小麥都收割好了。路得一直與婆婆住在一起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是路得與波阿斯的使女常在一處拾取麥穗，直到收完了大麥和小麥。路得仍與婆婆同住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So she stayed close by the maids of Boaz in order to glean until the end of the barley harvest and the wheat harvest. And she lived with her mother-in-law."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Ruth stayed close to the women of Boaz to glean until the barley and wheat harvests were finished. And she lived with her mother-in-law."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Ruth worked alongside the women in Boaz’s fields and gathered grain with them until the end of the barley harvest. Then she continued working with them through the wheat harvest in early summer. And all the while she lived with her mother-in-law."
      }
    },
