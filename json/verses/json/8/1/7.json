    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她们离开所居住的地方，启程返回犹大。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她離開居住的地方，兩個媳婦也隨她出發，要回到猶大地去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是她和兩個兒婦起行離開所住的地方，要回猶大地去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So she departed from the place where she was, and her two daughters-in-law with her; and they went on the way to return to the land of Judah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "With her two daughters-in-law she left the place where she had been living and set out on the road that would take them back to the land of Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "With her two daughters-in-law she set out from the place where she had been living, and they took the road that would lead them back to Judah."
      }
