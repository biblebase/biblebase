    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "姑娘，不要怕，你所说的一切，我都会去为你安排，城里所有的人都知道你是个贤德的女子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我女兒，現在你不要怕，你所說的，我一定去作。本城的人都知道你是個賢慧的女人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "女兒啊，現在不要懼怕，凡你所說的，我必照着行；我本城的人都知道你是個賢德的女子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now, my daughter, do not fear. I will do for you whatever you ask, for all my people in the city know that you are a woman of excellence."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And now, my daughter, don’t be afraid. I will do for you all you ask. All the people of my town know that you are a woman of noble character."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now don’t worry about a thing, my daughter. I will do what is necessary, for everyone in town knows you are a virtuous woman."
      }
    },
