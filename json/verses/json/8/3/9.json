    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "便问道：“你是谁？”她答道：“我是你的婢女路得，请用你的衣襟遮盖我，因为你是我的近亲。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就說：“你是誰？”她回答：“我是你的婢女路得，請用你的衣襟遮蓋你的婢女，因為你是我一個有買贖權的近親。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他就說：「你是誰？」回答說：「我是你的婢女路得。求你用你的衣襟遮蓋我，因為你是我一個至近的親屬。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said, “Who are you?” And she answered, “I am Ruth your maid. So spread your covering over your maid, for you are a close relative.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Who are you?” he asked. “I am your servant Ruth,” she said. “Spread the corner of your garment over me, since you are a guardian-redeemer of our family.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Who are you?” he asked. “I am your servant Ruth,” she replied. “Spread the corner of your covering over me, for you are my family redeemer.”"
      }
    },
