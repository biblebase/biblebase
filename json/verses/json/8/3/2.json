    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "波阿斯不是我们的亲戚吗？而且你又常与他的女工在一起。今天晚上他会在麦场簸大麦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你常常和他的女傭人在一起的波阿斯不是我們的親戚嗎？他今天晚上要在禾場上簸大麥。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你與波阿斯的使女常在一處，波阿斯不是我們的親族嗎？他今夜在場上簸大麥；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now is not Boaz our kinsman, with whose maids you were? Behold, he winnows barley at the threshing floor tonight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now Boaz, with whose women you have worked, is a relative of ours. Tonight he will be winnowing barley on the threshing floor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Boaz is a close relative of ours, and he’s been very kind by letting you gather grain with his young women. Tonight he will be winnowing barley at the threshing floor."
      }
    },
