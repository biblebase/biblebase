    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不错，我是你的近亲，可是有一个人比我更近。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不錯，我是你那有買贖權的近親，可惜還有一個有買贖權的近親比我更親。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我實在是你一個至近的親屬，只是還有一個人比我更近。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now it is true I am a close relative; however, there is a relative closer than I."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Although it is true that I am a guardian-redeemer of our family, there is another who is more closely related than I."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But while it’s true that I am one of your family redeemers, there is another man who is more closely related to you than I am."
      }
