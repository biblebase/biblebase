    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们这种表现正是上帝公义审判的明证，使你们配进上帝的国，你们正在为这国受苦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這正是　神公義判斷的明證，使你們可以算是配得上他的國；你們也是為了　神的國而受苦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這正是神公義判斷的明證，叫你們可算配得神的國；你們就是為這國受苦。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is a plain indication of God’s righteous judgment so that you will be considered worthy of the kingdom of God, for which indeed you are suffering."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All this is evidence that God’s judgment is right, and as a result you will be counted worthy of the kingdom of God, for which you are suffering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And God will use this persecution to show his justice and to make you worthy of his Kingdom, for which you are suffering."
      }
    },
