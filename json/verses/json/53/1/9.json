    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些人要受的刑罚就是离开主的面和祂荣耀的权能，永远灭亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們要受刑罰，就是永遠沉淪，離開主的面和他權能的榮光。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These will pay the penalty of eternal destruction, away from the presence of the Lord and from the glory of His power,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will be punished with everlasting destruction and shut out from the presence of the Lord and from the glory of his might"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will be punished with eternal destruction, forever separated from the Lord and from his glorious power."
      }
    },
