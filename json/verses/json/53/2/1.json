    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弟兄姊妹，关于主耶稣基督再来和我们到祂那里相聚的事情，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們，關於我們主耶穌基督的再來，和我們到他那裡聚集的事，我們求你們："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弟兄們，論到我們主耶穌基督降臨和我們到他那裏聚集，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now we request you, brethren, with regard to the coming of our Lord Jesus Christ and our gathering together to Him,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Concerning the coming of our Lord Jesus Christ and our being gathered to him, we ask you, brothers and sisters,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now, dear brothers and sisters, let us clarify some things about the coming of our Lord Jesus Christ and how we will be gathered to meet him."
      }
    },
