    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他会抵挡主，高抬自己超过一切所谓的神明和人们崇拜的对象，甚至坐在上帝的殿中以上帝自居！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他抵擋　神，抬舉自己，高過一切稱為神或受人敬拜的，甚至坐在　神的殿中，自稱為　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他是抵擋主，高擡自己，超過一切稱為神的和一切受人敬拜的，甚至坐在神的殿裏，自稱是神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "who opposes and exalts himself above every so-called god or object of worship, so that he takes his seat in the temple of God, displaying himself as being God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will oppose and will exalt himself over everything that is called God or is worshiped, so that he sets himself up in God’s temple, proclaiming himself to be God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will exalt himself and defy everything that people call god and every object of worship. He will even sit in the temple of God, claiming that he himself is God."
      }
    },
