    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝借着我们所传的福音呼召了你们，使你们可以得到我们主耶稣基督的荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，　神藉著我們所傳的福音呼召你們，使你們得著我們主耶穌基督的榮耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神藉我們所傳的福音召你們到這地步，好得着我們主耶穌基督的榮光。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It was for this He called you through our gospel, that you may gain the glory of our Lord Jesus Christ."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He called you to this through our gospel, that you might share in the glory of our Lord Jesus Christ."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He called you to salvation when we told you the Good News; now you can share in the glory of our Lord Jesus Christ."
      }
    },
