      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因我們聽說，在你們中間有人不按規矩而行，甚麼工都不做，反倒專管閒事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For we hear that some among you are leading an undisciplined life, doing no work at all, but acting like busybodies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We hear that some among you are idle and disruptive. They are not busy; they are busybodies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet we hear that some of you are living idle lives, refusing to work and meddling in other people’s business."
      }
    },
