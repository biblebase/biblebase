    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们并不是没有权利接受供应，而是要给你们树立学习的榜样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這不是因為我們沒有權利，而是要給你們作榜樣，好讓你們效法我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這並不是因我們沒有權柄，乃是要給你們作榜樣，叫你們效法我們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "not because we do not have the right to this, but in order to offer ourselves as a model for you, so that you would follow our example."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We did this, not because we do not have the right to such help, but in order to offer ourselves as a model for you to imitate."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We certainly had the right to ask you to feed us, but we wanted to give you an example to follow."
      }
    },
