      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "也没有白吃谁的饭，而是昼夜辛勤工作，免得成为你们任何人的负担。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也沒有白吃過誰的飯，反而辛苦勞碌，晝夜作工，免得加重你們任何一人的負擔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也未嘗白吃人的飯，倒是辛苦勞碌，晝夜做工，免得叫你們一人受累。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "nor did we eat anyone’s bread without paying for it, but with labor and hardship we kept working night and day so that we would not be a burden to any of you;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "nor did we eat anyone’s food without paying for it. On the contrary, we worked night and day, laboring and toiling so that we would not be a burden to any of you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We never accepted food from anyone without paying for it. We worked hard day and night so we would not be a burden to any of you."
      }
    },
