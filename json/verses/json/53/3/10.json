    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们在你们那里的时候，曾吩咐过你们：不肯工作的人没有资格吃饭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們在你們那裡的時候，曾經吩咐過你們，如果有人不肯作工，就不可吃飯。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們在你們那裏的時候，曾吩咐你們說，若有人不肯做工，就不可吃飯。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For even when we were with you, we used to give you this order: if anyone is not willing to work, then he is not to eat, either."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For even when we were with you, we gave you this rule: “The one who is unwilling to work shall not eat.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even while we were with you, we gave you this command: “Those unwilling to work will not get to eat.”"
      }
    },
