      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們靠主深信，你們現在是遵行我們所吩咐的，後來也必要遵行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We have confidence in the Lord concerning you, that you are doing and will continue to do what we command."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We have confidence in the Lord that you are doing and will continue to do the things we command."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And we are confident in the Lord that you are doing and will continue to do the things we commanded you."
      }
