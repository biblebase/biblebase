      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿主引导你们的心领悟上帝的爱和基督的坚忍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願主引導你們的心，使你們有　神的愛和基督的堅忍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願主引導你們的心，叫你們愛神，並學基督的忍耐！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May the Lord direct your hearts into the love of God and into the steadfastness of Christ."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the Lord direct your hearts into God’s love and Christ’s perseverance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May the Lord lead your hearts into a full understanding and expression of the love of God and the patient endurance that comes from Christ."
      }
