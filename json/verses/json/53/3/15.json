    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只是不可把他当作仇人，要像对待弟兄姊妹一样劝诫他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但不要把他看作仇敵，卻要勸他好像勸弟兄一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但不要以他為仇人，要勸他如弟兄。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet do not regard him as an enemy, but admonish him as a brother."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet do not regard them as an enemy, but warn them as you would a fellow believer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t think of them as enemies, but warn them as you would a brother or sister."
      }
    },
