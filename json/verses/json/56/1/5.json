    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当初，我把你留在克里特岛，是要你完成未办完的事工，并照我的指示在各城选立长老。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我從前留你在克里特島，是要你辦好沒有辦完的事，並且照我所吩咐的，在各城設立長老。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我從前留你在克里特，是要你將那沒有辦完的事都辦整齊了，又照我所吩咐你的，在各城設立長老。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For this reason I left you in Crete, that you would set in order what remains and appoint elders in every city as I directed you,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The reason I left you in Crete was that you might put in order what was left unfinished and appoint elders in every town, as I directed you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I left you on the island of Crete so you could complete our work there and appoint elders in each town as I instructed you."
      }
    },
