    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你住在悬崖峭壁间， 住在高山上， 自以为谁也不能把你拉下来， 但你的骄傲欺骗了你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你心中的傲氣欺騙了你； 你這住在巖石的隱密處， 居所在高處的啊！ 你心裡說： “誰能把我拉下地呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住在山穴中、居所在高處的啊， 你因狂傲自欺， 心裏說：誰能將我拉下地去呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The arrogance of your heart has deceived you, You who live in the clefts of the rock, In the loftiness of your dwelling place, Who say in your heart, ‘Who will bring me down to earth?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The pride of your heart has deceived you, you who live in the clefts of the rocks and make your home on the heights, you who say to yourself, ‘Who can bring me down to the ground?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have been deceived by your own pride because you live in a rock fortress and make your home high in the mountains. ‘Who can ever reach us way up here?’ you ask boastfully."
      }
    },
