    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们若能将梦和梦的意思告诉我，我必给你们礼物、赏赐和极大的尊荣。所以你们要将梦和梦的意思告诉我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們能把那夢和夢的意思向我解釋，就必從我這裡得禮物、獎賞和大尊榮，所以你們要把夢和夢的意思向我解釋。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若將夢和夢的講解告訴我，就必從我這裏得贈品和賞賜，並大尊榮。現在你們要將夢和夢的講解告訴我。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But if you declare the dream and its interpretation, you will receive from me gifts and a reward and great honor; therefore declare to me the dream and its interpretation.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if you tell me the dream and explain it, you will receive from me gifts and rewards and great honor. So tell me the dream and interpret it for me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if you tell me what I dreamed and what the dream means, I will give you many wonderful gifts and honors. Just tell me the dream and what it means!”"
      }
    },
