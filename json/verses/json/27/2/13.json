    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，处死智者的谕旨发出，但以理和他的同伴都在被杀之列。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是王的御旨發出，要把智慧人都殺死；人就尋找但以理和他的同伴，要殺死他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是命令發出，哲士將要見殺，人就尋找但以理和他的同伴，要殺他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the decree went forth that the wise men should be slain; and they looked for Daniel and his friends to kill them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the decree was issued to put the wise men to death, and men were sent to look for Daniel and his friends to put them to death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And because of the king’s decree, men were sent to find and kill Daniel and his friends."
      }
    },
