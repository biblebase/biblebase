    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王对但以理说：“你们的上帝真是万神之神、万王之主、奥秘的启示者，因为你能揭开这个奥秘。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王對但以理說：“你們的　神真是萬神之神，萬王之王，又是顯明隱祕事的，因為你能把這隱祕的事顯明出來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王對但以理說：「你既能顯明這奧祕的事，你們的神誠然是萬神之神、萬王之主，又是顯明奧祕事的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king answered Daniel and said, “Surely your God is a God of gods and a Lord of kings and a revealer of mysteries, since you have been able to reveal this mystery.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king said to Daniel, “Surely your God is the God of gods and the Lord of kings and a revealer of mysteries, for you were able to reveal this mystery.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king said to Daniel, “Truly, your God is the greatest of gods, the Lord over kings, a revealer of mysteries, for you have been able to reveal this secret.”"
      }
    },
