    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王又应允但以理的请求，派沙得拉、米煞和亚伯尼歌负责巴比伦省的事务。但以理仍在朝中供职。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但以理求王，王就委派沙得拉、米煞、亞伯尼歌管理巴比倫省的政務；但以理卻在朝中侍立。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但以理求王，王就派沙得拉、米煞、亞伯尼歌管理巴比倫省的事務，只是但以理常在朝中侍立。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Daniel made request of the king, and he appointed Shadrach, Meshach and Abed-nego over the administration of the province of Babylon, while Daniel was at the king’s court."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moreover, at Daniel’s request the king appointed Shadrach, Meshach and Abednego administrators over the province of Babylon, while Daniel himself remained at the royal court."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At Daniel’s request, the king appointed Shadrach, Meshach, and Abednego to be in charge of all the affairs of the province of Babylon, while Daniel remained in the king’s court."
      }
    },
