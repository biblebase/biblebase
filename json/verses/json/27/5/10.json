    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "太后听到王和大臣的喊声，便来到宴会厅，对王说：“愿王万岁！不要惊慌失色。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "太后因王和他的大臣所說的話，就進入宴會的大廳，對王說：“願王萬歲！你的心意不要驚惶，也不要臉色大變。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "太后因王和他大臣所說的話，就進入宴宮，說：「願王萬歲！你心意不要驚惶，臉面不要變色。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The queen entered the banquet hall because of the words of the king and his nobles; the queen spoke and said, “O king, live forever! Do not let your thoughts alarm you or your face be pale."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The queen, hearing the voices of the king and his nobles, came into the banquet hall. “May the king live forever!” she said. “Don’t be alarmed! Don’t look so pale!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when the queen mother heard what was happening, she hurried to the banquet hall. She said to Belshazzar, “Long live the king! Don’t be so pale and frightened."
      }
    },
