    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但他变得心高气傲、刚愎自用、狂妄自大，因而被革除王位、剥去尊荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但他心高氣傲、妄自尊大的時候，就從國位上被趕下來，他的尊榮也被奪去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但他心高氣傲，靈也剛愎，甚至行事狂傲，就被革去王位，奪去榮耀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when his heart was lifted up and his spirit became so proud that he behaved arrogantly, he was deposed from his royal throne and his glory was taken away from him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But when his heart became arrogant and hardened with pride, he was deposed from his royal throne and stripped of his glory."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when his heart and mind were puffed up with arrogance, he was brought down from his royal throne and stripped of his glory."
      }
    },
