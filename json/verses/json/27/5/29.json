    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，伯沙撒下令给但以理穿上紫袍，戴上金项链，又宣告他在国中位居第三。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是伯沙撒下令，人就把紫袍給但以理穿上，把金鍊戴在他的頸上，又宣告他在國中掌權，位列第三。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "伯沙撒下令，人就把紫袍給但以理穿上，把金鍊給他戴在頸項上，又傳令使他在國中位列第三。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Belshazzar gave orders, and they clothed Daniel with purple and put a necklace of gold around his neck, and issued a proclamation concerning him that he now had authority as the third ruler in the kingdom."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then at Belshazzar’s command, Daniel was clothed in purple, a gold chain was placed around his neck, and he was proclaimed the third highest ruler in the kingdom."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then at Belshazzar’s command, Daniel was dressed in purple robes, a gold chain was hung around his neck, and he was proclaimed the third highest ruler in the kingdom."
      }
    },
