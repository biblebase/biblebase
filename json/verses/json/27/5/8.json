      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王所有的智者进来后，竟无人能读懂或把意思告诉王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是王所有的智慧人都進來，卻不能讀那文字，也不能把意思向王說明。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是王的一切哲士都進來，卻不能讀那文字，也不能把講解告訴王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then all the king’s wise men came in, but they could not read the inscription or make known its interpretation to the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then all the king’s wise men came in, but they could not read the writing or tell the king what it meant."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when all the king’s wise men had come in, none of them could read the writing or tell him what it meant."
      }
    },
