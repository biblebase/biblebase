    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“伯沙撒啊，你是他的后裔，你虽然知道这一切事，仍不谦卑，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "伯沙撒啊！你是他的子孫，你雖然知道這一切，你的心仍不謙卑，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "伯沙撒啊，你是他的兒子，你雖知道這一切，你心仍不自卑，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet you, his son, Belshazzar, have not humbled your heart, even though you knew all this,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But you, Belshazzar, his son, have not humbled yourself, though you knew all this."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You are his successor, O Belshazzar, and you knew all this, yet you have not humbled yourself."
      }
    },
