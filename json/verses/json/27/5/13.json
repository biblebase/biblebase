    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，但以理被带到王面前。王问他：“你就是先王从犹大掳来的但以理吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是但以理被帶到王面前，王問但以理說：“你就是我先王從猶大擄來的猶大人但以理嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但以理就被領到王前。王問但以理說：「你是被擄之猶大人中的但以理嗎？就是我父王從猶大擄來的嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Daniel was brought in before the king. The king spoke and said to Daniel, “Are you that Daniel who is one of the exiles from Judah, whom my father the king brought from Judah?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Daniel was brought before the king, and the king said to him, “Are you Daniel, one of the exiles my father the king brought from Judah?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Daniel was brought in before the king. The king asked him, “Are you Daniel, one of the exiles brought from Judah by my predecessor, King Nebuchadnezzar?"
      }
    },
