    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我正沉思的时候，忽然从西面出现一只公山羊，穿越大地，脚不沾地。它双眼之间有一个大角。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我正在思考的時候，忽然有一隻公山羊從西而來，走遍全地，快得腳不沾塵；這山羊兩眼之間有一個顯眼的角。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我正思想的時候，見有一隻公山羊從西而來，遍行全地，腳不沾塵。這山羊兩眼當中有一非常的角。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While I was observing, behold, a male goat was coming from the west over the surface of the whole earth without touching the ground; and the goat had a conspicuous horn between his eyes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As I was thinking about this, suddenly a goat with a prominent horn between its eyes came from the west, crossing the whole earth without touching the ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "While I was watching, suddenly a male goat appeared from the west, crossing the land so swiftly that he didn’t even touch the ground. This goat, which had one very large horn between its eyes,"
      }
    },
