    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我在异象中看见自己在以拦省的书珊城，又看见自己站在乌莱河畔。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我在異象中觀看，看見自己在以攔省書珊城堡裡；我在異象中繼續觀看，看見自己在烏萊河的岸邊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我見了異象的時候，我以為在以攔省書珊城中；我見異象又如在烏萊河邊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I looked in the vision, and while I was looking I was in the citadel of Susa, which is in the province of Elam; and I looked in the vision and I myself was beside the Ulai Canal."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In my vision I saw myself in the citadel of Susa in the province of Elam; in the vision I was beside the Ulai Canal."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In this vision I was at the fortress of Susa, in the province of Elam, standing beside the Ulai River."
      }
    },
