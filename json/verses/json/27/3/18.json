    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即或不然，王啊，你要明白，我们也不会事奉你的神明或祭拜你立的金像。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "即或不然，王啊！你要知道，我們決不事奉你的神，也不向你所立的金像下拜。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "即或不然，王啊，你當知道我們決不事奉你的神，也不敬拜你所立的金像。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But even if He does not, let it be known to you, O king, that we are not going to serve your gods or worship the golden image that you have set up.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But even if he does not, we want you to know, Your Majesty, that we will not serve your gods or worship the image of gold you have set up.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But even if he doesn’t, we want to make it clear to you, Your Majesty, that we will never serve your gods or worship the gold statue you have set up.”"
      }
    },
