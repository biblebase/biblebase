      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又命令军中的勇士将沙得拉、米煞、亚伯尼歌绑起来，扔进烈焰熊熊的火窑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又吩咐他軍隊中幾個最精壯的士兵，把沙得拉、米煞和亞伯尼歌綁起來，扔在烈火的窯中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又吩咐他軍中的幾個壯士，將沙得拉、米煞、亞伯尼歌捆起來，扔在烈火的窰中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He commanded certain valiant warriors who were in his army to tie up Shadrach, Meshach and Abed-nego in order to cast them into the furnace of blazing fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and commanded some of the strongest soldiers in his army to tie up Shadrach, Meshach and Abednego and throw them into the blazing furnace."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he ordered some of the strongest men of his army to bind Shadrach, Meshach, and Abednego and throw them into the blazing furnace."
      }
    },
