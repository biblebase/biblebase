    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我来是要使你明白将来你同胞的遭遇，因为这异象是关于将来的事。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在我來是要使你明白日後所要發生在你的同胞身上的事，因為這異象是關於將來許多的年日的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在我來，要使你明白本國之民日後必遭遇的事，因為這異象關乎後來許多的日子。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now I have come to give you an understanding of what will happen to your people in the latter days, for the vision pertains to the days yet future.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now I have come to explain to you what will happen to your people in the future, for the vision concerns a time yet to come.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now I am here to explain what will happen to your people in the future, for this vision concerns a time yet to come.”"
      }
