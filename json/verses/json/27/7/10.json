    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从祂面前流出火河， 事奉祂的有千千， 侍立在祂面前的有万万。 祂坐下要审判， 案卷已经展开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有火像河湧出， 烈火從他面前冒出； 事奉他的有千千， 侍立在他面前的有萬萬。 審判已經開始， 案卷都展開了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從他面前有火，像河發出； 事奉他的有千千， 在他面前侍立的有萬萬； 他坐着要行審判， 案卷都展開了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A river of fire was flowing And coming out from before Him; Thousands upon thousands were attending Him, And myriads upon myriads were standing before Him; The court sat, And the books were opened."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A river of fire was flowing, coming out from before him. Thousands upon thousands attended him; ten thousand times ten thousand stood before him. The court was seated, and the books were opened."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and a river of fire was pouring out, flowing from his presence. Millions of angels ministered to him; many millions stood to attend him. Then the court began its session, and the books were opened."
      }
    },
