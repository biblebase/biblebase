    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但以理说：“我在夜间的异象中看见天风四面吹来，搅动大海。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但以理說：“在夜間的異象中，我看見天的四風，攪動大海。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但以理說： 我夜裏見異象，看見天的四風陡起，颳在大海之上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Daniel said, “I was looking in my vision by night, and behold, the four winds of heaven were stirring up the great sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Daniel said: “In my vision at night I looked, and there before me were the four winds of heaven churning up the great sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In my vision that night, I, Daniel, saw a great storm churning the surface of a great sea, with strong winds blowing from every direction."
      }
    },
