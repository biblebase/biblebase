    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“这就是我的梦。我但以理心中十分害怕，脸色苍白，但我没有把这事告诉别人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這事到此完畢。我但以理心中十分驚惶，臉色也變了，但我把這事存放在心裡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那事至此完畢。至於我－但以理，心中甚是驚惶，臉色也改變了，卻將那事存記在心。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“At this point the revelation ended. As for me, Daniel, my thoughts were greatly alarming me and my face grew pale, but I kept the matter to myself.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is the end of the matter. I, Daniel, was deeply troubled by my thoughts, and my face turned pale, but I kept the matter to myself.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That was the end of the vision. I, Daniel, was terrified by my thoughts and my face was pale with fear, but I kept these things to myself."
      }
    },
