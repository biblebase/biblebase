    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他得到权柄、荣耀和国度，各族、各邦、各语种的人都要事奉他。他的统治直到永远、没有穷尽，他的国度永不灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "得了權柄、尊榮和國度； 各國、各族和說各種語言的人都事奉他。 他的權柄是永遠的權柄，是不能廢去的； 他的國度是永不毀滅的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "得了權柄、榮耀、國度， 使各方、各國、各族的人都事奉他。 他的權柄是永遠的，不能廢去； 他的國必不敗壞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And to Him was given dominion, Glory and a kingdom, That all the peoples, nations and men of every language Might serve Him. His dominion is an everlasting dominion Which will not pass away; And His kingdom is one Which will not be destroyed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He was given authority, glory and sovereign power; all nations and peoples of every language worshiped him. His dominion is an everlasting dominion that will not pass away, and his kingdom is one that will never be destroyed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He was given authority, honor, and sovereignty over all the nations of the world, so that people of every race and nation and language would obey him. His rule is eternal—it will never end. His kingdom will never be destroyed."
      }
    },
