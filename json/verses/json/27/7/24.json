    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "十角是指这国中将兴起十个王，后来又兴起一王，与先前的王不同，他将制服三个王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於那十角，就是從這國中將要興起的十個王，在他們以後，必有另一個王興起，和先前的王都不相同；他必制伏三個王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於那十角，就是從這國中必興起的十王， 後來又興起一王， 與先前的不同； 他必制伏三王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As for the ten horns, out of this kingdom ten kings will arise; and another will arise after them, and he will be different from the previous ones and will subdue three kings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The ten horns are ten kings who will come from this kingdom. After them another king will arise, different from the earlier ones; he will subdue three kings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Its ten horns are ten kings who will rule that empire. Then another king will arise, different from the other ten, who will subdue three of them."
      }
    },
