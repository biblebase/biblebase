    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "术士、巫师、占星家和占卜者都来了，我将梦告诉他们，他们却不能给我解梦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是術士、用法術的、迦勒底人和占星家都進來；我當面把那夢告訴了他們，可是他們卻不能把夢的意思向我說明。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是那些術士、用法術的、迦勒底人、觀兆的都進來，我將那夢告訴了他們，他們卻不能把夢的講解告訴我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the magicians, the conjurers, the Chaldeans and the diviners came in and I related the dream to them, but they could not make its interpretation known to me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the magicians, enchanters, astrologers and diviners came, I told them the dream, but they could not interpret it for me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When all the magicians, enchanters, astrologers, and fortune-tellers came in, I told them the dream, but they could not tell me what it meant."
      }
    },
