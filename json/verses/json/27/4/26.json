    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "守望者命令留下树丕，表示等到你承认上天掌权后，你将重掌国权。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "守望者既然吩咐要留下樹根的餘幹，所以，等到你承認上天的至高者是掌權的，你的國就必再歸給你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "守望者既吩咐存留樹墩，等你知道諸天掌權，以後你的國必定歸你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And in that it was commanded to leave the stump with the roots of the tree, your kingdom will be assured to you after you recognize that it is Heaven that rules."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The command to leave the stump of the tree with its roots means that your kingdom will be restored to you when you acknowledge that Heaven rules."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the stump and roots of the tree were left in the ground. This means that you will receive your kingdom back again when you have learned that heaven rules."
      }
    },
