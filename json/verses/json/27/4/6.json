    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我便召巴比伦所有的智者来为我解梦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我就下令把巴比倫所有的智慧人，都帶到我面前來，要他們把夢的意思向我說明。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以我降旨召巴比倫的一切哲士到我面前，叫他們把夢的講解告訴我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I gave orders to bring into my presence all the wise men of Babylon, that they might make known to me the interpretation of the dream."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I commanded that all the wise men of Babylon be brought before me to interpret the dream for me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I issued an order calling in all the wise men of Babylon, so they could tell me what my dream meant."
      }
    },
