    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在我尼布甲尼撒颂赞、尊崇、敬奉天上的王，因为祂的作为公正，祂行事公平，能够贬抑行为骄傲的人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在我尼布甲尼撒讚美、尊崇、榮耀天上的王，因為他所作的一切都正確，他所行的也都公平；行為驕傲的，他都能貶低。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在我－尼布甲尼撒讚美、尊崇、恭敬天上的王；因為他所做的全都誠實，他所行的也都公平。那行動驕傲的，他能降為卑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now I, Nebuchadnezzar, praise, exalt and honor the King of heaven, for all His works are true and His ways just, and He is able to humble those who walk in pride.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now I, Nebuchadnezzar, praise and exalt and glorify the King of heaven, because everything he does is right and all his ways are just. And those who walk in pride he is able to humble."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Now I, Nebuchadnezzar, praise and glorify and honor the King of heaven. All his acts are just and true, and he is able to humble the proud.”"
      }
    },
