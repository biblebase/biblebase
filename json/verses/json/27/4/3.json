      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“祂的神迹何其伟大！ 祂的奇事何其可畏！ 祂的国度永远长存， 祂的统治直到万代。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的神蹟多麼偉大， 他的奇事多麼有力； 他的國是永遠的國，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的神蹟何其大！ 他的奇事何其盛！ 他的國是永遠的； 他的權柄存到萬代！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How great are His signs And how mighty are His wonders! His kingdom is an everlasting kingdom And His dominion is from generation to generation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How great are his signs, how mighty his wonders! His kingdom is an eternal kingdom; his dominion endures from generation to generation."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How great are his signs, how powerful his wonders! His kingdom will last forever, his rule through all generations."
      }
    },
