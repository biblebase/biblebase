    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘术士的首领伯提沙撒啊，我知道你有圣洁神明的灵，任何奥秘都难不倒你。请你为我解释我梦中所见的异象。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘術士的領袖伯提沙撒啊！因為我知道你裡面有聖神的靈，沒有甚麼隱祕的事能難倒你；因此，你要把我夢中所見的異象和夢的意思告訴我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "『術士的領袖伯提沙撒啊，因我知道你裏頭有聖神的靈，甚麼奧祕的事都不能使你為難。現在要把我夢中所見的異象和夢的講解告訴我。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘O Belteshazzar, chief of the magicians, since I know that a spirit of the holy gods is in you and no mystery baffles you, tell me the visions of my dream which I have seen, along with its interpretation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I said, “Belteshazzar, chief of the magicians, I know that the spirit of the holy gods is in you, and no mystery is too difficult for you. Here is my dream; interpret it for me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I said to him, ‘Belteshazzar, chief of the magicians, I know that the spirit of the holy gods is in you and that no mystery is too great for you to solve. Now tell me what my dream means."
      }
    },
