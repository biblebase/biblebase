    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说：“这宏伟的巴比伦岂不是我用大能建立为京都，以显我的威严和荣耀吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“這大巴比倫城不是我用大能大力建造作我的京都，為顯我威嚴的榮耀嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：「這大巴比倫不是我用大能大力建為京都，要顯我威嚴的榮耀嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king reflected and said, ‘Is this not Babylon the great, which I myself have built as a royal residence by the might of my power and for the glory of my majesty?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "he said, “Is not this the great Babylon I have built as the royal residence, by my mighty power and for the glory of my majesty?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As he looked out across the city, he said, ‘Look at this great city of Babylon! By my own mighty power, I have built this beautiful city as my royal residence to display my majestic splendor.’"
      }
    },
