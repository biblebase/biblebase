    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王啊，这苍劲有力的树就是你，你的威名高达穹苍，你的权柄延至地极。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王啊！你就是那樹，越來越偉大堅強；你的威勢漸長，高達於天；你的權柄直到地極。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「王啊，這漸長又堅固的樹就是你。你的威勢漸長及天，你的權柄管到地極。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "it is you, O king; for you have become great and grown strong, and your majesty has become great and reached to the sky and your dominion to the end of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your Majesty, you are that tree! You have become great and strong; your greatness has grown until it reaches the sky, and your dominion extends to distant parts of the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That tree, Your Majesty, is you. For you have grown strong and great; your greatness reaches up to heaven, and your rule to the ends of the earth."
      }
    },
