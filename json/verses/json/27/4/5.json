    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我做了一个梦，令我害怕。我躺在床上的时候，脑中出现的情景和异象使我恐惧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我作了一個夢，這夢使我懼怕；我在床上所見的夢幻和我腦海中出現的異象，都使我驚惶。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我做了一夢，使我懼怕。我在牀上的思念，並腦中的異象，使我驚惶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I saw a dream and it made me fearful; and these fantasies as I lay on my bed and the visions in my mind kept alarming me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I had a dream that made me afraid. As I was lying in bed, the images and visions that passed through my mind terrified me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But one night I had a dream that frightened me; I saw visions that terrified me as I lay in my bed."
      }
    },
