      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些官员一同来了，发现但以理向他的上帝祷告、祈求，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些人相約前來，看見但以理在他的　神面前祈禱懇求。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那些人就紛紛聚集，見但以理在他神面前祈禱懇求。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then these men came by agreement and found Daniel making petition and supplication before his God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then these men went as a group and found Daniel praying and asking God for help."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the officials went together to Daniel’s house and found him praying and asking for God’s help."
      }
