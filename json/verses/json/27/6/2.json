    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "总督之上，设三个总长，以维护王的统治，但以理是其中之一。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又在這些總督之上，設立三位監察司，監管總督的工作，免得王受虧損。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又在他們以上立總長三人（但以理在其中），使總督在他們三人面前回覆事務，免得王受虧損。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and over them three commissioners (of whom Daniel was one), that these satraps might be accountable to them, and that the king might not suffer loss."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "with three administrators over them, one of whom was Daniel. The satraps were made accountable to them so that the king might not suffer loss."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king also chose Daniel and two others as administrators to supervise the high officers and protect the king’s interests."
      }
    },
