    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来，大流士王传谕境内的各族、各邦、各语种的人，说：“愿你们大享平安！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "後來大利烏王寫信給住在全地的各國、各族和說各種語言的人，說：“願你們大享平安！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，大流士王傳旨，曉諭住在全地各方、各國、各族的人說：「願你們大享平安！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Darius the king wrote to all the peoples, nations and men of every language who were living in all the land: “May your peace abound!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then King Darius wrote to all the nations and peoples of every language in all the earth: “May you prosper greatly!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then King Darius sent this message to the people of every race and nation and language throughout the world: “Peace and prosperity to you!"
      }
    },
