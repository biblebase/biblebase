    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从废除日常献的祭、设立带来毁灭的可憎之物算起，必经过一千二百九十天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從廢除常獻祭，並設立那使地荒涼的可憎的像的時候起，必有一千二百九十天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從除掉常獻的燔祭，並設立那行毀壞可憎之物的時候，必有一千二百九十日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the time that the regular sacrifice is abolished and the abomination of desolation is set up, there will be 1,290 days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“From the time that the daily sacrifice is abolished and the abomination that causes desolation is set up, there will be 1,290 days."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“From the time the daily sacrifice is stopped and the sacrilegious object that causes desecration is set up to be worshiped, there will be 1,290 days."
      }
    },
