    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但以理啊，这些话要保密，要把这书封起来，直到末了。许多人必四处探究，因而知识必增多。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但以理啊！你要隱藏這些話，把這書密封，直到末期，必有許多人來往奔跑，知識增多。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但以理啊，你要隱藏這話，封閉這書，直到末時。必有多人來往奔跑，知識就必增長。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for you, Daniel, conceal these words and seal up the book until the end of time; many will go back and forth, and knowledge will increase.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you, Daniel, roll up and seal the words of the scroll until the time of the end. Many will go here and there to increase knowledge.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you, Daniel, keep this prophecy a secret; seal up the book until the time of the end, when many will rush here and there, and knowledge will increase.”"
      }
