    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我正祷告的时候，先前在异象中看见的那位加百列奉命疾飞而来。那是献晚祭的时候。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我還在禱告說話的時候，我先前在異象中所見的那位樣貌像人的加百列，約在獻晚祭的時候，快速地飛到我面前來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我正禱告的時候，先前在異象中所見的那位加百列，奉命迅速飛來，約在獻晚祭的時候，按手在我身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "while I was still speaking in prayer, then the man Gabriel, whom I had seen in the vision previously, came to me in my extreme weariness about the time of the evening offering."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "while I was still in prayer, Gabriel, the man I had seen in the earlier vision, came to me in swift flight about the time of the evening sacrifice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As I was praying, Gabriel, whom I had seen in the earlier vision, came swiftly to me at the time of the evening sacrifice."
      }
    },
