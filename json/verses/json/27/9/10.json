    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们没有听从我们的上帝耶和华的话，没有遵行祂借祂的仆人——众先知给我们颁布的律法。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們沒有聽從耶和華我們　神的話，沒有遵行他藉著他的僕人眾先知向我們頒布的律法。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也沒有聽從耶和華－我們神的話，沒有遵行他藉僕人眾先知向我們所陳明的律法。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "nor have we obeyed the voice of the Lord our God, to walk in His teachings which He set before us through His servants the prophets."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "we have not obeyed the Lord our God or kept the laws he gave us through his servants the prophets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We have not obeyed the Lord our God, for we have not followed the instructions he gave us through his servants the prophets."
      }
    },
