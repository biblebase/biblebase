    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人都违背你的律法，偏离正道，不听从你的话。你仆人摩西的律法书上所记载的咒诅和审判都落在了我们身上，因为我们得罪了你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列眾人都違背了你的律法，偏離了你，不聽從你的話，因此，　神的僕人摩西的律法書上所記載的咒詛和誓言，都傾倒在我們身上；我們實在得罪了　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列眾人都犯了你的律法，偏行，不聽從你的話；因此，在你僕人摩西律法上所寫的咒詛和誓言都傾在我們身上，因我們得罪了神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Indeed all Israel has transgressed Your law and turned aside, not obeying Your voice; so the curse has been poured out on us, along with the oath which is written in the law of Moses the servant of God, for we have sinned against Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All Israel has transgressed your law and turned away, refusing to obey you. “Therefore the curses and sworn judgments written in the Law of Moses, the servant of God, have been poured out on us, because we have sinned against you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All Israel has disobeyed your instruction and turned away, refusing to listen to your voice. “So now the solemn curses and judgments written in the Law of Moses, the servant of God, have been poured down on us because of our sin."
      }
    },
