    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "北方王必为所欲为，所向无敌。他必侵占佳美之地，手握毁灭之权。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那來攻擊南方王的，必任意而行；沒有人能在他面前站立得住；他必站在那榮美之地，手握毀壞之權。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "來攻擊他的，必任意而行，無人在北方王面前站立得住。他必站在那榮美之地，用手施行毀滅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But he who comes against him will do as he pleases, and no one will be able to withstand him; he will also stay for a time in the Beautiful Land, with destruction in his hand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The invader will do as he pleases; no one will be able to stand against him. He will establish himself in the Beautiful Land and will have the power to destroy it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The king of the north will march onward unopposed; none will be able to stop him. He will pause in the glorious land of Israel, intent on destroying it."
      }
    },
