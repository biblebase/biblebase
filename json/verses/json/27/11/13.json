    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为数年后，北方王必召集更庞大的军队，带着大量装备卷土重来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "北方王必再招聚大軍，比先前的更多；時機成熟的時候，他就率領裝備好了的大軍而來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「北方王必回來擺列大軍，比先前的更多。滿了所定的年數，他必率領大軍，帶極多的軍裝來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the king of the North will again raise a greater multitude than the former, and after an interval of some years he will press on with a great army and much equipment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the king of the North will muster another army, larger than the first; and after several years, he will advance with a huge army fully equipped."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“A few years later the king of the north will return with a fully equipped army far greater than before."
      }
    },
