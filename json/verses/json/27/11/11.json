    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "南方王必大怒，出来迎战北方王的大军，并击败他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "南方王必發大怒，出來和北方王交戰；北方王必擺列大軍，但這大軍必交在南方王的手裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "南方王必發烈怒，出來與北方王爭戰，擺列大軍；北方王的軍兵必交付他手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king of the South will be enraged and go forth and fight with the king of the North. Then the latter will raise a great multitude, but that multitude will be given into the hand of the former."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Then the king of the South will march out in a rage and fight against the king of the North, who will raise a large army, but it will be defeated."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then, in a rage, the king of the south will rally against the vast forces assembled by the king of the north and will defeat them."
      }
    },
