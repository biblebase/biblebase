    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他集中力量，鼓起勇气，率领大军进攻南方王。南方王也率领强大的军队迎战，却敌不过他，因为有人暗算南方王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必竭盡己力，鼓起勇氣，率領大軍攻擊南方王；南方王也必帶領極強大的軍隊奮起還擊，卻抵擋不住，因為有人設下計謀陷害南方王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「他必奮勇向前，率領大軍攻擊南方王；南方王也必以極大極強的軍兵與他爭戰，卻站立不住，因為有人設計謀害南方王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will stir up his strength and courage against the king of the South with a large army; so the king of the South will mobilize an extremely large and mighty army for war; but he will not stand, for schemes will be devised against him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“With a large army he will stir up his strength and courage against the king of the South. The king of the South will wage war with a large and very powerful army, but he will not be able to stand because of the plots devised against him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then he will stir up his courage and raise a great army against the king of the south. The king of the south will go to battle with a mighty army, but to no avail, for there will be plots against him."
      }
    },
