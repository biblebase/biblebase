    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他必侵入佳美之地，杀死成千上万的人，只有以东、摩押及亚扪人的首领得以逃脱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他也必侵入那榮美之地；必有許多國傾覆；但那些以東人、摩押人和亞捫人的首領，必被拯救脫離他的手。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又必進入那榮美之地，有許多國就被傾覆，但以東人、摩押人，和一大半亞捫人必脫離他的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will also enter the Beautiful Land, and many countries will fall; but these will be rescued out of his hand: Edom, Moab and the foremost of the sons of Ammon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will also invade the Beautiful Land. Many countries will fall, but Edom, Moab and the leaders of Ammon will be delivered from his hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will enter the glorious land of Israel, and many nations will fall, but Moab, Edom, and the best part of Ammon will escape."
      }
    },
