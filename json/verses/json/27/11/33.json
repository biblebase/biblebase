      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些智者必教导许多民众，但他们将在一段时间内被刀剑杀戮，或被烧死，或被掳去，或被抢掠。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "民中的智慧人必使許多人明白是非；但他們必有很多日子倒在刀劍之下，或被火燒，或被擄去，或被搶掠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "民間的智慧人必訓誨多人；然而他們多日必倒在刀下，或被火燒，或被擄掠搶奪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Those who have insight among the people will give understanding to the many; yet they will fall by sword and by flame, by captivity and by plunder for many days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Those who are wise will instruct many, though for a time they will fall by the sword or be burned or captured or plundered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Wise leaders will give instruction to many, but these teachers will die by fire and sword, or they will be jailed and robbed."
      }
