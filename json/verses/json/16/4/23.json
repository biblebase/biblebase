    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我和我的兄弟、仆人及跟从我的守卫都衣不解带，刀不离身。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，我與我的兄弟、我的僕人和跟隨我的人，都不脫衣服，各人帶自己的兵器去打水。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，我和弟兄僕人，並跟從我的護兵都不脫衣服，出去打水也帶兵器。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So neither I, my brothers, my servants, nor the men of the guard who followed me, none of us removed our clothes, each took his weapon even to the water."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Neither I nor my brothers nor my men nor the guards with me took off our clothes; each had his weapon, even when he went for water."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "During this time, none of us—not I, nor my relatives, nor my servants, nor the guards who were with me—ever took off our clothes. We carried our weapons with us at all times, even when we went for water."
      }
    },
