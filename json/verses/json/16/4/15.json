    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "敌人听说我们知道了他们的阴谋，上帝已破坏他们的诡计，就没有来。我们回到城墙那里，继续各人的工作。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們的仇敵既然聽見我們知道了這陰謀，　神也破壞了他們的計謀，就不敢來了。我們全都回到城牆那裡，各人回到原來的工作崗位。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "仇敵聽見我們知道他們的心意，見神也破壞他們的計謀，就不來了。我們都回到城牆那裏，各做各的工。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When our enemies heard that it was known to us, and that God had frustrated their plan, then all of us returned to the wall, each one to his work."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When our enemies heard that we were aware of their plot and that God had frustrated it, we all returned to the wall, each to our own work."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When our enemies heard that we knew of their plans and that God had frustrated them, we all returned to our work on the wall."
      }
    },
