    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "负责守卫库房的是玛他尼、八布迦、俄巴底亚、米书兰、达们和亚谷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪他尼、八布迦、俄巴底亞、米書蘭、達們和亞谷都是守門的，守衛城門的庫房。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瑪他尼、八布迦、俄巴底亞、米書蘭、達們、亞谷是守門的，就是在庫房那裏守門。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Mattaniah, Bakbukiah, Obadiah, Meshullam, Talmon and Akkub were gatekeepers keeping watch at the storehouses of the gates."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Mattaniah, Bakbukiah, Obadiah, Meshullam, Talmon and Akkub were gatekeepers who guarded the storerooms at the gates."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This included Mattaniah, Bakbukiah, and Obadiah. Meshullam, Talmon, and Akkub were the gatekeepers in charge of the storerooms at the gates."
      }
    },
