    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "远在大卫和亚萨的时代就有歌乐手，并有赞美、称颂上帝的诗歌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為遠在大衛和亞薩的時代，已有詩班長，並且有讚美稱頌　神的詩歌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "古時，在大衛和亞薩的日子，有歌唱的伶長，並有讚美稱謝神的詩歌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For in the days of David and Asaph, in ancient times, there were leaders of the singers, songs of praise and hymns of thanksgiving to God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For long ago, in the days of David and Asaph, there had been directors for the musicians and for the songs of praise and thanksgiving to God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The custom of having choir directors to lead the choirs in hymns of praise and thanksgiving to God began long ago in the days of David and Asaph."
      }
    },
