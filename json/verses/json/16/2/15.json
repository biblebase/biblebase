    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我就乘夜沿溪而上，察看城墙，然后转身取道谷门回城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我只得在夜間沿溪而上，視察城牆，然後轉回，經過谷門，回到城裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是夜間沿溪而上，察看城牆，又轉身進入谷門，就回來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I went up at night by the ravine and inspected the wall. Then I entered the Valley Gate again and returned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "so I went up the valley by night, examining the wall. Finally, I turned back and reentered through the Valley Gate."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So, though it was still dark, I went up the Kidron Valley instead, inspecting the wall before I turned back and entered again at the Valley Gate."
      }
    },
