    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以斯拉站在高处，他在全体会众面前一打开书卷，他们都站了起来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以斯拉在眾人眼前展開那書卷；他高高地站在眾民以上，他把書卷展開，眾民就站起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以斯拉站在眾民以上，在眾民眼前展開這書。他一展開，眾民就都站起來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ezra opened the book in the sight of all the people for he was standing above all the people; and when he opened it, all the people stood up."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ezra opened the book. All the people could see him because he was standing above them; and as he opened it, the people all stood up."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ezra stood on the platform in full view of all the people. When they saw him open the book, they all rose to their feet."
      }
    },
