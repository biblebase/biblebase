    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“天上的上帝耶和华啊，你是伟大而可畏的上帝，你向爱你、守你诫命的人守约，施慈爱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說：“耶和華天上的　神，偉大可畏的　神啊，你向那些愛你，遵守你誡命的人，守約施慈愛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「耶和華－天上的神，大而可畏的神啊，你向愛你、守你誡命的人守約施慈愛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I said, “I beseech You, O Lord God of heaven, the great and awesome God, who preserves the covenant and lovingkindness for those who love Him and keep His commandments,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I said: “Lord, the God of heaven, the great and awesome God, who keeps his covenant of love with those who love him and keep his commandments,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I said, “O Lord, God of heaven, the great and awesome God who keeps his covenant of unfailing love with those who love him and obey his commands,"
      }
    },
