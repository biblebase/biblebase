    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "白天，你用云柱带领他们；夜间，你用火柱照亮他们当走的路。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "日間你用雲柱引導他們， 夜間用火柱光照他們要行的路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且白晝用雲柱引導他們，黑夜用火柱照亮他們當行的路。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And with a pillar of cloud You led them by day, And with a pillar of fire by night To light for them the way In which they were to go."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "By day you led them with a pillar of cloud, and by night with a pillar of fire to give them light on the way they were to take."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You led our ancestors by a pillar of cloud during the day and a pillar of fire at night so that they could find their way."
      }
    },
