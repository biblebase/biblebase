    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在这临到我们的一切事上，你都是公义的，因为你行事信实，我们行事邪恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在一切臨到我們身上的事上，你都是公義的； 因為你所行的是信實的，我們所行的是邪惡的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在一切臨到我們的事上，你卻是公義的；因你所行的是誠實，我們所做的是邪惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "However, You are just in all that has come upon us; For You have dealt faithfully, but we have acted wickedly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In all that has happened to us, you have remained righteous; you have acted faithfully, while we acted wickedly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Every time you punished us you were being just. We have sinned greatly, and you gave us only what we deserved."
      }
    },
