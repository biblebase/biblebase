    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我就派人答复他说：“你所说的并非事实，是你心里捏造的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我就回覆他，說：“你說的這些事不是事實，都是你從心裡捏造出來的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我就差遣人去見他，說：「你所說的這事，一概沒有，是你心裏捏造的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I sent a message to him saying, “Such things as you are saying have not been done, but you are inventing them in your own mind.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I sent him this reply: “Nothing like what you are saying is happening; you are just making it up out of your head.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I replied, “There is no truth in any part of your story. You are making up the whole thing.”"
      }
    },
