    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我全心修筑城墙，没有购置田产，我的仆人也都参与修建城墙的工作。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我堅持只重修城牆的工程；所以我們不購置田產，我所有的僕人也都聚集在那裡工作。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且我恆心修造城牆，並沒有置買田地；我的僕人也都聚集在那裏做工。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I also applied myself to the work on this wall; we did not buy any land, and all my servants were gathered there for the work."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Instead, I devoted myself to the work on this wall. All my men were assembled there for the work; we did not acquire any land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I also devoted myself to working on the wall and refused to acquire any land. And I required all my servants to spend time working on the wall."
      }
    },
