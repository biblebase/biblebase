    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他又拆毁耶和华的殿内男庙妓的房屋，就是妇女为亚舍拉编织帐幔的地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他拆毀在耶和華殿內男性廟妓的房屋，就是婦女為亞舍拉編織袍子的地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又拆毀耶和華殿裏孌童的屋子，就是婦女為亞舍拉織帳子的屋子，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also broke down the houses of the male cult prostitutes which were in the house of the Lord, where the women were weaving hangings for the Asherah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He also tore down the quarters of the male shrine prostitutes that were in the temple of the Lord, the quarters where women did weaving for Asherah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He also tore down the living quarters of the male and female shrine prostitutes that were inside the Temple of the Lord, where the women wove coverings for the Asherah pole."
      }
    },
