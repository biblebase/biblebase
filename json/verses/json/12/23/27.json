    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“我要像驱逐以色列人一样将犹大人从我面前赶走。我要撇弃我所拣选的耶路撒冷和我名常在的殿。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：“我也要把猶大從我面前趕走，好像我把以色列除去一樣。我要丟棄我所揀選的城市耶路撒冷，以及聖殿，我曾說：‘我的名必立在那裡。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「我必將猶大人從我面前趕出，如同趕出以色列人一般；我必棄掉我從前所選擇的這城－耶路撒冷和我所說立我名的殿。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord said, “I will remove Judah also from My sight, as I have removed Israel. And I will cast off Jerusalem, this city which I have chosen, and the temple of which I said, ‘My name shall be there.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the Lord said, “I will remove Judah also from my presence as I removed Israel, and I will reject Jerusalem, the city I chose, and this temple, about which I said, ‘My Name shall be there.’”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Lord said, “I will also banish Judah from my presence just as I have banished Israel. And I will reject my chosen city of Jerusalem and the Temple where my name was to be honored.”"
      }
    },
