    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "哈薛辞别以利沙回去了。王问他：“以利沙对你说什么？”他答道：“以利沙告诉我，你一定会痊愈。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他離開以利沙，回到他主人那裡。便．哈達對他說：“以利沙對你說了些甚麼呢？”他說：“他對我說：‘你一定痊愈的。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "哈薛離開以利沙，回去見他的主人。主人問他說：「以利沙對你說甚麼？」回答說：「他告訴我你必能好。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he departed from Elisha and returned to his master, who said to him, “What did Elisha say to you?” And he answered, “He told me that you would surely recover.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Hazael left Elisha and returned to his master. When Ben-Hadad asked, “What did Elisha say to you?” Hazael replied, “He told me that you would certainly recover.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Hazael left Elisha and went back, the king asked him, “What did Elisha tell you?” And Hazael replied, “He told me that you will surely recover.”"
      }
    },
