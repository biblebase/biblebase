    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列王叫道：“唉！怎么办呢？耶和华招聚我们三王，是要把我们交给摩押王！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列王說：“哎喲，耶和華招聚這三位君王，是要把他們交在摩押人的手中。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列王說：「哀哉！耶和華招聚我們這三王，乃要交在摩押人的手裏。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king of Israel said, “Alas! For the Lord has called these three kings to give them into the hand of Moab.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“What!” exclaimed the king of Israel. “Has the Lord called us three kings together only to deliver us into the hands of Moab?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What should we do?” the king of Israel cried out. “The Lord has brought the three of us here to let the king of Moab defeat us.”"
      }
    },
