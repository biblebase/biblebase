    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押王米沙以牧羊为业，每年向以色列王进贡十万只公绵羊的羊毛和十万只羊羔。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩押王米沙是個以牧羊為生的人。他每年進貢十萬頭綿羊羔和十萬頭公羊羔給以色列王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩押王米沙牧養許多羊，每年將十萬羊羔的毛和十萬公綿羊的毛給以色列王進貢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Mesha king of Moab was a sheep breeder, and used to pay the king of Israel 100,000 lambs and the wool of 100,000 rams."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now Mesha king of Moab raised sheep, and he had to pay the king of Israel a tribute of a hundred thousand lambs and the wool of a hundred thousand rams."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King Mesha of Moab was a sheep breeder. He used to pay the king of Israel an annual tribute of 100,000 lambs and the wool of 100,000 rams."
      }
    },
