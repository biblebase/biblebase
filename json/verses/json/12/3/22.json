    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第二天早晨，日光照在水面上，摩押人起来后，看见对面的水像血一样红，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "第二天早上，他們起來的時候，太陽照在水上，摩押人從那邊看見水紅得像血一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "次日早晨，日光照在水上，摩押人起來，看見對面水紅如血，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They rose early in the morning, and the sun shone on the water, and the Moabites saw the water opposite them as red as blood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they got up early in the morning, the sun was shining on the water. To the Moabites across the way, the water looked red—like blood."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when they got up the next morning, the sun was shining across the water, making it appear red to the Moabites—like blood."
      }
    },
