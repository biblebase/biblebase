    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押王见战事不利，就率领七百名刀兵，企图从以东王那边突围，但没有成功。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩押王見戰事激烈，無法抵抗，於是帶領七百持刀的兵，想要突圍到以東王那裡，可是不成功，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩押王見陣勢甚大，難以對敵，就率領七百拿刀的兵，要衝過陣去到以東王那裏，卻是不能；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the king of Moab saw that the battle was too fierce for him, he took with him 700 men who drew swords, to break through to the king of Edom; but they could not."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the king of Moab saw that the battle had gone against him, he took with him seven hundred swordsmen to break through to the king of Edom, but they failed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the king of Moab saw that he was losing the battle, he led 700 of his swordsmen in a desperate attempt to break through the enemy lines near the king of Edom, but they failed."
      }
    },
