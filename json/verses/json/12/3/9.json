    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，以色列王跟犹大王和以东王联合出兵。他们绕道行了七天，军队和随行的牲畜都没有水喝了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是以色列王、猶大王和以東王，繞道行了七日的路程。軍隊和隨行的牲畜都沒有水喝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，以色列王和猶大王，並以東王，都一同去繞行七日的路程；軍隊和所帶的牲畜沒有水喝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the king of Israel went with the king of Judah and the king of Edom; and they made a circuit of seven days’ journey, and there was no water for the army or for the cattle that followed them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the king of Israel set out with the king of Judah and the king of Edom. After a roundabout march of seven days, the army had no more water for themselves or for the animals with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king of Edom and his troops joined them, and all three armies traveled along a roundabout route through the wilderness for seven days. But there was no water for the men or their animals."
      }
    },
