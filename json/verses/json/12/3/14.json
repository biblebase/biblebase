    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利沙说：“我事奉的是永活的万军之耶和华，我凭祂起誓，若不是看犹大王约沙法的情面，我决不理你，也不会见你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以利沙說：“我指著我所事奉永活的萬軍耶和華起誓，若不是看猶大王約沙法的情面，我根本不會理你，看也不看你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利沙說：「我指着所事奉永生的萬軍耶和華起誓，我若不看猶大王約沙法的情面，必不理你，不顧你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Elisha said, “As the Lord of hosts lives, before whom I stand, were it not that I regard the presence of Jehoshaphat the king of Judah, I would not look at you nor see you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Elisha said, “As surely as the Lord Almighty lives, whom I serve, if I did not have respect for the presence of Jehoshaphat king of Judah, I would not pay any attention to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Elisha replied, “As surely as the Lord Almighty lives, whom I serve, I wouldn’t even bother with you except for my respect for King Jehoshaphat of Judah."
      }
    },
