    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她叫来丈夫，对他说：“请你预备一个仆人和一头驴，我好赶快去见上帝的仆人，然后返回来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她呼喚她的丈夫，說：“請你派一個僕人和一頭驢子給我。我要趕到神人那裡，立刻就回來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "呼叫她丈夫說：「你叫一個僕人給我牽一匹驢來，我要快快地去見神人，就回來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then she called to her husband and said, “Please send me one of the servants and one of the donkeys, that I may run to the man of God and return.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She called her husband and said, “Please send me one of the servants and a donkey so I can go to the man of God quickly and return.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She sent a message to her husband: “Send one of the servants and a donkey so that I can hurry to the man of God and come right back.”"
      }
    },
