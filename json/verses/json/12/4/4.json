    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后回家关上门，你和你儿子在屋里把那瓶油倒在所有借来的器皿中，装满的放在一边。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後回去，把你和你兒子關在家裡，你要把油倒在所有器皿裡面，裝滿了的就放在一邊。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "回到家裏，關上門，你和你兒子在裏面將油倒在所有的器皿裏，倒滿了的放在一邊。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And you shall go in and shut the door behind you and your sons, and pour out into all these vessels, and you shall set aside what is full.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then go inside and shut the door behind you and your sons. Pour oil into all the jars, and as each is filled, put it to one side.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then go into your house with your sons and shut the door behind you. Pour olive oil from your flask into the jars, setting each one aside when it is filled.”"
      }
    },
