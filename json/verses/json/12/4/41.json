    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利沙说：“拿点面粉来。”他把面粉撒在锅里，说：“倒出来给众人喝吧。”于是，汤里没有毒了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“拿些麵給我。”他把麵放在鍋中，說：“倒出來給眾人吃吧。”鍋裡就沒有毒了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利沙說：「拿點麵來」，就把麵撒在鍋中，說：「倒出來，給眾人吃吧！」鍋中就沒有毒了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But he said, “Now bring meal.” He threw it into the pot and said, “Pour it out for the people that they may eat.” Then there was no harm in the pot."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Elisha said, “Get some flour.” He put it into the pot and said, “Serve it to the people to eat.” And there was nothing harmful in the pot."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Elisha said, “Bring me some flour.” Then he threw it into the pot and said, “Now it’s all right; go ahead and eat.” And then it did not harm them."
      }
    },
