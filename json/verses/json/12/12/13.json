    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "献到耶和华殿里的银子没有用于制造殿里的银碗、蜡剪、盆、号及其他金银器皿，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但耶和華殿裡的銀杯、燭剪、碗、號或任何金銀器皿，都沒有用奉到耶和華殿裡的銀子來做。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但那奉到耶和華殿的銀子，沒有用以做耶和華殿裏的銀杯、蠟剪、碗、號，和別樣的金銀器皿，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But there were not made for the house of the Lord silver cups, snuffers, bowls, trumpets, any vessels of gold, or vessels of silver from the money which was brought into the house of the Lord;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The money brought into the temple was not spent for making silver basins, wick trimmers, sprinkling bowls, trumpets or any other articles of gold or silver for the temple of the Lord;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The money brought to the Temple was not used for making silver bowls, lamp snuffers, basins, trumpets, or other articles of gold or silver for the Temple of the Lord."
      }
    },
