    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祷告说：“坐在二基路伯天使之上、以色列的上帝耶和华啊，唯有你是天下万国的上帝，你创造了天地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希西家在耶和華面前禱告，說：“耶和華以色列的　神，坐在二基路伯中間的啊，只有你是地上萬國的　神，天地是你創造的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希西家向耶和華禱告說：「坐在二基路伯上耶和華－以色列的神啊，你是天下萬國的神，你曾創造天地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hezekiah prayed before the Lord and said, “O Lord, the God of Israel, who are enthroned above the cherubim, You are the God, You alone, of all the kingdoms of the earth. You have made heaven and earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And Hezekiah prayed to the Lord: “Lord, the God of Israel, enthroned between the cherubim, you alone are God over all the kingdoms of the earth. You have made heaven and earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And Hezekiah prayed this prayer before the Lord: “O Lord, God of Israel, you are enthroned between the mighty cherubim! You alone are God of all the kingdoms of the earth. You alone created the heavens and the earth."
      }
    },
