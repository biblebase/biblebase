    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王又问他们：“在路上截住你们、对你们说这些话的，是什么样的人？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王對他們說：“那上來迎著你們，又對你們說這番話的是個怎樣的人？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王問他們說：「迎着你們來告訴你們這話的，是怎樣的人？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to them, “What kind of man was he who came up to meet you and spoke these words to you?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king asked them, “What kind of man was it who came to meet you and told you this?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What sort of man was he?” the king demanded. “What did he look like?”"
      }
