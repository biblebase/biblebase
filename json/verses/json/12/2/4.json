    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利亚对以利沙说：“你留在这里吧，耶和华差遣我去耶利哥。”以利沙说：“我凭永活的耶和华和你的性命起誓，我决不离开你。”于是二人前往耶利哥。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以利亞對以利沙說：“你留在這裡吧，因為耶和華差派我往耶利哥。”他說：“我指著永活的耶和華和你的性命起誓：我必不離開你。”於是他們來到耶利哥。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利亞對以利沙說：「耶和華差遣我往耶利哥去，你可以在這裏等候。」以利沙說：「我指着永生的耶和華，又敢在你面前起誓，我必不離開你。」於是二人到了耶利哥。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Elijah said to him, “Elisha, please stay here, for the Lord has sent me to Jericho.” But he said, “As the Lord lives, and as you yourself live, I will not leave you.” So they came to Jericho."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Elijah said to him, “Stay here, Elisha; the Lord has sent me to Jericho.” And he replied, “As surely as the Lord lives and as you live, I will not leave you.” So they went to Jericho."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Elijah said to Elisha, “Stay here, for the Lord has told me to go to Jericho.” But Elisha replied again, “As surely as the Lord lives and you yourself live, I will never leave you.” So they went on together to Jericho."
      }
    },
