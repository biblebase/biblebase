    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们回来的时候，以利沙仍在耶利哥。他对他们说：“我不是早就告诉你们不要去吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他們回到以利沙那裡；那時，他還停留在耶利哥，他對他們說：“我不是告訴了你們不必去的嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利沙仍然在耶利哥等候他們回到他那裏；他對他們說：「我豈沒有告訴你們不必去嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They returned to him while he was staying at Jericho; and he said to them, “Did I not say to you, ‘Do not go’?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they returned to Elisha, who was staying in Jericho, he said to them, “Didn’t I tell you not to go?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Elisha was still at Jericho when they returned. “Didn’t I tell you not to go?” he asked."
      }
    },
