    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们说：“我们不知道，你告诉我们吧！”耶户说：“他告诉我，耶和华说膏立我做以色列王。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們說：“謊話！請你告訴我們吧。”他說：“他這樣對我說：‘耶和華這樣說：我已經膏立你作王統治以色列。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們說：「這是假話，你據實地告訴我們。」回答說：「他如此如此對我說。他說：『耶和華如此說：我膏你作以色列王。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said, “It is a lie, tell us now.” And he said, “Thus and thus he said to me, ‘Thus says the Lord, “I have anointed you king over Israel.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“That’s not true!” they said. “Tell us.” Jehu said, “Here is what he told me: ‘This is what the Lord says: I anoint you king over Israel.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You’re hiding something,” they said. “Tell us.” So Jehu told them, “He said to me, ‘This is what the Lord says: I have anointed you to be king over Israel.’”"
      }
    },
