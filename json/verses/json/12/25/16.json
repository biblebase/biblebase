    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门为耶和华的殿所造的两根铜柱、一个铜海和一些盆座，用的铜多得无法计算。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門為耶和華殿所做的兩根銅柱、一個銅海和十個銅座，這一切器皿的銅，重得無法可稱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門為耶和華殿所造的兩根銅柱、一個銅海，和幾個盆座，這一切的銅，多得無法可稱。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The two pillars, the one sea, and the stands which Solomon had made for the house of the Lord—the bronze of all these vessels was beyond weight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The bronze from the two pillars, the Sea and the movable stands, which Solomon had made for the temple of the Lord, was more than could be weighed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The weight of the bronze from the two pillars, the Sea, and the water carts was too great to be measured. These things had been made for the Lord’s Temple in the days of Solomon."
      }
    },
