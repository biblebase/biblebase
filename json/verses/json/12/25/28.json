    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并好言相待，使他的地位高过被掳到巴比伦的其他各王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "並且安慰他，使他的地位高過和他一起在巴比倫的眾王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又對他說恩言，使他的位高過與他一同在巴比倫眾王的位，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and he spoke kindly to him and set his throne above the throne of the kings who were with him in Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He spoke kindly to him and gave him a seat of honor higher than those of the other kings who were with him in Babylon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He spoke kindly to Jehoiachin and gave him a higher place than all the other exiled kings in Babylon."
      }
    },
