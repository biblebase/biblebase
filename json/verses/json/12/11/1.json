      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚哈谢的母亲亚她利雅见儿子死了，便决定除掉王室后裔。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞哈謝的母親亞他利雅看見自己的兒子死了，就起來消滅王室所有的後裔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈謝的母親亞她利雅見她兒子死了，就起來剿滅王室。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Athaliah the mother of Ahaziah saw that her son was dead, she rose and destroyed all the royal offspring."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Athaliah the mother of Ahaziah saw that her son was dead, she proceeded to destroy the whole royal family."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Athaliah, the mother of King Ahaziah of Judah, learned that her son was dead, she began to destroy the rest of the royal family."
      }
