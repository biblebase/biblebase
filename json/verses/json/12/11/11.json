    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从殿右到殿左，在祭坛和殿周围，卫兵都手持兵器护卫王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "各衛兵手持兵器，從殿的右面到殿的左面，面對祭壇和殿，圍繞著王侍立。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "護衛兵手中各拿兵器，在壇和殿那裏，從殿右直到殿左，站在王子的四圍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The guards stood each with his weapons in his hand, from the right side of the house to the left side of the house, by the altar and by the house, around the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The guards, each with weapon in hand, stationed themselves around the king—near the altar and the temple, from the south side to the north side of the temple."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The palace guards stationed themselves around the king, with their weapons ready. They formed a line from the south side of the Temple around to the north side and all around the altar."
      }
    },
