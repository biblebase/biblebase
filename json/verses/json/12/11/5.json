    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶何耶大吩咐他们说：“你们在安息日值班的，三分之一要守卫王宫，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又吩咐他們說：“你們要這樣作：你們在安息日值班的，三分之一要守衛王宮；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "吩咐他們說：「你們當這樣行：凡安息日進班的三分之一要看守王宮，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He commanded them, saying, “This is the thing that you shall do: one third of you, who come in on the sabbath and keep watch over the king’s house"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He commanded them, saying, “This is what you are to do: You who are in the three companies that are going on duty on the Sabbath—a third of you guarding the royal palace,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jehoiada told them, “This is what you must do. A third of you who are on duty on the Sabbath are to guard the royal palace itself."
      }
    },
