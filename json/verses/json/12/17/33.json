    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们就这样既敬拜耶和华，又各按各的风俗供奉自己的神明。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們既懼怕耶和華，又服事他們的神；從哪個民族擄來的，就按照哪個民族的習俗服事他們的神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們又懼怕耶和華，又事奉自己的神，從何邦遷移，就隨何邦的風俗。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They feared the Lord and served their own gods according to the custom of the nations from among whom they had been carried away into exile."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They worshiped the Lord, but they also served their own gods in accordance with the customs of the nations from which they had been brought."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And though they worshiped the Lord, they continued to follow their own gods according to the religious customs of the nations from which they came."
      }
    },
