    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们也敬拜耶和华，却委任各种人做祭司，在高岗上的神庙为他们献祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們也懼怕耶和華，所以從他們中間選立邱壇的祭司，替他們在邱壇的廟裡獻祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們懼怕耶和華，也從他們中間立邱壇的祭司，為他們在有邱壇的殿中獻祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They also feared the Lord and appointed from among themselves priests of the high places, who acted for them in the houses of the high places."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They worshiped the Lord, but they also appointed all sorts of their own people to officiate for them as priests in the shrines at the high places."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These new residents worshiped the Lord, but they also appointed from among themselves all sorts of people as priests to offer sacrifices at their places of worship."
      }
