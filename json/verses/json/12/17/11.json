    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在各丘坛烧香，如同耶和华在他们面前赶走的外族人一样。他们邪恶的行为激怒了耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在各邱壇上獻祭和焚香，好像耶和華從他們的面前擄去的外族人一樣，又行惡事，使耶和華發怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在邱壇上燒香，效法耶和華在他們面前趕出的外邦人所行的，又行惡事惹動耶和華的怒氣；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and there they burned incense on all the high places as the nations did which the Lord had carried away to exile before them; and they did evil things provoking the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At every high place they burned incense, as the nations whom the Lord had driven out before them had done. They did wicked things that aroused the Lord’s anger."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They offered sacrifices on all the hilltops, just like the nations the Lord had driven from the land ahead of them. So the people of Israel had done many evil things, arousing the Lord’s anger."
      }
    },
