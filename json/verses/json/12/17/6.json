    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "何细亚执政第九年，撒玛利亚沦陷。亚述王把以色列人掳到亚述，安置在哈腊、歌散的哈博河一带以及玛代人的各城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "何細亞在位第九年，亞述王佔領了撒瑪利亞，把以色列人擄到亞述去，安置他們在哈臘，在哈博河和歌散河一帶，以及在瑪代人的各城。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "何細亞第九年亞述王攻取了撒馬利亞，將以色列人擄到亞述，把他們安置在哈臘與歌散的哈博河邊，並米底亞人的城邑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In the ninth year of Hoshea, the king of Assyria captured Samaria and carried Israel away into exile to Assyria, and settled them in Halah and Habor, on the river of Gozan, and in the cities of the Medes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the ninth year of Hoshea, the king of Assyria captured Samaria and deported the Israelites to Assyria. He settled them in Halah, in Gozan on the Habor River and in the towns of the Medes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Finally, in the ninth year of King Hoshea’s reign, Samaria fell, and the people of Israel were exiled to Assyria. They were settled in colonies in Halah, along the banks of the Habor River in Gozan, and in the cities of the Medes."
      }
    },
