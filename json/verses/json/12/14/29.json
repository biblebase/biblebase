    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶罗波安与祖先——以色列的先王们同眠后，他儿子撒迦利雅继位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶羅波安和他的祖先以色列諸王一同長眠。他的兒子撒迦利雅就接續他作王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶羅波安與他列祖以色列諸王同睡。他兒子撒迦利雅接續他作王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jeroboam slept with his fathers, even with the kings of Israel, and Zechariah his son became king in his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jeroboam rested with his ancestors, the kings of Israel. And Zechariah his son succeeded him as king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jeroboam II died, he was buried in Samaria with the kings of Israel. Then his son Zechariah became the next king."
      }
    },
