    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他把献给耶和华的铜坛从耶和华的殿前搬到新坛的北边，铜坛原来放在殿和新坛之间。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於耶和華面前的銅祭壇，他把它從殿前，從新壇與耶和華殿中間移至新壇的北邊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又將耶和華面前的銅壇從耶和華殿和新壇的中間搬到新壇的北邊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The bronze altar, which was before the Lord, he brought from the front of the house, from between his altar and the house of the Lord, and he put it on the north side of his altar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As for the bronze altar that stood before the Lord, he brought it from the front of the temple—from between the new altar and the temple of the Lord—and put it on the north side of the new altar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then King Ahaz removed the old bronze altar from its place in front of the Lord’s Temple, between the entrance and the new altar, and placed it on the north side of the new altar."
      }
    },
