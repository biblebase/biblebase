    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚哈斯王在大马士革见亚述王提革拉·毗列色的时候，看到一座坛，便派人将坛的模型和详细的制造方法送到乌利亚祭司那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞哈斯王到大馬士革會見亞述王提革拉．毘列色，看見在大馬士革的祭壇。亞哈斯王就派人把祭壇的圖樣、結構和詳盡的做法送到烏利亞祭司那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈斯王上大馬士革去迎接亞述王提革拉‧毗列色，在大馬士革看見一座壇，就照壇的規模樣式作法畫了圖樣，送到祭司烏利亞那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now King Ahaz went to Damascus to meet Tiglath-pileser king of Assyria, and saw the altar which was at Damascus; and King Ahaz sent to Urijah the priest the pattern of the altar and its model, according to all its workmanship."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then King Ahaz went to Damascus to meet Tiglath-Pileser king of Assyria. He saw an altar in Damascus and sent to Uriah the priest a sketch of the altar, with detailed plans for its construction."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King Ahaz then went to Damascus to meet with King Tiglath-pileser of Assyria. While he was there, he took special note of the altar. Then he sent a model of the altar to Uriah the priest, along with its design in full detail."
      }
    },
