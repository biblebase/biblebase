    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为了亚述王，他挪走耶和华殿里为安息日而盖的走廊，封闭了王从外面进耶和华殿的入口。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又因亞述王的緣故，他把耶和華殿裡為安息日而建造的廊子，和王從外邊進殿的廊子，從耶和華的殿內拆除。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又因亞述王的緣故，將耶和華殿為安息日所蓋的廊子和王從外入殿的廊子挪移，圍繞耶和華的殿。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The covered way for the sabbath which they had built in the house, and the outer entry of the king, he removed from the house of the Lord because of the king of Assyria."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He took away the Sabbath canopy that had been built at the temple and removed the royal entryway outside the temple of the Lord, in deference to the king of Assyria."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In deference to the king of Assyria, he also removed the canopy that had been constructed inside the palace for use on the Sabbath day, as well as the king’s outer entrance to the Temple of the Lord."
      }
    },
