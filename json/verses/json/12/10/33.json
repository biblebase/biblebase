    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "侵占了约旦河以东的土地，包括基列全境，即迦得人、吕便人和玛拿西人住的地方，从亚嫩谷附近的亚罗珥起，直到基列和巴珊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從約旦河以東，基列的全地，迦得人、流本人和瑪拿西人之地，從靠近亞嫩河的亞羅珥起，至基列和巴珊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "乃是約旦河東、基列全地，從靠近亞嫩谷邊的亞羅珥起，就是基列和巴珊的迦得人、呂便人、瑪拿西人之地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "from the Jordan eastward, all the land of Gilead, the Gadites and the Reubenites and the Manassites, from Aroer, which is by the valley of the Arnon, even Gilead and Bashan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "east of the Jordan in all the land of Gilead (the region of Gad, Reuben and Manasseh), from Aroer by the Arnon Gorge through Gilead to Bashan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "east of the Jordan River, including all of Gilead, Gad, Reuben, and Manasseh. He conquered the area from the town of Aroer by the Arnon Gorge to as far north as Gilead and Bashan."
      }
    },
