    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶户召集了所有的人，对他们说：“亚哈供奉巴力还不够热心，我耶户要更热心地供奉他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶戶集合眾民，對他們說：“從前亞哈信奉巴力還算是少，耶戶要信奉更多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶戶招聚眾民，對他們說：「亞哈事奉巴力還冷淡，耶戶卻更熱心。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jehu gathered all the people and said to them, “Ahab served Baal a little; Jehu will serve him much."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jehu brought all the people together and said to them, “Ahab served Baal a little; Jehu will serve him much."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jehu called a meeting of all the people of the city and said to them, “Ahab’s worship of Baal was nothing compared to the way I will worship him!"
      }
    },
