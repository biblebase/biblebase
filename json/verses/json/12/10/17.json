    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到了撒玛利亚，他把亚哈家在撒玛利亚剩下的人全部杀光，一个不留，应验了耶和华对以利亚说的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他來到撒瑪利亞，就擊殺亞哈家所有在撒瑪利亞剩下的人，把他們完全消滅，正如耶和華對以利亞所說的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了撒馬利亞，就把撒馬利亞的亞哈家剩下的人都殺了，直到滅盡，正如耶和華對以利亞所說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he came to Samaria, he killed all who remained to Ahab in Samaria, until he had destroyed him, according to the word of the Lord which He spoke to Elijah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jehu came to Samaria, he killed all who were left there of Ahab’s family; he destroyed them, according to the word of the Lord spoken to Elijah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jehu arrived in Samaria, he killed everyone who was left there from Ahab’s family, just as the Lord had promised through Elijah."
      }
    },
