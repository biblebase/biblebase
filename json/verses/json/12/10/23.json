    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后他和利甲的儿子约拿达进庙向他们宣布说：“你们仔细察看，这里不可有事奉耶和华的人，只允许拜巴力的人在这里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶戶和利甲的兒子約拿達來到巴力的廟中。他對信奉巴力的人說：“搜查看看，這裡不要有信奉耶和華的人和你們同在一起，只可以有信奉巴力的人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶戶和利甲的兒子約拿達進了巴力廟，對拜巴力的人說：「你們察看察看，在你們這裏不可有耶和華的僕人，只可容留拜巴力的人。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jehu went into the house of Baal with Jehonadab the son of Rechab; and he said to the worshipers of Baal, “Search and see that there is here with you none of the servants of the Lord, but only the worshipers of Baal.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jehu and Jehonadab son of Rekab went into the temple of Baal. Jehu said to the servants of Baal, “Look around and see that no one who serves the Lord is here with you—only servants of Baal.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jehu went into the temple of Baal with Jehonadab son of Recab. Jehu said to the worshipers of Baal, “Make sure no one who worships the Lord is here—only those who worship Baal.”"
      }
    },
