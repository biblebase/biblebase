    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这些显贵们接到信后，便杀了亚哈的七十个子孙，把他们的首级放在篮子里，送到耶斯列的耶户那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這信到達他們那裡的時候，他們就拿住王的眾子，把這七十人殺了，然後把他們的頭放在籃子裡，送到耶斯列耶戶那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "信一到，他們就把王的七十個兒子殺了，將首級裝在筐裏，送到在耶斯列的耶戶那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the letter came to them, they took the king’s sons and slaughtered them, seventy persons, and put their heads in baskets, and sent them to him at Jezreel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the letter arrived, these men took the princes and slaughtered all seventy of them. They put their heads in baskets and sent them to Jehu in Jezreel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the letter arrived, the leaders killed all seventy of the king’s sons. They placed their heads in baskets and presented them to Jehu at Jezreel."
      }
    },
