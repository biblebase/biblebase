    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要撇弃我残存的子民，把他们交给敌人，任其掳掠。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要捨棄我的產業的餘民，把他們交在他們的仇敵手上；他們要成為他們的仇敵的掠物和戰利品。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必棄掉所餘剩的子民，把他們交在仇敵手中，使他們成為一切仇敵擄掠之物；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will abandon the remnant of My inheritance and deliver them into the hand of their enemies, and they will become as plunder and spoil to all their enemies;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will forsake the remnant of my inheritance and give them into the hands of enemies. They will be looted and plundered by all their enemies;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I will reject even the remnant of my own people who are left, and I will hand them over as plunder for their enemies."
      }
    },
