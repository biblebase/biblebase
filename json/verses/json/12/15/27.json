    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大王亚撒利雅执政第五十二年，利玛利的儿子比加在撒玛利亚登基做以色列王，执政二十年。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大王亞撒利雅在位第五十二年，利瑪利的兒子比加在撒瑪利亞作王統治以色列共二十年。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大王亞撒利雅五十二年，利瑪利的兒子比加在撒馬利亞登基作以色列王二十年。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In the fifty-second year of Azariah king of Judah, Pekah son of Remaliah became king over Israel in Samaria, and reigned twenty years."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the fifty-second year of Azariah king of Judah, Pekah son of Remaliah became king of Israel in Samaria, and he reigned twenty years."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Pekah son of Remaliah began to rule over Israel in the fifty-second year of King Uzziah’s reign in Judah. He reigned in Samaria twenty years."
      }
    },
