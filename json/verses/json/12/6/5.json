    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有个人砍树的时候，斧头掉进了水里。他大喊：“唉！师傅，这把斧头是借来的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一個門徒砍伐木頭的時候，斧頭掉到水裡去了。他就喊著說：“慘啦！我主啊，這斧頭是借回來的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一人砍樹的時候，斧頭掉在水裏，他就呼叫說：「哀哉！我主啊，這斧子是借的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as one was felling a beam, the axe head fell into the water; and he cried out and said, “Alas, my master! For it was borrowed.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As one of them was cutting down a tree, the iron axhead fell into the water. “Oh no, my lord!” he cried out. “It was borrowed!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But as one of them was cutting a tree, his ax head fell into the river. “Oh, sir!” he cried. “It was a borrowed ax!”"
      }
    },
