    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "话音未落，使者已经到了，说：“既然这祸是从耶和华来的，我何必再仰望祂呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他和眾長老還在說話的時候，看哪，王就下到他那裡，說：“看哪，這災禍是從耶和華來的，我為甚麼還仰望耶和華呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正說話的時候，使者來到，王也到了，說：「這災禍是從耶和華那裏來的，我何必再仰望耶和華呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While he was still talking with them, behold, the messenger came down to him and he said, “Behold, this evil is from the Lord; why should I wait for the Lord any longer?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While he was still talking to them, the messenger came down to him. The king said, “This disaster is from the Lord. Why should I wait for the Lord any longer?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "While Elisha was still saying this, the messenger arrived. And the king said, “All this misery is from the Lord! Why should I wait for the Lord any longer?”"
      }
    },
