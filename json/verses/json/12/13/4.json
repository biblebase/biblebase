    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约哈斯求耶和华开恩，耶和华看见亚兰王非常残暴地欺压以色列人，便垂听他的祷告，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約哈斯懇求耶和華，耶和華垂聽他的禱告，因為看見了以色列所受的壓迫，就是亞蘭王對他們的壓迫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約哈斯懇求耶和華，耶和華就應允他，因為見以色列人所受亞蘭王的欺壓。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jehoahaz entreated the favor of the Lord, and the Lord listened to him; for He saw the oppression of Israel, how the king of Aram oppressed them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jehoahaz sought the Lord’s favor, and the Lord listened to him, for he saw how severely the king of Aram was oppressing Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jehoahaz prayed for the Lord’s help, and the Lord heard his prayer, for he could see how severely the king of Aram was oppressing Israel."
      }
    },
