    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约阿施其他的事及其一切所作所为，包括他英勇对抗犹大王亚玛谢的事都记在以色列的列王史上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約阿施其餘的事蹟和他所行的一切，以及他與猶大王亞瑪謝作戰時的英勇事蹟，不是都寫在以色列諸王的年代誌上嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約阿施其餘的事，凡他所行的和他與猶大王亞瑪謝爭戰的勇力，都寫在以色列諸王記上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the rest of the acts of Joash and all that he did and his might with which he fought against Amaziah king of Judah, are they not written in the Book of the Chronicles of the Kings of Israel?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As for the other events of the reign of Jehoash, all he did and his achievements, including his war against Amaziah king of Judah, are they not written in the book of the annals of the kings of Israel?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The rest of the events in Jehoash’s reign and everything he did, including the extent of his power and his war with King Amaziah of Judah, are recorded in The Book of the History of the Kings of Israel."
      }
    },
