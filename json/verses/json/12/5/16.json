    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利沙说：“我凭我事奉的永活上帝起誓，我决不接受。”乃缦再三请求，他仍不肯接受。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以利沙說：“我指著我所事奉永活的耶和華起誓，我決不收取任何禮物。”乃縵再三促請他接受，都被他拒絕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利沙說：「我指着所事奉永生的耶和華起誓，我必不受。」乃縵再三地求他，他卻不受。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But he said, “As the Lord lives, before whom I stand, I will take nothing.” And he urged him to take it, but he refused."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The prophet answered, “As surely as the Lord lives, whom I serve, I will not accept a thing.” And even though Naaman urged him, he refused."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Elisha replied, “As surely as the Lord lives, whom I serve, I will not accept any gifts.” And though Naaman urged him to take the gift, Elisha refused."
      }
