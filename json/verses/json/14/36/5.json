    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约雅敬二十五岁登基，在耶路撒冷执政十一年。他做他的上帝耶和华视为恶的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約雅敬登基的時候，是二十五歲；他在耶路撒冷作王共十一年，行耶和華他的　神看為惡的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約雅敬登基的時候年二十五歲，在耶路撒冷作王十一年，行耶和華－他神眼中看為惡的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jehoiakim was twenty-five years old when he became king, and he reigned eleven years in Jerusalem; and he did evil in the sight of the Lord his God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jehoiakim was twenty-five years old when he became king, and he reigned in Jerusalem eleven years. He did evil in the eyes of the Lord his God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jehoiakim was twenty-five years old when he became king, and he reigned in Jerusalem eleven years. He did what was evil in the sight of the Lord his God."
      }
    },
