    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门王和聚集到他那里的以色列全体会众在约柜前献祭，献上的牛羊多得不可胜数。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門王和那些聚集到他那裡的以色列全體會眾，都在約櫃前獻上牛羊為祭，數量無法計算，不能數點。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門王和聚集到他那裏的以色列全會眾都在約櫃前獻牛羊為祭，多得不可勝數。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And King Solomon and all the congregation of Israel who were assembled with him before the ark, were sacrificing so many sheep and oxen that they could not be counted or numbered."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and King Solomon and the entire assembly of Israel that had gathered about him were before the ark, sacrificing so many sheep and cattle that they could not be recorded or counted."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There, before the Ark, King Solomon and the entire community of Israel sacrificed so many sheep, goats, and cattle that no one could keep count!"
      }
    },
