    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "沙番书记又对王说：“希勒迦祭司交给我一卷书。”沙番就在王面前诵读这书。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "書記沙番又告訴王，說：“希勒家祭司還交給我一卷書卷。”於是沙番在王面前誦讀那書。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "書記沙番又對王說：「祭司希勒家遞給我一卷書。」沙番就在王面前讀那書。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, Shaphan the scribe told the king saying, “Hilkiah the priest gave me a book.” And Shaphan read from it in the presence of the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Shaphan the secretary informed the king, “Hilkiah the priest has given me a book.” And Shaphan read from it in the presence of the king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Shaphan also told the king, “Hilkiah the priest has given me a scroll.” So Shaphan read it to the king."
      }
    },
