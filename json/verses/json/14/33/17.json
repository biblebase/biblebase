    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然而，众人仍然在丘坛献祭，只是献给他们的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可是人民仍然在邱壇上獻祭，儘管只向耶和華他們的　神獻祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "百姓卻仍在邱壇上獻祭，只獻給耶和華－他們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nevertheless the people still sacrificed in the high places, although only to the Lord their God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The people, however, continued to sacrifice at the high places, but only to the Lord their God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "However, the people still sacrificed at the pagan shrines, though only to the Lord their God."
      }
    },
