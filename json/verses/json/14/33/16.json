    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他重建耶和华的祭坛，在上面献平安祭和感恩祭，又吩咐犹大人事奉以色列的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪拿西重修了耶和華的祭壇，在壇上獻上平安祭和感恩祭，又吩咐猶大人事奉耶和華以色列的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "重修耶和華的祭壇，在壇上獻平安祭、感謝祭，吩咐猶大人事奉耶和華－以色列的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He set up the altar of the Lord and sacrificed peace offerings and thank offerings on it; and he ordered Judah to serve the Lord God of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he restored the altar of the Lord and sacrificed fellowship offerings and thank offerings on it, and told Judah to serve the Lord, the God of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he restored the altar of the Lord and sacrificed peace offerings and thanksgiving offerings on it. He also encouraged the people of Judah to worship the Lord, the God of Israel."
      }
    },
