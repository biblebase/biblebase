    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们封锁殿廊的门，吹灭灯火，不在圣所里向以色列的上帝烧香、献燔祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們又封閉廊門，熄滅燈火，不在聖所中向以色列的　神燒香或獻燔祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "封鎖廊門，吹滅燈火，不在聖所中向以色列神燒香，或獻燔祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have also shut the doors of the porch and put out the lamps, and have not burned incense or offered burnt offerings in the holy place to the God of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They also shut the doors of the portico and put out the lamps. They did not burn incense or present any burnt offerings at the sanctuary to the God of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They also shut the doors to the Temple’s entry room, and they snuffed out the lamps. They stopped burning incense and presenting burnt offerings at the sanctuary of the God of Israel."
      }
    },
