    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门依照他父亲大卫的方法，统计所有寄居在以色列的外族人，共有十五万三千六百人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門按照他父親大衛數點的數目，又數點了住在以色列地所有的外族人，他們共有十五萬三千六百人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門仿照他父大衛數點住在以色列地所有寄居的外邦人，共有十五萬三千六百名；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Solomon numbered all the aliens who were in the land of Israel, following the census which his father David had taken; and 153,600 were found."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Solomon took a census of all the foreigners residing in Israel, after the census his father David had taken; and they were found to be 153,600."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon took a census of all foreigners in the land of Israel, like the census his father had taken, and he counted 153,600."
      }
    },
