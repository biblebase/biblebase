    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并派人去见泰尔王希兰，说：“你曾运香柏木给我父亲建造王宫，请你也照样帮助我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門派人去見推羅王希蘭說：“你怎樣待我的父親大衛，給他運來香柏木，為他建造宮殿居住，請你也這樣待我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門差人去見泰爾王希蘭，說：「你曾運香柏木與我父大衛建宮居住，求你也這樣待我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Solomon sent word to Huram the king of Tyre, saying, “As you dealt with David my father and sent him cedars to build him a house to dwell in, so do for me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Solomon sent this message to Hiram king of Tyre: “Send me cedar logs as you did for my father David when you sent him cedar to build a palace to live in."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon also sent this message to King Hiram at Tyre: “Send me cedar logs as you did for my father, David, when he was building his palace."
      }
    },
