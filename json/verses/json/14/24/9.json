    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "通告犹大和耶路撒冷的人民要将上帝的仆人摩西在旷野为以色列人定的税带来献给耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又在猶大和耶路撒冷發出通告，叫人把　神的僕人摩西，在曠野規定以色列人繳交的稅款帶來，獻給耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又通告猶大和耶路撒冷的百姓，要將神僕人摩西在曠野所吩咐以色列人的捐項給耶和華送來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They made a proclamation in Judah and Jerusalem to bring to the Lord the levy fixed by Moses the servant of God on Israel in the wilderness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A proclamation was then issued in Judah and Jerusalem that they should bring to the Lord the tax that Moses the servant of God had required of Israel in the wilderness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then a proclamation was sent throughout Judah and Jerusalem, telling the people to bring to the Lord the tax that Moses, the servant of God, had required of the Israelites in the wilderness."
      }
    },
