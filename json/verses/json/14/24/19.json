    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但耶和华仍然派先知到他们中间，引导他们归向祂。先知警告他们，他们却不听。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但耶和華仍然差派先知到他們中間，引導他們歸向耶和華；這些先知雖然警告他們，他們卻不肯聽從。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但神仍遣先知到他們那裏，引導他們歸向耶和華。這先知警戒他們，他們卻不肯聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet He sent prophets to them to bring them back to the Lord; though they testified against them, they would not listen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Although the Lord sent prophets to the people to bring them back to him, and though they testified against them, they would not listen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet the Lord sent prophets to bring them back to him. The prophets warned them, but still the people would not listen."
      }
