    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚哈谢二十二岁登基，在耶路撒冷执政一年。他母亲叫亚她利雅，是暗利的孙女。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞哈謝登基的時候是二十二歲；他在耶路撒冷作王一年。他的母親名叫亞他利雅，是暗利的孫女。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈謝登基的時候年四十二歲，在耶路撒冷作王一年。他母親名叫亞她利雅，是暗利的孫女。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ahaziah was twenty-two years old when he became king, and he reigned one year in Jerusalem. And his mother’s name was Athaliah, the granddaughter of Omri."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ahaziah was twenty-two years old when he became king, and he reigned in Jerusalem one year. His mother’s name was Athaliah, a granddaughter of Omri."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ahaziah was twenty-two years old when he became king, and he reigned in Jerusalem one year. His mother was Athaliah, a granddaughter of King Omri."
      }
