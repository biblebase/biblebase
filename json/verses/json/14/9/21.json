    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王有船队与希兰的仆人一起出海去他施，每三年就运回金银、象牙、猿猴和孔雀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為王的船隻和希蘭的僕人一同到他施去；往他施的船隻每三年回來一次，載著金銀、象牙、猿猴和孔雀回來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為王的船隻與希蘭的僕人一同往他施去；他施船隻三年一次裝載金、銀、象牙、猿猴、孔雀回來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the king had ships which went to Tarshish with the servants of Huram; once every three years the ships of Tarshish came bringing gold and silver, ivory and apes and peacocks."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king had a fleet of trading ships manned by Hiram’s servants. Once every three years it returned, carrying gold, silver and ivory, and apes and baboons."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king had a fleet of trading ships of Tarshish manned by the sailors sent by Hiram. Once every three years the ships returned, loaded with gold, silver, ivory, apes, and peacocks."
      }
    },
