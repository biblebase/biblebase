    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "蜡剪、碗、碟和火鼎。殿门和至圣所的门也是金的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又有剪刀、盤子、調羹和火鼎，都是用精金做的。殿門，就是進入至聖所裡面的門和正殿的門，都是金的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又用精金製造鑷子、盤子、調羹、火鼎。至於殿門和至聖所的門扇，並殿的門扇，都是金子妝飾的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and the snuffers, the bowls, the spoons and the firepans of pure gold; and the entrance of the house, its inner doors for the holy of holies and the doors of the house, that is, of the nave, of gold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the pure gold wick trimmers, sprinkling bowls, dishes and censers; and the gold doors of the temple: the inner doors to the Most Holy Place and the doors of the main hall."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "the lamp snuffers, bowls, ladles, and incense burners—all of solid gold; the doors for the entrances to the Most Holy Place and the main room of the Temple, overlaid with gold."
      }
