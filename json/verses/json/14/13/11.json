    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们每天早晚向耶和华献燔祭，烧芬芳的香，把供饼摆在洁净的桌子上，每晚点上金灯台的灯。我们遵守我们上帝耶和华的吩咐，你们却背弃祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們每日早晚向耶和華獻燔祭，焚燒芬芳的香；在清潔的桌子上擺上陳設餅；每天晚上燃點金燈臺的燈盞；我們謹守耶和華我們的　神的吩咐，你們卻離棄了他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "每日早晚向耶和華獻燔祭，燒美香，又在精金的桌子上擺陳設餅；又有金燈臺和燈盞，每晚點起，因為我們遵守耶和華－我們神的命；惟有你們離棄了他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every morning and evening they burn to the Lord burnt offerings and fragrant incense, and the showbread is set on the clean table, and the golden lampstand with its lamps is ready to light every evening; for we keep the charge of the Lord our God, but you have forsaken Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every morning and evening they present burnt offerings and fragrant incense to the Lord. They set out the bread on the ceremonially clean table and light the lamps on the gold lampstand every evening. We are observing the requirements of the Lord our God. But you have forsaken him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They present burnt offerings and fragrant incense to the Lord every morning and evening. They place the Bread of the Presence on the holy table, and they light the gold lampstand every evening. We are following the instructions of the Lord our God, but you have abandoned him."
      }
    },
