    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“如果有远方的外族人听说你的大能大力，慕名而来，向着这殿祷告，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“至於在你的子民以色列以外的外族人，他們為了你的大名、你大能的手和伸出來的膀臂，從遠方而來；他們來向著這殿禱告的時候，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「論到不屬你民以色列的外邦人，為你的大名和大能的手，並伸出來的膀臂，從遠方而來，向這殿禱告，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Also concerning the foreigner who is not from Your people Israel, when he comes from a far country for Your great name’s sake and Your mighty hand and Your outstretched arm, when they come and pray toward this house,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“As for the foreigner who does not belong to your people Israel but has come from a distant land because of your great name and your mighty hand and your outstretched arm—when they come and pray toward this temple,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“In the future, foreigners who do not belong to your people Israel will hear of you. They will come from distant lands when they hear of your great name and your strong hand and your powerful arm. And when they pray toward this Temple,"
      }
    },
