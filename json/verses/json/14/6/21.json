      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你仆人和你的以色列子民向这地方祈祷的时候，求你从你天上的居所垂听，赦免我们的罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你垂聽你的僕人和你的子民以色列的祈求，就是他們向著這地方所發的禱告；求你從天上，從你的居所垂聽，垂聽和赦免。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你僕人和你民以色列向此處祈禱的時候，求你從天上你的居所垂聽，垂聽而赦免。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Listen to the supplications of Your servant and of Your people Israel when they pray toward this place; hear from Your dwelling place, from heaven; hear and forgive."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hear the supplications of your servant and of your people Israel when they pray toward this place. Hear from heaven, your dwelling place; and when you hear, forgive."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May you hear the humble and earnest requests from me and your people Israel when we pray toward this place. Yes, hear us from heaven where you live, and when you hear, forgive."
      }
    },
