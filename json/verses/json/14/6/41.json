    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华上帝啊， 愿你起来与你大能的约柜一同进入你的安息之所！ 耶和华上帝啊， 愿你的祭司披上救恩， 愿你的圣民蒙福欢乐！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在，耶和華　神啊，求你起來，願你和你有大能的約櫃進入你安息的地方；耶和華　神啊，願你的祭司都披上救恩；願你的聖民在福樂中歡欣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華神啊，求你起來， 和你有能力的約櫃同入安息之所。 耶和華神啊，願你的祭司披上救恩； 願你的聖民蒙福歡樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Now therefore arise, O Lord God, to Your resting place, You and the ark of Your might; let Your priests, O Lord God, be clothed with salvation and let Your godly ones rejoice in what is good."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Now arise, Lord God, and come to your resting place, you and the ark of your might. May your priests, Lord God, be clothed with salvation, may your faithful people rejoice in your goodness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And now arise, O Lord God, and enter your resting place, along with the Ark, the symbol of your power. May your priests, O Lord God, be clothed with salvation; may your loyal servants rejoice in your goodness."
      }
    },
