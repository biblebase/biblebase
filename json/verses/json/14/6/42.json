      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“O Lord God, do not turn away the face of Your anointed; remember Your lovingkindness to Your servant David.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord God, do not reject your anointed one. Remember the great love promised to David your servant.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord God, do not reject the king you have anointed. Remember your unfailing love for your servant David.”"
      }
