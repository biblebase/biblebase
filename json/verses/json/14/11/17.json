    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们使犹大国坚固，他们效忠所罗门的儿子罗波安三年之久，并在此期间遵行大卫和所罗门的道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，他們就加強了猶大國的勢力，使所羅門的兒子羅波安強盛了三年，因為他們在這三年，跟隨了大衛和所羅門的道路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，就堅固猶大國，使所羅門的兒子羅波安強盛三年，因為他們三年遵行大衛和所羅門的道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They strengthened the kingdom of Judah and supported Rehoboam the son of Solomon for three years, for they walked in the way of David and Solomon for three years."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They strengthened the kingdom of Judah and supported Rehoboam son of Solomon three years, following the ways of David and Solomon during this time."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This strengthened the kingdom of Judah, and for three years they supported Rehoboam son of Solomon, for during those years they faithfully followed in the footsteps of David and Solomon."
      }
    },
