      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人用马将他的尸体驮回耶路撒冷，葬在犹大城他的祖坟里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後用馬把他的屍體運回來，埋葬在大衛城裡，和他的列祖一起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人就用馬將他的屍首馱回，葬在猶大京城他列祖的墳地裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they brought him on horses and buried him with his fathers in the city of Judah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He was brought back by horse and was buried with his ancestors in the City of Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They brought his body back on a horse, and he was buried with his ancestors in the City of David."
      }
