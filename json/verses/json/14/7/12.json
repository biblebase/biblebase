    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华在夜间向所罗门显现，对他说：“我已听见你的祷告，也已选择这殿作为给我献祭的地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "夜間，耶和華向所羅門顯現，對他說：“我已經聽了你的禱告，也為我自己選擇了這地方作獻祭的殿。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "夜間耶和華向所羅門顯現，對他說：「我已聽了你的禱告，也選擇這地方作為祭祀我的殿宇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord appeared to Solomon at night and said to him, “I have heard your prayer and have chosen this place for Myself as a house of sacrifice."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the Lord appeared to him at night and said: “I have heard your prayer and have chosen this place for myself as a temple for sacrifices."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then one night the Lord appeared to Solomon and said, “I have heard your prayer and have chosen this Temple as the place for making sacrifices."
      }
    },
