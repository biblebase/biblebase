    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门和从哈马口直至埃及小河而来的全体以色列人聚成一大群会众，一起守节期七天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，所羅門和全體以色列人，就是來自哈馬口，直到埃及小河的一群極大的會眾，一起舉行了七天的節期。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時所羅門和以色列眾人，就是從哈馬口直到埃及小河，所有的以色列人都聚集成為大會，守節七日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Solomon observed the feast at that time for seven days, and all Israel with him, a very great assembly who came from the entrance of Hamath to the brook of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Solomon observed the festival at that time for seven days, and all Israel with him—a vast assembly, people from Lebo Hamath to the Wadi of Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the next seven days Solomon and all Israel celebrated the Festival of Shelters. A large congregation had gathered from as far away as Lebo-hamath in the north and the Brook of Egypt in the south."
      }
    },
