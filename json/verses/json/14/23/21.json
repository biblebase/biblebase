    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "民众都欢喜快乐，城里安定，因为亚她利雅已死在刀下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是全國的人民都歡樂，全城也都寧靜；因為他們把亞他利雅用刀殺了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "國民都歡樂，合城都安靜。眾人已將亞她利雅用刀殺了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So all of the people of the land rejoiced and the city was quiet. For they had put Athaliah to death with the sword."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the people of the land rejoiced, and the city was calm, because Athaliah had been slain with the sword."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So all the people of the land rejoiced, and the city was peaceful because Athaliah had been killed."
      }
    },
