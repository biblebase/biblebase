    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要这样做，三分之一在安息日值班的祭司和利未人要把守各门，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要這樣行：你們中間每逢安息日來值班的祭司和利未人，三分之一要把守各門；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又說：「你們當這樣行：祭司和利未人凡安息日進班的，三分之一要把守各門，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is the thing which you shall do: one third of you, of the priests and Levites who come in on the sabbath, shall be gatekeepers,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now this is what you are to do: A third of you priests and Levites who are going on duty on the Sabbath are to keep watch at the doors,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what you must do. When you priests and Levites come on duty on the Sabbath, a third of you will serve as gatekeepers."
      }
    },
