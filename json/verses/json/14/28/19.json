    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华使犹大衰微，因为以色列王亚哈斯在犹大肆无忌惮，悖逆耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為猶大王亞哈斯在猶大放縱邪惡，大大地冒犯了耶和華，所以耶和華使猶大卑微。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為以色列王亞哈斯在猶大放肆，大大干犯耶和華，所以耶和華使猶大卑微。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord humbled Judah because of Ahaz king of Israel, for he had brought about a lack of restraint in Judah and was very unfaithful to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord had humbled Judah because of Ahaz king of Israel, for he had promoted wickedness in Judah and had been most unfaithful to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord was humbling Judah because of King Ahaz of Judah, for he had encouraged his people to sin and had been utterly unfaithful to the Lord."
      }
    },
