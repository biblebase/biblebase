    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门王没有让以色列人服劳役，而是让他们做战士、将领、战车长和骑兵长。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "唯獨以色列人，所羅門沒有驅使他們為他的工程作奴僕；他們作了他的戰士、軍長、車兵長和馬兵長。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟有以色列人，所羅門不使他們當奴僕做工，乃是作他的戰士、軍長的統領、車兵長、馬兵長。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Solomon did not make slaves for his work from the sons of Israel; they were men of war, his chief captains and commanders of his chariots and his horsemen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Solomon did not make slaves of the Israelites for his work; they were his fighting men, commanders of his captains, and commanders of his chariots and charioteers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Solomon did not conscript any of the Israelites for his labor force. Instead, he assigned them to serve as fighting men, officers in his army, commanders of his chariots, and charioteers."
      }
    },
