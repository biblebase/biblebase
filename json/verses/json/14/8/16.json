    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从耶和华的殿奠基直到完工，所罗门所有的计划都已顺利完成。这样，耶和华的殿落成了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從耶和華的殿奠基的日子起，直到建成，所羅門的一切工程都進行順利。這樣，耶和華的殿就完成了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門建造耶和華的殿，從立根基直到成功的日子，工料俱備。這樣，耶和華的殿全然完畢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus all the work of Solomon was carried out from the day of the foundation of the house of the Lord, and until it was finished. So the house of the Lord was completed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All Solomon’s work was carried out, from the day the foundation of the temple of the Lord was laid until its completion. So the temple of the Lord was finished."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Solomon made sure that all the work related to building the Temple of the Lord was carried out, from the day its foundation was laid to the day of its completion."
      }
    },
