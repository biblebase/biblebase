    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "原来亚扪人和摩押人攻击来自西珥山的人，消灭了他们，然后亚扪人和摩押人又自相残杀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "原來亞捫人和摩押人起來攻擊住西珥山的人，把他們殺盡滅絕；他們滅盡了住西珥山的人以後，竟又自相殘殺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為亞捫人和摩押人起來，擊殺住西珥山的人，將他們滅盡；滅盡住西珥山的人之後，他們又彼此自相擊殺。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the sons of Ammon and Moab rose up against the inhabitants of Mount Seir destroying them completely; and when they had finished with the inhabitants of Seir, they helped to destroy one another."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Ammonites and Moabites rose up against the men from Mount Seir to destroy and annihilate them. After they finished slaughtering the men from Seir, they helped to destroy one another."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The armies of Moab and Ammon turned against their allies from Mount Seir and killed every one of them. After they had destroyed the army of Seir, they began attacking each other."
      }
    },
