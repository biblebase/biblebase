      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约沙法效法他父亲亚撒，做耶和华视为正的事，不偏不离。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約沙法遵行他父親亞撒的道路，總不偏離，行耶和華看為正的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約沙法效法他父亞撒所行的，不偏左右，行耶和華眼中看為正的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He walked in the way of his father Asa and did not depart from it, doing right in the sight of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He followed the ways of his father Asa and did not stray from them; he did what was right in the eyes of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jehoshaphat was a good king, following the ways of his father, Asa. He did what was pleasing in the Lord’s sight."
      }
    },
