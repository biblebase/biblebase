    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的上帝啊，你不是曾经在你的以色列子民面前驱逐这地方的居民，把这片土地赐给你的朋友亚伯拉罕的后裔永远作产业吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們的　神啊，你不是曾經把這地的居民從你的子民以色列人面前趕走，把這地賜給你的朋友亞伯拉罕的後裔永遠作產業嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們的神啊，你不是曾在你民以色列人面前驅逐這地的居民，將這地賜給你朋友亞伯拉罕的後裔永遠為業嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Did You not, O our God, drive out the inhabitants of this land before Your people Israel and give it to the descendants of Abraham Your friend forever?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Our God, did you not drive out the inhabitants of this land before your people Israel and give it forever to the descendants of Abraham your friend?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O our God, did you not drive out those who lived in this land when your people Israel arrived? And did you not give this land forever to the descendants of your friend Abraham?"
      }
    },
