    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第四天，他们聚集到一个山谷，一起称颂耶和华。那里从此名叫比拉迦谷，直到今天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "第四日，他們聚集在比拉迦谷，在那裡稱頌耶和華，因此那地方名叫比拉迦谷，直到今日。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "第四日眾人聚集在比拉迦谷，在那裏稱頌耶和華。因此那地方名叫比拉迦谷，直到今日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then on the fourth day they assembled in the valley of Beracah, for there they blessed the Lord. Therefore they have named that place “The Valley of Beracah” until today."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the fourth day they assembled in the Valley of Berakah, where they praised the Lord. This is why it is called the Valley of Berakah to this day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On the fourth day they gathered in the Valley of Blessing, which got its name that day because the people praised and thanked the Lord there. It is still called the Valley of Blessing today."
      }
    },
