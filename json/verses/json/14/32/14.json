    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我祖先所灭的列国中，有哪国的神明能从我手中救自己的国民呢？难道你们的上帝能从我手中救你们吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我列祖消滅的那些國的眾神，有哪一個能拯救自己的子民脫離我的手呢？難道你們的　神能拯救你們脫離我的手嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我列祖所滅的國，那些神中誰能救自己的民脫離我手呢？難道你們的神能救你們脫離我手嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who was there among all the gods of those nations which my fathers utterly destroyed who could deliver his people out of my hand, that your God should be able to deliver you from my hand?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who of all the gods of these nations that my predecessors destroyed has been able to save his people from me? How then can your god deliver you from my hand?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Which of their gods was able to rescue its people from the destructive power of my predecessors? What makes you think your God can rescue you from me?"
      }
    },
