    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希西迦其他的事迹和他对耶和华的忠诚都记在亚摩斯的儿子以赛亚先知的《启示书》上，以及《犹大和以色列的列王史》上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希西家其餘的事蹟和他的善行，都記在亞摩斯的兒子以賽亞先知的異象書上，以及猶大和以色列諸王記上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希西家其餘的事和他的善行都寫在亞摩斯的兒子先知以賽亞的默示書上和猶大、以色列的諸王記上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the rest of the acts of Hezekiah and his deeds of devotion, behold, they are written in the vision of Isaiah the prophet, the son of Amoz, in the Book of the Kings of Judah and Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The other events of Hezekiah’s reign and his acts of devotion are written in the vision of the prophet Isaiah son of Amoz in the book of the kings of Judah and Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The rest of the events in Hezekiah’s reign and his acts of devotion are recorded in The Vision of the Prophet Isaiah Son of Amoz, which is included in The Book of the Kings of Judah and Israel."
      }
    },
