    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华与约沙法同在，因为他效法他祖先大卫起初所行的，没有寻求巴力，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華和約沙法同在，因為他遵行了他祖先大衛最初所行的道路，沒有尋求巴力。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華與約沙法同在；因為他行他祖大衛初行的道，不尋求巴力，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord was with Jehoshaphat because he followed the example of his father David’s earlier days and did not seek the Baals,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord was with Jehoshaphat because he followed the ways of his father David before him. He did not consult the Baals"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord was with Jehoshaphat because he followed the example of his father’s early years and did not worship the images of Baal."
      }
    },
