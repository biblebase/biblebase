    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，耶和华巩固他的王位，所有的犹大人都向他进贡，他极有财富和尊荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，耶和華使他統治的國穩固；猶大各地都給他進貢，所以約沙法的財富很多，極有尊榮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以耶和華堅定他的國，猶大眾人給他進貢；約沙法大有尊榮資財。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the Lord established the kingdom in his control, and all Judah brought tribute to Jehoshaphat, and he had great riches and honor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord established the kingdom under his control; and all Judah brought gifts to Jehoshaphat, so that he had great wealth and honor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the Lord established Jehoshaphat’s control over the kingdom of Judah. All the people of Judah brought gifts to Jehoshaphat, so he became very wealthy and highly esteemed."
      }
    },
