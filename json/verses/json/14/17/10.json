    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大周围的列国都惧怕耶和华，不敢与约沙法争战。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大地周圍的列國都懼怕耶和華，不敢和約沙法交戰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華使猶大四圍的列國都甚恐懼，不敢與約沙法爭戰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the dread of the Lord was on all the kingdoms of the lands which were around Judah, so that they did not make war against Jehoshaphat."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The fear of the Lord fell on all the kingdoms of the lands surrounding Judah, so that they did not go to war against Jehoshaphat."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the fear of the Lord fell over all the surrounding kingdoms so that none of them wanted to declare war on Jehoshaphat."
      }
    },
