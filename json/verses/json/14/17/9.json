    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们带着耶和华的律法书，走遍犹大各城镇教导民众。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們帶著耶和華的律法書，在猶大教訓人；他們走遍猶大各城，在民間教訓人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們帶着耶和華的律法書，走遍猶大各城教訓百姓。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They taught in Judah, having the book of the law of the Lord with them; and they went throughout all the cities of Judah and taught among the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They taught throughout Judah, taking with them the Book of the Law of the Lord; they went around to all the towns of Judah and taught the people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They took copies of the Book of the Law of the Lord and traveled around through all the towns of Judah, teaching the people."
      }
    },
