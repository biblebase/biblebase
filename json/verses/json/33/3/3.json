    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们吃我百姓的肉， 剥我百姓的皮， 打碎他们的骨头， 把他们切成块， 如同下在锅里、放进釜中的肉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們吃我民的肉， 剝他們的皮， 打斷他們的骨頭， 切成像鍋裡的塊， 像釜中的肉塊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "吃我民的肉， 剝他們的皮， 打折他們的骨頭， 分成塊子像要下鍋， 又像釜中的肉。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who eat the flesh of my people, Strip off their skin from them, Break their bones And chop them up as for the pot And as meat in a kettle.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who eat my people’s flesh, strip off their skin and break their bones in pieces; who chop them up like meat for the pan, like flesh for the pot?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, you eat my people’s flesh, strip off their skin, and break their bones. You chop them up like meat for the cooking pot."
      }
    },
