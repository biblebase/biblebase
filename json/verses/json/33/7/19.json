    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你必再怜悯我们， 将我们的罪恶践踏在脚下， 把我们的一切过犯抛进深海。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必再憐愛我們， 把我們的罪孽都踐踏在腳下， 又把我們的一切罪惡都投在深海裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "必再憐憫我們， 將我們的罪孽踏在腳下， 又將我們的一切罪投於深海。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will again have compassion on us; He will tread our iniquities under foot. Yes, You will cast all their sins Into the depths of the sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will again have compassion on us; you will tread our sins underfoot and hurl all our iniquities into the depths of the sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Once again you will have compassion on us. You will trample our sins under your feet and throw them into the depths of the ocean!"
      }
    },
