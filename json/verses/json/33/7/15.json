      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說： 我要把奇事顯給他們看， 好像出埃及地的時候一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“As in the days when you came out from the land of Egypt, I will show you miracles.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“As in the days when you came out of Egypt, I will show them my wonders.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yes,” says the Lord, “I will do mighty miracles for you, like those I did when I rescued you from slavery in Egypt.”"
      }
