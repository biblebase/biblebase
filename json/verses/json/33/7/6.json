    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为儿子藐视父亲， 女儿与母亲作对， 媳妇与婆婆作对； 家人之间反目成仇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "兒子藐視父親， 女兒抗拒母親， 媳婦與婆婆作對； 人的仇敵就是自己的家人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，兒子藐視父親； 女兒抗拒母親； 媳婦抗拒婆婆； 人的仇敵就是自己家裏的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For son treats father contemptuously, Daughter rises up against her mother, Daughter-in-law against her mother-in-law; A man’s enemies are the men of his own household."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For a son dishonors his father, a daughter rises up against her mother, a daughter-in-law against her mother-in-law— a man’s enemies are the members of his own household."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the son despises his father. The daughter defies her mother. The daughter-in-law defies her mother-in-law. Your enemies are right in your own household!"
      }
    },
