    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "列国看见这一切， 都必因自己势力衰微而羞愧， 都必用手捂口，耳朵变聋。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願萬國看見， 就為自己的一切勢力而慚愧； 願他們用手掩口， 耳朵變聾。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "列國看見這事就必為自己的勢力慚愧； 他們必用手摀口，掩耳不聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nations will see and be ashamed Of all their might. They will put their hand on their mouth, Their ears will be deaf."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nations will see and be ashamed, deprived of all their power. They will put their hands over their mouths and their ears will become deaf."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the nations of the world will stand amazed at what the Lord will do for you. They will be embarrassed at their feeble power. They will cover their mouths in silent awe, deaf to everything around them."
      }
    },
