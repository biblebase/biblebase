    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要相信邻居， 不要信赖朋友， 即使对你怀中的妻子也要守口如瓶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不可信靠鄰舍， 也不可信任朋友， 要守住你的嘴唇， 不可向躺在你懷中的妻子透露。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要倚賴鄰舍； 不要信靠密友。 要守住你的口； 不要向你懷中的妻提說。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not trust in a neighbor; Do not have confidence in a friend. From her who lies in your bosom Guard your lips."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not trust a neighbor; put no confidence in a friend. Even with the woman who lies in your embrace guard the words of your lips."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t trust anyone— not your best friend or even your wife!"
      }
    },
