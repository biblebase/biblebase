        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，耶和华离开祂的居所， 从天降临， 脚踏大地的高处。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！耶和華離開自己的地方， 他降下來，踏在地的高處。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，耶和華出了他的居所， 降臨步行地的高處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For behold, the Lord is coming forth from His place. He will come down and tread on the high places of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Look! The Lord is coming from his dwelling place; he comes down and treads on the heights of the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Look! The Lord is coming! He leaves his throne in heaven and tramples the heights of the earth."
      }
