    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为撒玛利亚的创伤无法救治， 已经祸及犹大， 直逼我百姓的城门，直逼耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為撒瑪利亞的創傷無法醫治， 並且殃及猶大， 直逼我子民的城門， 就是耶路撒冷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為撒馬利亞的傷痕無法醫治， 延及猶大和耶路撒冷我民的城門。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For her wound is incurable, For it has come to Judah; It has reached the gate of my people, Even to Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For Samaria’s plague is incurable; it has spread to Judah. It has reached the very gate of my people, even to Jerusalem itself."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For my people’s wound is too deep to heal. It has reached into Judah, even to the gates of Jerusalem."
      }
    },
