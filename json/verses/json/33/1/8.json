    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为此，我要痛哭哀号， 我要赤身光脚而行， 像豺狼哀嚎， 如鸵鸟悲鸣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為此我要痛哭哀號， 赤膊光腳而行； 又要哀號如豺狼， 悲鳴像鴕鳥。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "先知說：因此我必大聲哀號， 赤腳露體而行； 又要呼號如野狗， 哀鳴如鴕鳥。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because of this I must lament and wail, I must go barefoot and naked; I must make a lament like the jackals And a mourning like the ostriches."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because of this I will weep and wail; I will go about barefoot and naked. I will howl like a jackal and moan like an owl."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Therefore, I will mourn and lament. I will walk around barefoot and naked. I will howl like a jackal and moan like an owl."
      }
    },
