    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛利沙的居民啊， 我要使征服者到你们那里。 以色列的领袖要躲到亚杜兰。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪利沙的居民哪！ 我要再使那征服你的來到你那裡； 以色列的榮耀，必去到亞杜蘭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瑪利沙的居民哪， 我必使那奪取你的來到你這裏； 以色列的尊貴人必到亞杜蘭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, I will bring on you The one who takes possession, O inhabitant of Mareshah. The glory of Israel will enter Adullam."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will bring a conqueror against you who live in Mareshah. The nobles of Israel will flee to Adullam."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O people of Mareshah, I will bring a conqueror to capture your town. And the leaders of Israel will go to Adullam."
      }
    },
