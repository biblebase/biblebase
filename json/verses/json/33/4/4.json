    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人人都安然坐在自己的葡萄树和无花果树下， 不再担惊受怕。 这是万军之耶和华亲口说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "各人都要坐在自己的葡萄樹， 和無花果樹下， 無人驚嚇他們， 因為萬軍之耶和華親口說過了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人人都要坐在自己葡萄樹下 和無花果樹下，無人驚嚇。 這是萬軍之耶和華親口說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Each of them will sit under his vine And under his fig tree, With no one to make them afraid, For the mouth of the Lord of hosts has spoken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Everyone will sit under their own vine and under their own fig tree, and no one will make them afraid, for the Lord Almighty has spoken."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone will live in peace and prosperity, enjoying their own grapevines and fig trees, for there will be nothing to fear. The Lord of Heaven’s Armies has made this promise!"
      }
    },
