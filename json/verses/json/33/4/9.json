    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在你为何大声哭喊？ 是因为你没有王吗？ 难道你的谋士都死了吗， 以致你像分娩的妇人一样痛苦不堪？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在你為甚麼大聲呼喊？ 難道你們中間沒有君王嗎？ 或是你的謀士滅亡了， 以致疼痛抓緊你，好像臨產的婦人呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在你為何大聲哭號呢？ 疼痛抓住你彷彿產難的婦人， 是因你中間沒有君王嗎？ 你的謀士滅亡了嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Now, why do you cry out loudly? Is there no king among you, Or has your counselor perished, That agony has gripped you like a woman in childbirth?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why do you now cry aloud— have you no king? Has your ruler perished, that pain seizes you like that of a woman in labor?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But why are you now screaming in terror? Have you no king to lead you? Have your wise people all died? Pain has gripped you like a woman in childbirth."
      }
    },
