    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使瘸腿的成为余民， 让被掳的成为强国。 耶和华要在锡安山做王治理他们， 从那时直到永远。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必使瘸腿的作餘民； 使被放逐的成為強國； 耶和華必在錫安山作王統治他們， 從現在直到永遠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必使瘸腿的為餘剩之民， 使趕到遠方的為強盛之民。 耶和華要在錫安山作王治理他們， 從今直到永遠。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will make the lame a remnant And the outcasts a strong nation, And the Lord will reign over them in Mount Zion From now on and forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will make the lame my remnant, those driven away a strong nation. The Lord will rule over them in Mount Zion from that day and forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who are weak will survive as a remnant; those who were exiles will become a strong nation. Then I, the Lord, will rule from Jerusalem as their king forever.”"
      }
    },
