    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万民都遵从自己的神明， 但我们要永远遵从我们的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然，萬民各奉自己神的名行事， 我們卻要永遠奉耶和華我們　神的名而行。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬民各奉己神的名而行； 我們卻永永遠遠奉耶和華－我們神的名而行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Though all the peoples walk Each in the name of his god, As for us, we will walk In the name of the Lord our God forever and ever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the nations may walk in the name of their gods, but we will walk in the name of the Lord our God for ever and ever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Though the nations around us follow their idols, we will follow the Lord our God forever and ever."
      }
