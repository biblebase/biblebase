    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要拿什么朝见耶和华， 在天上的上帝面前叩拜呢？ 要献上一岁的牛犊作燔祭吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我朝見耶和華， 在至高的　神面前叩拜，當獻上甚麼呢？ 我朝見他的時候，當獻上燔祭， 當獻上一歲的牛犢嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我朝見耶和華， 在至高神面前跪拜，當獻上甚麼呢？ 豈可獻一歲的牛犢為燔祭嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "With what shall I come to the Lord And bow myself before the God on high? Shall I come to Him with burnt offerings, With yearling calves?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "With what shall I come before the Lord and bow down before the exalted God? Shall I come before him with burnt offerings, with calves a year old?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What can we bring to the Lord? Should we bring him burnt offerings? Should we bow before God Most High with offerings of yearling calves?"
      }
    },
