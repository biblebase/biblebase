      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你就要將行這惡事的男人或女人拉到城門外，用石頭將他打死。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then you shall bring out that man or that woman who has done this evil deed to your gates, that is, the man or the woman, and you shall stone them to death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "take the man or woman who has done this evil deed to your city gate and stone that person to death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "then the man or woman who has committed such an evil act must be taken to the gates of the town and stoned to death."
      }
    },
