    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“不可把有缺陷或残疾的牛羊献给你们的上帝耶和华，因为这是祂所憎恶的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“有殘疾，或有甚麼缺點的牛羊，都不可獻給耶和華你的　神，因為這是耶和華你的　神所憎惡的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「凡有殘疾，或有甚麼惡病的牛羊，你都不可獻給耶和華－你的神，因為這是耶和華－你神所憎惡的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall not sacrifice to the Lord your God an ox or a sheep which has a blemish or any defect, for that is a detestable thing to the Lord your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not sacrifice to the Lord your God an ox or a sheep that has any defect or flaw in it, for that would be detestable to him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Never sacrifice sick or defective cattle, sheep, or goats to the Lord your God, for he detests such gifts."
      }
    },
