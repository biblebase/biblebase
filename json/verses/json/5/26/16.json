    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们的上帝耶和华今天吩咐你们遵守这些律例和典章，你们要全心全意地谨慎遵守。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“耶和華你的　神今日吩咐你遵行這些律例和典章，所以你要一心一意謹守遵行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「耶和華－你的神今日吩咐你行這些律例典章，所以你要盡心盡性謹守遵行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“This day the Lord your God commands you to do these statutes and ordinances. You shall therefore be careful to do them with all your heart and with all your soul."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord your God commands you this day to follow these decrees and laws; carefully observe them with all your heart and with all your soul."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Today the Lord your God has commanded you to obey all these decrees and regulations. So be careful to obey them wholeheartedly."
      }
