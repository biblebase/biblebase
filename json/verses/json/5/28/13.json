    "sermons": [
      {
        "id": "home_sermons_sermon-2012-04-22",
        "title": "必有恩惠慈愛隨著我",
        "date": "2012-04-22",
        "preacher": "王文堂牧師",
        "verses": "申命記 28:1-14",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-04-22.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-04-22.pdf"
      }
    ],
    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必使你作頭不作尾，居上不居下，只要你聽從耶和華你的　神的誡命，就是我今日吩咐你的，謹守遵行；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord will make you the head and not the tail, and you only will be above, and you will not be underneath, if you listen to the commandments of the Lord your God, which I charge you today, to observe them carefully,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will make you the head, not the tail. If you pay attention to the commands of the Lord your God that I give you this day and carefully follow them, you will always be at the top, never at the bottom."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you listen to these commands of the Lord your God that I am giving you today, and if you carefully obey them, the Lord will make you the head and not the tail, and you will always be on top and never at the bottom."
      }
    },
