    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华要带你们和你们的王到一个你们和你们祖先都不知道的国家，你们要在那里供奉木石神像。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必把你和你所立統治你的君王，領到你和你的列祖都不認識的國那裡去；在那裡你必事奉別的神，就是木頭石頭所做的神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「耶和華必將你和你所立的王領到你和你列祖素不認識的國去；在那裏你必事奉木頭石頭的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord will bring you and your king, whom you set over you, to a nation which neither you nor your fathers have known, and there you shall serve other gods, wood and stone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will drive you and the king you set over you to a nation unknown to you or your ancestors. There you will worship other gods, gods of wood and stone."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The Lord will exile you and your king to a nation unknown to you and your ancestors. There in exile you will worship gods of wood and stone!"
      }
    },
