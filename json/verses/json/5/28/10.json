    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天下万民将看出你们是耶和华的子民，并惧怕你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "地上萬民見你被稱為耶和華名下的人，就必懼怕你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天下萬民見你歸在耶和華的名下，就要懼怕你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So all the peoples of the earth will see that you are called by the name of the Lord, and they will be afraid of you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then all the peoples on earth will see that you are called by the name of the Lord, and they will fear you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then all the nations of the world will see that you are a people claimed by the Lord, and they will stand in awe of you."
      }
