    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们若遵守你们上帝耶和华的诫命，遵行祂的旨意，祂必按自己的誓言使你们成为祂圣洁的子民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你謹守耶和華你的　神的誡命，遵守他的道路，耶和華就必照著他向你起過的誓，立你作他自己的聖民。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你若謹守耶和華－你神的誡命，遵行他的道，他必照着向你所起的誓立你作為自己的聖民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord will establish you as a holy people to Himself, as He swore to you, if you keep the commandments of the Lord your God and walk in His ways."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will establish you as his holy people, as he promised you on oath, if you keep the commands of the Lord your God and walk in obedience to him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If you obey the commands of the Lord your God and walk in his ways, the Lord will establish you as his holy people as he swore he would do."
      }
    },
