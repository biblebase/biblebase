    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂必使你们及子孙遭遇严重而长久的灾难和疾病，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華就必使你和你的後裔遭受奇災，就是大而長久的災，毒而長久的病。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then the Lord will bring extraordinary plagues on you and your descendants, even severe and lasting plagues, and miserable and chronic sicknesses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the Lord will send fearful plagues on you and your descendants, harsh and prolonged disasters, and severe and lingering illnesses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "then the Lord will overwhelm you and your children with indescribable plagues. These plagues will be intense and without relief, making you miserable and unbearably sick."
      }
