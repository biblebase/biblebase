    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“与你们素不相识的民族必吃光你们土地的出产和劳碌所得，你们必常受欺压，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你土地的出產和你勞碌得來的一切，都必被你不認識的民族吃盡；你必常常受壓迫和壓制；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的土產和你勞碌得來的，必被你所不認識的國民吃盡。你時常被欺負，受壓制，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A people whom you do not know shall eat up the produce of your ground and all your labors, and you will never be anything but oppressed and crushed continually."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A people that you do not know will eat what your land and labor produce, and you will have nothing but cruel oppression all your days."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A foreign nation you have never heard about will eat the crops you worked so hard to grow. You will suffer under constant oppression and harsh treatment."
      }
    },
