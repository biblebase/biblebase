    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "自己的儿女被外族人掳走，你们终日望眼欲穿，却无能为力。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的兒女必被交給別國的民，你必親眼看見，日日為他們焦慮；你必無能為力。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的兒女必歸與別國的民；你的眼目終日切望，甚至失明，你手中無力拯救。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your sons and your daughters shall be given to another people, while your eyes look on and yearn for them continually; but there will be nothing you can do."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your sons and daughters will be given to another nation, and you will wear out your eyes watching for them day after day, powerless to lift a hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will watch as your sons and daughters are taken away as slaves. Your heart will break for them, but you won’t be able to help them."
      }
    },
