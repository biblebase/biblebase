    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“祂要使你们遭受痨病、热症、疟疾、炙热、刀剑、干旱和霉烂之灾，直到你们灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必用癆病、熱病、炎症、瘧疾、乾旱、旱風和霉爛打擊你；它們必追趕你，直到你滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華要用癆病、熱病、火症、瘧疾、刀劍、旱風、霉爛攻擊你。這都要追趕你，直到你滅亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord will smite you with consumption and with fever and with inflammation and with fiery heat and with the sword and with blight and with mildew, and they will pursue you until you perish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will strike you with wasting disease, with fever and inflammation, with scorching heat and drought, with blight and mildew, which will plague you until you perish."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord will strike you with wasting diseases, fever, and inflammation, with scorching heat and drought, and with blight and mildew. These disasters will pursue you until you die."
      }
    },
