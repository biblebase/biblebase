    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们若全心顺服你们的上帝耶和华，谨遵祂今天借我吩咐你们的一切诫命，祂必使你们超越天下万国。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果你實在聽從耶和華你的　神的話，謹守遵行他的一切誡命，就是我今日吩咐你的，耶和華你的　神必使你超過地上所有的民族。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你若留意聽從耶和華－你神的話，謹守遵行他的一切誡命，就是我今日所吩咐你的，他必使你超乎天下萬民之上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Now it shall be, if you diligently obey the Lord your God, being careful to do all His commandments which I command you today, the Lord your God will set you high above all the nations of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you fully obey the Lord your God and carefully follow all his commands I give you today, the Lord your God will set you high above all the nations on earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If you fully obey the Lord your God and carefully keep all his commands that I am giving you today, the Lord your God will set you high above all the nations of the world."
      }
    },
