    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂向你们宣告祂的约，就是祂吩咐你们遵守的十诫，并写在两块石版上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "把他的約，就是他吩咐你們遵守的那十條誡命，向你們宣布出來；又把它們寫在兩塊石版上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他將所吩咐你們當守的約指示你們，就是十條誡，並將這誡寫在兩塊石版上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So He declared to you His covenant which He commanded you to perform, that is, the Ten Commandments; and He wrote them on two tablets of stone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He declared to you his covenant, the Ten Commandments, which he commanded you to follow and then wrote them on two stone tablets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He proclaimed his covenant—the Ten Commandments—which he commanded you to keep, and which he wrote on two stone tablets."
      }
    },
