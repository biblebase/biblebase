    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂让你们听见祂从天上来的声音，好教导你们，又在地上让你们看见祂的烈火，并听见祂在烈火中说的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他從天上使你聽見他的聲音，為要教導你；又在地上使你看見他的大火，並且叫你聽到他從火中所說的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他從天上使你聽見他的聲音，為要教訓你，又在地上使你看見他的烈火，並且聽見他從火中所說的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Out of the heavens He let you hear His voice to discipline you; and on earth He let you see His great fire, and you heard His words from the midst of the fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From heaven he made you hear his voice to discipline you. On earth he showed you his great fire, and you heard his words from out of the fire."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He let you hear his voice from heaven so he could instruct you. He let you see his great fire here on earth so he could speak to you from it."
      }
    },
