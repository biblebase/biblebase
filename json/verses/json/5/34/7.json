        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西去世时一百二十岁，眼睛没有昏花，精力没有衰败。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西死的時候，已經一百二十歲；他的眼睛沒有昏花，他的精力也沒有衰退。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西死的時候年一百二十歲；眼目沒有昏花，精神沒有衰敗。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Although Moses was one hundred and twenty years old when he died, his eye was not dim, nor his vigor abated."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moses was a hundred and twenty years old when he died, yet his eyes were not weak nor his strength gone."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses was 120 years old when he died, yet his eyesight was clear, and he was as strong as ever."
      }
