    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘凡雕刻或铸造耶和华所憎恶的神像，并偷偷供奉的必受咒诅！’ 全体民众要回答说，‘阿们！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘製造耶和華厭惡的雕像或鑄像，工人手中做的，並且在暗處設立的，那人必受咒詛。’眾民都要應聲說：‘阿們。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「『有人製造耶和華所憎惡的偶像，或雕刻，或鑄造，就是工匠手所做的，在暗中設立，那人必受咒詛！』百姓都要答應說：『阿們！』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Cursed is the man who makes an idol or a molten image, an abomination to the Lord, the work of the hands of the craftsman, and sets it up in secret.’ And all the people shall answer and say, ‘Amen.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Cursed is anyone who makes an idol—a thing detestable to the Lord, the work of skilled hands—and sets it up in secret.” Then all the people shall say, “Amen!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘Cursed is anyone who carves or casts an idol and secretly sets it up. These idols, the work of craftsmen, are detestable to the Lord.’ And all the people will reply, ‘Amen.’"
      }
    },
