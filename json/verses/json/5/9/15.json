    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我双手拿着那两块石版，转身从烈火熊熊的山上下来，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是我轉身，從山上下來，山上有火燒著；我手中拿著兩塊約版。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是我轉身下山，山被火燒着，兩塊約版在我兩手之中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“So I turned and came down from the mountain while the mountain was burning with fire, and the two tablets of the covenant were in my two hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I turned and went down from the mountain while it was ablaze with fire. And the two tablets of the covenant were in my hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“So while the mountain was blazing with fire I turned and came down, holding in my hands the two stone tablets inscribed with the terms of the covenant."
      }
    },
