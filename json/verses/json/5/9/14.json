    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要拦我，我要毁灭他们，从世上抹去他们的名字。我要使你的子孙成为一个比他们更强大的民族。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不要管我，我要消滅他們，把他們的名字從天下除去；我要使你成為一個比他們更強更大的國。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你且由着我，我要滅絕他們，將他們的名從天下塗抹，使你的後裔比他們成為更大更強的國。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let Me alone, that I may destroy them and blot out their name from under heaven; and I will make of you a nation mightier and greater than they.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let me alone, so that I may destroy them and blot out their name from under heaven. And I will make you into a nation stronger and more numerous than they.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Leave me alone so I may destroy them and erase their name from under heaven. Then I will make a mighty nation of your descendants, a nation larger and more powerful than they are.’"
      }
    },
