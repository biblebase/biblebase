    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华嘱咐嫩的儿子约书亚：“你要刚强勇敢，因为你要带领以色列人进入我起誓应许他们的土地，我必与你同在。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華吩咐嫩的兒子約書亞，說：“你當堅強勇敢，因為你要領以色列人進我向他們起誓應許的地，我必與你同在。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華囑咐嫩的兒子約書亞說：「你當剛強壯膽，因為你必領以色列人進我所起誓應許他們的地；我必與你同在。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He commissioned Joshua the son of Nun, and said, “Be strong and courageous, for you shall bring the sons of Israel into the land which I swore to them, and I will be with you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord gave this command to Joshua son of Nun: “Be strong and courageous, for you will bring the Israelites into the land I promised them on oath, and I myself will be with you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord commissioned Joshua son of Nun with these words: “Be strong and courageous, for you must bring the people of Israel into the land I swore to give them. I will be with you.”"
      }
    },
