    "sermons": [
      {
        "id": "home_sermons_sermon-2011-10-09",
        "title": "神向你所要的是什麼？",
        "date": "2011-10-09",
        "preacher": "王文堂牧師",
        "verses": "申命記 10:1-22",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-10-09.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-10-09.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，利未人在众支派中没有分到土地作产业，耶和华是他们的产业，这是你们的上帝耶和华对他们的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以利未人在兄弟中無分無業；照著耶和華你的　神應許他的，耶和華自己就是他的產業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以利未人在他弟兄中無分無業，耶和華是他的產業，正如耶和華－你神所應許他的。）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore, Levi does not have a portion or inheritance with his brothers; the Lord is his inheritance, just as the Lord your God spoke to him.)"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "That is why the Levites have no share or inheritance among their fellow Israelites; the Lord is their inheritance, as the Lord your God told them.)"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That is why the Levites have no share of property or possession of land among the other Israelite tribes. The Lord himself is their special possession, as the Lord your God told them.)"
      }
    },
