    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂带领我们离开埃及，为要把我们带进祂起誓赐给我们祖先的这片土地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "卻把我們從那裡領出來，為要領我們進入他向我們列祖起誓應許的地，要把這地賜給我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將我們從那裏領出來，要領我們進入他向我們列祖起誓應許之地，把這地賜給我們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He brought us out from there in order to bring us in, to give us the land which He had sworn to our fathers.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But he brought us out from there to bring us in and give us the land he promised on oath to our ancestors."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He brought us out of Egypt so he could give us this land he had sworn to give our ancestors."
      }
    },
