    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的上帝耶和华吩咐我们遵守这一切律例、敬畏祂，以便我们可以常常受益，生命无忧，正如今日的情形。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華又吩咐我們遵行這一切律例，敬畏耶和華我們的　神，使我們常常得享福樂，使我們的生命得著保全，像今日一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華又吩咐我們遵行這一切律例，要敬畏耶和華－我們的神，使我們常得好處，蒙他保全我們的生命，像今日一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the Lord commanded us to observe all these statutes, to fear the Lord our God for our good always and for our survival, as it is today."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord commanded us to obey all these decrees and to fear the Lord our God, so that we might always prosper and be kept alive, as is the case today."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the Lord our God commanded us to obey all these decrees and to fear him so he can continue to bless us and preserve our lives, as he has done to this day."
      }
    },
