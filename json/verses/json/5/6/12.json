    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要小心，不可忘记把你们从受奴役之地——埃及救出来的耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，你要謹慎，免得你忘記了耶和華，就是把你從埃及地，從為奴之家領出來的那一位。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時你要謹慎，免得你忘記將你從埃及地、為奴之家領出來的耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then watch yourself, that you do not forget the Lord who brought you from the land of Egypt, out of the house of slavery."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "be careful that you do not forget the Lord, who brought you out of Egypt, out of the land of slavery."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "be careful not to forget the Lord, who rescued you from slavery in the land of Egypt."
      }
