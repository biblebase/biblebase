    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所到之处无人能抵挡你们，因为你们的上帝耶和华会使那些地方的人对你们充满惧怕，正如祂对你们的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "必沒有一人能在你們面前站立得住；耶和華你們的　神，必照著他對你們所說的，使害怕和驚懼你們的心臨到你們將要踏足之地的居民。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "必無一人能在你們面前站立得住；耶和華－你們的神必照他所說的，使懼怕驚恐臨到你們所踏之地的居民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No man will be able to stand before you; the Lord your God will lay the dread of you and the fear of you on all the land on which you set foot, as He has spoken to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one will be able to stand against you. The Lord your God, as he promised you, will put the terror and fear of you on the whole land, wherever you go."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No one will be able to stand against you, for the Lord your God will cause the people to fear and dread you, as he promised, wherever you go in the whole land."
      }
    },
