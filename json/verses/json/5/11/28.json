    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们若不遵行你们的上帝耶和华的诫命，偏离我今天吩咐你们当走的道路，随从不认识的神明，必受咒诅。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們不聽從耶和華你們的　神的誡命，偏離了我今日吩咐你們要走的道路，去隨從你們素來不認識的別神，你們就必受咒詛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若不聽從耶和華－你們神的誡命，偏離我今日所吩咐你們的道，去事奉你們素來所不認識的別神，就必受禍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and the curse, if you do not listen to the commandments of the Lord your God, but turn aside from the way which I am commanding you today, by following other gods which you have not known."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the curse if you disobey the commands of the Lord your God and turn from the way that I command you today by following other gods, which you have not known."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you will be cursed if you reject the commands of the Lord your God and turn away from him and worship gods you have not known before."
      }
    },
