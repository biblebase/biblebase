    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要记住，我现在是对你们，而不是对你们的孩子说话。他们没有亲身经历、亲眼见过你们上帝耶和华的管教、威严、大能的手和伸出的臂膀；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "今日你們要知道，我並不是與你們的兒女說話，因為他們不知道，也沒有見過耶和華你們的　神的管教、偉大有力的手和伸出來的膀臂，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們今日當知道，我本不是和你們的兒女說話；因為他們不知道，也沒有看見耶和華－你們神的管教、威嚴、大能的手，和伸出來的膀臂，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Know this day that I am not speaking with your sons who have not known and who have not seen the discipline of the Lord your God—His greatness, His mighty hand and His outstretched arm,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember today that your children were not the ones who saw and experienced the discipline of the Lord your God: his majesty, his mighty hand, his outstretched arm;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Keep in mind that I am not talking now to your children, who have never experienced the discipline of the Lord your God or seen his greatness and his strong hand and powerful arm."
      }
    },
