    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "免得无辜人的血流在你们的上帝耶和华赐给你们作产业之地，以致你们担当枉杀无辜的罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "免得無辜人的血，流在耶和華你的　神要賜給你作產業的地上，以致流人血的罪歸在你身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "免得無辜之人的血流在耶和華－你神所賜你為業的地上，流血的罪就歸於你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So innocent blood will not be shed in the midst of your land which the Lord your God gives you as an inheritance, and bloodguiltiness be on you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do this so that innocent blood will not be shed in your land, which the Lord your God is giving you as your inheritance, and so that you will not be guilty of bloodshed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That way you will prevent the death of innocent people in the land the Lord your God is giving you as your special possession. You will not be held responsible for the death of innocent people."
      }
    },
