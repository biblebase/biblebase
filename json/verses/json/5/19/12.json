    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他本城的长老要派人去把他带回来，交给复仇的人，将他处死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他本城的長老要派人去，把他從那裡帶出來，交在報血仇的人手中，好把他處死。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "本城的長老就要打發人去，從那裏帶出他來，交在報血仇的手中，將他治死。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then the elders of his city shall send and take him from there and deliver him into the hand of the avenger of blood, that he may die."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the killer shall be sent for by the town elders, be brought back from the city, and be handed over to the avenger of blood to die."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In that case, the elders of the murderer’s hometown must send agents to the city of refuge to bring him back and hand him over to the dead person’s avenger to be put to death."
      }
    },
