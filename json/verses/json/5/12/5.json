    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的上帝耶和华要在你们众支派中选一个地方作祂的居所，你们要去那里敬拜祂，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華你們的　神從你們各支派中，選擇了甚麼地方作立他名的居所，你們就當到那裡求問他；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但耶和華－你們的神從你們各支派中選擇何處為立他名的居所，你們就當往那裏去求問，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But you shall seek the Lord at the place which the Lord your God will choose from all your tribes, to establish His name there for His dwelling, and there you shall come."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you are to seek the place the Lord your God will choose from among all your tribes to put his Name there for his dwelling. To that place you must go;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rather, you must seek the Lord your God at the place of worship he himself will choose from among all the tribes—the place where his name will be honored."
      }
    },
