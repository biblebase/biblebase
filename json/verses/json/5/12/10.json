    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但不久你们将渡过约旦河，定居在那片土地上。耶和华将使你们四境太平，安然居住，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是你們過了約旦河，住在耶和華你們的　神使你們承受為業的地，又使你們太平，脫離你們周圍的所有仇敵，安居樂業，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你們過了約旦河，得以住在耶和華－你們神使你們承受為業之地，又使你們太平，不被四圍的一切仇敵擾亂，安然居住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you cross the Jordan and live in the land which the Lord your God is giving you to inherit, and He gives you rest from all your enemies around you so that you live in security,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you will cross the Jordan and settle in the land the Lord your God is giving you as an inheritance, and he will give you rest from all your enemies around you so that you will live in safety."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you will soon cross the Jordan River and live in the land the Lord your God is giving you. When he gives you rest from all your enemies and you’re living safely in the land,"
      }
    },
