    "sermons": [
      {
        "id": "home_sermons_sermon-2011-10-30",
        "title": "你要謹慎的三件事",
        "date": "2011-10-30",
        "preacher": "王文堂牧師",
        "verses": "申命記 12:1-32",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-10-30.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-10-30.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要把我吩咐你们献的燔祭、各种牲祭、十一奉献、举祭和一切还愿祭带到你们的上帝耶和华选定的敬拜场所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，你要把我吩咐你們的燔祭和其他的祭物、十分之一的禮物和手中的貢獻，以及你們向耶和華許願要獻的最美的一切還願祭，都要帶到耶和華你們的　神選擇作為立他名的地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時要將我所吩咐你們的燔祭、平安祭、十分取一之物，和手中的舉祭，並向耶和華許願獻的一切美祭，都奉到耶和華－你們神所選擇要立為他名的居所。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then it shall come about that the place in which the Lord your God will choose for His name to dwell, there you shall bring all that I command you: your burnt offerings and your sacrifices, your tithes and the contribution of your hand, and all your choice votive offerings which you will vow to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then to the place the Lord your God will choose as a dwelling for his Name—there you are to bring everything I command you: your burnt offerings and sacrifices, your tithes and special gifts, and all the choice possessions you have vowed to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "you must bring everything I command you—your burnt offerings, your sacrifices, your tithes, your sacred offerings, and your offerings to fulfill a vow—to the designated place of worship, the place the Lord your God chooses for his name to be honored."
      }
    },
