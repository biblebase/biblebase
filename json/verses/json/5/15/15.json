    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要记住，你们曾在埃及做奴隶，你们的上帝耶和华拯救了你们。所以，我今天吩咐你们这样做。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要記得你在埃及地作過奴僕，耶和華你的　神救贖了你；因此我今日吩咐你這件事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要記念你在埃及地作過奴僕，耶和華－你的神將你救贖；因此，我今日吩咐你這件事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall remember that you were a slave in the land of Egypt, and the Lord your God redeemed you; therefore I command you this today."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember that you were slaves in Egypt and the Lord your God redeemed you. That is why I give you this command today."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Remember that you were once slaves in the land of Egypt and the Lord your God redeemed you! That is why I am giving you this command."
      }
    },
