    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“如果有人在野外遇见已经订婚的女子，强奸了她，必须处死那男子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果有人在田間遇見了已經許配人的少女，拉住她，和她同寢，只要把那和她同寢的人處死。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「若有男子在田野遇見已經許配人的女子，強與她行淫，只要將那男子治死。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But if in the field the man finds the girl who is engaged, and the man forces her and lies with her, then only the man who lies with her shall die."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if out in the country a man happens to meet a young woman pledged to be married and rapes her, only the man who has done this shall die."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But if the man meets the engaged woman out in the country, and he rapes her, then only the man must die."
      }
    },
