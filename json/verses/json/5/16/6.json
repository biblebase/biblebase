    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "而是要在傍晚日落时分，即你们离开埃及的时候，到你们的上帝耶和华选定的敬拜场所献逾越节的祭牲。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只能在耶和華你的　神選擇作他名的居所的地方，晚上日落的時候，就是你出埃及的時刻，宰殺逾越節的祭牲；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只當在耶和華－你神所選擇要立為他名的居所，晚上日落的時候，乃是你出埃及的時候，獻逾越節的祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but at the place where the Lord your God chooses to establish His name, you shall sacrifice the Passover in the evening at sunset, at the time that you came out of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "except in the place he will choose as a dwelling for his Name. There you must sacrifice the Passover in the evening, when the sun goes down, on the anniversary of your departure from Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must offer it only at the designated place of worship—the place the Lord your God chooses for his name to be honored. Sacrifice it there in the evening as the sun goes down on the anniversary of your exodus from Egypt."
      }
    },
