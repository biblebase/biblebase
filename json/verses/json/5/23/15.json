    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“如果有奴隶逃到你们那里避难，不可把逃亡的奴隶送交他们的主人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果一個奴僕離開了他的主人，逃到你那裡來，你不可把他送交他的主人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「若有奴僕脫了主人的手，逃到你那裏，你不可將他交付他的主人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall not hand over to his master a slave who has escaped from his master to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If a slave has taken refuge with you, do not hand them over to their master."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If slaves should escape from their masters and take refuge with you, you must not hand them over to their masters."
      }
    },
