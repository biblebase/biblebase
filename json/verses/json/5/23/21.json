    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“如果你们向你们的上帝耶和华许愿，不可迟迟不还愿，因为你们的上帝耶和华必追讨许愿不还的罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果你向耶和華你的　神許了願，就不可遲延還願，因為耶和華你的　神必向你追討，那時你就有罪了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你向耶和華－你的神許願，償還不可遲延；因為耶和華－你的神必定向你追討，你不償還就有罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“When you make a vow to the Lord your God, you shall not delay to pay it, for it would be sin in you, and the Lord your God will surely require it of you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you make a vow to the Lord your God, do not be slow to pay it, for the Lord your God will certainly demand it of you and you will be guilty of sin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When you make a vow to the Lord your God, be prompt in fulfilling whatever you promised him. For the Lord your God demands that you promptly fulfill all your vows, or you will be guilty of sin."
      }
    },
