    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你的助手——嫩的儿子约书亚可以进入。你要鼓励他，因为他将带领以色列承受那片土地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是站在你面前侍候你的嫩的兒子約書亞，他可以進到那裡去；你要鼓勵他，因為他要使以色列人承受那地為業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "伺候你、嫩的兒子約書亞，他必得進入那地；你要勉勵他，因為他要使以色列人承受那地為業。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joshua the son of Nun, who stands before you, he shall enter there; encourage him, for he will cause Israel to inherit it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But your assistant, Joshua son of Nun, will enter it. Encourage him, because he will lead Israel to inherit it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Instead, your assistant, Joshua son of Nun, will lead the people into the land. Encourage him, for he will lead Israel as they take possession of it."
      }
