    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要从各支派中选出有智慧、有见识、受尊重的人，我要立他们做你们的首领。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要按著支派選出一些有智慧、有見識、有經驗的人，我就立他們作你們的首領。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要按着各支派選舉有智慧、有見識、為眾人所認識的，我立他們為你們的首領。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Choose wise and discerning and experienced men from your tribes, and I will appoint them as your heads.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Choose some wise, understanding and respected men from each of your tribes, and I will set them over you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Choose some well-respected men from each tribe who are known for their wisdom and understanding, and I will appoint them as your leaders.’"
      }
    },
