    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂必改变你们被掳的境遇，怜悯你们，把你们从祂驱逐你们去的列国招聚回来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，耶和華你的　神就必使你這被擄的人回歸，他必憐憫你；耶和華你的　神必從你被分散到的萬民中，把你再召集回來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，耶和華－你的神必憐恤你，救回你這被擄的子民；耶和華－你的神要回轉過來，從分散你到的萬民中將你招聚回來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then the Lord your God will restore you from captivity, and have compassion on you, and will gather you again from all the peoples where the Lord your God has scattered you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "then the Lord your God will restore your fortunes and have compassion on you and gather you again from all the nations where he scattered you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "then the Lord your God will restore your fortunes. He will have mercy on you and gather you back from all the nations where he has scattered you."
      }
    },
