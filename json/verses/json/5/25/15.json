    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们必须诚实无欺，使用同样的砝码和量器，以便在你们的上帝耶和华要赐给你们的土地上得享长寿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要有準確公正的法碼，你要有準確公正的升斗，好使你在耶和華你的　神賜給你的地上，得享長壽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當用對準公平的法碼，公平的升斗。這樣，在耶和華－你神所賜你的地上，你的日子就可以長久。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall have a full and just weight; you shall have a full and just measure, that your days may be prolonged in the land which the Lord your God gives you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You must have accurate and honest weights and measures, so that you may live long in the land the Lord your God is giving you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, always use honest weights and measures, so that you may enjoy a long life in the land the Lord your God is giving you."
      }
    },
