    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的上帝耶和华要在你们中间选立一位像我一样的先知，你们要听从他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“耶和華你的　神要從你中間，就是從你的眾兄弟中間，給你興起一位先知來，像我一樣；你們要聽從他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－你的神要從你們弟兄中間給你興起一位先知，像我，你們要聽從他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The Lord your God will raise up for you a prophet like me from among you, from your countrymen, you shall listen to him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord your God will raise up for you a prophet like me from among you, from your fellow Israelites. You must listen to him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses continued, “The Lord your God will raise up for you a prophet like me from among your fellow Israelites. You must listen to him."
      }
    },
