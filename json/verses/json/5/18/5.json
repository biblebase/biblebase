    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你们的上帝耶和华从各支派中拣选了利未人，让他们世代奉祂的名事奉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華你的　神從你的各支派中把他揀選出來，使他和他的子孫，可以常常奉耶和華的名侍立供祭司的職。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為耶和華－你的神從你各支派中將他揀選出來，使他和他子孫永遠奉耶和華的名侍立，事奉。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord your God has chosen him and his sons from all your tribes, to stand and serve in the name of the Lord forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for the Lord your God has chosen them and their descendants out of all your tribes to stand and minister in the Lord’s name always."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Lord your God chose the tribe of Levi out of all your tribes to minister in the Lord’s name forever."
      }
