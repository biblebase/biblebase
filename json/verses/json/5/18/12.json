    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡做这些事的人都是耶和华所憎恶的。正是因为当地的民族做这些可憎之事，你们的上帝耶和华才要当着你们的面把他们赶走。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為行這些事的，都是耶和華厭惡的；就是因為這些可厭可惡的事的緣故，耶和華你的　神才把他們從你的面前趕走。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡行這些事的都為耶和華所憎惡；因那些國民行這可憎惡的事，所以耶和華－你的神將他們從你面前趕出。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For whoever does these things is detestable to the Lord; and because of these detestable things the Lord your God will drive them out before you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who does these things is detestable to the Lord; because of these same detestable practices the Lord your God will drive out those nations before you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who does these things is detestable to the Lord. It is because the other nations have done these detestable things that the Lord your God will drive them out ahead of you."
      }
    },
