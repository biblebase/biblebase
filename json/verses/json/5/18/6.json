    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以色列境内任何地方的利未人若愿意离开自己所住的城，去耶和华选定的地方，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果有住在以色列中任何一座城裡的利未人，離開了那裡，一心一意到耶和華選擇的地方去，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「利未人無論寄居在以色列中的哪一座城，若從那裏出來，一心願意到耶和華所選擇的地方，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Now if a Levite comes from any of your towns throughout Israel where he resides, and comes whenever he desires to the place which the Lord chooses,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If a Levite moves from one of your towns anywhere in Israel where he is living, and comes in all earnestness to the place the Lord will choose,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Suppose a Levite chooses to move from his town in Israel, wherever he is living, to the place the Lord chooses for worship."
      }
    },
