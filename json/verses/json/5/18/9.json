    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们进入你们的上帝耶和华将要赐给你们的土地后，不可仿效当地民族的可憎行径。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你到了耶和華你的　神要賜給你的那地的時候，那些外族人可厭可惡的事，你不可學習去行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你到了耶和華－你神所賜之地，那些國民所行可憎惡的事，你不可學着行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“When you enter the land which the Lord your God gives you, you shall not learn to imitate the detestable things of those nations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you enter the land the Lord your God is giving you, do not learn to imitate the detestable ways of the nations there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When you enter the land the Lord your God is giving you, be very careful not to imitate the detestable customs of the nations living there."
      }
