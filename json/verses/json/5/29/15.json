    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "也是与今天一同站在我们的上帝耶和华面前的人，以及今天不在这里的人立的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "而且也與那些今日和我們在這裡，一同站在耶和華我們的　神面前的人，以及那些今日不與我們同在這裡的人立這約，起這誓。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡與我們一同站在耶和華－我們神面前的，並今日不在我們這裏的人，我也與他們立這約，起這誓。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but both with those who stand here with us today in the presence of the Lord our God and with those who are not with us here today"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who are standing here with us today in the presence of the Lord our God but also with those who are not here today."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am making this covenant both with you who stand here today in the presence of the Lord our God, and also with the future generations who are not standing here today."
      }
    },
