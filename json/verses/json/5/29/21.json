    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华必把他从以色列各支派中隔离出来，使他按律法书上约中的一切咒诅受惩罚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必照著寫在這律法書上的約的一切咒詛，把他從以色列眾支派中分別出來，使他遭受災禍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也必照着寫在律法書上、約中的一切咒詛將他從以色列眾支派中分別出來，使他受禍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord will single him out for adversity from all the tribes of Israel, according to all the curses of the covenant which are written in this book of the law."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will single them out from all the tribes of Israel for disaster, according to all the curses of the covenant written in this Book of the Law."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord will separate them from all the tribes of Israel, to pour out on them all the curses of the covenant recorded in this Book of Instruction."
      }
    },
