    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“奥秘的事属于我们的上帝耶和华，只有显明的事永远属于我们和我们的子孙，以便我们可以遵守这律法上的一切话。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“奧祕的事，是屬於耶和華我們的　神的，只有顯露的事，是永遠屬於我們和我們子孫的，好叫我們遵行這律法上的一切話。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「隱祕的事是屬耶和華－我們神的；惟有明顯的事是永遠屬我們和我們子孫的，好叫我們遵行這律法上的一切話。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The secret things belong to the Lord our God, but the things revealed belong to us and to our sons forever, that we may observe all the words of this law."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The secret things belong to the Lord our God, but the things revealed belong to us and to our children forever, that we may follow all the words of this law."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The Lord our God has secrets known to no one. We are not accountable for them, but we and our children are accountable forever for all that he has revealed to us, so that we may obey all the terms of these instructions."
      }
    },
