    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "去供奉、祭拜他们所不认识、并非耶和华所分派的神明。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "去事奉別的神，向他們叩拜，就是他們不認識的神，也是耶和華沒有給他們指定要拜的神；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "去事奉敬拜素不認識的別神，是耶和華所未曾給他們安排的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They went and served other gods and worshiped them, gods whom they have not known and whom He had not allotted to them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They went off and worshiped other gods and bowed down to them, gods they did not know, gods he had not given them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Instead, they turned away to serve and worship gods they had not known before, gods that were not from the Lord."
      }
