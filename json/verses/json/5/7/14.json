    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天下万族之中，你们是最蒙福的，你们的男人、女人和牲畜没有不能生育的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你必蒙福勝過萬民；在你中間沒有不能生育的男女，也沒有不能生殖的牲畜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必蒙福勝過萬民；你們的男女沒有不能生養的，牲畜也沒有不能生育的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall be blessed above all peoples; there will be no male or female barren among you or among your cattle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will be blessed more than any other people; none of your men or women will be childless, nor will any of your livestock be without young."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will be blessed above all the nations of the earth. None of your men or women will be childless, and all your livestock will bear young."
      }
    },
