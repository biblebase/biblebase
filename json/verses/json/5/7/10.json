    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但祂必毫不迟延地报应、毁灭那些恨祂的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但向恨他的人，卻要當面報應他們，把他們滅盡；恨他的，他都要當面報應他，決不遲延。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "向恨他的人當面報應他們，將他們滅絕。凡恨他的人必報應他們，決不遲延。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but repays those who hate Him to their faces, to destroy them; He will not delay with him who hates Him, He will repay him to his face."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But those who hate him he will repay to their face by destruction; he will not be slow to repay to their face those who hate him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But he does not hesitate to punish and destroy those who reject him."
      }
    },
