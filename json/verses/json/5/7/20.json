    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的上帝耶和华还要派黄蜂去攻击他们，直到躲藏起来的残存者也全部灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華你的　神還要打發大黃蜂飛到他們中間，直到那些殘存躲藏起來的人，都從你面前消滅為止。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且耶和華－你神必打發黃蜂飛到他們中間，直到那剩下而藏躲的人從你面前滅亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, the Lord your God will send the hornet against them, until those who are left and hide themselves from you perish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moreover, the Lord your God will send the hornet among them until even the survivors who hide from you have perished."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And then the Lord your God will send terror to drive out the few survivors still hiding from you!"
      }
    },
