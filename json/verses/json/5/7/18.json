    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要怕他们，要牢记你们的上帝耶和华曾如何对付埃及和法老，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不要怕他們，只要牢牢地記住，耶和華你的　神向法老和全埃及所行的事，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你不要懼怕他們，要牢牢記念耶和華－你神向法老和埃及全地所行的事，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "you shall not be afraid of them; you shall well remember what the Lord your God did to Pharaoh and to all Egypt:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But do not be afraid of them; remember well what the Lord your God did to Pharaoh and to all Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But don’t be afraid of them! Just remember what the Lord your God did to Pharaoh and to all the land of Egypt."
      }
