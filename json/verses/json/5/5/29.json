    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但愿他们常常敬畏我，遵从我的一切诫命，以便他们和他们的子孙世世代代可以蒙福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但願他們常常存著這樣的心，敬畏我，遵守我的一切誡命，使他們和他們的子孫永享福樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟願他們存這樣的心敬畏我，常遵守我的一切誡命，使他們和他們的子孫永遠得福。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Oh that they had such a heart in them, that they would fear Me and keep all My commandments always, that it may be well with them and with their sons forever!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Oh, that their hearts would be inclined to fear me and keep all my commands always, so that it might go well with them and their children forever!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Oh, that they would always have hearts like this, that they might fear me and obey all my commands! If they did, they and their descendants would prosper forever."
      }
    },
