    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说，‘看啊，我们的上帝耶和华向我们彰显了祂的荣耀和威严，我们听见了祂从火中发出的声音。今天我们已经看见，人听见上帝说话后还能活着。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說：‘看哪，耶和華我們的　神把他的榮耀和偉大顯給我們看，我們又聽見他從火中發出來的聲音；今日我們看見了　神與人說話，而人還能活著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：『看哪，耶和華－我們神將他的榮光和他的大能顯給我們看，我們又聽見他的聲音從火中出來。今日我們得見神與人說話，人還存活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You said, ‘Behold, the Lord our God has shown us His glory and His greatness, and we have heard His voice from the midst of the fire; we have seen today that God speaks with man, yet he lives."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And you said, “The Lord our God has shown us his glory and his majesty, and we have heard his voice from the fire. Today we have seen that a person can live even if God speaks with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They said, ‘Look, the Lord our God has shown us his glory and greatness, and we have heard his voice from the heart of the fire. Today we have seen that God can speak to us humans, and yet we live!"
      }
    },
