      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「『不可為自己雕刻偶像，也不可做甚麼形像，彷彿上天、下地和地底下、水中的百物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘You shall not make for yourself an idol, or any likeness of what is in heaven above or on the earth beneath or in the water under the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You shall not make for yourself an image in the form of anything in heaven above or on the earth beneath or in the waters below."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must not make for yourself an idol of any kind, or an image of anything in the heavens or on the earth or in the sea."
      }
