    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，你们要谨遵你们上帝耶和华的吩咐，不可偏离左右。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，你們要照著耶和華你們的　神吩咐你們的，謹守遵行，不可偏離左右。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，你們要照耶和華－你們神所吩咐的謹守遵行，不可偏離左右。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So you shall observe to do just as the Lord your God has commanded you; you shall not turn aside to the right or to the left."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So be careful to do what the Lord your God has commanded you; do not turn aside to the right or to the left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Moses told the people, “You must be careful to obey all the commands of the Lord your God, following his instructions in every detail."
      }
