    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂爱自己的子民， 众圣者都听命于祂， 伏在祂的脚前， 领受祂的训诲。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他實在愛人民， 他的眾聖者都在你的手裡； 他們坐在你的腳前， 各自領受你的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他疼愛百姓； 眾聖徒都在他手中。 他們坐在他的腳下， 領受他的言語。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Indeed, He loves the people; All Your holy ones are in Your hand, And they followed in Your steps; Everyone receives of Your words."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely it is you who love the people; all the holy ones are in your hand. At your feet they all bow down, and from you receive instruction,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Indeed, he loves his people; all his holy ones are in his hands. They follow in his steps and accept his teaching."
      }
    },
