    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你赐他们力量， 悦纳他们的工作， 击碎仇敌的腰杆， 使敌人一蹶不振。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊，求你賜福他的能力， 悅納他手中的作為； 那些起來攻擊他和恨他的人，願你刺透他們的腰， 使他們不能再起來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求耶和華降福在他的財物上， 悅納他手裏所辦的事。 那些起來攻擊他和恨惡他的人， 願你刺透他們的腰， 使他們不得再起來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, bless his substance, And accept the work of his hands; Shatter the loins of those who rise up against him, And those who hate him, so that they will not rise again.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Bless all his skills, Lord, and be pleased with the work of his hands. Strike down those who rise against him, his foes till they rise no more.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Bless the ministry of the Levites, O Lord, and accept all the work of their hands. Hit their enemies where it hurts the most; strike down their foes so they never rise again.”"
      }
