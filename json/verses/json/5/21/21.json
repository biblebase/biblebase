    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "全城的人都要用石头打死他，从你们中间除掉这种罪恶。所有以色列人听说后，都会感到害怕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "全城的人就要用石頭打死他；這樣，你就把那惡從你們中間除掉。以色列眾人聽見了，就都懼怕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "本城的眾人就要用石頭將他打死。這樣，就把那惡從你們中間除掉，以色列眾人都要聽見害怕。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then all the men of his city shall stone him to death; so you shall remove the evil from your midst, and all Israel will hear of it and fear."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then all the men of his town are to stone him to death. You must purge the evil from among you. All Israel will hear of it and be afraid."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then all the men of his town must stone him to death. In this way, you will purge this evil from among you, and all Israel will hear about it and be afraid."
      }
