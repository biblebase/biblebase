    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "锻炼体魄益处还少，敬虔生活受益无穷，拥有今生和永世的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為操練身體，益處還少；唯獨操練敬虔，凡事有益，享有今生和來世的應許。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「操練身體，益處還少；惟獨敬虔，凡事都有益處，因有今生和來生的應許。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "for bodily discipline is only of little profit, but godliness is profitable for all things, since it holds promise for the present life and also for the life to come."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For physical training is of some value, but godliness has value for all things, holding promise for both the present life and the life to come."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Physical training is good, but training for godliness is much better, promising benefits in this life and in the life to come.”"
      }
    },
