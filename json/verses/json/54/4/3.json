    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "禁止人结婚和吃某些食物。其实那些食物是上帝创造的，为了让相信并且明白真理的人存感谢的心领受。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們禁止嫁娶，禁戒食物。食物本是　神所造的，是給信主和認識真理的人存感謝的心領受的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們禁止嫁娶，又禁戒食物，就是神所造、叫那信而明白真道的人感謝着領受的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "men who forbid marriage and advocate abstaining from foods which God has created to be gratefully shared in by those who believe and know the truth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They forbid people to marry and order them to abstain from certain foods, which God created to be received with thanksgiving by those who believe and who know the truth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will say it is wrong to be married and wrong to eat certain foods. But God created those foods to be eaten with thanks by faithful people who know the truth."
      }
    },
