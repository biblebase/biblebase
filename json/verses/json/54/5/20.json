      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些一直犯罪的人，你要当众责备他们，以警戒众人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "常常犯罪的，你要當眾責備他們，使其餘的人也有所懼怕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "犯罪的人，當在眾人面前責備他，叫其餘的人也可以懼怕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Those who continue in sin, rebuke in the presence of all, so that the rest also will be fearful of sinning."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But those elders who are sinning you are to reprove before everyone, so that the others may take warning."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who sin should be reprimanded in front of the whole church; this will serve as a strong warning to others."
      }
