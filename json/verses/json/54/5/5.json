    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "真正有需要、无依无靠的寡妇仰赖上帝的帮助，昼夜不住地祷告祈求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那無依無靠獨居的寡婦，她們仰望　神，晝夜不住地祈求禱告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那獨居無靠、真為寡婦的，是仰賴神，晝夜不住地祈求禱告。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now she who is a widow indeed and who has been left alone, has fixed her hope on God and continues in entreaties and prayers night and day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The widow who is really in need and left all alone puts her hope in God and continues night and day to pray and to ask God for help."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now a true widow, a woman who is truly alone in this world, has placed her hope in God. She prays night and day, asking God for his help."
      }
    },
