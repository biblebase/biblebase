    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有些人的罪很明显，自招审判，有些人的罪日后才暴露出来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有些人的罪是明顯的，就先受審判；有些人的罪是後來才顯露出來的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有些人的罪是明顯的，如同先到審判案前；有些人的罪是隨後跟了去的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sins of some men are quite evident, going before them to judgment; for others, their sins follow after."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The sins of some are obvious, reaching the place of judgment ahead of them; the sins of others trail behind them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Remember, the sins of some people are obvious, leading them to certain judgment. But there are others whose sins will not be revealed until later."
      }
    },
