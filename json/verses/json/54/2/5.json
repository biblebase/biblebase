    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝只有一位，在上帝和人类之间只有一位中保，就是降世为人的基督耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為　神只有一位，在　神和人中間也只有一位中保，就是降世為人的基督耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為只有一位神，在神和人中間，只有一位中保，乃是降世為人的基督耶穌；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For there is one God, and one mediator also between God and men, the man Christ Jesus,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For there is one God and one mediator between God and mankind, the man Christ Jesus,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For, There is one God and one Mediator who can reconcile God and humanity—the man Christ Jesus."
      }
    },
