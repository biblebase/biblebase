    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "做奴仆的要全心尊重自己的主人，免得上帝的名和我们的教导被人亵渎。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡負軛作奴僕的，應當看自己的主人是配受十分的敬重，免得　神的名和道理被人褻瀆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡在軛下作僕人的，當以自己主人配受十分的恭敬，免得神的名和道理被人褻瀆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All who are under the yoke as slaves are to regard their own masters as worthy of all honor so that the name of God and our doctrine will not be spoken against."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All who are under the yoke of slavery should consider their masters worthy of full respect, so that God’s name and our teaching may not be slandered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All slaves should show full respect for their masters so they will not bring shame on the name of God and his teaching."
      }
    },
