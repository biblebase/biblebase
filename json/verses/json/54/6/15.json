    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到了时候，配得称颂、独有权能的万王之王、万主之主要使基督显现。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了適當的時候，那可稱頌的、獨一的全能者，萬王之王，萬主之主，必把基督的顯現表明出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了日期，那可稱頌、獨有權能的萬王之王、萬主之主，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "which He will bring about at the proper time—He who is the blessed and only Sovereign, the King of kings and Lord of lords,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "which God will bring about in his own time—God, the blessed and only Ruler, the King of kings and Lord of lords,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For, At just the right time Christ will be revealed from heaven by the blessed and only almighty God, the King of all kings and Lord of all lords."
      }
    },
