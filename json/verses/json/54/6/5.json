    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "使那些思想败坏、失去真理的人争论不休，甚至把敬虔当作生财之道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就和那些心術敗壞，喪失真理的人不斷地爭吵。他們視敬虔為得利的門路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並那壞了心術、失喪真理之人的爭競。他們以敬虔為得利的門路。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and constant friction between men of depraved mind and deprived of the truth, who suppose that godliness is a means of gain."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and constant friction between people of corrupt mind, who have been robbed of the truth and who think that godliness is a means to financial gain."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These people always cause trouble. Their minds are corrupt, and they have turned their backs on the truth. To them, a show of godliness is just a way to become wealthy."
      }
    },
