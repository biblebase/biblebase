    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在赐万物生命的上帝面前，也在向本丢·彼拉多做过美好见证的基督耶稣面前，我嘱咐你："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我在賜生命給萬物的　神面前，並那在本丟．彼拉多面前見證過美好的信仰的基督耶穌面前囑咐你，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我在叫萬物生活的神面前，並在向本丟‧彼拉多作過那美好見證的基督耶穌面前囑咐你："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I charge you in the presence of God, who gives life to all things, and of Christ Jesus, who testified the good confession before Pontius Pilate,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the sight of God, who gives life to everything, and of Christ Jesus, who while testifying before Pontius Pilate made the good confession, I charge you"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I charge you before God, who gives life to all, and before Christ Jesus, who gave a good testimony before Pontius Pilate,"
      }
    },
