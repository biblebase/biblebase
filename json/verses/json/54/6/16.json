    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝是独一不朽的，住在人不能接近的光中，从来没有人见过祂，也没有人能够看见祂。愿尊贵和永远的权柄都归给祂。阿们！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只有他永遠不死，住在不能接近的光裡，沒有人見過他，人也不能看見他。願尊榮和永遠的權能都歸給他。阿們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是那獨一不死、住在人不能靠近的光裏，是人未曾看見、也是不能看見的，要將他顯明出來。但願尊貴和永遠的權能都歸給他。阿們！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "who alone possesses immortality and dwells in unapproachable light, whom no man has seen or can see. To Him be honor and eternal dominion! Amen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who alone is immortal and who lives in unapproachable light, whom no one has seen or can see. To him be honor and might forever. Amen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He alone can never die, and he lives in light so brilliant that no human can approach him. No human eye has ever seen him, nor ever will. All honor and power to him forever! Amen."
      }
    },
