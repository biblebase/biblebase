    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿尊贵、荣耀归给那位不朽的、肉眼不能见的万世君王——独一无二的上帝，直到永远。阿们！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但願尊貴榮耀歸給萬世的君王，就是那不朽壞、人不能見、獨一的　神，直到永永遠遠。阿們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但願尊貴、榮耀歸與那不能朽壞、不能看見、永世的君王、獨一的神，直到永永遠遠。阿們！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now to the King eternal, immortal, invisible, the only God, be honor and glory forever and ever. Amen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now to the King eternal, immortal, invisible, the only God, be honor and glory for ever and ever. Amen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All honor and glory to God forever and ever! He is the eternal King, the unseen one who never dies; he alone is God. Amen."
      }
