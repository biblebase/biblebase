      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “敌人要攻击巴比伦的居民及其首领和谋士；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "刀劍必攻擊迦勒底人， 攻擊巴比倫的居民， 攻擊它的領袖和智慧人。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「 有刀劍臨到迦勒底人和巴比倫的居民， 並她的首領與智慧人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A sword against the Chaldeans,” declares the Lord, “And against the inhabitants of Babylon And against her officials and her wise men!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“A sword against the Babylonians!” declares the Lord— “against those who live in Babylon and against her officials and wise men!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The sword of destruction will strike the Babylonians,” says the Lord. “It will strike the people of Babylon— her officials and wise men, too."
      }
    },
