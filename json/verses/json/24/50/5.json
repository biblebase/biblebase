    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们必打听前往锡安的路，朝锡安走去。他们必凭不会被遗忘的永恒之约归附耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們要詢問往錫安的路，又面向錫安，說： ‘來吧！我們要以那永不可忘記的約與耶和華連合。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必訪問錫安，又面向這裏，說：『來吧，你們要與耶和華聯合為永遠不忘的約。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will ask for the way to Zion, turning their faces in its direction; they will come that they may join themselves to the Lord  in an everlasting covenant that will not be forgotten."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will ask the way to Zion and turn their faces toward it. They will come and bind themselves to the Lord in an everlasting covenant that will not be forgotten."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will ask the way to Jerusalem and will start back home again. They will bind themselves to the Lord with an eternal covenant that will never be forgotten."
      }
