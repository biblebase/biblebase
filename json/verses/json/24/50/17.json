    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以色列人是一群被狮子驱散的羊，先被亚述王吞噬，后被巴比伦王尼布甲尼撒咬碎骨头。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列是被趕散的羊，它被獅子趕逐。先是亞述王把它吞滅，現在巴比倫王尼布甲尼撒要咬碎它的骨頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「以色列是打散的羊，是被獅子趕出的。首先是亞述王將他吞滅，末後是巴比倫王尼布甲尼撒將他的骨頭折斷。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Israel is a scattered flock, the lions have driven them away. The first one who devoured him was the king of Assyria, and this last one who has broken his bones is Nebuchadnezzar king of Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Israel is a scattered flock that lions have chased away. The first to devour them was the king of Assyria; the last to crush their bones was Nebuchadnezzar king of Babylon.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The Israelites are like sheep that have been scattered by lions. First the king of Assyria ate them up. Then King Nebuchadnezzar of Babylon cracked their bones.”"
      }
    },
