    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝要毁灭巴比伦，像毁灭所多玛、蛾摩拉及其周围的城邑一样，使那里人烟绝迹。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就像　神傾覆的所多瑪、蛾摩拉， 和它們鄰近的城鎮一樣； 必沒有人住在那裡， 也必沒有人在那裡寄居。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「必無人住在那裏，也無人在其中寄居，要像我傾覆所多瑪、蛾摩拉，和鄰近的城邑一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As when God overthrew Sodom And Gomorrah with its neighbors,” declares the Lord, “No man will live there, Nor will any son of man reside in it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As I overthrew Sodom and Gomorrah along with their neighboring towns,” declares the Lord, “so no one will live there; no people will dwell in it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will destroy it as I destroyed Sodom and Gomorrah and their neighboring towns,” says the Lord. “No one will live there; no one will inhabit it."
      }
    },
