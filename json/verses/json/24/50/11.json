    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“抢夺我产业的巴比伦人啊， 你们欢喜快乐， 像草场上欢跳的母牛犊， 又像嘶鸣的公马。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“搶奪我產業的啊！ 你們雖然歡喜快樂， 像踹穀的母牛犢跳躍， 像雄壯的馬嘶叫；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "搶奪我產業的啊， 你們因歡喜快樂， 且像踹穀撒歡的母牛犢， 又像發嘶聲的壯馬。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Because you are glad, because you are jubilant, O you who pillage My heritage, Because you skip about like a threshing heifer And neigh like stallions,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Because you rejoice and are glad, you who pillage my inheritance, because you frolic like a heifer threshing grain and neigh like stallions,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You rejoice and are glad, you who plundered my chosen people. You frisk about like a calf in a meadow and neigh like a stallion."
      }
    },
