    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“难道人藏起来我就看不见了吗？我岂不是充满天地之间吗？这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人怎能躲藏在隱密處， 以致我看不見他呢？ 。 我豈不是充滿天地嗎？” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「人豈能在隱密處藏身，使我看不見他呢？」耶和華說：「我豈不充滿天地嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Can a man hide himself in hiding places So I do not see him?” declares the Lord. “Do I not fill the heavens and the earth?” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who can hide in secret places so that I cannot see them?” declares the Lord. “Do not I fill heaven and earth?” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can anyone hide from me in a secret place? Am I not everywhere in all the heavens and earth?” says the Lord."
      }
    },
