    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要把我幸存的子民从我流放他们去的各地召集起来，领他们回到故土，使他们子孙兴旺。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我要親自把我羊群中餘剩的，從我把牠們趕逐到的各地，招聚出來，領牠們歸回自己的羊圈，使牠們繁殖增多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要將我羊羣中所餘剩的，從我趕他們到的各國內招聚出來，領他們歸回本圈；他們也必生養眾多。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Then I Myself will gather the remnant of My flock out of all the countries where I have driven them and bring them back to their pasture, and they will be fruitful and multiply."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I myself will gather the remnant of my flock out of all the countries where I have driven them and will bring them back to their pasture, where they will be fruitful and increase in number."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I will gather together the remnant of my flock from the countries where I have driven them. I will bring them back to their own sheepfold, and they will be fruitful and increase in number."
      }
    },
