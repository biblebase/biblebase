    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但有一天，我必使这城复兴，医治她的创伤，使她繁荣昌盛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！我必使這城的傷口痊愈康復，我必醫治城中的居民，並且使他們享有十分的平安和穩妥。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，我要使這城得以痊癒安舒，使城中的人得醫治，又將豐盛的平安和誠實顯明與他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I will bring to it health and healing, and I will heal them; and I will reveal to them an abundance of peace and truth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Nevertheless, I will bring health and healing to it; I will heal my people and will let them enjoy abundant peace and security."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Nevertheless, the time will come when I will heal Jerusalem’s wounds and give it prosperity and true peace."
      }
    },
