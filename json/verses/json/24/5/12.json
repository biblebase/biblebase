    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“他们撒谎说， ‘耶和华不会管我们， 我们必不会遇到灾祸、战争和饥荒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們不信耶和華， 說：“他不會作甚麼， 災禍必不臨到我們， 我們也不會看見刀劍和饑荒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們不認耶和華，說： 這並不是他， 災禍必不臨到我們； 刀劍和饑荒，我們也看不見。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have lied about the Lord And said, “Not He; Misfortune will not come on us, And we will not see sword or famine."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They have lied about the Lord; they said, “He will do nothing! No harm will come to us; we will never see sword or famine."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“They have lied about the Lord and said, ‘He won’t bother us! No disasters will come upon us. There will be no war or famine."
      }
    },
