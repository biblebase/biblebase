    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所有的城邑必被毁灭， 无一幸免； 山谷遭践踏， 平原被蹂躏。 这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "行毀滅的必來攻擊一切城鎮， 沒有一座城可以倖免； 山谷必被蹂躪， 平原必受破壞， 正如耶和華所說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行毀滅的必來到各城， 並無一城得免。 山谷必致敗落， 平原必被毀壞； 正如耶和華所說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A destroyer will come to every city, So that no city will escape; The valley also will be ruined And the plateau will be destroyed, As the Lord has said."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The destroyer will come against every town, and not a town will escape. The valley will be ruined and the plateau destroyed, because the Lord has spoken."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“All the towns will be destroyed, and no one will escape— either on the plateaus or in the valleys, for the Lord has spoken."
      }
    },
