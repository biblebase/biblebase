    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对我说：“你要在犹大各城和耶路撒冷的街头宣告，呼吁百姓听从、遵行这约。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華又對我說：“你要在猶大各城中和耶路撒冷的街道上，宣告這一切話，說：‘你們要聽從、遵行這約的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對我說：「你要在猶大城邑中和耶路撒冷街市上，宣告這一切話說：你們當聽從遵行這約的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the Lord said to me, “Proclaim all these words in the cities of Judah and in the streets of Jerusalem, saying, ‘Hear the words of this covenant and do them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said to me, “Proclaim all these words in the towns of Judah and in the streets of Jerusalem: ‘Listen to the terms of this covenant and follow them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord said, “Broadcast this message in the streets of Jerusalem. Go from town to town throughout the land and say, ‘Remember the ancient covenant, and do everything it requires."
      }
    },
