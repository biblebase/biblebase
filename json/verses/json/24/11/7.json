    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为自从我把你们祖先从埃及领出来的那天起，一直到今天，我一再劝诫他们要听从我的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為自從我領你們的列祖出埃及地那一天，我已鄭重警告他們，直到今日，我仍不斷警告他們，說：“你們要聽從我的話。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為我將你們列祖從埃及地領出來的那日，直到今日，都是從早起來，切切誥誡他們說：『你們當聽從我的話。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I solemnly warned your fathers in the day that I brought them up from the land of Egypt, even to this day, warning persistently, saying, “Listen to My voice.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the time I brought your ancestors up from Egypt until today, I warned them again and again, saying, “Obey me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I solemnly warned your ancestors when I brought them out of Egypt, “Obey me!” I have repeated this warning over and over to this day,"
      }
    },
