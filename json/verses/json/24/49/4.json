    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "背叛的人民啊， 你们为肥沃的山谷而感到自豪吗？ 你们依仗财富以为无人敢来侵犯。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "背道的居民哪！ 你們為甚麼因有山谷， 就是你們那肥沃的山谷而誇口呢？ 你們十分倚靠自己的財寶，說： ‘有誰敢來攻擊我們呢？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "背道的民哪， 你們為何因有山谷， 就是水流的山谷誇張呢？ 為何倚靠財寶說： 誰能來到我們這裏呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How boastful you are about the valleys! Your valley is flowing away, O backsliding daughter Who trusts in her treasures, saying, ‘Who will come against me?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why do you boast of your valleys, boast of your valleys so fruitful? Unfaithful Daughter Ammon, you trust in your riches and say, ‘Who will attack me?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You are proud of your fertile valleys, but they will soon be ruined. You trusted in your wealth, you rebellious daughter, and thought no one could ever harm you."
      }
    },
