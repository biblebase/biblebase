    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以东倾覆的声音震动大地，哭喊声直传到红海。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因他們倒下的響聲，地就震動；並且有哀叫的聲音，在紅海那裡也聽得到。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他們仆倒的聲音，地就震動。人在紅海那裏必聽見呼喊的聲音。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The earth has quaked at the noise of their downfall. There is an outcry! The noise of it has been heard at the Red Sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At the sound of their fall the earth will tremble; their cry will resound to the Red Sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The earth will shake with the noise of Edom’s fall, and its cry of despair will be heard all the way to the Red Sea."
      }
    },
