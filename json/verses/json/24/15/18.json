    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为什么我的痛楚无休无止？ 为什么我的创伤无法医治？ 难道你要像一条时断时流的小溪让我无法依靠吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的痛苦為甚麼長久不止呢？ 我的創傷為甚麼無法醫治，不得痊愈呢？ 你對我真的像叫人失望、 靠不住的溪流嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的痛苦為何長久不止呢？ 我的傷痕為何無法醫治、不能痊癒呢？ 難道你待我有詭詐，像流乾的河道嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why has my pain been perpetual And my wound incurable, refusing to be healed? Will You indeed be to me like a deceptive stream With water that is unreliable?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why is my pain unending and my wound grievous and incurable? You are to me like a deceptive brook, like a spring that fails."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why then does my suffering continue? Why is my wound so incurable? Your help seems as uncertain as a seasonal brook, like a spring that has gone dry.”"
      }
    },
