    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "善跑者跑不掉， 强壮者逃不了， 他们扑倒在北面的幼发拉底河边。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“快跑的跑不掉， 勇士也不能逃脫； 他們在北方、在幼發拉底河邊， 都絆跌仆倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要容快跑的逃避； 不要容勇士逃脫； 他們在北方幼發拉底河邊絆跌仆倒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let not the swift man flee, Nor the mighty man escape; In the north beside the river Euphrates They have stumbled and fallen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The swift cannot flee nor the strong escape. In the north by the River Euphrates they stumble and fall."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The swiftest runners cannot flee; the mightiest warriors cannot escape. By the Euphrates River to the north, they stumble and fall."
      }
    },
