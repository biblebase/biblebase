    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们在那里喊叫， ‘埃及王法老只会虚张声势， 他已错失良机。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在那裡喊叫： ‘埃及王法老不過是喧嘩的聲音； 他已失去機會。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們在那裏喊叫說： 埃及王法老不過是個聲音； 他已錯過所定的時候了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They cried there, ‘Pharaoh king of Egypt is but a big noise; He has let the appointed time pass by!’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There they will exclaim, ‘Pharaoh king of Egypt is only a loud noise; he has missed his opportunity.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There they will say, ‘Pharaoh, the king of Egypt, is a loudmouth who missed his opportunity!’"
      }
    },
