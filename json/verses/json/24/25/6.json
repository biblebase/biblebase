    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要随从、供奉、祭拜其他神明，不要制造神像惹耶和华发怒，免得祂惩罚你们。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們不要隨從別的神，不要事奉敬拜他們，也不可容你們手所作的惹我發怒；這樣，我就不會使禍患臨到你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可隨從別神事奉敬拜，以你們手所做的惹我發怒；這樣，我就不加害與你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and do not go after other gods to serve them and to worship them, and do not provoke Me to anger with the work of your hands, and I will do you no harm.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not follow other gods to serve and worship them; do not arouse my anger with what your hands have made. Then I will not harm you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not provoke my anger by worshiping idols you made with your own hands. Then I will not harm you.’"
      }
    },
