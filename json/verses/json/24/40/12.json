    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "便从各地回到犹大，去米斯巴见基大利。他们储备了大量的酒和夏天的果子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有的猶大人就從被趕逐到的各地方回來，到猶大地的米斯巴去見基大利，並且收存了許多的酒和夏天的果子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這一切猶大人就從所趕到的各處回來，到猶大地米斯巴的基大利那裏，又積蓄了許多的酒，並夏天的果子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then all the Jews returned from all the places to which they had been driven away and came to the land of Judah, to Gedaliah at Mizpah, and gathered in wine and summer fruit in great abundance."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "they all came back to the land of Judah, to Gedaliah at Mizpah, from all the countries where they had been scattered. And they harvested an abundance of wine and summer fruit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "they began to return to Judah from the places to which they had fled. They stopped at Mizpah to meet with Gedaliah and then went into the Judean countryside to gather a great harvest of grapes and other crops."
      }
    },
