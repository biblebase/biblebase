    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道这用来敬拜我的殿在你们眼中竟成了贼窝？这些事，我都看在眼里。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這稱為我名下的殿，難道在你們眼中看為賊窩嗎？但我都看見了。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這稱為我名下的殿在你們眼中豈可看為賊窩嗎？我都看見了。這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Has this house, which is called by My name, become a den of robbers in your sight? Behold, I, even I, have seen it,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Has this house, which bears my Name, become a den of robbers to you? But I have been watching! declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t you yourselves admit that this Temple, which bears my name, has become a den of thieves? Surely I see all the evil going on there. I, the Lord, have spoken!"
      }
    },
