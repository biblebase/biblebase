    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们不要相信那些谎言，说这是耶和华的殿，这是耶和华的殿，这是耶和华的殿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不要倚靠虛謊的話，說：‘這是耶和華的殿，是耶和華的殿，是耶和華的殿。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們不要倚靠虛謊的話，說：『這些是耶和華的殿，是耶和華的殿，是耶和華的殿！』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not trust in deceptive words, saying, ‘This is the temple of the Lord, the temple of the Lord, the temple of the Lord.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not trust in deceptive words and say, “This is the temple of the Lord, the temple of the Lord, the temple of the Lord!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But don’t be fooled by those who promise you safety simply because the Lord’s Temple is here. They chant, “The Lord’s Temple is here! The Lord’s Temple is here!”"
      }
