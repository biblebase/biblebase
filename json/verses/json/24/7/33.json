    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "飞鸟和走兽必吃这百姓的尸体，无人驱赶它们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“這人民的屍體，也必給空中的飛鳥和地上的野獸作食物，沒有人來驅趕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且這百姓的屍首必給空中的飛鳥和地上的野獸作食物，並無人鬨趕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The dead bodies of this people will be food for the birds of the sky and for the beasts of the earth; and no one will frighten them away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the carcasses of this people will become food for the birds and the wild animals, and there will be no one to frighten them away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The bodies of my people will be food for the vultures and wild animals, and no one will be left to scare them away."
      }
    },
