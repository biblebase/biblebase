    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，虽然我们的罪控告我们， 我们屡屡叛逆得罪你， 求你为了自己的尊名而拯救我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“耶和華啊！即使我們的罪孽指證我們， 還求你因你名的緣故施行拯救。 我們實在多次背道， 得罪了你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我們的罪孽雖然作見證告我們， 還求你為你名的緣故行事。 我們本是多次背道，得罪了你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Although our iniquities testify against us, O Lord, act for Your name’s sake! Truly our apostasies have been many, We have sinned against You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Although our sins testify against us, do something, Lord, for the sake of your name. For we have often rebelled; we have sinned against you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people say, “Our wickedness has caught up with us, Lord, but help us for the sake of your own reputation. We have turned away from you and sinned against you again and again."
      }
    },
