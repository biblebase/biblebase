    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然列国的神明根本不是真神， 却没有一国更换自己的神明。 但我的子民却把自己荣耀的上帝换作了无用的假神。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有哪一國把自己的神換掉呢？ 其實那些並不是神。 我的子民竟把他們的榮耀， 換了無益的偶像。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "豈有一國換了他的神嗎？ 其實這不是神！ 但我的百姓將他們的榮耀換了那無益的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Has a nation changed gods When they were not gods? But My people have changed their glory For that which does not profit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Has a nation ever changed its gods? (Yet they are not gods at all.) But my people have exchanged their glorious God for worthless idols."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Has any nation ever traded its gods for new ones, even though they are not gods at all? Yet my people have exchanged their glorious God for worthless idols!"
      }
    },
