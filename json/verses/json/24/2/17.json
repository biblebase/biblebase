    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你这不是自食其果吗？ 因为你的上帝耶和华一路引导你的时候， 你却背弃祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你這不是自作自受嗎？ 耶和華你的　神在路上引導你時， 你卻離棄了他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這事臨到你身上，不是你自招的嗎？ 不是因耶和華－你神引你行路的時候， 你離棄他嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Have you not done this to yourself By your forsaking the Lord your God When He led you in the way?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Have you not brought this on yourselves by forsaking the Lord your God when he led you in the way?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And you have brought this upon yourselves by rebelling against the Lord your God, even though he was leading you on the way!"
      }
    },
