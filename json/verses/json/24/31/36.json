    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“只要天地之律还在， 以色列邦国必永续不断。 这是耶和华说的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“這些定律若能從我面前廢掉， 以色列的後裔也就會在我面前斷絕， 永遠不再成為一個國家。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些定例若能在我面前廢掉， 以色列的後裔也就在我面前斷絕， 永遠不再成國。 這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If this fixed order departs From before Me,” declares the Lord, “Then the offspring of Israel also will cease From being a nation before Me forever.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Only if these decrees vanish from my sight,” declares the Lord, “will Israel ever cease being a nation before me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I am as likely to reject my people Israel as I am to abolish the laws of nature!”"
      }
    },
