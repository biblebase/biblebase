      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "疲倦的人，我必使他振作；愁苦的人，我必使他滿足。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "疲乏的人，我使他飽飫；愁煩的人，我使他知足。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I satisfy the weary ones and refresh everyone who languishes.”"
      },
