    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“看啊，时候将到，我要使以色列和犹大人丁兴旺，牲畜繁多。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪！日子快到，我必使以色列家和猶大家人口和牲畜繁衍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「日子將到，我要把人的種和牲畜的種播種在以色列家和猶大家。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, days are coming,” declares the Lord, “when I will sow the house of Israel and the house of Judah with the seed of man and with the seed of beast."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The days are coming,” declares the Lord, “when I will plant the kingdoms of Israel and Judah with the offspring of people and of animals."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The day is coming,” says the Lord, “when I will greatly increase the human population and the number of animals here in Israel and Judah."
      }
    },
