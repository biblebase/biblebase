    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，我的怒火在犹大的城邑和耶路撒冷的街上燃烧，使它们沦为废墟，正如今日一样。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，我必傾出我的烈怒和怒氣，在猶大的各城鎮和耶路撒冷的街上燒起，以致它們都荒涼廢棄，像今天一樣。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，我的怒氣和忿怒都倒出來，在猶大城邑中和耶路撒冷的街市上，如火着起，以致都荒廢淒涼，正如今日一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore My wrath and My anger were poured out and burned in the cities of Judah and in the streets of Jerusalem, so they have become a ruin and a desolation as it is this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore, my fierce anger was poured out; it raged against the towns of Judah and the streets of Jerusalem and made them the desolate ruins they are today."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And so my fury boiled over and fell like fire on the towns of Judah and into the streets of Jerusalem, and they are still a desolate ruin today."
      }
    },
