    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们无论尊卑都要死在这里，无人埋葬，无人哀悼，也无人为他们的死而割伤身体、剃光头发。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“尊大的和卑微的都必死在這地；沒有人埋葬他們，沒有人為他們哀哭，也沒有人為他們割傷自己，剃光頭髮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "連大帶小，都必在這地死亡，不得葬埋。人必不為他們哀哭，不用刀劃身，也不使頭光禿。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Both great men and small will die in this land; they will not be buried, they will not be lamented, nor will anyone gash himself or shave his head for them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Both high and low will die in this land. They will not be buried or mourned, and no one will cut themselves or shave their head for the dead."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Both the great and the lowly will die in this land. No one will bury them or mourn for them. Their friends will not cut themselves in sorrow or shave their heads in sadness."
      }
    },
