    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把乌利亚从埃及押到约雅敬王面前，王用刀杀了他，把尸首抛在平民的坟地中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們就把烏利亞從埃及帶出來，送到約雅敬王那裡。王用刀殺死他，把他的屍體拋在平民的墳地裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們就從埃及將烏利亞帶出來，送到約雅敬王那裏；王用刀殺了他，把他的屍首拋在平民的墳地中。）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they brought Uriah from Egypt and led him to King Jehoiakim, who slew him with a sword and cast his dead body into the burial place of the common people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They brought Uriah out of Egypt and took him to King Jehoiakim, who had him struck down with a sword and his body thrown into the burial place of the common people.)"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They took him prisoner and brought him back to King Jehoiakim. The king then killed Uriah with a sword and had him buried in an unmarked grave."
      }
    },
