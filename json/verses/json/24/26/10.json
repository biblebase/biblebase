    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大的首领得知后，就离开王宫，上到耶和华的殿，坐在耶和华殿新门的入口。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大的領袖聽見了這些事，就從王宮上到耶和華的殿，坐在耶和華殿裡新門的入口。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大的首領聽見這事，就從王宮上到耶和華的殿，坐在耶和華殿的新門口。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the officials of Judah heard these things, they came up from the king’s house to the house of the Lord and sat in the entrance of the New Gate of the Lord’s  house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the officials of Judah heard about these things, they went up from the royal palace to the house of the Lord and took their places at the entrance of the New Gate of the Lord’s house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the officials of Judah heard what was happening, they rushed over from the palace and sat down at the New Gate of the Temple to hold court."
      }
    },
