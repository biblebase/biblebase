    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "留在这城里的必死于战争、饥荒和瘟疫，出城向迦勒底人投降的必保全性命、逃过一死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡留在這城裡的，必因刀劍、饑荒、瘟疫而死；但出去向圍困你們的迦勒底人投降的，必可以活著，撿回自己的性命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住在這城裏的必遭刀劍、饑荒、瘟疫而死；但出去歸降圍困你們迦勒底人的必得存活，要以自己的命為掠物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who dwells in this city will die by the sword and by famine and by pestilence; but he who goes out and falls away to the Chaldeans who are besieging you will live, and he will have his own life as booty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever stays in this city will die by the sword, famine or plague. But whoever goes out and surrenders to the Babylonians who are besieging you will live; they will escape with their lives."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone who stays in Jerusalem will die from war, famine, or disease, but those who go out and surrender to the Babylonians will live. Their reward will be life!"
      }
    },
