    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我伸出大能的臂膀创造了大地、地上的人类和动物，我想把大地赐给谁就赐给谁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "是我用大能和伸出來的膀臂創造了大地，以及地面上的人和獸；我看給誰適合，就把地給誰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我用大能和伸出來的膀臂，創造大地和地上的人民、牲畜。我看給誰相宜，就把地給誰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I have made the earth, the men and the beasts which are on the face of the earth by My great power and by My outstretched arm, and I will give it to the one who is pleasing in My sight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "With my great power and outstretched arm I made the earth and its people and the animals that are on it, and I give it to anyone I please."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "With my great strength and powerful arm I made the earth and all its people and every animal. I can give these things of mine to anyone I choose."
      }
    },
