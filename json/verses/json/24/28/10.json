    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "哈拿尼雅先知把轭从耶利米先知的颈上取下，折断，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是哈拿尼雅先知把耶利米先知頸項上的軛取下來，折斷了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，先知哈拿尼雅將先知耶利米頸項上的軛取下來，折斷了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Hananiah the prophet took the yoke from the neck of Jeremiah the prophet and broke it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the prophet Hananiah took the yoke off the neck of the prophet Jeremiah and broke it,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Hananiah the prophet took the yoke off Jeremiah’s neck and broke it in pieces."
      }
    },
