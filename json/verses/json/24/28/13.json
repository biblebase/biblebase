    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你去告诉哈拿尼雅，耶和华说，‘你折断了木轭，却要换来铁轭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你去告訴哈拿尼雅說：‘耶和華這樣說：你折斷了木軛，卻要換來鐵軛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你去告訴哈拿尼雅說，耶和華如此說：你折斷木軛，卻換了鐵軛！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Go and speak to Hananiah, saying, ‘Thus says the Lord, “You have broken the yokes of wood, but you have made instead of them yokes of iron.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Go and tell Hananiah, ‘This is what the Lord says: You have broken a wooden yoke, but in its place you will get a yoke of iron."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Go and tell Hananiah, ‘This is what the Lord says: You have broken a wooden yoke, but you have replaced it with a yoke of iron."
      }
