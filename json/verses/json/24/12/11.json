    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们使这片土地荒凉， 以致它在我面前哀哭。 遍地如此荒凉，却无人在意。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們使地荒涼， 在我面前一片荒涼悲哀； 全地荒涼， 卻無人關心。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們使地荒涼； 地既荒涼，便向我悲哀。 全地荒涼，因無人介意。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It has been made a desolation, Desolate, it mourns before Me; The whole land has been made desolate, Because no man lays it to heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It will be made a wasteland, parched and desolate before me; the whole land will be laid waste because there is no one who cares."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They have made it an empty wasteland; I hear its mournful cry. The whole land is desolate, and no one even cares."
      }
    },
