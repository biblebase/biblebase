    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "杀戮者已来到旷野中光秃的山岭， 耶和华使刀剑横扫全境， 无人幸免。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在曠野一切光禿的高岡上， 行毀滅的已經來到了； 因為耶和華的刀。 從地這邊直到地那邊，要吞滅一切； 人人都沒有平安。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "滅命的都來到曠野中一切淨光的高處； 耶和華的刀從地這邊直到地那邊盡行殺滅。 凡有血氣的都不得平安。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On all the bare heights in the wilderness Destroyers have come, For a sword of the Lord is devouring From one end of the land even to the other; There is no peace for anyone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Over all the barren heights in the desert destroyers will swarm, for the sword of the Lord will devour from one end of the land to the other; no one will be safe."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On all the bare hilltops, destroying armies can be seen. The sword of the Lord devours people from one end of the nation to the other. No one will escape!"
      }
    },
