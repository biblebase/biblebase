    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华凭自己起誓说： “我要使敌人如蝗虫一样铺天盖地而来， 占据你们的家园， 欢呼胜利。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華指著自己起誓，說： “我必使敵人充滿你，像蝗蟲一樣布滿你的全地， 他們必對你發出勝利的吶喊。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華指着自己起誓說： 我必使敵人充滿你，像螞蚱一樣； 他們必吶喊攻擊你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord of hosts has sworn by Himself: “Surely I will fill you with a population like locusts, And they will cry out with shouts of victory over you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord Almighty has sworn by himself: I will surely fill you with troops, as with a swarm of locusts, and they will shout in triumph over you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord of Heaven’s Armies has taken this vow and has sworn to it by his own name: “Your cities will be filled with enemies, like fields swarming with locusts, and they will shout in triumph over you.”"
      }
    },
