    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “看啊，我要使毁灭的暴风袭击巴比伦和住在立加米的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華這樣說： “看哪！我必激動毀滅者的心， 去攻擊巴比倫和住在立加米的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華如此說： 我必使毀滅的風颳起， 攻擊巴比倫和住在立加米的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord: “Behold, I am going to arouse against Babylon And against the inhabitants of Leb-kamai The spirit of a destroyer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord says: “See, I will stir up the spirit of a destroyer against Babylon and the people of Leb Kamai."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord says: “I will stir up a destroyer against Babylon and the people of Babylonia."
      }
    },
