    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“我要当着我子民的面报应巴比伦人，因为他们在锡安犯罪作恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“但我必在你們眼前報復巴比倫和迦勒底所有的居民在錫安所作的一切惡事。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「我必在你們眼前報復巴比倫人和迦勒底居民在錫安所行的諸惡。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But I will repay Babylon and all the inhabitants of Chaldea for all their evil that they have done in Zion before your eyes,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Before your eyes I will repay Babylon and all who live in Babylonia for all the wrong they have done in Zion,” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I will repay Babylon and the people of Babylonia for all the wrong they have done to my people in Jerusalem,” says the Lord."
      }
    },
