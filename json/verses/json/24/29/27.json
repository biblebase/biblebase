    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在，亚拿突人耶利米在你们当中自称为先知，你为什么不斥责他呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在，亞拿突人耶利米向你們說預言，你為甚麼不斥責他呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在亞拿突人耶利米向你們自稱為先知，你們為何沒有責備他呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "now then, why have you not rebuked Jeremiah of Anathoth who prophesies to you?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So why have you not reprimanded Jeremiah from Anathoth, who poses as a prophet among you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So why have you done nothing to stop Jeremiah from Anathoth, who pretends to be a prophet among you?"
      }
    },
