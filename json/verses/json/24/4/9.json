    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“到那天，君王和官长必丧胆，祭司和先知必惊骇。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“到那日，君王和首領的勇氣消失， 祭司也要驚惶， 先知都必詫異。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「到那時，君王和首領的心都要消滅；祭司都要驚奇，先知都要詫異。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“It shall come about in that day,” declares the Lord, “that the heart of the king and the heart of the princes will fail; and the priests will be appalled and the prophets will be astounded.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“In that day,” declares the Lord, “the king and the officials will lose heart, the priests will be horrified, and the prophets will be appalled.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“In that day,” says the Lord, “the king and the officials will tremble in fear. The priests will be struck with horror, and the prophets will be appalled.”"
      }
    },
