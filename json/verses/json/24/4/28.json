    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，地要悲哀， 天要昏暗， 因为我言出必行， 决不反悔。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，地要悲哀， 天必昏暗。 因為我說過，就不後悔； 我定了意，決不改變。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，地要悲哀， 在上的天也必黑暗； 因為我言已出，我意已定， 必不後悔，也不轉意不做。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For this the earth shall mourn And the heavens above be dark, Because I have spoken, I have purposed, And I will not change My mind, nor will I turn from it.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore the earth will mourn and the heavens above grow dark, because I have spoken and will not relent, I have decided and will not turn back.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The earth will mourn and the heavens will be draped in black because of my decree against my people. I have made up my mind and will not change it.”"
      }
    },
