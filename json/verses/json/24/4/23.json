    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我俯瞰大地， 一片空虚混沌； 我仰望天空，毫无亮光。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我觀看地，地是空虛混沌； 我仰視天，天上也全然無光。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "先知說：我觀看地， 不料，地是空虛混沌； 我觀看天，天也無光。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I looked on the earth, and behold, it was formless and void; And to the heavens, and they had no light."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I looked at the earth, and it was formless and empty; and at the heavens, and their light was gone."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I looked at the earth, and it was empty and formless. I looked at the heavens, and there was no light."
      }
    },
