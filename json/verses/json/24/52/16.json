    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只留下一些最贫穷的人，让他们照料葡萄园、耕种田地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於那地最貧窮的人，護衛長尼布撒拉旦把他們留下，去修理葡萄園和耕種田地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但護衛長尼布撒拉旦留下些民中最窮的，使他們修理葡萄園，耕種田地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Nebuzaradan the captain of the guard left some of the poorest of the land to be vinedressers and plowmen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Nebuzaradan left behind the rest of the poorest people of the land to work the vineyards and fields."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Nebuzaradan allowed some of the poorest people to stay behind to care for the vineyards and fields."
      }
    },
