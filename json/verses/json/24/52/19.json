    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "护卫长还带走了杯、火鼎、碗、盆、灯台、碟和奠酒的杯等所有金银器皿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "此外，碗盆、火鼎、碗、鍋、燈臺、碟子和奠酒的爵，無論是金的或是銀的，護衛長都拿去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "杯、火鼎、碗、盆、燈臺、調羹、爵，無論金的銀的，護衛長也都帶去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The captain of the guard also took away the bowls, the firepans, the basins, the pots, the lampstands, the pans and the drink offering bowls, what was fine gold and what was fine silver."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The commander of the imperial guard took away the basins, censers, sprinkling bowls, pots, lampstands, dishes and bowls used for drink offerings—all that were made of pure gold or silver."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The captain of the guard also took the small bowls, incense burners, basins, pots, lampstands, ladles, bowls used for liquid offerings, and all the other articles made of pure gold or silver."
      }
    },
