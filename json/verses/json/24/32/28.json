    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，我要把这城交在巴比伦王尼布甲尼撒和他率领的迦勒底人手中，让他们攻取这城。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此耶和華這樣說：‘看哪！我必把這城交在迦勒底人的手中，交在巴比倫王尼布甲尼撒的手中，他必攻取這城。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華如此說：我必將這城交付迦勒底人的手和巴比倫王尼布甲尼撒的手，他必攻取這城。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore thus says the Lord, “Behold, I am about to give this city into the hand of the Chaldeans and into the hand of Nebuchadnezzar king of Babylon, and he will take it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore this is what the Lord says: I am about to give this city into the hands of the Babylonians and to Nebuchadnezzar king of Babylon, who will capture it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Therefore, this is what the Lord says: I will hand this city over to the Babylonians and to Nebuchadnezzar, king of Babylon, and he will capture it."
      }
    },
