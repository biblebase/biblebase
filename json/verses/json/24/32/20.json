    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你曾在埃及行神迹奇事，现今仍在以色列和世界各地行神迹奇事，使自己威名远扬，直到今日。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你曾在埃及地行神蹟奇事，你在以色列和全人類中間直到今日也是這樣，使你自己得了名聲，好像今日一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在埃及地顯神蹟奇事，直到今日在以色列和別人中間也是如此，使自己得了名聲，正如今日一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "who has set signs and wonders in the land of Egypt, and even to this day both in Israel and among mankind; and You have made a name for Yourself, as at this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You performed signs and wonders in Egypt and have continued them to this day, in Israel and among all mankind, and have gained the renown that is still yours."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You performed miraculous signs and wonders in the land of Egypt—things still remembered to this day! And you have continued to do great miracles in Israel and all around the world. You have made your name famous to this day."
      }
    },
