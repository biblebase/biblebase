    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为以色列的上帝——万军之耶和华说，将来必有人在这里重新买房屋、田地和葡萄园。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為萬軍之耶和華以色列的　神這樣說：將來在這地必再有人買房屋、田地和葡萄園。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為萬軍之耶和華－以色列的神如此說：將來在這地必有人再買房屋、田地，和葡萄園。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus says the Lord of hosts, the God of Israel, “Houses and fields and vineyards will again be bought in this land.” ’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this is what the Lord Almighty, the God of Israel, says: Houses, fields and vineyards will again be bought in this land.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For this is what the Lord of Heaven’s Armies, the God of Israel, says: ‘Someday people will again own property here in this land and will buy and sell houses and vineyards and fields.’”"
      }
    },
