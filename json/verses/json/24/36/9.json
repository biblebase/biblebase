    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大王约西亚的儿子约雅敬执政第五年九月，耶路撒冷和犹大各城的人都在耶和华面前宣告禁食。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大王約西亞的兒子約雅敬第五年九月，耶路撒冷的眾民，和所有從猶大各城來到耶路撒冷的人民，都在耶和華面前宣告禁食。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大王約西亞的兒子約雅敬第五年九月，耶路撒冷的眾民和那從猶大城邑來到耶路撒冷的眾民，在耶和華面前宣告禁食的日子，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now in the fifth year of Jehoiakim the son of Josiah, king of Judah, in the ninth month, all the people in Jerusalem and all the people who came from the cities of Judah to Jerusalem proclaimed a fast before the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the ninth month of the fifth year of Jehoiakim son of Josiah king of Judah, a time of fasting before the Lord was proclaimed for all the people in Jerusalem and those who had come from the towns of Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He did this on a day of sacred fasting held in late autumn, during the fifth year of the reign of Jehoiakim son of Josiah. People from all over Judah had come to Jerusalem to attend the services at the Temple on that day."
      }
    },
