    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米说：“不会的。只要你听从耶和华吩咐我对你说的话，你就可以平安无事，保全性命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米說：“迦勒底人必不會把你交給他們。求你聽從我對你所說的耶和華的話；這樣，你就可以平安無事，你的性命也可以存活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶利米說：「迦勒底人必不將你交出。求你聽從我對你所說耶和華的話，這樣你必得好處，你的命也必存活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Jeremiah said, “They will not give you over. Please obey the Lord in what I am saying to you, that it may go well with you and you may live."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“They will not hand you over,” Jeremiah replied. “Obey the Lord by doing what I tell you. Then it will go well with you, and your life will be spared."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jeremiah replied, “You won’t be handed over to them if you choose to obey the Lord. Your life will be spared, and all will go well for you."
      }
    },
