    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "西底迦王说：“我害怕那些已经向迦勒底人投降的犹大人，恐怕迦勒底人会把我交给他们，任由他们羞辱。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "西底家王對耶利米說：“我害怕那些已向迦勒底人投降的猶大人，恐怕迦勒底人把我交在他們手中，他們就會凌辱我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "西底家王對耶利米說：「我怕那些投降迦勒底人的猶大人，恐怕迦勒底人將我交在他們手中，他們戲弄我。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then King Zedekiah said to Jeremiah, “I dread the Jews who have gone over to the Chaldeans, for they may give me over into their hand and they will abuse me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "King Zedekiah said to Jeremiah, “I am afraid of the Jews who have gone over to the Babylonians, for the Babylonians may hand me over to them and they will mistreat me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But I am afraid to surrender,” the king said, “for the Babylonians may hand me over to the Judeans who have defected to them. And who knows what they will do to me!”"
      }
    },
