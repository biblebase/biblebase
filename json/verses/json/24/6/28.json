    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们顽固不化， 如铜如铁， 四处毁谤， 极其败坏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們都是十分悖逆的人， 到處搬弄是非。 他們是銅是鐵， 他們都是敗壞的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們都是極悖逆的， 往來讒謗人。 他們是銅是鐵， 都行壞事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All of them are stubbornly rebellious, Going about as a talebearer. They are bronze and iron; They, all of them, are corrupt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are all hardened rebels, going about to slander. They are bronze and iron; they all act corruptly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They are the worst kind of rebel, full of slander. They are as hard as bronze and iron, and they lead others into corruption."
      }
    },
