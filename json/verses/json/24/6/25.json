    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要去田野， 不要在路上走， 因为外面危机四伏， 仇敌杀气腾腾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不要出到田野去， 也不要在路上走， 因為四圍有仇敵的刀劍和驚嚇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們不要往田野去， 也不要行在路上， 因四圍有仇敵的刀劍和驚嚇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not go out into the field And do not walk on the road, For the enemy has a sword, Terror is on every side."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not go out to the fields or walk on the roads, for the enemy has a sword, and there is terror on every side."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t go out to the fields! Don’t travel on the roads! The enemy’s sword is everywhere and terrorizes us at every turn!"
      }
