    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，耶和华说： “列国啊，你们要听！ 众见证人啊，要知道这些百姓的结局！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "列國啊！因此你們要聽。 以色列會眾啊！要曉得他們將遭遇的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "列國啊，因此你們當聽！ 會眾啊，要知道他們必遭遇的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore hear, O nations, And know, O congregation, what is among them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore hear, you nations; you who are witnesses, observe what will happen to them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Therefore, listen to this, all you nations. Take note of my people’s situation."
      }
