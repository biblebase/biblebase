    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们必攻击你，但绝不能胜过你，因为我与你同在，我必拯救你。这是耶和华说的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們必攻擊你，卻不能勝過你，因為我與你同在，要拯救你。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們要攻擊你，卻不能勝你；因為我與你同在，要拯救你。這是耶和華說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will fight against you, but they will not overcome you, for I am with you to deliver you,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will fight against you but will not overcome you, for I am with you and will rescue you,” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will fight you, but they will fail. For I am with you, and I will take care of you. I, the Lord, have spoken!”"
      }
