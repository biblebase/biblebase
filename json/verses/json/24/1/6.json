    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我回答说：“主耶和华啊，我太年轻，不懂得怎样说话。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我就說：“主耶和華啊！ 我不曉得說話， 因為我年輕。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我就說：主耶和華啊，我不知怎樣說，因為我是年幼的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I said, “Alas, Lord God! Behold, I do not know how to speak, Because I am a youth.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Alas, Sovereign Lord,” I said, “I do not know how to speak; I am too young.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O Sovereign Lord,” I said, “I can’t speak for you! I’m too young!”"
      }
