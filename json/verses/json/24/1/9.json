    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后，耶和华伸手摸我的口，对我说：“看啊，我已把我的话放在你口中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是耶和華伸手按著我的口，對我說： “看哪！我把我的話放在你口中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是耶和華伸手按我的口，對我說： 我已將當說的話傳給你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord stretched out His hand and touched my mouth, and the Lord said to me, “Behold, I have put My words in your mouth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the Lord reached out his hand and touched my mouth and said to me, “I have put my words in your mouth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord reached out and touched my mouth and said, “Look, I have put my words in your mouth!"
      }
