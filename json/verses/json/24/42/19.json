    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大的余民啊，耶和华已告诉你们不要去埃及。别忘了我今日给你们的警告，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大餘剩的人哪，耶和華曾對你們說：‘你們不要去埃及。’你們要確實知道，我今天已經警告你們了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所剩下的猶大人哪，耶和華論到你們說：『不要進入埃及去。』你們要確實地知道我今日警教你們了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord has spoken to you, O remnant of Judah, “Do not go into Egypt!” You should clearly understand that today I have testified against you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Remnant of Judah, the Lord has told you, ‘Do not go to Egypt.’ Be sure of this: I warn you today"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Listen, you remnant of Judah. The Lord has told you: ‘Do not go to Egypt!’ Don’t forget this warning I have given you today."
      }
    },
