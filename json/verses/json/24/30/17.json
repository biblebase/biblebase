    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然你们这些锡安人无家可归， 无人关心， 但我要医治你们的创伤， 使你们康复。 这是耶和华说的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然人都說你是被放逐的， 是沒有人關心的錫安； 我卻必使你康復， 醫治你的創傷。’” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：我必使你痊癒， 醫好你的傷痕， 都因人稱你為被趕散的， 說：這是錫安，無人來探問的！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I will restore you to health And I will heal you of your wounds,’ declares the Lord, ‘Because they have called you an outcast, saying: “It is Zion; no one cares for her.” ’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I will restore you to health and heal your wounds,’ declares the Lord, ‘because you are called an outcast, Zion for whom no one cares.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will give you back your health and heal your wounds,” says the Lord. “For you are called an outcast— ‘Jerusalem for whom no one cares.’”"
      }
    },
