    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他为困苦和贫穷的人申冤， 所以他凡事顺利。 这才是真正认识我。 这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他為困苦和貧窮人伸冤， 那時他得享福樂。 這不是認識我的真義嗎？” 　這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他為困苦和窮乏人伸冤， 那時就得了福樂。 認識我不在乎此嗎？ 這是耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He pled the cause of the afflicted and needy; Then it was well. Is not that what it means to know Me?” Declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He defended the cause of the poor and needy, and so all went well. Is that not what it means to know me?” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He gave justice and help to the poor and needy, and everything went well for him. Isn’t that what it means to know me?” says the Lord."
      }
    },
