    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘坐在大卫宝座上的犹大王啊，你和你的臣仆以及进出这城门的百姓都要听耶和华的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說：‘坐大衛王位的猶大王啊！你和你的臣僕，以及從這些門進來的人民，都要聽耶和華的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：『坐大衛寶座的猶大王啊，你和你的臣僕，並進入城門的百姓，都當聽耶和華的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and say, ‘Hear the word of the Lord, O king of Judah, who sits on David’s throne, you and your servants and your people who enter these gates."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘Hear the word of the Lord to you, king of Judah, you who sit on David’s throne—you, your officials and your people who come through these gates."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘Listen to this message from the Lord, you king of Judah, sitting on David’s throne. Let your attendants and your people listen, too."
      }
    },
