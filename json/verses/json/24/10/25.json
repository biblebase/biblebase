    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你向不认识你的列国和不求告你名的民族发烈怒， 因为他们吞噬、毁灭雅各， 使他的家园一片荒凉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願你把你的烈怒傾倒在不認識你的列國， 和不求告你名的萬族身上； 因為他們吞吃了雅各， 他們吞吃了他，把他消滅， 使他的住處荒涼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願你將忿怒傾在不認識你的列國中， 和不求告你名的各族上； 因為他們吞了雅各，不但吞了，而且滅絕， 把他的住處變為荒場。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Pour out Your wrath on the nations that do not know You And on the families that do not call Your name; For they have devoured Jacob; They have devoured him and consumed him And have laid waste his habitation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Pour out your wrath on the nations that do not acknowledge you, on the peoples who do not call on your name. For they have devoured Jacob; they have devoured him completely and destroyed his homeland."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Pour out your wrath on the nations that refuse to acknowledge you— on the peoples that do not call upon your name. For they have devoured your people Israel; they have devoured and consumed them, making the land a desolate wilderness."
      }
    },
