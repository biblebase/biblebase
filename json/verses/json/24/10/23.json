    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，人不能驾驭自己的命运， 不能左右自己的将来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！我知道 人的道路是不由自己的； 人行走時，也不能確定自己的腳步。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我曉得人的道路不由自己， 行路的人也不能定自己的腳步。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I know, O Lord, that a man’s way is not in himself, Nor is it in a man who walks to direct his steps."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, I know that people’s lives are not their own; it is not for them to direct their steps."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I know, Lord, that our lives are not our own. We are not able to plan our own course."
      }
    },
