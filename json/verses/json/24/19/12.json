    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必这样惩罚这城和其中的居民，使这城像陀斐特一样。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必照樣對待這地方和這地方的居民，使這城像陀斐特一樣。’這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：我必向這地方和其中的居民如此行，使這城與陀斐特一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is how I will treat this place and its inhabitants,” declares the Lord, “so as to make this city like Topheth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what I will do to this place and to those who live here, declares the Lord. I will make this city like Topheth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what I will do to this place and its people, says the Lord. I will cause this city to become defiled like Topheth."
      }
    },
