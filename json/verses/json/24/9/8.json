    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们舌如毒箭，说话诡诈， 对邻居口蜜腹剑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的舌頭是殺人的利箭， 嘴裡說的是詭詐； 跟鄰舍說話滿口甜言蜜語， 心底裡卻設計陷害他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的舌頭是毒箭，說話詭詐； 人與鄰舍口說和平話， 心卻謀害他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their tongue is a deadly arrow; It speaks deceit; With his mouth one speaks peace to his neighbor, But inwardly he sets an ambush for him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their tongue is a deadly arrow; it speaks deceitfully. With their mouths they all speak cordially to their neighbors, but in their hearts they set traps for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For their tongues shoot lies like poisoned arrows. They speak friendly words to their neighbors while scheming in their heart to kill them."
      }
    },
