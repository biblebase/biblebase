    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "真希望我的头是水泉， 眼是泪泉， 好为我被杀的同胞昼夜哭泣；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但願我的頭是水井， 我的眼是淚的泉源； 我好為我同胞中被殺的， 晝夜哭泣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但願我的頭為水， 我的眼為淚的泉源， 我好為我百姓中被殺的人晝夜哭泣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Oh that my head were waters And my eyes a fountain of tears, That I might weep day and night For the slain of the daughter of my people!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Oh, that my head were a spring of water and my eyes a fountain of tears! I would weep day and night for the slain of my people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If only my head were a pool of water and my eyes a fountain of tears, I would weep day and night for all my people who have been slaughtered."
      }
    },
