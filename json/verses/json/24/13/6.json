    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "多日后，耶和华对我说：“去幼发拉底河边取回我吩咐你藏的腰带。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "過了多日，耶和華對我說：“起來，往幼發拉底河去，把我吩咐你藏在那裡的腰帶取出來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "過了多日，耶和華對我說：「你起來往幼發拉底河去，將我吩咐你藏在那裏的腰帶取出來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After many days the Lord said to me, “Arise, go to the Euphrates and take from there the waistband which I commanded you to hide there.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Many days later the Lord said to me, “Go now to Perath and get the belt I told you to hide there.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A long time afterward the Lord said to me, “Go back to the Euphrates and get the loincloth I told you to hide there.”"
      }
    },
