    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“这是耶和华说的，‘我也要这样消灭犹大的骄傲和耶路撒冷的狂妄。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“耶和華這樣說：‘我必照樣敗壞猶大的尊榮和耶路撒冷極大的尊榮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「耶和華如此說：我必照樣敗壞猶大的驕傲和耶路撒冷的大驕傲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Thus says the Lord, ‘Just so will I destroy the pride of Judah and the great pride of Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Lord says: ‘In the same way I will ruin the pride of Judah and the great pride of Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Lord says: This shows how I will rot away the pride of Judah and Jerusalem."
      }
    },
