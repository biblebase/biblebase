    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我就去幼发拉底河边把所藏的腰带挖出来，但腰带已经腐烂，不能用了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是我往幼發拉底河去，在那裡挖掘，把腰帶從我埋藏的地方取出來；看哪！腰帶已經腐爛了，完全沒用了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我就往幼發拉底河去，將腰帶從我所藏的地方刨出來，見腰帶已經變壞，毫無用了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I went to the Euphrates and dug, and I took the waistband from the place where I had hidden it; and lo, the waistband was ruined, it was totally worthless."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I went to Perath and dug up the belt and took it from the place where I had hidden it, but now it was ruined and completely useless."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I went to the Euphrates and dug it out of the hole where I had hidden it. But now it was rotting and falling apart. The loincloth was good for nothing."
      }
    },
