    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说，‘但那时，我必拯救你，使你不致落在你所惧怕的人手中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但到了那日，我必拯救你；你必不至交在你所懼怕的人手裡。’這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：到那日我必拯救你，你必不致交在你所怕的人手中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I will deliver you on that day,” declares the Lord, “and you will not be given into the hand of the men whom you dread."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I will rescue you on that day, declares the Lord; you will not be given into the hands of those you fear."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "but I will rescue you from those you fear so much."
      }
    },
