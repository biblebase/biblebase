    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我耶和华鉴察人心， 察看人的意念， 按照人的行为报应人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我耶和華是察透人心， 試驗人肺腑的， 要照著各人的行為， 和各人所作的事應得的結果報應各人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我－耶和華是鑒察人心、試驗人肺腑的， 要照各人所行的和他做事的結果報應他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I, the Lord, search the heart, I test the mind, Even to give to each man according to his ways, According to the results of his deeds."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I the Lord search the heart and examine the mind, to reward each person according to their conduct, according to what their deeds deserve.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I, the Lord, search all hearts and examine secret motives. I give all people their due rewards, according to what their actions deserve.”"
      }
    },
