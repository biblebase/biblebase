    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘耶和华说，你们要留心听我的话，在安息日不可运货物进耶路撒冷的城门，不可做任何工作，要守安息日为圣日。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華宣告說：“但如果你們真心聽從我，不在安息日挑擔子進入這城的城門，並且守安息日為聖日，在那日不作任何工作；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「你們若留意聽從我，在安息日不擔甚麼擔子進入這城的各門，只以安息日為聖日，在那日無論何工都不做；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But it will come about, if you listen attentively to Me,” declares the Lord, “to bring no load in through the gates of this city on the sabbath day, but to keep the sabbath day holy by doing no work on it,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if you are careful to obey me, declares the Lord, and bring no load through the gates of this city on the Sabbath, but keep the Sabbath day holy by not doing any work on it,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“‘But if you obey me, says the Lord, and do not carry on your trade at the gates or work on the Sabbath day, and if you keep it holy,"
      }
    },
