    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你为什么要偷偷地溜走？你为什么不告诉我，我好击鼓、弹琴、唱歌欢送你？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你為甚麼暗中逃走，瞞著我不讓我知道，使我可以歡歡喜喜地唱歌、擊鼓、彈琴給你送行呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你為甚麼暗暗地逃跑，偷着走，並不告訴我，叫我可以歡樂、唱歌、擊鼓、彈琴地送你回去？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why did you flee secretly and deceive me, and did not tell me so that I might have sent you away with joy and with songs, with timbrel and with lyre;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why did you run off secretly and deceive me? Why didn’t you tell me, so I could send you away with joy and singing to the music of timbrels and harps?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why did you slip away secretly? Why did you deceive me? And why didn’t you say you wanted to leave? I would have given you a farewell feast, with singing and music, accompanied by tambourines and harps."
      }
    },
