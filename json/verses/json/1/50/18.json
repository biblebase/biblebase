    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约瑟的哥哥们来见他，俯伏在他面前，说：“我们是你的奴仆。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的哥哥們也親自來了，俯伏在他面前，說：“看哪，我們是你的奴僕。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的哥哥們又來俯伏在他面前，說：「我們是你的僕人。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then his brothers also came and fell down before him and said, “Behold, we are your servants.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His brothers then came and threw themselves down before him. “We are your slaves,” they said."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then his brothers came and threw themselves down before Joseph. “Look, we are your slaves!” they said."
      }
