    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她就连忙放下肩上的水罐，说，‘请喝，我也打水给你的骆驼喝。’我喝了，她又打水给骆驼喝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她急忙從肩上拿下水瓶來，說：‘請喝，我還要給你的駱駝喝。’我喝了，她也打水給我的駱駝喝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她就急忙從肩頭上拿下瓶來，說：『請喝！我也給你的駱駝喝。』我便喝了；她又給我的駱駝喝了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She quickly lowered her jar from her shoulder, and said, ‘Drink, and I will water your camels also’; so I drank, and she watered the camels also."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“She quickly lowered her jar from her shoulder and said, ‘Drink, and I’ll water your camels too.’ So I drank, and she watered the camels also."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She quickly lowered her jug from her shoulder and said, ‘Yes, have a drink, and I will water your camels, too!’ So I drank, and then she watered the camels."
      }
    },
