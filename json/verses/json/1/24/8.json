    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果那女子不肯跟你来，你也算履行了你向我起的誓，只是你不可带我儿子回那里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果那女子不願跟你來，我叫你所起的這誓就與你無關了。只是不可帶我的兒子回到那裡去。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "倘若女子不肯跟你來，我使你起的誓就與你無干了，只是不可帶我的兒子回那裏去。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But if the woman is not willing to follow you, then you will be free from this my oath; only do not take my son back there.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If the woman is unwilling to come back with you, then you will be released from this oath of mine. Only do not take my son back there.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If she is unwilling to come back with you, then you are free from this oath of mine. But under no circumstances are you to take my son there.”"
      }
    },
