    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "利亚说：“我把婢女送给丈夫做妾，现在上帝给我报酬了。”因此，她就为孩子取名叫以萨迦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "利亞說：“　神給了我酬報，因為我把我的婢女給了我的丈夫。”於是給孩子起名叫以薩迦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利亞說：「神給了我價值，因為我把使女給了我丈夫」，於是給他起名叫以薩迦。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Leah said, “God has given me my wages because I gave my maid to my husband.” So she named him Issachar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Leah said, “God has rewarded me for giving my servant to my husband.” So she named him Issachar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She named him Issachar, for she said, “God has rewarded me for giving my servant to my husband as a wife.”"
      }
    },
