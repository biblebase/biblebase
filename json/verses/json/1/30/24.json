      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她给孩子取名叫约瑟，又说：“愿上帝再给我添一个儿子！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是給孩子起名叫約瑟，說：“願耶和華再給我一個兒子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就給他起名叫約瑟，意思說：「願耶和華再增添我一個兒子。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She named him Joseph, saying, “May the Lord give me another son.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She named him Joseph, and said, “May the Lord add to me another son.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And she named him Joseph, for she said, “May the Lord add yet another son to my family.”"
      }
