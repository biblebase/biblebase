    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伯兰对罗得说：“我们不该彼此争执，我们的牧人也不该互相争执，因为我们是骨肉至亲。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞伯蘭就對羅得說：“你我之間，切不可紛爭；你的牧人和我的牧人之間，也不可紛爭，因為我們是至親。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞伯蘭就對羅得說：「你我不可相爭，你的牧人和我的牧人也不可相爭，因為我們是骨肉。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Abram said to Lot, “Please let there be no strife between you and me, nor between my herdsmen and your herdsmen, for we are brothers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Abram said to Lot, “Let’s not have any quarreling between you and me, or between your herders and mine, for we are close relatives."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Finally Abram said to Lot, “Let’s not allow this conflict to come between us or our herdsmen. After all, we are close relatives!"
      }
    },
