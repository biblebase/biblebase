    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，罗得选了整个约旦河平原，向东迁移，他们便分开了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "羅得選擇了約旦河整個平原，於是向東移動，他們就彼此分開了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是羅得選擇約旦河的全平原，往東遷移；他們就彼此分離了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Lot chose for himself all the valley of the Jordan, and Lot journeyed eastward. Thus they separated from each other."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Lot chose for himself the whole plain of the Jordan and set out toward the east. The two men parted company:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lot chose for himself the whole Jordan Valley to the east of them. He went there with his flocks and servants and parted company with his uncle Abram."
      }
    },
