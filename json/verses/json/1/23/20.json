    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这块赫人的地和其中的山洞从此成了亚伯拉罕家的坟地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，那塊田和田中的洞，就從赫人歸給了亞伯拉罕為產業作墳地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從此，那塊田和田間的洞就藉着赫人定準歸與亞伯拉罕作墳地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the field and the cave that is in it, were deeded over to Abraham for a burial site by the sons of Heth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the field and the cave in it were deeded to Abraham by the Hittites as a burial site."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the field and the cave were transferred from the Hittites to Abraham for use as a permanent burial place."
      }
    },
