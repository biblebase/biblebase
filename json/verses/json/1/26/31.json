    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第二天早晨，以撒和他们互相起誓之后，送他们平安地离去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "第二天，他們清早起來，彼此起誓；以撒就送他們走，他們平平安安地離開他走了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們清早起來彼此起誓。以撒打發他們走，他們就平平安安地離開他走了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In the morning they arose early and exchanged oaths; then Isaac sent them away and they departed from him in peace."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Early the next morning the men swore an oath to each other. Then Isaac sent them on their way, and they went away peacefully."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Early the next morning, they each took a solemn oath not to interfere with each other. Then Isaac sent them home again, and they left him in peace."
      }
    },
