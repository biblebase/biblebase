    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，亚比米勒下令全国：凡伤害他们二人的，必被处死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是，亞比米勒吩咐全國人民，說：“凡是觸犯這人或是他的妻子的，必被處死。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是亞比米勒曉諭眾民說：「凡沾着這個人，或是他妻子的，定要把他治死。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Abimelech charged all the people, saying, “He who touches this man or his wife shall surely be put to death.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Abimelek gave orders to all the people: “Anyone who harms this man or his wife shall surely be put to death.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Abimelech issued a public proclamation: “Anyone who touches this man or his wife will be put to death!”"
      }
    },
