    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约瑟把梦告诉了父亲和哥哥们，他父亲就责备他说：“你做的是什么梦！难道你的父母弟兄都要来向你俯伏下拜吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約瑟把夢向他父親和哥哥們述說了，他父親就斥責他，說：“你所作的這夢是甚麼夢呢？我和你母親，以及你的兄弟，真的要來俯伏在地向你下拜嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約瑟將這夢告訴他父親和他哥哥們，他父親就責備他說：「你做的這是甚麼夢！難道我和你母親、你弟兄果然要來俯伏在地，向你下拜嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He related it to his father and to his brothers; and his father rebuked him and said to him, “What is this dream that you have had? Shall I and your mother and your brothers actually come to bow ourselves down before you to the ground?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he told his father as well as his brothers, his father rebuked him and said, “What is this dream you had? Will your mother and I and your brothers actually come and bow down to the ground before you?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This time he told the dream to his father as well as to his brothers, but his father scolded him. “What kind of dream is that?” he asked. “Will your mother and I and your brothers actually come and bow to the ground before you?”"
      }
