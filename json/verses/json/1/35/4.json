    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把所有外族的神像和自己的耳环都交给雅各，雅各把这些东西埋在示剑的橡树下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們就把自己手中所有外族人的神像，和自己耳朵上的環子，都交給雅各；雅各把這些東西埋在示劍附近的橡樹底下。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們就把外邦人的神像和他們耳朵上的環子交給雅各；雅各都藏在示劍那裏的橡樹底下。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they gave to Jacob all the foreign gods which they had and the rings which were in their ears, and Jacob hid them under the oak which was near Shechem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So they gave Jacob all the foreign gods they had and the rings in their ears, and Jacob buried them under the oak at Shechem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So they gave Jacob all their pagan idols and earrings, and he buried them under the great tree near Shechem."
      }
    },
