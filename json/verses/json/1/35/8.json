    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来利百加的奶妈底波拉死了，葬在伯特利山谷下面的橡树下，那棵树叫亚伦·巴古。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "利百加的乳母底波拉死了，葬在伯特利下邊的橡樹底下，所以給那棵樹起名叫亞倫．巴古。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利百加的奶母底波拉死了，就葬在伯特利下邊橡樹底下；那棵樹名叫亞倫‧巴古。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Deborah, Rebekah’s nurse, died, and she was buried below Bethel under the oak; it was named Allon-bacuth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now Deborah, Rebekah’s nurse, died and was buried under the oak outside Bethel. So it was named Allon Bakuth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Soon after this, Rebekah’s old nurse, Deborah, died. She was buried beneath the oak tree in the valley below Bethel. Ever since, the tree has been called Allon-bacuth (which means “oak of weeping”)."
      }
    },
