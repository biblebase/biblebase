    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大认出是自己的东西，就说：“她比我更有理，因为我没有让示拉娶她。”犹大没有再与她睡觉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大認出那些東西，就說：“她比我更有理，因為我沒有把她給我的兒子示拉為妻。”從此猶大就不再與她親近了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大承認說：「她比我更有義，因為我沒有將她給我的兒子示拉。」從此猶大不再與她同寢了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Judah recognized them, and said, “She is more righteous than I, inasmuch as I did not give her to my son Shelah.” And he did not have relations with her again."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Judah recognized them and said, “She is more righteous than I, since I wouldn’t give her to my son Shelah.” And he did not sleep with her again."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Judah recognized them immediately and said, “She is more righteous than I am, because I didn’t arrange for her to marry my son Shelah.” And Judah never slept with Tamar again."
      }
    },
