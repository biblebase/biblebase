    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他的朋友只好回去对犹大说：“我找不着她，当地人说那里没有庙妓。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那人回去見猶大，說：“我找不著她，並且那地的人都說：‘這裡是沒有妓女的。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他回去見猶大說：「我沒有找着她，並且那地方的人說：『這裏沒有妓女。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he returned to Judah, and said, “I did not find her; and furthermore, the men of the place said, ‘There has been no temple prostitute here.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So he went back to Judah and said, “I didn’t find her. Besides, the men who lived there said, ‘There hasn’t been any shrine prostitute here.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Hirah returned to Judah and told him, “I couldn’t find her anywhere, and the men of the village claim they’ve never had a shrine prostitute there.”"
      }
    },
