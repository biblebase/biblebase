    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "自己走到离孩子约一箭之远的地方，对着孩子坐下，说：“我不忍心看着他死啊！”夏甲坐在那里放声大哭起来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後自己走開，在離開約有一箭之遠的地方，相對而坐，說：“我不忍眼看孩子死去。”就相對坐著，放聲大哭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "自己走開約有一箭之遠，相對而坐，說：「我不忍見孩子死」，就相對而坐，放聲大哭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then she went and sat down opposite him, about a bowshot away, for she said, “Do not let me see the boy die.” And she sat opposite him, and lifted up her voice and wept."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then she went off and sat down about a bowshot away, for she thought, “I cannot watch the boy die.” And as she sat there, she began to sob."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then she went and sat down by herself about a hundred yards away. “I don’t want to watch the boy die,” she said, as she burst into tears."
      }
    },
