    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倒不如请我主先走，我迁就牲畜和孩子慢慢走，我在西珥与我主会合。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "請我主在僕人前頭先行；我要照著在我面前群畜和孩子們的速度，慢慢前行，直走到西珥我主那裡去。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求我主在僕人前頭走，我要量着在我面前羣畜和孩子的力量慢慢地前行，直走到西珥我主那裏。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Please let my lord pass on before his servant, and I will proceed at my leisure, according to the pace of the cattle that are before me and according to the pace of the children, until I come to my lord at Seir.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So let my lord go on ahead of his servant, while I move along slowly at the pace of the flocks and herds before me and the pace of the children, until I come to my lord in Seir.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Please, my lord, go ahead of your servant. We will follow slowly, at a pace that is comfortable for the livestock and the children. I will meet you at Seir.”"
      }
    },
