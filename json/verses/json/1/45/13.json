    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要把我在埃及享有的尊荣和你们见到的一切都告诉父亲，尽快把他接来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們也要把我在埃及的一切尊榮，和你們所看見的一切，都告訴我的父親，又要趕快把我的父親接到這裡來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們也要將我在埃及一切的榮耀和你們所看見的事都告訴我父親，又要趕緊地將我父親搬到我這裏來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now you must tell my father of all my splendor in Egypt, and all that you have seen; and you must hurry and bring my father down here.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Tell my father about all the honor accorded me in Egypt and about everything you have seen. And bring my father down here quickly.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Go tell my father of my honored position here in Egypt. Describe for him everything you have seen, and then bring my father here quickly.”"
      }
    },
