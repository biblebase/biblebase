    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "取名叫挪亚，他说：“耶和华咒诅了大地，以致我们艰辛劳苦，这孩子必使我们从艰辛劳苦中得安慰。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就給他起名叫挪亞，說：“這兒子必使我們從地上的操作和手中的勞苦得著安慰，因為耶和華曾經咒詛這地。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "給他起名叫挪亞，說：「這個兒子必為我們的操作和手中的勞苦安慰我們；這操作勞苦是因為耶和華咒詛地。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now he called his name Noah, saying, “This one will give us rest from our work and from the toil of our hands arising from the ground which the Lord has cursed.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He named him Noah and said, “He will comfort us in the labor and painful toil of our hands caused by the ground the Lord has cursed.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lamech named his son Noah, for he said, “May he bring us relief from our work and the painful labor of farming this ground that the Lord has cursed.”"
      }
    },
