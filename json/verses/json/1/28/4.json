    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿上帝把赐给亚伯拉罕的福气赐给你和你的后代，让你承受你寄居之地为产业，就是上帝赐给亚伯拉罕的地方。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願　神把應許給亞伯拉罕的福，賜給你和你的後裔，使你可以得著你寄居的地為業，就是　神賜給亞伯拉罕的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將應許亞伯拉罕的福賜給你和你的後裔，使你承受你所寄居的地為業，就是神賜給亞伯拉罕的地。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May He also give you the blessing of Abraham, to you and to your descendants with you, that you may possess the land of your sojournings, which God gave to Abraham.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May he give you and your descendants the blessing given to Abraham, so that you may take possession of the land where you now reside as a foreigner, the land God gave to Abraham.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May God pass on to you and your descendants the blessings he promised to Abraham. May you own this land where you are now living as a foreigner, for God gave this land to Abraham.”"
      }
    },
