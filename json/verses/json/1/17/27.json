    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伯拉罕家里所有的男子，包括家中出生的和买回来的，都一起接受了割礼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞伯拉罕家裡所有的男人，無論是在家裡生的，或是用銀子從外族人買來的，都與他一同受了割禮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "家裏所有的人，無論是在家裏生的，是用銀子從外人買的，也都一同受了割禮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the men of his household, who were born in the house or bought with money from a foreigner, were circumcised with him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And every male in Abraham’s household, including those born in his household or bought from a foreigner, was circumcised with him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "along with all the other men and boys of the household, whether they were born there or bought as servants. All were circumcised with him."
      }
    },
