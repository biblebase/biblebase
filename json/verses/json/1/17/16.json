    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必赐福给她，让她为你生一个儿子。她必成为万族的母亲，万民的君王必从她而出。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必賜福給她，也必使她為你生一個兒子；我要賜福給她，她也要作多國的母，萬族的君王必從她而出。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必賜福給她，也要使你從她得一個兒子。我要賜福給她，她也要作多國之母；必有百姓的君王從她而出。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will bless her, and indeed I will give you a son by her. Then I will bless her, and she shall be a mother of nations; kings of peoples will come from her.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will bless her and will surely give you a son by her. I will bless her so that she will be the mother of nations; kings of peoples will come from her.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I will bless her and give you a son from her! Yes, I will bless her richly, and she will become the mother of many nations. Kings of nations will be among her descendants.”"
      }
    },
