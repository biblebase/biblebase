    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他祝福约瑟说：“愿我祖先亚伯拉罕和以撒敬拜的上帝，牧养我一生直到今天的上帝，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他祝福約瑟，說： “願我祖亞伯拉罕和以撒所侍候的　神， 一生牧養我直到今日的　神，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他就給約瑟祝福說：「願我祖亞伯拉罕和我父以撒所事奉的神，就是一生牧養我直到今日的神，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He blessed Joseph, and said, “The God before whom my fathers Abraham and Isaac walked, The God who has been my shepherd all my life to this day,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he blessed Joseph and said, “May the God before whom my fathers Abraham and Isaac walked faithfully, the God who has been my shepherd all my life to this day,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he blessed Joseph and said, “May the God before whom my grandfather Abraham and my father, Isaac, walked— the God who has been my shepherd all my life, to this very day,"
      }
    },
