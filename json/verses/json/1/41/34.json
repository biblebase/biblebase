    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王也应该在各地委派官员，在七年丰收期内征收全国出产的五分之一，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老要這樣行，在國中指派官員，當七年豐收的時候，徵收埃及地出產的五分之一。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老當這樣行，又派官員管理這地。當七個豐年的時候，征收埃及地的五分之一，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let Pharaoh take action to appoint overseers in charge of the land, and let him exact a fifth of the produce of the land of Egypt in the seven years of abundance."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let Pharaoh appoint commissioners over the land to take a fifth of the harvest of Egypt during the seven years of abundance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Pharaoh should appoint supervisors over the land and let them collect one-fifth of all the crops during the seven good years."
      }
    },
