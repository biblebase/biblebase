    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这些粮食要留到七年饥荒时用，免得这片土地被饥荒毁灭。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些糧食要儲存起來，可以應付將來埃及地的七年饑荒，免得這地被饑荒所滅。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所積蓄的糧食可以防備埃及地將來的七個荒年，免得這地被饑荒所滅。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let the food become as a reserve for the land for the seven years of famine which will occur in the land of Egypt, so that the land will not perish during the famine.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This food should be held in reserve for the country, to be used during the seven years of famine that will come upon Egypt, so that the country may not be ruined by the famine.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That way there will be enough to eat when the seven years of famine come to the land of Egypt. Otherwise this famine will destroy the land.”"
      }
    },
