    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是，它们吃了以后却仍然像以前那么丑陋。这时，我就醒了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠們吃了以後，卻看不出牠們是吃掉了肥牛，因為牠們醜陋的樣子仍是與先前一樣。我就醒了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "吃了以後卻看不出是吃了，那醜陋的樣子仍舊和先前一樣。我就醒了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet when they had devoured them, it could not be detected that they had devoured them, for they were just as ugly as before. Then I awoke."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But even after they ate them, no one could tell that they had done so; they looked just as ugly as before. Then I woke up."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But afterward you wouldn’t have known it, for they were still as thin and scrawny as before! Then I woke up."
      }
    },
