    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "各国的人都到埃及来向约瑟买粮，因为天下到处都是大饥荒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "各地的人都到埃及去，向約瑟買糧，因為各地的饑荒都非常嚴重。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "各地的人都往埃及去，到約瑟那裏糴糧，因為天下的饑荒甚大。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The people of all the earth came to Egypt to buy grain from Joseph, because the famine was severe in all the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And all the world came to Egypt to buy grain from Joseph, because the famine was severe everywhere."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And people from all around came to Egypt to buy grain from Joseph because the famine was severe throughout the world."
      }
    },
