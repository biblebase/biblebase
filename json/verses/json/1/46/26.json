    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "跟雅各一同到埃及去的儿孙共有六十六人，他的儿媳妇除外。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有從雅各所生、與雅各一同來到埃及的人，除了他的兒婦之外，一共六十六人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那與雅各同到埃及的，除了他兒婦之外，凡從他所生的，共有六十六人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the persons belonging to Jacob, who came to Egypt, his direct descendants, not including the wives of Jacob’s sons, were sixty-six persons in all,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All those who went to Egypt with Jacob—those who were his direct descendants, not counting his sons’ wives—numbered sixty-six persons."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The total number of Jacob’s direct descendants who went with him to Egypt, not counting his sons’ wives, was sixty-six."
      }
    },
