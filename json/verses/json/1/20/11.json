    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伯拉罕回答说：“因为我想这里的人不敬畏上帝，他们必为了夺我妻子而杀我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞伯拉罕回答：“因為我以為這地方必定沒有敬畏　神的人，他們會因我妻子的緣故殺我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞伯拉罕說：「我以為這地方的人總不懼怕神，必為我妻子的緣故殺我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Abraham said, “Because I thought, surely there is no fear of God in this place, and they will kill me because of my wife."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Abraham replied, “I said to myself, ‘There is surely no fear of God in this place, and they will kill me because of my wife.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Abraham replied, “I thought, ‘This is a godless place. They will want my wife and will kill me to get her.’"
      }
    },
