    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他见那地方好作安身之处， 地土肥美，就垂下肩头， 做了奴隶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他看安居為美， 他看肥地為佳， 他屈肩負重， 成了服苦的奴僕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他以安靜為佳，以肥地為美， 便低肩背重，成為服苦的僕人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he saw that a resting place was good And that the land was pleasant, He bowed his shoulder to bear burdens, And became a slave at forced labor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he sees how good is his resting place and how pleasant is his land, he will bend his shoulder to the burden and submit to forced labor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When he sees how good the countryside is and how pleasant the land, he will bend his shoulder to the load and submit himself to hard labor."
      }
    },
