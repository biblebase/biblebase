    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雅各又嘱咐他们说：“我要离世了，你们要把我葬在赫人以弗仑田间的洞里，让我与祖先在一起。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雅各又囑咐他們，對他們說：“我快要歸到我的親族那裡去，你們要把我埋葬在赫人以弗崙田間的洞裡，與我的祖先在一起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又囑咐他們說：「我將要歸到我列祖那裏，你們要將我葬在赫人以弗崙田間的洞裏，與我祖我父在一處，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he charged them and said to them, “I am about to be gathered to my people; bury me with my fathers in the cave that is in the field of Ephron the Hittite,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he gave them these instructions: “I am about to be gathered to my people. Bury me with my fathers in the cave in the field of Ephron the Hittite,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jacob instructed them, “Soon I will die and join my ancestors. Bury me with my father and grandfather in the cave in the field of Ephron the Hittite."
      }
    },
