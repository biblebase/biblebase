    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你父亲的祝福高过亘古永存的峰峦， 多如绵延无尽的群山， 愿这一切的祝福都临到约瑟头上， 临到这超越众弟兄的人身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你父親的祝福， 勝過我祖先的祝福，勝過永遠山嶺上的美物。 願這些福都降在約瑟的頭上， 降在兄弟中作王子的那一位頭上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你父親所祝的福 勝過我祖先所祝的福， 如永世的山嶺，至極的邊界； 這些福必降在約瑟的頭上， 臨到那與弟兄迥別之人的頂上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The blessings of your father Have surpassed the blessings of my ancestors Up to the utmost bound of the everlasting hills; May they be on the head of Joseph, And on the crown of the head of the one distinguished among his brothers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your father’s blessings are greater than the blessings of the ancient mountains, than the bounty of the age-old hills. Let all these rest on the head of Joseph, on the brow of the prince among his brothers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May my fatherly blessings on you surpass the blessings of my ancestors, reaching to the heights of the eternal hills. May these blessings rest on the head of Joseph, who is a prince among his brothers."
      }
    },
