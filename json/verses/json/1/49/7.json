    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们狂暴凶残，该受咒诅！ 我要使他们分散在雅各的子孙中， 散居在以色列各地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的怒氣可咒，因為非常暴烈； 他們的烈怒可詛，因為十分兇猛； 我要把他們分散在雅各家； 把他們散居在以色列。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的怒氣暴烈可咒； 他們的忿恨殘忍可詛。 我要使他們分居在雅各家裏， 散住在以色列地中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Cursed be their anger, for it is fierce; And their wrath, for it is cruel. I will disperse them in Jacob, And scatter them in Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Cursed be their anger, so fierce, and their fury, so cruel! I will scatter them in Jacob and disperse them in Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A curse on their anger, for it is fierce; a curse on their wrath, for it is cruel. I will scatter them among the descendants of Jacob; I will disperse them throughout Israel."
      }
    },
