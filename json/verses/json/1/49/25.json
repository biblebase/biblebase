    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你父亲的上帝必帮助你， 全能者必赐你天上的恩泽、 地上的百福， 使你子孙兴旺、牛羊满圈。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "是你父親的　神幫助你， 是那全能者賜給你； 他把從天上來的福， 地下深淵蘊藏著的福， 哺乳和生育的福，都賜給你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你父親的神必幫助你； 那全能者必將天上所有的福， 地裏所藏的福，以及生產乳養的福，都賜給你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the God of your father who helps you, And by the Almighty who blesses you With blessings of heaven above, Blessings of the deep that lies beneath, Blessings of the breasts and of the womb."
      },
