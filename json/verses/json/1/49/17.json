    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他必成为路边的蛇， 道旁的毒蛇， 咬伤马蹄，使骑马的人坠落。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但要作路上的蛇， 道中的毒蛇， 他要咬傷馬蹄， 使騎馬的人向後墜落。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但必作道上的蛇，路中的虺， 咬傷馬蹄，使騎馬的墜落於後。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Dan shall be a serpent in the way, A horned snake in the path, That bites the horse’s heels, So that his rider falls backward."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Dan will be a snake by the roadside, a viper along the path, that bites the horse’s heels so that its rider tumbles backward."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Dan will be a snake beside the road, a poisonous viper along the path that bites the horse’s hooves so its rider is thrown off."
      }
    },
