    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们到了上帝指示的地方，亚伯拉罕就筑起祭坛，把柴摆在上面，然后把以撒捆起来放在柴上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們到了　神指示他的地方，亞伯拉罕就在那裡築了一座祭壇，擺好了柴，捆綁了自己的兒子以撒，就把他放在祭壇的柴上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們到了神所指示的地方，亞伯拉罕在那裏築壇，把柴擺好，捆綁他的兒子以撒，放在壇的柴上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they came to the place of which God had told him; and Abraham built the altar there and arranged the wood, and bound his son Isaac and laid him on the altar, on top of the wood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they reached the place God had told him about, Abraham built an altar there and arranged the wood on it. He bound his son Isaac and laid him on the altar, on top of the wood."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they arrived at the place where God had told him to go, Abraham built an altar and arranged the wood on it. Then he tied his son, Isaac, and laid him on the altar on top of the wood."
      }
    },
