    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，亚伯拉罕回到仆人等候的地方，他们一同返回亚伯拉罕居住的别示巴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞伯拉罕回到他的童僕那裡；他們就起程，一起到別是巴去了；亞伯拉罕就住在別是巴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是亞伯拉罕回到他僕人那裏，他們一同起身往別是巴去，亞伯拉罕就住在別是巴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Abraham returned to his young men, and they arose and went together to Beersheba; and Abraham lived at Beersheba."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Abraham returned to his servants, and they set off together for Beersheba. And Abraham stayed in Beersheba."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they returned to the servants and traveled back to Beersheba, where Abraham continued to live."
      }
