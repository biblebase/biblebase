    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雅各说：“请你告诉我你的名字。”那人说：“你为什么问我的名字？”他就在那里给雅各祝福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雅各問他，說：“請把你的名告訴我。”那人回答：“為甚麼問我的名呢？”他就在那裡給雅各祝福。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雅各問他說：「請將你的名告訴我。」那人說：「何必問我的名？」於是在那裏給雅各祝福。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jacob asked him and said, “Please tell me your name.” But he said, “Why is it that you ask my name?” And he blessed him there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jacob said, “Please tell me your name.” But he replied, “Why do you ask my name?” Then he blessed him there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Please tell me your name,” Jacob said. “Why do you want to know my name?” the man replied. Then he blessed Jacob there."
      }
    },
