    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雅各看见他们，就说：“这是上帝的军队！”于是称那地方为玛哈念。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雅各看見他們，就說：“這是　神的軍隊。”於是給那地方起名叫瑪哈念。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雅各看見他們就說：「這是神的軍兵」，於是給那地方起名叫瑪哈念。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jacob said when he saw them, “This is God’s camp.” So he named that place Mahanaim."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jacob saw them, he said, “This is the camp of God!” So he named that place Mahanaim."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jacob saw them, he exclaimed, “This is God’s camp!” So he named the place Mahanaim."
      }
