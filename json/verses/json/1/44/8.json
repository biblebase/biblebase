    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你看，我们把上次在口袋里发现的钱都从迦南带回来还你了，又怎么会偷你主人家的金银呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你看，我們從前在袋口發現的銀子，尚且從迦南地帶來還給你，我們又怎會從你主人的家裡偷取金銀呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你看，我們從前在口袋裏所見的銀子，尚且從迦南地帶來還你，我們怎能從你主人家裏偷竊金銀呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, the money which we found in the mouth of our sacks we have brought back to you from the land of Canaan. How then could we steal silver or gold from your lord’s house?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We even brought back to you from the land of Canaan the silver we found inside the mouths of our sacks. So why would we steal silver or gold from your master’s house?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Didn’t we return the money we found in our sacks? We brought it back all the way from the land of Canaan. Why would we steal silver or gold from your master’s house?"
      }
    },
