    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大说：“那人严厉地警告我们，‘不把你们的弟弟带来，你们就不要再来见我。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大對他說：“那人認真地警告我們說：‘如果你們的弟弟不與你們一起來，你們就不得見我的面。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大對他說：「那人諄諄地告誡我們說：『你們的兄弟若不與你們同來，你們就不得見我的面。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Judah spoke to him, however, saying, “The man solemnly warned us, ‘You shall not see my face unless your brother is with you.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Judah said to him, “The man warned us solemnly, ‘You will not see my face again unless your brother is with you.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Judah said, “The man was serious when he warned us, ‘You won’t see my face again unless your brother is with you.’"
      }
    },
