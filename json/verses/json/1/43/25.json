    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们知道要在那里吃午餐，就预备好礼物，等约瑟中午回来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是，他們預備好了禮物，等候約瑟中午到來，因為他們聽說他們要在那裡吃飯。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們就預備那禮物，等候約瑟晌午來，因為他們聽見要在那裏吃飯。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they prepared the present for Joseph’s coming at noon; for they had heard that they were to eat a meal there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They prepared their gifts for Joseph’s arrival at noon, because they had heard that they were to eat there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They were told they would be eating there, so they prepared their gifts for Joseph’s arrival at noon."
      }
    },
