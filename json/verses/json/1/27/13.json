    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他母亲利百加对他说：“孩子啊，有咒诅，我来承担。你只管听我的话，把羊羔牵来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他母親對他說：“我兒啊，願你所招的咒詛歸到我身上吧，你只管聽我的話，去把羊牽來給我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他母親對他說：「我兒，你招的咒詛歸到我身上；你只管聽我的話，去把羊羔給我拿來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But his mother said to him, “Your curse be on me, my son; only obey my voice, and go, get them for me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His mother said to him, “My son, let the curse fall on me. Just do what I say; go and get them for me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But his mother replied, “Then let the curse fall on me, my son! Just do what I tell you. Go out and get the goats for me!”"
      }
    },
