    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以扫的计谋传到利百加耳中。她就派人把雅各叫来，对他说：“你哥哥以扫想要杀你泄愤！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有人把利百加大兒子以掃所說的話告訴了利百加，她就派人把她小兒子雅各叫了來，對他說：“你哥哥以掃想要殺你，報仇雪恨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人把利百加大兒子以掃的話告訴利百加，她就打發人去，叫了她小兒子雅各來，對他說：「你哥哥以掃想要殺你，報仇雪恨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when the words of her elder son Esau were reported to Rebekah, she sent and called her younger son Jacob, and said to him, “Behold your brother Esau is consoling himself concerning you by planning to kill you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Rebekah was told what her older son Esau had said, she sent for her younger son Jacob and said to him, “Your brother Esau is planning to avenge himself by killing you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Rebekah heard about Esau’s plans. So she sent for Jacob and told him, “Listen, Esau is consoling himself by plotting to kill you."
      }
    },
