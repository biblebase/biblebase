    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝看见世界败坏了，因为世人行为败坏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神觀看大地，看見世界已經敗壞了；全人類在地上所行的都是敗壞的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神觀看世界，見是敗壞了；凡有血氣的人在地上都敗壞了行為。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God looked on the earth, and behold, it was corrupt; for all flesh had corrupted their way upon the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God saw how corrupt the earth had become, for all the people on earth had corrupted their ways."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God observed all this corruption in the world, for everyone on earth was corrupt."
      }
    },
