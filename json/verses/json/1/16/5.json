    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撒莱对亚伯兰说：“我受气都是因为你。我把婢女放在你怀里，她仗着自己怀了孕就轻视我，愿耶和华在你我之间主持公道。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒萊對亞伯蘭說：“我因你受屈；我把我的婢女送到你的懷中，她見自己有了孕，就輕看我。願耶和華在你我之間主持公道。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "撒萊對亞伯蘭說：「我因你受屈。我將我的使女放在你懷中，她見自己有了孕，就小看我。願耶和華在你我中間判斷。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Sarai said to Abram, “May the wrong done me be upon you. I gave my maid into your arms, but when she saw that she had conceived, I was despised in her sight. May the Lord judge between you and me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Sarai said to Abram, “You are responsible for the wrong I am suffering. I put my slave in your arms, and now that she knows she is pregnant, she despises me. May the Lord judge between you and me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Sarai said to Abram, “This is all your fault! I put my servant into your arms, but now that she’s pregnant she treats me with contempt. The Lord will show who’s wrong—you or me!”"
      }
    },
