    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我们不能把妹妹嫁给没有受过割礼的人，这对我们是一种羞辱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對他們說：“我們不能把妹妹給一個沒有受割禮的人為妻，因為那是我們的恥辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對他們說：「我們不能把我們的妹子給沒有受割禮的人為妻，因為那是我們的羞辱。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said to them, “We cannot do this thing, to give our sister to one who is uncircumcised, for that would be a disgrace to us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They said to them, “We can’t do such a thing; we can’t give our sister to a man who is not circumcised. That would be a disgrace to us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They said to them, “We couldn’t possibly allow this, because you’re not circumcised. It would be a disgrace for our sister to marry a man like you!"
      }
    },
