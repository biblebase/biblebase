    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为什么说她是你妹妹，以致我娶她为妻呢？现在你妻子在这里，带她走吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你為甚麼說她是你的妹妹，以致我娶了她作妻子呢？現在，你的妻子在這裡，帶她走吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為甚麼說她是你的妹子，以致我把她取來要作我的妻子？現在你的妻子在這裏，可以帶她走吧。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why did you say, ‘She is my sister,’ so that I took her for my wife? Now then, here is your wife, take her and go.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why did you say, ‘She is my sister,’ so that I took her to be my wife? Now then, here is your wife. Take her and go!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why did you say, ‘She is my sister,’ and allow me to take her as my wife? Now then, here is your wife. Take her and get out of here!”"
      }
