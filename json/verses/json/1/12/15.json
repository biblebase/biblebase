    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老的官员见了撒莱，就在法老面前称赞她的美貌，她便被带进法老的王宫。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老的一些大臣看見了撒萊，就在法老面前稱讚她；於是那女人被帶進法老的宮裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老的臣宰看見了她，就在法老面前誇獎她。那婦人就被帶進法老的宮去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Pharaoh’s officials saw her and praised her to Pharaoh; and the woman was taken into Pharaoh’s house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And when Pharaoh’s officials saw her, they praised her to Pharaoh, and she was taken into his palace."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the palace officials saw her, they sang her praises to Pharaoh, their king, and Sarai was taken into his palace."
      }
    },
