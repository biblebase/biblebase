    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必赐福给那些祝福你的人，咒诅那些咒诅你的人。世上万族必因你而蒙福。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "給你祝福的，我必賜福給他； 咒詛你的，我必咒詛他； 地上的萬族，都必因你得福。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為你祝福的，我必賜福與他；那咒詛你的，我必咒詛他。地上的萬族都要因你得福。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And I will bless those who bless you, And the one who curses you I will curse. And in you all the families of the earth will be blessed.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will bless those who bless you, and whoever curses you I will curse; and all peoples on earth will be blessed through you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will bless those who bless you and curse those who treat you with contempt. All the families on earth will be blessed through you.”"
      }
    },
