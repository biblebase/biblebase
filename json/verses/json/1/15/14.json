    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我必惩罚奴役他们的国家，之后他们必带着大量的财物离开那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們所要服事的那國，我要親自懲罰。後來他們必帶著很多財物，從那裡出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且他們所要服事的那國，我要懲罰，後來他們必帶着許多財物從那裏出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I will also judge the nation whom they will serve, and afterward they will come out with many possessions."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I will punish the nation they serve as slaves, and afterward they will come out with great possessions."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I will punish the nation that enslaves them, and in the end they will come away with great wealth."
      }
