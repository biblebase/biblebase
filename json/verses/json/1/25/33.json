    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雅各说：“好，你现在向我起誓保证吧！”于是，以扫就起誓把长子的名分卖给了雅各。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雅各說：“你先向我起誓吧。”以掃就向他起了誓，把自己的長子名分賣給雅各。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雅各說：「你今日對我起誓吧。」以掃就對他起了誓，把長子的名分賣給雅各。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jacob said, “First swear to me”; so he swore to him, and sold his birthright to Jacob."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Jacob said, “Swear to me first.” So he swore an oath to him, selling his birthright to Jacob."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jacob said, “First you must swear that your birthright is mine.” So Esau swore an oath, thereby selling all his rights as the firstborn to his brother, Jacob."
      }
