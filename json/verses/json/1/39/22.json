    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "监狱长把狱中所有囚犯及一切事务都交给约瑟管理。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，監獄長把監裡所有的囚犯都交在約瑟手裡；他們在那裡所作的一切事務，都由約瑟負責處理。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "司獄就把監裏所有的囚犯都交在約瑟的手下；他們在那裏所辦的事都是經他的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The chief jailer committed to Joseph’s charge all the prisoners who were in the jail; so that whatever was done there, he was responsible for it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the warden put Joseph in charge of all those held in the prison, and he was made responsible for all that was done there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Before long, the warden put Joseph in charge of all the other prisoners and over everything that happened in the prison."
      }
    },
