    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "三天之内，法老必砍下你的头，把你挂在木头上，鸟要来吃你的肉。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "三天之內，法老必砍下你的頭來，把你掛在木頭上，必有飛鳥來吃你身上的肉。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "三天之內，法老必斬斷你的頭，把你掛在木頭上，必有飛鳥來吃你身上的肉。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "within three more days Pharaoh will lift up your head from you and will hang you on a tree, and the birds will eat your flesh off you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Within three days Pharaoh will lift off your head and impale your body on a pole. And the birds will eat away your flesh.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Three days from now Pharaoh will lift you up and impale your body on a pole. Then birds will come and peck away at your flesh.”"
      }
