    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们口出狂言，空话连篇，以肉体的邪情私欲为饵，诱惑那些刚刚摆脱了荒谬生活的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們說虛妄誇大的話，用肉體的私慾和邪蕩的事，引誘那些剛剛逃脫了錯謬生活的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們說虛妄矜誇的大話，用肉身的情慾和邪淫的事引誘那些剛才脫離妄行的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For speaking out arrogant words of vanity they entice by fleshly desires, by sensuality, those who barely escape from the ones who live in error,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For they mouth empty, boastful words and, by appealing to the lustful desires of the flesh, they entice people who are just escaping from those who live in error."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They brag about themselves with empty, foolish boasting. With an appeal to twisted sexual desires, they lure back into sin those who have barely escaped from a lifestyle of deception."
      }
    },
