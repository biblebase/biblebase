    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们如同枯干的水井和被狂风吹散的雾气，有幽幽黑暗留给他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些人是無水的泉源，是暴風催逼的霧氣，有漆黑的幽暗為他們存留。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些人是無水的井，是狂風催逼的霧氣，有墨黑的幽暗為他們存留。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These are springs without water and mists driven by a storm, for whom the black darkness has been reserved."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These people are springs without water and mists driven by a storm. Blackest darkness is reserved for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These people are as useless as dried-up springs or as mist blown away by the wind. They are doomed to blackest darkness."
      }
    },
