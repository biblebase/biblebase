    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们明知道正路，却背弃了传给他们的神圣诫命，这样倒不如不知道的好。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "既然認識了義路，竟又背棄傳給他們的聖誡命，對他們來說，倒不如不認識好得多了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們曉得義路，竟背棄了傳給他們的聖命，倒不如不曉得為妙。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it would be better for them not to have known the way of righteousness, than having known it, to turn away from the holy commandment handed on to them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It would have been better for them not to have known the way of righteousness, than to have known it and then to turn their backs on the sacred command that was passed on to them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It would be better if they had never known the way to righteousness than to know it and then reject the command they were given to live a holy life."
      }
    },
