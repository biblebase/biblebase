    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即使力量和权能更大的天使都不敢在主面前用毁谤的话控告有尊荣的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就是天使，雖有更大的力量與權能，尚且不用毀謗的話在主面前控告他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是天使，雖然力量權能更大，還不用毀謗的話在主面前告他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "whereas angels who are greater in might and power do not bring a reviling judgment against them before the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "yet even angels, although they are stronger and more powerful, do not heap abuse on such beings when bringing judgment on them from the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the angels, who are far greater in power and strength, do not dare to bring from the Lord a charge of blasphemy against those supernatural beings."
      }
    },
