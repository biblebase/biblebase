      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神沒有姑息犯罪的天使，反而把他們丟入地獄，囚禁在幽暗的坑裡，等候審判。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是天使犯了罪，神也沒有寬容，曾把他們丟在地獄，交在黑暗坑中，等候審判。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if God did not spare angels when they sinned, but cast them into hell and committed them to pits of darkness, reserved for judgment;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For if God did not spare angels when they sinned, but sent them to hell, putting them in chains of darkness to be held for judgment;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For God did not spare even the angels who sinned. He threw them into hell, in gloomy pits of darkness, where they are being held until the day of judgment."
      }
