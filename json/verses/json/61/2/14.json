    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们眼中充满淫欲，不停地犯罪，引诱那些信心不坚定的人。他们贪婪成性，实在是该受咒诅！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們滿眼淫色，而且不住地犯罪。他們引誘心志不堅固的人。他們的心習慣了貪婪，是應當受咒詛的族類。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們滿眼是淫色，止不住犯罪，引誘那心不堅固的人，心中習慣了貪婪，正是被咒詛的種類。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "having eyes full of adultery that never cease from sin, enticing unstable souls, having a heart trained in greed, accursed children;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "With eyes full of adultery, they never stop sinning; they seduce the unstable; they are experts in greed—an accursed brood!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They commit adultery with their eyes, and their desire for sin is never satisfied. They lure unstable people into sin, and they are well trained in greed. They live under God’s curse."
      }
    },
