    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们故意忽略：太初，上帝凭口中的话语创造了诸天，并借着水从水中造出了地；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們故意忘記這事：在太古的時候，因著　神的話，就有了天，和從水而出、藉水而成的地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們故意忘記，從太古，憑神的命有了天，並從水而出、藉水而成的地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For when they maintain this, it escapes their notice that by the word of God the heavens existed long ago and the earth was formed out of water and by water,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But they deliberately forget that long ago by God’s word the heavens came into being and the earth was formed out of water and by water."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They deliberately forget that God made the heavens long ago by the word of his command, and he brought the earth out from the water and surrounded it with water."
      }
    },
