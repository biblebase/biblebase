    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "相反，你们要在我们的主和救主耶稣基督的恩典中不断长进，越来越认识祂。 愿荣耀归给祂，从现在直到永远。阿们！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們卻要在我們的主、救主耶穌基督的恩典和知識上長進。願榮耀歸給他，從現在直到永遠。阿們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們卻要在我們主－救主耶穌基督的恩典和知識上有長進。願榮耀歸給他，從今直到永遠。阿們！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but grow in the grace and knowledge of our Lord and Savior Jesus Christ. To Him be the glory, both now and to the day of eternity. Amen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But grow in the grace and knowledge of our Lord and Savior Jesus Christ. To him be glory both now and forever! Amen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rather, you must grow in the grace and knowledge of our Lord and Savior Jesus Christ. All glory to him, both now and forever! Amen."
      }
    },
