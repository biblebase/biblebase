    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "同样，现在的天地要凭上帝的话语存留到不敬虔的人被审判和毁灭的日子，那时天地要被火焚烧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但現在的天地，還是因著同樣的話可以存留，直到不敬虔的人受審判和遭滅亡的日子，用火焚燒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但現在的天地還是憑着那命存留，直留到不敬虔之人受審判遭沉淪的日子，用火焚燒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But by His word the present heavens and earth are being reserved for fire, kept for the day of judgment and destruction of ungodly men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "By the same word the present heavens and earth are reserved for fire, being kept for the day of judgment and destruction of the ungodly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And by the same word, the present heavens and earth have been stored up for fire. They are being kept for the day of judgment, when ungodly people will be destroyed."
      }
    },
