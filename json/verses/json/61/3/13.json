    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我们等候的是上帝应许的新天新地，有公义住在其中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是我們按照他所應許的，等候新天新地，有公義在那裡居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我們照他的應許，盼望新天新地，有義居在其中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But according to His promise we are looking for new heavens and a new earth, in which righteousness dwells."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But in keeping with his promise we are looking forward to a new heaven and a new earth, where righteousness dwells."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But we are looking forward to the new heavens and new earth he has promised, a world filled with God’s righteousness."
      }
    },
