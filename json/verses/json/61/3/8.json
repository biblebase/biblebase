    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亲爱的弟兄姊妹，有一件事你们不可忽略，就是主看一日如千年，千年如一日。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "親愛的，這一件事你們不可忘記：在主看來，一日如千年，千年如一日。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "親愛的弟兄啊，有一件事你們不可忘記，就是主看一日如千年，千年如一日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But do not let this one fact escape your notice, beloved, that with the Lord one day is like a thousand years, and a thousand years like one day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But do not forget this one thing, dear friends: With the Lord a day is like a thousand years, and a thousand years are like a day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you must not forget this one thing, dear friends: A day is like a thousand years to the Lord, and a thousand years is like a day."
      }
    },
