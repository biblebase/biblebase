    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂从父上帝那里得到了尊贵和荣耀，那时从极大的荣光中有声音对祂说：“这是我的爱子，我甚喜悦祂。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他從父　神得著尊貴榮耀的時候，在極顯赫的榮光中，有這樣的聲音對他說：“這是我的愛子，我所喜悅的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他從父神得尊貴榮耀的時候，從極大榮光之中有聲音出來，向他說：「這是我的愛子，我所喜悅的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For when He received honor and glory from God the Father, such an utterance as this was made to Him by the Majestic Glory, “This is My beloved Son with whom I am well-pleased”—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He received honor and glory from God the Father when the voice came to him from the Majestic Glory, saying, “This is my Son, whom I love; with him I am well pleased.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "when he received honor and glory from God the Father. The voice from the majestic glory of God said to him, “This is my dearly loved Son, who brings me great joy.”"
      }
    },
