    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是耶稣基督的奴仆和使徒西门·彼得，写信给借着我们的上帝和救主耶稣基督的义，与我们得到同样宝贵信心的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌基督的僕人和使徒西門．彼得，寫信給那靠著我們的　神和救主耶穌基督的義，得著和我們同樣寶貴信心的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "作耶穌基督僕人和使徒的西門‧彼得寫信給那因我們的神和救主耶穌基督之義、與我們同得一樣寶貴信心的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Simon Peter, a bond-servant and apostle of Jesus Christ, To those who have received a faith of the same kind as ours, by the righteousness of our God and Savior, Jesus Christ:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Simon Peter, a servant and apostle of Jesus Christ, To those who through the righteousness of our God and Savior Jesus Christ have received a faith as precious as ours:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This letter is from Simon Peter, a slave and apostle of Jesus Christ. I am writing to you who share the same precious faith we have. This faith was given to you because of the justice and fairness of Jesus Christ, our God and Savior."
      }
    },
