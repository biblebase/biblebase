    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的肚脐好像圆杯， 不缺调和的酒。 你的腰好比一堆麦子， 四周围绕着百合花。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的肚臍如圓杯， 永不缺乏調和的酒； 你的肚腹像一堆麥子， 周圍有百合花。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的肚臍如圓杯， 不缺調和的酒； 你的腰如一堆麥子， 周圍有百合花。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your navel is like a round goblet Which never lacks mixed wine; Your belly is like a heap of wheat Fenced about with lilies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your navel is a rounded goblet that never lacks blended wine. Your waist is a mound of wheat encircled by lilies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your navel is perfectly formed like a goblet filled with mixed wine. Between your thighs lies a mound of wheat bordered with lilies."
      }
    },
