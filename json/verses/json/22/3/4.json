    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我刚离开他们，便找到了我心爱的人。 我拉着他，不让他走； 我把他带回娘家， 到怀我者的卧室。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我剛剛離開他們， 就遇見我心所愛的； 我把他拉住，不讓他走， 等到我帶他到我母親的家， 進到懷我者的內室。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我剛離開他們就遇見我心所愛的。 我拉住他，不容他走， 領他入我母家， 到懷我者的內室。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Scarcely had I left them When I found him whom my soul loves; I held on to him and would not let him go Until I had brought him to my mother’s house, And into the room of her who conceived me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Scarcely had I passed them when I found the one my heart loves. I held him and would not let him go till I had brought him to my mother’s house, to the room of the one who conceived me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then scarcely had I left them when I found my love! I caught and held him tightly, then I brought him to my mother’s house, into my mother’s bed, where I had been conceived."
      }
