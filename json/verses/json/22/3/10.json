    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "银轿柱、金靠背、紫锦坐垫， 轿子里面的装饰都是耶路撒冷少女们用爱情编织的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他用銀子做轎柱， 用金子做轎底， 用紫色料子做轎的座墊； 內部所鋪設的， 是耶路撒冷眾女子的愛情。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "轎柱是用銀做的， 轎底是用金做的； 坐墊是紫色的， 其中所鋪的乃耶路撒冷眾女子的愛情。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He made its posts of silver, Its back of gold And its seat of purple fabric, With its interior lovingly fitted out By the daughters of Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Its posts he made of silver, its base of gold. Its seat was upholstered with purple, its interior inlaid with love. Daughters of Jerusalem,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Its posts are silver, its canopy gold; its cushions are purple. It was decorated with love by the young women of Jerusalem."
      }
    },
