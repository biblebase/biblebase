    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的妹妹，我的新娘， 你是紧锁的园子， 封闭的井，隐藏的泉源。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的妹妹，我的新婦，是上了閂的園子， 是上了鎖的水井，是封閉了的泉源。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我妹子，我新婦， 乃是關鎖的園， 禁閉的井，封閉的泉源。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A garden locked is my sister, my bride, A rock garden locked, a spring sealed up."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are a garden locked up, my sister, my bride; you are a spring enclosed, a sealed fountain."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You are my private garden, my treasure, my bride, a secluded spring, a hidden fountain."
      }
    },
