    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的爱人啊，你真是美极了！ 面纱掩映着那温柔如鸽的双眸， 你的秀发就好像跃下基列山冈的山羊群。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的佳偶啊！你很美麗； 你很美麗。 你的眼在面紗後面好像鴿子的眼睛； 頭髮如同從基列山下來的山羊群。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的佳偶，你甚美麗！你甚美麗！ 你的眼在帕子內好像鴿子眼。 你的頭髮如同山羊羣臥在基列山旁。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“How beautiful you are, my darling, How beautiful you are! Your eyes are like doves behind your veil; Your hair is like a flock of goats That have descended from Mount Gilead."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How beautiful you are, my darling! Oh, how beautiful! Your eyes behind your veil are doves. Your hair is like a flock of goats descending from the hills of Gilead."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You are beautiful, my darling, beautiful beyond words. Your eyes are like doves behind your veil. Your hair falls in waves, like a flock of goats winding down the slopes of Gilead."
      }
    },
