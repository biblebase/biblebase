    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的妹妹，我的新娘啊， 你夺去了我的心。 你的秋波一转， 你项链上的明珠一闪， 都令我倾倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的妹妹，我的新婦啊！你奪去了我的心。 你用你的眼神、 用你項鍊上的一顆珍珠奪去了我的心。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我妹子，我新婦， 你奪了我的心。 你用眼一看， 用你項上的一條金鍊， 奪了我的心！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have made my heart beat faster, my sister, my bride; You have made my heart beat faster with a single glance of your eyes, With a single strand of your necklace."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have stolen my heart, my sister, my bride; you have stolen my heart with one glance of your eyes, with one jewel of your necklace."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have captured my heart, my treasure, my bride. You hold it hostage with one glance of your eyes, with a single jewel of your necklace."
      }
    },
