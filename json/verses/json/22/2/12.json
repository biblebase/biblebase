    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大地百花盛开， 百鸟争鸣的季节已经来临， 田野也传来斑鸠的叫声。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "地上百花齊放， 百鳥齊鳴的時候已經來到； 斑鳩的聲音在我們境內也聽見了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "地上百花開放， 百鳥鳴叫的時候已經來到； 斑鳩的聲音在我們境內也聽見了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The flowers have already appeared in the land; The time has arrived for pruning the vines, And the voice of the turtledove has been heard in our land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Flowers appear on the earth; the season of singing has come, the cooing of doves is heard in our land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The flowers are springing up, the season of singing birds has come, and the cooing of turtledoves fills the air."
      }
    },
