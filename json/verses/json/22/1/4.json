    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你带我走，让我们一起奔跑吧！ 王啊，把我带进你的寝宫吧！ 我们为你欢喜快乐， 我们要颂扬你那胜似美酒的爱情。 难怪少女们都爱慕你！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願你吸引我，我們必快跑跟隨你。 王領我進入他的內室。 王啊！我們要因你歡喜快樂； 我們要提說你的愛，勝過提說美酒； 她們愛你是合理的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願你吸引我，我們就快跑跟隨你。 王帶我進了內室， 我們必因你歡喜快樂。 我們要稱讚你的愛情， 勝似稱讚美酒。 他們愛你是理所當然的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Draw me after you and let us run together! The king has brought me into his chambers.” “We will rejoice in you and be glad; We will extol your love more than wine. Rightly do they love you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Take me away with you—let us hurry! Let the king bring me into his chambers. We rejoice and delight in you; we will praise your love more than wine. How right they are to adore you!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Take me with you; come, let’s run! The king has brought me into his bedroom. How happy we are for you, O king. We praise your love even more than wine. How right they are to adore you."
      }
    },
