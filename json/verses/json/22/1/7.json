    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我心爱的人啊，告诉我吧， 你在哪里牧羊？ 中午带羊去哪里歇息？ 我何必在你同伴的羊群旁流浪， 像风尘女子一样呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我心所愛的啊！求你告訴我， 你在哪裡放羊？ 中午你在哪裡使羊群躺臥休息。 我為甚麼在你眾同伴的羊群旁邊， 好像蒙著臉的女人呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我心所愛的啊，求你告訴我， 你在何處牧羊？ 晌午在何處使羊歇臥？ 我何必在你同伴的羊羣旁邊 好像蒙着臉的人呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Tell me, O you whom my soul loves, Where do you pasture your flock, Where do you make it lie down at noon? For why should I be like one who veils herself Beside the flocks of your companions?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Tell me, you whom I love, where you graze your flock and where you rest your sheep at midday. Why should I be like a veiled woman beside the flocks of your friends?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Tell me, my love, where are you leading your flock today? Where will you rest your sheep at noon? For why should I wander like a prostitute among your friends and their flocks?"
      }
    },
