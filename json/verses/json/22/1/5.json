    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶路撒冷的少女啊！ 我虽然黑似基达的帐篷， 却美如所罗门的锦帐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶路撒冷的眾女子啊！ 我雖然黑，卻是秀美， 如同基達的帳棚， 好像所羅門的幔幕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷的眾女子啊， 我雖然黑，卻是秀美， 如同基達的帳棚， 好像所羅門的幔子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I am black but lovely, O daughters of Jerusalem, Like the tents of Kedar, Like the curtains of Solomon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Dark am I, yet lovely, daughters of Jerusalem, dark like the tents of Kedar, like the tent curtains of Solomon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am dark but beautiful, O women of Jerusalem— dark as the tents of Kedar, dark as the curtains of Solomon’s tents."
      }
    },
