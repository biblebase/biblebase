    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的美名如此芬芳， 如同倒出来的香膏， 难怪少女们都爱慕你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的膏油香氣芬芳； 你的名字如同倒出來的膏油； 所以眾童女都愛你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的膏油馨香； 你的名如同倒出來的香膏， 所以眾童女都愛你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your oils have a pleasing fragrance, Your name is like purified oil; Therefore the maidens love you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Pleasing is the fragrance of your perfumes; your name is like perfume poured out. No wonder the young women love you!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How pleasing is your fragrance; your name is like the spreading fragrance of scented oils. No wonder all the young women love you!"
      }
    },
