    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "绝色佳人啊！ 要是你不知道， 就跟着羊群的足迹， 到牧人的帐篷边喂养你的山羊羔吧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你這女子中最美麗的啊！ 你若不知道， 可以出去，跟隨羊群的腳蹤， 在牧人的帳棚邊， 牧放你的山羊羔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你這女子中極美麗的， 你若不知道， 只管跟隨羊羣的腳蹤去， 把你的山羊羔牧放在牧人帳棚的旁邊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If you yourself do not know, Most beautiful among women, Go forth on the trail of the flock And pasture your young goats By the tents of the shepherds."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you do not know, most beautiful of women, follow the tracks of the sheep and graze your young goats by the tents of the shepherds."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you don’t know, O most beautiful woman, follow the trail of my flock, and graze your young goats by the shepherds’ tents."
      }
    },
