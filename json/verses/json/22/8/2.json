    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我可以把你带进我母亲的家——她教养了我。 我要给你喝石榴汁酿的醇酒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要引領你， 領你到我母親家裡去；從小她就教導我。 我要給你喝石榴汁釀的香酒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必引導你， 領你進我母親的家； 我可以領受教訓， 也就使你喝石榴汁釀的香酒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I would lead you and bring you Into the house of my mother, who used to instruct me; I would give you spiced wine to drink from the juice of my pomegranates."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I would lead you and bring you to my mother’s house— she who has taught me. I would give you spiced wine to drink, the nectar of my pomegranates."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I would bring you to my childhood home, and there you would teach me. I would give you spiced wine to drink, my sweet pomegranate wine."
      }
    },
