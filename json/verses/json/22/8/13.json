    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你这住在园中的人啊， 同伴们都在聆听你的声音， 求你让我也听见吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你這住在園中的啊！ 我的同伴都在細聽你的聲音； 願你使我也聽見。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你這住在園中的， 同伴都要聽你的聲音， 求你使我也得聽見。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“O you who sit in the gardens, My companions are listening for your voice— Let me hear it!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You who dwell in the gardens with friends in attendance, let me hear your voice!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O my darling, lingering in the gardens, your companions are fortunate to hear your voice. Let me hear it, too!"
      }
