      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是一面墙， 双乳像城楼， 我在他眼中像找到平安的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我是牆，我的兩乳像其上的塔； 我在他的眼中像尋得安樂的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我是牆； 我兩乳像其上的樓。 那時，我在他眼中像得平安的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I was a wall, and my breasts were like towers; Then I became in his eyes as one who finds peace."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am a wall, and my breasts are like towers. Thus I have become in his eyes like one bringing contentment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I was a virgin, like a wall; now my breasts are like towers. When my lover looks at me, he is delighted with what he sees."
      }
