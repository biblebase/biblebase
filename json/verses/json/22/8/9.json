    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她若是墙， 我们要在上面建造银塔； 她若是门， 我们要用香柏木板围护她。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她若是牆， 我們就要在她上面建造銀塔； 她若是門， 我們就要用香柏木板圍護她。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她若是牆， 我們要在其上建造銀塔； 她若是門， 我們要用香柏木板圍護她。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If she is a wall, We will build on her a battlement of silver; But if she is a door, We will barricade her with planks of cedar.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If she is a wall, we will build towers of silver on her. If she is a door, we will enclose her with panels of cedar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If she is a virgin, like a wall, we will protect her with a silver tower. But if she is promiscuous, like a swinging door, we will block her door with a cedar bar."
      }
    },
