    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶路撒冷的少女啊， 我恳求你们， 不要叫醒或惊动爱情， 等它自发吧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶路撒冷的眾女子啊！我懇求你們， 不要激動，不要挑動愛情， 等它自發吧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷的眾女子啊， 我囑咐你們： 不要驚動、不要叫醒我所親愛的， 等他自己情願。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I want you to swear, O daughters of Jerusalem, Do not arouse or awaken my love Until she pleases.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Daughters of Jerusalem, I charge you: Do not arouse or awaken love until it so desires."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Promise me, O women of Jerusalem, not to awaken love until the time is right."
      }
    },
