    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门在巴力·哈们有一个葡萄园， 他把葡萄园租给农夫， 租金是每人一千块银子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門在巴力．哈們有個葡萄園； 他把這葡萄園租給看守的人， 各人要為葡萄園所得的果子付一萬一千四百克銀子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門在巴力‧哈們有一葡萄園； 他將這葡萄園交給看守的人， 為其中的果子必交一千舍客勒銀子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Solomon had a vineyard at Baal-hamon; He entrusted the vineyard to caretakers. Each one was to bring a thousand shekels of silver for its fruit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Solomon had a vineyard in Baal Hamon; he let out his vineyard to tenants. Each was to bring for its fruit a thousand shekels of silver."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon has a vineyard at Baal-hamon, which he leases out to tenant farmers. Each of them pays a thousand pieces of silver for harvesting its fruit."
      }
    },
