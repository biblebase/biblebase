    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他的双臂如同镶着水苍玉的金杖， 身体如同镶嵌蓝宝石的象牙。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的雙手像金管， 鑲嵌著水蒼玉； 他的軀體如象牙塊， 周圍包著藍寶石。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的兩手好像金管， 鑲嵌水蒼玉； 他的身體如同雕刻的象牙， 周圍鑲嵌藍寶石。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His hands are rods of gold Set with beryl; His abdomen is carved ivory Inlaid with sapphires."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His arms are rods of gold set with topaz. His body is like polished ivory decorated with lapis lazuli."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "His arms are like rounded bars of gold, set with beryl. His body is like bright ivory, glowing with lapis lazuli."
      }
    },
