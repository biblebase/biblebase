    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他的眼睛清澈明亮， 如同溪旁用奶洗净的白鸽， 又像镶嵌合适的宝石。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的眼好像溪水旁的鴿子， 在奶中洗淨，塑得合式。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的眼如溪水旁的鴿子眼， 用奶洗淨，安得合式。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His eyes are like doves Beside streams of water, Bathed in milk, And reposed in their setting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His eyes are like doves by the water streams, washed in milk, mounted like jewels."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "His eyes sparkle like doves beside springs of water; they are set like jewels washed in milk."
      }
    },
