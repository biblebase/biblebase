    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶路撒冷的少女啊， 我恳求你们， 倘若遇见我的良人， 请告诉他我思爱成病。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶路撒冷的眾女子啊！我囑咐你們： 你們若遇見我的良人， 你們要告訴他甚麼呢？ 你們要告訴他，我患了相思病。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷的眾女子啊，我囑咐你們： 若遇見我的良人， 要告訴他，我因思愛成病。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I adjure you, O daughters of Jerusalem, If you find my beloved, As to what you will tell him: For I am lovesick.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Daughters of Jerusalem, I charge you— if you find my beloved, what will you tell him? Tell him I am faint with love."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Make this promise, O women of Jerusalem— If you find my lover, tell him I am weak with love."
      }
    },
