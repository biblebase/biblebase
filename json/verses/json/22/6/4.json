    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的爱人啊， 你像得撒一样秀丽， 像耶路撒冷一样佳美， 像旌旗飘扬的军队一样威严。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的佳偶啊！你美麗如得撒， 秀麗如耶路撒冷， 威武如展開旗幟的軍隊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的佳偶啊，你美麗如得撒， 秀美如耶路撒冷， 威武如展開旌旗的軍隊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You are as beautiful as Tirzah, my darling, As lovely as Jerusalem, As awesome as an army with banners."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are as beautiful as Tirzah, my darling, as lovely as Jerusalem, as majestic as troops with banners."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You are beautiful, my darling, like the lovely city of Tirzah. Yes, as beautiful as Jerusalem, as majestic as an army with billowing banners."
      }
