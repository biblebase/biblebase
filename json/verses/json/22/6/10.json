    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“这位灿烂似晨光， 皎洁如明月，耀眼如太阳， 亮丽如布满天际之星辰的是谁呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“那出現如破曉的晨光， 美麗如月亮， 燦爛如日頭， 威武有如展開旗幟的軍隊的是誰呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那向外觀看、如晨光發現、 美麗如月亮、皎潔如日頭、 威武如展開旌旗軍隊的是誰呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Who is this that grows like the dawn, As beautiful as the full moon, As pure as the sun, As awesome as an army with banners?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who is this that appears like the dawn, fair as the moon, bright as the sun, majestic as the stars in procession?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Who is this, arising like the dawn, as fair as the moon, as bright as the sun, as majestic as an army with billowing banners?”"
      }
    },
