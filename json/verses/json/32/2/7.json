    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的生命渐渐消逝的时候， 我就想起了耶和华。 我的祷告进入你的圣殿， 达到你面前。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當我心靈疲弱時， 我就思念耶和華； 我的禱告直達你面前， 進入你的聖殿。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我心在我裏面發昏的時候， 我就想念耶和華。 我的禱告進入你的聖殿， 達到你的面前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While I was fainting away, I remembered the Lord, And my prayer came to You, Into Your holy temple."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When my life was ebbing away, I remembered you, Lord, and my prayer rose to you, to your holy temple."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As my life was slipping away, I remembered the Lord. And my earnest prayer went out to you in your holy Temple."
      }
    },
