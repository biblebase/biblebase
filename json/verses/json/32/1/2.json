    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你起来前往尼尼微大城，大声警告那里的居民，因为他们的罪恶滔天，已经达到我面前。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“起來！到尼尼微大城去，警告其中的居民，因為他們的惡行已經達到我面前。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你起來往尼尼微大城去，向其中的居民呼喊，因為他們的惡達到我面前。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Arise, go to Nineveh the great city and cry against it, for their wickedness has come up before Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Go to the great city of Nineveh and preach against it, because its wickedness has come up before me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Get up and go to the great city of Nineveh. Announce my judgment against it because I have seen how wicked its people are.”"
      }
    },
