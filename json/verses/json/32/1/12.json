    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约拿对他们说：“我知道这场风暴是因我而起的，你们把我抬起来抛进海里，风浪自然会平静。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他回答說：“你們把我抬起來，投在海裡，海浪就會平靜，因為我知道，這場大風暴臨到你們，是為了我的緣故。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對他們說：「你們將我擡起來，拋在海中，海就平靜了；我知道你們遭這大風是因我的緣故。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to them, “Pick me up and throw me into the sea. Then the sea will become calm for you, for I know that on account of me this great storm has come upon you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Pick me up and throw me into the sea,” he replied, “and it will become calm. I know that it is my fault that this great storm has come upon you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Throw me into the sea,” Jonah said, “and it will become calm again. I know that this terrible storm is all my fault.”"
      }
    },
