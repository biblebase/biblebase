    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那是降烈怒的日子， 是困苦艰难的日子， 是摧残毁坏的日子， 是黑暗幽冥的日子， 是阴霾密布的日子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那日是忿怒的日子， 是困苦艱難的日子， 是荒廢淒涼的日子， 是黑暗幽冥的日子， 是密雲漆黑的日子，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那日是忿怒的日子， 是急難困苦的日子， 是荒廢淒涼的日子， 是黑暗幽冥、密雲烏黑的日子，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A day of wrath is that day, A day of trouble and distress, A day of destruction and desolation, A day of darkness and gloom, A day of clouds and thick darkness,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "That day will be a day of wrath— a day of distress and anguish, a day of trouble and ruin, a day of darkness and gloom, a day of clouds and blackness—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It will be a day when the Lord’s anger is poured out— a day of terrible distress and anguish, a day of ruin and desolation, a day of darkness and gloom, a day of clouds and blackness,"
      }
    },
