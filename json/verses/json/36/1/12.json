    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时，我必提着灯巡查耶路撒冷， 惩罚那些安于罪中的人。 他们心想，‘耶和华不赐福也不降祸。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時， 我必提燈搜查耶路撒冷， 懲罰那些在罪中陶然自得的人。 這些人心裡說： ‘耶和華必不賜福，也不降禍。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，我必用燈巡查耶路撒冷； 我必懲罰那些如酒在渣滓上澄清的； 他們心裏說： 耶和華必不降福，也不降禍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It will come about at that time That I will search Jerusalem with lamps, And I will punish the men Who are stagnant in spirit, Who say in their hearts, ‘The Lord will not do good or evil!’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At that time I will search Jerusalem with lamps and punish those who are complacent, who are like wine left on its dregs, who think, ‘The Lord will do nothing, either good or bad.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I will search with lanterns in Jerusalem’s darkest corners to punish those who sit complacent in their sins. They think the Lord will do nothing to them, either good or bad."
      }
    },
