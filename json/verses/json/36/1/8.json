    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“在我献祭的日子， 我必惩罚首领和王子， 以及所有穿外族服装的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“到了我耶和華獻祭的日子， 我必懲罰首領和王子， 懲罰所有穿外族衣服的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了我－耶和華獻祭的日子， 必懲罰首領和王子， 並一切穿外邦衣服的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Then it will come about on the day of the Lord’s sacrifice That I will punish the princes, the king’s sons And all who clothe themselves with foreign garments."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“On the day of the Lord’s sacrifice I will punish the officials and the king’s sons and all those clad in foreign clothes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“On that day of judgment,” says the Lord, “I will punish the leaders and princes of Judah and all those following pagan customs."
      }
    },
