    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她的首领是咆哮的狮子， 她的审判官是夜间的豺狼， 不留一点猎物到天明。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "城中的領袖都是咆哮的獅子， 城中的審判官是夜間的豺狼， 甚麼都不留到早晨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她中間的首領是咆哮的獅子； 她的審判官是晚上的豺狼， 一點食物也不留到早晨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Her princes within her are roaring lions, Her judges are wolves at evening; They leave nothing for the morning."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Her officials within her are roaring lions; her rulers are evening wolves, who leave nothing for the morning."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Its leaders are like roaring lions hunting for their victims. Its judges are like ravenous wolves at evening time, who by dawn have left no trace of their prey."
      }
    },
