        "text": "她不听命令，不受管教； 不倚靠耶和华， 不亲近她的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她不聽話， 也不受管教； 她不倚靠耶和華， 也不親近她的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她不聽從命令，不領受訓誨， 不倚靠耶和華， 不親近她的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She heeded no voice, She accepted no instruction. She did not trust in the Lord, She did not draw near to her God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She obeys no one, she accepts no correction. She does not trust in the Lord, she does not draw near to her God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No one can tell it anything; it refuses all correction. It does not trust in the Lord or draw near to its God."
      }
