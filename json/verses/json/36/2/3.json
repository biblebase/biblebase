    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世上所有遵守耶和华典章的谦卑人啊， 要寻求耶和华，追求公义，为人谦卑。 也许在耶和华发怒的日子， 你们可以得到庇护。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "地上所有謙卑的人， 就是遵守耶和華典章的人哪！ 你們要尋求耶和華， 你們要尋求公義，尋求謙卑。 在耶和華忿怒的日子， 你們或許得以隱藏起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "世上遵守耶和華典章的謙卑人哪， 你們都當尋求耶和華！ 當尋求公義謙卑， 或者在耶和華發怒的日子可以隱藏起來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Seek the Lord, All you humble of the earth Who have carried out His ordinances; Seek righteousness, seek humility. Perhaps you will be hidden In the day of the Lord’s anger."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Seek the Lord, all you humble of the land, you who do what he commands. Seek righteousness, seek humility; perhaps you will be sheltered on the day of the Lord’s anger."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Seek the Lord, all who are humble, and follow his commands. Seek to do what is right and to live humbly. Perhaps even yet the Lord will protect you— protect you from his anger on that day of destruction."
      }
    },
