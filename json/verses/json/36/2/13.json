    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂必伸手攻击北方，毁灭亚述， 使尼尼微一片荒凉， 如干旱的旷野。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必伸手攻擊北方， 毀滅亞述， 使尼尼微變成荒場， 乾旱像曠野。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華必伸手攻擊北方，毀滅亞述， 使尼尼微荒涼，又乾旱如曠野。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He will stretch out His hand against the north And destroy Assyria, And He will make Nineveh a desolation, Parched like the wilderness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will stretch out his hand against the north and destroy Assyria, leaving Nineveh utterly desolate and dry as the desert."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the Lord will strike the lands of the north with his fist, destroying the land of Assyria. He will make its great capital, Nineveh, a desolate wasteland, parched like a desert."
      }
    },
