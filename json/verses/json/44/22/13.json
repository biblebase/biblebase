    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他来探望我，站在我身边说，‘扫罗弟兄，重见光明吧！’就在那一刻，我抬头看见了他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他來見我，站在我旁邊，對我說：‘掃羅弟兄，你現在可以看見了。’我立刻往上一看，看見了他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他來見我，站在旁邊，對我說：『兄弟掃羅，你可以看見。』我當時往上一看，就看見了他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "came to me, and standing near said to me, ‘Brother Saul, receive your sight!’ And at that very time I looked up at him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He stood beside me and said, ‘Brother Saul, receive your sight!’ And at that very moment I was able to see him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He came and stood beside me and said, ‘Brother Saul, regain your sight.’ And that very moment I could see him!"
      }
