    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的同伴虽然也看见那道强光，却听不懂那位说话者的声音。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "跟我在一起的人，只看見那光，卻聽不清楚那位對我說話的聲音。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "與我同行的人看見了那光，卻沒有聽明那位對我說話的聲音。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And those who were with me saw the light, to be sure, but did not understand the voice of the One who was speaking to me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My companions saw the light, but they did not understand the voice of him who was speaking to me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people with me saw the light but didn’t understand the voice speaking to me."
      }
    },
