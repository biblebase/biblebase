    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把保罗绑起来正要鞭打，保罗问旁边的百夫长：“未经定罪就拷打罗马公民合法吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "士兵正用皮帶綁他的時候，保羅對站在旁邊的百夫長說：“你們鞭打一個還沒有定罪的羅馬公民，是合法的嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "剛用皮條捆上，保羅對旁邊站着的百夫長說：「人是羅馬人，又沒有定罪，你們就鞭打他，有這個例嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when they stretched him out with thongs, Paul said to the centurion who was standing by, “Is it lawful for you to scourge a man who is a Roman and uncondemned?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As they stretched him out to flog him, Paul said to the centurion standing there, “Is it legal for you to flog a Roman citizen who hasn’t even been found guilty?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they tied Paul down to lash him, Paul said to the officer standing there, “Is it legal for you to whip a Roman citizen who hasn’t even been tried?”"
      }
    },
