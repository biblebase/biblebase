    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "保罗刚走上台阶，众人便凶猛地拥挤过来，士兵们只好把他举起来抬着走。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "保羅到了臺階下面的時候，士兵把他抬起來，因為群眾猛擠，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了臺階上，眾人擠得兇猛，兵丁只得將保羅擡起來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he got to the stairs, he was carried by the soldiers because of the violence of the mob;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Paul reached the steps, the violence of the mob was so great he had to be carried by the soldiers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As Paul reached the stairs, the mob grew so violent the soldiers had to lift him to their shoulders to protect him."
      }
    },
