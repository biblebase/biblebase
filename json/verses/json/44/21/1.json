    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们和众人道别之后，乘船直接驶往哥士，第二天到达罗底，从那里前往帕大喇，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們離別了他們以後，船就直航到了哥士，第二天到羅底，從那裡開往帕大拉；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們離別了眾人，就開船一直行到哥士。第二天到了羅底，從那裏到帕大喇，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When we had parted from them and had set sail, we ran a straight course to Cos and the next day to Rhodes and from there to Patara;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After we had torn ourselves away from them, we put out to sea and sailed straight to Kos. The next day we went to Rhodes and from there to Patara."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After saying farewell to the Ephesian elders, we sailed straight to the island of Cos. The next day we reached Rhodes and then went to Patara."
      }
    },
