    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不久前煽动叛乱、带着四千暴徒逃到旷野去的那个埃及人是你吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "難道你不就是不久以前作亂的、帶領四千個殺手到曠野去的那個埃及人嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你莫非是從前作亂、帶領四千兇徒往曠野去的那埃及人嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then you are not the Egyptian who some time ago stirred up a revolt and led the four thousand men of the Assassins out into the wilderness?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Aren’t you the Egyptian who started a revolt and led four thousand terrorists out into the wilderness some time ago?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Aren’t you the Egyptian who led a rebellion some time ago and took 4,000 members of the Assassins out into the desert?”"
      }
