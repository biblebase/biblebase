    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恳求他将保罗押回耶路撒冷，他们想在途中埋伏杀害保罗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求他恩准對付保羅，把保羅解來耶路撒冷，他們好埋伏在路上殺死他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又央告他，求他的情，將保羅提到耶路撒冷來，他們要在路上埋伏殺害他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "requesting a concession against Paul, that he might have him brought to Jerusalem (at the same time, setting an ambush to kill him on the way)."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They requested Festus, as a favor to them, to have Paul transferred to Jerusalem, for they were preparing an ambush to kill him along the way."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They asked Festus as a favor to transfer Paul to Jerusalem (planning to ambush and kill him on the way)."
      }
    },
