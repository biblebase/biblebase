      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们都站起来当面指控他，但所告的并非我料想的罪行，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "原告都站在那裡，他們所控訴他的，並不是我所意料的惡事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "告他的人站着告他；所告的，並沒有我所逆料的那等惡事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the accusers stood up, they began bringing charges against him not of such crimes as I was expecting,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When his accusers got up to speak, they did not charge him with any of the crimes I had expected."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the accusations made against him weren’t any of the crimes I expected."
      }
