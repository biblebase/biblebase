    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要取得授权书到大马士革的各会堂搜捕信耶稣的人，无论男女，都要把他们押回耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要求他發公函給大馬士革各會堂，如果發現奉行這道的人，准他連男帶女緝拿到耶路撒冷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求文書給大馬士革的各會堂，若是找着信奉這道的人，無論男女，都准他捆綁帶到耶路撒冷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and asked for letters from him to the synagogues at Damascus, so that if he found any belonging to the Way, both men and women, he might bring them bound to Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and asked him for letters to the synagogues in Damascus, so that if he found any there who belonged to the Way, whether men or women, he might take them as prisoners to Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He requested letters addressed to the synagogues in Damascus, asking for their cooperation in the arrest of any followers of the Way he found there. He wanted to bring them—both men and women—back to Jerusalem in chains."
      }
    },
