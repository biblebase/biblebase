      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The disciples, as each one was able, decided to provide help for the brothers and sisters living in Judea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the believers in Antioch decided to send relief to the brothers and sisters in Judea, everyone giving as much as they could."
      }
    },
