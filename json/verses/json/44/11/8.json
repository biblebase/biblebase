    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我说，‘主啊，这可不行！我从未吃过任何污秽不洁之物！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我說：‘主啊，千萬不可，因為俗物或不潔的東西，從來沒有進過我的口。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我說：『主啊，這是不可的！凡俗而不潔淨的物從來沒有入過我的口。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I said, ‘By no means, Lord, for nothing unholy or unclean has ever entered my mouth.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I replied, ‘Surely not, Lord! Nothing impure or unclean has ever entered my mouth.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“‘No, Lord,’ I replied. ‘I have never eaten anything that our Jewish laws have declared impure or unclean.’"
      }
    },
