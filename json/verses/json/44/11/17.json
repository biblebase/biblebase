    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "既然上帝给他们恩赐，就如我们信主耶稣基督时给我们一样，我是谁，怎能拦阻上帝？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神既然把同樣的恩賜給他們，像給我們這些信了主耶穌基督的人一樣，我是誰，我能夠阻止　神嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神既然給他們恩賜，像在我們信主耶穌基督的時候給了我們一樣；我是誰，能攔阻神呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore if God gave to them the same gift as He gave to us also after believing in the Lord Jesus Christ, who was I that I could stand in God’s way?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So if God gave them the same gift he gave us who believed in the Lord Jesus Christ, who was I to think that I could stand in God’s way?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And since God gave these Gentiles the same gift he gave us when we believed in the Lord Jesus Christ, who was I to stand in God’s way?”"
      }
    },
