    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他到达后，看见上帝所赐的恩典，就万分高兴，劝勉他们要全心地忠于主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他到了那裡，看見　神所施的恩，就很歡喜，勸勉眾人堅心靠主。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他到了那裏，看見神所賜的恩就歡喜，勸勉眾人，立定心志，恆久靠主。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then when he arrived and witnessed the grace of God, he rejoiced and began to encourage them all with resolute heart to remain true to the Lord;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he arrived and saw what the grace of God had done, he was glad and encouraged them all to remain true to the Lord with all their hearts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When he arrived and saw this evidence of God’s blessing, he was filled with joy, and he encouraged the believers to stay true to the Lord."
      }
    },
