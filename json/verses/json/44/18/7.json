    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "保罗就离开那里，来到一位敬畏上帝、名叫提多·犹士都的人家里，他家就在会堂隔壁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他離開那裡，到了提多．猶士都的家去。這人是敬拜　神的，他的家靠近會堂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是離開那裏，到了一個人的家中；這人名叫提多‧猶士都，是敬拜神的，他的家靠近會堂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he left there and went to the house of a man named Titius Justus, a worshiper of God, whose house was next to the synagogue."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Paul left the synagogue and went next door to the house of Titius Justus, a worshiper of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he left and went to the home of Titius Justus, a Gentile who worshiped God and lived next door to the synagogue."
      }
    },
