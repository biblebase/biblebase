    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们还派人作伪证说：“司提凡常常讲侮辱圣地和律法的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "並且造了假的證供說：“這人不斷抨擊聖地和律法。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "設下假見證，說：「這個人說話，不住地糟踐聖所和律法。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They put forward false witnesses who said, “This man incessantly speaks against this holy place and the Law;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They produced false witnesses, who testified, “This fellow never stops speaking against this holy place and against the law."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The lying witnesses said, “This man is always speaking against the holy Temple and against the law of Moses."
      }
    },
