    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又煽动百姓、长老和律法教师抓住司提凡，把他押到公会。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又煽動民眾、長老、經學家，這些人就來捉拿他，把他帶到公議會，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們又聳動了百姓、長老，並文士，就忽然來捉拿他，把他帶到公會去，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they stirred up the people, the elders and the scribes, and they came up to him and dragged him away and brought him before the Council."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So they stirred up the people and the elders and the teachers of the law. They seized Stephen and brought him before the Sanhedrin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This roused the people, the elders, and the teachers of religious law. So they arrested Stephen and brought him before the high council."
      }
    },
