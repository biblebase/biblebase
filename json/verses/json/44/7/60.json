    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“主啊，不要追究他们的罪！”说完后，就断气了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後跪下來大聲喊著說：“主啊，不要把這罪歸給他們！”說了這話，就睡了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又跪下大聲喊着說：「主啊，不要將這罪歸於他們！」說了這話，就睡了。掃羅也喜悅他被害。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then falling on his knees, he cried out with a loud voice, “Lord, do not hold this sin against them!” Having said this, he fell asleep."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he fell on his knees and cried out, “Lord, do not hold this sin against them.” When he had said this, he fell asleep."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He fell to his knees, shouting, “Lord, don’t charge them with this sin!” And with that, he died."
      }
    },
