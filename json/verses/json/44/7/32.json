    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘我是你祖先的上帝，是亚伯拉罕、以撒、雅各的上帝。’摩西战战兢兢，不敢观看。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘我是你祖宗的　神，就是亞伯拉罕、以撒、雅各的　神。’摩西戰戰兢兢，不敢觀看。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "『我是你列祖的神，就是亞伯拉罕的神，以撒的神，雅各的神。』摩西戰戰兢兢，不敢觀看。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘I am the God of your fathers, the God of Abraham and Isaac and Jacob.’ Moses shook with fear and would not venture to look."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘I am the God of your fathers, the God of Abraham, Isaac and Jacob.’ Moses trembled with fear and did not dare to look."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘I am the God of your ancestors—the God of Abraham, Isaac, and Jacob.’ Moses shook with terror and did not dare to look."
      }
    },
