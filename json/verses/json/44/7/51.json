    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们这些顽固不化、耳不听心不悔的人，经常抗拒圣灵，所作所为和你们的祖先如出一辙！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們頸項剛硬、心和耳都未受割禮的人哪！你們時常抗拒聖靈，你們的祖先怎樣，你們也怎樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們這硬着頸項、心與耳未受割禮的人，常時抗拒聖靈！你們的祖宗怎樣，你們也怎樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You men who are stiff-necked and uncircumcised in heart and ears are always resisting the Holy Spirit; you are doing just as your fathers did."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You stiff-necked people! Your hearts and ears are still uncircumcised. You are just like your ancestors: You always resist the Holy Spirit!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You stubborn people! You are heathen at heart and deaf to the truth. Must you forever resist the Holy Spirit? That’s what your ancestors did, and so do you!"
      }
    },
