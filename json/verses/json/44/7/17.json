    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“上帝对亚伯拉罕所应许的日子快到的时侯，以色列人在埃及的人口已大大增加。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“　神給亞伯拉罕的應許快要實現的時候，以色列人在埃及人口繁盛增多；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「及至神應許亞伯拉罕的日期將到，以色列民在埃及興盛眾多，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But as the time of the promise was approaching which God had assured to Abraham, the people increased and multiplied in Egypt,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“As the time drew near for God to fulfill his promise to Abraham, the number of our people in Egypt had greatly increased."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“As the time drew near when God would fulfill his promise to Abraham, the number of our people in Egypt greatly increased."
      }
    },
