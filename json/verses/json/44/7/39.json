    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“可是我们的祖先不听从他，甚至弃绝他，心里对埃及恋恋不舍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們的祖先不肯聽從他，反而把他推開，他們的心已經轉向了埃及，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們的祖宗不肯聽從，反棄絕他，心裏歸向埃及，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Our fathers were unwilling to be obedient to him, but repudiated him and in their hearts turned back to Egypt,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But our ancestors refused to obey him. Instead, they rejected him and in their hearts turned back to Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But our ancestors refused to listen to Moses. They rejected him and wanted to return to Egypt."
      }
    },
