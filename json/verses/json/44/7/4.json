    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伯拉罕就离开迦勒底人的地方，在哈兰住了下来。他父亲过世以后，上帝又领他到你们现在居住的这个地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他就離開迦勒底人的地方，住在哈蘭。他父親死後，　神又叫他從那裡遷到你們現在所住的地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他就離開迦勒底人之地，住在哈蘭。他父親死了以後，神使他從那裏搬到你們現在所住之地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he left the land of the Chaldeans and settled in Haran. From there, after his father died, God had him move to this country in which you are now living."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“So he left the land of the Chaldeans and settled in Harran. After the death of his father, God sent him to this land where you are now living."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Abraham left the land of the Chaldeans and lived in Haran until his father died. Then God brought him here to the land where you now live."
      }
    },
