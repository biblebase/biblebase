    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他用毒计苦害我们的同胞，虐待我们的祖先，强令他们把婴孩抛弃，不让婴孩存活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他就謀害我們的同胞，虐待我們的祖先，逼他們拋棄自己的嬰孩，不容嬰孩存活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他用詭計待我們的宗族，苦害我們的祖宗，叫他們丟棄嬰孩，使嬰孩不能存活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It was he who took shrewd advantage of our race and mistreated our fathers so that they would expose their infants and they would not survive."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He dealt treacherously with our people and oppressed our ancestors by forcing them to throw out their newborn babies so that they would die."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This king exploited our people and oppressed them, forcing parents to abandon their newborn babies so they would die."
      }
    },
