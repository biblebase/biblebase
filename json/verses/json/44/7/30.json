    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“过了四十年，在西奈山的旷野，上帝的天使在荆棘火焰中向摩西显现。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“過了四十年，在西奈山的曠野，有一位使者，在荊棘中的火燄裡，向摩西顯現。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「過了四十年，在西奈山的曠野，有一位天使從荊棘火焰中向摩西顯現。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“After forty years had passed, an angel appeared to him in the wilderness of Mount Sinai, in the flame of a burning thorn bush."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“After forty years had passed, an angel appeared to Moses in the flames of a burning bush in the desert near Mount Sinai."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Forty years later, in the desert near Mount Sinai, an angel appeared to Moses in the flame of a burning bush."
      }
