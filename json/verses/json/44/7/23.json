    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他四十岁那年决定去探望自己的同胞以色列人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“到了四十歲，他心中起了一個念頭，要去看望自己的同胞以色列人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「他將到四十歲，心中起意去看望他的弟兄以色列人；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when he was approaching the age of forty, it entered his mind to visit his brethren, the sons of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When Moses was forty years old, he decided to visit his own people, the Israelites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“One day when Moses was forty years old, he decided to visit his relatives, the people of Israel."
      }
    },
