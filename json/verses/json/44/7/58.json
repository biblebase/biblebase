    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把司提凡拉到城外，用石头打他。指控他的人把他们的衣服交给一个名叫扫罗的青年看管。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "把他推出城外，用石頭打他。那些證人把自己的衣服，放在一個名叫掃羅的青年人腳前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "把他推到城外，用石頭打他。作見證的人把衣裳放在一個少年人名叫掃羅的腳前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they had driven him out of the city, they began stoning him; and the witnesses laid aside their robes at the feet of a young man named Saul."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "dragged him out of the city and began to stone him. Meanwhile, the witnesses laid their coats at the feet of a young man named Saul."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and dragged him out of the city and began to stone him. His accusers took off their coats and laid them at the feet of a young man named Saul."
      }
    },
