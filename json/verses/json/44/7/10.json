    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "救他脱离一切危难，又赐他过人的智慧，使他在埃及法老面前得到恩宠，官拜宰相治理全国，并管理王室。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "救他脫離一切苦難，使他在埃及王法老面前，有智慧、得恩寵。法老立他為首相，管理埃及和法老的全家。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "救他脫離一切苦難，又使他在埃及王法老面前得恩典，有智慧。法老就派他作埃及國的宰相兼管全家。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and rescued him from all his afflictions, and granted him favor and wisdom in the sight of Pharaoh, king of Egypt, and he made him governor over Egypt and all his household."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and rescued him from all his troubles. He gave Joseph wisdom and enabled him to gain the goodwill of Pharaoh king of Egypt. So Pharaoh made him ruler over Egypt and all his palace."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and rescued him from all his troubles. And God gave him favor before Pharaoh, king of Egypt. God also gave Joseph unusual wisdom, so that Pharaoh appointed him governor over all of Egypt and put him in charge of the palace."
      }
    },
