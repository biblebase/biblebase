      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這人領百姓出來，在埃及，在紅海，在曠野，四十年間行了奇事神蹟。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This man led them out, performing wonders and signs in the land of Egypt and in the Red Sea and in the wilderness for forty years."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He led them out of Egypt and performed wonders and signs in Egypt, at the Red Sea and for forty years in the wilderness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And by means of many wonders and miraculous signs, he led them out of Egypt, through the Red Sea, and through the wilderness for forty years."
      }
