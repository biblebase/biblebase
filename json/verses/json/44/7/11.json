    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来，埃及和迦南全境遭遇饥荒，灾情严重，我们的先祖们断了粮。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "後來埃及和迦南全地遭遇饑荒、大災難，我們的祖先找不到糧食。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "後來埃及和迦南全地遭遇饑荒，大受艱難，我們的祖宗就絕了糧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Now a famine came over all Egypt and Canaan, and great affliction with it, and our fathers could find no food."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Then a famine struck all Egypt and Canaan, bringing great suffering, and our ancestors could not find food."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But a famine came upon Egypt and Canaan. There was great misery, and our ancestors ran out of food."
      }
    },
