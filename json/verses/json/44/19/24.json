    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一个名叫底米丢的银匠以制造亚底米女神银龛为业，使从事这门生意的工匠获利丰厚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一個銀匠，名叫低米丟，是製造亞底米女神銀龕的。他讓技工們作了不少的生意。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一個銀匠，名叫底米丟，是製造亞底米神銀龕的，他使這樣手藝人生意發達。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For a man named Demetrius, a silversmith, who made silver shrines of Artemis, was bringing no little business to the craftsmen;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A silversmith named Demetrius, who made silver shrines of Artemis, brought in a lot of business for the craftsmen there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It began with Demetrius, a silversmith who had a large business manufacturing silver shrines of the Greek goddess Artemis. He kept many craftsmen busy."
      }
    },
