    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "被邪灵附身的人随即扑向他们，攻击他们，制服了他们，使他们赤着身子带着伤从屋里逃走了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "邪靈所附的那人就撲到他們身上，制伏了兩人，勝過了他們，使他們赤著身帶著傷，從那房子逃了出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡鬼所附的人就跳在他們身上，勝了其中二人，制伏他們，叫他們赤着身子受了傷，從那房子裏逃出去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the man, in whom was the evil spirit, leaped on them and subdued all of them and overpowered them, so that they fled out of that house naked and wounded."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the man who had the evil spirit jumped on them and overpowered them all. He gave them such a beating that they ran out of the house naked and bleeding."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the man with the evil spirit leaped on them, overpowered them, and attacked them with such violence that they fled from the house, naked and battered."
      }
    },
