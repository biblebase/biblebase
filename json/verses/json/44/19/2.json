    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "保罗问他们：“你们信的时候有没有领受圣灵？” 他们说：“没有！我们从来没有听过圣灵的事。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "問他們說：“你們信的時候，受了聖靈沒有？”他們說：“沒有，連聖靈賜下來這件事，我們也沒有聽過。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "問他們說：「你們信的時候受了聖靈沒有？」他們回答說：「沒有，也未曾聽見有聖靈賜下來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to them, “Did you receive the Holy Spirit when you believed?” And they said to him, “No, we have not even heard whether there is a Holy Spirit.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and asked them, “Did you receive the Holy Spirit when you believed?” They answered, “No, we have not even heard that there is a Holy Spirit.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Did you receive the Holy Spirit when you believed?” he asked them. “No,” they replied, “we haven’t even heard that there is a Holy Spirit.”"
      }
    },
