    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们带来的这些人既没有偷庙里的东西，也没有亵渎我们的女神。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們帶來的這些人，既沒有行劫廟宇，也沒有褻瀆我們的女神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們把這些人帶來，他們並沒有偷竊廟中之物，也沒有謗讟我們的女神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For you have brought these men here who are neither robbers of temples nor blasphemers of our goddess."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have brought these men here, though they have neither robbed temples nor blasphemed our goddess."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have brought these men here, but they have stolen nothing from the temple and have not spoken against our goddess."
      }
    },
