    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "保罗把手按在他们身上时，圣灵便降在他们身上，他们就说方言，讲上帝的信息。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "保羅為他們按手，聖靈就降在他們身上，他們就用各種的語言講話，並且說預言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "保羅按手在他們頭上，聖靈便降在他們身上，他們就說方言，又說預言。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And when Paul had laid his hands upon them, the Holy Spirit came on them, and they began speaking with tongues and prophesying."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Paul placed his hands on them, the Holy Spirit came on them, and they spoke in tongues and prophesied."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then when Paul laid his hands on them, the Holy Spirit came on them, and they spoke in other tongues and prophesied."
      }
    },
