    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "邪灵回答说：“耶稣，我认识；保罗，我也知道。但你们是谁？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "邪靈回答他們：“耶穌我認識，保羅我也知道；你們是誰？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡鬼回答他們說：「耶穌我認識，保羅我也知道。你們卻是誰呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the evil spirit answered and said to them, “I recognize Jesus, and I know about Paul, but who are you?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One day the evil spirit answered them, “Jesus I know, and Paul I know about, but who are you?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But one time when they tried it, the evil spirit replied, “I know Jesus, and I know Paul, but who are you?”"
      }
    },
