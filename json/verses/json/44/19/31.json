    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他的好友——亚细亚的几位官员也派人劝他别冒险进入戏院。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "還有幾位亞西亞的首長，是保羅的朋友，派人來勸他，不要冒險到劇場裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "還有亞細亞幾位首領，是保羅的朋友，打發人來勸他，不要冒險到戲園裏去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Also some of the Asiarchs who were friends of his sent to him and repeatedly urged him not to venture into the theater."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even some of the officials of the province, friends of Paul, sent him a message begging him not to venture into the theater."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some of the officials of the province, friends of Paul, also sent a message to him, begging him not to risk his life by entering the amphitheater."
      }
    },
