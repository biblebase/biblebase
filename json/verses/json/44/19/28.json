    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人听后，怒气填胸，高声喊叫：“以弗所人的亚底米女神真伟大！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們聽了，怒氣沖沖喊著說：“以弗所人的女神，偉大的亞底米啊！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人聽見，就怒氣填胸，喊着說：「大哉，以弗所人的亞底米啊！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they heard this and were filled with rage, they began crying out, saying, “Great is Artemis of the Ephesians!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they heard this, they were furious and began shouting: “Great is Artemis of the Ephesians!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At this their anger boiled, and they began shouting, “Great is Artemis of the Ephesians!”"
      }
    },
