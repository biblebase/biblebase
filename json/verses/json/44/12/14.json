    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她听出是彼得的声音，喜出望外，竟然没有开门就跑进去告诉大家：“彼得在门外！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她認出是彼得的聲音，歡喜到顧不得開門，就跑進去報告，說彼得站在門外。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "聽得是彼得的聲音，就歡喜的顧不得開門，跑進去告訴眾人說：「彼得站在門外。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When she recognized Peter’s voice, because of her joy she did not open the gate, but ran in and announced that Peter was standing in front of the gate."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When she recognized Peter’s voice, she was so overjoyed she ran back without opening it and exclaimed, “Peter is at the door!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When she recognized Peter’s voice, she was so overjoyed that, instead of opening the door, she ran back inside and told everyone, “Peter is standing at the door!”"
      }
    },
