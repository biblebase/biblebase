    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把他关在监里，由四班卫兵，每班四人轮流看守，想等逾越节过后当众惩办他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "捉到了，就把他關在監裡，交給四班士兵看守，每班四個人，打算過了逾越節，把他提出來交給民眾。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希律拿了彼得，收在監裏，交付四班兵丁看守，每班四個人，意思要在逾越節後把他提出來，當着百姓辦他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he had seized him, he put him in prison, delivering him to four squads of soldiers to guard him, intending after the Passover to bring him out before the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After arresting him, he put him in prison, handing him over to be guarded by four squads of four soldiers each. Herod intended to bring him out for public trial after the Passover."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he imprisoned him, placing him under the guard of four squads of four soldiers each. Herod intended to bring Peter out for public trial after the Passover."
      }
    },
