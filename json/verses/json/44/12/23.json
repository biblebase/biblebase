    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希律没有把荣耀归给上帝，主的天使立刻惩罚他，他当场被虫子咬死了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他不歸榮耀給　神，所以主的使者立刻擊打他，他被蟲咬，就斷了氣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希律不歸榮耀給神，所以主的使者立刻罰他，他被蟲所咬，氣就絕了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And immediately an angel of the Lord struck him because he did not give God the glory, and he was eaten by worms and died."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Immediately, because Herod did not give praise to God, an angel of the Lord struck him down, and he was eaten by worms and died."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Instantly, an angel of the Lord struck Herod with a sickness, because he accepted the people’s worship instead of giving the glory to God. So he was consumed with worms and died."
      }
    },
