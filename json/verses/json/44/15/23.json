    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们带去的书信这样说：“安提阿、叙利亚和基利迦的外族弟兄姊妹，你们的弟兄——众使徒和长老向你们问安！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是寫信給他們帶去，信上說：“使徒和作長老的弟兄們，向安提阿、敘利亞、基利家的外族眾弟兄問安。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是寫信交付他們，內中說：「使徒和作長老的弟兄們問安提阿、敘利亞、基利家外邦眾弟兄的安。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they sent this letter by them, “The apostles and the brethren who are elders, to the brethren in Antioch and Syria and Cilicia who are from the Gentiles, greetings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "With them they sent the following letter: The apostles and elders, your brothers, To the Gentile believers in Antioch, Syria and Cilicia: Greetings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is the letter they took with them: “This letter is from the apostles and elders, your brothers in Jerusalem. It is written to the Gentile believers in Antioch, Syria, and Cilicia. Greetings!"
      }
    },
