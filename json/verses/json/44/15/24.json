    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“听说有几个人从我们这里去了你们那里，教导你们必须接受割礼并遵守摩西的律法。他们的言论使你们大感困惑。其实我们从来没有授权他们这样做。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們聽說有人從我們這裡出去，說了一些話攪擾你們，使你們心裡不安，其實我們並沒有吩咐他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們聽說，有幾個人從我們這裏出去，用言語攪擾你們，惑亂你們的心。其實我們並沒有吩咐他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Since we have heard that some of our number to whom we gave no instruction have disturbed you with their words, unsettling your souls,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We have heard that some went out from us without our authorization and disturbed you, troubling your minds by what they said."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“We understand that some men from here have troubled you and upset you with their teaching, but we did not send them!"
      }
    },
