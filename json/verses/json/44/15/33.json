    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "住了些日子后，安提阿的弟兄姊妹以平安的祝福为他们送行，让他们回耶路撒冷复命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "住了一段時間，弟兄們就給他們送行，祝一路平安；他們就回到差派他們的人那裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住了些日子，弟兄們打發他們平平安安地回到差遣他們的人那裏去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After they had spent time there, they were sent away from the brethren in peace to those who had sent them out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After spending some time there, they were sent off by the believers with the blessing of peace to return to those who had sent them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They stayed for a while, and then the believers sent them back to the church in Jerusalem with a blessing of peace."
      }
