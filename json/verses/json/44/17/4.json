    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不少人被说服，成为保罗和西拉的同道，其中有许多是敬畏上帝的希腊人和有地位的妇女。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們中間有人給說服了，就附從了保羅和西拉；還有一大群虔誠的希臘人，和不少顯要的婦女。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們中間有些人聽了勸，就附從保羅和西拉，並有許多虔敬的希臘人，尊貴的婦女也不少。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And some of them were persuaded and joined Paul and Silas, along with a large number of the God-fearing Greeks and a number of the leading women."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Some of the Jews were persuaded and joined Paul and Silas, as did a large number of God-fearing Greeks and quite a few prominent women."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some of the Jews who listened were persuaded and joined Paul and Silas, along with many God-fearing Greek men and quite a few prominent women."
      }
    },
