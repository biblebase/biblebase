    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“这位创造宇宙万物的上帝是天地的主宰，并不住在人手建造的庙宇里，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "創造宇宙和其中萬有的　神，既然是天地的主，就不住在人手所造的殿宇，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "創造宇宙和其中萬物的神，既是天地的主，就不住人手所造的殿，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The God who made the world and all things in it, since He is Lord of heaven and earth, does not dwell in temples made with hands;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The God who made the world and everything in it is the Lord of heaven and earth and does not live in temples built by human hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“He is the God who made the world and everything in it. Since he is Lord of heaven and earth, he doesn’t live in man-made temples,"
      }
    },
