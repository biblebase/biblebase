    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "也不需要人的侍奉，因为祂一无所缺。祂将生命、气息和万物赐给世人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也不受人手的服事，好像他缺少甚麼；他自己反而把生命、氣息和一切，賜給萬人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也不用人手服事，好像缺少甚麼；自己倒將生命、氣息、萬物，賜給萬人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "nor is He served by human hands, as though He needed anything, since He Himself gives to all people life and breath and all things;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he is not served by human hands, as if he needed anything. Rather, he himself gives everyone life and breath and everything else."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and human hands can’t serve his needs—for he has no needs. He himself gives life and breath to everything, and he satisfies every need."
      }
    },
