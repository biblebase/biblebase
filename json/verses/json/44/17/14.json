    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "庇哩亚的弟兄姊妹立刻送保罗到海边，西拉和提摩太仍然留在庇哩亚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弟兄們立刻把保羅送到海邊去；西拉和提摩太仍然留在比里亞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當時弟兄們便打發保羅往海邊去，西拉和提摩太仍住在庇哩亞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then immediately the brethren sent Paul out to go as far as the sea; and Silas and Timothy remained there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The believers immediately sent Paul to the coast, but Silas and Timothy stayed at Berea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The believers acted at once, sending Paul on to the coast, while Silas and Timothy remained behind."
      }
    },
