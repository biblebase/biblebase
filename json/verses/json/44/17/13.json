    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是，帖撒罗尼迦的犹太人听到保罗在庇哩亚宣扬上帝的道，就赶来怂恿、煽动众人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但帖撒羅尼迦的猶太人，知道保羅又在比里亞宣講　神的道，就到那裡去，煽動挑撥群眾。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但帖撒羅尼迦的猶太人知道保羅又在庇哩亞傳神的道，也就往那裏去，聳動攪擾眾人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when the Jews of Thessalonica found out that the word of God had been proclaimed by Paul in Berea also, they came there as well, agitating and stirring up the crowds."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But when the Jews in Thessalonica learned that Paul was preaching the word of God at Berea, some of them went there too, agitating the crowds and stirring them up."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when some Jews in Thessalonica learned that Paul was preaching the word of God in Berea, they went there and stirred up trouble."
      }
    },
