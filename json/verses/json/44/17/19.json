    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们带保罗到一个称为亚略·巴古的论坛，问他：“我们可以知道你所教导的这个新道理吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們拉著保羅，把他帶到亞略．巴古那裡，說：“你所講的這個新道理，我們可以知道嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們就把他帶到亞略‧巴古，說：「你所講的這新道，我們也可以知道嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they took him and brought him to the Areopagus, saying, “May we know what this new teaching is which you are proclaiming?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they took him and brought him to a meeting of the Areopagus, where they said to him, “May we know what this new teaching is that you are presenting?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they took him to the high council of the city. “Come and tell us about this new teaching,” they said."
      }
    },
