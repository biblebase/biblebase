    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世上的君王一同行动，官长聚集起来， 要抵挡主和祂所膏立的王。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "地上的君王都起來， 首領聚在一起， 敵對主和他的受膏者。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "世上的君王一齊起來， 臣宰也聚集， 要敵擋主， 並主的受膏者。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The kings of the earth took their stand, And the rulers were gathered together Against the Lord and against His Christ.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The kings of the earth rise up and the rulers band together against the Lord and against his anointed one.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The kings of the earth prepared for battle; the rulers gathered together against the Lord and against his Messiah.’"
      }
    },
