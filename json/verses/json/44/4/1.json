    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼得和约翰向百姓讲道的时候，祭司、圣殿护卫长和撒都该人赶来了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "使徒們正對群眾講話的時候，祭司、聖殿的守衛長，和撒都該人來到他們那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使徒對百姓說話的時候，祭司們和守殿官，並撒都該人忽然來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As they were speaking to the people, the priests and the captain of the temple guard and the Sadducees came up to them,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priests and the captain of the temple guard and the Sadducees came up to Peter and John while they were speaking to the people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "While Peter and John were speaking to the people, they were confronted by the priests, the captain of the Temple guard, and some of the Sadducees."
      }
