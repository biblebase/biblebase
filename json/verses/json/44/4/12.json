    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "除祂以外，别无救主，因为普天之下没有赐下别的名我们可以靠着得救！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "除了他以外，別無拯救，因為在天下人間，沒有賜下別的名，我們可以靠著得救。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "除他以外，別無拯救；因為在天下人間，沒有賜下別的名，我們可以靠着得救。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And there is salvation in no one else; for there is no other name under heaven that has been given among men by which we must be saved.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Salvation is found in no one else, for there is no other name under heaven given to mankind by which we must be saved.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There is salvation in no one else! God has given no other name under heaven by which we must be saved.”"
      }
    },
