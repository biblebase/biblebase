    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“果然如此，希律和本丢·彼拉多、外族人和以色列人都聚集在这城里，要对抗你膏立的圣子耶稣，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希律和本丟．彼拉多，外族人和以色列民，真的在這城裡聚集，反對你所膏立的聖僕耶穌，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希律和本丟‧彼拉多，外邦人和以色列民，果然在這城裏聚集，要攻打你所膏的聖僕耶穌，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For truly in this city there were gathered together against Your holy servant Jesus, whom You anointed, both Herod and Pontius Pilate, along with the Gentiles and the peoples of Israel,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Indeed Herod and Pontius Pilate met together with the Gentiles and the people of Israel in this city to conspire against your holy servant Jesus, whom you anointed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“In fact, this has happened here in this very city! For Herod Antipas, Pontius Pilate the governor, the Gentiles, and the people of Israel were all united against Jesus, your holy servant, whom you anointed."
      }
    },
