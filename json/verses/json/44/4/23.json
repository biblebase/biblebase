    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼得和约翰被释放之后，就去找其他信徒，把祭司长和长老的话都告诉大家。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼得和約翰被釋放了之後，回到自己的人那裡去，把祭司長和長老說的一切，都告訴他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "二人既被釋放，就到會友那裏去，把祭司長和長老所說的話都告訴他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they had been released, they went to their own companions and reported all that the chief priests and the elders had said to them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On their release, Peter and John went back to their own people and reported all that the chief priests and the elders had said to them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As soon as they were freed, Peter and John returned to the other believers and told them what the leading priests and elders had said."
      }
