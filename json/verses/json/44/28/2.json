    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "岛上的居民对我们非常友善。因为下雨，天气又冷，他们就生火接待我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當地的人對我們非常友善；因為下過一場雨，天氣又冷，他們就生了火來招待我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "土人看待我們，有非常的情分；因為當時下雨，天氣又冷，就生火接待我們眾人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The natives showed us extraordinary kindness; for because of the rain that had set in and because of the cold, they kindled a fire and received us all."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The islanders showed us unusual kindness. They built a fire and welcomed us all because it was raining and cold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people of the island were very kind to us. It was cold and rainy, so they built a fire on the shore to welcome us."
      }
    },
