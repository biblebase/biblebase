    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "保罗站起来向大家挥手示意，说：“各位以色列同胞和各位敬畏上帝的外族朋友们，请听我说。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "保羅就站起來，作了一個手勢，說： “以色列人和敬畏　神的人，請聽！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "保羅就站起來，舉手，說：「以色列人和一切敬畏神的人，請聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Paul stood up, and motioning with his hand said, “Men of Israel, and you who fear God, listen:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Standing up, Paul motioned with his hand and said: “Fellow Israelites and you Gentiles who worship God, listen to me!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Paul stood, lifted his hand to quiet them, and started speaking. “Men of Israel,” he said, “and you God-fearing Gentiles, listen to me."
      }
