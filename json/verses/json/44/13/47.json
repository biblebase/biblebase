    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为主这样吩咐我们， “‘我已使你成为外族人的光， 好把救恩带到地极。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為主曾這樣吩咐我們說： ‘我已立你作外族人的光， 使你把救恩帶到地極去。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為主曾這樣吩咐我們說： 我已經立你作外邦人的光， 叫你施行救恩，直到地極。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For so the Lord has commanded us, ‘I have placed You as a light for the Gentiles, That You may bring salvation to the end of the earth.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this is what the Lord has commanded us: “ ‘I have made you a light for the Gentiles, that you may bring salvation to the ends of the earth.’”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Lord gave us this command when he said, ‘I have made you a light to the Gentiles, to bring salvation to the farthest corners of the earth.’”"
      }
    },
