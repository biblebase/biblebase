    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "外族人听后，非常欢喜，颂赞主的道。凡被选定得永生的人都信了主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "外族人聽見了就歡喜，讚美主的道，凡指定得永生的都信了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "外邦人聽見這話，就歡喜了，讚美神的道；凡預定得永生的人都信了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the Gentiles heard this, they began rejoicing and glorifying the word of the Lord; and as many as had been appointed to eternal life believed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the Gentiles heard this, they were glad and honored the word of the Lord; and all who were appointed for eternal life believed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the Gentiles heard this, they were very glad and thanked the Lord for his message; and all who were chosen for eternal life became believers."
      }
