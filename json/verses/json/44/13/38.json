    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，弟兄们，你们应该知道，赦罪的信息是借着耶稣传给你们的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以弟兄們，你們當知道，赦罪之道是由這位耶穌傳給你們的。在你們靠摩西律法不能稱義的一切事上，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，弟兄們，你們當曉得：赦罪的道是由這人傳給你們的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore let it be known to you, brethren, that through Him forgiveness of sins is proclaimed to you,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Therefore, my friends, I want you to know that through Jesus the forgiveness of sins is proclaimed to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Brothers, listen! We are here to proclaim that through this man Jesus there is forgiveness for your sins."
      }
