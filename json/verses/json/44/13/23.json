    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝照自己的应许，从大卫的后裔中为以色列人立了一位救主，就是耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神照著應許，已經從這人的後裔中，給以色列帶來了一位救主，就是耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從這人的後裔中，神已經照着所應許的，為以色列人立了一位救主，就是耶穌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the descendants of this man, according to promise, God has brought to Israel a Savior, Jesus,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“From this man’s descendants God has brought to Israel the Savior Jesus, as he promised."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And it is one of King David’s descendants, Jesus, who is God’s promised Savior of Israel!"
      }
    },
