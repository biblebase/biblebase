    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又名扫罗的保罗被圣灵充满，盯着他说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅，也就是保羅，卻被聖靈充滿，定睛看著他，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅又名保羅，被聖靈充滿，定睛看他，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Saul, who was also known as Paul, filled with the Holy Spirit, fixed his gaze on him,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Saul, who was also called Paul, filled with the Holy Spirit, looked straight at Elymas and said,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Saul, also known as Paul, was filled with the Holy Spirit, and he looked the sorcerer in the eye."
      }
    },
