    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "之后有许多天，那些从加利利跟随祂上耶路撒冷的人都看见过祂，他们如今在百姓中都是祂的见证人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有許多日子，他向那些跟他一同從加利利上耶路撒冷的人顯現，現在這些人在民眾面前作了他的見證人，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那從加利利同他上耶路撒冷的人多日看見他，這些人如今在民間是他的見證。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and for many days He appeared to those who came up with Him from Galilee to Jerusalem, the very ones who are now His witnesses to the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and for many days he was seen by those who had traveled with him from Galilee to Jerusalem. They are now his witnesses to our people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And over a period of many days he appeared to those who had gone with him from Galilee to Jerusalem. They are now his witnesses to the people of Israel."
      }
    },
