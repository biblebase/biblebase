    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘藐视真理的人啊，看吧！ 你们要在惊惧中灭亡， 因为我要在你们的时代行一件事， 即使有人告诉你们， 你们也不会信。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘傲慢的人哪！ 你們要看、要驚奇、要滅亡， 因為在你們的日子，我要作一件事， 就算有人告訴你們，你們總是不信。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主說： 你們這輕慢的人要觀看，要驚奇，要滅亡； 因為在你們的時候，我行一件事， 雖有人告訴你們， 你們總是不信。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Behold, you scoffers, and marvel, and perish; For I am accomplishing a work in your days, A work which you will never believe, though someone should describe it to you.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Look, you scoffers, wonder and perish, for I am going to do something in your days that you would never believe, even if someone told you.’”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘Look, you mockers, be amazed and die! For I am doing something in your own day, something you wouldn’t believe even if someone told you about it.’”"
      }
    },
