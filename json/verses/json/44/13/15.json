    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "读完律法书和先知书后，会堂主管派人告诉他们：“弟兄们，如果你们有什么劝勉众人的话，请讲。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "宣讀了律法和先知書以後，會堂的理事們派人到他們那裡來，說：“弟兄們，如果有甚麼勸勉眾人的話，請說吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "讀完了律法和先知的書，管會堂的叫人過去，對他們說：「二位兄台，若有甚麼勸勉眾人的話，請說。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After the reading of the Law and the Prophets the synagogue officials sent to them, saying, “Brethren, if you have any word of exhortation for the people, say it.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After the reading from the Law and the Prophets, the leaders of the synagogue sent word to them, saying, “Brothers, if you have a word of exhortation for the people, please speak.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After the usual readings from the books of Moses and the prophets, those in charge of the service sent them this message: “Brothers, if you have any word of encouragement for the people, come and give it.”"
      }
    },
