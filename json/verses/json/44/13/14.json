    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们由别加继续前行，来到彼西底区的安提阿。在安息日那天，他们进了会堂坐下来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們從別加往前走，到了彼西底的安提阿，在安息日進了會堂，就坐下來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們離了別加往前行，來到彼西底的安提阿，在安息日進會堂坐下。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But going on from Perga, they arrived at Pisidian Antioch, and on the Sabbath day they went into the synagogue and sat down."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From Perga they went on to Pisidian Antioch. On the Sabbath they entered the synagogue and sat down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Paul and Barnabas traveled inland to Antioch of Pisidia. On the Sabbath they went to the synagogue for the services."
      }
    },
