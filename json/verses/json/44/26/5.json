    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们认识我很久了，如果他们肯作证的话，他们可以证明我从小就属于犹太教中最严格的法利赛派。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果他們肯作證的話，他們是早就知道，我是按著我們祖宗最嚴格的教派，過著法利賽人的生活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們若肯作見證就曉得，我從起初是按着我們教中最嚴緊的教門作了法利賽人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "since they have known about me for a long time, if they are willing to testify, that I lived as a Pharisee according to the strictest sect of our religion."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They have known me for a long time and can testify, if they are willing, that I conformed to the strictest sect of our religion, living as a Pharisee."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If they would admit it, they know that I have been a member of the Pharisees, the strictest sect of our religion."
      }
    },
