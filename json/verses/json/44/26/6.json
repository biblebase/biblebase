    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在我站在这里受审，是因为我盼望上帝给我们祖先的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在我站著受審，是為了對　神向我們祖先的應許存著盼望。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在我站在這裏受審，是因為指望神向我們祖宗所應許的；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And now I am standing trial for the hope of the promise made by God to our fathers;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And now it is because of my hope in what God has promised our ancestors that I am on trial today."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now I am on trial because of my hope in the fulfillment of God’s promise made to our ancestors."
      }
    },
