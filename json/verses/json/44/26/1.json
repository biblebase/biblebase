      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚基帕对保罗说：“准你为自己辩护。”于是保罗伸手示意，然后为自己辩护说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞基帕對保羅說：“准你為自己申辯。”於是保羅伸手辯護說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞基帕對保羅說：「准你為自己辯明。」於是保羅伸手分訴，說：「"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Agrippa said to Paul, “You are permitted to speak for yourself.” Then Paul stretched out his hand and proceeded to make his defense:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Agrippa said to Paul, “You have permission to speak for yourself.” So Paul motioned with his hand and began his defense:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Agrippa said to Paul, “You may speak in your defense.” So Paul, gesturing with his hand, started his defense:"
      }
