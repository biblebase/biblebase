    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王啊！大约中午时分，我在路上看见一道比太阳还亮的光从天上照在我和同行的人周围。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王啊，就在中午的時候，我在路上看見一道光，比太陽更明亮，從天上四面照著我和同行的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王啊，我在路上，晌午的時候，看見從天發光，比日頭還亮，四面照着我並與我同行的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "at midday, O King, I saw on the way a light from heaven, brighter than the sun, shining all around me and those who were journeying with me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "About noon, King Agrippa, as I was on the road, I saw a light from heaven, brighter than the sun, blazing around me and my companions."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "About noon, Your Majesty, as I was on the road, a light from heaven brighter than the sun shone down on me and my companions."
      }
    },
