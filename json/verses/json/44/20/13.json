    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们照保罗的吩咐先乘船到亚朔预备在那里接他，他打算由陆路步行到那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們先上船，開往亞朔，照著保羅的安排，要在那邊接他，因為他自己要走陸路去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們先上船，開往亞朔去，意思要在那裏接保羅；因為他是這樣安排的，他自己打算要步行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But we, going ahead to the ship, set sail for Assos, intending from there to take Paul on board; for so he had arranged it, intending himself to go by land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We went on ahead to the ship and sailed for Assos, where we were going to take Paul aboard. He had made this arrangement because he was going there on foot."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Paul went by land to Assos, where he had arranged for us to join him, while we traveled by ship."
      }
    },
