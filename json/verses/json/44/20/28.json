    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要为自己，也要为教会的群羊警醒，因为圣灵已经任命你们做群羊的监督，牧养上帝的教会。教会是主用自己的血赎回来的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "聖靈既然立你們為全群的監督，牧養　神用自己的血所贖來的教會，你們就應當為自己謹慎，也為全群謹慎。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "聖靈立你們作全羣的監督，你們就當為自己謹慎，也為全羣謹慎，牧養神的教會，就是他用自己血所買來的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be on guard for yourselves and for all the flock, among which the Holy Spirit has made you overseers, to shepherd the church of God which He purchased with His own blood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Keep watch over yourselves and all the flock of which the Holy Spirit has made you overseers. Be shepherds of the church of God, which he bought with his own blood."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“So guard yourselves and God’s people. Feed and shepherd God’s flock—his church, purchased with his own blood—over which the Holy Spirit has appointed you as leaders."
      }
    },
