    "sermons": [
      {
        "id": "home_sermons_sermon-2009-12-06",
        "title": "基督徒生活守則(六)",
        "date": "2009-12-06",
        "preacher": "王文堂牧師",
        "verses": "使徒行傳 20:35",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2009/Sermon-2009-12-06.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2009/Sermon-2009-12-06.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我凡事以身作则，教导你们勤奋工作，帮助软弱的人，并牢记主耶稣的话，‘施比受更有福。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我凡事以身作則，你們必須照樣辛勞，扶助軟弱的人，並且記念主耶穌的話：‘施比受更為有福。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我凡事給你們作榜樣，叫你們知道應當這樣勞苦，扶助軟弱的人，又當記念主耶穌的話，說：『施比受更為有福。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In everything I showed you that by working hard in this manner you must help the weak and remember the words of the Lord Jesus, that He Himself said, ‘It is more blessed to give than to receive.’  ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In everything I did, I showed you that by this kind of hard work we must help the weak, remembering the words the Lord Jesus himself said: ‘It is more blessed to give than to receive.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I have been a constant example of how you can help those in need by working hard. You should remember the words of the Lord Jesus: ‘It is more blessed to give than to receive.’”"
      }
    },
