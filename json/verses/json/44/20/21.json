    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我劝犹太人和希腊人都要悔改，归向上帝，信靠主耶稣基督。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我對猶太人和希臘人都作過見證，要他們悔改歸向　神，信靠我們的主耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又對猶太人和希臘人證明當向神悔改，信靠我主耶穌基督。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "solemnly testifying to both Jews and Greeks of repentance toward God and faith in our Lord Jesus Christ."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have declared to both Jews and Greeks that they must turn to God in repentance and have faith in our Lord Jesus."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have had one message for Jews and Greeks alike—the necessity of repenting from sin and turning to God, and of having faith in our Lord Jesus."
      }
    },
