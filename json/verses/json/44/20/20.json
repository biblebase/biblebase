    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们也知道，凡是对你们有益的事，我没有避而不宣，我总是在公开场合和各人家中教导你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們也知道，我從來沒有留下一件有益的事，不在眾人面前或在各人家裡告訴你們，教導你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們也知道，凡與你們有益的，我沒有一樣避諱不說的，或在眾人面前，或在各人家裏，我都教導你們；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "how I did not shrink from declaring to you anything that was profitable, and teaching you publicly and from house to house,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You know that I have not hesitated to preach anything that would be helpful to you but have taught you publicly and from house to house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I never shrank back from telling you what you needed to hear, either publicly or in your homes."
      }
    },
