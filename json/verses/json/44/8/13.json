    "sermons": [
      {
        "id": "home_sermons_sermon-2017-05-21",
        "title": "腓利",
        "date": "2017-05-21",
        "preacher": "王文堂牧師",
        "verses": "使徒行傳 8:1-40",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2017/Sermon-2017-05-21.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2017/Sermon-2017-05-21.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "西门本人也信了基督，接受了洗礼，并常常追随腓利左右。腓利所行的神迹奇事令他非常惊奇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "連西門自己也信了，他受洗之後，常和腓利在一起，看見所發生的神蹟和大能的事，就覺得很驚奇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "西門自己也信了；既受了洗，就常與腓利在一處，看見他所行的神蹟和大異能，就甚驚奇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even Simon himself believed; and after being baptized, he continued on with Philip, and as he observed signs and great miracles taking place, he was constantly amazed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Simon himself believed and was baptized. And he followed Philip everywhere, astonished by the great signs and miracles he saw."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Simon himself believed and was baptized. He began following Philip wherever he went, and he was amazed by the signs and great miracles Philip performed."
      }
    },
