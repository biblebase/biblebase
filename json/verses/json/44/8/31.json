    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他说：“没有人为我解释，我怎能明白呢？”他就请腓利上车和他一起坐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“沒有人指導我，怎能明白呢？”於是請腓利上車，同他坐在一起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：「沒有人指教我，怎能明白呢？」於是請腓利上車，與他同坐。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he said, “Well, how could I, unless someone guides me?” And he invited Philip to come up and sit with him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“How can I,” he said, “unless someone explains it to me?” So he invited Philip to come up and sit with him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The man replied, “How can I, unless someone instructs me?” And he urged Philip to come up into the carriage and sit with him."
      }
