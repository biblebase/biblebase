    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们往前走的时候，经过一处有水的地方，太监说：“你看，这里有水，我可以在这里接受洗礼吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們一路走，到了有水的地方，太監說：“你看，這裡有水，有甚麼可以阻止我受洗呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "二人正往前走，到了有水的地方，太監說：「看哪，這裏有水，我受洗有甚麼妨礙呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As they went along the road they came to some water; and the eunuch *said, “Look! Water! What prevents me from being baptized?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As they traveled along the road, they came to some water and the eunuch said, “Look, here is water. What can stand in the way of my being baptized?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As they rode along, they came to some water, and the eunuch said, “Look! There’s some water! Why can’t I be baptized?”"
      }
