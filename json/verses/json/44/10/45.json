    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "跟彼得一起来的那些严守割礼的信徒，看见圣灵的恩赐也浇灌到外族人身上，都很惊奇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些受了割禮、跟彼得一同來的信徒，因為聖靈的恩賜也澆灌在外族人的身上，都很驚訝；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那些奉割禮、和彼得同來的信徒，見聖靈的恩賜也澆在外邦人身上，就都希奇；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the circumcised believers who came with Peter were amazed, because the gift of the Holy Spirit had been poured out on the Gentiles also."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The circumcised believers who had come with Peter were astonished that the gift of the Holy Spirit had been poured out even on Gentiles."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Jewish believers who came with Peter were amazed that the gift of the Holy Spirit had been poured out on the Gentiles, too."
      }
    },
