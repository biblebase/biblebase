    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一天，大约下午三点，他在异象中清楚地看见一位上帝的天使进来呼唤他：“哥尼流！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一天，大約下午三點鐘，他在異象中，清清楚楚看見　神的一位天使來到他那裡，對他說：“哥尼流！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一天，約在申初，他在異象中明明看見神的一個使者進去，到他那裏，說：「哥尼流。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "About the ninth hour of the day he clearly saw in a vision an angel of God who had just come in and said to him, “Cornelius!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One day at about three in the afternoon he had a vision. He distinctly saw an angel of God, who came to him and said, “Cornelius!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One afternoon about three o’clock, he had a vision in which he saw an angel of God coming toward him. “Cornelius!” the angel said."
      }
    },
