    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第二天，风浪依然猛烈，他们开始把货物抛进海里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "風浪把我們顛簸得很厲害，第二天他們就把貨物拋在海裡，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們被風浪逼得甚急，第二天眾人就把貨物拋在海裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The next day as we were being violently storm-tossed, they began to jettison the cargo;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We took such a violent battering from the storm that the next day they began to throw the cargo overboard."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The next day, as gale-force winds continued to batter the ship, the crew began throwing the cargo overboard."
      }
