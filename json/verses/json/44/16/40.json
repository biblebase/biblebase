    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "二人离开监狱，来到吕底亚家中，见了弟兄姊妹，劝勉一番之后，便离开了那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "兩人出了監，就到呂底亞的家裡去，見了弟兄們，勸勉他們一番，就離開了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "二人出了監，往呂底亞家裏去， 見了弟兄們，勸慰他們一番，就走了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They went out of the prison and entered the house of Lydia, and when they saw the brethren, they encouraged them and departed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After Paul and Silas came out of the prison, they went to Lydia’s house, where they met with the brothers and sisters and encouraged them. Then they left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Paul and Silas left the prison, they returned to the home of Lydia. There they met with the believers and encouraged them once more. Then they left town."
      }
    },
