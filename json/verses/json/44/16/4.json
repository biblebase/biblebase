    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们走遍各城，把耶路撒冷的使徒和长老所定下的规条教导当地的门徒遵守。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們經過各城，把耶路撒冷的使徒和長老所定的規條，交給門徒遵守。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們經過各城，把耶路撒冷使徒和長老所定的條規交給門徒遵守。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now while they were passing through the cities, they were delivering the decrees which had been decided upon by the apostles and elders who were in Jerusalem, for them to observe."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As they traveled from town to town, they delivered the decisions reached by the apostles and elders in Jerusalem for the people to obey."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they went from town to town, instructing the believers to follow the decisions made by the apostles and elders in Jerusalem."
      }
    },
