    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她的主人们眼见财路断绝了，就把保罗和西拉揪住，拖到广场去见官长。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她的主人們看見發財的希望完了，就揪住保羅和西拉，拉到市中心去見官長，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使女的主人們見得利的指望沒有了，便揪住保羅和西拉，拉他們到市上去見首領；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when her masters saw that their hope of profit was gone, they seized Paul and Silas and dragged them into the market place before the authorities,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When her owners realized that their hope of making money was gone, they seized Paul and Silas and dragged them into the marketplace to face the authorities."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Her masters’ hopes of wealth were now shattered, so they grabbed Paul and Silas and dragged them before the authorities at the marketplace."
      }
    },
