    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我不敢耽误大人太久，只求大人容我们简单叙述。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在我不想多煩擾你，只求你寬容一下，聽我們講幾句話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟恐多說，你嫌煩絮，只求你寬容聽我們說幾句話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But, that I may not weary you any further, I beg you to grant us, by your kindness, a brief hearing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But in order not to weary you further, I would request that you be kind enough to hear us briefly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I don’t want to bore you, so please give me your attention for only a moment."
      }
    },
