    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们对大人的恩德感激不尽。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們隨時隨地感激不盡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「腓力斯大人，我們因你得以大享太平，並且這一國的弊病，因着你的先見得以更正了；我們隨時隨地滿心感謝不盡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "we acknowledge this in every way and everywhere, most excellent Felix, with all thankfulness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Everywhere and in every way, most excellent Felix, we acknowledge this with profound gratitude."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For all of this, Your Excellency, we are very grateful to you."
      }
    },
