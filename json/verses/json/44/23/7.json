    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这句话立刻引起法利赛人和撒都该人之间的争论，公会分成了两派。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說了這話，法利賽人和撒都該人就起了爭論，會眾也分裂了，成為兩派。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說了這話，法利賽人和撒都該人就爭論起來，會眾分為兩黨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As he said this, there occurred a dissension between the Pharisees and Sadducees, and the assembly was divided."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he said this, a dispute broke out between the Pharisees and the Sadducees, and the assembly was divided."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This divided the council—the Pharisees against the Sadducees—"
      }
