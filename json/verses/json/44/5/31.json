    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝把祂提升到自己的右边，立祂为君王和救主，好赐给以色列人悔改的机会，使他们的罪得到赦免。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神把他高舉在自己的右邊，作元首作救主，把悔改的心賜給了以色列人，使他們罪得赦免。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神且用右手將他高舉，叫他作君王，作救主，將悔改的心和赦罪的恩賜給以色列人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He is the one whom God exalted to His right hand as a Prince and a Savior, to grant repentance to Israel, and forgiveness of sins."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God exalted him to his own right hand as Prince and Savior that he might bring Israel to repentance and forgive their sins."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then God put him in the place of honor at his right hand as Prince and Savior. He did this so the people of Israel would repent of their sins and be forgiven."
      }
    },
