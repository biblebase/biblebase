    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "还有大群的人从耶路撒冷附近的城镇带着病人和被污鬼搅扰的人赶来，他们都得了医治。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶路撒冷周圍城市的人，也帶著病人和受污靈纏擾的，蜂擁而來，結果病人全都醫好了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "還有許多人帶着病人和被污鬼纏磨的，從耶路撒冷四圍的城邑來，全都得了醫治。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Also the people from the cities in the vicinity of Jerusalem were coming together, bringing people who were sick or afflicted with unclean spirits, and they were all being healed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Crowds gathered also from the towns around Jerusalem, bringing their sick and those tormented by impure spirits, and all of them were healed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Crowds came from the villages around Jerusalem, bringing their sick and those possessed by evil spirits, and they were all healed."
      }
    },
