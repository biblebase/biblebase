    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚拿尼亚听见这话，当场倒地而死。听见这件事的人都非常惧怕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞拿尼亞一聽見這話，就仆倒斷了氣。所有聽見的人都十分害怕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞拿尼亞聽見這話，就仆倒，斷了氣；聽見的人都甚懼怕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And as he heard these words, Ananias fell down and breathed his last; and great fear came over all who heard of it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Ananias heard this, he fell down and died. And great fear seized all who heard what had happened."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As soon as Ananias heard these words, he fell to the floor and died. Everyone who heard about it was terrified."
      }
    },
