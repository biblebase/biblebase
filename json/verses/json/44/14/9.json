    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他也听保罗讲道。保罗定睛看他，见这个人有信心，可以得医治，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他聽保羅講道；保羅注視他，見他有信心，可以治好，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他聽保羅講道，保羅定睛看他，見他有信心，可得痊癒，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This man was listening to Paul as he spoke, who, when he had fixed his gaze on him and had seen that he had faith to be made well,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He listened to Paul as he was speaking. Paul looked directly at him, saw that he had faith to be healed"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and listening as Paul preached. Looking straight at him, Paul realized he had faith to be healed."
      }
    },
