    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当门徒围过来看他的时候，他站了起来，走回城里。第二天，保罗和巴拿巴前往特庇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "門徒正圍著他的時候，他竟然站起來，走進城裡去了。第二天，他跟巴拿巴一同到特庇去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門徒正圍着他，他就起來，走進城去。 第二天，同巴拿巴往特庇去，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But while the disciples stood around him, he got up and entered the city. The next day he went away with Barnabas to Derbe."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But after the disciples had gathered around him, he got up and went back into the city. The next day he and Barnabas left for Derbe."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But as the believers gathered around him, he got up and went back into the town. The next day he left with Barnabas for Derbe."
      }
    },
