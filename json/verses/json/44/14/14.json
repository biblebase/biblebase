    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴拿巴和保罗见此情形，就撕裂衣服，冲进人群中，大声喊着说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴拿巴和保羅兩個使徒聽見了，就撕裂衣服，跳進群眾中間，喊著說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴拿巴、保羅二使徒聽見，就撕開衣裳，跳進眾人中間，喊着說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when the apostles Barnabas and Paul heard of it, they tore their robes and rushed out into the crowd, crying out"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But when the apostles Barnabas and Paul heard of this, they tore their clothes and rushed out into the crowd, shouting:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when the apostles Barnabas and Paul heard what was happening, they tore their clothing in dismay and ran out among the people, shouting,"
      }
    },
