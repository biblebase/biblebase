    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们对基督有何看法？祂是谁的后裔？”他们答道：“大卫的后裔。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們對基督的看法怎樣？他是誰的子孫呢？”他們回答：“大衛的子孫。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「論到基督，你們的意見如何？他是誰的子孫呢？」他們回答說：「是大衛的子孫。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“What do you think about the Christ, whose son is He?” They *said to Him, “The son of David.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“What do you think about the Messiah? Whose son is he?” “The son of David,” they replied."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What do you think about the Messiah? Whose son is he?” They replied, “He is the son of David.”"
      }
