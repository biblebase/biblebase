    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂说，‘我是亚伯拉罕的上帝，以撒的上帝，雅各的上帝。’上帝不是死人的上帝，而是活人的上帝。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：『我是亞伯拉罕的神，以撒的神，雅各的神。』神不是死人的神，乃是活人的神。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘I am the God of Abraham, and the God of Isaac, and the God of Jacob’? He is not the God of the dead but of the living.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘I am the God of Abraham, the God of Isaac, and the God of Jacob’? He is not the God of the dead but of the living.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘I am the God of Abraham, the God of Isaac, and the God of Jacob.’ So he is the God of the living, not the dead.”"
      }
    },
