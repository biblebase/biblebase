    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无论谁为我的名而撇下房屋、弟兄、姊妹、父母、儿女或田地，都要得到百倍的赏赐，而且承受永生。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡為我的名撇下房屋、兄弟、姊妹、父母、兒女或田地的，他必得著百倍，並且承受永生。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡為我的名撇下房屋，或是弟兄、姊妹、父親、母親、兒女、田地的，必要得着百倍，並且承受永生。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And everyone who has left houses or brothers or sisters or father or mother or children or farms for My name’s sake, will receive many times as much, and will inherit eternal life."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And everyone who has left houses or brothers or sisters or father or mother or wife  or children or fields for my sake will receive a hundred times as much and will inherit eternal life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And everyone who has given up houses or brothers or sisters or father or mother or children or property, for my sake, will receive a hundred times as much in return and will inherit eternal life."
      }
    },
