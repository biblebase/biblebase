    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人带着小孩子来见耶稣，请求耶稣为他们按手祷告，却受到门徒的责备。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，有人帶了小孩子到耶穌面前，求他給他們按手祈禱，門徒就責備那些人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，有人帶着小孩子來見耶穌，要耶穌給他們按手禱告，門徒就責備那些人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then some children were brought to Him so that He might lay His hands on them and pray; and the disciples rebuked them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then people brought little children to Jesus for him to place his hands on them and pray for them. But the disciples rebuked them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One day some parents brought their children to Jesus so he could lay his hands on them and pray for them. But the disciples scolded the parents for bothering him."
      }
    },
