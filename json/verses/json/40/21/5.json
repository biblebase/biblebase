    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“要对锡安城说，‘看啊，你的君王来了！祂谦卑地骑着驴，骑着一头驴驹。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“要對錫安的居民說： ‘看哪，你的王來到你這裡了； 他是溫柔的，他騎著驢，騎的是小驢。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要對錫安的居民說： 看哪，你的王來到你這裏， 是溫柔的，又騎着驢， 就是騎着驢駒子。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Say to the daughter of Zion, ‘Behold your King is coming to you, Gentle, and mounted on a donkey, Even on a colt, the foal of a beast of burden.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Say to Daughter Zion, ‘See, your king comes to you, gentle and riding on a donkey, and on a colt, the foal of a donkey.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Tell the people of Jerusalem, ‘Look, your King is coming to you. He is humble, riding on a donkey— riding on a donkey’s colt.’”"
      }
    },
