    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡跌在这石头上的人，一定粉身碎骨；这石头落在谁身上，就会把谁砸烂。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "誰跌在這石頭上，就必摔碎；這石頭掉在誰的身上，就必把他壓得粉碎。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誰掉在這石頭上，必要跌碎；這石頭掉在誰的身上，就要把誰砸得稀爛。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he who falls on this stone will be broken to pieces; but on whomever it falls, it will scatter him like dust.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who falls on this stone will be broken to pieces; anyone on whom it falls will be crushed.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who stumbles over that stone will be broken to pieces, and it will crush anyone it falls on.”"
      }
