    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我告诉你们，不要跟恶人作对。有人打你的右脸，连左脸也转过来让他打。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可是我告訴你們，不要與惡人對抗，有人打你的右臉，把另一邊也轉過來讓他打；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是我告訴你們，不要與惡人作對。有人打你的右臉，連左臉也轉過來由他打；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I say to you, do not resist an evil person; but whoever slaps you on your right cheek, turn the other to him also."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I tell you, do not resist an evil person. If anyone slaps you on the right cheek, turn to them the other cheek also."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I say, do not resist an evil person! If someone slaps you on the right cheek, offer the other cheek also."
      }
    },
