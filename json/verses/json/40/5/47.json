    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你们只问候自己的弟兄，有什么特别呢？就是外族人也会这样做。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們單問候你們的弟兄，有甚麼特別呢？教外人不也是這樣作嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若單請你弟兄的安，比人有甚麼長處呢？就是外邦人不也是這樣行嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you greet only your brothers, what more are you doing than others? Do not even the Gentiles do the same?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And if you greet only your own people, what are you doing more than others? Do not even pagans do that?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you are kind only to your friends, how are you different from anyone else? Even pagans do that."
      }
    },
