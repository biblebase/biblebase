    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为义受迫害的人有福了， 因为天国是他们的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為義遭受迫害的人有福了， 因為天國是他們的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為義受逼迫的人有福了！ 因為天國是他們的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Blessed are those who have been persecuted for the sake of righteousness, for theirs is the kingdom of heaven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Blessed are those who are persecuted because of righteousness, for theirs is the kingdom of heaven."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God blesses those who are persecuted for doing right, for the Kingdom of Heaven is theirs."
      }
    },
