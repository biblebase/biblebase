    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我告诉你们，不可起誓。不可指着天起誓，因为天是上帝的宝座。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可是我告訴你們，總不可發誓，不可指著天發誓，因為天是　神的寶座；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是我告訴你們，甚麼誓都不可起。不可指着天起誓，因為天是神的座位；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I say to you, make no oath at all, either by heaven, for it is the throne of God,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I tell you, do not swear an oath at all: either by heaven, for it is God’s throne;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I say, do not make any vows! Do not say, ‘By heaven!’ because heaven is God’s throne."
      }
    },
