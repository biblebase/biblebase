    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“所以，你在祭坛前献祭的时候，要是想起有弟兄和你有过节，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以你在祭壇上獻供物的時候，如果在那裡想起你的弟兄對你不滿，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，你在祭壇上獻禮物的時候，若想起弟兄向你懷怨，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore if you are presenting your offering at the altar, and there remember that your brother has something against you,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Therefore, if you are offering your gift at the altar and there remember that your brother or sister has something against you,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“So if you are presenting a sacrifice at the altar in the Temple and you suddenly remember that someone has something against you,"
      }
