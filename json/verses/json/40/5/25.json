    "sermons": [
      {
        "id": "home_sermons_sermon-2009-07-12",
        "title": "遵行神的旨意",
        "date": "2009-07-12",
        "preacher": "王文堂牧師",
        "verses": "馬太福音 5:13-48",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2009/Sermon-2009-07-12.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2009/Sermon-2009-07-12.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“趁着你和告你的人还在去法庭的路上，你要赶紧与对方和解。不然，他会把你交给审判官，审判官会把你交给差役关进监牢。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "趁著你和你的對頭還在路上的時候，要趕快與他和解，免得他抓你去見法官，法官把你交給獄警，關在監裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你同告你的對頭還在路上，就趕緊與他和息，恐怕他把你送給審判官，審判官交付衙役，你就下在監裏了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Make friends quickly with your opponent at law while you are with him on the way, so that your opponent may not hand you over to the judge, and the judge to the officer, and you be thrown into prison."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Settle matters quickly with your adversary who is taking you to court. Do it while you are still together on the way, or your adversary may hand you over to the judge, and the judge may hand you over to the officer, and you may be thrown into prison."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When you are on the way to court with your adversary, settle your differences quickly. Otherwise, your accuser may hand you over to the judge, who will hand you over to an officer, and you will be thrown into prison."
      }
    },
