    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们戏弄完了，就脱去祂的袍子，给祂穿上原来的衣服，把祂押出去钉十字架。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "戲弄完了，就脫下他的外袍，給他穿回自己的衣服，帶去釘十字架。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "戲弄完了，就給他脫了袍子，仍穿上他自己的衣服，帶他出去，要釘十字架。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After they had mocked Him, they took the scarlet robe off Him and put His own garments back on Him, and led Him away to crucify Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After they had mocked him, they took off the robe and put his own clothes on him. Then they led him away to crucify him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they were finally tired of mocking him, they took off the robe and put his own clothes on him again. Then they led him away to be crucified."
      }
