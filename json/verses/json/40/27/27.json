      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "士兵把耶稣押进总督府，叫全营的士兵都聚集在祂周围。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "總督的士兵把耶穌帶到總督府，召集全隊士兵到他面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巡撫的兵就把耶穌帶進衙門，叫全營的兵都聚集在他那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the soldiers of the governor took Jesus into the Praetorium and gathered the whole Roman cohort around Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the governor’s soldiers took Jesus into the Praetorium and gathered the whole company of soldiers around him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some of the governor’s soldiers took Jesus into their headquarters and called out the entire regiment."
      }
    },
