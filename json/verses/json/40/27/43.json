    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂说信靠上帝，如果上帝喜悦祂，就让上帝来救祂吧！因为祂自称是上帝的儿子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他信靠　神；如果　神喜悅他，就讓　神現在救他吧，因為他說自己是　神的兒子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他倚靠神，神若喜悅他，現在可以救他；因為他曾說：『我是神的兒子。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He trusts in God; let God rescue  Him now, if He delights in Him; for He said, ‘I am the Son of God.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He trusts in God. Let God rescue him now if he wants him, for he said, ‘I am the Son of God.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He trusted God, so let God rescue him now if he wants him! For he said, ‘I am the Son of God.’”"
      }
    },
