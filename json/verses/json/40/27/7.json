    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们商议，决定用这些钱买下陶匠的地作为埋葬异乡人的坟场。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們商議之後，就用那些錢買了“陶匠的田”，用來作外國人的墳地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們商議，就用那銀錢買了窰戶的一塊田，為要埋葬外鄉人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they conferred together and with the money bought the Potter’s Field as a burial place for strangers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So they decided to use the money to buy the potter’s field as a burial place for foreigners."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After some discussion they finally decided to buy the potter’s field, and they made it into a cemetery for foreigners."
      }
    },
