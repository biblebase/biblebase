    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼拉多正坐堂断案时，他夫人派人来对他说：“你不要插手这个义人的事！我今天在梦中因为这个人受了许多苦。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼拉多坐在審判臺上的時候，他的夫人派人來說：“你不要干涉這個義人的事，因為昨夜我在夢中因他受了很多的苦。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正坐堂的時候，他的夫人打發人來說：「這義人的事，你一點不可管，因為我今天在夢中為他受了許多的苦。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While he was sitting on the judgment seat, his wife sent him a message, saying, “Have nothing to do with that righteous Man; for last night I suffered greatly in a dream because of Him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While Pilate was sitting on the judge’s seat, his wife sent him this message: “Don’t have anything to do with that innocent man, for I have suffered a great deal today in a dream because of him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Just then, as Pilate was sitting on the judgment seat, his wife sent him this message: “Leave that innocent man alone. I suffered through a terrible nightmare about him last night.”"
      }
    },
