      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "路上，他们遇见一个叫西门的古利奈人，就强迫他背耶稣的十字架。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們出來的時候，遇見一個古利奈人，名叫西門，就強迫他背耶穌的十字架。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們出來的時候，遇見一個古利奈人，名叫西門，就勉強他同去，好背着耶穌的十字架。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As they were coming out, they found a man of Cyrene named Simon, whom they pressed into service to bear His cross."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As they were going out, they met a man from Cyrene, named Simon, and they forced him to carry the cross."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Along the way, they came across a man named Simon, who was from Cyrene, and the soldiers forced him to carry Jesus’ cross."
      }
