    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人责备他们，叫他们不要出声，他们却喊得更大声了：“主啊，大卫的后裔啊，可怜我们吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "群眾責備他們，叫他們不要出聲，他們卻更加放聲喊叫：“主啊，大衛的子孫，可憐我們吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人責備他們，不許他們作聲；他們卻越發喊着說：「主啊，大衛的子孫，可憐我們吧！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The crowd sternly told them to be quiet, but they cried out all the more, “Lord, Son of David, have mercy on us!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The crowd rebuked them and told them to be quiet, but they shouted all the louder, “Lord, Son of David, have mercy on us!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Be quiet!” the crowd yelled at them. But they only shouted louder, “Lord, Son of David, have mercy on us!”"
      }
