    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只是在她生下孩子之前没有与她同房。约瑟给孩子取名叫耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只是在孩子出生以前，並沒有與她同房。約瑟給孩子起名叫耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是沒有和她同房，等她生了兒子，就給他起名叫耶穌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but kept her a virgin until she gave birth to a Son; and he called His name Jesus."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But he did not consummate their marriage until she gave birth to a son. And he gave him the name Jesus."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But he did not have sexual relations with her until her son was born. And Joseph named him Jesus."
      }
    },
