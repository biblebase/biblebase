    "sermons": [
      {
        "id": "home_sermons_sermon-2011-02-20",
        "title": "那日子，那時辰",
        "date": "2011-02-20",
        "preacher": "王文堂牧師",
        "verses": "馬太福音 24:1-44",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-02-20.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-02-20.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“但没有人知道那日子和时辰何时来到，连天上的天使也不知道，人子也不知道，只有天父知道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“至於那日子和時間，沒有人知道，連天上的使者和子也不知道，只有父知道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「但那日子，那時辰，沒有人知道，連天上的使者也不知道，子也不知道，惟獨父知道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But of that day and hour no one knows, not even the angels of heaven, nor the Son, but the Father alone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But about that day or hour no one knows, not even the angels in heaven, nor the Son,    but only the Father."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“However, no one knows the day or hour when these things will happen, not even the angels in heaven or the Son himself. Only the Father knows."
      }
    },
