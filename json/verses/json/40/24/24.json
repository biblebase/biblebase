    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为假基督和假先知将出现，行很大的神迹奇事来迷惑人，如果可能，甚至要迷惑上帝拣选的子民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為必有假基督和假先知出現，顯大神蹟和奇事；如果可以的話，他們連選民也要迷惑。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為假基督、假先知將要起來，顯大神蹟、大奇事，倘若能行，連選民也就迷惑了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For false Christs and false prophets will arise and will show great signs and wonders, so as to mislead, if possible, even the elect."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For false messiahs and false prophets will appear and perform great signs and wonders to deceive, if possible, even the elect."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For false messiahs and false prophets will rise up and perform great signs and wonders so as to deceive, if possible, even God’s chosen ones."
      }
    },
