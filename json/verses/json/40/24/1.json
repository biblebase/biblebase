    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣离开圣殿时，门徒上前把宏伟的圣殿指给祂看。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌出了聖殿，往前走的時候，門徒前來把聖殿的建築指給他看。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌出了聖殿，正走的時候，門徒進前來，把殿宇指給他看。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus came out from the temple and was going away when His disciples came up to point out the temple buildings to Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus left the temple and was walking away when his disciples came up to him to call his attention to its buildings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As Jesus was leaving the Temple grounds, his disciples pointed out to him the various Temple buildings."
      }
    },
