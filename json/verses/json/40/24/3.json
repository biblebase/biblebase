    "sermons": [
      {
        "id": "home_sermons_sermon-2011-02-20",
        "title": "那日子，那時辰",
        "date": "2011-02-20",
        "preacher": "王文堂牧師",
        "verses": "馬太福音 24:1-44",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-02-20.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-02-20.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣正坐在橄榄山上，门徒私下来问祂：“请告诉我们，这事什么时候会发生？你再来和世界末日的时候会有什么预兆？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌坐在橄欖山上，門徒暗中前來問他：“請告訴我們，甚麼時候會有這些事呢？你的降臨和這世代的終結，有甚麼預兆呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌在橄欖山上坐着，門徒暗暗地來說：「請告訴我們，甚麼時候有這些事？你降臨和世界的末了有甚麼預兆呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As He was sitting on the Mount of Olives, the disciples came to Him privately, saying, “Tell us, when will these things happen, and what will be the sign of Your coming, and of the end of the age?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As Jesus was sitting on the Mount of Olives, the disciples came to him privately. “Tell us,” they said, “when will this happen, and what will be the sign of your coming and of the end of the age?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Later, Jesus sat on the Mount of Olives. His disciples came to him privately and said, “Tell us, when will all this happen? What sign will signal your return and the end of the world?”"
      }
    },
