    "sermons": [
      {
        "id": "home_sermons_sermon-2011-02-20",
        "title": "那日子，那時辰",
        "date": "2011-02-20",
        "preacher": "王文堂牧師",
        "verses": "馬太福音 24:1-44",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-02-20.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-02-20.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“当灾难的日子一过， “‘太阳变黑， 月亮无光， 众星陨落， 天体震动。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“那些日子的災難剛過去： 太陽就變黑了， 月亮也不發光， 眾星從天墜落， 天上的萬象震動。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「那些日子的災難一過去， 日頭就變黑了， 月亮也不放光， 眾星要從天上墜落， 天勢都要震動。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But immediately after the tribulation of those days the sun will be darkened, and the moon will not give its light, and the stars will fall from the sky, and the powers of the heavens will be shaken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Immediately after the distress of those days “ ‘the sun will be darkened, and the moon will not give its light; the stars will fall from the sky, and the heavenly bodies will be shaken.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Immediately after the anguish of those days, the sun will be darkened, the moon will give no light, the stars will fall from the sky, and the powers in the heavens will be shaken."
      }
    },
