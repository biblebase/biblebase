    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "门徒说：“在这荒野，我们到哪里找足够的食物给这么多人吃呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "門徒對他說：“我們在這曠野地方，哪裡有足夠的食物給這許多的人吃飽呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門徒說：「我們在這野地，哪裏有這麼多的餅叫這許多人吃飽呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The disciples *said to Him, “Where would we get so many loaves in this desolate place to satisfy such a large crowd?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His disciples answered, “Where could we get enough bread in this remote place to feed such a crowd?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The disciples replied, “Where would we get enough food here in the wilderness for such a huge crowd?”"
      }
    },
