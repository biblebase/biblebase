    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "善人心存良善，就从他里面发出良善；恶人心存邪恶，就从他里面发出邪恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "良善的人從他良善的心發出良善，邪惡的人從他邪惡的心發出邪惡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "善人從他心裏所存的善就發出善來；惡人從他心裏所存的惡就發出惡來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The good man brings out of his good treasure what is good; and the evil man brings out of his evil treasure what is evil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A good man brings good things out of the good stored up in him, and an evil man brings evil things out of the evil stored up in him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A good person produces good things from the treasury of a good heart, and an evil person produces evil things from the treasury of an evil heart."
      }
    },
