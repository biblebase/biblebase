    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "压伤的芦苇，祂不折断； 将残的灯火，祂不吹灭； 祂终必使正义得胜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "壓傷的蘆葦，他不折斷， 將殘的燈火，他不吹滅； 直到他施行公理，使公理得勝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "壓傷的蘆葦，他不折斷； 將殘的燈火，他不吹滅； 等他施行公理，叫公理得勝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A battered reed He will not break off, And a smoldering wick He will not put out, Until He leads justice to victory."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A bruised reed he will not break, and a smoldering wick he will not snuff out, till he has brought justice through to victory."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will not crush the weakest reed or put out a flickering candle. Finally he will cause justice to be victorious."
      }
    },
