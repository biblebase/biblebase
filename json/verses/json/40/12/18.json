    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“看啊！我所拣选、 所眷爱、所喜悦的仆人， 我要将我的灵赐给祂， 祂要向万邦宣扬正义。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪！我所揀選的僕人， 我所愛，心裡所喜悅的； 我要把我的靈賜給他， 他必向萬國宣揚公理。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪！我的僕人， 我所揀選、所親愛、心裏所喜悅的， 我要將我的靈賜給他； 他必將公理傳給外邦。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, My Servant whom I have chosen; My Beloved in whom My soul is well-pleased; I will put My Spirit upon Him, And He shall proclaim justice to the Gentiles."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Here is my servant whom I have chosen, the one I love, in whom I delight; I will put my Spirit on him, and he will proclaim justice to the nations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Look at my Servant, whom I have chosen. He is my Beloved, who pleases me. I will put my Spirit upon him, and he will proclaim justice to the nations."
      }
    },
