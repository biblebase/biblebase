    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时，耶稣在安息日经过一片麦田。祂的门徒饿了，便随手搓麦穗吃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "安息日那天，耶穌從麥田經過；他的門徒餓了，就摘了些麥穗來吃。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，耶穌在安息日從麥地經過。他的門徒餓了，就掐起麥穗來吃。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "At that time Jesus went through the grainfields on the Sabbath, and His disciples became hungry and began to pick the heads of grain and eat."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At that time Jesus went through the grainfields on the Sabbath. His disciples were hungry and began to pick some heads of grain and eat them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At about that time Jesus was walking through some grainfields on the Sabbath. His disciples were hungry, so they began breaking off some heads of grain and eating them."
      }
    },
