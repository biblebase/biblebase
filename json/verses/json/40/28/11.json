    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她们还在赶路的时候，有些守墓的卫兵已经进城把整件事告诉祭司长。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她們去的時候，有些衛兵進了城，把一切所發生的事向祭司長報告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她們去的時候，看守的兵有幾個進城去，將所經歷的事都報給祭司長。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now while they were on their way, some of the guard came into the city and reported to the chief priests all that had happened."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While the women were on their way, some of the guards went into the city and reported to the chief priests everything that had happened."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As the women were on their way, some of the guards went into the city and told the leading priests what had happened."
      }
