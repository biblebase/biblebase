    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对她们说：“不要害怕。去告诉我的弟兄，叫他们到加利利去，他们将在那里见到我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對她們說：“不要怕，去告訴我的弟兄，叫他們到加利利去，他們在那裡必看見我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對她們說：「不要害怕！你們去告訴我的弟兄，叫他們往加利利去，在那裏必見我。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jesus *said to them, “Do not be afraid; go and take word to My brethren to leave for Galilee, and there they will see Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jesus said to them, “Do not be afraid. Go and tell my brothers to go to Galilee; there they will see me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus said to them, “Don’t be afraid! Go tell my brothers to leave for Galilee, and they will see me there.”"
      }
    },
