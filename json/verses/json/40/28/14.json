    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果这件事传到总督那里，我们一定会替你们出面，保你们无事。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果總督知道了這件事，我們會說服他，不會牽連你們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "倘若這話被巡撫聽見，有我們勸他，保你們無事。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And if this should come to the governor’s ears, we will win him over and keep you out of trouble.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If this report gets to the governor, we will satisfy him and keep you out of trouble.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If the governor hears about it, we’ll stand up for you so you won’t get in trouble.”"
      }
    },
