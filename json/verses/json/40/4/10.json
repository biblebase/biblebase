    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“撒旦，走开！圣经上说，‘要敬拜主——你的上帝，单单事奉祂。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但耶穌說：“撒但，走開！經上記著： ‘當拜主你的　神， 單要事奉他。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「撒但，退去吧！因為經上記着說： 當拜主－你的神， 單要事奉他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jesus *said to him, “Go, Satan! For it is written, ‘You shall worship the Lord your God, and serve Him only.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus said to him, “Away from me, Satan! For it is written: ‘Worship the Lord your God, and serve him only.’”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Get out of here, Satan,” Jesus told him. “For the Scriptures say, ‘You must worship the Lord your God and serve only him.’”"
      }
    },
