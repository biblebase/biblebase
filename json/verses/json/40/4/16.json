    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们住在黑暗中的人看见了大光， 活在死亡阴影下的人被光照亮了！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "住在黑暗中的人民， 看見了大光； 死亡幽暗之地的居民， 有光照亮他們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那坐在黑暗裏的百姓看見了大光； 坐在死蔭之地的人有光發現照着他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The people who were sitting in darkness saw a great Light, And those who were sitting in the land and shadow of death, Upon them a Light dawned.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the people living in darkness have seen a great light; on those living in the land of the shadow of death a light has dawned.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "the people who sat in darkness have seen a great light. And for those who lived in the land where death casts its shadow, a light has shined.”"
      }
