    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "魔鬼再带耶稣到一座极高的山上，把世上万国及其荣华富贵展示给祂看，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "最後，魔鬼帶耶穌上了一座極高的山，把世界各國和各國的榮華都指給他看。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "魔鬼又帶他上了一座最高的山，將世上的萬國與萬國的榮華都指給他看，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Again, the devil *took Him to a very high mountain and *showed Him all the kingdoms of the world and their glory;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Again, the devil took him to a very high mountain and showed him all the kingdoms of the world and their splendor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Next the devil took him to the peak of a very high mountain and showed him all the kingdoms of the world and their glory."
      }
