    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可像他们那样，因为在你们祷告以前，你们的父已经知道你们的需要了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不可像他們，因為在你們祈求以先，你們的父已經知道你們的需要了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們不可效法他們；因為你們沒有祈求以先，你們所需用的，你們的父早已知道了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So do not be like them; for your Father knows what you need before you ask Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not be like them, for your Father knows what you need before you ask him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t be like them, for your Father knows exactly what you need even before you ask him!"
      }
