    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“何必为穿着忧虑呢？你们看看野地的百合花是如何生长的，它们既不劳苦，也不纺织。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "何必為衣服憂慮呢？試想田野的百合花怎樣生長，它們不勞苦，也不紡織。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "何必為衣裳憂慮呢？你想野地裏的百合花怎麼長起來；它也不勞苦，也不紡線。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And why are you worried about clothing? Observe how the lilies of the field grow; they do not toil nor do they spin,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“And why do you worry about clothes? See how the flowers of the field grow. They do not labor or spin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And why worry about your clothing? Look at the lilies of the field and how they grow. They don’t work or make their clothing,"
      }
    },
