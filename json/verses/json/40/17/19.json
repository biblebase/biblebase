    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "事后，门徒私下问耶稣：“我们为什么赶不走那鬼呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "門徒悄悄地前來問耶穌：“為甚麼我們不能把鬼趕出去呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門徒暗暗地到耶穌跟前，說：「我們為甚麼不能趕出那鬼呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the disciples came to Jesus privately and said, “Why could we not drive it out?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the disciples came to Jesus in private and asked, “Why couldn’t we drive it out?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Afterward the disciples asked Jesus privately, “Why couldn’t we cast out that demon?”"
      }
    },
