    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣斥责附在孩子身上的鬼，鬼就离开了那孩子，从此他就好了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌斥責那鬼，鬼就從孩子身上出來；從那時起，孩子就好了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌斥責那鬼，鬼就出來；從此孩子就痊癒了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus rebuked him, and the demon came out of him, and the boy was cured at once."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus rebuked the demon, and it came out of the boy, and he was healed at that moment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus rebuked the demon in the boy, and it left him. From that moment the boy was well."
      }
    },
