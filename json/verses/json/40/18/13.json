    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我实在告诉你们，如果找到了，他会非常欢喜，甚至比有那九十九只没有迷失的羊还欢喜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我實在告訴你們，他若找到了，就為這一隻羊歡喜，勝過為那九十九隻沒有迷失的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若是找着了，我實在告訴你們，他為這一隻羊歡喜，比為那沒有迷路的九十九隻歡喜還大呢！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If it turns out that he finds it, truly I say to you, he rejoices over it more than over the ninety-nine which have not gone astray."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And if he finds it, truly I tell you, he is happier about that one sheep than about the ninety-nine that did not wander off."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And if he finds it, I tell you the truth, he will rejoice over it more than over the ninety-nine that didn’t wander away!"
      }
    },
