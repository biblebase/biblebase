    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为这个人无法清还债款，王就下令把他及其妻儿和所有财产全部卖掉还债。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他沒有錢償還，主人就下令叫人把他和他的妻子兒女，以及一切所有的都賣掉，用來償還。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他沒有甚麼償還之物，主人吩咐把他和他妻子兒女，並一切所有的都賣了償還。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But since he did not have the means to repay, his lord commanded him to be sold, along with his wife and children and all that he had, and repayment to be made."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Since he was not able to pay, the master ordered that he and his wife and his children and all that he had be sold to repay the debt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He couldn’t pay, so his master ordered that he be sold—along with his wife, his children, and everything he owned—to pay the debt."
      }
    },
