    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你们不从心里饶恕自己的弟兄，我的天父也要这样对待你们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們各人不從心裡饒恕你的弟兄，我的天父也必這樣待你們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們各人若不從心裏饒恕你的弟兄，我天父也要這樣待你們了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My heavenly Father will also do the same to you, if each of you does not forgive his brother from your heart.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is how my heavenly Father will treat each of you unless you forgive your brother or sister from your heart.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“That’s what my heavenly Father will do to you if you refuse to forgive your brothers and sisters from your heart.”"
      }
