      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果他不听劝告，你就带一两位弟兄去见他，让两三个人为谈话作证。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果他不肯聽，就另外帶一兩個人同去，好使一切話，憑兩三個證人的口，可以確定。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他若不聽，你就另外帶一兩個人同去，要憑兩三個人的口作見證，句句都可定準。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But if he does not listen to you, take one or two more with you, so that by the mouth of two or three witnesses every fact may be confirmed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if they will not listen, take one or two others along, so that ‘every matter may be established by the testimony of two or three witnesses.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if you are unsuccessful, take one or two others with you and go back again, so that everything you say may be confirmed by two or three witnesses."
      }
    },
