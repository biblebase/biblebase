    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我又告诉你们，如果你们当中有两个人在地上同心合意地祈求，不论求什么，我天上的父必为你们成就。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我又告訴你們，你們當中若有兩個人，在地上同心為甚麼事祈求，我在天上的父必為他們成全。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我又告訴你們，若是你們中間有兩個人在地上同心合意地求甚麼事，我在天上的父必為他們成全。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Again I say to you, that if two of you agree on earth about anything that they may ask, it shall be done for them by My Father who is in heaven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Again, truly I tell you that if two of you on earth agree about anything they ask for, it will be done for them by my Father in heaven."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I also tell you this: If two of you agree here on earth concerning anything you ask, my Father in heaven will do it for you."
      }
    },
