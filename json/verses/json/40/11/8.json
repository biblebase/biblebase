    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果不是，你们到底想看什么？是看穿绫罗绸缎的人吗？那些穿绫罗绸缎的人生活在王宫里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們出去到底要看甚麼？身穿華麗衣裳的人嗎？那些穿著華麗衣裳的人，是在王宮裡的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們出去到底是要看甚麼？要看穿細軟衣服的人嗎？那穿細軟衣服的人是在王宮裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But what did you go out to see? A man dressed in soft clothing? Those who wear soft clothing are in kings’ palaces!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If not, what did you go out to see? A man dressed in fine clothes? No, those who wear fine clothes are in kings’ palaces."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Or were you expecting to see a man dressed in expensive clothes? No, people with expensive clothes live in palaces."
      }
