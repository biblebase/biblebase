    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "出卖耶稣的犹大预先和他们定了暗号，说：“我亲吻谁，谁就是耶稣，你们把祂抓起来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "出賣耶穌的人給他們一個暗號，說：“我跟誰親吻，誰就是他；你們可以抓住他。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那賣耶穌的給了他們一個暗號，說：「我與誰親嘴，誰就是他。你們可以拿住他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now he who was betraying Him gave them a sign, saying, “Whomever I kiss, He is the one; seize Him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the betrayer had arranged a signal with them: “The one I kiss is the man; arrest him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The traitor, Judas, had given them a prearranged signal: “You will know which one to arrest when I greet him with a kiss.”"
      }
    },
