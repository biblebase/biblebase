    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对他说：“朋友，你要做的事，快动手吧。”于是那些人上前，下手捉拿耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對他說：“朋友，你來要作的事，快作吧！”於是那些人上前來，動手拿住耶穌，逮捕了他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他說：「朋友，你來要做的事，就做吧。」於是那些人上前，下手拿住耶穌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus said to him, “Friend, do what you have come for.” Then they came and laid hands on Jesus and seized Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus replied, “Do what you came for, friend.” Then the men stepped forward, seized Jesus and arrested him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus said, “My friend, go ahead and do what you have come for.” Then the others grabbed Jesus and arrested him."
      }
    },
