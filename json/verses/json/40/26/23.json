    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂说：“那和我一同在盘子里蘸饼吃的就是要出卖我的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他回答：“那和我一同把手蘸在盤子裡的人，他要出賣我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌回答說：「同我蘸手在盤子裏的，就是他要賣我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He answered, “He who dipped his hand with Me in the bowl is the one who will betray Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus replied, “The one who has dipped his hand into the bowl with me will betray me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He replied, “One of you who has just eaten from this bowl with me will betray me."
      }
    },
