    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "过了一会儿，旁边站着的人过来对彼得说：“你肯定也是他们一伙的，听你的口音就知道了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "過了一會，站在那裡的人前來對他說：“你的確是他們中間的一個，因為一聽你的口音就認出來了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "過了不多的時候，旁邊站着的人前來，對彼得說：「你真是他們一黨的，你的口音把你露出來了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A little later the bystanders came up and said to Peter, “Surely you too are one of them; for even the way you talk gives you away.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After a little while, those standing there went up to Peter and said, “Surely you are one of them; your accent gives you away.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A little later some of the other bystanders came over to Peter and said, “You must be one of them; we can tell by your Galilean accent.”"
      }
    },
