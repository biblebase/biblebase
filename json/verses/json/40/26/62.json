    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大祭司站起来质问耶稣：“你不回答吗？这些人作证控告你的是什么呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大祭司就站起來，對耶穌說：“你為甚麼不回答？這些人作證，控告你的是甚麼呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大祭司就站起來，對耶穌說：「你甚麼都不回答嗎？這些人作見證告你的是甚麼呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The high priest stood up and said to Him, “Do You not answer? What is it that these men are testifying against You?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the high priest stood up and said to Jesus, “Are you not going to answer? What is this testimony that these men are bringing against you?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the high priest stood up and said to Jesus, “Well, aren’t you going to answer these charges? What do you have to say for yourself?”"
      }
