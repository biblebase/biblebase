    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼得远远地跟着耶稣，一直跟到大祭司的院子里。他坐在卫兵当中，想知道事情的结果。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼得遠遠地跟著耶穌，直到大祭司的官邸。他進到裡面，和差役坐在一起，要看事情怎樣了結。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼得遠遠地跟着耶穌，直到大祭司的院子，進到裏面，就和差役同坐，要看這事到底怎樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Peter was following Him at a distance as far as the courtyard of the high priest, and entered in, and sat down with the officers to see the outcome."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Peter followed him at a distance, right up to the courtyard of the high priest. He entered and sat down with the guards to see the outcome."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Meanwhile, Peter followed him at a distance and came to the high priest’s courtyard. He went in and sat with the guards and waited to see how it would all end."
      }
    },
