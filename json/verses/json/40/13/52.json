    "sermons": [
      {
        "id": "home_sermons_sermon-2010-01-24",
        "title": "天國的比喻",
        "date": "2010-01-24",
        "preacher": "王文堂牧師",
        "verses": "馬太福音 13:1-52",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2010/Sermon-2010-01-24.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2010/Sermon-2010-01-24.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“律法教师成为天国的门徒后，就像一个家的主人，能从他的库房里拿出新的和旧的东西来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說：“所以，每一個作天國門徒的經學家，就像家主從寶庫中拿出新和舊的東西來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：「凡文士受教作天國的門徒，就像一個家主從他庫裏拿出新舊的東西來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus said to them, “Therefore every scribe who has become a disciple of the kingdom of heaven is like a head of a household, who brings out of his treasure things new and old.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to them, “Therefore every teacher of the law who has become a disciple in the kingdom of heaven is like the owner of a house who brings out of his storeroom new treasures as well as old.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he added, “Every teacher of religious law who becomes a disciple in the Kingdom of Heaven is like a homeowner who brings from his storeroom new gems of truth as well as old.”"
      }
    },
