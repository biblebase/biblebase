    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "麦田代表整个世界，那些好种子就是天国的子民。毒麦就是那些属于魔鬼的人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "田就是世界，好種子就是屬天國的人，稗子就是屬那惡者的人，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "田地就是世界；好種就是天國之子；稗子就是那惡者之子；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and the field is the world; and as for the good seed, these are the sons of the kingdom; and the tares are the sons of the evil one;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The field is the world, and the good seed stands for the people of the kingdom. The weeds are the people of the evil one,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The field is the world, and the good seed represents the people of the Kingdom. The weeds are the people who belong to the evil one."
      }
    },
