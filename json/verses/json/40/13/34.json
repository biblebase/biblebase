    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣总是用比喻对众人讲这些事，祂每次对他们讲话时都用比喻。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌用比喻向群眾講了這一切，他所講的，沒有不用比喻的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這都是耶穌用比喻對眾人說的話；若不用比喻，就不對他們說甚麼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All these things Jesus spoke to the crowds in parables, and He did not speak to them without a parable."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus spoke all these things to the crowd in parables; he did not say anything to them without using a parable."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus always used stories and illustrations like these when speaking to the crowds. In fact, he never spoke to them without using such parables."
      }
    },
