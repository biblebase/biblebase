    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人们睡觉的时候，仇敌过来把毒麦撒在他的麦田里，就走了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人們睡了的時候，他的仇敵來把稗子撒在麥子中間，就走了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "及至人睡覺的時候，有仇敵來，將稗子撒在麥子裏就走了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But while his men were sleeping, his enemy came and sowed tares among the wheat, and went away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But while everyone was sleeping, his enemy came and sowed weeds among the wheat, and went away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But that night as the workers slept, his enemy came and planted weeds among the wheat, then slipped away."
      }
    },
