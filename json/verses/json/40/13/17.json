    "sermons": [
      {
        "id": "home_sermons_sermon-2010-01-24",
        "title": "天國的比喻",
        "date": "2010-01-24",
        "preacher": "王文堂牧師",
        "verses": "馬太福音 13:1-52",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2010/Sermon-2010-01-24.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2010/Sermon-2010-01-24.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我实在告诉你们，以前有许多先知和义人曾渴望看见你们所看见的，听见你们所听见的，却未能如愿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我實在告訴你們，曾經有許多先知和義人想看你們所看見的，卻沒有看到，想聽你們所聽見的，卻沒有聽到。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我實在告訴你們，從前有許多先知和義人要看你們所看的，卻沒有看見，要聽你們所聽的，卻沒有聽見。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For truly I say to you that many prophets and righteous men desired to see what you see, and did not see it, and to hear what you hear, and did not hear it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For truly I tell you, many prophets and righteous people longed to see what you see but did not see it, and to hear what you hear but did not hear it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I tell you the truth, many prophets and righteous people longed to see what you see, but they didn’t see it. And they longed to hear what you hear, but they didn’t hear it."
      }
    },
