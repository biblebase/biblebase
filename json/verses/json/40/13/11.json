    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣回答说：“天国的奥秘只让你们知道，不让他们知道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他回答：“天國的奧祕，只給你們知道，卻不給他們知道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌回答說：「因為天國的奧祕只叫你們知道，不叫他們知道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus answered them, “To you it has been granted to know the mysteries of the kingdom of heaven, but to them it has not been granted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He replied, “Because the knowledge of the secrets of the kingdom of heaven has been given to you, but not to them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He replied, “You are permitted to understand the secrets of the Kingdom of Heaven, but others are not."
      }
    },
