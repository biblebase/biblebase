    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣又给他们讲了一个比喻：“天国就像一粒芥菜种，人把它种在田里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌又對他們講了另外一個比喻，說：“天國好像一粒芥菜種，人拿去把它種在田裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又設個比喻對他們說：「天國好像一粒芥菜種，有人拿去種在田裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He presented another parable to them, saying, “The kingdom of heaven is like a mustard seed, which a man took and sowed in his field;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He told them another parable: “The kingdom of heaven is like a mustard seed, which a man took and planted in his field."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Here is another illustration Jesus used: “The Kingdom of Heaven is like a mustard seed planted in a field."
      }
    },
