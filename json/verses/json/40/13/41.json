    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人子也将派天使把一切引人犯罪和作孽的人从祂国里挑出来，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，人子要差派他的使者，把一切使人犯罪的事和不法之徒，從他的國中拔掉，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人子要差遣使者，把一切叫人跌倒的和作惡的，從他國裏挑出來，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Son of Man will send forth His angels, and they will gather out of His kingdom all stumbling blocks, and those who commit lawlessness,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Son of Man will send out his angels, and they will weed out of his kingdom everything that causes sin and all who do evil."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Son of Man will send his angels, and they will remove from his Kingdom everything that causes sin and all who do evil."
      }
    },
