    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这是要应验先知的话：“我要开口讲比喻，道出创世以来隐藏的事。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這就應驗了先知所說的： “我要開口用比喻， 把創世以來隱祕的事說出來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這是要應驗先知的話，說： 我要開口用比喻， 把創世以來所隱藏的事發明出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This was to fulfill what was spoken through the prophet: “I will open My mouth in parables; I will utter things hidden since the foundation of the world.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So was fulfilled what was spoken through the prophet: “I will open my mouth in parables, I will utter things hidden since the creation of the world.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This fulfilled what God had spoken through the prophet: “I will speak to you in parables. I will explain things hidden since the creation of the world.”"
      }
    },
