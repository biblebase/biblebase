    "sermons": [
      {
        "id": "home_sermons_sermon-2010-01-24",
        "title": "天國的比喻",
        "date": "2010-01-24",
        "preacher": "王文堂牧師",
        "verses": "馬太福音 13:1-52",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2010/Sermon-2010-01-24.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2010/Sermon-2010-01-24.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为这些人心灵麻木， 耳朵发背，眼睛昏花， 以致眼睛看不见，耳朵听不见， 心里不明白，无法回心转意， 得不到我的医治。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為這人民的心思遲鈍， 用不靈的耳朵去聽， 又閉上了眼睛； 免得自己眼睛看見，耳朵聽見， 心裡明白，回轉過來， 我就醫好他們。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為這百姓油蒙了心， 耳朵發沉， 眼睛閉着， 恐怕眼睛看見， 耳朵聽見， 心裏明白，回轉過來， 我就醫治他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the heart of this people has become dull, With their ears they scarcely hear, And they have closed their eyes, Otherwise they would see with their eyes, Hear with their ears, And understand with their heart and return, And I would heal them.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this people’s heart has become calloused; they hardly hear with their ears, and they have closed their eyes. Otherwise they might see with their eyes, hear with their ears, understand with their hearts and turn, and I would heal them.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the hearts of these people are hardened, and their ears cannot hear, and they have closed their eyes— so their eyes cannot see, and their ears cannot hear, and their hearts cannot understand, and they cannot turn to me and let me heal them.’"
      }
    },
