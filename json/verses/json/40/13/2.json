    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有一大群人聚集到祂周围，祂只好上船坐下，众人都站在岸上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有很多人聚集到他那裡。於是，他上船坐下來，眾人都站在岸上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有許多人到他那裏聚集，他只得上船坐下，眾人都站在岸上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And large crowds gathered to Him, so He got into a boat and sat down, and the whole crowd was standing on the beach."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Such large crowds gathered around him that he got into a boat and sat in it, while all the people stood on the shore."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A large crowd soon gathered around him, so he got into a boat. Then he sat there and taught as the people stood on the shore."
      }
    },
