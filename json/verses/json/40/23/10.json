    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要让人称你们为导师，因为你们只有一位导师——基督。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們也不要被人稱為師傅，因為只有一位是你們的師傅，就是基督。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也不要受師尊的稱呼，因為只有一位是你們的師尊，就是基督。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not be called leaders; for One is your Leader, that is, Christ."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nor are you to be called instructors, for you have one Instructor, the Messiah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And don’t let anyone call you ‘Teacher,’ for you have only one teacher, the Messiah."
      }
    },
