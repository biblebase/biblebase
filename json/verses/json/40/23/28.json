    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们虚有公义的外表，内心却充满了各种的虚伪和罪恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "照樣，你們外面看來像義人，裡面卻充塞著虛偽和不法。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們也是如此，在人前，外面顯出公義來，裏面卻裝滿了假善和不法的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So you, too, outwardly appear righteous to men, but inwardly you are full of hypocrisy and lawlessness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the same way, on the outside you appear to people as righteous but on the inside you are full of hypocrisy and wickedness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Outwardly you look like righteous people, but inwardly your hearts are filled with hypocrisy and lawlessness."
      }
    },
