    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你们不要让别人称呼你们为老师，因为你们只有一位老师，你们彼此都是弟兄。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然而你們不要被人稱為‘拉比’，因為只有一位是你們的老師，你們都是弟兄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你們不要受拉比的稱呼，因為只有一位是你們的夫子；你們都是弟兄。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But do not be called Rabbi; for One is your Teacher, and you are all brothers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But you are not to be called ‘Rabbi,’ for you have one Teacher, and you are all brothers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Don’t let anyone call you ‘Rabbi,’ for you have only one teacher, and all of you are equal as brothers and sisters."
      }
    },
