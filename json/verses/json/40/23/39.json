    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我告诉你们，从现在起，直到你们说‘奉主名来的当受称颂’那天之前，你们再也见不到我了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我告訴你們，從今以後，你們一定見不到我，直等到你們說：‘奉主名來的，是應當稱頌的。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我告訴你們，從今以後，你們不得再見我，直等到你們說：『奉主名來的是應當稱頌的。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I say to you, from now on you will not see Me until you say, ‘Blessed is He who comes in the name of the Lord!’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I tell you, you will not see me again until you say, ‘Blessed is he who comes in the name of the Lord.’”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I tell you this, you will never see me again until you say, ‘Blessings on the one who comes in the name of the Lord!’”"
      }
    },
