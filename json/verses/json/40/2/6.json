    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘犹大地区的伯利恒啊！ 你在犹大各城中并不是最小的， 因为有一位君王要从你那里出来， 牧养我的以色列子民。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘猶大地的伯利恆啊！ 你在猶大的領袖中，並不是最小的， 因為必有一位領袖從你那裡出來， 牧養我的子民以色列。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大地的伯利恆啊， 你在猶大諸城中並不是最小的； 因為將來有一位君王要從你那裏出來， 牧養我以色列民。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘And you, Bethlehem, land of Judah, Are by no means least among the leaders of Judah; For out of you shall come forth a Ruler Who will shepherd My people Israel.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘But you, Bethlehem, in the land of Judah, are by no means least among the rulers of Judah; for out of you will come a ruler who will shepherd my people Israel.’”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘And you, O Bethlehem in the land of Judah, are not least among the ruling cities of Judah, for a ruler will come from you who will be the shepherd for my people Israel.’”"
      }
    },
