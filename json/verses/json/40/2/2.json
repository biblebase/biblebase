    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "问道：“那生来做犹太人之王的在哪里呢？我们在东方看见祂的星，特来朝拜祂。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說：“那生下來作猶太人的王的在哪裡？我們看見他的星出現，特來朝拜他。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「那生下來作猶太人之王的在哪裏？我們在東方看見他的星，特來拜他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Where is He who has been born King of the Jews? For we saw His star in the east and have come to worship Him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and asked, “Where is the one who has been born king of the Jews? We saw his star when it rose and have come to worship him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Where is the newborn king of the Jews? We saw his star as it rose, and we have come to worship him.”"
      }
    },
