    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，希律暗中召见那几位智者，仔细查问那星出现的准确时间，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希律暗中把星象家召來，仔細查問他們，那顆星甚麼時候出現，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當下，希律暗暗地召了博士來，細問那星是甚麼時候出現的，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Herod secretly called the magi and determined from them the exact time the star appeared."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Herod called the Magi secretly and found out from them the exact time the star had appeared."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Herod called for a private meeting with the wise men, and he learned from them the time when the star first appeared."
      }
