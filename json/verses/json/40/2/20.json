      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“起来，带着孩子和祂母亲回以色列去吧！因为要杀害孩子的人已经死了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說：“那些要殺害這孩子的人已經死了。起來，帶著孩子和他母親回以色列地去吧。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「起來！帶着小孩子和他母親往以色列地去，因為要害小孩子性命的人已經死了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Get up, take the Child and His mother, and go into the land of Israel; for those who sought the Child’s life are dead.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and said, “Get up, take the child and his mother and go to the land of Israel, for those who were trying to take the child’s life are dead.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Get up!” the angel said. “Take the child and his mother back to the land of Israel, because those who were trying to kill the child are dead.”"
      }
