    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们听了王的吩咐，就去了。忽然，那颗曾在东方出现的星又出现在他们前面，引领他们来到小孩耶稣所在的地方，便停下来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們聽命去了。他們在東方看見的那顆星，忽然在他們前頭，領他們到那小孩所在的地方，就在上頭停住了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們聽見王的話就去了。在東方所看見的那星忽然在他們前頭行，直行到小孩子的地方，就在上頭停住了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After hearing the king, they went their way; and the star, which they had seen in the east, went on before them until it came and stood over the place where the Child was."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After they had heard the king, they went on their way, and the star they had seen when it rose went ahead of them until it stopped over the place where the child was."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After this interview the wise men went their way. And the star they had seen in the east guided them to Bethlehem. It went ahead of them and stopped over the place where the child was."
      }
    },
