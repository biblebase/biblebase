    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你们怎样论断别人，也必照样被论断；你们用什么尺度衡量别人，也要用什么尺度衡量你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們怎樣判斷人，也必怎樣被判斷；你們用甚麼標準衡量人，也必照樣被衡量。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你們怎樣論斷人，也必怎樣被論斷；你們用甚麼量器量給人，也必用甚麼量器量給你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For in the way you judge, you will be judged; and by your standard of measure, it will be measured to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For in the same way you judge others, you will be judged, and with the measure you use, it will be measured to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you will be treated as you treat others. The standard you use in judging is the standard by which you will be judged."
      }
    },
