    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万民要聚集在祂面前。祂要把他们分开，就像牧人把绵羊和山羊分开一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬族要聚集在他面前，他要把他們彼此分開，好像牧羊人把綿羊和山羊分開一樣："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬民都要聚集在他面前。他要把他們分別出來，好像牧羊的分別綿羊山羊一般，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the nations will be gathered before Him; and He will separate them from one another, as the shepherd separates the sheep from the goats;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the nations will be gathered before him, and he will separate the people one from another as a shepherd separates the sheep from the goats."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the nations will be gathered in his presence, and he will separate the people as a shepherd separates the sheep from the goats."
      }
    },
