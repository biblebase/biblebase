    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“当人子在祂的荣耀中与众天使降临的时候，祂要坐在荣耀的宝座上，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“當人子在他的榮耀裡，帶著所有的使者降臨的時候，他要坐在榮耀的寶座上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「當人子在他榮耀裏、同着眾天使降臨的時候，要坐在他榮耀的寶座上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But when the Son of Man comes in His glory, and all the angels with Him, then He will sit on His glorious throne."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When the Son of Man comes in his glory, and all the angels with him, he will sit on his glorious throne."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But when the Son of Man comes in his glory, and all the angels with him, then he will sit upon his glorious throne."
      }
    },
