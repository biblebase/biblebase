    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "试图保全自己生命的反而会失去生命，但为我舍弃生命的反而会得到生命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "顧惜自己生命的，必要喪掉生命；但為我犧牲生命的，必要得著生命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "得着生命的，將要失喪生命；為我失喪生命的，將要得着生命。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who has found his life will lose it, and he who has lost his life for My sake will find it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever finds their life will lose it, and whoever loses their life for my sake will find it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you cling to your life, you will lose it; but if you give up your life for me, you will find it."
      }
    },
