    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“好，你来吧！” 于是，彼得就从船上下去，走在湖面上，要去耶稣那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“來吧！”彼得就從船上下來，行在水面上，走向耶穌那裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「你來吧。」彼得就從船上下去，在水面上走，要到耶穌那裏去；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said, “Come!” And Peter got out of the boat, and walked on the water and came toward Jesus."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Come,” he said. Then Peter got down out of the boat, walked on the water and came toward Jesus."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yes, come,” Jesus said. So Peter went over the side of the boat and walked on the water toward Jesus."
      }
    },
