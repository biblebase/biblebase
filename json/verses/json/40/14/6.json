    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在希律的生日那天，希罗底的女儿出来在众人面前跳舞。希律非常高兴，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了希律生日的那天，希羅底的女兒在眾人面前跳舞，希律非常高興，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了希律的生日，希羅底的女兒在眾人面前跳舞，使希律歡喜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when Herod’s birthday came, the daughter of Herodias danced before them and pleased Herod,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On Herod’s birthday the daughter of Herodias danced for the guests and pleased Herod so much"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But at a birthday party for Herod, Herodias’s daughter performed a dance that greatly pleased him,"
      }
