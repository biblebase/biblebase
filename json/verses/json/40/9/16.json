    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“没有人用新布缝补旧衣，恐怕新布会把旧衣扯破，破洞会更大。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有人會拿一塊新布補在舊衣服上，因為補上的會把衣服扯破，裂的地方就更大了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "沒有人把新布補在舊衣服上；因為所補上的反帶壞了那衣服，破的就更大了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But no one puts a patch of unshrunk cloth on an old garment; for the patch pulls away from the garment, and a worse tear results."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“No one sews a patch of unshrunk cloth on an old garment, for the patch will pull away from the garment, making the tear worse."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Besides, who would patch old clothing with new cloth? For the new patch would shrink and rip away from the old cloth, leaving an even bigger tear than before."
      }
    },
