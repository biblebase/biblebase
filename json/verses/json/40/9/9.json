    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣离开那里往前走，看见一个名叫马太的人坐在收税站里，便对他说：“跟从我！”马太就站起来跟从了耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌從那裡往前走，看見一個人，名叫馬太，坐在稅關那裡，就對他說：“來跟從我！”他就起來跟從了耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌從那裏往前走，看見一個人名叫馬太，坐在稅關上，就對他說：「你跟從我來。」他就起來跟從了耶穌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As Jesus went on from there, He saw a man called Matthew, sitting in the tax collector’s booth; and He *said to him, “Follow Me!” And he got up and followed Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As Jesus went on from there, he saw a man named Matthew sitting at the tax collector’s booth. “Follow me,” he told him, and Matthew got up and followed him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As Jesus was walking along, he saw a man named Matthew sitting at his tax collector’s booth. “Follow me and be my disciple,” Jesus said to him. So Matthew got up and followed him."
      }
    },
