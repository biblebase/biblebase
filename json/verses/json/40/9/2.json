    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人用担架把一个瘫子抬到耶稣面前。耶稣看见他们的信心，就对瘫子说：“孩子，放心吧！你的罪得到赦免了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有人把一個躺在床上的癱子帶到他那裡。耶穌看見他們的信心，就對癱子說：“孩子，放心！你的罪赦了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人用褥子擡着一個癱子到耶穌跟前來。耶穌見他們的信心，就對癱子說：「小子，放心吧！你的罪赦了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they brought to Him a paralytic lying on a bed. Seeing their faith, Jesus said to the paralytic, “Take courage, son; your sins are forgiven.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Some men brought to him a paralyzed man, lying on a mat. When Jesus saw their faith, he said to the man, “Take heart, son; your sins are forgiven.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some people brought to him a paralyzed man on a mat. Seeing their faith, Jesus said to the paralyzed man, “Be encouraged, my child! Your sins are forgiven.”"
      }
    },
