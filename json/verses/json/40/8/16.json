    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当天晚上，有人带着许多被鬼附身的人来见耶稣。耶稣一句话就把鬼赶出去了，并医好了所有的病人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了黃昏，有些人帶了許多被鬼附的人來見耶穌，他只用一句話就把污靈趕出去，並且醫好了所有患病的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了晚上，有人帶着許多被鬼附的來到耶穌跟前，他只用一句話就把鬼都趕出去，並且治好了一切有病的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When evening came, they brought to Him many who were demon-possessed; and He cast out the spirits with a word, and healed all who were ill."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When evening came, many who were demon-possessed were brought to him, and he drove out the spirits with a word and healed all the sick."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That evening many demon-possessed people were brought to Jesus. He cast out the evil spirits with a simple command, and he healed all the sick."
      }
    },
