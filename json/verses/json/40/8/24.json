    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "忽然，湖面上狂风大作，波涛汹涌，船快要被巨浪吞没，耶稣却在睡觉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "忽然海上起了狂風，甚至船都被波濤掩蓋，但耶穌卻睡著了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "海裏忽然起了暴風，甚至船被波浪掩蓋；耶穌卻睡着了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And behold, there arose a great storm on the sea, so that the boat was being covered with the waves; but Jesus Himself was asleep."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Suddenly a furious storm came up on the lake, so that the waves swept over the boat. But Jesus was sleeping."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Suddenly, a fierce storm struck the lake, with waves breaking into the boat. But Jesus was sleeping."
      }
    },
