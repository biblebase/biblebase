    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "门徒连忙叫醒耶稣，说：“主啊，救命！我们快淹死了！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們走來把他叫醒，說：“主啊！救命啊！我們沒命了！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "門徒來叫醒了他，說：「主啊，救我們，我們喪命啦！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they came to Him and woke Him, saying, “Save us, Lord; we are perishing!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The disciples went and woke him, saying, “Lord, save us! We’re going to drown!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The disciples went and woke him up, shouting, “Lord, save us! We’re going to drown!”"
      }
