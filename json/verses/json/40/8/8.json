    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "百夫长说：“主啊，我不配让你亲自来我家。你只要说一句话，我仆人就会痊愈。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "百夫長回答：“主啊，要你到舍下來，實在不敢當。只要你說一句話，我的僕人就必好了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "百夫長回答說：「主啊，你到我舍下，我不敢當；只要你說一句話，我的僕人就必好了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the centurion said, “Lord, I am not worthy for You to come under my roof, but just say the word, and my servant will be healed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The centurion replied, “Lord, I do not deserve to have you come under my roof. But just say the word, and my servant will be healed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the officer said, “Lord, I am not worthy to have you come into my home. Just say the word from where you are, and my servant will be healed."
      }
    },
