    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我实在告诉你们，有些站在这里的人会在有生之年看见人子降临在祂的国度里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我實在告訴你們，站在這裡的，有人在沒有嘗過死味以前，必要看見人子帶著他的國降臨。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我實在告訴你們，站在這裏的，有人在沒嘗死味以前必看見人子降臨在他的國裏。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Truly I say to you, there are some of those who are standing here who will not taste death until they see the Son of Man coming in His kingdom.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Truly I tell you, some who are standing here will not taste death before they see the Son of Man coming in his kingdom.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I tell you the truth, some standing here right now will not die before they see the Son of Man coming in his Kingdom.”"
      }
    },
