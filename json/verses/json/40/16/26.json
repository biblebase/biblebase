    "sermons": [
      {
        "id": "home_sermons_sermon-2009-03-08",
        "title": "僅此一生",
        "date": "2009-03-08",
        "preacher": "邱志健牧師",
        "verses": "馬太福音 16:24-26",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2009/Sermon-2009-03-08.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人若赚得全世界，却丧失自己的生命，又有什么益处呢？人还能拿什么换回自己的生命呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人若賺得全世界，卻賠上自己的生命，有甚麼好處呢？人還能用甚麼換回自己的生命呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人若賺得全世界，賠上自己的生命，有甚麼益處呢？人還能拿甚麼換生命呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For what will it profit a man if he gains the whole world and forfeits his soul? Or what will a man give in exchange for his soul?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What good will it be for someone to gain the whole world, yet forfeit their soul? Or what can anyone give in exchange for their soul?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And what do you benefit if you gain the whole world but lose your own soul? Is anything worth more than your soul?"
      }
    },
