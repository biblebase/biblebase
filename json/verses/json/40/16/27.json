    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“因为人子要在祂父的荣耀中与众天使一起降临，那时，祂将按照各人的行为报应各人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人子要在父的榮耀裡和眾天使一同降臨，那時他要照各人的行為報應各人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人子要在他父的榮耀裏，同着眾使者降臨；那時候，他要照各人的行為報應各人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Son of Man is going to come in the glory of His Father with His angels, and will then repay every man according to his deeds."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the Son of Man is going to come in his Father’s glory with his angels, and then he will reward each person according to what they have done."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Son of Man will come with his angels in the glory of his Father and will judge all people according to their deeds."
      }
    },
