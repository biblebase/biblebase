    "sermons": [
      {
        "id": "home_sermons_sermon-2009-03-08",
        "title": "僅此一生",
        "date": "2009-03-08",
        "preacher": "邱志健牧師",
        "verses": "馬太福音 16:24-26",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2009/Sermon-2009-03-08.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为想救自己生命的，必失去生命；但为了我而失去生命的，必得到生命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是想救自己生命的，必喪掉生命；但為我犧牲生命的，必得著生命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，凡要救自己生命的，必喪掉生命；凡為我喪掉生命的，必得着生命。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For whoever wishes to save his life will lose it; but whoever loses his life for My sake will find it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For whoever wants to save their life  will lose it, but whoever loses their life for me will find it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you try to hang on to your life, you will lose it. But if you give up your life for my sake, you will save it."
      }
    },
