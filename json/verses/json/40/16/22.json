    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼得把耶稣拉到一边，劝阻祂说：“主啊，千万不可！这件事绝不会发生在你身上！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼得就把他拉到一邊，責怪他說：“主啊，千萬不可這樣，這事一定不會發生在你身上的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼得就拉着他，勸他說：「主啊，萬不可如此！這事必不臨到你身上。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Peter took Him aside and began to rebuke Him, saying, “God forbid it, Lord! This shall never happen to You.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Peter took him aside and began to rebuke him. “Never, Lord!” he said. “This shall never happen to you!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Peter took him aside and began to reprimand him for saying such things. “Heaven forbid, Lord,” he said. “This will never happen to you!”"
      }
    },
