    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在祂面前，群山震动，丘陵消融。 在祂面前，大地隆起， 世界和世上的一切都战栗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大山在他面前震動， 小山也都融化； 大地在他面前廢去， 世界和所有住在世上的，也都這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大山因他震動； 小山也都消化。 大地在他面前突起； 世界和住在其間的也都如此。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Mountains quake because of Him And the hills dissolve; Indeed the earth is upheaved by His presence, The world and all the inhabitants in it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The mountains quake before him and the hills melt away. The earth trembles at his presence, the world and all who live in it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In his presence the mountains quake, and the hills melt away; the earth trembles, and its people are destroyed."
      }
