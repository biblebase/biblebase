    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂斥责大海，使大海干涸； 祂使一切河流枯竭。 巴珊和迦密的草木凋零， 黎巴嫩的鲜花衰残。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他一斥責海，海就乾了； 他使一切江河枯乾。 巴珊和迦密的樹林枯槁， 黎巴嫩的花卉凋殘。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他斥責海，使海乾了， 使一切江河乾涸。 巴珊和迦密的樹林衰殘； 黎巴嫩的花草也衰殘了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He rebukes the sea and makes it dry; He dries up all the rivers. Bashan and Carmel wither; The blossoms of Lebanon wither."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He rebukes the sea and dries it up; he makes all the rivers run dry. Bashan and Carmel wither and the blossoms of Lebanon fade."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At his command the oceans dry up, and the rivers disappear. The lush pastures of Bashan and Carmel fade, and the green forests of Lebanon wither."
      }
    },
