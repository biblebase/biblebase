    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尼尼微啊， 你也必像醉汉一样， 你必寻找藏身之处躲避敌人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於你，你必喝醉， 必被隱藏； 你必因仇敵的緣故， 尋找避難所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你也必喝醉，必被埋藏， 並因仇敵的緣故尋求避難所。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You too will become drunk, You will be hidden. You too will search for a refuge from the enemy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You too will become drunk; you will go into hiding and seek refuge from the enemy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And you, Nineveh, will also stagger like a drunkard. You will hide for fear of the attacking enemy."
      }
