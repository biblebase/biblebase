      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的商贾多过天上的繁星， 但他们像吃光后飞去的蝗虫。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你增加了你的商人， 多過天上的星辰。 蚱蜢退了殼，就飛去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你增添商賈，多過天上的星； 蝻子吃盡而去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have increased your traders more than the stars of heaven— The creeping locust strips and flies away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have increased the number of your merchants till they are more numerous than the stars in the sky, but like locusts they strip the land and then fly away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your merchants have multiplied until they outnumber the stars. But like a swarm of locusts, they strip the land and fly away."
      }
    },
