    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚述王啊！ 你的牧人沉睡，贵族酣眠， 你的百姓分散在群山上， 无人招聚他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞述王啊！你的牧人睡覺， 你的貴族安歇， 你的子民分散在各山上， 沒有人招聚他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞述王啊，你的牧人睡覺； 你的貴冑安歇； 你的人民散在山間，無人招聚。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your shepherds are sleeping, O king of Assyria; Your nobles are lying down. Your people are scattered on the mountains And there is no one to regather them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "King of Assyria, your shepherds slumber; your nobles lie down to rest. Your people are scattered on the mountains with no one to gather them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your shepherds are asleep, O Assyrian king; your princes lie dead in the dust. Your people are scattered across the mountains with no one to gather them together."
      }
    },
