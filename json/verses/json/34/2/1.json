    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尼尼微啊， 攻击者已经向你扑来。 你要进驻堡垒， 把守道路， 集中全力， 准备上阵！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "尼尼微啊！ 那分散邦國的必上來攻擊你， 你要固守堡壘， 嚴防要道，束緊你的腰， 大大增強你的力量。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "尼尼微啊，那打碎邦國的上來攻擊你。 你要看守保障，謹防道路， 使腰強壯，大大勉力。 （"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The one who scatters has come up against you. Man the fortress, watch the road; Strengthen your back, summon all your strength."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "An attacker advances against you, Nineveh. Guard the fortress, watch the road, brace yourselves, marshal all your strength!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your enemy is coming to crush you, Nineveh. Man the ramparts! Watch the roads! Prepare your defenses! Call out your forces!"
      }
    },
