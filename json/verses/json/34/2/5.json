    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚述王召集将领， 他们一路跌跌撞撞， 奔上城墙，架起防御盾牌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞述王召集他的權臣， 他們跌跌撞撞而來， 急急忙忙走上城牆， 豎起了防禦的擋牌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "尼尼微王招聚他的貴冑； 他們步行絆跌， 速上城牆，預備擋牌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He remembers his nobles; They stumble in their march, They hurry to her wall, And the mantelet is set up."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nineveh summons her picked troops, yet they stumble on their way. They dash to the city wall; the protective shield is put in place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king shouts to his officers; they stumble in their haste, rushing to the walls to set up their defenses."
      }
    },
