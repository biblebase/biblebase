    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谕旨的抄本作为法令颁布到各省，通知各族为那天做好准备。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "諭文抄本頒行各省，通告各族，使他們準備好這一天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "抄錄這旨意，頒行各省，宣告各族，使他們預備等候那日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A copy of the edict to be issued as law in every province was published to all the peoples so that they should be ready for this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A copy of the text of the edict was to be issued as law in every province and made known to the people of every nationality so they would be ready for that day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A copy of this decree was to be issued as law in every province and proclaimed to all peoples, so that they would be ready to do their duty on the appointed day."
      }
    },
