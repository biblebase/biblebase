    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来，亚哈随鲁王擢升亚甲人哈米大他的儿子哈曼，使他的权位高过他所有的同僚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這事以後，亞哈隨魯王使亞甲族哈米大他的兒子哈曼晉陞；王提拔他，使他的地位高過所有與他在一起的大臣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這事以後，亞哈隨魯王擡舉亞甲族哈米大他的兒子哈曼，使他高升，叫他的爵位超過與他同事的一切臣宰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After these events King Ahasuerus promoted Haman, the son of Hammedatha the Agagite, and advanced him and established his authority over all the princes who were with him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After these events, King Xerxes honored Haman son of Hammedatha, the Agagite, elevating him and giving him a seat of honor higher than that of all the other nobles."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some time later King Xerxes promoted Haman son of Hammedatha the Agagite over all the other nobles, making him the most powerful official in the empire."
      }
    },
