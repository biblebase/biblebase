    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚比孩的女儿以斯帖王后和犹太人末底改以全权写第二封信，嘱咐犹太人守这普珥节，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞比孩的女兒王后以斯帖和猶大人末底改以全權寫了這第二封信，囑咐猶大人守這普珥節。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞比孩的女兒－王后以斯帖和猶大人末底改以全權寫第二封信，堅囑猶大人守這「普珥日」，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Queen Esther, daughter of Abihail, with Mordecai the Jew, wrote with full authority to confirm this second letter about Purim."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Queen Esther, daughter of Abihail, along with Mordecai the Jew, wrote with full authority to confirm this second letter concerning Purim."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Queen Esther, the daughter of Abihail, along with Mordecai the Jew, wrote another letter putting the queen’s full authority behind Mordecai’s letter to establish the Festival of Purim."
      }
    },
