    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王允准了，便在书珊城颁布谕旨，哈曼十个儿子的尸体便被吊了起来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王就下令這樣行；諭旨傳遍書珊，人就把哈曼的十個兒子掛在木架上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王便允准如此行。旨意傳在書珊，人就把哈曼十個兒子的屍首掛起來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the king commanded that it should be done so; and an edict was issued in Susa, and Haman’s ten sons were hanged."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the king commanded that this be done. An edict was issued in Susa, and they impaled the ten sons of Haman."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the king agreed, and the decree was announced in Susa. And they impaled the bodies of Haman’s ten sons."
      }
    },
