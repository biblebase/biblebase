    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴比伦王尼布甲尼撒曾从耶路撒冷掳走犹大王耶哥尼雅和百姓，末底改也在其中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "末底改在巴比倫王尼布甲尼撒擄走猶大王耶哥尼雅和別的俘虜的時候，也同時從耶路撒冷被擄走。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從前巴比倫王尼布甲尼撒將猶大王耶哥尼雅和百姓從耶路撒冷擄去，末底改也在其內。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "who had been taken into exile from Jerusalem with the captives who had been exiled with Jeconiah king of Judah, whom Nebuchadnezzar the king of Babylon had exiled."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who had been carried into exile from Jerusalem by Nebuchadnezzar king of Babylon, among those taken captive with Jehoiachin king of Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "His family had been among those who, with King Jehoiachin of Judah, had been exiled from Jerusalem to Babylon by King Nebuchadnezzar."
      }
    },
