    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "末底改知道后，便通知以斯帖王后，以斯帖就以末底改的名义禀告王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這陰謀給末底改知道了，他就告訴王后以斯帖；以斯帖就以末底改的名義對王說起這事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "末底改知道了，就告訴王后以斯帖。以斯帖奉末底改的名，報告於王；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the plot became known to Mordecai and he told Queen Esther, and Esther informed the king in Mordecai’s name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Mordecai found out about the plot and told Queen Esther, who in turn reported it to the king, giving credit to Mordecai."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Mordecai heard about the plot and gave the information to Queen Esther. She then told the king about it and gave Mordecai credit for the report."
      }
    },
