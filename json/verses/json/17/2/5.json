    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当时，书珊城中有个犹太人，名叫末底改，是便雅悯人基士的曾孙、示每的孙子、雅珥的儿子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在書珊城裡，有一個猶大人，名叫末底改，是便雅憫人基士的曾孫、示每的孫子、睚珥的兒子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "書珊城有一個猶大人，名叫末底改，是便雅憫人基士的曾孫，示每的孫子，睚珥的兒子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now there was at the citadel in Susa a Jew whose name was Mordecai, the son of Jair, the son of Shimei, the son of Kish, a Benjamite,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now there was in the citadel of Susa a Jew of the tribe of Benjamin, named Mordecai son of Jair, the son of Shimei, the son of Kish,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At that time there was a Jewish man in the fortress of Susa whose name was Mordecai son of Jair. He was from the tribe of Benjamin and was a descendant of Kish and Shimei."
      }
    },
