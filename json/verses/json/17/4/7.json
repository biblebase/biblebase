    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "末底改便将自己的遭遇及哈曼为灭绝犹太人而捐入国库的银子数目都告诉了他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "末底改把他遭遇的一切事，以及哈曼為滅盡猶大人答應捐銀給王庫的數目，都告訴了他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "末底改將自己所遇的事，並哈曼為滅絕猶大人應許捐入王庫的銀數都告訴了他；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Mordecai told him all that had happened to him, and the exact amount of money that Haman had promised to pay to the king’s treasuries for the destruction of the Jews."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Mordecai told him everything that had happened to him, including the exact amount of money Haman had promised to pay into the royal treasury for the destruction of the Jews."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Mordecai told him the whole story, including the exact amount of money Haman had promised to pay into the royal treasury for the destruction of the Jews."
      }
    },
