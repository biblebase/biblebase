    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王问他们：“瓦实提王后违抗太监所传的谕旨，按律该如何处置她？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王問他們說：“王后瓦實提不遵行亞哈隨魯王藉著太監所傳的命令，按照法令應怎樣辦理呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「王后瓦實提不遵太監所傳的王命，照例應當怎樣辦理呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“According to law, what is to be done with Queen Vashti, because she did not obey the command of King Ahasuerus delivered by the eunuchs?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“According to law, what must be done to Queen Vashti?” he asked. “She has not obeyed the command of King Xerxes that the eunuchs have taken to her.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What must be done to Queen Vashti?” the king demanded. “What penalty does the law provide for a queen who refuses to obey the king’s orders, properly sent through his eunuchs?”"
      }
    },
