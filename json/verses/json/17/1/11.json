    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "去请瓦实提王后戴着后冠到王面前，向百姓和官员展现她的美貌，因为她非常美丽。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "去請王后瓦實提，戴著后冕到王面前，使眾人和大臣欣賞她的美麗；因為她的容貌很美。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "請王后瓦實提頭戴王后的冠冕到王面前，使各等臣民看她的美貌，因為她容貌甚美。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "to bring Queen Vashti before the king with her royal crown in order to display her beauty to the people and the princes, for she was beautiful."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "to bring before him Queen Vashti, wearing her royal crown, in order to display her beauty to the people and nobles, for she was lovely to look at."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "to bring Queen Vashti to him with the royal crown on her head. He wanted the nobles and all the other men to gaze on her beauty, for she was a very beautiful woman."
      }
    },
