    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "之后，他又一连七天在御花园宴请书珊城里所有的居民，无论尊卑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些日子過了，王又為所有在書珊城的人民，無論尊卑大小，在御園的院子裡擺設筵席七天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這日子滿了，又為所有住書珊城的大小人民在御園的院子裏設擺筵席七日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When these days were completed, the king gave a banquet lasting seven days for all the people who were present at the citadel in Susa, from the greatest to the least, in the court of the garden of the king’s palace."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When these days were over, the king gave a banquet, lasting seven days, in the enclosed garden of the king’s palace, for all the people from the least to the greatest who were in the citadel of Susa."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When it was all over, the king gave a banquet for all the people, from the greatest to the least, who were in the fortress of Susa. It lasted for seven days and was held in the courtyard of the palace garden."
      }
    },
