      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "整整一百八十天，他展示自己帝国的财富和王权的威荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把自己尊榮之國的財富和偉大威風的榮華，向他們展現很多日子，共有一百八十天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他把他榮耀之國的豐富和他美好威嚴的尊貴給他們看了許多日，就是一百八十日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he displayed the riches of his royal glory and the splendor of his great majesty for many days, 180 days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For a full 180 days he displayed the vast wealth of his kingdom and the splendor and glory of his majesty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The celebration lasted 180 days—a tremendous display of the opulent wealth of his empire and the pomp and splendor of his majesty."
      }
