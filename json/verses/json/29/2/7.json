    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它们快速奔跑像勇士， 攀登城墙如战士， 有条不紊地前进。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們奔跑像勇士， 又像戰士爬上城牆； 他們各按自己的路前行， 並不偏離路線。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牠們如勇士奔跑， 像戰士爬城； 各都步行，不亂隊伍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They run like mighty men, They climb the wall like soldiers; And they each march in line, Nor do they deviate from their paths."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They charge like warriors; they scale walls like soldiers. They all march in line, not swerving from their course."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The attackers march like warriors and scale city walls like soldiers. Straight forward they march, never breaking rank."
      }
