    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "队伍整齐，各行其道， 势如破竹，无坚不摧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們彼此不擁擠， 各按自己的道前行； 甚至他們持著兵器跌倒， 也不會被剪除。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼此並不擁擠，向前各行其路， 直闖兵器，不偏左右。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They do not crowd each other, They march everyone in his path; When they burst through the defenses, They do not break ranks."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They do not jostle each other; each marches straight ahead. They plunge through defenses without breaking ranks."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They never jostle each other; each moves in exactly the right position. They break through defenses without missing a step."
      }
    },
