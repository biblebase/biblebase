    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，你们就知道我在以色列， 知道我是你们的上帝耶和华， 除我以外别无他神。 我的子民永远不会再蒙羞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，你們必知道我是在以色列中間， 又知道我是耶和華你們的　神，除了我再沒有別的神。 我的子民必永遠不會羞愧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們必知道我是在以色列中間， 又知道我是耶和華－你們的神； 在我以外並無別神。 我的百姓必永遠不致羞愧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus you will know that I am in the midst of Israel, And that I am the Lord your God, And there is no other; And My people will never be put to shame."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then you will know that I am in Israel, that I am the Lord your God, and that there is no other; never again will my people be shamed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then you will know that I am among my people Israel, that I am the Lord your God, and there is no other. Never again will my people be disgraced."
      }
    },
