    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有一群凶猛的蝗虫侵犯我的国土， 它们不计其数， 有雄狮的獠牙和母狮的利齿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為有一民族上來侵犯我的國土， 他們強大又無數； 他們的牙齒像獅子的牙齒， 他們有母獅的大牙。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一隊蝗蟲又強盛又無數， 侵犯我的地； 牠的牙齒如獅子的牙齒， 大牙如母獅的大牙。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For a nation has invaded my land, Mighty and without number; Its teeth are the teeth of a lion, And it has the fangs of a lioness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A nation has invaded my land, a mighty army without number; it has the teeth of a lion, the fangs of a lioness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A vast army of locusts has invaded my land, a terrible army too numerous to count. Its teeth are like lions’ teeth, its fangs like those of a lioness."
      }
    },
