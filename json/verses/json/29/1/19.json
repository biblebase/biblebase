    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，我向你呼求， 因为火焰吞灭了旷野的草场， 烈火烧光了田野的树木。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！我向你呼求， 因為火吞滅了曠野的草場， 火燄燒盡了田間所有的樹木。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我向你求告， 因為火燒滅曠野的草場； 火焰燒盡田野的樹木。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To You, O Lord, I cry; For fire has devoured the pastures of the wilderness And the flame has burned up all the trees of the field."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To you, Lord, I call, for fire has devoured the pastures in the wilderness and flames have burned up all the trees of the field."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, help us! The fire has consumed the wilderness pastures, and flames have burned up all the trees."
      }
    },
