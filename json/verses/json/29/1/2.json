    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "长老们啊，请听我说！ 地上的所有居民啊，要留心听！ 在你们或你们祖先的时代可曾发生过这样的事？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "老年人哪！你們要聽這話。 這地所有的居民啊！你們要留心聽。 在你們的日子， 或在你們祖先的日子， 曾有這樣的事嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "老年人哪，當聽我的話； 國中的居民哪，都要側耳而聽。 在你們的日子， 或你們列祖的日子， 曾有這樣的事嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hear this, O elders, And listen, all inhabitants of the land. Has anything like this happened in your days Or in your fathers’ days?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hear this, you elders; listen, all who live in the land. Has anything like this ever happened in your days or in the days of your ancestors?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Hear this, you leaders of the people. Listen, all who live in the land. In all your history, has anything like this happened before?"
      }
    },
