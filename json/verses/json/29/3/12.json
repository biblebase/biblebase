    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“万国都要起来， 上到约沙法谷受审判， 因为我要坐在那里审判他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“萬國都當奮起， 上到約沙法谷去。 因為我必坐在那裡， 審判四圍的列國。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬民都當興起， 上到約沙法谷； 因為我必坐在那裏， 審判四圍的列國。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let the nations be aroused And come up to the valley of Jehoshaphat, For there I will sit to judge All the surrounding nations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Let the nations be roused; let them advance into the Valley of Jehoshaphat, for there I will sit to judge all the nations on every side."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Let the nations be called to arms. Let them march to the valley of Jehoshaphat. There I, the Lord, will sit to pronounce judgment on them all."
      }
    },
