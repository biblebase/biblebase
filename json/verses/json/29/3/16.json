    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华从锡安发出怒吼， 从耶路撒冷发出雷声， 天地为之震动。 但耶和华却是祂子民的避难所， 是以色列人的堡垒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華從錫安吼叫， 並從耶路撒冷發聲， 天地就震動。 耶和華卻要作他子民的避難所， 作以色列眾子的保障。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華必從錫安吼叫， 從耶路撒冷發聲， 天地就震動。 耶和華卻要作他百姓的避難所， 作以色列人的保障。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord roars from Zion And utters His voice from Jerusalem, And the heavens and the earth tremble. But the Lord is a refuge for His people And a stronghold to the sons of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will roar from Zion and thunder from Jerusalem; the earth and the heavens will tremble. But the Lord will be a refuge for his people, a stronghold for the people of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord’s voice will roar from Zion and thunder from Jerusalem, and the heavens and the earth will shake. But the Lord will be a refuge for his people, a strong fortress for the people of Israel."
      }
    },
