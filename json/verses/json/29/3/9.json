    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们去向万国宣告： “要召集勇士，征调军队， 前去打仗！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要在列國中宣告以下這話： “你們要預備爭戰， 要激動勇士。 讓所有的戰士都近前來，讓他們都上去攻擊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當在萬民中宣告說： 要預備打仗； 激動勇士， 使一切戰士上前來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Proclaim this among the nations: Prepare a war; rouse the mighty men! Let all the soldiers draw near, let them come up!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Proclaim this among the nations: Prepare for war! Rouse the warriors! Let all the fighting men draw near and attack."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Say to the nations far and wide: “Get ready for war! Call out your best warriors. Let all your fighting men advance for the attack."
      }
    },
