    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后用香柏木、牛膝草、朱红色线和另一只活鸟蘸掺了清水的鸟血，向房子洒七次。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後拿香柏木、牛膝草、朱紅色線和那隻活鳥，都一同蘸在被宰殺的鳥血和活水中，向房屋彈七次。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "把香柏木、牛膝草、朱紅色線，並那活鳥，都蘸在被宰的鳥血中與活水中，用以灑房子七次。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he shall take the cedar wood and the hyssop and the scarlet string, with the live bird, and dip them in the blood of the slain bird as well as in the running water, and sprinkle the house seven times."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he is to take the cedar wood, the hyssop, the scarlet yarn and the live bird, dip them into the blood of the dead bird and the fresh water, and sprinkle the house seven times."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will take the cedar stick, the hyssop branch, the scarlet yarn, and the live bird, and dip them into the blood of the slaughtered bird and into the fresh water. Then he will sprinkle the house seven times."
      }
    },
