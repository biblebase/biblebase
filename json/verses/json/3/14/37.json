    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如发现墙壁上出现红色或绿色霉斑，并且侵入墙中，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他要察看那病症，如果發現在房屋的牆上有發綠或是發紅的腐蝕斑紋，而且有深透入牆的現象；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他要察看那災病，災病若在房子的牆上有發綠或發紅的凹斑紋，現象窪於牆，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he shall look at the mark, and if the mark on the walls of the house has greenish or reddish depressions and appears deeper than the surface,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He is to examine the mold on the walls, and if it has greenish or reddish depressions that appear to be deeper than the surface of the wall,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the priest will go in and examine the mildew on the walls. If he finds greenish or reddish streaks and the contamination appears to go deeper than the wall’s surface,"
      }
    },
