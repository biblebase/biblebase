    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "还要按自己的能力献两只斑鸠或雏鸽，一只作赎罪祭，一只作燔祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又要按他的經濟能力，拿兩隻斑鳩或是兩隻雛鴿，一隻作贖罪祭，一隻作燔祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又照他的力量取兩隻斑鳩或是兩隻雛鴿，一隻作贖罪祭，一隻作燔祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and two turtledoves or two young pigeons which are within his means, the one shall be a sin offering and the other a burnt offering."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and two doves or two young pigeons, such as they can afford, one for a sin offering and the other for a burnt offering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The offering must also include two turtledoves or two young pigeons, whichever the person can afford. One of the pair must be used for the sin offering and the other for a burnt offering."
      }
