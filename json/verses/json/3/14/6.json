    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后祭司要把另一只鸟，连同香柏木、朱红色线和牛膝草一起蘸在掺了清水的鸟血里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於那隻活鳥，祭司要把牠與香柏木、朱紅色線和牛膝草一起拿來，把這些物品和活鳥一同蘸於那在活水上宰殺的鳥血中，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於那隻活鳥，祭司要把牠和香柏木、朱紅色線並牛膝草一同蘸於宰在活水上的鳥血中，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As for the live bird, he shall take it together with the cedar wood and the scarlet string and the hyssop, and shall dip them and the live bird in the blood of the bird that was slain over the running water."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He is then to take the live bird and dip it, together with the cedar wood, the scarlet yarn and the hyssop, into the blood of the bird that was killed over the fresh water."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will take the live bird, the cedar stick, the scarlet yarn, and the hyssop branch, and dip them into the blood of the bird that was slaughtered over the fresh water."
      }
    },
