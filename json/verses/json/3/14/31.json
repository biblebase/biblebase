    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一只作赎罪祭，一只作燔祭。这样，祭司在耶和华面前为他赎了罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他要按著自己的經濟能力，獻上一隻作贖罪祭，一隻作燔祭，連同素祭一同獻上。祭司要在耶和華面前，為求潔淨的人贖罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是他所能辦的，一隻為贖罪祭，一隻為燔祭，與素祭一同獻上；祭司要在耶和華面前為他贖罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He shall offer what he can afford, the one for a sin offering and the other for a burnt offering, together with the grain offering. So the priest shall make atonement before the Lord on behalf of the one to be cleansed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "one as a sin offering and the other as a burnt offering, together with the grain offering. In this way the priest will make atonement before the Lord on behalf of the one to be cleansed.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One of them is for a sin offering and the other for a burnt offering, to be presented along with the grain offering. Through this process, the priest will purify the person before the Lord."
      }
    },
