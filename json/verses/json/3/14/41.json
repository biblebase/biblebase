    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后命人刮掉屋内墙壁上的灰泥，倒在城外不洁净的地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要刮淨房屋裡面的四周，把刮下來的灰泥倒在城外不潔淨的地方；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也要叫人刮房內的四圍，所刮掉的灰泥要倒在城外不潔淨之處；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He shall have the house scraped all around inside, and they shall dump the plaster that they scrape off at an unclean place outside the city."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He must have all the inside walls of the house scraped and the material that is scraped off dumped into an unclean place outside the town."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Next the inside walls of the entire house must be scraped thoroughly and the scrapings dumped in the unclean place outside the town."
      }
    },
