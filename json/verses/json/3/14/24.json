    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司要用作赎过祭的羊羔和一杯油作摇祭，在耶和华面前摇一摇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司要取那隻作贖愆祭的公羊羔和那三分之一公升油；祭司把這些，在耶和華面前搖一搖當作搖祭；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司要把贖愆祭的羊羔和那一羅革油一同作搖祭，在耶和華面前搖一搖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The priest shall take the lamb of the guilt offering and the log of oil, and the priest shall offer them for a wave offering before the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priest is to take the lamb for the guilt offering, together with the log of oil, and wave them before the Lord as a wave offering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The priest will take the lamb for the guilt offering, along with the olive oil, and lift them up as a special offering to the Lord."
      }
    },
