    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司进去检查前，要吩咐人把房子腾空，免得房子里的一切都变得不洁净。之后，祭司要进去检查，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司進去察看病症之前，要先吩咐人把房屋搬空，免得房屋裡的一切都成了不潔；然後祭司才進去，察看那房屋。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司還沒有進去察看災病以前，就要吩咐人把房子騰空，免得房子裏所有的都成了不潔淨；然後祭司要進去察看房子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The priest shall then command that they empty the house before the priest goes in to look at the mark, so that everything in the house need not become unclean; and afterward the priest shall go in to look at the house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priest is to order the house to be emptied before he goes in to examine the mold, so that nothing in the house will be pronounced unclean. After this the priest is to go in and inspect the house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Before the priest goes in to inspect the house, he must have the house emptied so nothing inside will be pronounced ceremonially unclean."
      }
    },
