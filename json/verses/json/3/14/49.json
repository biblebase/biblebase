    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为洁净房子，祭司要取两只鸟、香柏木、朱红色线和牛膝草，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為了潔淨那房屋，祭司要拿兩隻鳥、香柏木、朱紅色線和牛膝草；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要為潔淨房子取兩隻鳥和香柏木、朱紅色線並牛膝草，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To cleanse the house then, he shall take two birds and cedar wood and a scarlet string and hyssop,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To purify the house he is to take two birds and some cedar wood, scarlet yarn and hyssop."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "To purify the house the priest must take two birds, a stick of cedar, some scarlet yarn, and a hyssop branch."
      }
    },
