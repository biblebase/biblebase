      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司要将祭牲的血洒在会幕门口耶和华的祭坛上，把脂肪焚烧献给耶和华作馨香之祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司要把血潑在會幕門口耶和華的祭壇上，把脂肪焚燒，獻給耶和華作馨香的祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司要把血灑在會幕門口、耶和華的壇上，把脂油焚燒，獻給耶和華為馨香的祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The priest shall sprinkle the blood on the altar of the Lord at the doorway of the tent of meeting, and offer up the fat in smoke as a soothing aroma to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priest is to splash the blood against the altar of the Lord at the entrance to the tent of meeting and burn the fat as an aroma pleasing to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the priest will be able to splatter the blood against the Lord’s altar at the entrance of the Tabernacle, and he will burn the fat as a pleasing aroma to the Lord."
      }
    },
