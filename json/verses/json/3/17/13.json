    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人或寄居在以色列人中间的外族人如果捕到可吃的鸟或兽，必须放尽它的血，用土掩埋，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "任何以色列人，或是在你們中間寄居的外人中，有人打獵得了可以吃的禽獸，總要放盡牠的血，用泥土掩蓋。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡以色列人，或是寄居在他們中間的外人，若打獵得了可吃的禽獸，必放出牠的血來，用土掩蓋。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So when any man from the sons of Israel, or from the aliens who sojourn among them, in hunting catches a beast or a bird which may be eaten, he shall pour out its blood and cover it with earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Any Israelite or any foreigner residing among you who hunts any animal or bird that may be eaten must drain out the blood and cover it with earth,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And if any native Israelite or foreigner living among you goes hunting and kills an animal or bird that is approved for eating, he must drain its blood and cover it with earth."
      }
    },
