    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不论是以色列人，还是寄居在以色列人中间的外族人，献燔祭或其他祭物时，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要對他們說：以色列家的任何人，或是在你們中間寄居的外人，獻燔祭或是平安祭的時候，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你要曉諭他們說：凡以色列家中的人，或是寄居在他們中間的外人，獻燔祭或是平安祭，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Then you shall say to them, ‘Any man from the house of Israel, or from the aliens who sojourn among them, who offers a burnt offering or sacrifice,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Say to them: ‘Any Israelite or any foreigner residing among them who offers a burnt offering or sacrifice"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Give them this command as well. If any native Israelite or foreigner living among you offers a burnt offering or a sacrifice"
      }
    },
