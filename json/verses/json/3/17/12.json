    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以耶和华吩咐以色列人和寄居在以色列人中间的外族人不可吃血。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以我對以色列人說：你們中間任何人都不可吃血，在你們中間寄居的外人也不可吃血。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，我對以色列人說：你們都不可吃血；寄居在你們中間的外人也不可吃血。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore I said to the sons of Israel, ‘No person among you may eat blood, nor may any alien who sojourns among you eat blood.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore I say to the Israelites, “None of you may eat blood, nor may any foreigner residing among you eat blood.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That is why I have said to the people of Israel, ‘You must never eat or drink blood—neither you nor the foreigners living among you.’"
      }
    },
