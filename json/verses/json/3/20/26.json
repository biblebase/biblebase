      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus you are to be holy to Me, for I the Lord am holy; and I have set you apart from the peoples to be Mine."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are to be holy to me because I, the Lord, am holy, and I have set you apart from the nations to be my own."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must be holy because I, the Lord, am holy. I have set you apart from all other people to be my very own."
      }
