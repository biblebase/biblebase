    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“咒骂父母的，必被处死。他咒骂父母，必须承担自己的罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“咒罵自己父母的，必要把他處死；他咒罵自己的父母，必須承擔流血的罪責。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡咒罵父母的，總要治死他；他咒罵了父母，他的罪要歸到他身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘If there is anyone who curses his father or his mother, he shall surely be put to death; he has cursed his father or his mother, his bloodguiltiness is upon him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Anyone who curses their father or mother is to be put to death. Because they have cursed their father or mother, their blood will be on their own head."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Anyone who dishonors father or mother must be put to death. Such a person is guilty of a capital offense."
      }
