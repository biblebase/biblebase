    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果那地方的人对此置之不理，没有处死那人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果有人把他的兒子獻給摩洛，而本地的人掩目不看，不把他處死，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那人把兒女獻給摩洛，本地人若佯為不見，不把他治死，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If the people of the land, however, should ever disregard that man when he gives any of his offspring to Molech, so as not to put him to death,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If the members of the community close their eyes when that man sacrifices one of his children to Molek and if they fail to put him to death,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And if the people of the community ignore those who offer their children to Molech and refuse to execute them,"
      }
    },
