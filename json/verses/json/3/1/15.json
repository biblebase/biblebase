    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司要把鸟带到祭坛前，拧下鸟头，放在祭坛上焚烧；要在祭坛旁放尽鸟血；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司要把鳥帶到祭壇那裡，扭下牠的頭來，焚燒在祭壇上；又把鳥血擠出來，流在祭壇的壁上；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司要把鳥拿到壇前，揪下頭來，把鳥燒在壇上；鳥的血要流在壇的旁邊；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The priest shall bring it to the altar, and wring off its head and offer it up in smoke on the altar; and its blood is to be drained out on the side of the altar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priest shall bring it to the altar, wring off the head and burn it on the altar; its blood shall be drained out on the side of the altar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The priest will take the bird to the altar, wring off its head, and burn it on the altar. But first he must drain its blood against the side of the altar."
      }
    },
