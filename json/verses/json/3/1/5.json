    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他要在耶和华面前宰牛，然后祭司——亚伦的子孙要把牛血洒在会幕门口的祭坛四周。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他要在耶和華面前宰殺那公牛，亞倫子孫作祭司的，要奉上血，潑在會幕門口祭壇的四周。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他要在耶和華面前宰公牛；亞倫子孫作祭司的，要奉上血，把血灑在會幕門口、壇的周圍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He shall slay the young bull before the Lord; and Aaron’s sons the priests shall offer up the blood and sprinkle the blood around on the altar that is at the doorway of the tent of meeting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are to slaughter the young bull before the Lord, and then Aaron’s sons the priests shall bring the blood and splash it against the sides of the altar at the entrance to the tent of meeting."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then slaughter the young bull in the Lord’s presence, and Aaron’s sons, the priests, will present the animal’s blood by splattering it against all sides of the altar that stands at the entrance to the Tabernacle."
      }
    },
