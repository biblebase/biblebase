    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你把以下条例告诉以色列人。 “如果你们有人献祭给耶和华，要用牛羊作祭物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要告訴以色列人說：如果你們中間有人把供物獻給耶和華，就要從牛群羊群中獻家畜為供物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你曉諭以色列人說：你們中間若有人獻供物給耶和華，要從牛羣羊羣中獻牲畜為供物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Speak to the sons of Israel and say to them, ‘When any man of you brings an offering to the Lord, you shall bring your offering of animals from the herd or the flock."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Speak to the Israelites and say to them: ‘When anyone among you brings an offering to the Lord, bring as your offering an animal from either the herd or the flock."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Give the following instructions to the people of Israel. When you present an animal as an offering to the Lord, you may take it from your herd of cattle or your flock of sheep and goats."
      }
    },
