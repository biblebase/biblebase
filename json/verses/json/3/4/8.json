    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他要取出作赎罪祭的公牛犊的所有脂肪，包括遮盖内脏的脂肪和内脏上的脂肪、"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他要取出贖罪祭的公牛所有的脂肪，就是遮蓋內臟的脂肪和內臟上的一切脂肪，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要把贖罪祭公牛所有的脂油，乃是蓋臟的脂油和臟上所有的脂油，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He shall remove from it all the fat of the bull of the sin offering: the fat that covers the entrails, and all the fat which is on the entrails,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He shall remove all the fat from the bull of the sin offering—all the fat that is connected to the internal organs,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the priest must remove all the fat of the bull to be offered as a sin offering. This includes all the fat around the internal organs,"
      }
    },
