      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司要用手指蘸一些羊血抹在燔祭坛的角上，剩下的血要倒在燔祭坛脚旁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司要用指頭取一些贖罪祭牲的血，抹燔祭壇的四角，其餘的羊血，都要倒在祭壇腳那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司要用指頭蘸些贖罪祭牲的血，抹在燔祭壇的四角上，所有的血都要倒在壇的腳那裏，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The priest is to take some of the blood of the sin offering with his finger and put it on the horns of the altar of burnt offering, and all the rest of its blood he shall pour out at the base of the altar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the priest shall take some of the blood of the sin offering with his finger and put it on the horns of the altar of burnt offering and pour out the rest of the blood at the base of the altar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the priest will dip his finger in the blood of the sin offering and put it on the horns of the altar for burnt offerings. He will pour out the rest of the blood at the base of the altar."
      }
    },
