    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“刚出生的牛、绵羊或山羊，前七天要与母亲在一起。从第八天起，可以把它们献给我作火祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“公牛或是綿羊或是山羊生下來以後，七天要與牠的母親在一起，從第八天開始，牠就可以蒙悅納，作獻給耶和華火祭的供物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「才生的公牛，或是綿羊或是山羊，七天當跟着母；從第八天以後，可以當供物蒙悅納，作為耶和華的火祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“When an ox or a sheep or a goat is born, it shall remain seven days with its mother, and from the eighth day on it shall be accepted as a sacrifice of an offering by fire to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When a calf, a lamb or a goat is born, it is to remain with its mother for seven days. From the eighth day on, it will be acceptable as a food offering presented to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When a calf or lamb or goat is born, it must be left with its mother for seven days. From the eighth day on, it will be acceptable as a special gift to the Lord."
      }
    },
