    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可献给耶和华睪丸受损、被压碎、破裂或被阉割的牲畜。在你们境内不可做这样的事，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "內臟損傷、壓碎、破裂，或割掉的，你們都不可獻與耶和華；在你們將來居住的地方，你們不可行這樣的事；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "腎子損傷的，或是壓碎的，或是破裂的，或是騸了的，不可獻給耶和華，在你們的地上也不可這樣行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Also anything with its testicles bruised or crushed or torn or cut, you shall not offer to the Lord, or sacrifice in your land,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You must not offer to the Lord an animal whose testicles are bruised, crushed, torn or cut. You must not do this in your own land,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If an animal has damaged testicles or is castrated, you may not offer it to the Lord. You must never do this in your own land,"
      }
