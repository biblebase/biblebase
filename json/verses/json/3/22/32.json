    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可亵渎我的圣名，我要在以色列人中彰显我的圣洁，我是使你们圣洁的耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不可褻瀆我的聖名，我在以色列人中間要被尊為聖；我是使你們分別為聖的耶和華，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們不可褻瀆我的聖名；我在以色列人中，卻要被尊為聖。我是叫你們成聖的耶和華，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall not profane My holy name, but I will be sanctified among the sons of Israel; I am the Lord who sanctifies you,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not profane my holy name, for I must be acknowledged as holy by the Israelites. I am the Lord, who made you holy"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not bring shame on my holy name, for I will display my holiness among the people of Israel. I am the Lord who makes you holy."
      }
    },
