    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但安息年间土地里自生自长的，你们和你们的仆婢、雇工与住在你们中间的外族人都可以吃，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "地在安息年的自然出產，是給你們作食物的；就是給你，你的僕婢、雇工和寄居的，就是與你們一起居住的人；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "地在安息年所出的，要給你和你的僕人、婢女、雇工人，並寄居的外人當食物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All of you shall have the sabbath products of the land for food; yourself, and your male and female slaves, and your hired man and your foreign resident, those who live as aliens with you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whatever the land yields during the sabbath year will be food for you—for yourself, your male and female servants, and the hired worker and temporary resident who live among you,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you may eat whatever the land produces on its own during its Sabbath. This applies to you, your male and female servants, your hired workers, and the temporary residents who live with you."
      }
    },
