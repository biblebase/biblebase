    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为以色列人是我的仆人，是我从埃及领出来的仆人。我是你们的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為以色列人都是屬我的僕人；他們是我的僕人，是我把他們從埃及地領了出來的；我是耶和華你們的　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為以色列人都是我的僕人，是我從埃及地領出來的。我是耶和華－你們的神。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the sons of Israel are My servants; they are My servants whom I brought out from the land of Egypt. I am the Lord your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for the Israelites belong to me as servants. They are my servants, whom I brought out of Egypt. I am the Lord your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the people of Israel belong to me. They are my servants, whom I brought out of the land of Egypt. I am the Lord your God."
      }
