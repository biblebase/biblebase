    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“如果有人卖掉自己城里的房子，要保留一年赎回权。卖掉房子的一年之内，他可以赎回。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果有人賣了一所城內的住宅，在賣了以後的一整年之內，他有贖回的權利；在這些日子以內，他隨時可贖回。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人若賣城內的住宅，賣了以後，一年之內可以贖回；在一整年，必有贖回的權柄。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Likewise, if a man sells a dwelling house in a walled city, then his redemption right remains valid until a full year from its sale; his right of redemption lasts a full year."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Anyone who sells a house in a walled city retains the right of redemption a full year after its sale. During that time the seller may redeem it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Anyone who sells a house inside a walled town has the right to buy it back for a full year after its sale. During that year, the seller retains the right to buy it back."
      }
    },
