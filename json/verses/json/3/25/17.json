    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要敬畏你们的上帝，不可彼此亏负。我是你们的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不可欺負對方，卻要敬畏你的　神，因為我是耶和華你們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們彼此不可虧負，只要敬畏你們的神，因為我是耶和華－你們的神。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So you shall not wrong one another, but you shall fear your God; for I am the Lord your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not take advantage of each other, but fear your God. I am the Lord your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Show your fear of God by not taking advantage of each other. I am the Lord your God."
      }
