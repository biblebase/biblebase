    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为以色列人是我的仆人，是我从埃及带出来的，所以他们不可卖身为奴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們是我的僕人，是我從埃及地領出來的，他們不可以賣作奴僕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他們是我的僕人，是我從埃及地領出來的，不可賣為奴僕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they are My servants whom I brought out from the land of Egypt; they are not to be sold in a slave sale."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because the Israelites are my servants, whom I brought out of Egypt, they must not be sold as slaves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people of Israel are my servants, whom I brought out of the land of Egypt, so they must never be sold as slaves."
      }
    },
