    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到了禧年，他和孩子们便可以离开你们，回到自己的宗族和祖业。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了禧年，他和他的兒女要離開你，歸回他本家，歸回自己祖宗的地業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了禧年，他和他兒女要離開你，一同出去歸回本家，到他祖宗的地業那裏去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He shall then go out from you, he and his sons with him, and shall go back to his family, that he may return to the property of his forefathers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they and their children are to be released, and they will go back to their own clans and to the property of their ancestors."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At that time he and his children will no longer be obligated to you, and they will return to their clans and go back to the land originally allotted to their ancestors."
      }
    },
