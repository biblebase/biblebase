    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是你们的上帝耶和华。我曾经带领你们离开埃及，为要把迦南赐给你们，并做你们的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我是耶和華你們的　神，曾經把你們從埃及地領出來，要把迦南地賜給你們，而且要作你們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我是耶和華－你們的神，曾領你們從埃及地出來，為要把迦南地賜給你們，要作你們的神。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am the Lord your God, who brought you out of the land of Egypt to give you the land of Canaan and to be your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am the Lord your God, who brought you out of Egypt to give you the land of Canaan and to be your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am the Lord your God, who brought you out of the land of Egypt to give you the land of Canaan and to be your God."
      }
    },
