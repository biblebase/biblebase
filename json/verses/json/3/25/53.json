    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "买主要待他如按年雇佣的工人。你们要确保买主不会苛待他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "買主要像每年雇用的工人那樣待他。不可在你眼前苛刻地管轄他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他和買主同住，要像每年雇的工人，買主不可嚴嚴地轄管他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like a man hired year by year he shall be with him; he shall not rule over him with severity in your sight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are to be treated as workers hired from year to year; you must see to it that those to whom they owe service do not rule over them ruthlessly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The foreigner must treat them as workers hired on a yearly basis. You must not allow a foreigner to treat any of your fellow Israelites harshly."
      }
    },
