    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "距下个禧年的年数多，价钱就高；年数少，价钱就低。因为卖的是田地收成的次数。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "年數多，你付的買價就要增多。年數少，你付的買價就要減少。因為他是按出產的數值賣給你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "年歲若多，要照數加添價值；年歲若少，要照數減去價值，因為他照收成的數目賣給你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In proportion to the extent of the years you shall increase its price, and in proportion to the fewness of the years you shall diminish its price, for it is a number of crops he is selling to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the years are many, you are to increase the price, and when the years are few, you are to decrease the price, because what is really being sold to you is the number of crops."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The more years until the next jubilee, the higher the price; the fewer years, the lower the price. After all, the person selling the land is actually selling you a certain number of harvests."
      }
    },
