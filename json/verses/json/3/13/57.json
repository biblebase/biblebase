    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以后衣物、编织品或皮革上若再次出现霉斑，便是旧患复发，要把它烧掉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果病症再出現在剩下的衣服、編結物、紡織物或任何皮子做的物件上，那就是病症仍然發作，染了病症的物件，要用火燒掉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若仍現在衣服上，或是經上、緯上、皮子做的甚麼物件上，這就是災病又發了，必用火焚燒那染災病的物件。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and if it appears again in the garment, whether in the warp or in the woof, or in any article of leather, it is an outbreak; the article with the mark shall be burned in the fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if it reappears in the fabric, in the woven or knitted material, or in the leather article, it is a spreading mold; whatever has the mold must be burned."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If the spot later reappears on the clothing, the fabric, or the leather article, the mildew is clearly spreading, and the contaminated object must be burned up."
      }
    },
