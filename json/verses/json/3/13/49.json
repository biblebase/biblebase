    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只要发现霉斑是绿色或红色的，那就是霉变，要拿给祭司检查。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "病症若是在衣服或皮子、編結物或紡織物，或任何皮子做成的器具上發綠或發紅，那就是發霉的現象，要給祭司察看。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "或在衣服上、皮子上，經上、緯上，或在皮子做的甚麼物件上，這災病若是發綠，或是發紅，是大痲瘋的災病，要給祭司察看。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "if the mark is greenish or reddish in the garment or in the leather, or in the warp or in the woof, or in any article of leather, it is a leprous mark and shall be shown to the priest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "if the affected area in the fabric, the leather, the woven or knitted material, or any leather article, is greenish or reddish, it is a defiling mold and must be shown to the priest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If the contaminated area in the clothing, the animal hide, the fabric, or the leather article has turned greenish or reddish, it is contaminated with mildew and must be shown to the priest."
      }
    },
