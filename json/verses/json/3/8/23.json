    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西宰了羊，将一些羊血抹在亚伦的右耳垂、右手大拇指和右脚大脚趾上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西宰殺了羊，取了一些羊血，抹在亞倫的右耳垂上和右手的大拇指上，以及右腳的大拇趾上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就宰了羊。摩西把些血抹在亞倫的右耳垂上和右手的大拇指上，並右腳的大拇指上，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moses slaughtered it and took some of its blood and put it on the lobe of Aaron’s right ear, and on the thumb of his right hand and on the big toe of his right foot."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moses slaughtered the ram and took some of its blood and put it on the lobe of Aaron’s right ear, on the thumb of his right hand and on the big toe of his right foot."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and Moses slaughtered it. Then Moses took some of its blood and applied it to the lobe of Aaron’s right ear, the thumb of his right hand, and the big toe of his right foot."
      }
    },
