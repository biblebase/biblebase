    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又照耶和华的吩咐，把公牛的皮、肉和粪便带到营外烧掉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於那公牛，牠的皮、肉和糞，都要在營外用火燒掉，都是照著耶和華吩咐摩西的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟有公牛，連皮帶肉並糞，用火燒在營外，都是照耶和華所吩咐摩西的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the bull and its hide and its flesh and its refuse he burned in the fire outside the camp, just as the Lord had commanded Moses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the bull with its hide and its flesh and its intestines he burned up outside the camp, as the Lord commanded Moses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He took the rest of the bull, including its hide, meat, and dung, and burned it on a fire outside the camp, just as the Lord had commanded him."
      }
    },
