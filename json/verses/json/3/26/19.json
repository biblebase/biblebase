    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要重挫你们的傲气，使你们头上的天如铁，脚下的地如铜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必粉碎你們誇耀的力量；我要使你們的天像鐵，你們的地像銅；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必斷絕你們因勢力而有的驕傲，又要使覆你們的天如鐵，載你們的地如銅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will also break down your pride of power; I will also make your sky like iron and your earth like bronze."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will break down your stubborn pride and make the sky above you like iron and the ground beneath you like bronze."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will break your proud spirit by making the skies as unyielding as iron and the earth as hard as bronze."
      }
    },
