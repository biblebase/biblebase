    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“如果你们受过这些惩罚，仍然不知悔改，我要把比先前严厉七倍的惩罚加在你们身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "經過這些以後，如果你們還不聽從我，我就要因你們的罪加重七倍管教你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們因這些事若還不聽從我，我就要為你們的罪加七倍懲罰你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If also after these things you do not obey Me, then I will punish you seven times more for your sins."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘If after all this you will not listen to me, I will punish you for your sins seven times over."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And if, in spite of all this, you still disobey me, I will punish you seven times over for your sins."
      }
    },
