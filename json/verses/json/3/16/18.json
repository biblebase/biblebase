    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "之后，他要来到耶和华面前的祭坛那里，为祭坛赎罪。他要取一些牛血和羊血，抹在祭坛的四角上，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他出來，就到耶和華面前的祭壇那裡，為壇贖罪。他要取一點公牛的血和山羊的血，抹祭壇的四角。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他出來，要到耶和華面前的壇那裏，在壇上行贖罪之禮，又要取些公牛的血和公山羊的血，抹在壇上四角的周圍；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he shall go out to the altar that is before the Lord and make atonement for it, and shall take some of the blood of the bull and of the blood of the goat and put it on the horns of the altar on all sides."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Then he shall come out to the altar that is before the Lord and make atonement for it. He shall take some of the bull’s blood and some of the goat’s blood and put it on all the horns of the altar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then Aaron will come out to purify the altar that stands before the Lord. He will do this by taking some of the blood from the bull and the goat and putting it on each of the horns of the altar."
      }
    },
