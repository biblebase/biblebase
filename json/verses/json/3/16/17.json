    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从他进入圣所为自己全家和以色列全体会众赎罪开始，直到他赎完罪出来为止，任何人都不可待在会幕里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他進入至聖所贖罪的時候，會幕裡不可有人；直到他為自己和家人，以及以色列全體會眾，贖了罪出來為止。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他進聖所贖罪的時候，會幕裏不可有人，直等到他為自己和本家並以色列全會眾贖了罪出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he goes in to make atonement in the holy place, no one shall be in the tent of meeting until he comes out, that he may make atonement for himself and for his household and for all the assembly of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one is to be in the tent of meeting from the time Aaron goes in to make atonement in the Most Holy Place until he comes out, having made atonement for himself, his household and the whole community of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No one else is allowed inside the Tabernacle when Aaron enters it for the purification ceremony in the Most Holy Place. No one may enter until he comes out again after purifying himself, his family, and all the congregation of Israel, making them right with the Lord."
      }
    },
