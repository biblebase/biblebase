    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他要在耶和华面前烧香，使香的烟笼罩约柜上的施恩座，免得他死亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在耶和華面前把香放在火上，使香的煙雲遮掩約櫃上的施恩座，免得他死亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在耶和華面前，把香放在火上，使香的煙雲遮掩法櫃上的施恩座，免得他死亡；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He shall put the incense on the fire before the Lord, that the cloud of incense may cover the mercy seat that is on the ark of the testimony, otherwise he will die."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He is to put the incense on the fire before the Lord, and the smoke of the incense will conceal the atonement cover above the tablets of the covenant law, so that he will not die."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There in the Lord’s presence he will put the incense on the burning coals so that a cloud of incense will rise over the Ark’s cover—the place of atonement—that rests on the Ark of the Covenant. If he follows these instructions, he will not die."
      }
    },
