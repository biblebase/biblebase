    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“女人若在经期以外多日血漏或经期过长，在此期间便不洁净，像在经期内一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“女人若是在月經污穢期以外，血漏多日；或是月經污穢期過長，患有血漏症；她在這些血漏的日子是不潔淨的，好像她在月經污穢的日子不潔淨一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「女人若在經期以外患多日的血漏，或是經期過長，有了漏症，她就因這漏症不潔淨，與她在經期不潔淨一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Now if a woman has a discharge of her blood many days, not at the period of her menstrual impurity, or if she has a discharge beyond that period, all the days of her impure discharge she shall continue as though in her menstrual impurity; she is unclean."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘When a woman has a discharge of blood for many days at a time other than her monthly period or has a discharge that continues beyond her period, she will be unclean as long as she has the discharge, just as in the days of her period."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If a woman has a flow of blood for many days that is unrelated to her menstrual period, or if the blood continues beyond the normal period, she is ceremonially unclean. As during her menstrual period, the woman will be unclean as long as the discharge continues."
      }
    },
