      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and whoever sits on the thing on which the man with the discharge has been sitting, shall wash his clothes and bathe in water and be unclean until evening."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever sits on anything that the man with a discharge sat on must wash their clothes and bathe with water, and they will be unclean till evening."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you sit where the man with the discharge has sat, you must wash your clothes and bathe yourself in water, and you will remain unclean until evening."
      }
    },
