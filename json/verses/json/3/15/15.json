    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一只作赎罪祭，一只作燔祭。这样，祭司便在耶和华面前为他的漏症赎了罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司要把牠們獻上，一隻作贖罪祭，一隻作燔祭；祭司因那人的異常排泄，要在耶和華面前為他贖罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司要獻上一隻為贖罪祭，一隻為燔祭；因那人患的漏症，祭司要在耶和華面前為他贖罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and the priest shall offer them, one for a sin offering and the other for a burnt offering. So the priest shall make atonement on his behalf before the Lord because of his discharge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priest is to sacrifice them, the one for a sin offering and the other for a burnt offering. In this way he will make atonement before the Lord for the man because of his discharge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The priest will offer one bird for a sin offering and the other for a burnt offering. Through this process, the priest will purify the man before the Lord for his discharge."
      }
    },
