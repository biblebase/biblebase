      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "月经期间，她躺过或坐过的东西都不洁净。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在她污穢的時期，她躺過的東西，都不潔淨；她坐過的東西，也都不潔淨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "女人在污穢之中，凡她所躺的物件都為不潔淨，所坐的物件也都不潔淨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Everything also on which she lies during her menstrual impurity shall be unclean, and everything on which she sits shall be unclean."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Anything she lies on during her period will be unclean, and anything she sits on will be unclean."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anything on which the woman lies or sits during the time of her period will be unclean."
      }
