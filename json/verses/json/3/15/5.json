      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Anyone, moreover, who touches his bed shall wash his clothes and bathe in water and be unclean until evening;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who touches his bed must wash their clothes and bathe with water, and they will be unclean till evening."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So if you touch the man’s bed, you must wash your clothes and bathe yourself in water, and you will remain unclean until evening."
      }
