    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "男人若与她同房，沾染了她的经血，就要不洁净七天，他躺过的床也不洁净。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "男人若是與那女人同睡，染上她的污穢，就不潔淨七天；他躺過的床都不潔淨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "男人若與那女人同房，染了她的污穢，就要七天不潔淨；所躺的牀也為不潔淨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If a man actually lies with her so that her menstrual impurity is on him, he shall be unclean seven days, and every bed on which he lies shall be unclean."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘If a man has sexual relations with her and her monthly flow touches him, he will be unclean for seven days; any bed he lies on will be unclean."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If a man has sexual intercourse with her and her blood touches him, her menstrual impurity will be transmitted to him. He will remain unclean for seven days, and any bed on which he lies will be unclean."
      }
    },
