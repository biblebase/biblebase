    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是耶和华，我带你们离开埃及，为要做你们的上帝。你们要圣洁，因为我是圣洁的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為我是耶和華，曾把你們從埃及地領上來，為要作你們的　神；你們要分別為聖，因為我是聖潔的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我是把你們從埃及地領出來的耶和華，要作你們的神；所以你們要聖潔，因為我是聖潔的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I am the Lord who brought you up from the land of Egypt to be your God; thus you shall be holy, for I am holy.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am the Lord, who brought you up out of Egypt to be your God; therefore be holy, because I am holy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I, the Lord, am the one who brought you up from the land of Egypt, that I might be your God. Therefore, you must be holy because I am holy."
      }
    },
