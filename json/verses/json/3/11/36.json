    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果它们的尸体掉进水泉或储水池里，水泉和储水池仍然是洁净的，但碰到尸体的人是不洁净的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是水源和儲水池仍算是潔淨的。只有觸摸水中屍體的，才是不潔淨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但是泉源或是聚水的池子仍是潔淨；惟挨了那死的，就不潔淨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nevertheless a spring or a cistern collecting water shall be clean, though the one who touches their carcass shall be unclean."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A spring, however, or a cistern for collecting water remains clean, but anyone who touches one of these carcasses is unclean."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“However, if the carcass of such an animal falls into a spring or a cistern, the water will still be clean. But anyone who touches the carcass will be defiled."
      }
    },
