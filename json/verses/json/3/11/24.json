    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以下的动物会使你们不洁净。任何人接触到它们的尸体，都不洁净，要等到傍晚才能洁净。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“以下事物，會使你們成為不潔：觸摸動物屍體的，就不潔淨到晚上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「這些都能使你們不潔淨。凡摸了死的，必不潔淨到晚上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘By these, moreover, you will be made unclean: whoever touches their carcasses becomes unclean until evening,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘You will make yourselves unclean by these; whoever touches their carcasses will be unclean till evening."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The following creatures will make you ceremonially unclean. If any of you touch their carcasses, you will be defiled until evening."
      }
    },
