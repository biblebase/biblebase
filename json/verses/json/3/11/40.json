    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "任何人如果吃它的肉，或拿它的尸体，都必须洗净衣服，等到傍晚才能洁净。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "誰吃了牠的屍體，就要洗淨自己的衣服，並且不潔淨到晚上；誰拾起這屍體，也要洗淨自己的衣服，並且不潔淨到晚上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人吃那死了的走獸，必不潔淨到晚上，並要洗衣服；拿了死走獸的，必不潔淨到晚上，並要洗衣服。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He too, who eats some of its carcass shall wash his clothes and be unclean until evening, and the one who picks up its carcass shall wash his clothes and be unclean until evening."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who eats some of its carcass must wash their clothes, and they will be unclean till evening. Anyone who picks up the carcass must wash their clothes, and they will be unclean till evening."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you eat any of its meat or carry away its carcass, you must wash your clothes, and you will remain defiled until evening."
      }
    },
