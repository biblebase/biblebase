    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "除了早晨的燔祭，亚伦又取来素祭，从中拿了一把献在坛上焚烧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又獻上素祭，從其中取了滿滿的一把，在祭壇上焚燒。這是在早晨的燔祭以外的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又奉上素祭，從其中取一滿把，燒在壇上；這是在早晨的燔祭以外。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Next he presented the grain offering, and filled his hand with some of it and offered it up in smoke on the altar, besides the burnt offering of the morning."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He also brought the grain offering, took a handful of it and burned it on the altar in addition to the morning’s burnt offering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He also presented the grain offering, burning a handful of the flour mixture on the altar, in addition to the regular burnt offering for the morning."
      }
    },
