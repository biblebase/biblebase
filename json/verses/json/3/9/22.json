    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦献完赎罪祭、燔祭和平安祭，举手为民众祝福后，走下祭坛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫向眾人舉起手，為他們祝福；他獻了贖罪祭、燔祭和平安祭，就下來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫向百姓舉手，為他們祝福。他獻了贖罪祭、燔祭、平安祭就下來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Aaron lifted up his hands toward the people and blessed them, and he stepped down after making the sin offering and the burnt offering and the peace offerings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Aaron lifted his hands toward the people and blessed them. And having sacrificed the sin offering, the burnt offering and the fellowship offering, he stepped down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After that, Aaron raised his hands toward the people and blessed them. Then, after presenting the sin offering, the burnt offering, and the peace offering, he stepped down from the altar."
      }
    },
