      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦宰了燔祭祭牲，他儿子们把祭牲的血递给他洒在祭坛四周。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫宰殺了燔祭牲，他的兒子把血遞給他，潑在祭壇的周圍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫宰了燔祭牲，他兒子把血遞給他，他就灑在壇的周圍，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he slaughtered the burnt offering; and Aaron’s sons handed the blood to him and he sprinkled it around on the altar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he slaughtered the burnt offering. His sons handed him the blood, and he splashed it against the sides of the altar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Next Aaron slaughtered the animal for the burnt offering. His sons brought him the blood, and he splattered it against all sides of the altar."
      }
    },
