    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "献祭者要从平安祭中献上火祭给耶和华。他要取出遮盖内脏的脂肪和内脏上的脂肪、"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他要從平安祭中奉上獻給耶和華的火祭，就是把遮蓋內臟的脂肪，和內臟上的一切脂肪，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從平安祭中，將火祭獻給耶和華，也要把蓋臟的脂油和臟上所有的脂油，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the sacrifice of the peace offerings he shall present an offering by fire to the Lord, the fat that covers the entrails and all the fat that is on the entrails,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the fellowship offering you are to bring a food offering to the Lord: the internal organs and all the fat that is connected to them,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The priest must present part of this peace offering as a special gift to the Lord. This includes all the fat around the internal organs,"
      }
    },
