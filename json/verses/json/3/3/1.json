    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“如果有人用牛献作平安祭，必须在耶和华面前献上毫无残疾的公牛或母牛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人的供物若是作平安祭，如果獻的是牛，必須在耶和華面前獻一頭沒有殘疾的公牛或母牛；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人獻供物為平安祭，若是從牛羣中獻，無論是公的是母的，必用沒有殘疾的獻在耶和華面前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Now if his offering is a sacrifice of peace offerings, if he is going to offer out of the herd, whether male or female, he shall offer it without defect before the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘If your offering is a fellowship offering, and you offer an animal from the herd, whether male or female, you are to present before the Lord an animal without defect."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If you present an animal from the herd as a peace offering to the Lord, it may be a male or a female, but it must have no defects."
      }
    },
