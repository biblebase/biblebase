    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司要把这一切放在祭坛燃烧的木柴上和燔祭一起焚烧，作为蒙耶和华悦纳的馨香火祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫的子孫要把這些放在祭壇炭火上的木柴上面，在燔祭上焚燒，作獻給耶和華馨香的火祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫的子孫要把這些燒在壇的燔祭上，就是在火的柴上，是獻與耶和華為馨香的火祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Aaron’s sons shall offer it up in smoke on the altar on the burnt offering, which is on the wood that is on the fire; it is an offering by fire of a soothing aroma to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Aaron’s sons are to burn it on the altar on top of the burnt offering that is lying on the burning wood; it is a food offering, an aroma pleasing to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and Aaron’s sons will burn them on top of the burnt offering on the wood burning on the altar. It is a special gift, a pleasing aroma to the Lord."
      }
    },
