    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那人要自担罪责，因为他亵渎了我的圣物。要将他从民中铲除。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那吃祭肉的人，要擔當自己的罪責，因為他褻瀆了耶和華的聖物；那人必從自己的族人中被剪除。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡吃的人必擔當他的罪孽；因為他褻瀆了耶和華的聖物，那人必從民中剪除。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Everyone who eats it will bear his iniquity, for he has profaned the holy thing of the Lord; and that person shall be cut off from his people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever eats it will be held responsible because they have desecrated what is holy to the Lord; they must be cut off from their people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who eats it on the third day will be punished for defiling what is holy to the Lord and will be cut off from the community."
      }
    },
