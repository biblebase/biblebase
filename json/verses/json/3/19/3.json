    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们每个人都必须孝敬父母，遵守我的安息日。我是你们的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "各人要孝敬父母，也要謹守我的安息日；我是耶和華你們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們各人都當孝敬父母，也要守我的安息日。我是耶和華－你們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every one of you shall reverence his mother and his father, and you shall keep My sabbaths; I am the Lord your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Each of you must respect your mother and father, and you must observe my Sabbaths. I am the Lord your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Each of you must show great respect for your mother and father, and you must always observe my Sabbath days of rest. I am the Lord your God."
      }
    },
