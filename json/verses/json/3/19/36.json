    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要使用准确的秤、尺子和升斗。我是你们的上帝耶和华，曾带领你们离开埃及。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要用公正的天平，公正的法碼，公正的升斗，公正的容器；我是耶和華你們的　神，就是曾把你們從埃及地領出來的那位。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要用公道天平、公道法碼、公道升斗、公道秤。我是耶和華－你們的神，曾把你們從埃及地領出來的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall have just balances, just weights, a just ephah, and a just hin; I am the Lord your God, who brought you out from the land of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Use honest scales and honest weights, an honest ephah and an honest hin. I am the Lord your God, who brought you out of Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your scales and weights must be accurate. Your containers for measuring dry materials or liquids must be accurate. I am the Lord your God who brought you out of the land of Egypt."
      }
    },
