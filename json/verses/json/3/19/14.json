      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可咒骂聋子，也不可绊倒盲人，应当敬畏你们的上帝。我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不可咒罵聾子，也不可把絆腳石放在瞎子面前；卻要敬畏你的　神；我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可咒罵聾子，也不可將絆腳石放在瞎子面前，只要敬畏你的神。我是耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not curse a deaf man, nor place a stumbling block before the blind, but you shall revere your God; I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Do not curse the deaf or put a stumbling block in front of the blind, but fear your God. I am the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do not insult the deaf or cause the blind to stumble. You must fear your God; I am the Lord."
      }
