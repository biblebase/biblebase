        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“在白髮的人面前，你要起立；要尊敬老年人，又要敬畏你的　神；我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「在白髮的人面前，你要站起來；也要尊敬老人，又要敬畏你的神。我是耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘You shall rise up before the grayheaded and honor the aged, and you shall revere your God; I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Stand up in the presence of the aged, show respect for the elderly and revere your God. I am the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Stand up in the presence of the elderly, and show respect for the aged. Fear your God. I am the Lord."
      }
