    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对同胞不可心中怀恨。同胞有错，要当面指正，免得自己因他而担罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你不可心裡恨你的兄弟；應坦誠責備你的鄰舍，免得你因他擔當罪過。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「不可心裏恨你的弟兄；總要指摘你的鄰舍，免得因他擔罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘You shall not hate your fellow countryman in your heart; you may surely reprove your neighbor, but shall not incur sin because of him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Do not hate a fellow Israelite in your heart. Rebuke your neighbor frankly so you will not share in their guilt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do not nurse hatred in your heart for any of your relatives. Confront people directly so you will not be held guilty for their sin."
      }
