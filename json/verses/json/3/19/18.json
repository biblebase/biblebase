    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可报复，不可埋怨同胞，要爱邻如己。我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不可報復，也不可向你的族人懷恨，卻要愛你的鄰舍好像愛自己；我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可報仇，也不可埋怨你本國的子民，卻要愛人如己。我是耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not take vengeance, nor bear any grudge against the sons of your people, but you shall love your neighbor as yourself; I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Do not seek revenge or bear a grudge against anyone among your people, but love your neighbor as yourself. I am the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do not seek revenge or bear a grudge against a fellow Israelite, but love your neighbor as yourself. I am the Lord."
      }
    },
