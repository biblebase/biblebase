    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要摘净葡萄园里的葡萄，也不要捡掉在地上的葡萄。要把这些留给穷人和寄居在你们中间的外族人。我是你们的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不可把你葡萄園的果子都摘盡，也不可拾取你葡萄園中掉下的；要把它們留給窮人和寄居的外人；我是耶和華你們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可摘盡葡萄園的果子，也不可拾取葡萄園所掉的果子；要留給窮人和寄居的。我是耶和華－你們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nor shall you glean your vineyard, nor shall you gather the fallen fruit of your vineyard; you shall leave them for the needy and for the stranger. I am the Lord your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not go over your vineyard a second time or pick up the grapes that have fallen. Leave them for the poor and the foreigner. I am the Lord your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is the same with your grape crop—do not strip every last bunch of grapes from the vines, and do not pick up the grapes that fall to the ground. Leave them for the poor and the foreigners living among you. I am the Lord your God."
      }
    },
