    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以圣所的秤为准，要为二十至六十岁的男子估价五百五十克银子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你應該這樣估價：從二十歲到六十歲的男人，你要按著聖所衡量銀子的標準，估價五百五十克銀子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你估定的，從二十歲到六十歲的男人，要按聖所的平，估定價銀五十舍客勒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If your valuation is of the male from twenty years even to sixty years old, then your valuation shall be fifty shekels of silver, after the shekel of the sanctuary."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "set the value of a male between the ages of twenty and sixty at fifty shekels of silver, according to the sanctuary shekel;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "here is the scale of values to be used. A man between the ages of twenty and sixty is valued at fifty shekels of silver, as measured by the sanctuary shekel."
      }
    },
