    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“如果有人许愿献牲畜给耶和华作供物，所献的牲畜就会成为圣物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果所許的是可以獻給耶和華為供物的牲畜，獻給耶和華的任何牲畜，都要分別為聖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「所許的若是牲畜，就是人獻給耶和華為供物的，凡這一類獻給耶和華的，都要成為聖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Now if it is an animal of the kind which men can present as an offering to the Lord, any such that one gives to the Lord shall be holy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘If what they vowed is an animal that is acceptable as an offering to the Lord, such an animal given to the Lord becomes holy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If your vow involves giving an animal that is acceptable as an offering to the Lord, any gift to the Lord will be considered holy."
      }
    },
