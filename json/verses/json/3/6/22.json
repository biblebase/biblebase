    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦子孙中继任大祭司的，受膏时要献上素祭。这种祭物属于耶和华，必须全部烧掉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫的子孫繼承他作受膏立的祭司的，都要獻這素祭，把它完全焚燒，獻給耶和華，作永遠的律例。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫的子孫中，接續他為受膏的祭司，要把這素祭獻上，要全燒給耶和華。這是永遠的定例。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The anointed priest who will be in his place among his sons shall offer it. By a permanent ordinance it shall be entirely offered up in smoke to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The son who is to succeed him as anointed priest shall prepare it. It is the Lord’s perpetual share and is to be burned completely."
      },
