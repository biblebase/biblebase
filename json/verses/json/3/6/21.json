      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要把这些细面粉调上油，用煎锅做饼，切成块献上，作为蒙耶和华悦纳的馨香之祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要把這些細麵在煎盤上用油調和作餅，烘好了，你就拿進來；將擘成碎塊的素祭獻上，獻給耶和華為馨香的祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要在鐵鏊上用油調和做成，調勻了，你就拿進來；烤好了分成塊子，獻給耶和華為馨香的素祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It shall be prepared with oil on a griddle. When it is well stirred, you shall bring it. You shall present the grain offering in baked pieces as a soothing aroma to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It must be prepared with oil on a griddle; bring it well-mixed and present the grain offering broken in pieces as an aroma pleasing to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It must be carefully mixed with olive oil and cooked on a griddle. Then slice this grain offering and present it as a pleasing aroma to the Lord."
      }
    },
