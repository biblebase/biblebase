    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他要从羊群中选一只毫无残疾的公绵羊，或把同等价值的银子带到祭司那里，献给耶和华作赎过祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "還要照著你的估價，把贖愆祭牲，就是從羊群中取一隻沒有殘疾的公綿羊，牽到耶和華面前，給祭司作贖愆祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也要照你所估定的價，把贖愆祭牲－就是羊羣中一隻沒有殘疾的公綿羊－牽到耶和華面前，給祭司為贖愆祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he shall bring to the priest his guilt offering to the Lord, a ram without defect from the flock, according to your valuation, for a guilt offering,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And as a penalty they must bring to the priest, that is, to the Lord, their guilt offering, a ram from the flock, one without defect and of the proper value."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As a guilt offering to the Lord, you must bring to the priest your own ram with no defects, or you may buy one of equal value."
      }
    },
