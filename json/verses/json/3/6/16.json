      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦及其子孙要吃剩下的祭物，要在会幕院子里的圣洁之处做无酵饼吃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫和他的子孫要吃剩餘的祭物，在聖潔的地方作無酵餅吃；他們要在會幕的院子裡吃。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所剩下的，亞倫和他子孫要吃，必在聖處不帶酵而吃，要在會幕的院子裏吃。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "What is left of it Aaron and his sons are to eat. It shall be eaten as unleavened cakes in a holy place; they are to eat it in the court of the tent of meeting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Aaron and his sons shall eat the rest of it, but it is to be eaten without yeast in the sanctuary area; they are to eat it in the courtyard of the tent of meeting."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Aaron and his sons may eat the rest of the flour, but it must be baked without yeast and eaten in a sacred place within the courtyard of the Tabernacle."
      }
    },
