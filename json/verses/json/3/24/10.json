    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有一个人，母亲是以色列人，父亲是埃及人，在营中与一个以色列人打起架来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一個以色列婦人的兒子，父親是埃及人，在以色列人中間出入；這以色列婦人的兒子在營裡和一個以色列人爭吵；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一個以色列婦人的兒子，他父親是埃及人，一日閒遊在以色列人中。這以色列婦人的兒子和一個以色列人在營裏爭鬥。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the son of an Israelite woman, whose father was an Egyptian, went out among the sons of Israel; and the Israelite woman’s son and a man of Israel struggled with each other in the camp."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the son of an Israelite mother and an Egyptian father went out among the Israelites, and a fight broke out in the camp between him and an Israelite."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One day a man who had an Israelite mother and an Egyptian father came out of his tent and got into a fight with one of the Israelite men."
      }
    },
