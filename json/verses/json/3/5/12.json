    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他要把细面粉带到祭司那里，祭司要从中取出一把作为象征放在坛上，与献给耶和华的火祭一起焚烧。这是赎罪祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他要把供物帶到祭司那裡，祭司從其中取出一把，作為象徵，放在祭壇耶和華的火祭上面焚燒；這是贖罪祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他要把供物帶到祭司那裏，祭司要取出自己的一把來作為紀念，按獻給耶和華火祭的條例燒在壇上；這是贖罪祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He shall bring it to the priest, and the priest shall take his handful of it as its memorial portion and offer it up in smoke on the altar, with the offerings of the Lord by fire: it is a sin offering."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are to bring it to the priest, who shall take a handful of it as a memorial portion and burn it on the altar on top of the food offerings presented to the Lord. It is a sin offering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Take the flour to the priest, who will scoop out a handful as a representative portion. He will burn it on the altar on top of the special gifts presented to the Lord. It is an offering for sin."
      }
    },
