    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要视祭司为圣洁的，因为他们负责献你们上帝的食物。要视他们为圣洁的，因为使你们圣洁的我——耶和华是圣洁的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以你要使他分別為聖，因為他把食物獻給你的　神；你要以他為聖，因為我耶和華使你們分別為聖的，是聖潔的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以你要使他成聖，因為他奉獻你神的食物；你要以他為聖，因為我－使你們成聖的耶和華－是聖的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall consecrate him, therefore, for he offers the food of your God; he shall be holy to you; for I the Lord, who sanctifies you, am holy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Regard them as holy, because they offer up the food of your God. Consider them holy, because I the Lord am holy—I who make you holy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must treat them as holy because they offer up food to your God. You must consider them holy because I, the Lord, am holy, and I make you holy."
      }
    },
