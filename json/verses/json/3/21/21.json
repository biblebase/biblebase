    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦祭司子孙中有残疾的都不可向耶和华献火祭。他们既然有残疾，就不可近前来献我的食物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫祭司的後裔中，有殘疾的，都不可上前來獻耶和華的火祭；他既有殘疾，就不可上前來獻他的　神的食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司亞倫的後裔，凡有殘疾的，都不可近前來，將火祭獻給耶和華。他有殘疾，不可近前來獻神的食物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No man among the descendants of Aaron the priest who has a defect is to come near to offer the Lord’s offerings by fire; since he has a defect, he shall not come near to offer the food of his God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No descendant of Aaron the priest who has any defect is to come near to present the food offerings to the Lord. He has a defect; he must not come near to offer the food of his God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No descendant of Aaron who has a defect may approach the altar to present special gifts to the Lord. Since he has a defect, he may not approach the altar to offer food to his God."
      }
    },
