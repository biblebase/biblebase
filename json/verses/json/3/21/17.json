    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你把以下条例告诉亚伦。 “你的子孙中，任何有残疾的人都不可前来献他上帝的食物，这是世世代代都要遵守的律例。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要告訴亞倫：你世世代代的後裔中，有殘疾的，就不可走近來獻他　神的食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你告訴亞倫說：你世世代代的後裔，凡有殘疾的，都不可近前來獻他神的食物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Speak to Aaron, saying, ‘No man of your offspring throughout their generations who has a defect shall approach to offer the food of his God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Say to Aaron: ‘For the generations to come none of your descendants who has a defect may come near to offer the food of his God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Give the following instructions to Aaron: In all future generations, none of your descendants who has any defect will qualify to offer food to his God."
      }
    },
