      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not uncover the nakedness of a woman and of her daughter, nor shall you take her son’s daughter or her daughter’s daughter, to uncover her nakedness; they are blood relatives. It is lewdness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Do not have sexual relations with both a woman and her daughter. Do not have sexual relations with either her son’s daughter or her daughter’s daughter; they are her close relatives. That is wickedness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do not have sexual relations with both a woman and her daughter. And do not take her granddaughter, whether her son’s daughter or her daughter’s daughter, and have sexual relations with her. They are close relatives, and this would be a wicked act."
      }
