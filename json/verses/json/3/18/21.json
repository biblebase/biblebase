      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可把儿女当作祭物献给假神摩洛，不可亵渎你上帝的名。我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不可把你的子孫獻給摩洛，也不可褻瀆你　神的名；我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可使你的兒女經火歸與摩洛，也不可褻瀆你神的名。我是耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not give any of your offspring to offer them to Molech, nor shall you profane the name of your God; I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Do not give any of your children to be sacrificed to Molek, for you must not profane the name of your God. I am the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do not permit any of your children to be offered as a sacrifice to Molech, for you must not bring shame on the name of your God. I am the Lord."
      }
    },
