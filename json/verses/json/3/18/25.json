    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们居住的土地也都被玷污了，所以我因他们的罪恶而惩罚那片土地，我要使那片土地吐出他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "連地也被玷污，所以我追討那地的罪孽，那地就把居民吐出去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "連地也玷污了，所以我追討那地的罪孽，那地也吐出它的居民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the land has become defiled, therefore I have brought its punishment upon it, so the land has spewed out its inhabitants."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even the land was defiled; so I punished it for its sin, and the land vomited out its inhabitants."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because the entire land has become defiled, I am punishing the people who live there. I will cause the land to vomit them out."
      }
    },
