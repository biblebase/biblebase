    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果是自愿献的或为了还愿而献的祭物，要在当天吃祭肉，剩下的可以留到第二天吃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他獻的供物若是還願祭，或是甘心祭，獻祭那日可以吃；剩下的，第二天也可以吃。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若所獻的是為還願，或是甘心獻的，必在獻祭的日子吃，所剩下的第二天也可以吃。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But if the sacrifice of his offering is a votive or a freewill offering, it shall be eaten on the day that he offers his sacrifice, and on the next day what is left of it may be eaten;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘If, however, their offering is the result of a vow or is a freewill offering, the sacrifice shall be eaten on the day they offer it, but anything left over may be eaten on the next day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If you bring an offering to fulfill a vow or as a voluntary offering, the meat must be eaten on the same day the sacrifice is offered, but whatever is left over may be eaten on the second day."
      }
    },
