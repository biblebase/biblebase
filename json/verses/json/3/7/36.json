    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司受膏之日，耶和华就命令以色列人把这部分祭肉分给他们，这是他们世世代代应得之份。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就是耶和華膏立他們的時候，命令以色列人給他們的。這是世世代代永遠的律例。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是在摩西膏他們的日子，耶和華吩咐以色列人給他們的。這是他們世世代代永得的分。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These the Lord had commanded to be given them from the sons of Israel in the day that He anointed them. It is their due forever throughout their generations.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the day they were anointed, the Lord commanded that the Israelites give this to them as their perpetual share for the generations to come."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On the day they were anointed, the Lord commanded the Israelites to give these portions to the priests as their permanent share from generation to generation.”"
      }
    },
