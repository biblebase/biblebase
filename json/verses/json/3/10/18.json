    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "既然这祭牲的血没有被带进圣所，你们本应按照我的吩咐在圣洁之处吃这祭肉。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這祭牲的血既然沒有帶進聖所裡去，你們就應該照著我所吩咐的，在聖所裡吃這祭肉。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，這祭牲的血並沒有拿到聖所裏去，你們本當照我所吩咐的，在聖所裏吃這祭肉。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, since its blood had not been brought inside, into the sanctuary, you should certainly have eaten it in the sanctuary, just as I commanded.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Since its blood was not taken into the Holy Place, you should have eaten the goat in the sanctuary area, as I commanded.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Since the animal’s blood was not brought into the Holy Place, you should have eaten the meat in the sacred area as I ordered you.”"
      }
    },
