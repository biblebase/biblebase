    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "近来，你们像仇敌一样起来攻击我的子民。 你们剥去那些如从战场归来一样毫无戒备的过路人的外衣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們卻像仇敵一樣，起來攻擊我的子民； 你們竟從安詳過路的人身上剝去衣服， 從不願作戰的人身上剝去外衣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "然而，近來我的民興起如仇敵， 從那些安然經過不願打仗之人身上剝去外衣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Recently My people have arisen as an enemy— You strip the robe off the garment From unsuspecting passers-by, From those returned from war."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lately my people have risen up like an enemy. You strip off the rich robe from those who pass by without a care, like men returning from battle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet to this very hour my people rise against me like an enemy! You steal the shirts right off the backs of those who trusted you, making them as ragged as men returning from battle."
      }
