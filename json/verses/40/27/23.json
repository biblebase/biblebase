    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼拉多问：“为什么？祂犯了什么罪？” 他们却更大声地喊叫：“把祂钉在十字架上！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼拉多說：“為甚麼呢？他作了甚麼惡事呢？”眾人更加大聲喊叫：“把他釘十字架！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巡撫說：「為甚麼呢？他做了甚麼惡事呢？」他們便極力地喊着說：「把他釘十字架！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he said, “Why, what evil has He done?” But they kept shouting all the more, saying, “Crucify Him!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Why? What crime has he committed?” asked Pilate. But they shouted all the louder, “Crucify him!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why?” Pilate demanded. “What crime has he committed?” But the mob roared even louder, “Crucify him!”"
      }
    },
