    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我会清楚地告诉他们，‘我从来不认识你们，你们这些作恶的人走开！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但我必向他們聲明：‘我從來不認識你們；你們這些作惡的人，離開我去吧！’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我就明明地告訴他們說：『我從來不認識你們，你們這些作惡的人，離開我去吧！』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And then I will declare to them, ‘I never knew you; depart from Me, you who practice lawlessness.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I will tell them plainly, ‘I never knew you. Away from me, you evildoers!’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I will reply, ‘I never knew you. Get away from me, you who break God’s laws.’"
      }
