      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "What I tell you in the darkness, speak in the light; and what you hear whispered in your ear, proclaim upon the housetops."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What I tell you in the dark, speak in the daylight; what is whispered in your ear, proclaim from the roofs."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What I tell you now in the darkness, shout abroad when daybreak comes. What I whisper in your ear, shout from the housetops for all to hear!"
      }
