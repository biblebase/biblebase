    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要因我的缘故被带到官长和君王面前，在他们和外族人面前为我做见证。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們為我的緣故，也要被帶到統治者和君王面前，向他們和外族人作見證。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且你們要為我的緣故被送到諸侯君王面前，對他們和外邦人作見證。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and you will even be brought before governors and kings for My sake, as a testimony to them and to the Gentiles."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On my account you will be brought before governors and kings as witnesses to them and to the Gentiles."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will stand trial before governors and kings because you are my followers. But this will be your opportunity to tell the rulers and other unbelievers about me."
      }
    },
