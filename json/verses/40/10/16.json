    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“听着，我差你们出去，就好像使羊走进狼群一般。所以，你们要像蛇一样机灵，像鸽子一样驯良。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“現在，我差派你們出去，好像羊進到狼群中間；所以你們要像蛇一樣機警，像鴿子一樣純潔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我差你們去，如同羊進入狼羣；所以你們要靈巧像蛇，馴良像鴿子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, I send you out as sheep in the midst of wolves; so be shrewd as serpents and innocent as doves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I am sending you out like sheep among wolves. Therefore be as shrewd as snakes and as innocent as doves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Look, I am sending you out as sheep among wolves. So be as shrewd as snakes and harmless as doves."
      }
