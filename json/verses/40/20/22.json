    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣回答说：“你们不知道自己在求什么。我要喝的那一杯，你们能喝吗？” 他们说：“我们能。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌回答：“你們不知道你們求的是甚麼。我將要喝的杯，你們能喝嗎？”他們說：“能。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌回答說：「你們不知道所求的是甚麼；我將要喝的杯，你們能喝嗎？」他們說：「我們能。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Jesus answered, “You do not know what you are asking. Are you able to drink the cup that I am about to drink?” They *said to Him, “We are able.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You don’t know what you are asking,” Jesus said to them. “Can you drink the cup I am going to drink?” “We can,” they answered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jesus answered by saying to them, “You don’t know what you are asking! Are you able to drink from the bitter cup of suffering I am about to drink?” “Oh yes,” they replied, “we are able!”"
      }
