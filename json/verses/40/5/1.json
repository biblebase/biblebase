    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣看见这些人群，就上了山，刚坐下，门徒便走到祂跟前，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌看見群眾，就上了山；他坐下之後，門徒來到他跟前，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌看見這許多的人，就上了山，既已坐下，門徒到他跟前來，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Jesus saw the crowds, He went up on the mountain; and after He sat down, His disciples came to Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now when Jesus saw the crowds, he went up on a mountainside and sat down. His disciples came to him,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One day as he saw the crowds gathering, Jesus went up on the mountainside and sat down. His disciples gathered around him,"
      }
