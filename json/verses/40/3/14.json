      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约翰想要拦住祂，就说：“应该是你为我施洗才对，你反倒来找我？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約翰想要阻止他，說：“我應該受你的洗，你卻到我這裡來嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約翰想要攔住他，說：「我當受你的洗，你反倒上我這裏來嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But John tried to prevent Him, saying, “I have need to be baptized by You, and do You come to me?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But John tried to deter him, saying, “I need to be baptized by you, and do you come to me?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But John tried to talk him out of it. “I am the one who needs to be baptized by you,” he said, “so why are you coming to me?”"
      }
