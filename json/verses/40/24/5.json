    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为将来会有许多人冒我的名而来，说，‘我是基督’欺骗许多人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為許多人要假冒我的名而來，說：‘我就是基督’，並且要迷惑許多的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為將來有好些人冒我的名來，說：『我是基督』，並且要迷惑許多人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For many will come in My name, saying, ‘I am the Christ,’ and will mislead many."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For many will come in my name, claiming, ‘I am the Messiah,’ and will deceive many."
      },
