    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣进了房子，那两个瞎子来到祂面前。耶稣问他们：“你们相信我能做这事吗？”他们说：“主啊，我们相信。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌進了房子，他們來到他那裡。耶穌問他們：“你們信我能作這些事嗎？”他們回答：“主啊，我們信。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌進了房子，瞎子就來到他跟前。耶穌說：「你們信我能做這事嗎？」他們說：「主啊，我們信。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He entered the house, the blind men came up to Him, and Jesus *said to them, “Do you believe that I am able to do this?” They *said to Him, “Yes, Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he had gone indoors, the blind men came to him, and he asked them, “Do you believe that I am able to do this?” “Yes, Lord,” they replied."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They went right into the house where he was staying, and Jesus asked them, “Do you believe I can make you see?” “Yes, Lord,” they told him, “we do.”"
      }
    },
