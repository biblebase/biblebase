    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "非利士人召集大军与以色列人交战。亚吉对大卫说：“你应该明白，你和部下要随我一起出战。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，非利士人聚集他們的軍隊，要與以色列人爭戰。亞吉對大衛說：“你要清楚知道，你和跟隨你的人都要在軍隊中與我一同出征。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，非利士人聚集軍旅，要與以色列人打仗。亞吉對大衛說：「你當知道，你和跟隨你的人都要隨我出戰。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "In those days the Philistines gathered their forces for war, to fight against Israel. And Achish said to David, “Understand that you and your men are to go out with me in the army.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now it came about in those days that the Philistines gathered their armed camps for war, to fight against Israel. And Achish said to David, “Know assuredly that you will go out with me in the camp, you and your men.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In those days the Philistines gathered their forces to fight against Israel. Achish said to David, “You must understand that you and your men will accompany me in the army.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now it happened in those days that the Philistines gathered their armies together for war, to fight with Israel. And Achish said to David, “You assuredly know that you will go out with me to battle, you and your men.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "About that time the Philistines mustered their armies for another war with Israel. King Achish told David, “You and your men will be expected to join me in battle.”"
      }
    },
