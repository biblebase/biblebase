    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.2.11-36",
            "paragraphs": [
              "iii.撒母耳面對示羅的敗落（二11～36）"
            ]
          },
          {
            "verse": "1sa.2.11",
            "paragraphs": [
              "11.那孩子……事奉的耶和華：動詞「事奉」（希伯來：šāraṯ）多出現在家庭與政治的背景中，但最常用來指聖所中的服事；撒母耳以祭司的身分事奉，但到他達到法定年齡之前，仍是在以利的指導之下。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利加拿返回了拉玛，小撒母耳却留在了示罗，在祭司以利的带领下事奉耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "後來，以利加拿回拉瑪自己的家去。那孩子卻留在以利祭司面前事奉耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利加拿往拉瑪回家去了。那孩子在祭司以利面前事奉耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Elkanah went to his home at Ramah. But the boy ministered to the Lord before Eli the priest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Elkanah went home to Ramah, but the boy ministered before the Lord under Eli the priest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Elkanah returned home to Ramah without Samuel. And the boy served the Lord by assisting Eli the priest."
      }
    },
