      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now then, O king, come down according to all the desire of your soul to do so; and our part shall be to surrender him into the king’s hand.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now, Your Majesty, come down whenever it pleases you to do so, and we will be responsible for giving him into your hands.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Come down whenever you’re ready, O king, and we will catch him and hand him over to you!”"
      }
