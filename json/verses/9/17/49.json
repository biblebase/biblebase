    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.17.48-49",
            "paragraphs": [
              "48～49.那非利士人幾乎還沒有時間朝大衛移動，就已經被大衛的一顆石子擊倒了；大衛甩出的這顆石子無比準確，正好就貫穿歌利亞精心設計的軍裝上惟一的弱點。決鬥在一瞬間就退出了。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他从袋里拿出一块石子用投石器猛力抛去，石子击中歌利亚的额头，嵌了进去，他迎面栽倒在地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛伸手入口袋裡，從裡面拿出了一塊石子來，用機弦甩去，擊中了那非利士人的前額；石子穿入前額裡面，他就仆倒，面伏於地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛用手從囊中掏出一塊石子來，用機弦甩去，打中非利士人的額，石子進入額內，他就仆倒，面伏於地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And David put his hand into his bag and took from it a stone and slung it, and struck the Philistine on his forehead. And the stone sank into his forehead, so that he fell on his face to the ground."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Reaching into his bag and taking out a stone, he slung it and struck the Philistine on the forehead. The stone sank into his forehead, and he fell facedown on the ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Reaching into his shepherd’s bag and taking out a stone, he hurled it with his sling and hit the Philistine in the forehead. The stone sank in, and Goliath stumbled and fell face down on the ground."
      }
    },
