    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.17.55-58",
            "paragraphs": [
              "55～58.掃羅既然許諾要將女兒給以色列人的鬥士，就很想知道準女婿的家庭。讀者由於得到許多特別挑選的資料，會詫異何以掃羅和押尼珥都不知道這位鬥士是誰。在這件事之後，這家人再也不會輕易為人遺忘了。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗问他：“年轻人，你是谁的儿子？”大卫答道：“我是你仆人伯利恒人耶西的儿子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅問他：“年輕人啊，你是誰的兒子？”大衛回答：“我是你僕人伯利恆人耶西的兒子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅問他說：「少年人哪，你是誰的兒子？」大衛說：「我是你僕人伯利恆人耶西的兒子。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Saul said to him, “Whose son are you, young man?” And David answered, “I am the son of your servant Jesse the Bethlehemite.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Whose son are you, young man?” Saul asked him. David said, “I am the son of your servant Jesse of Bethlehem.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Tell me about your father, young man,” Saul said. And David replied, “His name is Jesse, and we live in Bethlehem.”"
      }
