    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.17.55-58",
            "paragraphs": [
              "55～58.掃羅既然許諾要將女兒給以色列人的鬥士，就很想知道準女婿的家庭。讀者由於得到許多特別挑選的資料，會詫異何以掃羅和押尼珥都不知道這位鬥士是誰。在這件事之後，這家人再也不會輕易為人遺忘了。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗王说：“你去查查他是谁的儿子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王說：“你去問問這個少年是誰的兒子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王說：「你可以問問那幼年人是誰的兒子。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king said, “You inquire whose son the youth is.”"
      },
