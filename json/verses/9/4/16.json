    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.4.16",
            "paragraphs": [
              "16.報信之人一看出以利是瞎眼的，就說明自己是從戰場上來的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "报信的人对以利说：“我今天是从战场逃回来的。”以利问：“我的孩子，情况怎么样？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那人告訴以利：“我是從戰場上來的，今天從戰場上逃回來。”以利問：“我兒，事情怎麼樣？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那人對以利說：「我是從陣上來的，今日我從陣上逃回。」以利說：「我兒，事情怎樣？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The man said to Eli, “I am the one who came from the battle line. Indeed, I escaped from the battle line today.” And he said, “How did things go, my son?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He told Eli, “I have just come from the battle line; I fled from it this very day.” Eli asked, “What happened, my son?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He said to Eli, “I have just come from the battlefield—I was there this very day.” “What happened, my son?” Eli demanded."
      }
    },
