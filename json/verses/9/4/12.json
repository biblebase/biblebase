    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.4.12",
            "paragraphs": [
              "12.從西弗到示羅整整二十哩（34公裏）；就著這條路線看來，主要都是上坡路，直到山上的鄉村，這個逃跑的人必須要有非常好的體魄，才能在當日走完這段路程。他蓬頭垢發的樣子說明他正在舉哀，一眼就可以看出他帶來的是壞消息。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "1sa.4.12",
            "paragraphs": [
              "四12撕裂衣服和頭蒙灰塵，是為死人哀悼或為國難哀傷的表現。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当天有一个便雅悯人逃出了战场，他撕烂衣服，头蒙灰尘，来到示罗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一個便雅憫人跑離戰場，衣服撕裂，頭蒙塵土，當天來到示羅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當日，有一個便雅憫人從陣上逃跑，衣服撕裂，頭蒙灰塵，來到示羅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now a man of Benjamin ran from the battle line and came to Shiloh the same day with his clothes torn and dust on his head."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "That same day a Benjamite ran from the battle line and went to Shiloh with his clothes torn and dust on his head."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A man from the tribe of Benjamin ran from the battlefield and arrived at Shiloh later that same day. He had torn his clothes and put dust on his head to show his grief."
      }
