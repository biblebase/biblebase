    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.3.10",
            "paragraphs": [
              "10.耶和華又來站著，所以就成為可見的，並且說話。撒母耳現在同時領受了話語和異象。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "1sa.3.10",
            "paragraphs": [
              "三10\"敬聽\"。這字詞的意思是：靜聽而且準備遵從。撒母耳正在聆聽神的話，並且決意順服神。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华来站在那里，再次像刚才一样呼唤：“撒母耳！撒母耳！”撒母耳答道：“请吩咐，仆人恭听。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華又來站著，像前幾次一樣呼喚：“撒母耳，撒母耳！”他回答：“請說！僕人敬聽。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華又來站着，像前三次呼喚說：「撒母耳啊！撒母耳啊！」撒母耳回答說：「請說，僕人敬聽！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord came and stood and called as at other times, “Samuel! Samuel!” And Samuel said, “Speak, for Your servant is listening.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord came and stood there, calling as at the other times, “Samuel! Samuel!” Then Samuel said, “Speak, for your servant is listening.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the Lord came and called as before, “Samuel! Samuel!” And Samuel replied, “Speak, your servant is listening.”"
      }
    },
