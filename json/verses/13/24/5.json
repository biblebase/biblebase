    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为以利亚撒的子孙中和以他玛的子孙中都有人在圣所做首领事奉上帝，他们便用抽签的方法公平地分班，分别在以利亚撒和以他玛家族中抽签。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以抽籤的方式平均地把他們分開，因為在以利亞撒的子孫中和以他瑪的子孫中都有人在聖所作領袖，以及在　神面前作領袖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "都掣籤分立，彼此一樣。在聖所和神面前作首領的有以利亞撒的子孫，也有以他瑪的子孫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus they were divided by lot, the one as the other; for they were officers of the sanctuary and officers of God, both from the descendants of Eleazar and the descendants of Ithamar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They divided them impartially by casting lots, for there were officials of the sanctuary and officials of God among the descendants of both Eleazar and Ithamar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All tasks were assigned to the various groups by means of sacred lots so that no preference would be shown, for there were many qualified officials serving God in the sanctuary from among the descendants of both Eleazar and Ithamar."
      }
    },
