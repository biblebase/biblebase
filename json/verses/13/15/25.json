    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫和以色列的长老以及千夫长去俄别·以东家，高高兴兴地将耶和华的约柜抬上来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是大衛和以色列的眾長老，以及千夫長一起前去，從俄別．以東的家歡歡喜喜地把耶和華的約櫃抬上來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，大衛和以色列的長老，並千夫長都去從俄別‧以東的家歡歡喜喜地將耶和華的約櫃擡上來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So it was David, with the elders of Israel and the captains over thousands, who went to bring up the ark of the covenant of the Lord from the house of Obed-edom with joy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So David and the elders of Israel and the commanders of units of a thousand went to bring up the ark of the covenant of the Lord from the house of Obed-Edom, with rejoicing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then David and the elders of Israel and the generals of the army went to the house of Obed-edom to bring the Ark of the Lord’s Covenant up to Jerusalem with a great celebration."
      }
    },
