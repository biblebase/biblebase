    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.15.27",
            "paragraphs": [
              "一五27\"細麻布的以弗得\"。參看撒母耳記下六章14節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫和抬约柜的利未人、歌乐手及其领班基拿尼雅都穿上细麻衣，大卫还穿上细麻布的以弗得。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛和所有抬約櫃的利未人，以及歌唱的人和歌唱的人的領袖基拿尼雅，都穿上細麻布的外袍；大衛另外穿上細麻布的以弗得。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛和擡約櫃的利未人，並歌唱人的首領基拿尼雅，以及歌唱的人，都穿着細麻布的外袍；大衛另外穿着細麻布的以弗得。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "David was clothed with a robe of fine linen, as also were all the Levites who were carrying the ark, and the singers and Chenaniah the leader of the music of the singers. And David wore a linen ephod."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now David was clothed with a robe of fine linen with all the Levites who were carrying the ark, and the singers and Chenaniah the leader of the singing with the singers. David also wore an ephod of linen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now David was clothed in a robe of fine linen, as were all the Levites who were carrying the ark, and as were the musicians, and Kenaniah, who was in charge of the singing of the choirs. David also wore a linen ephod."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "David was clothed with a robe of fine linen, as were all the Levites who bore the ark, the singers, and Chenaniah the music master with the singers. David also wore a linen ephod."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "David was dressed in a robe of fine linen, as were all the Levites who carried the Ark, and also the singers, and Kenaniah the choir leader. David was also wearing a priestly garment."
      }
    },
