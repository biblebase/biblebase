    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.16.37",
            "paragraphs": [
              "一六37\"在約櫃前\"。約櫃當時已在耶路撒冷，但會幕仍在基遍（39節）。直到所羅門的聖殿建成，兩者始再次在一起（王上八4）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫派亚萨及其亲族天天在约柜前事奉耶和华，尽当尽的职分；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是在那裡，就是耶和華的約櫃前，大衛留下亞薩和他的親族，照著每日的本分，在約櫃前不斷地事奉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛派亞薩和他的弟兄在約櫃前常常事奉耶和華，一日盡一日的職分；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So David left Asaph and his brothers there before the ark of the covenant of the Lord to minister regularly before the ark as each day required,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he left Asaph and his relatives there before the ark of the covenant of the Lord to minister before the ark continually, as every day’s work required;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "David left Asaph and his associates before the ark of the covenant of the Lord to minister there regularly, according to each day’s requirements."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So he left Asaph and his brothers there before the ark of the covenant of the Lord to minister before the ark regularly, as every day’s work required;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "David arranged for Asaph and his fellow Levites to serve regularly before the Ark of the Lord’s Covenant, doing whatever needed to be done each day."
      }
    },
