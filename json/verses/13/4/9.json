    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.4.9-10",
            "paragraphs": [
              "四9,10\"雅比斯\"。他透過充滿信心的禱告，勝過其名字所蘊含的痛苦。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雅比斯比他的众弟兄尊贵，他母亲给他取名叫雅比斯，意思是：“我生他时非常痛苦”。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雅比斯在眾兄弟中最受人尊重，他母親給他起名叫雅比斯，意思說：“我生產時十分痛苦。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雅比斯比他眾弟兄更尊貴，他母親給他起名叫雅比斯，意思說：我生他甚是痛苦。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Jabez was more honorable than his brothers; and his mother called his name Jabez, saying, “Because I bore him in pain.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jabez was more honorable than his brothers, and his mother named him Jabez saying, “Because I bore him with pain.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jabez was more honorable than his brothers. His mother had named him Jabez, saying, “I gave birth to him in pain.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now Jabez was more honorable than his brothers, and his mother called his name Jabez, saying, “Because I bore him in pain.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There was a man named Jabez who was more honorable than any of his brothers. His mother named him Jabez because his birth had been so painful."
      }
    },
