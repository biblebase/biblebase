    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "找到了一片肥沃的草场，又辽阔又安静。从前含族人在那里居住。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們找到了一塊肥美的草場；那地十分寬闊，又清靜、又安寧；從前住在那裡的是含族的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "尋得肥美的草場地，又寬闊又平靜。從前住那裏的是含族的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They found rich and good pasture, and the land was broad and quiet and peaceful; for those who lived there formerly were Hamites."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They found rich, good pasture, and the land was spacious, peaceful and quiet. Some Hamites had lived there formerly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They found lush pastures there, and the land was spacious, quiet, and peaceful. Some of Ham’s descendants had been living in that region."
      }
    },
