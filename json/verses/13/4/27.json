    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "示每有十六个儿子和六个女儿，他弟兄的儿女不多。他们整个宗族的人不如犹大宗族的人多。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "示每有十六個兒子，六個女兒；他兄弟們的兒女卻不多；他們各家的兒女，都不如猶大族的子孫那樣多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "示每有十六個兒子，六個女兒，他弟兄的兒女不多，他們各家不如猶大族的人丁增多。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Shimei had sixteen sons and six daughters; but his brothers did not have many sons, nor did all their family multiply like the sons of Judah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Shimei had sixteen sons and six daughters, but his brothers did not have many children; so their entire clan did not become as numerous as the people of Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Shimei had sixteen sons and six daughters, but none of his brothers had large families. So Simeon’s tribe never grew as large as the tribe of Judah."
      }
    },
