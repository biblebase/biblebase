    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "来自约旦河东边的吕便支派、迦得支派和玛拿西半个支派的十二万拥有各种兵器的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在約旦河東岸，流本支派、迦得支派和瑪拿西半個支派的人中，能拿各種兵器作戰的，共有十二萬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約旦河東的呂便支派、迦得支派、瑪拿西半支派，拿着各樣兵器打仗的有十二萬人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the other side of the Jordan, of the Reubenites and the Gadites and of the half-tribe of Manasseh, there were 120,000 with all kinds of weapons of war for the battle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and from east of the Jordan, from Reuben, Gad and the half-tribe of Manasseh, armed with every type of weapon—120,000."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From the east side of the Jordan River—where the tribes of Reuben and Gad and the half-tribe of Manasseh lived—there were 120,000 troops armed with every kind of weapon."
      }
    },
