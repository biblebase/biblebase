    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我算什么，我的人民算什么，怎么配向你奉献？因为万物都从你而来，我们只是把从你那里得来的献给你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我算甚麼？我的人民又算甚麼？竟有力量這樣樂意奉獻？因為萬物都是從你而來，我們只是把從你手裡得來的，奉獻給你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我算甚麼，我的民算甚麼，竟能如此樂意奉獻？因為萬物都從你而來，我們把從你而得的獻給你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But who am I and who are my people that we should be able to offer as generously as this? For all things come from You, and from Your hand we have given You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But who am I, and who are my people, that we should be able to give as generously as this? Everything comes from you, and we have given you only what comes from your hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But who am I, and who are my people, that we could give anything to you? Everything we have has come from you, and we give you only what you first gave us!"
      }
    },
