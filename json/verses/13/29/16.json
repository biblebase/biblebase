    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的上帝耶和华啊，我们为你的圣名建造殿宇所预备的这一切财物都是从你那里来的，都属于你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華我們的　神啊，我們預備的這一切財物，要為你的聖名建造殿宇，都是從你而來的，也都是屬於你的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－我們的神啊，我們預備這許多材料，要為你的聖名建造殿宇，都是從你而來，都是屬你的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord our God, all this abundance that we have provided to build You a house for Your holy name, it is from Your hand, and all is Yours."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord our God, all this abundance that we have provided for building you a temple for your Holy Name comes from your hand, and all of it belongs to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O Lord our God, even this material we have gathered to build a Temple to honor your holy name comes from you! It all belongs to you!"
      }
    },
