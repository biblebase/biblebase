    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫王又对全体会众说：“我儿所罗门是上帝特别拣选的，但他还年轻，缺乏经验。建殿的工程浩大，因为这殿不是为人造的，乃是为耶和华上帝造的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛王對全體會眾說：“我的兒子所羅門，是　神所揀選的，現在還年幼識淺，而這工程非常浩大；因為這殿宇不是為人建造，而是為耶和華　神建造的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛王對會眾說：「我兒子所羅門是神特選的，還年幼嬌嫩；這工程甚大，因這殿不是為人，乃是為耶和華神建造的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then King David said to the entire assembly, “My son Solomon, whom alone God has chosen, is still young and inexperienced and the work is great; for the temple is not for man, but for the Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then King David said to the whole assembly: “My son Solomon, the one whom God has chosen, is young and inexperienced. The task is great, because this palatial structure is not for man but for the Lord God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then King David turned to the entire assembly and said, “My son Solomon, whom God has clearly chosen as the next king of Israel, is still young and inexperienced. The work ahead of him is enormous, for the Temple he will build is not for mere mortals—it is for the Lord God himself!"
      }
    },
