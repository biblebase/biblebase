    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "比那三十个勇士更有声望，只是不及前三位勇士。大卫派他做护卫长。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他比那三十個勇士更有聲望，只是還不及前三個勇士。大衛立他作侍衛長。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他比那三十個勇士都尊貴，只是不及前三個勇士。大衛立他作護衛長。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, he was honored among the thirty, but he did not attain to the three; and David appointed him over his guard."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He was held in greater honor than any of the Thirty, but he was not included among the Three. And David put him in charge of his bodyguard."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He was more honored than the other members of the Thirty, though he was not one of the Three. And David made him captain of his bodyguard."
      }
