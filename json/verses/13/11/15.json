    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.11.15",
            "paragraphs": [
              "一一15\"亞杜蘭洞\"。參看撒母耳記上二十二章1節的腳註。\"利乏音谷\"。鄰近耶路撒冷，向伯利恒方向的一個山谷。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一次，三十位勇士中有三位到亚杜兰洞附近的磐石那里见大卫，当时非利士的军队驻扎在利乏音谷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "三十個首領中有三個，下到亞杜蘭洞附近的磐石那裡去見大衛；非利士人的軍隊在利乏音谷安營。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "三十個勇士中的三個人下到磐石那裏，進了亞杜蘭洞見大衛；非利士的軍隊在利乏音谷安營。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Three of the thirty chief men went down to the rock to David at the cave of Adullam, when the army of Philistines was encamped in the Valley of Rephaim."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now three of the thirty chief men went down to the rock to David, into the cave of Adullam, while the army of the Philistines was camping in the valley of Rephaim."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Three of the thirty chiefs came down to David to the rock at the cave of Adullam, while a band of Philistines was encamped in the Valley of Rephaim."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now three of the thirty chief men went down to the rock to David, into the cave of Adullam; and the army of the Philistines encamped in the Valley of Rephaim."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Once when David was at the rock near the cave of Adullam, the Philistine army was camped in the valley of Rephaim. The Three (who were among the Thirty—an elite group among David’s fighting men) went down to meet him there."
      }
    },
