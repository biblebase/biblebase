    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他曾在巴斯·达闵与大卫一起抵抗非利士人。那里有一块大麦田，以色列人战败逃跑，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他從前與大衛在巴斯．達閔，非利士人聚集在那裡預備作戰。那裡有塊長滿大麥的田；眾人從非利士人面前逃跑了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他從前與大衛在巴斯‧達閔，非利士人聚集要打仗。那裏有一塊長滿大麥的田，眾民就在非利士人面前逃跑；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He was with David at Pas-dammim when the Philistines were gathered there for battle. There was a plot of ground full of barley, and the men fled from the Philistines."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He was with David at Pasdammim when the Philistines were gathered together there to battle, and there was a plot of ground full of barley; and the people fled before the Philistines."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He was with David at Pas Dammim when the Philistines gathered there for battle. At a place where there was a field full of barley, the troops fled from the Philistines."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He was with David at Pasdammim. Now there the Philistines were gathered for battle, and there was a piece of ground full of barley. So the people fled from the Philistines."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He was with David when the Philistines gathered for battle at Pas-dammim and attacked the Israelites in a field full of barley. The Israelite army fled,"
      }
    },
