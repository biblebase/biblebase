    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "1ch.13.14",
            "paragraphs": [
              "13:14",
              "賜福給俄別以東的家：神賜福給俄別以東的家無疑是因為他順服神，懷著敬畏之心接受了約櫃；給一個人帶來殺身之禍的東西卻給另一個人帶來賜福。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝的约柜在俄别·以东家存放了三个月。耶和华使俄别·以东家及其拥有的一切都蒙福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神的約櫃存放在俄別．以東的家裡三個月，耶和華賜福給俄別．以東的家和他的一切。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神的約櫃在俄別‧以東家中三個月，耶和華賜福給俄別‧以東的家和他一切所有的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus the ark of God remained with the family of Obed-edom in his house three months; and the Lord blessed the family of Obed-edom with all that he had."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The ark of God remained with the family of Obed-Edom in his house for three months, and the Lord blessed his household and everything he had."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Ark of God remained there in Obed-edom’s house for three months, and the Lord blessed the household of Obed-edom and everything he owned."
      }
    },
