    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫因耶和华击杀乌撒而烦恼，就称那地方为毗列斯·乌撒，沿用至今。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛因耶和華忽然殺死烏撒，非常不悅，就稱那地方為毘列斯．烏撒，直到今日。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛因耶和華擊殺烏撒，心裏愁煩，就稱那地方為毗列斯‧烏撒，直到今日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David became angry because of the Lord’s outburst against Uzza; and he called that place Perez-uzza to this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then David was angry because the Lord’s wrath had broken out against Uzzah, and to this day that place is called Perez Uzzah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "David was angry because the Lord’s anger had burst out against Uzzah. He named that place Perez-uzzah (which means “to burst out against Uzzah”), as it is still called today."
      }
