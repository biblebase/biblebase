      "leili": {
        "content": [
          {
            "verse": "1ch.17.11-14",
            "paragraphs": [
              "一七11～14關於這偉大的大衛之約的含意，參看撒母耳記下七章12至16節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要做他的父亲，他要做我的儿子。我的慈爱曾离开在你之前的扫罗王，但我的慈爱必不离开他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要作他的父親，他要作我的兒子；我的慈愛必不離開他，像離開在你以前的那位一樣；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要作他的父，他要作我的子；並不使我的慈愛離開他，像離開在你以前的掃羅一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will be his father and he shall be My son; and I will not take My lovingkindness away from him, as I took it from him who was before you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will be his father, and he will be my son. I will never take my love away from him, as I took it away from your predecessor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will be his father, and he will be my son. I will never take my favor from him as I took it from the one who ruled before you."
      }
    },
