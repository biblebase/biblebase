    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的上帝啊，因为你曾启示仆人，说要为仆人建立王朝，仆人才敢这样向你祈求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的　神啊，因為你啟示了你的僕人，要你的僕人為你建造殿宇，所以你僕人才敢在你面前這樣禱告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的神啊，因你啟示僕人說，我必為你建立家室，所以僕人大膽在你面前祈禱。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You, O my God, have revealed to Your servant that You will build for him a house; therefore Your servant has found courage to pray before You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You, my God, have revealed to your servant that you will build a house for him. So your servant has found courage to pray to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O my God, I have been bold enough to pray to you because you have revealed to your servant that you will build a house for him—a dynasty of kings!"
      }
