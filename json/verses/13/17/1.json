    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "1ch.17.1",
            "paragraphs": [
              "17:1",
              "1~27神與大衛立約：17章與撒下7:1~29幾乎完全一樣。關於此約的各個方面，參“神與大衛之約”一文。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "1ch.17.1",
            "paragraphs": [
              "一七1\"在幔子\"。即在帳幕。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫住进宫殿后，对拿单先知说：“你看，我住在香柏木建的王宫中，耶和华的约柜却在帐篷里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛住在自己宮中的時候，他對拿單先知說：“看哪，我住在香柏木做的王宮，耶和華的約櫃卻在帳幕裡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛住在自己宮中，對先知拿單說：「看哪，我住在香柏木的宮中，耶和華的約櫃反在幔子裏。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And it came about, when David dwelt in his house, that David said to Nathan the prophet, “Behold, I am dwelling in a house of cedar, but the ark of the covenant of the Lord is under curtains.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After David was settled in his palace, he said to Nathan the prophet, “Here I am, living in a house of cedar, while the ark of the covenant of the Lord is under a tent.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When David was settled in his palace, he summoned Nathan the prophet. “Look,” David said, “I am living in a beautiful cedar palace, but the Ark of the Lord’s Covenant is out there under a tent!”"
      }
    },
