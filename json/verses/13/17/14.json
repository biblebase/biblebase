      "leili": {
        "content": [
          {
            "verse": "1ch.17.11-14",
            "paragraphs": [
              "一七11～14關於這偉大的大衛之約的含意，參看撒母耳記下七章12至16節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使他永远治理我的家和我的国。他的王位必永远稳固。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我卻要把他堅立在我的家裡和我的國裡，直到永遠；他的王位必永遠堅立。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我卻要將他永遠堅立在我家裏和我國裏；他的國位也必堅定，直到永遠。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I will settle him in My house and in My kingdom forever, and his throne shall be established forever.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will set him over my house and my kingdom forever; his throne will be established forever.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will confirm him as king over my house and my kingdom for all time, and his throne will be secure forever.’”"
      }
