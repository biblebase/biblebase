    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，大卫把这些器皿和他从以东、摩押、亚扪、非利士和亚玛力各国夺来的金银都分别出来，奉献给耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛王把這些器皿，連同從各國奪取的金銀，就是從以東、摩押、亞捫人、非利士人和亞瑪力人奪取的，都分別為聖獻給耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛王將這些器皿，並從各國奪來的金銀，就是從以東、摩押、亞捫、非利士、亞瑪力人所奪來的，都分別為聖獻給耶和華。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "These also King David dedicated to the Lord, together with the silver and gold that he had carried off from all the nations, from Edom, Moab, the Ammonites, the Philistines, and Amalek."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "King David also dedicated these to the Lord with the silver and the gold which he had carried away from all the nations: from Edom, Moab, the sons of Ammon, the Philistines, and from Amalek."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "King David dedicated these articles to the Lord, as he had done with the silver and gold he had taken from all these nations: Edom and Moab, the Ammonites and the Philistines, and Amalek."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "King David also dedicated these to the Lord, along with the silver and gold that he had brought from all these nations—from Edom, from Moab, from the people of Ammon, from the Philistines, and from Amalek."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King David dedicated all these gifts to the Lord, along with the silver and gold he had taken from the other nations—from Edom, Moab, Ammon, Philistia, and Amalek."
      }
    },
