    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫在以东驻兵，以东人都臣服他。耶和华使他无往而不胜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛在以東設立駐防營，所有以東人就都臣服大衛。大衛無論到甚麼地方去，耶和華都使他得勝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛在以東地設立防營，以東人就都歸服他。大衛無論往哪裏去，耶和華都使他得勝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he put garrisons in Edom, and all the Edomites became servants to David. And the Lord helped David wherever he went."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He put garrisons in Edom, and all the Edomites became subject to David. The Lord gave David victory wherever he went."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He placed army garrisons in Edom, and all the Edomites became David’s subjects. In fact, the Lord made David victorious wherever he went."
      }
