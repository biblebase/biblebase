    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们同族的弟兄有米迦勒、米书兰、示巴、约赖、雅干、细亚和希伯，共七人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們同家族的兄弟是米迦勒、米書蘭、示巴、約賴、雅干、細亞和希伯，共七人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們族弟兄是米迦勒、米書蘭、示巴、約賴、雅干、細亞、希伯，共七人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their kinsmen of their fathers’ households were Michael, Meshullam, Sheba, Jorai, Jacan, Zia and Eber, seven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their relatives, by families, were: Michael, Meshullam, Sheba, Jorai, Jakan, Zia and Eber—seven in all."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their relatives, the leaders of seven other clans, were Michael, Meshullam, Sheba, Jorai, Jacan, Zia, and Eber."
      }
