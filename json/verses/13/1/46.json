    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "户珊死后，比达的儿子哈达继位，定都亚未得，他曾在摩押地区击败米甸人。"
      },
      "cnv": {
        "version_id": "CNV",
