    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.25.8",
            "paragraphs": [
              "二五8\"分了班次\"。為師的（熟練的人）是第7節的二百八十八人，而為徒的是第二十三章5節的四千人。他們掣簽來決定二十四班的劃分，借以配合二十四隊的祭司。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这些人不分长幼、师徒，都抽签分班。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些人，無論大小，不分師生，都一同抽籤分班次。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些人無論大小，為師的、為徒的，都一同掣籤分了班次。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they cast lots for their duties, small and great, teacher and pupil alike."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They cast lots for their duties, all alike, the small as well as the great, the teacher as well as the pupil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Young and old alike, teacher as well as student, cast lots for their duties."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they cast lots for their duty, the small as well as the great, the teacher with the student."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The musicians were appointed to their term of service by means of sacred lots, without regard to whether they were young or old, teacher or student."
      }
