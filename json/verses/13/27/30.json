      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "管理駱駝的，是以實瑪利人阿比勒；管理驢群的，是米崙人耶希底亞；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掌管駝羣的是以實瑪利人阿比勒。掌管驢羣的是米崙人耶希底亞。掌管羊羣的是夏甲人雅悉。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Obil the Ishmaelite had charge of the camels; and Jehdeiah the Meronothite had charge of the donkeys."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Obil the Ishmaelite was in charge of the camels. Jehdeiah the Meronothite was in charge of the donkeys."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Obil the Ishmaelite was in charge of the camels. Jehdeiah from Meronoth was in charge of the donkeys."
      }
    },
