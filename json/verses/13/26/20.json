    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.26.20",
            "paragraphs": [
              "二六20\"神殿的府庫\"。聖殿日常的收入是從指定的奉獻和特殊的獻禮而來（利二七；民一八16；代上二九7,8）。\"聖物的府庫\"。參看第26至28節、第十八章11節和歷代誌下五章1節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "利未人亚希雅负责掌管上帝殿里的库房和放奉献之物的库房。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們其他的利未親族，就管理神殿裡的庫房和聖物的庫房。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利未子孫中有亞希雅掌管神殿的府庫和聖物的府庫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Levites, their relatives, had charge of the treasures of the house of God and of the treasures of the dedicated gifts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their fellow Levites were in charge of the treasuries of the house of God and the treasuries for the dedicated things."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Other Levites, led by Ahijah, were in charge of the treasuries of the house of God and the treasuries of the gifts dedicated to the Lord."
      }
