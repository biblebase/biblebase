    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你去告诉大卫，我有三样灾祸，他可以选择让我降哪一样给他。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你去告訴大衛，說：‘耶和華這樣說：我給你提出三件事，你可以選擇一件，我好向你施行。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你去告訴大衛說，耶和華如此說：我有三樣災，隨你選擇一樣，我好降與你。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Go and say to David, ‘Thus says the Lord, Three things I offer you; choose one of them, that I may do it to you.’”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Go and speak to David, saying, ‘Thus says the Lord, “I offer you three things; choose for yourself one of them, which I will do to you.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Go and tell David, ‘This is what the Lord says: I am giving you three options. Choose one of them for me to carry out against you.’ ”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Go and tell David, saying, ‘Thus says the Lord: “I offer you three things; choose one of them for yourself, that I may do it to you.” ’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Go and say to David, ‘This is what the Lord says: I will give you three choices. Choose one of these punishments, and I will inflict it on you.’”"
      }
