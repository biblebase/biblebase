    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "1ch.21.7",
            "paragraphs": [
              "21:7",
              "神……便降災給以色列人：以色列民很可能也支持大衛數點百姓的罪惡之舉。約押自己就知道數點是大逆之罪，會使以色列人陷在罪裏(3節)。因此，可以想象得到，大多數以色列人都因贊同數點一事而犯了罪。因著舉國上下人心的驕傲，他們就一同有份於大衛的罪，也一同被審判(參下一條註釋)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝也不喜悦这事，便降灾给以色列人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神不喜悅這事，所以他擊打以色列人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神不喜悅這數點百姓的事，便降災給以色列人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God was displeased with this thing, so He struck Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This command was also evil in the sight of God; so he punished Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God was very displeased with the census, and he punished Israel for it."
      }
    },
