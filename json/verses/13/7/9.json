    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.7.6-12",
            "paragraphs": [
              "七6～12\"便雅憫的兒子\"。參看創世記四十六章21節和民數記二十六章38至41節，還有第八章1節及其後的補充資料。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "比结的儿子都是族长。按家谱的记载，他们宗族共有两万零二百个英勇的战士。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們都是各家族的首領，是英勇的戰士，按著家譜登記的，共有二萬零二百人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們都是族長，是大能的勇士。按着家譜計算，他們的子孫共有二萬零二百人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They were enrolled by genealogy, according to their generations, heads of their fathers’ households, 20,200 mighty men of valor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their genealogical record listed the heads of families and 20,200 fighting men."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each of them was the leader of an ancestral clan. The total number of mighty warriors and leaders from these clans was 20,200, as listed in their genealogical records."
      }
    },
