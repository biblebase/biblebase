    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲有一个女儿叫舍伊拉，她建了上伯·和仑、下伯·和仑和乌羡·舍伊拉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的女兒是舍伊拉，就是建造上伯．和崙、下伯．和崙和烏羨．舍伊拉的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的女兒名叫舍伊拉，就是建築上伯‧和崙、下伯‧和崙與烏羨‧舍伊拉的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His daughter was Sheerah, who built lower and upper Beth-horon, also Uzzen-sheerah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His daughter was Sheerah, who built Lower and Upper Beth Horon as well as Uzzen Sheerah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He had a daughter named Sheerah. She built the towns of Lower and Upper Beth-horon and Uzzen-sheerah."
      }
    },
