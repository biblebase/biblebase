    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华吹一口气， 草就枯干，花也凋残。 人类诚然像草。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "草必枯乾，花必凋謝； 因為耶和華的氣吹在上面； 真的，這民的確是草！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "草必枯乾，花必凋殘， 因為耶和華的氣吹在其上； 百姓誠然是草。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The grass withers, the flower fades, When the breath of the Lord blows upon it; Surely the people are grass."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The grass withers and the flowers fall, because the breath of the Lord blows on them. Surely the people are grass."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The grass withers and the flowers fade beneath the breath of the Lord. And so it is with people."
      }
