      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe to him who says to a father, ‘What are you begetting?’ Or to a woman, ‘To what are you giving birth?’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Woe to the one who says to a father, ‘What have you begotten?’ or to a mother, ‘What have you brought to birth?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
