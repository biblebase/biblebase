    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "isa.9.15",
            "paragraphs": [
              "15.以賽亞通常都會解釋他所講的隱喻（如，八7）。尊貴人：「臉面高舉的人」，指不在位的重要人士。尾：這是以賽亞俏皮的寫法！那些先知自認為是屬靈的領袖，但事實上卻像狗尾巴，只會應群眾的要求搖擺（三十9～11；王上二十二6；彌二11）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "头是指他们的长老和首领， 尾是指教人虚谎之事的假先知。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "長老和尊貴人就是頭； 用謊言教導人的先知就是尾。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "長老和尊貴人就是頭， 以謊言教人的先知就是尾。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The head is the elder and honorable man, And the prophet who teaches falsehood is the tail."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the elders and dignitaries are the head, the prophets who teach lies are the tail."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The leaders of Israel are the head, and the lying prophets are the tail."
      }
    },
