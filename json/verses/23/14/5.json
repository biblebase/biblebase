    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "isa.14.4-11",
            "paragraphs": [
              "一四4～11一首嘲弄巴比倫王（及其制度）的歌曲，內容既有歷史，也有末世意義。作者甚至形容\"陰間\"（希伯來文是Sheol）正在歡迎這巴比倫王（9節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华折断了邪恶君主的权杖。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華折斷了惡人的杖 和統治者的棒，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華折斷了惡人的杖， 轄制人的圭，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord has broken the staff of the wicked, The scepter of rulers"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord has broken the rod of the wicked, the scepter of the rulers,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Lord has crushed your wicked power and broken your evil rule."
      }
