    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以下是关于埃及的预言： 看啊，耶和华驾着疾驰的云来到埃及。 埃及的偶像在祂面前颤抖， 埃及人胆战心惊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "關於埃及的默示： 看哪！耶和華駕著輕快的雲， 來到埃及； 埃及的偶像在他面前戰兢， 埃及人的心都驚怕。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "有关埃及的默示： 看哪，耶和华乘驾飞快的云， 即将临到埃及！ 埃及的偶像将在他面前颤抖， 埃及人都心融胆丧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論埃及的默示： 看哪，耶和華乘駕快雲， 臨到埃及。 埃及的偶像在他面前戰兢； 埃及人的心在裏面消化。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "An oracle concerning Egypt. Behold, the Lord  is riding on a swift cloud and comes to Egypt; and the idols of Egypt will tremble at his presence, and the heart of the Egyptians will melt within them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The oracle concerning Egypt. Behold, the Lord is riding on a swift cloud and is about to come to Egypt; The idols of Egypt will tremble at His presence, And the heart of the Egyptians will melt within them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A prophecy against Egypt: See, the Lord rides on a swift cloud and is coming to Egypt. The idols of Egypt tremble before him, and the hearts of the Egyptians melt with fear."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The burden against Egypt. Behold, the Lord  rides on a swift cloud, And will come into Egypt; The idols of Egypt will totter at His presence, And the heart of Egypt will melt in its midst."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This message came to me concerning Egypt: Look! The Lord is advancing against Egypt, riding on a swift cloud. The idols of Egypt tremble. The hearts of the Egyptians melt with fear."
      }
