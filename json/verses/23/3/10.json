    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "isa.3.10",
            "paragraphs": [
              "3:10",
              "要論義人說：神指示以賽亞勉勵那些生活在不義社會中依然忠於神的人。盡管義人現今要為義受苦，但是神會使萬事成為美好，並且豐豐富富地賞賜他們。至於惡人，神必定要對他們施行審判(參太5:10註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要告诉义人：他们有福了， 必得善报。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要告訴義人，他們必得福樂， 因為他們必享自己行為所結的果子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要論義人說：他必享福樂， 因為要吃自己行為所結的果子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Say to the righteous that it will go well with them, For they will eat the fruit of their actions."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Tell the righteous it will be well with them, for they will enjoy the fruit of their deeds."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Tell the godly that all will be well for them. They will enjoy the rich reward they have earned!"
      }
