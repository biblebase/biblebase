    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "isa.3.1",
            "paragraphs": [
              "3:1",
              "從耶路撒冷……除掉：民眾的罪使神不得不向整個社會和各個階層的人施行審判(2~3節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "isa.3.1",
            "paragraphs": [
              "三1\"除掉眾人所倚靠的、仗賴的\"。借尼布甲尼撒（參看王下二四15,16）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主——万军之耶和华要从耶路撒冷和犹大拿去众人倚靠的一切：粮食、水、"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！主萬軍之耶和華，快要從耶路撒冷和猶大， 除掉人所倚靠的和所倚賴的，就是眾人所倚靠的糧， 眾人所倚賴的水；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主－萬軍之耶和華從耶路撒冷和猶大， 除掉眾人所倚靠的，所仗賴的， 就是所倚靠的糧，所仗賴的水；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For behold, the Lord God of hosts is going to remove from Jerusalem and Judah Both supply and support, the whole supply of bread And the whole supply of water;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See now, the Lord, the Lord Almighty, is about to take from Jerusalem and Judah both supply and support: all supplies of food and all supplies of water,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord, the Lord of Heaven’s Armies, will take away from Jerusalem and Judah everything they depend on: every bit of bread and every drop of water,"
      }
    },
