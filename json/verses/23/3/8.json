    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶路撒冷必崩溃， 犹大必败落， 因为他们的言行冒犯耶和华， 蔑视荣耀的主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶路撒冷敗落，猶大傾倒， 是因為他們的舌頭和行為都敵對耶和華， 惹怒了他那充滿榮光的眼目。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷敗落， 猶大傾倒； 因為他們的舌頭和行為與耶和華反對， 惹了他榮光的眼目。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For Jerusalem has stumbled and Judah has fallen, Because their speech and their actions are against the Lord, To rebel against His glorious presence."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jerusalem staggers, Judah is falling; their words and deeds are against the Lord, defying his glorious presence."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For Jerusalem will stumble, and Judah will fall, because they speak out against the Lord and refuse to obey him. They provoke him to his face."
      }
