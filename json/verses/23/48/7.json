    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这些事过去没有，现在才有， 在今天以前你从未听过， 免得你说，‘这些事我早就知道了。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些事是現在才造成的，並不是從古時就有的； 在今天以前，你還未曾聽見過， 免得你說：‘看哪！這些事我早已知道了。’"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这些事是现在造的， 不是从前就有的， 是你今日以先未曾听过的， 免得你说： ‘看哪，这些事我早已知道了！’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這事是現今造的，並非從古就有； 在今日以先，你也未曾聽見， 免得你說：這事我早已知道了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They are created now, not long ago; before today you have never heard of them, lest you should say, ‘Behold, I knew them.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They are created now and not long ago; And before today you have not heard them, So that you will not say, ‘Behold, I knew them.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are created now, and not long ago; you have not heard of them before today. So you cannot say, ‘Yes, I knew of them.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They are created now and not from the beginning; And before this day you have not heard them, Lest you should say, ‘Of course I knew them.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They are brand new, not things from the past. So you cannot say, ‘We knew that all the time!’"
      }
