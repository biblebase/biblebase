    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "isa.64.1-7",
            "paragraphs": [
              "六四1～7祈求神幹預的禱告，如在西乃山上的禱告，也是以色列人在災難日子裏要作出的禱告。比較第4節與哥林多前書二章9節。\"汙穢的衣服\"（6節）。直譯作：某些日子的衣服，指婦女的經期，因而說明罪本質的汙染和厭惡。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无人呼求你， 无人向你求助， 因为你掩面不理我们， 让我们在自己的罪中灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有人呼求你的名， 沒有人奮起抓著你； 因為你掩面不顧我們， 使我們在自己罪孽的權勢下融化。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "没有人呼求你的名， 没有人奋起去紧抓你； 因为你向我们掩面， 使我们在自己罪孽的权势下消融。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且無人求告你的名； 無人奮力抓住你。 原來你掩面不顧我們， 使我們因罪孽消化。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "There is no one who calls upon your name, who rouses himself to take hold of you; for you have hidden your face from us, and have made us melt in the hand of our iniquities."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is no one who calls on Your name, Who arouses himself to take hold of You; For You have hidden Your face from us And have delivered us into the power of our iniquities."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one calls on your name or strives to lay hold of you; for you have hidden your face from us and have given us over to our sins."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And there  is no one who calls on Your name, Who stirs himself up to take hold of You; For You have hidden Your face from us, And have consumed us because of our iniquities."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet no one calls on your name or pleads with you for mercy. Therefore, you have turned away from us and turned us over to our sins."
      }
    },
