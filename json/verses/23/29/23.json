    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为他们看见我赐给他们的子孙时， 必尊我的名为圣， 必尊雅各的圣者为圣， 必敬畏以色列的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他看見了他的子孫在他中間， 就是我手所作的。 他們必尊我的名為聖， 必尊雅各的聖者為聖， 也必敬畏以色列的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但他看見他的眾子， 就是我手的工作在他那裏， 他們必尊我的名為聖， 必尊雅各的聖者為聖， 必敬畏以色列的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when he sees his children, the work of My hands, in his midst, They will sanctify My name; Indeed, they will sanctify the Holy One of Jacob And will stand in awe of the God of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they see among them their children, the work of my hands, they will keep my name holy; they will acknowledge the holiness of the Holy One of Jacob, and will stand in awe of the God of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For when they see their many children and all the blessings I have given them, they will recognize the holiness of the Holy One of Jacob. They will stand in awe of the God of Israel."
      }
    },
