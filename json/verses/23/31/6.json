    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "isa.31.6-7",
            "paragraphs": [
              "6～7.歸向：悔改始於心中，但要借著生活方向的改變來表明其實際（一27）。悖逆：見一5。聖經總是註意最終極的事。這裏的那日，是指最後主的日子（參二20）。聖經的信仰總是要預備好，不單預備勝過下一次的危機，更要預備站在神面前（路二十一36）。既然那日會顯明偶像乃是「非神」（二8），只不過是人的權宜作法（手所造），那麽現在就要丟棄它們，否則等到那時候，就太遲了。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人啊，你们曾经严重地悖逆耶和华，现在归向祂吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人哪！你們要回轉，歸向那曾被你們徹底背棄的耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人哪，你們深深地悖逆耶和華，現今要歸向他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Return to Him from whom you have deeply defected, O sons of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Return, you Israelites, to the One you have so greatly revolted against."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Though you are such wicked rebels, my people, come and return to the Lord."
      }
    },
