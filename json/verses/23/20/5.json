    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "isa.20.5-6",
            "paragraphs": [
              "5～6.羞愧：因為所計畫的失敗、所信賴的諾言落空、所懷的盼望逆轉。沿海一帶：被擄的人會沿著海岸道路走到亞述。住在那一帶的人將最先親眼目睹信靠埃及的愚昧結果，因此亦將首先下結論說，若要抵擋亞述，必須另請高明。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些仰赖古实、仗着埃及夸口的人必惊恐羞愧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人必因他們所仰望的古實和他們所誇耀的埃及，驚惶羞愧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人必因所仰望的古實，所誇耀的埃及，驚惶羞愧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they will be dismayed and ashamed because of Cush their hope and Egypt their boast."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who trusted in Cush and boasted in Egypt will be dismayed and put to shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Philistines will be thrown into panic, for they counted on the power of Ethiopia and boasted of their allies in Egypt!"
      }
