    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我独自在榨酒池踩踏， 万民中无人跟我在一起。 我在愤怒中踩踏他们， 在烈怒中践踏他们； 他们的血溅到我的衣服上， 染红了我的衣服。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我獨自踹酒槽； 萬民之中沒有一人與我同在； 我在忿怒中把他們踹下， 在烈怒中把他們踐踏； 他們的血濺在我的衣服上， 我把我所有的衣裳都染污了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“我独自踹酒槽， 万民中没有一人与我在一起。 我在怒气中踹他们， 在怒火中踩踏他们； 他们的血水溅在我的衣服上， 我把全身的衣服都染污了；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我獨自踹酒醡； 眾民中無一人與我同在。 我發怒將他們踹下， 發烈怒將他們踐踏。 他們的血濺在我衣服上， 並且污染了我一切的衣裳。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“I have trodden the winepress alone, and from the peoples no one was with me; I trod them in my anger and trampled them in my wrath; their lifeblood spattered on my garments, and stained all my apparel."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I have trodden the wine trough alone, And from the peoples there was no man with Me. I also trod them in My anger And trampled them in My wrath; And their lifeblood is sprinkled on My garments, And I stained all My raiment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I have trodden the winepress alone; from the nations no one was with me. I trampled them in my anger and trod them down in my wrath; their blood spattered my garments, and I stained all my clothing."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“I have trodden the winepress alone, And from the peoples no one was with Me. For I have trodden them in My anger, And trampled them in My fury; Their blood is sprinkled upon My garments, And I have stained all My robes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I have been treading the winepress alone; no one was there to help me. In my anger I have trampled my enemies as if they were grapes. In my fury I have trampled my foes. Their blood has stained my clothes."
      }
    },
