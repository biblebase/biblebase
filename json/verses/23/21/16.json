    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对我说：“如同雇工合约规定的做工年限，一年之内，基达所有的荣耀必消失，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主對我這樣說：“按照雇工的年數，一年之內，基達的一切光榮都必化為烏有。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主對我這樣說：「一年之內，照雇工的年數，基達的一切榮耀必歸於無有。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus the Lord said to me, “In a year, as a hired man would count it, all the splendor of Kedar will terminate;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord says to me: “Within one year, as a servant bound by contract would count it, all the splendor of Kedar will come to an end."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord said to me, “Within a year, counting each day, all the glory of Kedar will come to an end."
      }
    },
