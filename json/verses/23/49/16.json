        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！我已經把你刻在我的掌上； 你的城牆常常在我面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，我將你銘刻在我掌上； 你的牆垣常在我眼前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I have inscribed you on the palms of My hands; Your walls are continually before Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See, I have engraved you on the palms of my hands; your walls are ever before me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "See, I have written your name on the palms of my hands. Always in my mind is a picture of Jerusalem’s walls in ruins."
      }
