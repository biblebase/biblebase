    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对他们说：“你们都要背弃我，因为圣经上说， “‘我要击打牧人， 羊群将四散。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對他們說：“你們都要後退，因為經上記著： ‘我要擊打牧人， 羊群就分散了。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他們說：「你們都要跌倒了，因為經上記着說： 我要擊打牧人， 羊就分散了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus *said to them, “You will all fall away, because it is written, ‘I will strike down the shepherd, and the sheep shall be scattered.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You will all fall away,” Jesus told them, “for it is written: “ ‘I will strike the shepherd, and the sheep will be scattered.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On the way, Jesus told them, “All of you will desert me. For the Scriptures say, ‘God will strike the Shepherd, and the sheep will be scattered.’"
      }
    },
