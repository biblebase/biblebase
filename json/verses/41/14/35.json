    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣往前走了不远，俯伏在地祷告，如果可以，不要让那时刻临到祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌稍往前走，俯伏在地上禱告：如果可能的話，使那時刻不要臨到他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他就稍往前走，俯伏在地，禱告說：「倘若可行，便叫那時候過去。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He went a little beyond them, and fell to the ground and began to pray that if it were possible, the hour might pass Him by."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Going a little farther, he fell to the ground and prayed that if possible the hour might pass from him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He went on a little farther and fell to the ground. He prayed that, if it were possible, the awful hour awaiting him might pass him by."
      }
    },
