    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他进哪一家，你们也跟着进去，对那家的主人说，‘我们的老师问你，祂的客房在哪里，祂要和门徒在里面吃逾越节的晚餐。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "無論他進入哪一家，你們要對那家主說，老師說：‘我的客廳在哪裡？我和門徒好在那裡吃逾越節的晚餐。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他進哪家去，你們就對那家的主人說：『夫子說：客房在哪裏？我與門徒好在那裏吃逾越節的筵席。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and wherever he enters, say to the owner of the house, ‘The Teacher says, “Where is My guest room in which I may eat the Passover with My disciples?” ’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Say to the owner of the house he enters, ‘The Teacher asks: Where is my guest room, where I may eat the Passover with my disciples?’"
      },
      "nlt": {
