    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们听了喜出望外，答应给他酬金。于是犹大伺机出卖耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們聽見了就很歡喜，答應給他銀子。於是猶大就找機會出賣耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們聽見就歡喜，又應許給他銀子；他就尋思如何得便把耶穌交給他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They were glad when they heard this, and promised to give him money. And he began seeking how to betray Him at an opportune time."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They were delighted to hear this and promised to give him money. So he watched for an opportunity to hand him over."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They were delighted when they heard why he had come, and they promised to give him money. So he began looking for an opportunity to betray Jesus."
      }
    },
