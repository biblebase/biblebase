    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是耶稣带着彼得、雅各和约翰向前走。祂觉得极其难过，非常伤痛，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他帶了彼得、雅各、約翰一起去，就驚懼起來，非常難過。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他带了彼得、雅各和约翰一起去。这时候，他开始忧惧起来，极其难过，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是帶着彼得、雅各、約翰同去，就驚恐起來，極其難過，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he took with him Peter and James and John, and began to be greatly distressed and troubled."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He *took with Him Peter and James and John, and began to be very distressed and troubled."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He took Peter, James and John along with him, and he began to be deeply distressed and troubled."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And He took Peter, James, and John with Him, and He began to be troubled and deeply distressed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He took Peter, James, and John with him, and he became deeply troubled and distressed."
      }
