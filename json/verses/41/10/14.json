    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣看见后很生气，对门徒说：“让小孩子到我这里来，不要阻止他们，因为上帝的国属于这样的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌看見了就生氣，對門徒說：“讓小孩子到我這裡來，不要禁止他們，因為　神的國是屬於這樣的人的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌看見就惱怒，對門徒說：「讓小孩子到我這裏來，不要禁止他們；因為在神國的，正是這樣的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when Jesus saw this, He was indignant and said to them, “Permit the children to come to Me; do not hinder them; for the kingdom of God belongs to such as these."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jesus saw this, he was indignant. He said to them, “Let the little children come to me, and do not hinder them, for the kingdom of God belongs to such as these."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jesus saw what was happening, he was angry with his disciples. He said to them, “Let the children come to me. Don’t stop them! For the Kingdom of God belongs to those who are like these children."
      }
    },
