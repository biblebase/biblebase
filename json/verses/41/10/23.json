    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣看看周围的人，对门徒说：“有钱人进上帝的国真难啊！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌周圍觀看，對門徒說：“富有的人要進　神的國，是多麼難哪！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌周圍一看，對門徒說：「有錢財的人進神的國是何等地難哪！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus, looking around, *said to His disciples, “How hard it will be for those who are wealthy to enter the kingdom of God!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus looked around and said to his disciples, “How hard it is for the rich to enter the kingdom of God!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus looked around and said to his disciples, “How hard it is for the rich to enter the Kingdom of God!”"
      }
