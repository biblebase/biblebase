        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "死人复活之后，将不娶也不嫁，就像天上的天使一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為人從死裡復活以後，也不娶，也不嫁，而是像天上的天使一樣。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "当人从死人中复活以后，他们既不娶也不嫁，而是像诸天之上的天使一样。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人從死裏復活，也不娶也不嫁，乃像天上的使者一樣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For when they rise from the dead, they neither marry nor are given in marriage, but are like angels in heaven."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For when they rise from the dead, they neither marry nor are given in marriage, but are like angels in heaven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the dead rise, they will neither marry nor be given in marriage; they will be like the angels in heaven."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For when they rise from the dead, they neither marry nor are given in marriage, but are like angels in heaven."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For when the dead rise, they will neither marry nor be given in marriage. In this respect they will be like the angels in heaven."
      }
