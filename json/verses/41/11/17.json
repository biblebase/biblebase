    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂教导他们说：“圣经上不是记载‘我的殿必称为万民祷告的殿’吗？你们竟把它变成了贼窝。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又教訓眾人說：“經上不是寫著 ‘我的殿要稱為萬國禱告的殿’嗎？ 你們竟把它弄成賊窩了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "便教訓他們說：「經上不是記着說： 我的殿必稱為萬國禱告的殿嗎？ 你們倒使它 成為賊窩了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He began to teach and say to them, “Is it not written, ‘My house shall be called a house of prayer for all the nations’? But you have made it a robbers’ den.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And as he taught them, he said, “Is it not written: ‘My house will be called a house of prayer for all nations’? But you have made it ‘a den of robbers.’”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He said to them, “The Scriptures declare, ‘My Temple will be called a house of prayer for all nations,’ but you have turned it into a den of thieves.”"
      }
