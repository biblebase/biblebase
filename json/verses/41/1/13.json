    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂在旷野受撒旦的试探四十天。祂与野兽在一起，有天使服侍祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他在那裡四十天，受撒但的試探，和野獸在一起，有天使來服事他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他在曠野四十天，受撒但的試探，並與野獸同在一處，且有天使來伺候他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He was in the wilderness forty days being tempted by Satan; and He was with the wild beasts, and the angels were ministering to Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and he was in the wilderness forty days, being tempted by Satan. He was with the wild animals, and angels attended him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "where he was tempted by Satan for forty days. He was out among the wild animals, and angels took care of him."
      }
