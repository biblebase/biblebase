    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大约凌晨三点钟，祂看见门徒在逆风中摇橹，非常吃力，就从水面上朝门徒走去，想要从他们旁边经过。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看見門徒辛辛苦苦地搖櫓，因為風不順。天快亮的時候，耶穌在海面上向他們走去，想要趕過他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看見門徒因風不順，搖櫓甚苦。夜裏約有四更天，就在海面上走，往他們那裏去，意思要走過他們去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Seeing them straining at the oars, for the wind was against them, at about the fourth watch of the night He *came to them, walking on the sea; and He intended to pass by them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He saw the disciples straining at the oars, because the wind was against them. Shortly before dawn he went out to them, walking on the lake. He was about to pass by them,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He saw that they were in serious trouble, rowing hard and struggling against the wind and waves. About three o’clock in the morning Jesus came toward them, walking on the water. He intended to go past them,"
      }
    },
