    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "清早，祭司长、长老、律法教师和全公会的人商定后，便把耶稣绑起来押送到彼拉多那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "一到清晨，祭司長和長老、經學家以及公議會全體一致議決，把耶穌綁起來，押去交給彼拉多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "一到早晨，祭司長和長老、文士、全公會的人大家商議，就把耶穌捆綁，解去交給彼拉多。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Early in the morning the chief priests with the elders and scribes and the whole Council, immediately held a consultation; and binding Jesus, they led Him away and delivered Him to Pilate."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Very early in the morning, the chief priests, with the elders, the teachers of the law and the whole Sanhedrin, made their plans. So they bound Jesus, led him away and handed him over to Pilate."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Very early in the morning the leading priests, the elders, and the teachers of religious law—the entire high council—met to discuss their next step. They bound Jesus, led him away, and took him to Pilate, the Roman governor."
      }
