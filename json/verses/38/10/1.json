    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要在春季向耶和华求雨。 祂创造雷雨， 为世人降下甘霖， 使田间长出青翠之物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在春雨的季節，你們要向耶和華求雨。 耶和華閃發雷電， 給人降雨，使各人的田地生長蔬菜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當春雨的時候， 你們要向發閃電的耶和華求雨。 他必為眾人降下甘霖， 使田園生長菜蔬。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ask rain from the Lord at the time of the spring rain— The Lord who makes the storm clouds; And He will give them showers of rain, vegetation in the field to each man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ask the Lord for rain in the springtime; it is the Lord who sends the thunderstorms. He gives showers of rain to all people, and plants of the field to everyone."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ask the Lord for rain in the spring, for he makes the storm clouds. And he will send showers of rain so every field becomes a lush pasture."
      }
    },
