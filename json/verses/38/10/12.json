    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "zec.10.12",
            "paragraphs": [
              "12.這一節是本詩後半的中心，正如第6節為前半的中心，所重復的應許皆為我要堅固。四個希伯來字以簡潔的交錯式組成這裏的信息∶a.我要堅固他們；b.在耶和華裏；b1.奉祂的名；a1.他們要前行233。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必让他们靠着我强盛， 他们必奉我的名行事。 这是耶和华说的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必使我的子民因耶和華而強盛， 他們要奉耶和華的名行事為人。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必使他們倚靠我，得以堅固； 一舉一動必奉我的名。 這是耶和華說的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I will make them strong in the Lord, and they shall walk in his name,” declares the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And I will strengthen them in the Lord, And in His name they will walk,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will strengthen them in the Lord and in his name they will live securely,” declares the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“So I will strengthen them in the Lord, And they shall walk up and down in His name,” Says the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "By my power I will make my people strong, and by my authority they will go wherever they wish. I, the Lord, have spoken!”"
      }
    },
