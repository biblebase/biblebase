    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "num.29.7",
            "paragraphs": [
              "二九7-11關於贖罪日，參看利未記十六章的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“七月十日，你们要举行圣会，禁食，不可工作。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘在七月十日，你們要有聖會，刻苦己心，甚麼都不可作，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「七月初十日，你們當有聖會；要刻苦己心，甚麼工都不可做。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Then on the tenth day of this seventh month you shall have a holy convocation, and you shall humble yourselves; you shall not do any work."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘On the tenth day of this seventh month hold a sacred assembly. You must deny yourselves and do no work."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Ten days later, on the tenth day of the same month, you must call another holy assembly. On that day, the Day of Atonement, the people must go without food and must do no ordinary work."
      }
    },
