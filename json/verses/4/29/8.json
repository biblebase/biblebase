      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要给耶和华献上馨香的燔祭，即毫无残疾的公牛犊一头、公绵羊一只和一岁的公羊羔七只；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只要把燔祭作馨香的祭獻給耶和華，就是一頭公牛犢、一隻公綿羊、七隻一歲沒有殘疾的公羊羔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只要將公牛犢一隻，公綿羊一隻，一歲的公羊羔七隻，都要沒有殘疾的，作為馨香的燔祭獻給耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall present a burnt offering to the Lord  as a soothing aroma: one bull, one ram, seven male lambs one year old, having them without defect;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Present as an aroma pleasing to the Lord a burnt offering of one young bull, one ram and seven male lambs a year old, all without defect."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must present a burnt offering as a pleasing aroma to the Lord. It will consist of one young bull, one ram, and seven one-year-old male lambs, all with no defects."
      }
