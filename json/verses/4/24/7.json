    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "num.24.7",
            "paragraphs": [
              "二四7\"亞甲\"是撒母耳所殺的王（撒上一五32,33）。巴蘭在此預言以色列在政治上的優勢。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们沐浴充沛的甘霖， 撒种于湿润的沃土。 他们的君王高过亚甲， 国度名震四方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "必有水從他的桶中流出來， 他的後裔必在多水之處； 他的王必超過亞甲， 他的國度必被高舉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "水要從他的桶裏流出； 種子要撒在多水之處。 他的王必超過亞甲； 他的國必要振興。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Water will flow from his buckets, And his seed will be by many waters, And his king shall be higher than Agag, And his kingdom shall be exalted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
