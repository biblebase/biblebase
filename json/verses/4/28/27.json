    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "num.28.26-31",
            "paragraphs": [
              "26～31.第二個國家性節期七七節，本段又稱之為「初熟果子日」（和合本：莊稼初熟……的日子）。這節期在大麥收成之後舉行（參：出二十三16；利二十三15～21；申十六9～12）。由於它發生在除酵節之後七個星期（五十日），希臘文稱之為五旬節，亦即是聖靈降臨節（Whitsun；徒二1）。這節獻祭的數目與月朔及除酵節相同。268"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要把两头公牛犊、一只公绵羊和七只一岁的公羊羔献给耶和华作馨香的燔祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只要把燔祭作為馨香的祭獻給耶和華，就是兩頭公牛犢、一隻公綿羊、七隻一歲的公羊羔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只要將公牛犢兩隻，公綿羊一隻，一歲的公羊羔七隻，作為馨香的燔祭獻給耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall offer a burnt offering for a soothing aroma to the Lord: two young bulls, one ram, seven male lambs one year old;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Present a burnt offering of two young bulls, one ram and seven male lambs a year old as an aroma pleasing to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Present a special burnt offering on that day as a pleasing aroma to the Lord. It will consist of two young bulls, one ram, and seven one-year-old male lambs."
      }
    },
