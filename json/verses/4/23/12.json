    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "num.23.11-12",
            "paragraphs": [
              "11～12.巴蘭肯定以色列在神計畫中的特殊地位，當然不是巴勒想要聽的話。和以後的資訊相比，第一個資訊並沒有很詳細地解釋神怎樣幫助以色列；然而耶和華所支持的不是摩押而是以色列，卻是再清楚不過的事。故此巴勒暴跳如雷。巴蘭則請他不要忘記自己受聘時所約定的條件：耶和華傳給我的話，我能不謹慎傳說嗎？（12節；參5節；二十二38）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴兰说：“耶和华让我说的话，我岂能不说？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴蘭回答，說：“耶和華傳給我的話，我怎能不謹慎說呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他回答說：「耶和華傳給我的話，我能不謹慎傳說嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He replied, “Must I not be careful to speak what the Lord puts in my mouth?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He answered, “Must I not speak what the Lord puts in my mouth?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Balaam replied, “I will speak only the message that the Lord puts in my mouth.”"
      }
    },
