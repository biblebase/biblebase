    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "没有邪术能害雅各， 没有咒语可伤以色列。 时候一到，人们必谈论雅各， 谈论以色列，说，‘看啊， 上帝的作为何等伟大！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在雅各中沒有觀兆的， 在以色列中也沒有占卜； 到了適當的時候，必有話對雅各和以色列說： ‘　神為他們行了何等的大事！’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "斷沒有法術可以害雅各， 也沒有占卜可以害以色列。 現在必有人論及雅各，就是論及以色列說： 神為他行了何等的大事！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For there is no omen against Jacob, Nor is there any divination against Israel; At the proper time it shall be said to Jacob And to Israel, what God has done!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There is no divination against Jacob, no evil omens against Israel. It will now be said of Jacob and of Israel, ‘See what God has done!’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No curse can touch Jacob; no magic has any power against Israel. For now it will be said of Jacob, ‘What wonders God has done for Israel!’"
      }
    },
