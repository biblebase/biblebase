    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "num.22.15-20",
            "paragraphs": [
              "iii.巴蘭第二次遇見神（二十二15～20）",
              "情節漸加緊張。又一批使臣被差遣來懇求巴蘭，比先前的又多又尊貴（15節），更談到巨額的報酬（17～18節）。然而巴蘭堅持透過夜間異象尋求神的旨意。這個異象記敘得比較簡短，結論卻出乎意外地改變了本來的政策。巴蘭可以去，但他只能說神容許的話，做神容許的事（20節）。本段故意要令聽者或讀者覺得意外，自問表面看來，神怎麽會改變初衷？神至終會否批準巴蘭咒詛以色列？接下來一連串緊張刺激的事件毫不含糊地解答了這類問題。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们可以在这里过夜，看看耶和华对我还有什么吩咐。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在請你們今夜也住在這裡，等我得知耶和華還要向我說甚麼。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在我請你們今夜在這裏住宿，等我得知耶和華還要對我說甚麼。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So you, too, please stay here tonight, that I may know what more the Lord will say to me.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now please, you also stay here tonight, and I will find out what else the Lord will speak to me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now spend the night here so that I can find out what else the Lord will tell me.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now therefore, please, you also stay here tonight, that I may know what more the Lord will say to me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But stay here one more night, and I will see if the Lord has anything else to say to me.”"
      }
    },
