    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "num.14.7-9",
            "paragraphs": [
              "7～9.約書亞和迦勒的話與其他探子的話遙遙呼應153（十三27～29、32～33）。他們沒有排拒同伴有關當地居民的報告，只是堅持不當懼怕這些人。他們空有次神（demi-gods）的形貌，但那蔭庇他們的（直譯作「蔭」，在炎熱的近東，用這象喻描述神的保佑最是貼切不過；詩九十一，一二一5）已經離開他們，有耶和華與我們同在。整個均勢經已扭轉。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果耶和华喜悦我们，祂必把我们带到那片土地，将那奶蜜之乡赐给我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華若是喜悅我們，就必把我們領進那地，把那地賜給我們，那地原是流奶與蜜的地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華若喜悅我們，就必將我們領進那地，把地賜給我們；那地原是流奶與蜜之地。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If the Lord delights in us, he will bring us into this land and give it to us, a land that flows with milk and honey."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If the Lord is pleased with us, then He will bring us into this land and give it to us—a land which flows with milk and honey."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If the Lord is pleased with us, he will lead us into that land, a land flowing with milk and honey, and will give it to us."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "If the Lord  delights in us, then He will bring us into this land and give it to us, ‘a land which flows with milk and honey.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And if the Lord is pleased with us, he will bring us safely into that land and give it to us. It is a rich land flowing with milk and honey."
      }
    },
