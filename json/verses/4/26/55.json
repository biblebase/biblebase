    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "num.26.52-56",
            "paragraphs": [
              "52～56.土地必須按照各支派的大小比例來分配：支派大，得到的產業也會比較大。但為了避免爭論，確實地點則拈鬮決定。這通常視為發現神旨意的肯定有效方法（箴十六33，十八18）。藉拈鬮來決定土地的分配，從摩西之前直到如今，都可以在近東找得著例證。約書亞記十五章以下描述這些指令付諸實行。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人要按各自的支派抽签分地，承受产业。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然這樣，地還是要藉著抽籤分配；他們要按著自己父家支派的名字，承受產業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雖是這樣，還要拈鬮分地。他們要按着祖宗各支派的名字承受為業。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the land shall be divided by lot. They shall receive their inheritance according to the names of the tribes of their fathers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be sure that the land is distributed by lot. What each group inherits will be according to the names for its ancestral tribe."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you must assign the land by lot, and give land to each ancestral tribe according to the number of names on the list."
      }
    },
