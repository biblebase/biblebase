      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以上是膏抹祭坛时以色列各首领为献坛之礼所献的供物，共计有银盘、银碗和金碟子各十二个，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "用膏抹祭壇的日子，以色列的眾領袖獻上奉獻祭壇的供物，共有銀盤十二個、銀碗十二個、金盂十二個，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "用膏抹壇的日子，以色列的眾首領為行獻壇之禮所獻的是：銀盤子十二個，銀碗十二個，金盂十二個；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This was the dedication offering for the altar from the leaders of Israel when it was anointed: twelve silver dishes, twelve silver bowls, twelve gold pans,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These were the offerings of the Israelite leaders for the dedication of the altar when it was anointed: twelve silver plates, twelve silver sprinkling bowls and twelve gold dishes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So this was the dedication offering brought by the leaders of Israel at the time the altar was anointed: twelve silver platters, twelve silver basins, and twelve gold incense containers."
      }
