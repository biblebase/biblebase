    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为顾念罪恶本性的人与上帝为敌，不服从上帝的律法，也无力服从。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為以肉體為念就是與　神為仇，既不服從　神的律法，也的確不能夠服從；"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "原来属肉体的思想是敌对神的，因为它不服从神的律法，其实也不能服从；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "原來體貼肉體的，就是與神為仇；因為不服神的律法，也是不能服，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For the mind that is set on the flesh is hostile to God, for it does not submit to God’s law; indeed, it cannot."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "because the mind set on the flesh is hostile toward God; for it does not subject itself to the law of God, for it is not even able to do so,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The mind governed by the flesh is hostile to God; it does not submit to God’s law, nor can it do so."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Because the carnal mind is enmity against God; for it is not subject to the law of God, nor indeed can be."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the sinful nature is always hostile to God. It never did obey God’s laws, and it never will."
      }
    },
