      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要以恶报恶。要尊重别人，众人都认为好的事，应该竭力去做。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不可以惡報惡。大家以為美的事，要努力去作。"
      },
      "cunp": {
