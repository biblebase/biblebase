    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但如今福音传遍了这一带，况且我几年来一直盼望去你们那里，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但現在這一帶再沒有可傳的地方，而我多年來又很想去見你們，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但如今，在這裏再沒有可傳的地方，而且這好幾年，我切心想望到西班牙去的時候，可以到你們那裏，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but now, with no further place for me in these regions, and since I have had for many years a longing to come to you"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But now that there is no more place for me to work in these regions, and since I have been longing for many years to visit you,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But now I have finished my work in these regions, and after all these long years of waiting, I am eager to visit you."
      }
    },
