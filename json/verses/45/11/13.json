    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我有话要对你们外族人说，因为我是外族人的使徒，我尊重自己的职分，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我現在對你們外族人說話，因為我是外族人的使徒，所以尊重我的職分，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我對你們外邦人說這話；因我是外邦人的使徒，所以敬重我的職分，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I am speaking to you who are Gentiles. Inasmuch then as I am an apostle of Gentiles, I magnify my ministry,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am talking to you Gentiles. Inasmuch as I am the apostle to the Gentiles, I take pride in my ministry"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am saying all this especially for you Gentiles. God has appointed me as the apostle to the Gentiles. I stress this,"
      }
    },
