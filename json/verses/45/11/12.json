    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果他们的过犯给世界带来了富足，他们的失败给外族人带来了富足，那么，他们发愤回头的时候岂不要带来更大的祝福吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "既然他們的過犯可以使世人富足，他們的失敗可以使外族人富足，何況他們的豐盛呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若他們的過失為天下的富足，他們的缺乏為外邦人的富足，何況他們的豐滿呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now if their transgression is riches for the world and their failure is riches for the Gentiles, how much more will their fulfillment be!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if their transgression means riches for the world, and their loss means riches for the Gentiles, how much greater riches will their full inclusion bring!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now if the Gentiles were enriched because the people of Israel turned down God’s offer of salvation, think how much greater a blessing the world will share when they finally accept it."
      }
