    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那么，你这教导别人的，为什么不教导自己呢？你教导人不可偷盗，自己却偷盗吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你既然教導別人，難道不教導自己嗎？你傳講不可偷竊，自己卻偷竊嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你既是教導別人，還不教導自己嗎？你講說人不可偷竊，自己還偷竊嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "you, therefore, who teach another, do you not teach yourself? You who preach that one shall not steal, do you steal?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "you, then, who teach others, do you not teach yourself? You who preach against stealing, do you steal?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Well then, if you teach others, why don’t you teach yourself? You tell others not to steal, but do you steal?"
      }
