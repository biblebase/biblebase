    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然有些人不相信，但那有什么关系呢？难道上帝的信实可靠会因他们的不信而化为无有吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "即使有人不信，又有甚麼關係呢？難道他們的不信會使　神的信實無效嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "即便有不信的，這有何妨呢？難道他們的不信就廢掉神的信嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "What then? If some did not believe, their unbelief will not nullify the faithfulness of God, will it?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What if some were unfaithful? Will their unfaithfulness nullify God’s faithfulness?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "True, some of them were unfaithful; but just because they were unfaithful, does that mean God will be unfaithful?"
      }
    },
