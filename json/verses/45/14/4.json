    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你是谁，竟然批评别人的仆人？他做得是否合宜，自然有他的主人负责。他必能做得合宜，因为主能使他做得合宜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你是誰，竟然批評別人的家僕呢？他或站穩或跌倒，只和自己的主人有關；但他必定站穩，因為主能夠使他站穩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你是誰，竟論斷別人的僕人呢？他或站住或跌倒，自有他的主人在；而且他也必要站住，因為主能使他站住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who are you to judge the servant of another? To his own master he stands or falls; and he will stand, for the Lord is able to make him stand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who are you to judge someone else’s servant? To their own master, servants stand or fall. And they will stand, for the Lord is able to make them stand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Who are you to condemn someone else’s servants? Their own master will judge whether they stand or fall. And with the Lord’s help, they will stand and receive his approval."
      }
