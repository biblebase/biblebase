    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把上帝的真理当作谎言，祭拜、供奉受造之物，却不敬拜、事奉造物主。主是永远值得称颂的。阿们！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們用虛謊取代了　神的真理，敬拜事奉受造之物，卻不敬拜事奉造物的主。他是永遠可稱頌的，阿們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們將神的真實變為虛謊，去敬拜事奉受造之物，不敬奉那造物的主－主乃是可稱頌的，直到永遠。阿們！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they exchanged the truth of God for a lie, and worshiped and served the creature rather than the Creator, who is blessed forever. Amen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They exchanged the truth about God for a lie, and worshiped and served created things rather than the Creator—who is forever praised. Amen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They traded the truth about God for a lie. So they worshiped and served the things God created instead of the Creator himself, who is worthy of eternal praise! Amen."
      }
    },
