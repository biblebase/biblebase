    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我在傳揚他兒子福音的事上，用心靈事奉的　神，可以作證我是怎樣不斷地記念你們，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我在他兒子福音上，用心靈所事奉的神可以見證，我怎樣不住地提到你們；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For God, whom I serve in my spirit in the preaching of the gospel of His Son, is my witness as to how unceasingly I make mention of you,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God, whom I serve in my spirit in preaching the gospel of his Son, is my witness how constantly I remember you"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God knows how often I pray for you. Day and night I bring you and your needs in prayer to God, whom I serve with all my heart by spreading the Good News about his Son."
      }
    },
