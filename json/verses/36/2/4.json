    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "迦萨必被抛弃， 亚实基伦必沦为废墟， 亚实突的居民必在中午被赶走， 以革伦必被连根拔除。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為迦薩必被拋棄， 亞實基倫必成荒場， 亞實突的居民必在晌午被趕走； 以革倫必連根拔起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "迦薩必致見棄； 亞實基倫必然荒涼。 人在正午必趕出亞實突的民； 以革倫也被拔出根來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For Gaza will be abandoned And Ashkelon a desolation; Ashdod will be driven out at noon And Ekron will be uprooted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Gaza will be abandoned and Ashkelon left in ruins. At midday Ashdod will be emptied and Ekron uprooted."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Gaza and Ashkelon will be abandoned, Ashdod and Ekron torn down."
      }
