    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "2sa.24.1-25",
            "paragraphs": [
              "Ａ'神的審判復臨以色列（二十四1～25）",
              "撒母耳記下最後這個插曲，顯出大衛正在軟弱的時刻，而以色列人需要矯正。比大衛的聲望更重要的一些事正瀕臨危險；然而，他的悔改以及對百姓利益的關切，卻給自己和以色列人帶來益處。"
            ]
          },
          {
            "verse": "2sa.24.20-21",
            "paragraphs": [
              "20～21.亞勞拿「向下」（RSV）觀看：禾場通常是在高處，以便利用每一陣風；這裏是指大衛城北方的某地區，聖殿所在的位置可以俯瞰汲淪溪谷，當然是個非常適合的位置，然而在這記載中並沒有提及聖殿（對照：代上二十一1）。瘟疫造成的死亡威脅，使禾場的議價變得異常急迫，以便能夠獻上祭物。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚劳拿看见王及他的臣仆来了，就上前俯伏在地，向王跪拜，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞勞拿往下觀看，看見王和他的臣僕正朝著他這邊走過來，就出去迎接，臉伏於地向王叩拜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞勞拿觀看，見王和他臣僕前來，就迎接出去，臉伏於地，向王下拜，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And when Araunah looked down, he saw the king and his servants coming on toward him. And Araunah went out and paid homage to the king with his face to the ground."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Araunah looked down and saw the king and his servants crossing over toward him; and Araunah went out and bowed his face to the ground before the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Araunah looked and saw the king and his officials coming toward him, he went out and bowed down before the king with his face to the ground."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now Araunah looked, and saw the king and his servants coming toward him. So Araunah went out and bowed before the king with his face to the ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Araunah saw the king and his men coming toward him, he came and bowed before the king with his face to the ground."
      }
    },
