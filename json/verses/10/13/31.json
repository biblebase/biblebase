    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "2sa.13.23-39",
            "paragraphs": [
              "ii.押沙龍復仇（十三23～39）"
            ]
          },
          {
            "verse": "2sa.13.30-31",
            "paragraphs": [
              "30～31.一個報信的人受到這突如其來的驚嚇，余悸猶存地將發生之事訛傳至王宮，說押沙龍將王的眾子都殺了，甚至更誇張地說：沒有留下一個，仿佛要確定消息是可信的。然而王卻將這份通報照單全收，悲不自禁。這件可怕的事豈不是他造成的嗎？因為是他允許所有兒子離開京城，尤其是讓暗嫩去。王的朝臣全都與沮喪的王一同舉哀。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "2sa.13.30-33",
            "paragraphs": [
              "一三30～33大衛所聽到的風聲是誇大的；約拿達看出真相，叫王安心。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王便站起来撕裂衣服，躺在地上。他的臣仆也都撕裂衣服，站在旁边。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王就起來，撕裂衣服，躺在地上。侍立在他身旁的所有臣僕，也都撕裂衣服。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王就起來，撕裂衣服，躺在地上。王的臣僕也都撕裂衣服，站在旁邊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king arose, tore his clothes and lay on the ground; and all his servants were standing by with clothes torn."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king stood up, tore his clothes and lay down on the ground; and all his attendants stood by with their clothes torn."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king got up, tore his robe, and threw himself on the ground. His advisers also tore their clothes in horror and sorrow."
      }
