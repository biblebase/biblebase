    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约拿达对大卫王说：“请看，王子们都回来了！正如仆人所言。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約拿達對王說：“看哪！王的兒子都回來了。正如你僕人所說的，事情果然是這樣。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約拿達對王說：「看哪，王的眾子都來了，果然與你僕人所說的相合。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jonadab said to the king, “Behold, the king’s sons have come; according to your servant’s word, so it happened.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jonadab said to the king, “See, the king’s sons have come; it has happened just as your servant said.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Look!” Jonadab told the king. “There they are now! The king’s sons are coming, just as I said.”"
      }
