    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "事后暗嫩非常憎恨她玛，对她的恨比以前对她的爱还强烈。他对她玛说：“你起来走吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "事後，暗嫩非常恨她；他對他瑪的恨比以前對她的愛更大，所以暗嫩對她說：“你起來！走吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "隨後，暗嫩極其恨她，那恨她的心比先前愛她的心更甚，對她說：「你起來，去吧！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Amnon hated her with a very great hatred; for the hatred with which he hated her was greater than the love with which he had loved her. And Amnon said to her, “Get up, go away!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Amnon hated her with intense hatred. In fact, he hated her more than he had loved her. Amnon said to her, “Get up and get out!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then suddenly Amnon’s love turned to hate, and he hated her even more than he had loved her. “Get out of here!” he snarled at her."
      }
