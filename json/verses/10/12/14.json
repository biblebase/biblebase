    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2sa.12.14",
            "paragraphs": [
              "一二14為了神在列國中聖潔的聲譽，犯奸淫的罪是必須受到審判的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你做的丑事给了仇敌亵渎耶和华的机会，所以你这个孩子必定死。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只是因為你作了這事，使耶和華的仇敵大得褻瀆的機會，所以你所生的兒子必定死。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是你行這事，叫耶和華的仇敵大得褻瀆的機會，故此，你所得的孩子必定要死。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "However, because by this deed you have given occasion to the enemies of the Lord to blaspheme, the child also that is born to you shall surely die.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But because by doing this you have shown utter contempt for the Lord, the son born to you will die.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Nevertheless, because you have shown utter contempt for the word of the Lord by doing this, your child will die.”"
      }
    },
