      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫的军队杀了三百六十个跟随押尼珥的便雅悯人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但大衛的臣僕擊殺便雅憫人和跟隨押尼珥的人，共有三百六十人死了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但大衛的僕人殺了便雅憫人和跟隨押尼珥的人，共三百六十名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the servants of David had struck down many of Benjamin and Abner’s men, so that three hundred and sixty men died."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But David’s men had killed three hundred and sixty Benjamites who were with Abner."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But 360 of Abner’s men had been killed, all from the tribe of Benjamin."
      }
    },
