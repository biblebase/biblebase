    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿耶和华以慈爱和信实待你们，我也会厚待你们，因为你们做了这事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們既作了這事，現在就願耶和華以慈愛和誠實待你們，我也要善待你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們既行了這事，願耶和華以慈愛誠實待你們，我也要為此厚待你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now may the Lord show lovingkindness and truth to you; and I also will show this goodness to you, because you have done this thing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the Lord now show you kindness and faithfulness, and I too will show you the same favor because you have done this."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May the Lord be loyal to you in return and reward you with his unfailing love! And I, too, will reward you for what you have done."
      }
    },
