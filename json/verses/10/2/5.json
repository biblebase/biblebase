    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就派人去告诉他们：“你们这样厚待你们的主人扫罗，把他安葬，愿耶和华赐福给你们！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛就差派使者到基列．雅比人那裡去，對他們說：“願耶和華賜福給你們，因為你們厚待了你們的主掃羅，把他埋葬了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛就差人去見基列‧雅比人，對他們說：「你們厚待你們的主－掃羅，將他葬埋。願耶和華賜福與你們！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David sent messengers to the men of Jabesh-gilead, and said to them, “May you be blessed of the Lord because you have shown this kindness to Saul your lord, and have buried him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "he sent messengers to them to say to them, “The Lord bless you for showing this kindness to Saul your master by burying him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "he sent them this message: “May the Lord bless you for being so loyal to your master Saul and giving him a decent burial."
      }
    },
