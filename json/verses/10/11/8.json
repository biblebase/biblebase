    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后吩咐他回家休息。乌利亚离开王宫后，收到大卫送的一份礼物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛對烏利亞說：“你回家去，洗洗腳吧！”烏利亞就離開了王宮。隨後王的一份禮物也跟著送去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛對烏利亞說：「你回家去，洗洗腳吧！」烏利亞出了王宮，隨後王送他一分食物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David said to Uriah, “Go down to your house, and wash your feet.” And Uriah went out of the king’s house, and a present from the king was sent out after him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then David said to Uriah, “Go down to your house and wash your feet.” So Uriah left the palace, and a gift from the king was sent after him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he told Uriah, “Go on home and relax.” David even sent a gift to Uriah after he had left the palace."
      }
