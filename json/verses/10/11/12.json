    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫对乌利亚说：“你再住一天，明天我会派你回去。”于是乌利亚那日就留在耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛對烏利亞說：“你今天還留在這裡，明天我才打發你去。”那一天，烏利亞就留在耶路撒冷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛吩咐烏利亞說：「你今日仍住在這裏，明日我打發你去。」於是烏利亞那日和次日住在耶路撒冷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David said to Uriah, “Stay here today also, and tomorrow I will let you go.” So Uriah remained in Jerusalem that day and the next."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then David said to him, “Stay here one more day, and tomorrow I will send you back.” So Uriah remained in Jerusalem that day and the next."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Well, stay here today,” David told him, “and tomorrow you may return to the army.” So Uriah stayed in Jerusalem that day and the next."
      }
