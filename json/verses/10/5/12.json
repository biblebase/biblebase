      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛就知道耶和華已經立他作以色列的王，又因自己的子民以色列的緣故，使他的國興盛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛就知道耶和華堅立他作以色列王，又為自己的民以色列使他的國興旺。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And David realized that the Lord had established him as king over Israel, and that He had exalted his kingdom for the sake of His people Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then David knew that the Lord had established him as king over Israel and had exalted his kingdom for the sake of his people Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And David realized that the Lord had confirmed him as king over Israel and had blessed his kingdom for the sake of his people Israel."
      }
