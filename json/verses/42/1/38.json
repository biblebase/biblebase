    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛丽亚说：“我是主的婢女，愿你所说的话在我身上成就。”于是天使离开了她。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "馬利亞說：“我是主的婢女，願照你的話成就在我身上！”天使就離開她去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "馬利亞說：「我是主的使女，情願照你的話成就在我身上。」天使就離開她去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Mary said, “Behold, the bondslave of the Lord; may it be done to me according to your word.” And the angel departed from her."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I am the Lord’s servant,” Mary answered. “May your word to me be fulfilled.” Then the angel left her."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Mary responded, “I am the Lord’s servant. May everything you have said about me come true.” And then the angel left her."
      }
    },
