    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "luk.22.56-57",
            "paragraphs": [
              "56～57.四卷福音書全都同意︰第一次的挑戰是來自一個使女，約翰更說她是看門的人。她努力地看著彼得（定睛看他的意思是「凝視著他」，指一種仔細的查看），然後說︰這個人素來也是同那人一夥的。從一個小使女的口中說出這樣的評論，算不得甚麽可怕的考驗。我們沒有聽到門徒們受到甚麽控訴，所以沒有明顯的理由可以說明彼得為甚麽不承認。但他是置身於仇敵中間，充滿了懼怕；他采取比較容易脫身的方法，說︰我不認得祂！"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "matangna": {
        "content": [
          {
            "verse": "luk.22.54-62",
            "paragraphs": [
              "十一．彼得不認主並痛哭（二二54～62）"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有个婢女看见彼得坐在火堆边，打量他一番后，说：“这人是与耶稣一伙的！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一個使女，見他面向火光坐著，就注視他，說：“這人是和他一夥的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一個使女看見彼得坐在火光裏，就定睛看他，說：「這個人素來也是同那人一夥的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And a servant-girl, seeing him as he sat in the firelight and looking intently at him, said, “This man was with Him too.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A servant girl saw him seated there in the firelight. She looked closely at him and said, “This man was with him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A servant girl noticed him in the firelight and began staring at him. Finally she said, “This man was one of Jesus’ followers!”"
      }
    },
