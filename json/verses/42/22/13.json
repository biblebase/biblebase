    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们进了城，所遇见的果然和耶稣所说的一样。他们便在那里预备逾越节的晚餐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們去了，所遇見的正像耶穌所說的一樣，就預備好了逾越節的晚餐。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們去了，所遇見的正如耶穌所說的；他們就預備了逾越節的筵席。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they left and found everything just as He had told them; and they prepared the Passover."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They left and found things just as Jesus had told them. So they prepared the Passover."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They went off to the city and found everything just as Jesus had said, and they prepared the Passover meal there."
      }
