    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣快到城门口时，从城里走出一队送殡的人，死者是一个寡妇的独子，有许多城中的人陪着她。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他走近城門的時候，有人把一個死人抬出來，是個獨生子，他母親又是個寡婦。城裡有一大群人陪著她。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將近城門，有一個死人被擡出來。這人是他母親獨生的兒子；他母親又是寡婦。有城裏的許多人同着寡婦送殯。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now as He approached the gate of the city, a dead man was being carried out, the only son of his mother, and she was a widow; and a sizeable crowd from the city was with her."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As he approached the town gate, a dead person was being carried out—the only son of his mother, and she was a widow. And a large crowd from the town was with her."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A funeral procession was coming out as he approached the village gate. The young man who had died was a widow’s only son, and a large crowd from the village was with her."
      }
