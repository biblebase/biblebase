    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "请耶稣的法利赛人看在眼里，心想：“如果这人真的是先知，就该知道摸祂的是谁，是个什么样的女人，她是个罪人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "請他的法利賽人看見了，心裡說：“這人若是先知，必定知道摸他的是誰，是怎樣的女人，因為她是個罪人！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "請耶穌的法利賽人看見這事，心裏說：「這人若是先知，必知道摸他的是誰，是個怎樣的女人；乃是個罪人。」"
      },
      "nasb": {
