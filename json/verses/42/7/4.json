    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们便来恳求耶稣，说：“这位百夫长值得你帮助，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "長老們就來見耶穌，懇切地求他說：“你給他行這事，是他配得的，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們到了耶穌那裏，就切切地求他說：「你給他行這事是他所配得的；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they came to Jesus, they earnestly implored Him, saying, “He is worthy for You to grant this to him;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they came to Jesus, they pleaded earnestly with him, “This man deserves to have you do this,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So they earnestly begged Jesus to help the man. “If anyone deserves your help, he does,” they said,"
      }
