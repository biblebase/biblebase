    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那从前被鬼附身的人恳求跟耶稣同行，耶稣却对他说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "鬼已經離開的那人求耶穌，要跟他在一起；但耶穌打發他回去，說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "鬼所離開的那人懇求和耶穌同在；耶穌卻打發他回去，說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the man from whom the demons had gone out was begging Him that he might accompany Him; but He sent him away, saying,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The man from whom the demons had gone out begged to go with him, but Jesus sent him away, saying,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The man who had been freed from the demons begged to go with him. But Jesus sent him home, saying,"
      }
    },
