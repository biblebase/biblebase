    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣还在说话的时候，有人从雅鲁家赶来，对雅鲁说：“你的女儿已经死了，不用麻烦老师了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌還在說話的時候，有人從會堂主管家裡來，說：“你的女兒死了，不必再勞動老師了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "還說話的時候，有人從管會堂的家裏來，說：「你的女兒死了，不要勞動夫子。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While He was still speaking, someone *came from the house of the synagogue official, saying, “Your daughter has died; do not trouble the Teacher anymore.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While Jesus was still speaking, someone came from the house of Jairus, the synagogue leader. “Your daughter is dead,” he said. “Don’t bother the teacher anymore.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "While he was still speaking to her, a messenger arrived from the home of Jairus, the leader of the synagogue. He told him, “Your daughter is dead. There’s no use troubling the Teacher now.”"
      }
