    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "格拉森地区的居民都十分害怕，请求耶稣离开那里，耶稣便乘船回去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "格拉森一帶的人，都要求耶穌離開他們，因為他們大大懼怕。他就上船回去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "格拉森四圍的人，因為害怕得很，都求耶穌離開他們；耶穌就上船回去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And all the people of the country of the Gerasenes and the surrounding district asked Him to leave them, for they were gripped with great fear; and He got into a boat and returned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then all the people of the region of the Gerasenes asked Jesus to leave them, because they were overcome with fear. So he got into the boat and left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And all the people in the region of the Gerasenes begged Jesus to go away and leave them alone, for a great wave of fear swept over them. So Jesus returned to the boat and left, crossing back to the other side of the lake."
      }
    },
