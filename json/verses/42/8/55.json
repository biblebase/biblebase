    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她的灵魂就回来了，她立刻站了起来。耶稣吩咐人给她东西吃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她的靈魂回來了，她就立刻起來。耶穌吩咐給她東西吃。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她的靈魂便回來，她就立刻起來了。耶穌吩咐給她東西吃。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And her spirit returned, and she got up immediately; and He gave orders for something to be given her to eat."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Her spirit returned, and at once she stood up. Then Jesus told them to give her something to eat."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And at that moment her life returned, and she immediately stood up! Then Jesus told them to give her something to eat."
      }
