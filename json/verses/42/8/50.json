    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣听见，就对雅鲁说：“不要怕，只要信。你的女儿一定会好的！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌聽見就對他說：“不要怕，只要信，她必得痊愈。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌聽見就對他說：「不要怕，只要信！你的女兒就必得救。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when Jesus heard this, He answered him, “Do not be afraid any longer; only believe, and she will be made well.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hearing this, Jesus said to Jairus, “Don’t be afraid; just believe, and she will be healed.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when Jesus heard what had happened, he said to Jairus, “Don’t be afraid. Just have faith, and she will be healed.”"
      }
