    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "质问祂：“告诉我们，你凭什么权柄做这些事？谁授权给你了？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對他說：“告訴我們，你憑著甚麼權柄作這些事？給你這權柄的是誰？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "問他說：「你告訴我們，你仗着甚麼權柄做這些事？給你這權柄的是誰呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they spoke, saying to Him, “Tell us by what authority You are doing these things, or who is the one who gave You this authority?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Tell us by what authority you are doing these things,” they said. “Who gave you this authority?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They demanded, “By what authority are you doing all these things? Who gave you the right?”"
      }
