    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是他们把园主的儿子拖到葡萄园外杀了。 “那么，园主会怎样处治他们呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他們把園主的愛子推出葡萄園外殺了。主人要怎樣對待他們呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是把他推出葡萄園外，殺了。這樣，葡萄園的主人要怎樣處治他們呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they threw him out of the vineyard and killed him. What, then, will the owner of the vineyard do to them?"
      },
