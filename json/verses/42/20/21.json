    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些密探问耶稣：“老师，我们知道你所讲所传的道都是正确的，也知道你不看人的情面，只按真理传上帝的道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "奸細問他：“老師，我們知道你所說所教的都對，並且不講情面，只照著真理把　神的道教導人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "奸細就問耶穌說：「夫子，我們曉得你所講所傳都是正道，也不取人的外貌，乃是誠誠實實傳神的道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They questioned Him, saying, “Teacher, we know that You speak and teach correctly, and You are not partial to any, but teach the way of God in truth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the spies questioned him: “Teacher, we know that you speak and teach what is right, and that you do not show partiality but teach the way of God in accordance with the truth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Teacher,” they said, “we know that you speak and teach what is right and are not influenced by what others think. You teach the way of God truthfully."
      }
    },
