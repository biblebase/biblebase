    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "审判官总是充耳不闻，但后来心里想，‘虽然我不怕上帝，也不在乎人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他多次不肯，後來心裡說：‘我雖然不懼怕　神，也不尊敬人，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他多日不准， 後來心裏說：『我雖不懼怕神，也不尊重世人，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For a while he was unwilling; but afterward he said to himself, ‘Even though I do not fear God nor respect man,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“For some time he refused. But finally he said to himself, ‘Even though I don’t fear God or care what people think,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The judge ignored her for a while, but finally he said to himself, ‘I don’t fear God or care about people,"
      }
