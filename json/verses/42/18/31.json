    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣把十二使徒带到一边，对他们说：“你们要留意，我们现在前往耶路撒冷，先知书上有关人子的记载都要应验，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌把十二門徒帶到一邊，對他們說：“我們現在上耶路撒冷去，先知所寫的一切，都要成就在人子身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌帶着十二個門徒，對他們說：「看哪，我們上耶路撒冷去，先知所寫的一切事都要成就在人子身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He took the twelve aside and said to them, “Behold, we are going up to Jerusalem, and all things which are written through the prophets about the Son of Man will be accomplished."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus took the Twelve aside and told them, “We are going up to Jerusalem, and everything that is written by the prophets about the Son of Man will be fulfilled."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Taking the twelve disciples aside, Jesus said, “Listen, we’re going up to Jerusalem, where all the predictions of the prophets concerning the Son of Man will come true."
      }
