    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是，他们都找借口推辞。头一个说，‘我刚买了一块田，必须去看一看，请恕我不能参加。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人一致推辭，頭一個說：‘我買了一塊地，不得不去看一看，請原諒我。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人一口同音地推辭。頭一個說：『我買了一塊地，必須去看看。請你准我辭了。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But they all alike began to make excuses. The first one said to him, ‘I have bought a piece of land and I need to go out and look at it; please consider me excused.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But they all alike began to make excuses. The first said, ‘I have just bought a field, and I must go and see it. Please excuse me.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But they all began making excuses. One said, ‘I have just bought a field and must inspect it. Please excuse me.’"
      }
    },
