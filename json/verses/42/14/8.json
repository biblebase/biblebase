    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“参加婚宴的时候，不要坐在首位，因为或许有更尊贵的宾客来赴宴，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你被邀請參加婚筵的時候，不要自己坐在高位上，恐怕有比你更尊貴的客人也被請來；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你被人請去赴婚姻的筵席，不要坐在首位上，恐怕有比你尊貴的客被他請來；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“When you are invited by someone to a wedding feast, do not take the place of honor, for someone more distinguished than you may have been invited by him,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When someone invites you to a wedding feast, do not take the place of honor, for a person more distinguished than you may have been invited."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When you are invited to a wedding feast, don’t sit in the seat of honor. What if someone who is more distinguished than you has also been invited?"
      }
    },
