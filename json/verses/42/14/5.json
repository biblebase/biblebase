    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后又问他们：“如果你们有驴或牛在安息日掉进井里，难道你们不会立刻把它拉上来吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就對他們說：“你們中間誰的兒子或牛在安息日掉在井裡，不立刻把他拉上來呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "便對他們說：「你們中間誰有驢或有牛，在安息日掉在井裏，不立時拉牠上來呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to them, “Which one of you will have a son or an ox fall into a well, and will not immediately pull him out on a Sabbath day?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he asked them, “If one of you has a child  or an ox that falls into a well on the Sabbath day, will you not immediately pull it out?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he turned to them and said, “Which of you doesn’t work on the Sabbath? If your son or your cow falls into a pit, don’t you rush to get him out?”"
      }
