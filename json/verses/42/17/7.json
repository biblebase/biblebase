    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们谁会对种田或放羊回来的奴仆说，‘请赶快坐下来吃饭’？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們中間誰有僕人去耕地或是放羊，從田裡回來，你就對他說：‘快過來坐下吃飯’；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們誰有僕人耕地或是放羊，從田裏回來，就對他說：『你快來坐下吃飯』呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Which of you, having a slave plowing or tending sheep, will say to him when he has come in from the field, ‘Come immediately and sit down to eat’?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Suppose one of you has a servant plowing or looking after the sheep. Will he say to the servant when he comes in from the field, ‘Come along now and sit down to eat’?"
