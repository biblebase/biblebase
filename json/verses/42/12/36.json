    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "像奴仆们等候主人从婚宴回来。主人回来一叩门，奴仆就可以立即给他开门。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "像等候自己的主人從婚筵回來一樣，好叫你們在主人回來敲門時，立刻給他開門。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "自己好像僕人等候主人從婚姻的筵席上回來。他來到，叩門，就立刻給他開門。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be like men who are waiting for their master when he returns from the wedding feast, so that they may immediately open the door to him when he comes and knocks."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "like servants waiting for their master to return from a wedding banquet, so that when he comes and knocks they can immediately open the door for him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "as though you were waiting for your master to return from the wedding feast. Then you will be ready to open the door and let him in the moment he arrives and knocks."
      }
    },
