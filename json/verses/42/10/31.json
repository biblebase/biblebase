    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“刚好有位祭司经过，看见那人躺在地上，连忙从旁边绕过，继续赶路。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正好有一個祭司，從那條路下來，看見他，就從旁邊走過去了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "恰好有一个祭司从那条路下来，看见他，就从另一边走了过去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "偶然有一個祭司從這條路下來，看見他就從那邊過去了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now by chance a priest was going down that road, and when he saw him he passed by on the other side."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And by chance a priest was going down on that road, and when he saw him, he passed by on the other side."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A priest happened to be going down the same road, and when he saw the man, he passed by on the other side."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now by chance a certain priest came down that road. And when he saw him, he passed by on the other side."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“By chance a priest came along. But when he saw the man lying there, he crossed to the other side of the road and passed him by."
      }
