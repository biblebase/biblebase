    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“又比如一个妇人有十块钱，不小心弄丢了一块，难道她不点亮灯，打扫房子，仔细寻找一直到找着为止吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“或是一個女人，有十個銀幣，失去一個，哪能不點上燈，打掃屋子，細細地找，直到找著呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「或是一個婦人有十塊錢，若失落一塊，豈不點上燈，打掃屋子，細細地找，直到找着嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Or what woman, if she has ten silver coins and loses one coin, does not light a lamp and sweep the house and search carefully until she finds it?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Or suppose a woman has ten silver coins    and loses one. Doesn’t she light a lamp, sweep the house and search carefully until she finds it?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Or suppose a woman has ten silver coins and loses one. Won’t she light a lamp and sweep the entire house and search carefully until she finds it?"
      }
