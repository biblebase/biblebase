    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她找到后，会邀朋友邻居来，说，‘我丢的钱找到了，来和我一同庆祝吧！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "既找著了，就請朋友鄰舍來，說：‘請大家和我一同歡樂，因為我失去的那個銀幣，已經找到了。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "找着了，就請朋友鄰舍來，對他們說：『我失落的那塊錢已經找着了，你們和我一同歡喜吧！』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When she has found it, she calls together her friends and neighbors, saying, ‘Rejoice with me, for I have found the coin which I had lost!’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And when she finds it, she calls her friends and neighbors together and says, ‘Rejoice with me; I have found my lost coin.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when she finds it, she will call in her friends and neighbors and say, ‘Rejoice with me because I have found my lost coin.’"
      }
