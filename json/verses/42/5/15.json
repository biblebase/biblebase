    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "结果耶稣的名声传得更广了，成群的人聚集到祂那里听祂讲道、求祂医病。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但他的名聲卻越發傳揚出去，成群的人來聚集，要聽道，並且要使他們的疾病痊愈。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但耶穌的名聲越發傳揚出去。有極多的人聚集來聽道，也指望醫治他們的病。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the news about Him was spreading even farther, and large crowds were gathering to hear Him and to be healed of their sicknesses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet the news about him spread all the more, so that crowds of people came to hear him and to be healed of their sicknesses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But despite Jesus’ instructions, the report of his power spread even faster, and vast crowds came to hear him preach and to be healed of their diseases."
      }
    },
