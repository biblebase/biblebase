    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "结果耶稣的名声传得更广了，成群的人聚集到祂那里听祂讲道、求祂医病。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但他的名聲卻越發傳揚出去，成群的人來聚集，要聽道，並且要使他們的疾病痊愈。"
      },
      "cunp": {
