    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就上了西门的那条船，请西门把船稍微划离岸边，然后坐在船上教导众人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他上了西門的那一隻船，請他撐開，離岸不遠，就坐下，從船上教導眾人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一隻船是西門的，耶穌就上去，請他把船撐開，稍微離岸，就坐下，從船上教訓眾人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He got into one of the boats, which was Simon’s, and asked him to put out a little way from the land. And He sat down and began teaching the people from the boat."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He got into one of the boats, the one belonging to Simon, and asked him to put out a little from shore. Then he sat down and taught the people from the boat."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Stepping into one of the boats, Jesus asked Simon, its owner, to push it out into the water. So he sat in the boat and taught the crowds from there."
      }
