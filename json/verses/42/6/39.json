    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣又给他们讲了个比喻，说：“瞎子岂能给瞎子带路？二人岂不是要双双掉进坑里吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌又用比喻對他們說：“瞎子怎能給瞎子領路呢？兩個人不都要掉在坑裡嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌又用比喻對他們說：「瞎子豈能領瞎子，兩個人不是都要掉在坑裏嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He also spoke a parable to them: “A blind man cannot guide a blind man, can he? Will they not both fall into a pit?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He also told them this parable: “Can the blind lead the blind? Will they not both fall into a pit?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus gave the following illustration: “Can one blind person lead another? Won’t they both fall into a ditch?"
      }
    },
