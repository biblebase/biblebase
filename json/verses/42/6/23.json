    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时你们要欢喜雀跃，因为你们在天上有大赏赐！他们的祖先也曾这样恶待以前的先知。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時你們應該歡喜跳躍，因為你們在天上的賞賜是大的，他們的祖先對待先知也是這樣。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“在那一天，你们当欢喜，当欢然跳跃，因为看哪，你们在天上的报偿是大的。要知道，那些人的祖先也是同样对待先知们的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當那日，你們要歡喜跳躍，因為你們在天上的賞賜是大的。他們的祖宗待先知也是這樣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Rejoice in that day, and leap for joy, for behold, your reward is great in heaven; for so their fathers did to the prophets."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be glad in that day and leap for joy, for behold, your reward is great in heaven. For in the same way their fathers used to treat the prophets."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Rejoice in that day and leap for joy, because great is your reward in heaven. For that is how their ancestors treated the prophets."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Rejoice in that day and leap for joy! For indeed your reward is great in heaven, For in like manner their fathers did to the prophets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When that happens, be happy! Yes, leap for joy! For a great reward awaits you in heaven. And remember, their ancestors treated the ancient prophets that same way."
      }
    },
