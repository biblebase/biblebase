    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣的事迹很快便传到希律耳中，令他十分困惑，因为有人说：“约翰死而复活了”，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "分封王希律聽見所發生的一切事，猶豫不定，因為有人說：“約翰從死人中復活了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "分封的王希律聽見耶穌所做的一切事，就游移不定；因為有人說：「是約翰從死裏復活」；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Herod the tetrarch heard of all that was happening; and he was greatly perplexed, because it was said by some that John had risen from the dead,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now Herod the tetrarch heard about all that was going on. And he was perplexed because some were saying that John had been raised from the dead,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Herod Antipas, the ruler of Galilee, heard about everything Jesus was doing, he was puzzled. Some were saying that John the Baptist had been raised from the dead."
      }
    },
