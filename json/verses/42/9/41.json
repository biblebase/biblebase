    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣回答说：“唉！这又不信又败坏的世代啊！我要跟你们在一起容忍你们多久呢？把你的儿子带来吧。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說：“唉！這不信又乖謬的世代啊！我跟你們在一起，忍受你們，要到幾時呢？把你兒子帶到這裡來吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「噯！這又不信又悖謬的世代啊，我在你們這裏，忍耐你們要到幾時呢？將你的兒子帶到這裏來吧！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus answered and said, “You unbelieving and perverted generation, how long shall I be with you and put up with you? Bring your son here.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You unbelieving and perverse generation,” Jesus replied, “how long shall I stay with you and put up with you? Bring your son here.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus said, “You faithless and corrupt people! How long must I be with you and put up with you?” Then he said to the man, “Bring your son here.”"
      }
