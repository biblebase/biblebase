      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "貪財的法利賽人聽見這些話，就嗤笑耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法利賽人是貪愛錢財的；他們聽見這一切話，就嗤笑耶穌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the Pharisees, who were lovers of money, were listening to all these things and were scoffing at Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Pharisees, who loved money, heard all this and were sneering at Jesus."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Pharisees, who dearly loved their money, heard all this and scoffed at him."
      }
