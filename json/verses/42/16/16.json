    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约翰还没有来以前，律法书和先知书是你们的准则。他出来后，上帝国的福音开始广传，人人都在努力进上帝的国。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "律法和先知到約翰就結束了。從此，　神的國的福音就傳開了，人人都要努力進去。"
      },
      "cunp": {
