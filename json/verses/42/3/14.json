    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有些军人问：“我们该怎么办呢？”约翰说：“不可敲诈勒索，自己有粮饷就当知足。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "兵丁也問他：“至於我們，我們又應當作甚麼呢？”他說：“不要恐嚇，不要敲詐，當以自己的糧餉為滿足。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "有些当兵的也问他，说：“那么我们呢？我们该做什么呢？” 约翰说：“不要敲诈任何人，也不要勒索人，当以自己的粮饷为满足。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又有兵丁問他說：「我們當做甚麼呢？」約翰說：「不要以強暴待人，也不要訛詐人，自己有錢糧就當知足。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Soldiers also asked him, “And we, what shall we do?” And he said to them, “Do not extort money from anyone by threats or by false accusation, and be content with your wages.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Some soldiers were questioning him, saying, “And what about us, what shall we do?” And he said to them, “Do not take money from anyone by force, or accuse anyone falsely, and be content with your wages.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then some soldiers asked him, “And what should we do?” He replied, “Don’t extort money and don’t accuse people falsely—be content with your pay.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Likewise the soldiers asked him, saying, “And what shall we do?” So he said to them, “Do not intimidate anyone or accuse falsely, and be content with your wages.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What should we do?” asked some soldiers. John replied, “Don’t extort money or make false accusations. And be content with your pay.”"
      }
