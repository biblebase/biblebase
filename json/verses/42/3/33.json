        "text": "亞米拿達、亞當民、亞蘭、希斯崙、法勒斯、猶大、"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拿順是亞米拿達的兒子；亞米拿達是亞蘭的兒子；亞蘭是希斯崙的兒子；希斯崙是法勒斯的兒子；法勒斯是猶大的兒子；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "the son of Amminadab, the son of Admin, the son of Ram, the son of Hezron, the son of Perez, the son of Judah,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the son of Amminadab, the son of Ram, the son of Hezron, the son of Perez, the son of Judah,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Nahshon was the son of Amminadab. Amminadab was the son of Admin. Admin was the son of Arni. Arni was the son of Hezron. Hezron was the son of Perez. Perez was the son of Judah."
      }
