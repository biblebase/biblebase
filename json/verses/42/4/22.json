    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大家都称赞祂，听见祂那些充满恩典的话，都感到惊奇，就说：“这不是约瑟的儿子吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人稱讚他，希奇他口中所出的恩言，並且說：“這不是約瑟的兒子嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人都稱讚他，並希奇他口中所出的恩言；又說：「這不是約瑟的兒子嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And all were speaking well of Him, and wondering at the gracious words which were falling from His lips; and they were saying, “Is this not Joseph’s son?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All spoke well of him and were amazed at the gracious words that came from his lips. “Isn’t this Joseph’s son?” they asked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone spoke well of him and was amazed by the gracious words that came from his lips. “How can this be?” they asked. “Isn’t this Joseph’s son?”"
      }
