    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但以利亚并没有奉命去帮助她们任何人，只奉命去帮助住在西顿地区撒勒法的一个寡妇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以利亞沒有奉差遣往他們中間任何一個那裡去，只到西頓撒勒法的一個寡婦那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利亞並沒有奉差往她們一個人那裏去，只奉差往西頓的撒勒法一個寡婦那裏去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and yet Elijah was sent to none of them, but only to Zarephath, in the land of Sidon, to a woman who was a widow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet Elijah was not sent to any of them, but to a widow in Zarephath in the region of Sidon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet Elijah was not sent to any of them. He was sent instead to a foreigner—a widow of Zarephath in the land of Sidon."
      }
