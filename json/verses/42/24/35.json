    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "二人也把路上发生的事以及掰饼时认出主的经过述说了一遍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "兩個人就把在路上的事，和擘餅的時候他們怎樣認出耶穌來的事，都述說了一遍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "兩個人就把路上所遇見，和擘餅的時候怎麼被他們認出來的事，都述說了一遍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They began to relate their experiences on the road and how He was recognized by them in the breaking of the bread."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the two told what had happened on the way, and how Jesus was recognized by them when he broke the bread."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the two from Emmaus told their story of how Jesus had appeared to them as they were walking along the road, and how they had recognized him as he was breaking the bread."
      }
    },
