    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "门徒又惊又喜，半信半疑。耶稣问：“你们这里有吃的吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們歡喜到不敢相信，並且很驚奇。耶穌說：“你們這裡有甚麼吃的沒有？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們正喜得不敢信，並且希奇；耶穌就說：「你們這裏有甚麼吃的沒有？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While they still could not believe it because of their joy and amazement, He said to them, “Have you anything here to eat?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And while they still did not believe it because of joy and amazement, he asked them, “Do you have anything here to eat?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Still they stood there in disbelief, filled with joy and wonder. Then he asked them, “Do you have anything here to eat?”"
      }
