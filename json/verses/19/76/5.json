    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "强敌被掳掠，他们倒地而亡， 再也不能还手。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "心裡勇敢的人都被搶掠， 他們長睡不起； 所有大能的勇士都無力舉手。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心中勇敢的人都被搶奪； 他們睡了長覺，沒有一個英雄能措手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The stouthearted were plundered, They sank into sleep; And none of the warriors could use his hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The valiant lie plundered, they sleep their last sleep; not one of the warriors can lift his hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Our boldest enemies have been plundered. They lie before us in the sleep of death. No warrior could lift a hand against us."
      }
    },
