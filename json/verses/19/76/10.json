    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.76.10",
            "paragraphs": [
              "76:10",
              "要成全你的榮美：",
              "1.因著神向欺壓他百姓的人施行憤怒的審判，那些蒙拯救的人必要贊美他。",
              "2.神使惡人的忿怒轉化為他救拔屬他的人並為他們成就大事的良機；例如，法老向以色列大發怒氣卻成了神證明他行奇事的大能並救贖以色列人出離埃及的大好時機(出5~12章)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你向人类发怒为你带来荣耀， 你准备发尽你的烈怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人的忿怒必使你得稱讚， 人的餘怒必成為你的裝飾。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "人的怒火，也会使你得称赞； 怒火后的余剩之民，你也会用来束腰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人的忿怒要成全你的榮美； 人的餘怒，你要禁止。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Surely the wrath of man shall praise you; the remnant of wrath you will put on like a belt."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the wrath of man shall praise You; With a remnant of wrath You will gird Yourself."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely your wrath against mankind brings you praise, and the survivors of your wrath are restrained."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Surely the wrath of man shall praise You; With the remainder of wrath You shall gird Yourself."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Human defiance only enhances your glory, for you use it as a weapon."
      }
    },
