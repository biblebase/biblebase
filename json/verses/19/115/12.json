    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华眷顾我们，赐福给我们。 祂要赐福给以色列人， 赐福给亚伦家。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華顧念我們， 必賜福給我們； 他必賜福給以色列家， 賜福給亞倫家。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華向來眷念我們； 他還要賜福給我們： 要賜福給以色列的家， 賜福給亞倫的家。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord has been mindful of us; He will bless us; He will bless the house of Israel; He will bless the house of Aaron."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord remembers us and will bless us: He will bless his people Israel, he will bless the house of Aaron,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord remembers us and will bless us. He will bless the people of Israel and bless the priests, the descendants of Aaron."
      }
