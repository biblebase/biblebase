        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, do save, we beseech You; O Lord, we beseech You, do send prosperity!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, save us! Lord, grant us success!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Please, Lord, please save us. Please, Lord, please give us success."
      }
