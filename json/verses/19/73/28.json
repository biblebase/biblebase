    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.73.27-28",
            "paragraphs": [
              "七三27,28\"離棄你行邪淫的\"。即在靈裏對神不忠的。亞薩正確地下結論說，惡人必然衰亡，而縱然面對逆境，亞薩仍在神裏找到避難所。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对于我，到上帝面前是何等美好。 我以主耶和华为我的避难所， 并宣扬祂的一切作为。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對我來說，親近　神是美好的， 我以主耶和華為我的避難所； 我要述說你的一切作為。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我親近神是與我有益； 我以主耶和華為我的避難所， 好叫我述說你一切的作為。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, the nearness of God is my good; I have made the Lord God my refuge, That I may tell of all Your works."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But as for me, it is good to be near God. I have made the Sovereign Lord my refuge; I will tell of all your deeds."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But as for me, how good it is to be near God! I have made the Sovereign Lord my shelter, and I will tell everyone about the wonderful things you do."
      }
    },
