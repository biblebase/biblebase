    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.73.10",
            "paragraphs": [
              "七三10即使是神的子民，也會受引誘去做出不正當的行為。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝的子民也跟随他们， 听从他们的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此他的人民歸回那裡去， 並且喝光了大量的水。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以神的民歸到這裏， 喝盡了滿杯的苦水。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore his people return to this place, And waters of abundance are drunk by them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore their people turn to them and drink up waters in abundance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And so the people are dismayed and confused, drinking in all their words."
      }
    },
