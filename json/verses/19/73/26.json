    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尽管我身心俱衰， 上帝永远是我心中的力量， 永远属于我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的肉身和我的內心雖然漸漸衰弱， 　神卻永遠是我心裡的磐石，是我的業分。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的肉體和我的心腸衰殘； 但神是我心裏的力量， 又是我的福分，直到永遠。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My flesh and my heart may fail, But God is the strength of my heart and my portion forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My flesh and my heart may fail, but God is the strength of my heart and my portion forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My health may fail, and my spirit may grow weak, but God remains the strength of my heart; he is mine forever."
      }
    },
