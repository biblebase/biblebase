    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.73.2-3",
            "paragraphs": [
              "七三2,3\"我的腳幾乎失閃\"。由於嫉妒\"惡人……享平安\"，他對耶和華的信心幾乎失去。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我却身陷险地， 几乎失脚跌倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於我，我的腳幾乎滑跌， 我險些跌倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於我，我的腳幾乎失閃； 我的腳險些滑跌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, my feet came close to stumbling, My steps had almost slipped."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But as for me, my feet had almost slipped; I had nearly lost my foothold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But as for me, I almost lost my footing. My feet were slipping, and I was almost gone."
      }
    },
