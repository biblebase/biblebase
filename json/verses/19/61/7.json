    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他在你的护佑下永远做王， 求你以慈爱和信实来保护他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
