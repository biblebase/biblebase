    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.61.2-4",
            "paragraphs": [
              "六一2～4“那比我更高的盤石”。戴維不能靠自己的力量到達的庇護之所，而那裏能給他保護與安全。神就是那庇護之所，詩中用了四個比喻來描述：“避難所”、“堅固臺”（堡壘）、“帳幕”和“翅膀下的隱密處”。參看第五十七篇1節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我心里沮丧之时， 从地极呼求你， 求你领我到高高的磐石上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我內心沮喪的時候， 我從地極向你呼求； 求你領我到那比我更高的磐石上面去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我心裏發昏的時候， 我要從地極求告你。 求你領我到那比我更高的磐石！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the end of the earth I call to You when my heart is faint; Lead me to the rock that is higher than I."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the ends of the earth I call to you, I call as my heart grows faint; lead me to the rock that is higher than I."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From the ends of the earth, I cry to you for help when my heart is overwhelmed. Lead me to the towering rock of safety,"
      }
    },
