    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我心满意足，如享盛宴， 唱起欢快的歌赞美你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的心滿足，就像飽享了骨髓肥油； 我要用歡樂的嘴唇讚美你。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我的心就像饱尝了骨髓脂油， 我的口要以欢乐的嘴唇发出赞美。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "My soul will be satisfied as with fat and rich food, and my mouth will praise you with joyful lips,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My soul is satisfied as with marrow and fatness, And my mouth offers praises with joyful lips."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will be fully satisfied as with the richest of foods; with singing lips my mouth will praise you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "My soul shall be satisfied as with marrow and fatness, And my mouth shall praise You with joyful lips."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You satisfy me more than the richest feast. I will praise you with songs of joy."
      }
    },
