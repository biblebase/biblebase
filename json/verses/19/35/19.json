    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.35.19-28",
            "paragraphs": [
              "幸災樂禍（三十五19～28）",
              "無故的憎恨，是惡對善的基本反應（在第7節已經強調過），因此耶穌視第19節（及六十九4），不是指大衛意外的不幸，乃是指祂所預定的遭遇。這就是“他們律法上所寫的話”（約十五25），是有關即將發生之事的權威性啟示。這個模式在祂的案件上一展無遺，在大衛的一生中斷續可見，而我們也命定要如此經歷（約十五18以下）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.35.19-21",
            "paragraphs": [
              "三五19,21\"擠眼\"和\"大大張口攻擊我\"都是表示惡意和輕蔑的姿態。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你不要让无故攻击我的人幸灾乐祸， 不要让无故恨我的人沾沾自喜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你不容那些無理與我為敵的，向我誇耀； 不讓那些無故恨我的，向我擠眼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你不容那無理與我為仇的向我誇耀！ 不容那無故恨我的向我擠眼！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not let those who are wrongfully my enemies rejoice over me; Nor let those who hate me without cause wink maliciously."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not let those gloat over me who are my enemies without cause; do not let those who hate me without reason maliciously wink the eye."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t let my treacherous enemies rejoice over my defeat. Don’t let those who hate me without cause gloat over my sorrow."
      }
    },
