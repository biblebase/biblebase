    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.35.19-28",
            "paragraphs": [
              "幸災樂禍（三十五19～28）",
              "無故的憎恨，是惡對善的基本反應（在第7節已經強調過），因此耶穌視第19節（及六十九4），不是指大衛意外的不幸，乃是指祂所預定的遭遇。這就是“他們律法上所寫的話”（約十五25），是有關即將發生之事的權威性啟示。這個模式在祂的案件上一展無遺，在大衛的一生中斷續可見，而我們也命定要如此經歷（約十五18以下）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿幸灾乐祸的人蒙羞受辱， 自高自大的人无地自容！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願那些喜歡我遭難的， 一同蒙羞抱愧； 願那些對我妄自尊大的， 都披上慚愧和恥辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願那喜歡我遭難的一同抱愧蒙羞！ 願那向我妄自尊大的披慚愧，蒙羞辱！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let those be ashamed and humiliated altogether who rejoice at my distress; Let those be clothed with shame and dishonor who magnify themselves over me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May all who gloat over my distress be put to shame and confusion; may all who exalt themselves over me be clothed with shame and disgrace."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May those who rejoice at my troubles be humiliated and disgraced. May those who triumph over me be covered with shame and dishonor."
      }
    },
