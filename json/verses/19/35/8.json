    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.35.1-10",
            "paragraphs": [
              "詭計（三十五1～10）",
              "第1～3節的軍事用語是比喻，其實頭一個比方是借用法庭的狀況（相爭）。因此本詩適用於任何遭怨恨、被陷害的情形。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.35.7-8",
            "paragraphs": [
              "三五7,8詩中提到公義的經典例子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他们突遭祸患， 作茧自缚，自食恶果！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願毀滅在不知不覺間臨到他身上， 願他暗設的網羅纏住自己， 願他落在其中遭毀滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願災禍忽然臨到他身上！ 願他暗設的網纏住自己！ 願他落在其中遭災禍！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let destruction come upon him unawares, And let the net which he hid catch himself; Into that very destruction let him fall."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "may ruin overtake them by surprise— may the net they hid entangle them, may they fall into the pit, to their ruin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So let sudden ruin come upon them! Let them be caught in the trap they set for me! Let them be destroyed in the pit they dug for me."
      }
    },
