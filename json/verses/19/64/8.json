      "leili": {
        "content": [
          {
            "verse": "psa.64.8",
            "paragraphs": [
              "六四8他們的舌頭最終必把他們自己絆倒。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们必自作自受， 被自己的舌头所害。 看见的人都嘲笑他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們各人反被自己的舌頭所害，必然跌倒； 所有看見他們的，都必搖頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必然絆跌，被自己的舌頭所害； 凡看見他們的必都搖頭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they will make him stumble; Their own tongue is against them; All who see them will shake the head."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will turn their own tongues against them and bring them to ruin; all who see them will shake their heads in scorn."
      },
      "nlt": {
