    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "生活在这种光景里的人有福了！ 尊耶和华为上帝的人有福了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "得享這樣景況的人民，是有福的， 有耶和華作他們　神的，這人是有福的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "遇見這光景的百姓便為有福！ 有耶和華為他們的神，這百姓便為有福！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How blessed are the people who are so situated; How blessed are the people whose God is the Lord!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Blessed is the people of whom this is true; blessed is the people whose God is the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, joyful are those who live like this! Joyful indeed are those whose God is the Lord."
      }
    },
