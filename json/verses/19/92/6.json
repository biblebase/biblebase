    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.92.6",
            "paragraphs": [
              "九二6\"畜類（沒有常識的、愚蠢的）人\"不能明白神偉大的工作。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无知的人不会明白， 愚蠢的人也不会了解："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "無理性的人不曉得， 愚昧的人不明白這事："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "畜類人不曉得； 愚頑人也不明白。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A senseless man has no knowledge, Nor does a stupid man understand this:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Senseless people do not know, fools do not understand,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Only a simpleton would not know, and only a fool would not understand this:"
      }
