    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.17-39",
            "paragraphs": [
              "七八17～39概述以色列人在曠野飄流期間的反叛，以及神向他們所發的忿怒。"
            ]
          },
          {
            "verse": "psa.78.25",
            "paragraphs": [
              "七八25\"大能者的食物\"。從逃邙降的食物，配得給天使食用的食物。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们吃了天使的食物， 上帝赐给他们丰富的食物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是人吃了天使的食物， 是　神賜下的食糧，使他們飽足。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "各人吃大能者的食物； 他賜下糧食，使他們飽足。"
      },
      "nasb": {
