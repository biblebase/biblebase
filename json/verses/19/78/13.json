    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.12-16",
            "paragraphs": [
              "七八12～16\"瑣安\"。埃及東北面之歌珊的省會。簡述神在拯救以色列人時，於埃及施行的、與水有關的災難，以及與水有關的神跡。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂把海水分开， 带领他们安然渡过； 祂使海水堆起如墙壁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把海分開，領他們走過去， 又使海水直立像一道堤壩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他將海分裂，使他們過去， 又叫水立起如壘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He divided the sea and caused them to pass through, And He made the waters stand up like a heap."
      },
