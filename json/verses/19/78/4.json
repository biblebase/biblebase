    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们不要在子孙面前隐瞒事实， 要把耶和华当受称颂的作为、 祂的权能和所行的奇事告诉下一代。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們不把這些事向他們的子孫隱瞞， 卻要把耶和華應得的讚美、他的能力， 和他所行的奇事， 都向後代的人述說。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們不將這些事向他們的子孫隱瞞， 要將耶和華的美德和他的能力， 並他奇妙的作為，述說給後代聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We will not conceal them from their children, But tell to the generation to come the praises of the Lord, And His strength and His wondrous works that He has done."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We will not hide them from their descendants; we will tell the next generation the praiseworthy deeds of the Lord, his power, and the wonders he has done."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We will not hide these truths from our children; we will tell the next generation about the glorious deeds of the Lord, about his power and his mighty wonders."
      }
    },
