    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.17-39",
            "paragraphs": [
              "七八17～39概述以色列人在曠野飄流期間的反叛，以及神向他們所發的忿怒。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
