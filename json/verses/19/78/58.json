    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们建造丘坛，惹祂发怒； 他们竖起神像，令祂愤怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因他們的邱壇，惹起他的怒氣， 因他們雕刻的偶像，激起他的憤恨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他們的邱壇惹了他的怒氣； 因他們雕刻的偶像觸動他的憤恨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they provoked Him with their high places And aroused His jealousy with their graven images."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They angered him with their high places; they aroused his jealousy with their idols."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They angered God by building shrines to other gods; they made him jealous with their idols."
      }
