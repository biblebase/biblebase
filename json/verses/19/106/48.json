      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Blessed be the Lord, the God of Israel, From everlasting even to everlasting. And let all the people say, “Amen.” Praise the Lord!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Praise be to the Lord, the God of Israel, from everlasting to everlasting. Let all the people say, “Amen!” Praise the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Praise the Lord, the God of Israel, who lives from everlasting to everlasting! Let all the people say, “Amen!” Praise the Lord!"
      }
