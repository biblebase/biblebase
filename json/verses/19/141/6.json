    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.141.6-7",
            "paragraphs": [
              "一四一6,7直到不敬神之人的領袖（審判官）受審判，他們的\"骨頭散在墓旁\"，他們才聽大衛的話。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的首领被抛下悬崖的时候， 他们就会知道我所言不虚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的官長被摔在山崖下的時候， 他們就要聽我的話，因為我的話甘美。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们的审判官被扔下悬崖， 他们才会听我的言语， 因为这言语令人欣喜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的審判官被扔在巖下； 眾人要聽我的話，因為這話甘甜。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When their judges are thrown over the cliff, then they shall hear my words, for they are pleasant."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their judges are thrown down by the sides of the rock, And they hear my words, for they are pleasant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their rulers will be thrown down from the cliffs, and the wicked will learn that my words were well spoken."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Their judges are overthrown by the sides of the cliff, And they hear my words, for they are sweet."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When their leaders are thrown down from a cliff, the wicked will listen to my words and find them true."
      }
    },
