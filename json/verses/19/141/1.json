    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 我向你呼求，求你快来帮助我； 我向你呼求的时候，求你垂听。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！我呼求你， 求你快來幫助我； 我呼求你的時候，求你留心聽我的聲音。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我曾求告你， 求你快快臨到我這裏！ 我求告你的時候， 願你留心聽我的聲音！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, I call upon You; hasten to me! Give ear to my voice when I call to You!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I call to you, Lord, come quickly to me; hear me when I call to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, I am calling to you. Please hurry! Listen when I cry to you for help!"
      }
    },
