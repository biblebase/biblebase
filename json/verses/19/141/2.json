    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿我的祷告如香升到你面前， 愿我举手所献的祷告如同晚祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願我的禱告好像香安放在你面前； 願我的手高舉好像獻晚祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願我的禱告如香陳列在你面前！ 願我舉手祈求，如獻晚祭！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May my prayer be counted as incense before You; The lifting up of my hands as the evening offering."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May my prayer be set before you like incense; may the lifting up of my hands be like the evening sacrifice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Accept my prayer as incense offered to you, and my upraised hands as an evening offering."
      }
    },
