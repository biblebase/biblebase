    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.62.3-4",
            "paragraphs": [
              "六二3,4“把他毀壞”。戴維的仇敵意圖借“謊話”，在他軟弱的情況下（“歪斜”、“將倒”）推翻他。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我就像一面摇摇欲坠的墙壁、行将倒塌的篱笆， 你们要攻击我、置我于死地到何时呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們大家攻擊一個人，要把他殺害， 如同拆毀傾斜的牆，將倒的壁， 要到幾時呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們大家攻擊一人，把他毀壞， 如同毀壞歪斜的牆、將倒的壁，要到幾時呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How long will you assail a man, That you may murder him, all of you, Like a leaning wall, like a tottering fence?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How long will you assault me? Would all of you throw me down— this leaning wall, this tottering fence?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So many enemies against one man— all of them trying to kill me. To them I’m just a broken-down wall or a tottering fence."
      }
    },
