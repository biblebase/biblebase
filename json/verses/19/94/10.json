    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.94.8-11",
            "paragraphs": [
              "九四8～11這裏警告惡人，他們必須向神交代一切，因為祂看見和認識每個人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "管教列国的上帝难道不惩罚你们吗？ 赐知识的上帝难道不知道吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那管教萬國的，不施行審判嗎？ 那教導人的，自己沒有知識嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "管教列邦的，就是叫人得知識的， 難道自己不懲治人嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who chastens the nations, will He not rebuke, Even He who teaches man knowledge?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Does he who disciplines nations not punish? Does he who teaches mankind lack knowledge?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He punishes the nations—won’t he also punish you? He knows everything—doesn’t he also know what you are doing?"
      }
    },
