    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.94.20-23",
            "paragraphs": [
              "九四20～23第20節的問題（神為何容許邪惡的統治者使用律法，屈枉正直？）的回答方法是：承認邪惡的存在（21節），神關懷屬祂的子民（22節），以及最終的懲罰（23節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "借律例制造不幸的首领， 岂能与你联盟？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "憑著律例製造奸惡， 藉著權位行毀滅的，怎能和你相交呢？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "那些坐在奸恶王位上的， 就是藉着律例制造祸患的， 难道能成为你的盟友吗？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那藉着律例架弄殘害、 在位上行奸惡的，豈能與你相交嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Can wicked rulers be allied with you, those who frame injustice by statute?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Can a throne of destruction be allied with You, One which devises mischief by decree?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Can a corrupt throne be allied with you— a throne that brings on misery by its decrees?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Shall the throne of iniquity, which devises evil by law, Have fellowship with You?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can unjust leaders claim that God is on their side— leaders whose decrees permit injustice?"
      }
    },
