    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "危难之时，祂保护我， 把我藏在祂的圣幕里， 高高地安置在磐石上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為在我遭難的日子， 他必把我藏在他的帳棚裡， 把我隱藏在他帳幕的隱密處， 又把我高舉在磐石上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為我遭遇患難，他必暗暗地保守我； 在他亭子裏，把我藏在他帳幕的隱密處， 將我高舉在磐石上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For in the day of trouble He will conceal me in His tabernacle; In the secret place of His tent He will hide me; He will lift me up on a rock."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For in the day of trouble he will keep me safe in his dwelling; he will hide me in the shelter of his sacred tent and set me high upon a rock."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For he will conceal me there when troubles come; he will hide me in his sanctuary. He will place me out of reach on a high rock."
      }
    },
