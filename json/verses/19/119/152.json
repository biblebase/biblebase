    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.145-152",
            "paragraphs": [
              "一一九145～152詩人呼求神的拯救，因為他遵守神的道。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我很早就从你的法度中知道， 你的法度永远长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我從你的法度早已知道， 這些法度是你立定，存到永遠的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因學你的法度， 久已知道是你永遠立定的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Of old I have known from Your testimonies That You have founded them forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Long ago I learned from your statutes that you established them to last forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have known from my earliest days that your laws will last forever."
      }
