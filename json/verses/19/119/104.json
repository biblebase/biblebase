    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我从你的法则中得到智慧， 我厌恶一切诡诈之道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我藉著你的訓詞，得以明白事理； 因此，我恨惡一切虛謊的道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我藉着你的訓詞得以明白， 所以我恨一切的假道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From Your precepts I get understanding; Therefore I hate every false way."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I gain understanding from your precepts; therefore I hate every wrong path."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your commandments give me understanding; no wonder I hate every false way of life."
      }
    },
