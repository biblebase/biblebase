    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.119.113",
            "paragraphs": [
              "119:113",
              "心懷二意的人，為我所恨；但你的律法為我所愛：我們若不恨惡罪(113,115,119,128節；參來1:9註)，也不保守一顆聖潔敬畏神和他的審判的心，就不可能真正愛神的話。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.119.113-120",
            "paragraphs": [
              "一一九113～120詩人懼怕神的審判。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我厌恶心怀二意的人， 我爱慕你的律法。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我恨惡心懷二意的人， 我卻喜愛你的律法。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心懷二意的人為我所恨； 但你的律法為我所愛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I hate those who are double-minded, But I love Your law."
