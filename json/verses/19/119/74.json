        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愿敬畏你的人看见我就欢喜， 因为我期盼你的话语。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "敬畏你的人見我就要歡喜， 因我仰望你的話。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Those who fear you shall see me and rejoice, because I have hoped in your word."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May those who fear You see me and be glad, Because I wait for Your word."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May those who fear you rejoice when they see me, for I have put my hope in your word."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Those who fear You will be glad when they see me, Because I have hoped in Your word."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May all who fear you find in me a cause for joy, for I have put my hope in your word."
      }
