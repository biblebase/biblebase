    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.119.136",
            "paragraphs": [
              "119:136",
              "我的眼淚下流成河：當熱愛神話語的人看見不虔之輩棄絕、藐視神的律法時，必會感到痛苦傷心，甚至氣忿(53節)；無疑，他們必須不斷忍耐這一切，直到主基督再來的日子(參路19:45註；彼後2:8註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.119.129-136",
            "paragraphs": [
              "一一九129～136祈求神把他建立在祂的話語上。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我泪流成河， 因为人们不遵行你的律法。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的眼淚像河水湧流， 因為人都不謹守你的律法。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的眼淚下流成河， 因為他們不守你的律法。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My eyes shed streams of water, Because they do not keep Your law."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Streams of tears flow from my eyes, for your law is not obeyed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rivers of tears gush from my eyes because people disobey your instructions."
      }
    },
