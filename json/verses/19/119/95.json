        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "恶人等着要消灭我； 但我要领悟你的法度。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人等待我，要滅絕我， 我卻要揣摩你的法度。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The wicked lie in wait to destroy me, but I consider your testimonies."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wicked wait for me to destroy me; I shall diligently consider Your testimonies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wicked are waiting to destroy me, but I will ponder your statutes."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The wicked wait for me to destroy me, But I will consider Your testimonies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Though the wicked hide along the way to kill me, I will quietly keep my mind on your laws."
      }
