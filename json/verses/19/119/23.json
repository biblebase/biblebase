    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.119.23",
            "paragraphs": [
              "119:23",
              "首領坐著妄論我：詩人在此見證說，他因追求遵行神的道，過公義的生活而連連遭到譏諷、輕蔑和誹謗；然而這都沒能阻止他跟從神的道。今天，凡是忠心於基督和他話語的人當曉得，他們也同樣會遭到指責和嘲諷；他們會被那些隨從世俗的不敬虔之風的人稱為律法主義者或極端分子(參太5:10註；路6:22註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.119.17-24",
            "paragraphs": [
              "一一九17～24對神話語的認識不斷地增加（18節），能使人懂得從屬天的角度看生命（23節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然权贵们坐着毁谤我， 仆人仍要默想你的律例。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖有作領袖的坐著毀謗我， 你的僕人卻默想你的律例。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雖有首領坐着妄論我， 你僕人卻思想你的律例。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even though princes sit and talk against me, Your servant meditates on Your statutes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Though rulers sit together and slander me, your servant will meditate on your decrees."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even princes sit and speak against me, but I will meditate on your decrees."
      }
    },
