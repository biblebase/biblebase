    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.119.50",
            "paragraphs": [
              "119:50",
              "這話……我在患難中……得安慰：神已命定，當他的忠心兒女經受患難痛苦時，他必借聖靈的大能以他的話語來安慰他們，使他們有盼望，有力量。因為神的話語是活潑有功效的(比較來4:12)，所以能復興並堅固那些常常在神和他話語裏面的人(比較約14:27)。所以，在面臨患難的時候，就當轉而仰望神和神的道，等候聖靈將平安帶到心裏(比較腓4:6~9)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.119.49-56",
            "paragraphs": [
              "一一九49～56他對神律法的熱心使得他惱怒那些憎恨律法的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的应许是我生命的支柱， 是我患难中的安慰。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的應許使我存活； 這就是我在困苦中的安慰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這話將我救活了； 我在患難中，因此得安慰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is my comfort in my affliction, That Your word has revived me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My comfort in my suffering is this: Your promise preserves my life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your promise revives me; it comforts me in all my troubles."
      }
    },
