    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.49-56",
            "paragraphs": [
              "一一九49～56他對神律法的熱心使得他惱怒那些憎恨律法的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我见恶人丢弃你的律法， 就怒火中烧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為惡人離棄你的律法， 我就非常忿怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我見惡人離棄你的律法， 就怒氣發作，猶如火燒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Burning indignation has seized me because of the wicked, Who forsake Your law."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Indignation grips me because of the wicked, who have forsaken your law."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I become furious with the wicked, because they reject your instructions."
      }
