      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 我知道你的法令公义， 你是凭信实管教我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！我知道你的判語是公義的， 為了你的信實，你使我受苦。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华啊，我知道你的法规是公义的； 你磨炼我也是因着你的信实！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我知道你的判語是公義的； 你使我受苦是以誠實待我。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I know, O Lord, that your rules are righteous, and that in faithfulness you have afflicted me."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I know, O Lord, that Your judgments are righteous, And that in faithfulness You have afflicted me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I know, Lord, that your laws are righteous, and that in faithfulness you have afflicted me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I know, O Lord, that Your judgments are right, And that in faithfulness You have afflicted me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I know, O Lord, that your regulations are fair; you disciplined me because I needed it."
      }
