    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.20.6-8",
            "paragraphs": [
              "祂必應允（二十6～8）",
              "此段突然轉為肯定的把握，請看第六篇結尾的註釋。這時景觀也擴大了：原本是向錫安求助（2節），現在幫助則來自天上。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在我知道耶和华拯救祂膏立的王， 祂从圣天之上应允他， 用右手的大能拯救他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在我確知，耶和華拯救自己的受膏者； 他必從他的聖天上應允他， 用自己右手的能力拯救他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在我知道耶和華救護他的受膏者， 必從他的聖天上應允他， 用右手的能力救護他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now I know that the Lord saves His anointed; He will answer him from His holy heaven With the saving strength of His right hand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now this I know: The Lord gives victory to his anointed. He answers him from his heavenly sanctuary with the victorious power of his right hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now I know that the Lord rescues his anointed king. He will answer him from his holy heaven and rescue him by his great power."
      }
    },
