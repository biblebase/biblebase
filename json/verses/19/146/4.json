    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们气息一断，便归回尘土， 他们的打算转眼成空。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的氣一斷，就歸回塵土； 他們所計劃的，當天就幻滅了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的氣一斷，就歸回塵土； 他所打算的，當日就消滅了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His spirit departs, he returns to the earth; In that very day his thoughts perish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When their spirit departs, they return to the ground; on that very day their plans come to nothing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they breathe their last, they return to the earth, and all their plans die with them."
      }
