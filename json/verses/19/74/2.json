    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你眷顾你在古时所救赎的子民， 你拣选为产业的族类， 求你眷顾你所居住的锡安山。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你記念你在古時買贖的會眾， 就是你贖回作你產業的民族； 求你記念你所居住的錫安山。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你記念你古時所得來的會眾， 就是你所贖、作你產業支派的， 並記念你向來所居住的錫安山。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Remember Your congregation, which You have purchased of old, Which You have redeemed to be the tribe of Your inheritance; And this Mount Zion, where You have dwelt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember the nation you purchased long ago, the people of your inheritance, whom you redeemed— Mount Zion, where you dwelt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Remember that we are the people you chose long ago, the tribe you redeemed as your own special possession! And remember Jerusalem, your home here on earth."
      }
    },
