    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 仇敌嘲笑你的名要到何时呢？ 他们要永无休止地辱骂你吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！敵人辱罵你要到幾時呢？ 仇敵褻慢你的名要到永遠嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，敵人辱罵要到幾時呢？ 仇敵褻瀆你的名要到永遠嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How long, O God, will the adversary revile, And the enemy spurn Your name forever?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How long will the enemy mock you, God? Will the foe revile your name forever?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How long, O God, will you allow our enemies to insult you? Will you let them dishonor your name forever?"
      }
    },
