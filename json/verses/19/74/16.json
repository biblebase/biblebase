      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "白昼和黑夜都属于你， 你设立了日月。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "白晝是你的，黑夜也是你的， 月亮和太陽，都是你設立的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "白晝屬你，黑夜也屬你； 亮光和日頭是你所預備的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yours is the day, Yours also is the night; You have prepared the light and the sun."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The day is yours, and yours also the night; you established the sun and moon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Both day and night belong to you; you made the starlight and the sun."
      }
