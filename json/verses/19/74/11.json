    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你为何不伸出大能的右手？ 求你出手给他们致命的一击。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你為甚麼把你的手，就是你的右手收回呢？ 求你從懷中抽出來毀滅他們。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你为什么把你的手、把你的右手收回去呢？ 求你从怀中伸出手，灭绝他们！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你為甚麼縮回你的右手？ 求你從懷中伸出來，毀滅他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Why do you hold back your hand, your right hand? Take it from the fold of your garment and destroy them!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why do You withdraw Your hand, even Your right hand? From within Your bosom, destroy them!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why do you hold back your hand, your right hand? Take it from the folds of your garment and destroy them!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Why do You withdraw Your hand, even Your right hand? Take  it out of Your bosom and destroy them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why do you hold back your strong right hand? Unleash your powerful fist and destroy them."
      }
