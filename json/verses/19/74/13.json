    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.74.13-14",
            "paragraphs": [
              "七四13,14神毀滅\"大魚\"（海怪）和\"鱷魚\"（參看伯三8的腳註），是詩歌裏常用來形容神在出埃及時得勝埃及人的比喻。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你曾用大能分开海水， 打碎水中巨兽的头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你曾用你的大能分開大海， 把海中怪獸的頭都打碎了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你曾用能力將海分開， 將水中大魚的頭打破。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You divided the sea by Your strength; You broke the heads of the sea monsters in the waters."
      },
