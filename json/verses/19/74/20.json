      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Consider the covenant; For the dark places of the land are full of the habitations of violence."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Have regard for your covenant, because haunts of violence fill the dark places of the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Remember your covenant promises, for the land is full of darkness and violence!"
      }
