    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.54.6",
            "paragraphs": [
              "五四6\"甘心\"。自願地獻祭，或獻上甘心（平安）祭。參看利未記第三章的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 我甘心乐意献上祭物， 我要赞美你的名， 因为你的名是美善的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要甘心情願獻祭給你； 耶和華啊！我必稱讚你的名， 因你的名是美好的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要把甘心祭獻給你。 耶和華啊，我要稱讚你的名；這名本為美好。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Willingly I will sacrifice to You; I will give thanks to Your name, O Lord, for it is good."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will sacrifice a freewill offering to you; I will praise your name, Lord, for it is good."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will sacrifice a voluntary offering to you; I will praise your name, O Lord, for it is good."
      }
