    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "云层倒出雨水， 天上雷霆霹雳，电光四射。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "密雲傾降雨水， 天空發出響聲， 你的箭閃射四方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雲中倒出水來； 天空發出響聲； 你的箭也飛行四方。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The clouds poured out water; The skies gave forth a sound; Your arrows flashed here and there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The clouds poured down water, the heavens resounded with thunder; your arrows flashed back and forth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The clouds poured down rain; the thunder rumbled in the sky. Your arrows of lightning flashed."
      }
