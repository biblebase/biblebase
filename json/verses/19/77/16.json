    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，海水看见你就战栗， 深渊看见你就颤抖。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！眾水看見你， 眾水看見你就懼怕， 深淵也都戰抖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，諸水見你， 一見就都驚惶； 深淵也都戰抖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The waters saw You, O God; The waters saw You, they were in anguish; The deeps also trembled."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The waters saw you, God, the waters saw you and writhed; the very depths were convulsed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the Red Sea saw you, O God, its waters looked and trembled! The sea quaked to its very depths."
      }
    },
