    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我在困境中寻求主。 我整夜举手祷告， 我的心无法得到安慰。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我在患難的日子尋求主， 我整夜舉手禱告，總不倦怠， 我的心不肯受安慰。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "在我患难的日子里，我求问主， 夜间我的手伸着祈求，也不倦怠； 我的灵魂不肯受安慰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我在患難之日尋求主； 我在夜間不住地舉手禱告； 我的心不肯受安慰。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "In the day of my trouble I seek the Lord; in the night my hand is stretched out without wearying; my soul refuses to be comforted."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In the day of my trouble I sought the Lord; In the night my hand was stretched out without weariness; My soul refused to be comforted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When I was in distress, I sought the Lord; at night I stretched out untiring hands, and I would not be comforted."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "In the day of my trouble I sought the Lord; My hand was stretched out in the night without ceasing; My soul refused to be comforted."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When I was in deep trouble, I searched for the Lord. All night long I prayed, with hands lifted toward heaven, but my soul was not comforted."
      }
    },
