    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂降下黑暗，使黑暗笼罩那里； 他们没有违背祂的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他命黑暗降下，使那地黑暗； 他的話是不能違背的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他命黑暗，就有黑暗； 沒有違背他話的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He sent darkness and made it dark; And they did not rebel against His words."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He sent darkness and made the land dark— for had they not rebelled against his words?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord blanketed Egypt in darkness, for they had defied his commands to let his people go."
      }
