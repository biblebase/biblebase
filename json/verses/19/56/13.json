    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你救我脱离死亡， 使我没有跌倒， 让我可以活在你面前， 沐浴生命之光。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你救了我的性命脫離死亡； 你不是救了我的腳不跌倒， 使我在生命的光中，行在　神你的面前嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你救我的命脫離死亡。 你豈不是救護我的腳不跌倒、 使我在生命光中行在神面前嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You have delivered my soul from death, Indeed my feet from stumbling, So that I may walk before God In the light of the living."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For you have delivered me from death and my feet from stumbling, that I may walk before God in the light of life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you have rescued me from death; you have kept my feet from slipping. So now I can walk in your presence, O God, in your life-giving light."
      }
