      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們經過的日子都在你震怒之下； 我們度盡的年歲好像一聲歎息。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For all our days have declined in Your fury; We have finished our years like a sigh."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All our days pass away under your wrath; we finish our years with a moan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We live our lives beneath your wrath, ending our years with a groan."
      }
