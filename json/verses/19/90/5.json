    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你像急流一般把世人冲走， 叫他们如梦消逝。 他们像清晨的嫩草，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使世人消逝，像被洪水沖去； 他們好像睡了一覺； 他們又像在早晨生長的草，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你冲去他们，他们就沉睡； 他们如同清晨生长的草——"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你叫他們如水沖去； 他們如睡一覺。 早晨，他們如生長的草，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You sweep them away as with a flood; they are like a dream, like grass that is renewed in the morning:"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have swept them away like a flood, they fall asleep; In the morning they are like grass which sprouts anew."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet you sweep people away in the sleep of death— they are like the new grass of the morning:"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You carry them away like a flood; They  are like a sleep. In the morning they are like grass which grows up:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You sweep people away like dreams that disappear. They are like grass that springs up in the morning."
      }
    },
