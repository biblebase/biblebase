    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的仇敌设下网罗， 我心中沮丧。 他们在我走的路上挖了陷阱， 自己却掉了进去。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們為我的腳設下了網羅， 使我低頭屈服； 他們在我面前挖了坑， 自己反掉進坑中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們為我的腳設下網羅，壓制我的心； 他們在我面前挖了坑，自己反掉在其中。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have prepared a net for my steps; My soul is bowed down; They dug a pit before me; They themselves have fallen into the midst of it. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They spread a net for my feet— I was bowed down in distress. They dug a pit in my path— but they have fallen into it themselves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My enemies have set a trap for me. I am weary from distress. They have dug a deep pit in my path, but they themselves have fallen into it. Interlude"
      }
    },
