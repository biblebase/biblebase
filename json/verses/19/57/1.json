    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.57.1",
            "paragraphs": [
              "57:1",
              "在你翅膀的蔭下：神翅膀的蔭蔽指他的慈愛、庇護、力量和恩惠的同在。當我們在禱告中親近神，在信心中倚靠神時，就必蒙他翅膀的保護(參17:8；36:7；91:4)。當我們藏在他翅膀的蔭下時，任何與他為我們所定之旨意相抵觸的事都不能加害於我們(2節；參17:8註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.57.1",
            "paragraphs": [
              "五七1\"在你翅膀的蔭下\"。耶和華是保護祂子民的庇蔭所。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，求你怜悯我，怜悯我， 因为我投靠你。 我要在你翅膀下得到荫庇， 直到灾难结束。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊，求你恩待我！求你恩待我！ 因為我投靠你； 我要投靠在你翅膀的蔭下， 直到災害過去。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，求你恩待我、恩待我， 因为我的心投靠你！ 我投靠在你翅膀的荫庇下， 直到祸患过去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，求你憐憫我，憐憫我！ 因為我的心投靠你。 我要投靠在你翅膀的蔭下， 等到災害過去。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Be merciful to me, O God, be merciful to me, for in you my soul takes refuge; in the shadow of your wings I will take refuge, till the storms of destruction pass by."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be gracious to me, O God, be gracious to me, For my soul takes refuge in You; And in the shadow of Your wings I will take refuge Until destruction passes by."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Have mercy on me, my God, have mercy on me, for in you I take refuge. I will take refuge in the shadow of your wings until the disaster has passed."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Be merciful to me, O God, be merciful to me! For my soul trusts in You; And in the shadow of Your wings I will make my refuge, Until these calamities have passed by."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Have mercy on me, O God, have mercy! I look to you for protection. I will hide beneath the shadow of your wings until the danger passes by."
      }
    },
