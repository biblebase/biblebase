    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当他受审时， 愿他被判为有罪， 他的祷告也算为罪过。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他受審判的時候，願他被判為有罪； 願他的禱告成為罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他受審判的時候， 願他出來擔當罪名！ 願他的祈禱反成為罪！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he is judged, let him come forth guilty, And let his prayer become sin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he is tried, let him be found guilty, and may his prayers condemn him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When his case comes up for judgment, let him be pronounced guilty. Count his prayers as sins."
      }
