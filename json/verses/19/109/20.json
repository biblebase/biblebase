        "text": "願敵對我和用惡言攻擊我的， 都從耶和華那裡得到這報應。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愿这就是来自耶和华的报偿， 加给那些控告我的人，那些说恶言攻击我的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這就是我對頭和用惡言議論我的人 從耶和華那裏所受的報應。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "May this be the reward of my accusers from the Lord, of those who speak evil against my life!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let this be the reward of my accusers from the Lord, And of those who speak evil against my soul."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May this be the Lord’s payment to my accusers, to those who speak evil of me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Let this be the Lord’s reward to my accusers, And to those who speak evil against my person."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May those curses become the Lord’s punishment for my accusers who speak evil of me."
      }
