      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿耶和华这样报应那些诬告我， 以恶言攻击我的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
