    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊！唯有你永远做王， 你的大名万代长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你卻永遠坐著為王， 你可記念的名也必存到萬代。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟你－耶和華必存到永遠； 你可記念的名也存到萬代。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But You, O Lord, abide forever, And Your name to all generations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you, Lord, sit enthroned forever; your renown endures through all generations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you, O Lord, will sit on your throne forever. Your fame will endure to every generation."
      }
    },
