    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华从至高的圣所俯视人间， 从天上察看大地，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華從他至高的聖所裡垂看， 從天上觀看大地，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，他從至高的聖所垂看； 耶和華從天向地觀察，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He looked down from His holy height; From heaven the Lord gazed upon the earth,"
      },
