    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华永永远远爱敬畏祂的人， 以公义待他们的子子孙孙，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但從亙古到永遠，耶和華的慈愛臨到敬畏他的人； 他的公義也歸於他們的子子孫孫，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "但是耶和华的慈爱临到敬畏他的人， 从永远到永远； 他的公义归于他们的子子孙孙，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但耶和華的慈愛歸於敬畏他的人， 從亙古到永遠； 他的公義也歸於子子孫孫－"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But the steadfast love of the Lord is from everlasting to everlasting on those who fear him, and his righteousness to children’s children,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the lovingkindness of the Lord is from everlasting to everlasting on those who fear Him, And His righteousness to children’s children,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But from everlasting to everlasting the Lord’s love is with those who fear him, and his righteousness with their children’s children—"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But the mercy of the Lord  is from everlasting to everlasting On those who fear Him, And His righteousness to children’s children,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the love of the Lord remains forever with those who fear him. His salvation extends to the children’s children"
      }
    },
