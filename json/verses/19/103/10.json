    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂没有按我们的过犯对待我们， 也没有照我们的罪恶惩罚我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他沒有按著我們的罪過待我們， 也沒有照著我們的罪孽報應我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他沒有按我們的罪過待我們， 也沒有照我們的罪孽報應我們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has not dealt with us according to our sins, Nor rewarded us according to our iniquities."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "he does not treat us as our sins deserve or repay us according to our iniquities."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He does not punish us for all our sins; he does not deal harshly with us, as we deserve."
      }
    },
