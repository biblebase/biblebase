    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂救赎我的生命脱离死亡， 以慈爱和怜悯环绕我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他救贖你的性命脫離死亡， 以慈愛和憐憫作你的冠冕；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他救贖你的命脫離死亡， 以仁愛和慈悲為你的冠冕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who redeems your life from the pit, Who crowns you with lovingkindness and compassion;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who redeems your life from the pit and crowns you with love and compassion,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He redeems me from death and crowns me with love and tender mercies."
      }
    },
