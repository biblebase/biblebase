    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.87.1-3",
            "paragraphs": [
              "八七1～3神建立了\"錫安\"（賽一四32），神愛它，神也談及它的榮耀（賽二2～4；四2～6；二八16）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在雅各的住处中， 祂最喜爱锡安的门。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華愛錫安的城門， 勝過雅各的一切居所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他愛錫安的門， 勝於愛雅各一切的住處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord loves the gates of Zion More than all the other dwelling places of Jacob."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord loves the gates of Zion more than all the other dwellings of Jacob."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He loves the city of Jerusalem more than any other city in Israel."
      }
