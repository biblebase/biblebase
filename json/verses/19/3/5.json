    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.3.5",
            "paragraphs": [
              "3:5",
              "我躺下睡覺：若信徒心中堅信神垂聽禱告，懇切求告神，並恒切地以他的信實為糧(4節)，那麽他就可以平安躺下，安然入睡(比較4:8)。因為即使在他們睡覺時，神也會圍繞他們，以恩惠護衛他們(參127:2；箴3:24)"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我躺下，我睡觉，我醒来， 都蒙耶和华看顾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我躺下，我睡覺， 我醒來，都因耶和華在扶持著我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我躺下睡覺，我醒着， 耶和華都保佑我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I lay down and slept; I awoke, for the Lord sustains me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I lie down and sleep; I wake again, because the Lord sustains me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I lay down and slept, yet I woke up in safety, for the Lord was watching over me."
      }
    },
