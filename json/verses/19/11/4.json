      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord is in His holy temple; the Lord’s throne is in heaven; His eyes behold, His eyelids test the sons of men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord is in his holy temple; the Lord is on his heavenly throne. He observes everyone on earth; his eyes examine them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Lord is in his holy Temple; the Lord still rules from heaven. He watches everyone closely, examining every person on earth."
      }
