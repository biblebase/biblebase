    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.4.8",
            "paragraphs": [
              "有根基的平安（四8）",
              "既……又（both，和合本未譯，指“躺下”與“睡覺”）一字通常的意思是“一起”，這裏似乎意味“同時進行”。參葛利紐（Gelineau）：“立刻睡著”。本篇詩的最後一個字，是由“相信”的字根而來，NEB譯得最貼切：“不害怕”。參，箴言一33。如果這種狀況是固為根基很牢因的緣故所生，那麽，豈不是比處在安全狀態下更美好？"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只有你耶和华使我安然居住， 我必高枕无忧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必平平安安躺下睡覺， 因為只有你耶和華能使我安然居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必安然躺下睡覺， 因為獨有你－耶和華使我安然居住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In peace I will both lie down and sleep, For You alone, O Lord, make me to dwell in safety."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In peace I will lie down and sleep, for you alone, Lord, make me dwell in safety."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In peace I will lie down and sleep, for you alone, O Lord, will keep me safe."
      }
    },
