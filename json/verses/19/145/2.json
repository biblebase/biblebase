    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.145.2",
            "paragraphs": [
              "145:2",
              "我要天天稱頌你：我們每天都要為神的祝福和恩賜而贊美感謝他。他是我們的創造者、救贖主和預備者(比較34:1)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要天天称颂你， 永永远远赞美你的名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要天天稱頌你， 我要永永遠遠讚美你的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要天天稱頌你， 也要永永遠遠讚美你的名！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every day I will bless You, And I will praise Your name forever and ever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every day I will praise you and extol your name for ever and ever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will praise you every day; yes, I will praise you forever."
      }
    },
