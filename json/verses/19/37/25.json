    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.37.12-26",
            "paragraphs": [
              "隱藏的幫助（三十七12～26）",
              "到目前為止，戰場乃在信徒心中，他們因惡人的明目張膽而激忿不平。現在詩人從外面觀察這兩種人，比較他們的道路與結局。直到本詩的末尾，幾乎每一節都講到義人或惡人，或類似用語。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.37.25",
            "paragraphs": [
              "三七25偷懶的人淪落至靠\"討飯\"為生（箴二○4）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从幼年到老年， 我从未见过义人遭弃， 也未见其后人讨饭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我從前年幼，現在年老， 從未見過義人被棄， 也從未見過他的後裔討飯。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我從前年幼，現在年老， 卻未見過義人被棄， 也未見過他的後裔討飯。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have been young and now I am old, Yet I have not seen the righteous forsaken Or his descendants begging bread."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I was young and now I am old, yet I have never seen the righteous forsaken or their children begging bread."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Once I was young, and now I am old. Yet I have never seen the godly abandoned or their children begging for bread."
      }
    },
