    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.37.27-40",
            "paragraphs": [
              "前瞻（三十七27～40）",
              "雖然在本詩中，這不算是新觀點，但在本段內它卻特別突顯，首先是永遠的重復出現，在第27、28（強調語）、29節（同義字）；然後是35、36節的小比方；最後則是藉37、38節所強調的\\cs9後裔表明。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华帮助他们， 拯救他们脱离恶人， 因为他们投靠祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華幫助他們，搭救他們； 他搭救他們脫離惡人，拯救他們， 因為他們投靠他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華幫助他們，解救他們； 他解救他們脫離惡人，把他們救出來， 因為他們投靠他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord helps them and delivers them; He delivers them from the wicked and saves them, Because they take refuge in Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord helps them and delivers them; he delivers them from the wicked and saves them, because they take refuge in him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord helps them, rescuing them from the wicked. He saves them, and they find shelter in him."
      }
    },
