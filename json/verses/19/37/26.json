    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.37.12-26",
            "paragraphs": [
              "隱藏的幫助（三十七12～26）",
              "到目前為止，戰場乃在信徒心中，他們因惡人的明目張膽而激忿不平。現在詩人從外面觀察這兩種人，比較他們的道路與結局。直到本詩的末尾，幾乎每一節都講到義人或惡人，或類似用語。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们乐善好施， 后代都蒙祝福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他常常慷慨借給人； 他的後裔必定蒙福。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他终日恩待人、借予人， 连他的后裔也成为人的祝福。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他終日恩待人，借給人； 他的後裔也蒙福！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He is ever lending generously, and his children become a blessing."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All day long he is gracious and lends, And his descendants are a blessing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are always generous and lend freely; their children will be a blessing."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He  is ever merciful, and lends; And his descendants are blessed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The godly always give generous loans to others, and their children are a blessing."
      }
    },
