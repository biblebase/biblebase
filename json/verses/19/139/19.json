    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，愿你杀戮恶人！ 嗜血成性的人啊，你们走开！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！甚願你殺戮惡人； 你們流人血的啊，離開我去吧！"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，但愿你杀掉恶人！ 杀人流血的人哪，你们远离我吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你必要殺戮惡人； 所以，你們好流人血的，離開我去吧！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Oh that you would slay the wicked, O God! O men of blood, depart from me!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O that You would slay the wicked, O God; Depart from me, therefore, men of bloodshed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If only you, God, would slay the wicked! Away from me, you who are bloodthirsty!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Oh, that You would slay the wicked, O God! Depart from me, therefore, you bloodthirsty men."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O God, if only you would destroy the wicked! Get out of my life, you murderers!"
      }
