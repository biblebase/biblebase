    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.139.9",
            "paragraphs": [
              "一三九9即使人能從東（\"清晨\"）飛到西（地中海最遠之處），也不能逃避神的存在。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "纵使我乘着晨风飞到遥远的海岸居住，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我展開清晨的翅膀， 飛到海的極處居住，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若展開清晨的翅膀， 飛到海極居住，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I take the wings of the dawn, If I dwell in the remotest part of the sea,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If I rise on the wings of the dawn, if I settle on the far side of the sea,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If I ride the wings of the morning, if I dwell by the farthest oceans,"
      }
    },
