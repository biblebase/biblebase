    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.139.7",
            "paragraphs": [
              "139:7",
              "7~12我往哪裏去躲避你的靈：神的兒女行事永遠不可脫離神的眷顧、引導和扶持(參10節,這是理解7節的關鍵)。無論何境，無論將來或現在發生什麽，他都與我們在一起。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我去哪里可躲开你的灵？ 我跑到哪里可避开你的面？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我到哪裡去躲避你的靈？ 我往哪裡去逃避你的面呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我往哪裏去躲避你的靈？ 我往哪裏逃、躲避你的面？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Where can I go from Your Spirit? Or where can I flee from Your presence?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Where can I go from your Spirit? Where can I flee from your presence?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I can never escape from your Spirit! I can never get away from your presence!"
      }
    },
