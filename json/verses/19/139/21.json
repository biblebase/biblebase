    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.139.21",
            "paragraphs": [
              "139:21",
              "攻擊你的，我豈不憎嫌他們嗎：詩人因為常常思想神對他的奇妙大愛，便以自己的滿腔熱愛和忠誠來報答神。那些抵擋神、褻瀆他奇妙聖名的人使詩人義憤填膺。因著對神和神的大工的深切同情，詩人堅定地與神站在一起，恨神所恨，愛神所愛(參來1:9註)。他為身邊世人的一切邪惡淫行而深感憂慮傷痛(參彼後2:7~8)。我們這些親身經歷了神的拯救、慈愛和良善的人，心中不當存有一絲一毫對世界和世俗之道的愛慕之心(參約一2:15~16)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 我憎恨那些憎恨你的人， 我厌恶那些攻击你的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！恨惡你的，我怎能不恨惡他們呢？ 起來攻擊你的，我怎能不厭惡他們呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，恨惡你的，我豈不恨惡他們嗎？ 攻擊你的，我豈不憎嫌他們嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do I not hate those who hate You, O Lord? And do I not loathe those who rise up against You?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do I not hate those who hate you, Lord, and abhor those who are in rebellion against you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, shouldn’t I hate those who hate you? Shouldn’t I despise those who oppose you?"
      }
