    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.86.2-15",
            "paragraphs": [
              "八六2～15\"虔誠人\"。即享受約關系和神的\"慈愛\"（希伯來文是hesed；5,13,15節）之民當中的忠貞者。參看何西阿書二章19節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，狂傲的人攻击我， 一群凶残之徒要杀害我， 他们藐视你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！驕傲的人起來攻擊我， 一群強暴的人尋索我的性命， 他們不把你放在眼內。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，驕傲的人起來攻擊我， 又有一黨強橫的人尋索我的命； 他們沒有將你放在眼中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O God, arrogant men have risen up against me, And a band of violent men have sought my life, And they have not set You before them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Arrogant foes are attacking me, O God; ruthless people are trying to kill me— they have no regard for you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O God, insolent people rise up against me; a violent gang is trying to kill me. You mean nothing to them."
      }
    },
