    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.86.2-15",
            "paragraphs": [
              "八六2～15\"虔誠人\"。即享受約關系和神的\"慈愛\"（希伯來文是hesed；5,13,15節）之民當中的忠貞者。參看何西阿書二章19節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你保护我的性命， 因为我忠于你。 你是我的上帝，我信靠你， 求你拯救仆人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你保護我的性命，因為我是虔誠的人； 我的　神啊！求你拯救這倚靠你的僕人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你保存我的性命，因我是虔誠人。 我的神啊，求你拯救這倚靠你的僕人！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Preserve my soul, for I am a godly man; O You my God, save Your servant who trusts in You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Guard my life, for I am faithful to you; save your servant who trusts in you. You are my God;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Protect me, for I am devoted to you. Save me, for I serve you and trust you. You are my God."
      }
