    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.147.15-20",
            "paragraphs": [
              "一四七15～20祂的話語能命令和操控大自然的力量（15～18節），但祂特別把祂的律法頒布給以色列，使她與其它各國有所不同（19,20節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂未曾这样对待其他国家， 他们不知道祂的律法。 你们要赞美耶和华！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他從沒有這樣對待其他各國； 他們都不知道他的典章。 你們要讚美耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "別國他都沒有這樣待過； 至於他的典章，他們向來沒有知道。 你們要讚美耶和華！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has not dealt thus with any nation; And as for His ordinances, they have not known them. Praise the Lord!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He has done this for no other nation; they do not know his laws. Praise the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He has not done this for any other nation; they do not know his regulations. Praise the Lord!"
      }
    },
