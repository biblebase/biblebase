    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂以云霞遮蔽天空， 降雨水滋润大地， 使山上长出绿草。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他以密雲遮蓋天空， 為大地預備雨水， 使群山長滿青草。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他用乌云遮盖天空， 为大地预备雨水，使群山长出青草。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他用雲遮天，為地降雨， 使草生長在山上。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He covers the heavens with clouds; he prepares rain for the earth; he makes grass grow on the hills."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who covers the heavens with clouds, Who provides rain for the earth, Who makes grass to grow on the mountains."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He covers the sky with clouds; he supplies the earth with rain and makes grass grow on the hills."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Who covers the heavens with clouds, Who prepares rain for the earth, Who makes grass to grow on the mountains."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He covers the heavens with clouds, provides rain for the earth, and makes the grass grow in mountain pastures."
      }
    },
