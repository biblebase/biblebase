    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.147.15-20",
            "paragraphs": [
              "一四七15～20祂的話語能命令和操控大自然的力量（15～18節），但祂特別把祂的律法頒布給以色列，使她與其它各國有所不同（19,20節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂将自己的话传于雅各， 将自己的律例和法令指示以色列。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把自己的話向雅各頒布， 把自己的律例和典章向以色列頒布。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他將他的道指示雅各， 將他的律例典章指示以色列。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He declares His words to Jacob, His statutes and His ordinances to Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He has revealed his word to Jacob, his laws and decrees to Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He has revealed his words to Jacob, his decrees and regulations to Israel."
      }
