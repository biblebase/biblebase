      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "汪洋浩瀚， 充满了无数的大小水族，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那裡有海，又大又廣； 海裡有無數的活物， 大小活物都有。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那裏有海，又大又廣； 其中有無數的動物， 大小活物都有。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is the sea, great and broad, In which are swarms without number, Animals both small and great."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There is the sea, vast and spacious, teeming with creatures beyond number— living things both large and small."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Here is the ocean, vast and wide, teeming with life of every kind, both large and small."
      }
