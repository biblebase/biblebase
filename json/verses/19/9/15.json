      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "外邦人陷在自己所掘的坑中； 他們的腳在自己暗設的網羅裏纏住了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The nations have sunk down in the pit which they have made; In the net which they hid, their own foot has been caught."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The nations have fallen into the pit they have dug; their feet are caught in the net they have hidden."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The nations have fallen into the pit they dug for others. Their own feet have been caught in the trap they set."
      }
    },
