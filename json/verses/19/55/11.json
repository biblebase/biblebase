    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.55.10-11",
            "paragraphs": [
              "五五10,11作為君王，大衛尤其擔心耶路撒冷城已經成為叛軍和恐怖分子的閱兵場。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "毁坏的势力到处肆虐， 恐吓与欺诈遍布大街小巷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "城中也有毀滅人的事， 欺壓和詭詐不離城裡的街道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "邪惡在其中； 欺壓和詭詐不離街市。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Destruction is in her midst; Oppression and deceit do not depart from her streets."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Destructive forces are at work in the city; threats and lies never leave its streets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everything is falling apart; threats and cheating are rampant in the streets."
      }
    },
