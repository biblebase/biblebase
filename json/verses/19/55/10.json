    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们昼夜在城墙上出没， 城内充满了邪恶和压迫，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人日夜在城牆上繞行， 在城裡盡是邪惡與禍害；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們在城牆上晝夜繞行； 在城內也有罪孽和奸惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Day and night they go around her upon her walls, And iniquity and mischief are in her midst."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Day and night they prowl about on its walls; malice and abuse are within it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Its walls are patrolled day and night against invaders, but the real danger is wickedness within the city."
      }
    },
