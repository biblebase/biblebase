    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然许多人攻击我， 祂必救我平安脱离险境。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他救贖我的性命脫離攻擊我的人， 使我得著平安， 儘管攻擊我的人的確很多。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他救赎我的性命脱离攻击我的人， 使我得享平安， 尽管敌对我的人众多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他救贖我命脫離攻擊我的人， 使我得享平安， 因為與我相爭的人甚多。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He redeems my soul in safety from the battle that I wage, for many are arrayed against me."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will redeem my soul in peace from the battle which is against me, For they are many who strive with me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He rescues me unharmed from the battle waged against me, even though many oppose me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He has redeemed my soul in peace from the battle that  was against me, For there were many against me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He ransoms me and keeps me safe from the battle waged against me, though many still oppose me."
      }
    },
