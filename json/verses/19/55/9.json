    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.55.9",
            "paragraphs": [
              "五五9\"吞滅\"。更可作：變混亂。意思是：願這樣的混亂和象巴別塔的變亂口音，發生在我的仇敵身上。參看創世記十一章7、9節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主啊， 我在城中看见暴力和争斗， 求你使他们言语混乱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主啊！擾亂惡人的計謀， 使他們的意見分歧， 因為我在城中看見了強暴和爭競的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主啊，求你吞滅他們，變亂他們的舌頭！ 因為我在城中見了強暴爭競的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Confuse, O Lord, divide their tongues, For I have seen violence and strife in the city."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, confuse the wicked, confound their words, for I see violence and strife in the city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Confuse them, Lord, and frustrate their plans, for I see violence and conflict in the city."
      }
    },
