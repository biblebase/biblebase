    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 你必把恶人送进灭亡的坑里。 嗜血成性的骗子必早早夭亡。 但我要信靠你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！你必使惡人墮入滅亡的深坑裡； 流人血和行詭詐的人必活不到半世； 至於我，我必倚靠你。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，你必使恶人下到灭亡的深坑里！ 那些杀人流血、行诡诈的人，必活不过半生； 至于我，我要依靠你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你必使惡人下入滅亡的坑； 流人血、行詭詐的人必活不到半世， 但我要倚靠你。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But you, O God, will cast them down into the pit of destruction; men of blood and treachery shall not live out half their days. But I will trust in you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But You, O God, will bring them down to the pit of destruction; Men of bloodshed and deceit will not live out half their days. But I will trust in You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you, God, will bring down the wicked into the pit of decay; the bloodthirsty and deceitful will not live out half their days. But as for me, I trust in you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But You, O God, shall bring them down to the pit of destruction; Bloodthirsty and deceitful men shall not live out half their days; But I will trust in You."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you, O God, will send the wicked down to the pit of destruction. Murderers and liars will die young, but I am trusting you to save me."
      }
    },
