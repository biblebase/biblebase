    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华必把那些偏行恶道的人与作恶的人一同赶走。 愿平安临到以色列。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於那些偏行彎曲道路的人， 耶和華必把他們和作惡的人一同除去。 願平安歸於以色列。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於那偏行彎曲道路的人， 耶和華必使他和作惡的人一同出去受刑。 願平安歸於以色列！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for those who turn aside to their crooked ways, The Lord will lead them away with the doers of iniquity. Peace be upon Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But those who turn to crooked ways the Lord will banish with the evildoers. Peace be on Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But banish those who turn to crooked ways, O Lord. Take them away with those who do evil. May Israel have peace!"
      }
