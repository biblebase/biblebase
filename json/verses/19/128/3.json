    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.128.3",
            "paragraphs": [
              "一二八3\"在你的內室\"。相對於喧嚷不受約束的妻子（箴七11）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你妻子在家中生养众多， 像结实累累的葡萄树， 你的桌子周围坐满了你的儿女， 如同橄榄树苗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的妻子在你的內室， 好像多結果子的葡萄樹； 你的兒女環繞著你的桌子， 好像橄欖樹的枝條。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你妻子在你的內室，好像多結果子的葡萄樹； 你兒女圍繞你的桌子，好像橄欖栽子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your wife shall be like a fruitful vine Within your house, Your children like olive plants Around your table."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your wife will be like a fruitful vine within your house; your children will be like olive shoots around your table."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your wife will be like a fruitful grapevine, flourishing within your home. Your children will be like vigorous young olive trees as they sit around your table."
      }
    },
