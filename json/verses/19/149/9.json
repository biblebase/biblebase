    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.149.6-9",
            "paragraphs": [
              "一四九6～9這段展望基督千禧年國度建立時，列國都被征服。參看以賽亞書五十四章11至17節和彌迦書五章4至15節的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "按他们的罪状审判他们。 这就是祂忠心的子民所得的荣耀。 你们要赞美耶和华！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要在他們身上施行記錄在冊上的審判。 這就是他所有聖民的尊榮。 你們要讚美耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要在他們身上施行所記錄的審判。 他的聖民都有這榮耀。 你們要讚美耶和華！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To execute on them the judgment written; This is an honor for all His godly ones. Praise the Lord!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "to carry out the sentence written against them— this is the glory of all his faithful people. Praise the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "to execute the judgment written against them. This is the glorious privilege of his faithful ones. Praise the Lord!"
      }
    },
