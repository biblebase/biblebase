    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂救赎了自己的子民， 立了永远的约， 祂的名神圣可畏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他向自己的子民施行救贖； 他命定自己的約，直到永遠； 他的名神聖可畏。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他向百姓施行救贖， 命定他的約，直到永遠； 他的名聖而可畏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has sent redemption to His people; He has ordained His covenant forever; Holy and awesome is His name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He provided redemption for his people; he ordained his covenant forever— holy and awesome is his name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He has paid a full ransom for his people. He has guaranteed his covenant with them forever. What a holy, awe-inspiring name he has!"
      }
    },
