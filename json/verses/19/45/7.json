    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.45.6-7",
            "paragraphs": [
              "四五6,7第6節稱王為神，第7節卻又把王與神作出區分。第6節可能是用來描述君王（也許是所羅門）的一個誇張句法，但終極的對象卻是耶穌基督（來一8,9）。神應許大衛家要永遠坐在寶座上（撒下七16），而耶穌便是那最後和永遠的坐寶座者（路一32,33）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你喜爱公义，憎恶邪恶， 因此上帝，你的上帝， 用喜乐之油膏你， 使你超过同伴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你喜愛公義，恨惡邪惡， 所以，　神，就是你的　神， 用喜樂的油膏抹你，勝過膏抹你的同伴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你喜愛公義，恨惡罪惡； 所以神－就是你的神－用喜樂油膏你， 勝過膏你的同伴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have loved righteousness and hated wickedness; Therefore God, Your God, has anointed You With the oil of joy above Your fellows."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You love righteousness and hate wickedness; therefore God, your God, has set you above your companions by anointing you with the oil of joy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You love justice and hate evil. Therefore God, your God, has anointed you, pouring out the oil of joy on you more than on anyone else."
      }
    },
