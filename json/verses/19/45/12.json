      "leili": {
        "content": [
          {
            "verse": "psa.45.12",
            "paragraphs": [
              "四五12\"推羅的女子\"。把推羅的民人格化。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
