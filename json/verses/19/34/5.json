    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.34.1-10",
            "paragraphs": [
              "與我一同喜樂！（三十四1～10）",
              "本詩的前一半，個人的見證（1～2a、4、6節）和重復呼召人同來贊美、更新信心，兩者交替出現。如此分享祝福的方式，請參二十二22以下的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.34.5",
            "paragraphs": [
              "三四5\"有光榮\"。發光，容光煥發。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡仰望祂的必有荣光， 不致蒙羞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人仰望他，就有光彩， 他們的臉必不蒙羞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡仰望他的，便有光榮； 他們的臉必不蒙羞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They looked to Him and were radiant, And their faces will never be ashamed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who look to him are radiant; their faces are never covered with shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who look to him for help will be radiant with joy; no shadow of shame will darken their faces."
      }
