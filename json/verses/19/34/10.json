      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The young lions do lack and suffer hunger; But they who seek the Lord shall not be in want of any good thing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The lions may grow weak and hungry, but those who seek the Lord lack no good thing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even strong young lions sometimes go hungry, but those who trust in the Lord will lack no good thing."
      }
