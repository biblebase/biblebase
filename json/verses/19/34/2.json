    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.34.1-10",
            "paragraphs": [
              "與我一同喜樂！（三十四1～10）",
              "本詩的前一半，個人的見證（1～2a、4、6節）和重復呼召人同來贊美、更新信心，兩者交替出現。如此分享祝福的方式，請參二十二22以下的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要夸耀祂的作为， 困苦人听见必欢欣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的心要因耶和華誇耀， 困苦的人聽見了就喜樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的心必因耶和華誇耀； 謙卑人聽見就要喜樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My soul will make its boast in the Lord; The humble will hear it and rejoice."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will glory in the Lord; let the afflicted hear and rejoice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will boast only in the Lord; let all who are helpless take heart."
      }
    },
