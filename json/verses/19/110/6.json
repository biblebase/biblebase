    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.110.6",
            "paragraphs": [
              "110:6",
              "他要在列邦中刑罰惡人：這節經文生動地描述了基督必如勇士再來的景象；他將打敗並審判一切抵擋神的國和神的義的人(參啟19:11~21)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂要审判列国， 使列国尸横遍野。 祂要毁灭世上的首领。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必在列國中施行審判， 屍首就遍滿各處； 他要剿滅地上各處的首領。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他要在列邦中刑罰惡人， 屍首就遍滿各處； 他要在許多國中打破仇敵的頭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will judge among the nations, He will fill them with corpses, He will shatter the chief men over a broad country."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will judge the nations, heaping up the dead and crushing the rulers of the whole earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will punish the nations and fill their lands with corpses; he will shatter heads over the whole earth."
      }
    },
