    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.129.8",
            "paragraphs": [
              "一二九8錫安的仇敵不會聽見慣常友善的問候和祝福。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "没有一个路过的人说： “愿耶和华赐福给你们！ 我们奉耶和华的名祝福你们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "過路的人也不說： “願耶和華所賜的福臨到你們； 我們奉耶和華的名祝福你們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "過路的也不說： 願耶和華所賜的福歸與你們！ 我們奉耶和華的名給你們祝福！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nor do those who pass by say, “The blessing of the Lord be upon you; We bless you in the name of the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May those who pass by not say to them, “The blessing of the Lord be on you; we bless you in the name of the Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And may those who pass by refuse to give them this blessing: “The Lord bless you; we bless you in the Lord’s name.”"
      }
    },
