    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "敌人迫害我，打垮我， 使我生活在黑暗中， 像死去很久的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "仇敵逼迫我， 把我壓倒在地， 使我住在黑暗之處， 像死了很久的人一樣。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "仇敌追逼我， 把我的生命压碎在地； 他使我住在黑暗之地， 如同死了很久的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "原來仇敵逼迫我， 將我打倒在地， 使我住在幽暗之處， 像死了許久的人一樣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For the enemy has pursued my soul; he has crushed my life to the ground; he has made me sit in darkness like those long dead."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the enemy has persecuted my soul; He has crushed my life to the ground; He has made me dwell in dark places, like those who have long been dead."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The enemy pursues me, he crushes me to the ground; he makes me dwell in the darkness like those long dead."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For the enemy has persecuted my soul; He has crushed my life to the ground; He has made me dwell in darkness, Like those who have long been dead."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My enemy has chased me. He has knocked me to the ground and forces me to live in darkness like those in the grave."
      }
