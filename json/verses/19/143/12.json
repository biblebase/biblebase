    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.143.11-12",
            "paragraphs": [
              "一四三11,12大衛祈求耶和華基於祂的\"名\"、祂的\"公義\"和祂的\"慈愛\"，保全他的性命（\"將我救活\"）。參看何西阿書二章19節的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你铲除我的仇敌， 因为你爱我。 求你消灭一切迫害我的人， 因为我是你的仆人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你因你的慈愛使我的仇敵滅絕， 求你消滅所有苦害我的人， 因為我是你的僕人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "憑你的慈愛剪除我的仇敵， 滅絕一切苦待我的人， 因我是你的僕人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And in Your lovingkindness, cut off my enemies And destroy all those who afflict my soul, For I am Your servant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In your unfailing love, silence my enemies; destroy all my foes, for I am your servant."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In your unfailing love, silence all my enemies and destroy all my foes, for I am your servant."
      }
    },
