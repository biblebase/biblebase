    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.143.1",
            "paragraphs": [
              "143:1",
              "1~12求你聽我的禱告：和上一首詩所描述的一樣，詩人被極大的患難所包圍，感到自己已到了忍耐的極限(3~4,7節；比較104:29)。他惟一能做的就是在禱告中盼望神復興他、並救他脫離這極大的試煉(10~11節；參上一條註釋)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你垂听我的祷告， 倾听我的呼求， 求你凭你的信实和公义应允我的祈求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你聽我的禱告， 留心聽我的懇求， 憑你的信實和公義應允我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你聽我的禱告， 留心聽我的懇求， 憑你的信實和公義應允我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hear my prayer, O Lord, Give ear to my supplications! Answer me in Your faithfulness, in Your righteousness!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, hear my prayer, listen to my cry for mercy; in your faithfulness and righteousness come to my relief."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Hear my prayer, O Lord; listen to my plea! Answer me because you are faithful and righteous."
      }
    },
