    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.66.5",
            "paragraphs": [
              "66:5",
              "你們來看神所行的：一個信徒團契必須彰顯出聖靈的同在和大能，使他們榮耀神並能向世人說：“你們來看神所行的”(參“信徒的屬靈恩賜”一文)。神的子民若不能豐富地彰顯神的權能，就難以叫失喪的人相信基督福音的真理(參“神跡”一文)。我們還必須在個人的生活中向人見證神在我們生命中不斷的眷顧(16節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "来吧，看看上帝的作为， 祂为世人行了何等奇妙的事！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們來看　神的作為， 他向世人所作的事是可畏懼的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你们当来，观看神的作为！ 他向世人所行的事是可畏的："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們來看神所行的， 他向世人所做之事是可畏的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Come and see what God has done: he is awesome in his deeds toward the children of man."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Come and see the works of God, Who is awesome in His deeds toward the sons of men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Come and see what God has done, his awesome deeds for mankind!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Come and see the works of God; He  is awesome in  His doing toward the sons of men."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Come and see what our God has done, what awesome miracles he performs for people!"
      }
    },
