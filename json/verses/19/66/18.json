    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.66.18",
            "paragraphs": [
              "66:18",
              "我若心裏註重罪孽：行不義、貪享罪中之樂的人，根本不要指望神會應允他們的禱告。神要我們脫離罪惡；只有這樣，他才會像父親待兒子一樣回應我們(林後6:14~18；參雅4:3註；約一3:22註；參“有功效的禱告”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.66.18-19",
            "paragraphs": [
              "六六18,19罪褫奪人哀求的資格；一顆無可指摘的心保證神必垂聽我們的禱告（約壹三21）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若我心中藏匿罪恶， 主必不垂听我的呼求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我心裡偏向罪孽， 主必不聽；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若心裏注重罪孽， 主必不聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I regard wickedness in my heart, The Lord will not hear;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If I had cherished sin in my heart, the Lord would not have listened;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If I had not confessed the sin in my heart, the Lord would not have listened."
      }
