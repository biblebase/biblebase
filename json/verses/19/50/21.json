    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我对你的所作所为默然不语， 你就以为我与你是同道。 现在我要责备你， 当面指出你的罪状。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你作了這些事，我默不作聲； 你以為我和你一樣？ 其實我要責備你，要當面指控你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你行了這些事，我還閉口不言， 你想我恰和你一樣； 其實我要責備你，將這些事擺在你眼前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These things you have done and I kept silence; You thought that I was just like you; I will reprove you and state the case in order before your eyes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you did these things and I kept silent, you thought I was exactly like you. But I now arraign you and set my accusations before you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "While you did all this, I remained silent, and you thought I didn’t care. But now I will rebuke you, listing all my charges against you."
      }
    },
