    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的主耶和华啊， 你的名在地上何其尊贵！ 你的荣耀充满诸天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華我們的主啊！ 你的名在全地是多麼威嚴， 你把你的榮美彰顯在天上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－我們的主啊， 你的名在全地何其美！ 你將你的榮耀彰顯於天。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, our Lord, How majestic is Your name in all the earth, Who have displayed Your splendor above the heavens!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, our Lord, how majestic is your name in all the earth! You have set your glory in the heavens."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, our Lord, your majestic name fills the earth! Your glory is higher than the heavens."
      }
    },
