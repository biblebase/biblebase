    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "列邦的首领聚集在一起， 要做亚伯拉罕之上帝的子民， 因为上帝是主宰天下的君王， 祂至高无上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬民中的顯貴都聚集起來， 要作亞伯拉罕的　神的子民； 因為地上的君王都歸順　神； 他被尊為至高。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "列邦的君王聚集要作亞伯拉罕之神的民。 因為世界的盾牌是屬神的； 他為至高！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The princes of the people have assembled themselves as the people of the God of Abraham, For the shields of the earth belong to God; He is highly exalted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The nobles of the nations assemble as the people of the God of Abraham, for the kings of the earth belong to God; he is greatly exalted."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The rulers of the world have gathered together with the people of the God of Abraham. For all the kings of the earth belong to God. He is highly honored everywhere."
      }
