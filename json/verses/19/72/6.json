    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他的统治如春雨降在新割过的草场上， 又像甘霖滋润大地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必像雨降在已割的草地上， 像甘霖滋潤大地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必降臨，像雨降在已割的草地上， 如甘霖滋潤田地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May he come down like rain upon the mown grass, Like showers that water the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May he be like rain falling on a mown field, like showers watering the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May the king’s rule be refreshing like spring rain on freshly cut grass, like the showers that water the earth."
      }
    },
