    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他保护百姓中受苦的人， 拯救贫穷的人，打倒欺压者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必為民間的困苦人伸冤， 拯救貧窮人， 粉碎那欺壓人的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愿他为子民中的困苦人施行审判， 拯救贫穷人的儿女，压碎那欺压者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必為民中的困苦人伸冤， 拯救窮乏之輩， 壓碎那欺壓人的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "May he defend the cause of the poor of the people, give deliverance to the children of the needy, and crush the oppressor!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May he vindicate the afflicted of the people, Save the children of the needy And crush the oppressor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May he defend the afflicted among the people and save the children of the needy; may he crush the oppressor."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He will bring justice to the poor of the people; He will save the children of the needy, And will break in pieces the oppressor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Help him to defend the poor, to rescue the children of the needy, and to crush their oppressors."
      }
    },
