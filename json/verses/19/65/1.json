    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.65.1",
            "paragraphs": [
              "六五1\"錫安\"。耶路撒冷。\"願\"。參看利未記二十二章21節，以及詩篇第六十一篇8節和利未記七章12至17節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 人们在锡安赞美你， 向你履行誓言。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！在錫安城裡， 人們都在等候要頌讚你， 他們也要向你償還所許的願。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，錫安的人都等候讚美你； 所許的願也要向你償還。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There will be silence before You, and praise in Zion, O God, And to You the vow will be performed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Praise awaits you, our God, in Zion; to you our vows will be fulfilled."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What mighty praise, O God, belongs to you in Zion. We will fulfill our vows to you,"
      }
    },
