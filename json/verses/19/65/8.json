    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你奇妙的作为使远在地极的人心生敬畏， 你使日出之地和日落之处都传来欢呼声。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，住在地極的人都因你的神蹟起了敬畏的心； 你使日出和日落之地的人都歡呼歌唱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住在地極的人因你的神蹟懼怕； 你使日出日落之地都歡呼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They who dwell in the ends of the earth stand in awe of Your signs; You make the dawn and the sunset shout for joy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The whole earth is filled with awe at your wonders; where morning dawns, where evening fades, you call forth songs of joy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who live at the ends of the earth stand in awe of your wonders. From where the sun rises to where it sets, you inspire shouts of joy."
      }
