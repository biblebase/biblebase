    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你降下甘霖，浇透垄沟， 滋润垄背，使地松软、 长出庄稼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你灌溉地的犁溝，潤平犁脊， 又降雨露使地鬆軟， 並且賜福給地上所生長的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你澆透地的犂溝，潤平犂脊， 降甘霖，使地軟和； 其中發長的，蒙你賜福。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You water its furrows abundantly, You settle its ridges, You soften it with showers, You bless its growth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You drench its furrows and level its ridges; you soften it with showers and bless its crops."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You drench the plowed ground with rain, melting the clods and leveling the ridges. You soften the earth with showers and bless its abundant crops."
      }
    },
