    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华是王，祂身披威严； 耶和华身披威严，腰束力量。 世界坚立不动。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華作王，他以威嚴為衣； 耶和華以威嚴為衣，以能力束腰， 世界就得以堅立，永不動搖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華作王！ 他以威嚴為衣穿上； 耶和華以能力為衣，以能力束腰， 世界就堅定，不得動搖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord reigns, He is clothed with majesty; The Lord has clothed and girded Himself with strength; Indeed, the world is firmly established, it will not be moved."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord reigns, he is robed in majesty; the Lord is robed in majesty and armed with strength; indeed, the world is established, firm and secure."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord is king! He is robed in majesty. Indeed, the Lord is robed in majesty and armed with strength. The world stands firm and cannot be shaken."
      }
    },
