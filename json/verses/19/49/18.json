    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们活着时自以为有福， 人们都赞扬他们的成就，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他在世的時候，雖然自稱是有福的，並且說： “只要你豐足，人就稱讚你。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他活着的時候，雖然自誇為有福 （你若利己，人必誇獎你）；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Though while he lives he congratulates himself— And though men praise you when you do well for yourself—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Though while they live they count themselves blessed— and people praise you when you prosper—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In this life they consider themselves fortunate and are applauded for their success."
      }
    },
