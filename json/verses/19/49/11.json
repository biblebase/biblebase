    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尽管他们用自己的名字来为产业命名， 坟墓却是他们永远的归宿， 世代的住处。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們心裡想，他們的家必永存， 他們的住處必留到萬代； 他們以自己的名，稱自己的地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們心裏思想： 他們的家室必永存， 住宅必留到萬代； 他們以自己的名稱自己的地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their inner thought is that their houses are forever And their dwelling places to all generations; They have called their lands after their own names."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their tombs will remain their houses forever, their dwellings for endless generations, though they had named lands after themselves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The grave is their eternal home, where they will stay forever. They may name their estates after themselves,"
      }
    },
