    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "危难来临，四面被奸诈之徒包围时，我何必惧怕？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在患難的日子，暗中迫害我的惡人圍繞我的時候， 我何須懼怕呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在患難的日子，奸惡隨我腳跟，四面環繞我， 我何必懼怕？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why should I fear in days of adversity, When the iniquity of my foes surrounds me,"
