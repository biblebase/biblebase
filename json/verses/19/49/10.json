    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众所周知，即使智者也要死去， 愚人和庸者也要灭亡， 将他们的财富留给别人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他看見智慧人死去， 愚昧人和愚頑人一同滅亡， 把他們的財產留給別人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "要知道，人必看见智慧人也会死； 愚昧人和愚拙人一同灭亡， 把他们的财富留给别人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必見智慧人死， 又見愚頑人和畜類人一同滅亡， 將他們的財貨留給別人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For he sees that even the wise die; the fool and the stupid alike must perish and leave their wealth to others."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For he sees that even wise men die; The stupid and the senseless alike perish And leave their wealth to others."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For all can see that the wise die, that the foolish and the senseless also perish, leaving their wealth to others."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For he sees wise men die; Likewise the fool and the senseless person perish, And leave their wealth to others."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who are wise must finally die, just like the foolish and senseless, leaving all their wealth behind."
      }
    },
