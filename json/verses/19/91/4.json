    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂必用祂的羽毛遮盖你， 用祂的双翼保护你， 祂的信实是你的盾牌和壁垒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必用自己的羽毛遮蓋你， 你要投靠在他的翅膀底下； 他的信實像盾牌，像堅壘。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必用自己的翎毛遮蔽你； 你要投靠在他的翅膀底下； 他的誠實是大小的盾牌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will cover you with His pinions, And under His wings you may seek refuge; His faithfulness is a shield and bulwark."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will cover you with his feathers, and under his wings you will find refuge; his faithfulness will be your shield and rampart."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will cover you with his feathers. He will shelter you with his wings. His faithful promises are your armor and protection."
      }
    },
