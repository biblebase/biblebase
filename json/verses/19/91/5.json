    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.91.5-7",
            "paragraphs": [
              "九一5～7在神裏面，我們無論何時（5節），無論在何等危險的情況中（6節）和在何種境況裏（7節），都必得安穩。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你必不惧怕黑夜的恐怖， 或白日的飞箭，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不必害怕黑夜的驚恐， 或是白日的飛箭；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必不怕黑夜的驚駭， 或是白日飛的箭，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will not be afraid of the terror by night, Or of the arrow that flies by day;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will not fear the terror of night, nor the arrow that flies by day,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not be afraid of the terrors of the night, nor the arrow that flies in the day."
      }
