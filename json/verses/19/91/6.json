    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.91.5-7",
            "paragraphs": [
              "九一5～7在神裏面，我們無論何時（5節），無論在何等危險的情況中（6節）和在何種境況裏（7節），都必得安穩。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "或黑暗中横行的瘟疫， 或正午肆虐的灾难。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也不必害怕黑暗中流行的瘟疫， 或是在正午把人毀滅的毒病。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也不怕黑夜行的瘟疫， 或是午間滅人的毒病。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Of the pestilence that stalks in darkness, Or of the destruction that lays waste at noon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "nor the pestilence that stalks in the darkness, nor the plague that destroys at midday."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not dread the disease that stalks in darkness, nor the disaster that strikes at midday."
      }
    },
