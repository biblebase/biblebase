    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.84.6",
            "paragraphs": [
              "八四6\"流淚谷\"。不是某個指定的地方，而是指一個流淚的地方或荒涼的山谷。這裏的意思是：朝聖者的煩惱變成了祝福。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们走过流泪谷， 这谷就变成泉源之地， 秋雨使谷中水塘遍布。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們經過乾旱的山谷， 使這谷變為泉源之地， 更有秋雨使這谷到處都是水池。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們經過「流淚谷」，叫這谷變為泉源之地； 並有秋雨之福蓋滿了全谷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Passing through the valley of Baca they make it a spring; The early rain also covers it with blessings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As they pass through the Valley of Baka, they make it a place of springs; the autumn rains also cover it with pools."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they walk through the Valley of Weeping, it will become a place of refreshing springs. The autumn rains will clothe it with blessings."
      }
    },
