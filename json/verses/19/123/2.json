    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们要仰望我们的上帝耶和华， 如同仆人仰望主人、婢女仰望主母， 一直到蒙祂怜悯。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "僕人的眼睛怎樣望著主人的手， 婢女的眼睛怎樣望著主母的手， 我們的眼睛也照樣望著耶和華我們的　神， 直到他向我們施恩。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "看哪！仆人的眼睛怎样望着自己主人的手， 婢女的眼睛怎样望着自己主母的手， 我们的眼睛也照样望着耶和华我们的神， 直到他恩待我们。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪，僕人的眼睛怎樣望主人的手， 使女的眼睛怎樣望主母的手， 我們的眼睛也照樣望耶和華－我們的神， 直到他憐憫我們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Behold, as the eyes of servants look to the hand of their master, as the eyes of a maidservant to the hand of her mistress, so our eyes look to the Lord our God, till he has mercy upon us."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, as the eyes of servants look to the hand of their master, As the eyes of a maid to the hand of her mistress, So our eyes look to the Lord our God, Until He is gracious to us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As the eyes of slaves look to the hand of their master, as the eyes of a female slave look to the hand of her mistress, so our eyes look to the Lord our God, till he shows us his mercy."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Behold, as the eyes of servants look to the hand of their masters, As the eyes of a maid to the hand of her mistress, So our eyes look to the Lord our God, Until He has mercy on us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We keep looking to the Lord our God for his mercy, just as servants keep their eyes on their master, as a slave girl watches her mistress for the slightest signal."
      }
