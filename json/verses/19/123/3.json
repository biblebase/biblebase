    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你怜悯我们，求你怜悯我们， 因为我们饱受鄙视。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你向我們施恩，求你向我們施恩； 因為我們飽受藐視，到了極點。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华啊，求你恩待我们， 求你恩待我们！ 因为我们实在受尽了藐视——"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你憐憫我們，憐憫我們！ 因為我們被藐視，已到極處。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Have mercy upon us, O Lord, have mercy upon us, for we have had more than enough of contempt."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be gracious to us, O Lord, be gracious to us, For we are greatly filled with contempt."
      },
