    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.43.3-4",
            "paragraphs": [
              "四三3,4“聖山”（耶路撒冷）和“祭壇”是詩人達到最高目標的途徑——最高目標就是與神自己的相交。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要到上帝的祭坛前， 到赐我喜乐的上帝面前。 上帝啊，我的上帝， 我要弹琴赞美你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我就走到　神的祭壇前， 到　神、我極大的喜樂那裡。 　神啊！我的　神啊！ 我要彈琴稱讚你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我就走到神的祭壇， 到我最喜樂的神那裏。 神啊，我的神， 我要彈琴稱讚你！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I will go to the altar of God, To God my exceeding joy; And upon the lyre I shall praise You, O God, my God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I will go to the altar of God, to God, my joy and my delight. I will praise you with the lyre, O God, my God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There I will go to the altar of God, to God—the source of all my joy. I will praise you with my harp, O God, my God!"
      }
    },
