    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.43.3-4",
            "paragraphs": [
              "四三3,4“聖山”（耶路撒冷）和“祭壇”是詩人達到最高目標的途徑——最高目標就是與神自己的相交。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你以光明和真理引导我， 领我到你的圣山，到你的居所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你發出你的亮光和真理，好引導我， 帶我到你的聖山， 到你的居所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你發出你的亮光和真實，好引導我， 帶我到你的聖山，到你的居所！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O send out Your light and Your truth, let them lead me; Let them bring me to Your holy hill And to Your dwelling places."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Send me your light and your faithful care, let them lead me; let them bring me to your holy mountain, to the place where you dwell."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Send out your light and your truth; let them guide me. Let them lead me to your holy mountain, to the place where you live."
      }
