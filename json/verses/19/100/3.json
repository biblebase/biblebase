    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要知道耶和华是上帝， 祂创造了我们， 我们属于祂，是祂的子民， 是祂草场上的羊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要知道耶和華是　神； 他創造了我們，我們是屬他的； 我們是他的子民，也是他草場上的羊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們當曉得耶和華是神！ 我們是他造的，也是屬他的； 我們是他的民，也是他草場的羊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Know that the Lord Himself is God; It is He who has made us, and not we ourselves; We are His people and the sheep of His pasture."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Know that the Lord is God. It is he who made us, and we are his; we are his people, the sheep of his pasture."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Acknowledge that the Lord is God! He made us, and we are his. We are his people, the sheep of his pasture."
      }
    },
