    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.100.2",
            "paragraphs": [
              "100:2",
              "當來向他歌唱：無論個人還是聚會，都應當首先“向耶和華”歌唱(1節)，並要因感受到他的同在而歡喜快樂。在歌唱中我們記念他創造並救贖我們的大恩，認識到我們是他的民，他是我們的大牧人(3節)；並要歌頌他永不止息的慈愛和信實(5節；參弗5:19註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要高高兴兴地事奉耶和华， 到祂面前来欢唱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "應當歡歡喜喜事奉耶和華， 歡唱著到他的面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們當樂意事奉耶和華， 當來向他歌唱！"
      },
