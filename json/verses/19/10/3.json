      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人誇耀心中的慾望， 他稱讚貪財的人，卻藐視耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為惡人以心願自誇； 貪財的背棄耶和華，並且輕慢他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the wicked boasts of his heart’s desire, And the greedy man curses and spurns the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He boasts about the cravings of his heart; he blesses the greedy and reviles the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For they brag about their evil desires; they praise the greedy and curse the Lord."
      }
