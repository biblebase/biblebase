    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.39.13",
            "paragraphs": [
              "三九13\"寬容我\"。意思是不再因罪懲罰他，以至他可以享受他的日子。\"力量復原\"。高興快活的樣子。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你宽恕我， 好让我在离世之前能重展笑容。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你不要怒視我， 使我在去而不返之先，可以喜樂。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你寬容我， 使我在去而不返之先可以力量復原。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Turn Your gaze away from me, that I may smile again Before I depart and am no more.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Look away from me, that I may enjoy life again before I depart and am no more.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Leave me alone so I can smile again before I am gone and exist no more."
      }
    },
