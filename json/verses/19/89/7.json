    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.89.5-7",
            "paragraphs": [
              "八九5～7\"聖者……神的眾子……聖者\"。指天使。參看第二十九章1節的腳註。"
            ]
          },
          {
            "verse": "psa.89.5-14",
            "paragraphs": [
              "八九5～14詩人因神的屬性（5～8節）和祂的作為--尤其是在創世時的作為（10～14節）--而贊美祂。\"拉哈伯\"指埃及，如在第八十七篇4節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂在众圣者的会中大受敬畏， 祂的威严无与伦比。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在聖者的議會中，　神是令人驚懼的， 比一切在他周圍的都更為可畏。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他在聖者的會中，是大有威嚴的神， 比一切在他四圍的更可畏懼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A God greatly feared in the council of the holy ones, And awesome above all those who are around Him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the council of the holy ones God is greatly feared; he is more awesome than all who surround him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The highest angelic powers stand in awe of God. He is far more awesome than all who surround his throne."
      }
    },
