    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.89.5-14",
            "paragraphs": [
              "八九5～14詩人因神的屬性（5～8節）和祂的作為--尤其是在創世時的作為（10～14節）--而贊美祂。\"拉哈伯\"指埃及，如在第八十七篇4節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天地都属于你， 世界和其中的一切都是你造的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天是你的，地也是你的； 世界和世界充滿的，都是你建立的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天屬你，地也屬你； 世界和其中所充滿的都為你所建立。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The heavens are Yours, the earth also is Yours; The world and all it contains, You have founded them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The heavens are yours, and yours also the earth; you founded the world and all that is in it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The heavens are yours, and the earth is yours; everything in the world is yours—you created it all."
      }
    },
