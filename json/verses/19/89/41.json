    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "路人都趁机劫掠他的财物， 邻居都嘲笑他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "過路的都搶掠他， 他成了鄰國的人羞辱的對象。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡過路的人都搶奪他； 他成為鄰邦的羞辱。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All who pass along the way plunder him; He has become a reproach to his neighbors."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All who pass by have plundered him; he has become the scorn of his neighbors."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone who comes along has robbed him, and he has become a joke to his neighbors."
      }
