    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.89.29",
            "paragraphs": [
              "八九29彌賽亞是最終要應驗大衛之約、在地上坐大衛的\"寶座\"作王的那位。基督現今並非在天上坐在大衛的寶座上。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使他的后裔永无穷尽， 让他的王位与天同存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要使他的後裔存到永遠， 使他的王位好像天一樣長久。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我要坚立他的后裔，直到永远； 使他的宝座如天之久。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我也要使他的後裔存到永遠， 使他的寶座如天之久。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I will establish his offspring forever and his throne as the days of the heavens."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I will establish his descendants forever And his throne as the days of heaven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will establish his line forever, his throne as long as the heavens endure."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "His seed also I will make to  endure forever, And his throne as the days of heaven."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will preserve an heir for him; his throne will be as endless as the days of heaven."
      }
