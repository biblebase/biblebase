      "leili": {
        "content": [
          {
            "verse": "psa.89.38-45",
            "paragraphs": [
              "八九38～45這哀歌聲言，神由於大衛王朝的君王落敗蒙羞而摒棄這約。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使他未老先衰， 满面羞愧。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你減少了他青春的日子， 使他披上恥辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你減少他青年的日子， 又使他蒙羞。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have shortened the days of his youth; You have covered him with shame. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have cut short the days of his youth; you have covered him with a mantle of shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have made him old before his time and publicly disgraced him. Interlude"
      }
    },
