    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.89.30-37",
            "paragraphs": [
              "八九30～37雖然神會懲罰不順服的人，但卻不會背棄祂與大衛所立的約。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我不会收回我的慈爱， 也不会背弃我的信实。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但我必不把我的慈愛從他身上收回， 也必不背棄我的信實。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "然而，我不会从他身上收回我的慈爱， 也不会背弃我的信实。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是我必不將我的慈愛全然收回， 也必不叫我的信實廢棄。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "but I will not remove from him my steadfast love or be false to my faithfulness."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I will not break off My lovingkindness from him, Nor deal falsely in My faithfulness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but I will not take my love from him, nor will I ever betray my faithfulness."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Nevertheless My lovingkindness I will not utterly take from him, Nor allow My faithfulness to fail."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I will never stop loving him nor fail to keep my promise to him."
      }
    },
