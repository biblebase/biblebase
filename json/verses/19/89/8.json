    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.89.5-14",
            "paragraphs": [
              "八九5～14詩人因神的屬性（5～8節）和祂的作為--尤其是在創世時的作為（10～14節）--而贊美祂。\"拉哈伯\"指埃及，如在第八十七篇4節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之上帝耶和华啊， 谁能像你能力伟大，信实无比？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華萬軍之　神啊！有誰像你？ 耶和華啊！你是大能的，你的信實在你的周圍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－萬軍之神啊， 哪一個大能者像你耶和華？ 你的信實是在你的四圍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord God of hosts, who is like You, O mighty Lord? Your faithfulness also surrounds You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who is like you, Lord God Almighty? You, Lord, are mighty, and your faithfulness surrounds you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord God of Heaven’s Armies! Where is there anyone as mighty as you, O Lord? You are entirely faithful."
      }
    },
