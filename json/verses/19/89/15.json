    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 懂得向你欢呼的人有福了， 他们走在你的荣光之中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "曉得向你歡呼的人民是有福的， 耶和華啊！他們在你的臉光中行走。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "知道向你歡呼的，那民是有福的！ 耶和華啊，他們在你臉上的光裏行走。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How blessed are the people who know the joyful sound! O Lord, they walk in the light of Your countenance."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Blessed are those who have learned to acclaim you, who walk in the light of your presence, Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Happy are those who hear the joyful call to worship, for they will walk in the light of your presence, Lord."
      }
