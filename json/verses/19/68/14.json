    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.68.14",
            "paragraphs": [
              "六八14神趕散迦南的列王時，他們就象雪被風吹往陰森的（示劍附近的）\"撒們\"山麓。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "全能的上帝驱散了众王， 势如大雪洒落在撒们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "全能者趕散境內列王的時候， 就像撒們山上下大雪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "全能者在境內趕散列王的時候， 勢如飄雪在撒們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the Almighty scattered the kings there, It was snowing in Zalmon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the Almighty scattered the kings in the land, it was like snow fallen on Mount Zalmon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Almighty scattered the enemy kings like a blowing snowstorm on Mount Zalmon."
      }
