    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.68.5",
            "paragraphs": [
              "68:5",
              "作孤兒的父：新舊約聖經都強調說神是信徒的父，他喜悅保護他百姓中那些軟弱、貧乏、受屈和孤獨的人。信徒若感到自己在世上孤單無助，就當求神把自己放在他特別的眷顧和庇護中(參路7:13註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "住在圣所的上帝是孤儿的父亲，寡妇的保护者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神在他的聖居所， 作孤兒的父親，作寡婦的伸冤者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神在他的聖所作孤兒的父， 作寡婦的伸冤者。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A father of the fatherless and a judge for the widows, Is God in His holy habitation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A father to the fatherless, a defender of widows, is God in his holy dwelling."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Father to the fatherless, defender of widows— this is God, whose dwelling is holy."
      }
    },
