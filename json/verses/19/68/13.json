    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.68.13",
            "paragraphs": [
              "六八13雖然有些以色列人不肯參加戰役，但神仍把戰利品賜給祂的子民（鴿子）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即使羊圈里的看羊人也披金戴银， 就像鸽子镀银的翅膀和金光闪闪的羽毛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們躺臥在羊圈中， 如同鴿子的翅膀鍍上白銀， 羽毛鍍上黃金。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們安臥在羊圈的時候， 好像鴿子的翅膀鍍白銀，翎毛鍍黃金一般。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you lie down among the sheepfolds, You are like the wings of a dove covered with silver, And its pinions with glistening gold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even while you sleep among the sheep pens, the wings of my dove are sheathed with silver, its feathers with shining gold.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even those who lived among the sheepfolds found treasures— doves with wings of silver and feathers of gold."
      }
    },
