      "leili": {
        "content": [
          {
            "verse": "psa.68.11",
            "paragraphs": [
              "六八11\"傳好信息的婦女成了大群\"。以色列得勝的時候，婦女們大都以歌唱和舞蹈來慶祝（出一五20；撒上一八6,7）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主下达命令， 成群的妇女便奔走相告："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主發出命令， 傳揚好信息的婦女就成了一大群。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主發命令， 傳好信息的婦女成了大羣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord gives the command; The women who proclaim the good tidings are a great host:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord announces the word, and the women who proclaim it are a mighty throng:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord gives the word, and a great army brings the good news."
      }
    },
