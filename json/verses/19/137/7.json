    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.137.7",
            "paragraphs": [
              "一三七7有關以東人犯罪得罪以色列人的詳情，參看俄巴底亞書11至14節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你记住耶路撒冷沦陷时以东人的行径。 他们喊道： “拆毁这城，把它夷为平地！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你記念以東人在耶路撒冷遭難的日子所行的， 他們說：“拆毀它，拆毀它， 直拆到根基。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷  遭難的日子， 以東人說：拆毀！拆毀！ 直拆到根基！ 耶和華啊，求你記念這仇！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Remember, O Lord, against the sons of Edom The day of Jerusalem, Who said, “Raze it, raze it To its very foundation.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember, Lord, what the Edomites did on the day Jerusalem fell. “Tear it down,” they cried, “tear it down to its foundations!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, remember what the Edomites did on the day the armies of Babylon captured Jerusalem. “Destroy it!” they yelled. “Level it to the ground!”"
      }
    },
