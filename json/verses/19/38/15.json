        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！我等候你； 主我的　神啊！你必應允我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我仰望你！ 主－我的神啊，你必應允我！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I hope in You, O Lord; You will answer, O Lord my God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, I wait for you; you will answer, Lord my God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I am waiting for you, O Lord. You must answer for me, O Lord my God."
      }
