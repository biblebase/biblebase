    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.38.9-14",
            "paragraphs": [
              "孤寂的受害者（三十八9～14）",
              "從開頭的主字（此處不是耶和華一字，而是如第22節，“我的主人”），以至最後在朋友離棄、仇敵惡言以向之時，都沒有回話（14節），可以看出他痛定思痛的謙卑。"
            ]
          },
          {
            "verse": "psa.38.12-14",
            "paragraphs": [
              "12～14.第12節口舌不停的情形，與第13、14節的沈默，成了強烈的對比，讓人憶起主受審判的那一幕。大衛的沈默一方面是出於信心（15節；參，三十七7），一方面是因認罪（3～5節；參，撒下十六11）；但我們主的沈默，以及其與我們的關系，見彼得前書二18～25。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.38.13-14",
            "paragraphs": [
              "三八13,14大衛沒有因應仇敵的指控為自己辯護（12節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我就像一个不能听、不能辩的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我竟變成了一個像是不能聽見的人， 變成了一個口中不能反駁的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我如不聽見的人， 口中沒有回話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yes, I am like a man who does not hear, And in whose mouth are no arguments."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have become like one who does not hear, whose mouth can offer no reply."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I choose to hear nothing, and I make no reply."
      }
    },
