    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿所有寻求你的人因你而欢喜快乐， 愿渴望蒙你拯救的人时常说： “上帝当受尊崇！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願所有尋求你的， 都因你歡喜快樂； 願那些喜愛你救恩的， 常說：“要尊　神為大。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願一切尋求你的，因你高興歡喜； 願那些喜愛你救恩的，常說：當尊神為大！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let all who seek You rejoice and be glad in You; And let those who love Your salvation say continually, “Let God be magnified.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But may all who seek you rejoice and be glad in you; may those who long for your saving help always say, “The Lord is great!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But may all who search for you be filled with joy and gladness in you. May those who love your salvation repeatedly shout, “God is great!”"
      }
    },
