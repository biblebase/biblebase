    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.82.1",
            "paragraphs": [
              "八二1\"諸神\"。指人類的統治者與百姓的審判官（也見於第6節；參看五八1和約一○34的腳註）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝站在天庭的会中， 向众神明宣布祂的判决，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神站在大能者的會中， 在眾神之中施行審判，說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神站在有權力者的會中， 在諸神中行審判，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God takes His stand in His own congregation; He judges in the midst of the rulers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God presides in the great assembly; he renders judgment among the “gods”:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God presides over heaven’s court; he pronounces judgment on the heavenly beings:"
      }
