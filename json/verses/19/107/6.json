    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.107.6",
            "paragraphs": [
              "107:6",
              "哀求耶和華：詩人4次用到這樣的語句，並4次提到神“從他們的禍患中”搭救他們(6,13,19,28節)。神時常會領自己的兒女到一個無法自助自救、而他人也不能幫助的地步，好叫他們謙卑己心，以孩子般的信心來呼求他。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们在危难中呼求耶和华， 祂就拯救他们脱离困境，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在急難中呼求耶和華， 耶和華就救他們脫離困苦；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，他們在苦難中哀求耶和華； 他從他們的禍患中搭救他們，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they cried out to the Lord in their trouble; He delivered them out of their distresses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they cried out to the Lord in their trouble, and he delivered them from their distress."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Lord, help!” they cried in their trouble, and he rescued them from their distress."
      }
    },
