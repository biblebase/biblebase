      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们因风平浪静而欢喜， 祂带领他们到所向往的港湾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "風平浪靜了，他們就歡喜； 他引領他們到他們所願去的港口。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "風息浪靜，他們便歡喜； 他就引他們到所願去的海口。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they were glad because they were quiet, So He guided them to their desired haven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They were glad when it grew calm, and he guided them to their desired haven."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What a blessing was that stillness as he brought them safely into harbor!"
      }
