    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但祂搭救贫苦的人脱离苦难， 使他们家族兴旺，多如羊群。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他卻把貧窮的人安置在高處，免受苦難， 使他的家族繁衍，好像羊群。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他卻將窮乏人安置在高處，脫離苦難， 使他的家屬多如羊羣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But He sets the needy securely on high away from affliction, And makes his families like a flock."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But he lifted the needy out of their affliction and increased their families like flocks."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But he rescues the poor from trouble and increases their families like flocks of sheep."
      }
    },
