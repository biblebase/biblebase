    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有一条河给上帝的城——至高者的圣所带来喜乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一條河，它眾多的支流使　神的城充滿快樂； 這城就是至高者居住的聖所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一道河，這河的分汊使神的城歡喜； 這城就是至高者居住的聖所。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is a river whose streams make glad the city of God, The holy dwelling places of the Most High."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There is a river whose streams make glad the city of God, the holy place where the Most High dwells."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A river brings joy to the city of our God, the sacred home of the Most High."
      }
    },
