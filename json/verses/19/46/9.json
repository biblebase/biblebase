    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂平息天下的战争， 祂断弓、折枪、烧毁盾牌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他使戰爭止息，直到地極； 他把弓折毀，把矛砍斷， 把戰車用火焚燒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他止息刀兵，直到地極； 他折弓、斷槍，把戰車焚燒在火中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He makes wars to cease to the end of the earth; He breaks the bow and cuts the spear in two; He burns the chariots with fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He makes wars cease to the ends of the earth. He breaks the bow and shatters the spear; he burns the shields with fire."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He causes wars to end throughout the earth. He breaks the bow and snaps the spear; he burns the shields with fire."
      }
    },
