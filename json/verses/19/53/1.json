        "content": [
          {
            "verse": "psa.53.1",
            "paragraphs": [
              "53:1",
              "1~6愚頑人：這首詩與詩14篇幾乎是一樣的(參14:1~7註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.53.1",
            "paragraphs": [
              "五三1留意思想（\"沒有神\"）怎樣影響行為（\"邪惡\"）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚昧人心里想： “没有上帝。” 他们全然败坏，行为邪恶， 无人行善。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚頑人心裡說：“沒有　神。” 他們都是敗壞，行了可憎的不義； 沒有一個行善的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愚顽人心里说：“没有神！” 他们都败坏，行可憎不义之事； 没有人行善。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚頑人心裏說：沒有神。 他們都是邪惡，行了可憎惡的罪孽； 沒有一個人行善。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The fool says in his heart, “There is no God.” They are corrupt, doing abominable iniquity; there is none who does good."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The fool has said in his heart, “There is no God,” They are corrupt, and have committed abominable injustice; There is no one who does good."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The fool says in his heart, “There is no God.” They are corrupt, and their ways are vile; there is no one who does good."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The fool has said in his heart, “There  is no God.” They are corrupt, and have done abominable iniquity; There  is none who does good."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Only fools say in their hearts, “There is no God.” They are corrupt, and their actions are evil; not one of them does good!"
      }
    },
