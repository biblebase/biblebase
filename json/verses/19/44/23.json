    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.44.23-26",
            "paragraphs": [
              "四四23～26懇求神醒來看看他們的苦況，並在他們即將死去（\"伏於塵土\"）的處境下\"幫助\"他們。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主啊，求你醒来， 你为何沉睡？ 求你起来，不要永远丢弃我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主啊！求你醒來，為甚麼還睡著呢？ 求你起來，不要永遠棄絕我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主啊，求你睡醒，為何儘睡呢？ 求你興起，不要永遠丟棄我們！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Arouse Yourself, why do You sleep, O Lord? Awake, do not reject us forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Awake, Lord! Why do you sleep? Rouse yourself! Do not reject us forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wake up, O Lord! Why do you sleep? Get up! Do not reject us forever."
      }
    },
