    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.44.17-22",
            "paragraphs": [
              "四四17～22為國民的忠心辯護，類似一些個人哀悼詩篇裏，詩人對自己無罪的辯解。參看第十七篇3節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝怎会不知道呢？ 祂洞悉人心中的秘密。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神不會查究這事嗎？ 因為他知道人心的隱祕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神豈不鑒察這事嗎？ 因為他曉得人心裏的隱秘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Would not God find this out? For He knows the secrets of the heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "would not God have discovered it, since he knows the secrets of the heart?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God would surely have known it, for he knows the secrets of every heart."
      }
