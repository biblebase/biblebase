    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.44.17-22",
            "paragraphs": [
              "四四17～22為國民的忠心辯護，類似一些個人哀悼詩篇裏，詩人對自己無罪的辯解。參看第十七篇3節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然这一切临到我们身上， 我们却没有忘记你， 也没有违背你的约。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這一切臨到我們身上，我們卻沒有忘記你， 也沒有違背你的約。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這都臨到我們身上， 我們卻沒有忘記你， 也沒有違背你的約。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All this has come upon us, but we have not forgotten You, And we have not dealt falsely with Your covenant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All this came upon us, though we had not forgotten you; we had not been false to your covenant."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All this has happened though we have not forgotten you. We have not violated your covenant."
      }
