    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.44.2",
            "paragraphs": [
              "四四2提到在約書亞帶領下征服迦南地的事。\"我們列祖\"。即以色列。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你亲手赶出外族， 把我们的祖先安置在那里； 你击溃列邦， 使我们的祖先兴旺发达。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你曾親手把列國趕出去， 卻栽培了我們的列祖； 你曾苦待眾民， 卻使我們的列祖昌盛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你曾用手趕出外邦人， 卻栽培了我們列祖； 你苦待列邦， 卻叫我們列祖發達。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You with Your own hand drove out the nations; Then You planted them; You afflicted the peoples, Then You spread them abroad."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "With your hand you drove out the nations and planted our ancestors; you crushed the peoples and made our ancestors flourish."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You drove out the pagan nations by your power and gave all the land to our ancestors. You crushed their enemies and set our ancestors free."
      }
    },
