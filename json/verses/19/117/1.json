    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.117.1",
            "paragraphs": [
              "117:1",
              "萬國啊：保羅在羅15:11中引用此節經文，證明舊約聖經早已預言了神將要使萬國萬民得享他的救恩(比較詩67篇)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万国啊，你们要赞美耶和华！ 万民啊，你们要颂赞祂！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬國啊！你們要讚美耶和華； 萬民哪！你們要頌讚他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬國啊，你們都當讚美耶和華！ 萬民哪，你們都當頌讚他！"
      },
      "nasb": {
