    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，他们要暗害我。 耶和华啊，我并未犯罪作恶， 凶残的人却攻击我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！他們埋伏要害我的性命； 強盛的人聚集起來攻擊我； 耶和華啊！這不是因我的過犯， 也不是因我的罪惡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，他們埋伏要害我的命； 有能力的人聚集來攻擊我。 耶和華啊，這不是為我的過犯， 也不是為我的罪愆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For behold, they have set an ambush for my life; Fierce men launch an attack against me, Not for my transgression nor for my sin, O Lord,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See how they lie in wait for me! Fierce men conspire against me for no offense or sin of mine, Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They have set an ambush for me. Fierce enemies are out there waiting, Lord, though I have not sinned or offended them."
      }
    },
