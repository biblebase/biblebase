    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.59.11",
            "paragraphs": [
              "五九11戴維求神懲罰惡人，以至百姓知道犯罪的嚴重，尤其是言語上的犯罪（12節）。他求神把他們分散，而不是殺死。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "保护我们的主啊， 求你不要杀掉他们， 免得我的百姓忘记教训。 求你用你的能力驱散他们， 使他们沦为卑贱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要殺害他們，免得我的人民忘記了； 主啊！你是我們的盾牌， 求你用你的能力，使他們飄流無定， 並且降為卑微。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要殺他們，恐怕我的民忘記。 主啊，你是我們的盾牌； 求你用你的能力使他們四散， 且降為卑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not slay them, or my people will forget; Scatter them by Your power, and bring them down, O Lord, our shield."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But do not kill them, Lord our shield, or my people will forget. In your might uproot them and bring them down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t kill them, for my people soon forget such lessons; stagger them with your power, and bring them to their knees, O Lord our shield."
      }
    },
