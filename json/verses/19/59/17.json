    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，你是我的力量， 我要颂赞你， 你是我的堡垒， 是爱我的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的力量啊！我要向你歌唱； 　神啊！你是我的高臺， 是向我施慈愛的　神。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我的力量啊，我要歌颂你！ 神哪！因为你是我的庇护所， 是我慈爱的神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的力量啊，我要歌頌你； 因為神是我的高臺， 是賜恩與我的神。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "O my Strength, I will sing praises to you, for you, O God, are my fortress, the God who shows me steadfast love."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O my strength, I will sing praises to You; For God is my stronghold, the God who shows me lovingkindness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are my strength, I sing praise to you; you, God, are my fortress, my God on whom I can rely."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "To You, O my Strength, I will sing praises; For God is my defense, My God of mercy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O my Strength, to you I sing praises, for you, O God, are my refuge, the God who shows me unfailing love."
      }
