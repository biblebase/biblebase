    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要看顾国中忠于上帝的人， 让他们与我同住。 行事正直的人才能服侍我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的眼目必看顧國中的誠實人， 使他們與我同住； 行為正直的， 必要侍候我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我眼要看國中的誠實人，叫他們與我同住； 行為完全的，他要伺候我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My eyes shall be upon the faithful of the land, that they may dwell with me; He who walks in a blameless way is the one who will minister to me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My eyes will be on the faithful in the land, that they may dwell with me; the one whose walk is blameless will minister to me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will search for faithful people to be my companions. Only those who are above reproach will be allowed to serve me."
      }
