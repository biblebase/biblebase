    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要消灭暗中毁谤邻居的人， 也不容忍心高气傲的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "暗中誹謗鄰舍的，我必把他滅絕； 眼目高傲，心裡驕橫的， 我必不容忍他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在暗中讒謗他鄰居的，我必將他滅絕； 眼目高傲、心裏驕縱的，我必不容他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whoever secretly slanders his neighbor, him I will destroy; No one who has a haughty look and an arrogant heart will I endure."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever slanders their neighbor in secret, I will put to silence; whoever has haughty eyes and a proud heart, I will not tolerate."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will not tolerate people who slander their neighbors. I will not endure conceit and pride."
      }
