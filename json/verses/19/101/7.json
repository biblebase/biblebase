    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "诡诈的人必不得与我同住， 口出谎言的人必不得侍立在我面前。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "行詭詐的，必不得住在我的家裡； 說謊話的，必不能在我眼前堅立。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行詭詐的，必不得住在我家裏； 說謊話的，必不得立在我眼前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who practices deceit shall not dwell within my house; He who speaks falsehood shall not maintain his position before me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one who practices deceit will dwell in my house; no one who speaks falsely will stand in my presence."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will not allow deceivers to serve in my house, and liars will not stay in my presence."
      }
    },
