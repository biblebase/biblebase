    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.131.2",
            "paragraphs": [
              "一三一2正如孩童經過惱人的斷奶時期，找到平靜安穩，大衛也從所有追逐私利的境況裏得到拯救，在耶和華裏面找到安穩。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的心灵平静安稳， 如同母亲身边断奶的孩子， 我的心灵如同断奶的孩子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的心平靜安穩， 像斷過奶的孩子躺在他母親的懷中， 我的心在我裡面真像斷過奶的孩子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的心平穩安靜， 好像斷過奶的孩子在他母親的懷中； 我的心在我裏面真像斷過奶的孩子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Surely I have composed and quieted my soul; Like a weaned child rests against his mother, My soul is like a weaned child within me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I have calmed and quieted myself, I am like a weaned child with its mother; like a weaned child I am content."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Instead, I have calmed and quieted myself, like a weaned child who no longer cries for its mother’s milk. Yes, like a weaned child is my soul within me."
      }
    },
