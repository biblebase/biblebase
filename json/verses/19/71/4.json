        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的神啊，求你救我脫離惡人的手， 脫離不義和殘暴之人的手。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Rescue me, O my God, from the hand of the wicked, from the grasp of the unjust and cruel man."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Rescue me, O my God, out of the hand of the wicked, Out of the grasp of the wrongdoer and ruthless man,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Deliver me, my God, from the hand of the wicked, from the grasp of those who are evil and cruel."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Deliver me, O my God, out of the hand of the wicked, Out of the hand of the unrighteous and cruel man."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My God, rescue me from the power of the wicked, from the clutches of cruel oppressors."
      }
