    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.71.15",
            "paragraphs": [
              "七一15\"不計其數\"。神無盡的恩慈是不能數算的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然我无法测度你的公义和拯救之恩， 但我要终日传扬你的作为。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的口要述說你的公義， 終日講述你的救恩， 儘管我不知道它們的數量。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的口終日要述說你的公義和你的救恩， 因我不計其數。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My mouth shall tell of Your righteousness And of Your salvation all day long; For I do not know the sum of them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My mouth will tell of your righteous deeds, of your saving acts all day long— though I know not how to relate them all."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will tell everyone about your righteousness. All day long I will proclaim your saving power, though I am not skilled with words."
      }
    },
