    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你成为保护我的磐石， 让我可以随时投靠你。 求你下令救我， 因为你是我的磐石和堡垒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你作我避難的磐石，使我可以常來投靠； 你已經下令救我， 因為你是我的巖石、我的堅壘。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你作我常住的磐石； 你已經命定要救我， 因為你是我的巖石，我的山寨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be to me a rock of habitation to which I may continually come; You have given commandment to save me, For You are my rock and my fortress."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
