    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的上帝啊， 我要弹琴赞美你的信实； 以色列的圣者啊， 我要伴随着琴声赞美你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的　神啊！ 我要彈琴稱讚你，稱讚你的信實； 以色列的聖者啊！ 我要鼓瑟歌頌你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的神啊，我要鼓瑟稱讚你， 稱讚你的誠實！ 以色列的聖者啊，我要彈琴歌頌你！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will also praise You with a harp, Even Your truth, O my God; To You I will sing praises with the lyre, O Holy One of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will praise you with the harp for your faithfulness, my God; I will sing praise to you with the lyre, Holy One of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I will praise you with music on the harp, because you are faithful to your promises, O my God. I will sing praises to you with a lyre, O Holy One of Israel."
      }
    },
