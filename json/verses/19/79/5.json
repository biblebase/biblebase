    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你向我们发怒， 要到何时呢？ 难道要到永远吗？ 你的怒火要烧到何时呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！要到幾時呢？ 你要永遠懷怒嗎？ 你的憤恨要像火焚燒嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，這到幾時呢？ 你要動怒到永遠嗎？ 你的憤恨要如火焚燒嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How long, O Lord? Will You be angry forever? Will Your jealousy burn like fire?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How long, Lord? Will you be angry forever? How long will your jealousy burn like fire?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, how long will you be angry with us? Forever? How long will your jealousy burn like fire?"
      }
