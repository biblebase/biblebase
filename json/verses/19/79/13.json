    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，你的子民，你草场上的羊必永远称谢你，世代称颂你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，我們作你的子民，作你牧場上的羊的， 要永遠稱謝你； 我們要世世代代述說你可稱頌的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，你的民，你草場的羊， 要稱謝你，直到永遠； 要述說讚美你的話，直到萬代。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So we Your people and the sheep of Your pasture Will give thanks to You forever; To all generations we will tell of Your praise."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then we your people, the sheep of your pasture, will praise you forever; from generation to generation we will proclaim your praise."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then we your people, the sheep of your pasture, will thank you forever and ever, praising your greatness from generation to generation."
      }
