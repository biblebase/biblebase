    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.79.1",
            "paragraphs": [
              "79:1",
              "1~13外邦人進入：詩人在此祈求神赦免以色列人背道的罪(8~9節)，懲罰那些毀壞耶路撒冷和神聖殿的外邦異族(6~7節；巴比倫人曾於主前586年毀壞耶路撒冷)。他認識到那些外邦異族是神借以傾倒他忿怒的器皿(5節)，然而他們向以色列民所行的卻是出於對神和他選民的恨惡(1~7節；比較賽10:5~11；47:6~7)。詩人如此祈求，乃是因為他關註神的榮耀，盼望神的名在外邦不信的人中被尊崇(9~13節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.79.1-5",
            "paragraphs": [
              "七九1～5這段經文描述耶路撒冷在主前586年毀滅於巴比倫軍隊手中。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 外族人侵占你的产业， 玷污你的圣殿， 使耶路撒冷沦为废墟。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！外族人侵入你的產業， 污穢了你的聖殿， 使耶路撒冷成為廢墟。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，外邦人進入你的產業， 污穢你的聖殿，使耶路撒冷變成荒堆，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O God, the nations have invaded Your inheritance; They have defiled Your holy temple; They have laid Jerusalem in ruins."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "O God, the nations have invaded your inheritance; they have defiled your holy temple, they have reduced Jerusalem to rubble."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O God, pagan nations have conquered your land, your special possession. They have defiled your holy Temple and made Jerusalem a heap of ruins."
      }
    },
