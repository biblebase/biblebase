      "leili": {
        "content": [
          {
            "verse": "psa.79.8-10",
            "paragraphs": [
              "七九8～10詩人祈求神的慈悲憐憫（8,9節），並訴諸神的榮耀（10節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拯救我们的上帝啊， 求你为了自己荣耀的名而帮助我们， 为你名的缘故拯救我们， 赦免我们的罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "拯救我們的　神啊！ 求你因你名的榮耀幫助我們； 為你名的緣故， 搭救我們，赦免我們的罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拯救我們的神啊，求你因你名的榮耀幫助我們！ 為你名的緣故搭救我們，赦免我們的罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Help us, O God of our salvation, for the glory of Your name; And deliver us and forgive our sins for Your name’s sake."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Help us, God our Savior, for the glory of your name; deliver us and forgive our sins for your name’s sake."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Help us, O God of our salvation! Help us for the glory of your name. Save us and forgive our sins for the honor of your name."
      }
    },
