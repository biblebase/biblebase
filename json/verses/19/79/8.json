    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.79.8",
            "paragraphs": [
              "七九8\"先祖的罪孽\"。前代所犯的罪，尤其是拜偶像和不給田地享安息年，結果導致百姓被擄和本詩篇之時代的人受苦。參看歷代誌下三十六章14至21節的腳註。"
            ]
          },
          {
            "verse": "psa.79.8-10",
            "paragraphs": [
              "七九8～10詩人祈求神的慈悲憐憫（8,9節），並訴諸神的榮耀（10節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你不要向我们追讨我们祖先的罪， 愿你快快地怜悯我们， 因为我们已经落入绝望中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你不要記住我們祖先的罪孽； 願你的憐憫快快臨到我們， 因為我們落到極卑微的地步。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你不要記念我們先祖的罪孽，向我們追討； 願你的慈悲快迎着我們， 因為我們落到極卑微的地步。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not remember the iniquities of our forefathers against us; Let Your compassion come quickly to meet us, For we are brought very low."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not hold against us the sins of past generations; may your mercy come quickly to meet us, for we are in desperate need."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not hold us guilty for the sins of our ancestors! Let your compassion quickly meet our needs, for we are on the brink of despair."
      }
    },
