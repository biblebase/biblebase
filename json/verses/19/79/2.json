    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把你仆人的尸体喂飞鸟， 把你忠心子民的尸体给野兽吃，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們把你僕人的屍首， 給空中的飛鳥啄食； 把你聖民的肉， 給地上的野獸吞噬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "把你僕人的屍首交與天空的飛鳥為食， 把你聖民的肉交與地上的野獸，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have given the dead bodies of Your servants for food to the birds of the heavens, The flesh of Your godly ones to the beasts of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They have left the dead bodies of your servants as food for the birds of the sky, the flesh of your own people for the animals of the wild."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They have left the bodies of your servants as food for the birds of heaven. The flesh of your godly ones has become food for the wild animals."
      }
    },
