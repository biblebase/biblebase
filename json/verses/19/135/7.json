    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂使云雾从地极上升， 发出电光，带来雨水， 从祂的仓库带出风来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他使雲霧從地極上升， 發出閃電隨雨而來， 又從他的府庫吹出風來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他使雲霧從地極上騰， 造電隨雨而閃， 從府庫中帶出風來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He causes the vapors to ascend from the ends of the earth; Who makes lightnings for the rain, Who brings forth the wind from His treasuries."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He makes clouds rise from the ends of the earth; he sends lightning with the rain and brings out the wind from his storehouses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He causes the clouds to rise over the whole earth. He sends the lightning with the rain and releases the wind from his storehouses."
      }
