    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华在天上、地下、海洋、 深渊按自己的旨意行事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在天上，在地上，在海中，在一切深處， 耶和華喜歡甚麼，就作甚麼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華在天上，在地下， 在海中，在一切的深處， 都隨自己的意旨而行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whatever the Lord pleases, He does, In heaven and in earth, in the seas and in all deeps."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord does whatever pleases him, in the heavens and on the earth, in the seas and all their depths."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord does whatever pleases him throughout all heaven and earth, and on the seas and in their depths."
      }
