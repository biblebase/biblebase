    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂救我的性命脱离死亡， 止住我眼中的泪水， 使我双脚免于滑倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主啊！你救了我的性命脫離死亡， 你使我的眼睛不致流淚， 使我的雙腳不致跌倒。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "是的，耶和华搭救了我的灵魂脱离死亡， 使我的眼睛不致流泪， 使我的脚不致绊跌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主啊，你救我的命免了死亡， 救我的眼免了流淚， 救我的腳免了跌倒。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For you have delivered my soul from death, my eyes from tears, my feet from stumbling;"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You have rescued my soul from death, My eyes from tears, My feet from stumbling."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For you, Lord, have delivered me from death, my eyes from tears, my feet from stumbling,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For You have delivered my soul from death, My eyes from tears, And my feet from falling."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He has saved me from death, my eyes from tears, my feet from stumbling."
      }
    },
