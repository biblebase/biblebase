    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列各支派，耶和华的子民， 都遵照祂赐的法度去那里称谢祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾支派，就是耶和華的支派， 都上那裡去； 照著以色列的定例， 稱頌耶和華的名。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "众支派，就是耶和华的众支派，都上那里去—— 这是照着以色列的法度，为要称颂耶和华的名；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾支派，就是耶和華的支派，上那裏去， 按以色列的常例稱讚耶和華的名。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "to which the tribes go up, the tribes of the Lord, as was decreed for Israel, to give thanks to the name of the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To which the tribes go up, even the tribes of the Lord— An ordinance for Israel— To give thanks to the name of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "That is where the tribes go up— the tribes of the Lord— to praise the name of the Lord according to the statute given to Israel."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Where the tribes go up, The tribes of the Lord, To the Testimony of Israel, To give thanks to the name of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the tribes of Israel—the Lord’s people— make their pilgrimage here. They come to give thanks to the name of the Lord, as the law requires of Israel."
      }
