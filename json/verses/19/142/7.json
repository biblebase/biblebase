    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.142.7",
            "paragraphs": [
              "一四二7\"被囚之地\"。不是監牢，而是因躲避掃羅而受到限制和約束。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你救我脱离牢笼， 好让我赞美你。 因为你恩待我， 义人必聚集在我周围。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你把我從牢獄中領出來， 好讓我稱讚你的名； 義人必環繞著我， 因為你以厚恩待我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你領我出離被囚之地， 我好稱讚你的名。 義人必環繞我， 因為你是用厚恩待我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Bring my soul out of prison, So that I may give thanks to Your name; The righteous will surround me, For You will deal bountifully with me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Set me free from my prison, that I may praise your name. Then the righteous will gather about me because of your goodness to me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Bring me out of prison so I can thank you. The godly will crowd around me, for you are good to me.”"
      }
    },
