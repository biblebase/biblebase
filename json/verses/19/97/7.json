    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.97.7",
            "paragraphs": [
              "九七7\"萬神\"（也見於第9節）。指天使（比較來一6）或異教諸神（九六5），或一切超自然的生物，不管是天使還是不存在的神明。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿一切祭拜神像， 以神像夸口的人都蒙羞。 所有的神明啊， 你们都要向耶和华下拜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願所有事奉雕刻的偶像的， 以虛無的偶像為誇口的，都蒙羞； 眾神哪！你們都要拜他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願一切事奉雕刻的偶像、 靠虛無之神自誇的，都蒙羞愧。 萬神哪，你們都當拜他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let all those be ashamed who serve graven images, Who boast themselves of idols; Worship Him, all you gods."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All who worship images are put to shame, those who boast in idols— worship him, all you gods!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who worship idols are disgraced— all who brag about their worthless gods— for every god must bow to him."
      }
    },
