    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.97.7",
            "paragraphs": [
              "九七7\"萬神\"（也見於第9節）。指天使（比較來一6）或異教諸神（九六5），或一切超自然的生物，不管是天使還是不存在的神明。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿一切祭拜神像， 以神像夸口的人都蒙羞。 所有的神明啊， 你们都要向耶和华下拜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願所有事奉雕刻的偶像的， 以虛無的偶像為誇口的，都蒙羞； 眾神哪！你們都要拜他。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愿一切服事雕刻偶像、以无用的偶像夸耀的，都蒙羞辱； 一切的神明啊，你们都当向他下拜！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願一切事奉雕刻的偶像、 靠虛無之神自誇的，都蒙羞愧。 萬神哪，你們都當拜他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "All worshipers of images are put to shame, who make their boast in worthless idols; worship him, all you gods!"
      },
      "nasb": {
