    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.97.10",
            "paragraphs": [
              "97:10",
              "你們愛耶和華的，都當恨惡罪惡：口稱愛主的人，今生都會受到試驗，以顯明他有幾分恨惡罪惡的心。一個真正重生、與基督合而為一、又有聖靈內住的人必會愛神所愛，恨神所恨。所以，我們應該為世上的邪惡、殘暴和不虔不義感到痛心，為那些生命被罪惡所毀壞的人而憂傷；若神的家中包容罪惡和邪淫敗壞，我們怎能不為之悲痛欲絕(參林前5:2註；來1:9註)！"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "爱耶和华的人都要憎恨罪恶， 因为祂保护敬虔人的生命， 从恶人手中拯救他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們愛耶和華的，都要恨惡罪惡； 耶和華保護聖民的性命， 救他們脫離惡人的手。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們愛耶和華的，都當恨惡罪惡； 他保護聖民的性命， 搭救他們脫離惡人的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hate evil, you who love the Lord, Who preserves the souls of His godly ones; He delivers them from the hand of the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let those who love the Lord hate evil, for he guards the lives of his faithful ones and delivers them from the hand of the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You who love the Lord, hate evil! He protects the lives of his godly people and rescues them from the power of the wicked."
      }
    },
