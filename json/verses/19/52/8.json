          {
            "verse": "psa.52.8",
            "paragraphs": [
              "五二8\"象……青橄欖樹\"。興旺與長壽的象征。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我就像上帝殿中的一棵橄榄树，枝繁叶茂， 我永永远远信靠上帝的慈爱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於我，我就像神殿中茂盛的橄欖樹一樣； 我倚靠　神的慈愛，直到永永遠遠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於我，就像神殿中的青橄欖樹； 我永永遠遠倚靠神的慈愛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, I am like a green olive tree in the house of God; I trust in the lovingkindness of God forever and ever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I am like an olive tree flourishing in the house of God; I trust in God’s unfailing love for ever and ever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I am like an olive tree, thriving in the house of God. I will always trust in God’s unfailing love."
      }
