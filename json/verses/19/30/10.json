    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.30.6-10",
            "paragraphs": [
              "愚人的自誇（三十6～10）",
              "平順的希伯來文字根出現的時候，總離不開安舒的環境和粗心隨便，參，如：耶利米書二十二21的不肯聽從，與箴言一32令人遭災的自滿自大；而箴言的底下一節，則說明了粗心隨便與真正無憂無慮的分別。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你垂听我的呼求，怜悯我！ 耶和华啊，求你帮助我！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你垂聽，恩待我； 耶和華啊！求你幫助我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你應允我，憐恤我！ 耶和華啊，求你幫助我！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Hear, O Lord, and be gracious to me; O Lord, be my helper.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
