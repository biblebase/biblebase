    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.5.7",
            "paragraphs": [
              "五7\"慈愛\"。希伯來文是hesed，意思是\"堅定不移的愛\"。參看何西阿書二章19節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你有无限的慈爱， 我要进入你的居所， 我要满怀敬畏地向你的圣殿下拜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於我，我必憑著你豐盛的慈愛， 進入你的殿； 我要存著敬畏你的心， 向你的聖所敬拜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "至於我，我必憑你豐盛的慈愛進入你的居所； 我必存敬畏你的心向你的聖殿下拜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, by Your abundant lovingkindness I will enter Your house, At Your holy temple I will bow in reverence for You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I, by your great love, can come into your house; in reverence I bow down toward your holy temple."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because of your unfailing love, I can enter your house; I will worship at your Temple with deepest awe."
      }
    },
