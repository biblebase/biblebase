    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.138.2",
            "paragraphs": [
              "一三八2\"你的話\"。神回應詩人之禱告所說的話，超過詩人從前從神的名字所認識的一切。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要向你的圣殿下拜， 称谢你的名， 因为你是慈爱信实的， 你显明自己的尊名和应许伟大无比。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要向你的聖殿下拜， 我要因你的慈愛和信實稱謝你的名； 因為你使你的名和應許顯為大， 超過一切。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要向你的聖殿下拜， 為你的慈愛和誠實稱讚你的名； 因你使你的話顯為大， 過於你所應許的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will bow down toward Your holy temple And give thanks to Your name for Your lovingkindness and Your truth; For You have magnified Your word according to all Your name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will bow down toward your holy temple and will praise your name for your unfailing love and your faithfulness, for you have so exalted your solemn decree that it surpasses your fame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I bow before your holy Temple as I worship. I praise your name for your unfailing love and faithfulness; for your promises are backed by all the honor of your name."
      }
    },
