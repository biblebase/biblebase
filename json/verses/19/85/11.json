    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.85.9-11",
            "paragraphs": [
              "八五9～11當神的\"榮耀\"在地上掌權，百姓便要經歷到第10、11節的特質（在千禧年達到完全）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "忠信从地而生， 公义从天而现。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "信實從地上生出， 公義從天上俯視。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誠實從地而生； 公義從天而現。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Truth springs from the earth, And righteousness looks down from heaven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Faithfulness springs forth from the earth, and righteousness looks down from heaven."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Truth springs up from the earth, and righteousness smiles down from heaven."
      }
