    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.85.9-11",
            "paragraphs": [
              "八五9～11當神的\"榮耀\"在地上掌權，百姓便要經歷到第10、11節的特質（在千禧年達到完全）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂拯救敬畏祂的人， 好让祂的荣耀常驻在我们的地上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的救恩的確臨近敬畏他的人， 使他的榮耀住在我們的地上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的救恩誠然與敬畏他的人相近， 叫榮耀住在我們的地上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Surely His salvation is near to those who fear Him, That glory may dwell in our land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely his salvation is near those who fear him, that his glory may dwell in our land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Surely his salvation is near to those who fear him, so our land will be filled with his glory."
      }
    },
