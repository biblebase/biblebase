    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.85.9-11",
            "paragraphs": [
              "八五9～11當神的\"榮耀\"在地上掌權，百姓便要經歷到第10、11節的特質（在千禧年達到完全）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂拯救敬畏祂的人， 好让祂的荣耀常驻在我们的地上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的救恩的確臨近敬畏他的人， 使他的榮耀住在我們的地上。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他的救恩必然在敬畏他的人身边， 好使荣耀居留在我们的地上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的救恩誠然與敬畏他的人相近， 叫榮耀住在我們的地上。"
      },
