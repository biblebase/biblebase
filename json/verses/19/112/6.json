    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.112.4-9",
            "paragraphs": [
              "一一二4～9義人是別人的祝福（4節），他為人慷慨（5,9節），在逆境中也處變不驚（6～8節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他必永不动摇， 义人必永远蒙眷顾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他永遠不會動搖， 義人必永遠被記念。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他永不動搖； 義人被記念，直到永遠。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For he will never be shaken; The righteous will be remembered forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely the righteous will never be shaken; they will be remembered forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Such people will not be overcome by evil. Those who are righteous will be long remembered."
      }
