    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.112.4-9",
            "paragraphs": [
              "一一二4～9義人是別人的祝福（4節），他為人慷慨（5,9節），在逆境中也處變不驚（6～8節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "慷慨借贷， 行事公正的人必亨通。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "恩待人，借貸給別人，按公正處理自己事務的， 這人必享福樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "施恩與人、借貸與人的，這人事情順利； 他被審判的時候要訴明自己的冤。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is well with the man who is gracious and lends; He will maintain his cause in judgment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Good will come to those who are generous and lend freely, who conduct their affairs with justice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Good comes to those who lend money generously and conduct their business fairly."
      }
    },
