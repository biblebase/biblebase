    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他慷慨施舍，周济穷人； 他的仁义永远长存； 他必充满力量，得享尊荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他廣施錢財，賙濟窮人； 他的仁義存到永遠； 他的角必被高舉，大有榮耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
