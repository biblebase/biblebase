    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.69.22",
            "paragraphs": [
              "69:22",
              "22~28願他們的筵席……變為網羅：詩人祈求神向那些抵擋神又加害於義人的惡人施行審判。保羅引用該節經文描述那些棄絕基督和他救恩的猶太人(羅11:9~10)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他们面前的宴席变为网罗， 成为他们的陷阱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願他們的筵席在他們面前變為網羅、 報應和陷阱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願他們的筵席在他們面前變為網羅， 在他們平安的時候變為機檻。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May their table before them become a snare; And when they are in peace, may it become a trap."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the table set before them become a snare; may it become retribution and a trap."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let the bountiful table set before them become a snare and their prosperity become a trap."
      }
    },
