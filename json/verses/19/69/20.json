    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的辱骂使我心碎， 令我绝望无助。 我渴望有人同情，却没有一个； 期望有人安慰，却无一人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "辱罵傷了我的心， 我憂愁難過； 我希望有人同情，卻一個也沒有； 我希望有人安慰，還是找不到一個。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "辱罵傷破了我的心， 我又滿了憂愁。 我指望有人體恤，卻沒有一個； 我指望有人安慰，卻找不着一個。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Reproach has broken my heart and I am so sick. And I looked for sympathy, but there was none, And for comforters, but I found none."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Scorn has broken my heart and has left me helpless; I looked for sympathy, but there was none, for comforters, but I found none."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their insults have broken my heart, and I am in despair. If only one person would show some pity; if only one would turn and comfort me."
      }
    },
