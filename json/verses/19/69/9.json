    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.69.9",
            "paragraphs": [
              "69:9",
              "心裏焦急，如同火燒：詩人對神的殿和神的國充滿熱心和公義之心，這卻使他飽受棄絕、淩辱和孤獨(6~9節)。他斥責罪，又呼求神復興、潔凈並重建他的子民；為此他被那些滿足於屬靈現狀的人們大大欺壓苦害(9~11節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我对你的殿充满炙热的爱， 辱骂你之人的辱骂都落在我身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因我為你的殿，心中迫切如同火燒； 辱罵你的人的辱罵，都落在我身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因我為你的殿心裏焦急，如同火燒， 並且辱罵你人的辱罵都落在我身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For zeal for Your house has consumed me, And the reproaches of those who reproach You have fallen on me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for zeal for your house consumes me, and the insults of those who insult you fall on me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Passion for your house has consumed me, and the insults of those who insult you have fallen on me."
      }
