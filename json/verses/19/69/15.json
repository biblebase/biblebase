    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你不要让洪水淹没我， 深渊吞灭我， 坟墓吞噬我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你不要讓波濤淹沒我， 不要讓深水吞滅我， 也不要讓深坑把我封閉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你不容大水漫過我， 不容深淵吞滅我， 不容坑坎在我以上合口。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May the flood of water not overflow me Nor the deep swallow me up, Nor the pit shut its mouth on me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not let the floodwaters engulf me or the depths swallow me up or the pit close its mouth over me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t let the floods overwhelm me, or the deep waters swallow me, or the pit of death devour me."
      }
    },
