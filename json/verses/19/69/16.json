    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你答应我的祷告， 因为你充满慈爱和良善； 求你以无限的怜悯眷顾我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你應允我，因為你的慈愛美善； 求你照著你豐盛的憐憫轉臉垂顧我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你應允我！ 因為你的慈愛本為美好； 求你按你豐盛的慈悲回轉眷顧我！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Answer me, O Lord, for Your lovingkindness is good; According to the greatness of Your compassion, turn to me,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Answer me, Lord, out of the goodness of your love; in your great mercy turn to me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Answer my prayers, O Lord, for your unfailing love is wonderful. Take care of me, for your mercy is so plentiful."
      }
    },
