    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我连连呼救，已声嘶力竭； 我期盼上帝的帮助，望眼欲穿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我因不住呼求而疲倦，我的喉嚨發乾； 我因等候我的　神，眼睛昏花。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因呼求困乏，喉嚨發乾； 我因等候神，眼睛失明。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am weary with my crying; my throat is parched; My eyes fail while I wait for my God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am worn out calling for help; my throat is parched. My eyes fail, looking for my God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am exhausted from crying for help; my throat is parched. My eyes are swollen with weeping, waiting for my God to help me."
      }
    },
