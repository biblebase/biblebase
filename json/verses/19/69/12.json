      "leili": {
        "content": [
          {
            "verse": "psa.69.12",
            "paragraphs": [
              "六九12所有人，從（\"坐在城門口的\"）法官（參看伯二8；二九7）至\"酒徒\"，都嘲笑他。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
