    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.69.12",
            "paragraphs": [
              "六九12所有人，從（\"坐在城門口的\"）法官（參看伯二8；二九7）至\"酒徒\"，都嘲笑他。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我成了街谈巷议的话题， 醉汉作歌取笑我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "坐在城門口的人對我議論紛紛， 我成了酒徒之歌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "坐在城門口的談論我； 酒徒也以我為歌曲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Those who sit in the gate talk about me, And I am the song of the drunkards."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who sit at the gate mock me, and I am the song of the drunkards."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am the favorite topic of town gossip, and all the drunks sing about me."
      }
    },
