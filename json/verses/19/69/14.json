    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你救我脱离泥沼， 不要让我沉下去； 求你救我脱离恨我的人， 使我离开深渊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你救我脫離淤泥， 不要容我沉下去； 求你救我脫離那些恨我的人， 救我脫離深水。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你搭救我出離淤泥， 不叫我陷在其中； 求你使我脫離那些恨我的人， 使我出離深水。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Deliver me from the mire and do not let me sink; May I be delivered from my foes and from the deep waters."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Rescue me from the mire, do not let me sink; deliver me from those who hate me, from the deep waters."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rescue me from the mud; don’t let me sink any deeper! Save me from those who hate me, and pull me from these deep waters."
      }
