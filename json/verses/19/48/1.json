    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.48.1",
            "paragraphs": [
              "48:1",
              "我們神的城：我們的神、大君王(2節)的城就是耶路撒冷(太5:35)。它要被堅立，“直到永遠”(8節)。神在舊約中所創始的事，他必在末後的新耶路撒冷成全(啟21:10~22:5；參“耶路撒冷城”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华何等伟大！ 我们的上帝在祂的城中， 在祂的圣山上当大受颂赞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在我們　神的城中，在他的聖山上， 耶和華是至大的，應該大受讚美。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華本為大！ 在我們神的城中， 在他的聖山上，該受大讚美。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Great is the Lord, and greatly to be praised, In the city of our God, His holy mountain."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Great is the Lord, and most worthy of praise, in the city of our God, his holy mountain."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How great is the Lord, how deserving of praise, in the city of our God, which sits on his holy mountain!"
      }
    },
