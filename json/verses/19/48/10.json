    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，你秉公行义， 你的名普世皆知， 颂赞你的声音响彻地极。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！你受的讚美， 就像你的名一樣，達到地極； 你的右手滿了公義。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，对你的赞美正如你的名，也达到地极！ 你的右手充满公义。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你受的讚美正與你的名相稱，直到地極！ 你的右手滿了公義。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "As your name, O God, so your praise reaches to the ends of the earth. Your right hand is filled with righteousness."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As is Your name, O God, So is Your praise to the ends of the earth; Your right hand is full of righteousness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like your name, O God, your praise reaches to the ends of the earth; your right hand is filled with righteousness."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "According to Your name, O God, So is Your praise to the ends of the earth; Your right hand is full of righteousness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As your name deserves, O God, you will be praised to the ends of the earth. Your strong right hand is filled with victory."
      }
    },
