    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.48.7",
            "paragraphs": [
              "四八7\"他施的船只\"。雖然是古時最大的船只，但它們並不能抵擋神強大的威力。參看以賽亞書二章16節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你毁灭他们， 如同东风摧毁他施的船只。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你用東風摧毀他施的船隊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你用東風打破他施的船隻。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "With the east wind You break the ships of Tarshish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You destroyed them like ships of Tarshish shattered by an east wind."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You destroyed them like the mighty ships of Tarshish shattered by a powerful east wind."
      }
    },
