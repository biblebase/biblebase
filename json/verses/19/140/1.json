    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.140.1",
            "paragraphs": [
              "140:1",
              "1~13耶和華啊，求你拯救我脫離兇惡的人：我們不僅要祈求神救我們脫離兇惡的人，還要求告他救我們脫離那“惡者”，就是撒但。他是我們最大的敵人，隨時都尋找機會要吞滅、毀壞我們(參太4:10註；太6:13註；參“勝過撒但和鬼魔的能力”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊！ 求你拯救我脱离恶人， 保护我脱离残暴之徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你拯救我脫離惡人， 求你保護我脫離強暴的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你拯救我脫離凶惡的人， 保護我脫離強暴的人！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Rescue me, O Lord, from evil men; Preserve me from violent men"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Rescue me, Lord, from evildoers; protect me from the violent,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, rescue me from evil people. Protect me from those who are violent,"
      }
    },
