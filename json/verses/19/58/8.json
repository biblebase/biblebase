    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他们像蜗牛一样溶为烂泥， 又如流产的胎儿不见天日。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願他們像蝸牛般蠕行消融， 如同婦人流產未見天日的胎兒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願他們像蝸牛消化過去， 又像婦人墜落未見天日的胎。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let them be as a snail which melts away as it goes along, Like the miscarriages of a woman which never see the sun."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May they be like a slug that melts away as it moves along, like a stillborn child that never sees the sun."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May they be like snails that dissolve into slime, like a stillborn child who will never see the sun."
      }
    },
