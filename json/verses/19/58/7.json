    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他们如流水逝去， 愿他们弯弓搭箭时箭头折断。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願他們像流水般消逝； 願他們射箭的時候，箭頭折斷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願他們消滅，如急流的水一般； 他們瞅準射箭的時候，願箭頭彷彿砍斷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let them flow away like water that runs off; When he aims his arrows, let them be as headless shafts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let them vanish like water that flows away; when they draw the bow, let their arrows fall short."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May they disappear like water into thirsty ground. Make their weapons useless in their hands."
      }
    },
