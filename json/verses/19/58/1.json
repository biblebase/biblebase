    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.58.1",
            "paragraphs": [
              "五八1本節可譯作：“掌權者啊，你們真的講公義嗎？世人啊，你們有按公平審判嗎？”。“掌權者”。神（復數），或大能者。那顯然是指不公義的審判者，他們諷刺地被稱為“神”，或他們自己取了那樣一個受尊敬的稱謂。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们做官长的主持公道吗？ 你们的审判公正吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掌權者啊！你們真的講公義嗎？ 你們真的按照正直審判世人嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "世人哪，你們默然不語，真合公義嗎？ 施行審判，豈按正直嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do you indeed speak righteousness, O gods? Do you judge uprightly, O sons of men?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do you rulers indeed speak justly? Do you judge people with equity?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Justice—do you rulers know the meaning of the word? Do you judge the people fairly?"
      }
    },
