    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.58.10",
            "paragraphs": [
              "58:10",
              "義人見仇敵遭報就歡喜：這節經文表現了當邪惡被擊敗摧毀時神子民的滿足和喜樂。同樣，新約信徒也當盼望並祈求神鏟除一切罪惡，等候神在新天新地建立他的國度(參啟6:9~10；11:15~17；18:20；19:1~4,13~18；21:1~7)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人必因恶人遭报而欢欣， 并用恶人的血洗脚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人看見仇敵遭報就歡喜； 他要在惡人的血中洗自己的腳。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人見仇敵遭報就歡喜， 要在惡人的血中洗腳。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The righteous will rejoice when he sees the vengeance; He will wash his feet in the blood of the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The righteous will be glad when they are avenged, when they dip their feet in the blood of the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The godly will rejoice when they see injustice avenged. They will wash their feet in the blood of the wicked."
      }
