    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不，你们心中图谋不义， 你们在地上行凶作恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不是的，你們心中策劃奸惡； 你們的手在地上施行強暴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不然！你們是心中作惡； 你們在地上秤出你們手所行的強暴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No, in heart you work unrighteousness; On earth you weigh out the violence of your hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No, in your heart you devise injustice, and your hands mete out violence on the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No! You plot injustice in your hearts. You spread violence throughout the land."
      }
