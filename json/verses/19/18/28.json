      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必點着我的燈； 耶和華－我的神必照明我的黑暗。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You light my lamp; The Lord my God illumines my darkness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
