    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂救我脱离仇敌， 使我胜过强敌， 拯救我脱离残暴之徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他救我脫離我的仇敵。 你還把我高舉起來，高過那些起來攻擊我的人， 又救我脫離了強暴的人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你救我脱离我的仇敌； 是的，你举起我高过那些兴起敌对我的人， 你又解救我脱离残暴的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你救我脫離仇敵， 又把我舉起，高過那些起來攻擊我的； 你救我脫離強暴的人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "who rescued me from my enemies; yes, you exalted me above those who rose against me; you delivered me from the man of violence."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He delivers me from my enemies; Surely You lift me above those who rise up against me; You rescue me from the violent man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who saves me from my enemies. You exalted me above my foes; from a violent man you rescued me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He delivers me from my enemies. You also lift me up above those who rise against me; You have delivered me from the violent man."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and rescues me from my enemies. You hold me safe beyond the reach of my enemies; you save me from violent opponents."
      }
    },
