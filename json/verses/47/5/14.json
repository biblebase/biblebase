    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为基督的爱激励我们，因为我们确信：基督一人既代替所有的人死了，所有的人就都死了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "原來基督的愛催逼著我們，因為我們斷定一個人替眾人死了，眾人就都死了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "原來基督的愛激勵我們；因我們想，一人既替眾人死，眾人就都死了；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the love of Christ controls us, having concluded this, that one died for all, therefore all died;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For Christ’s love compels us, because we are convinced that one died for all, and therefore all died."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Either way, Christ’s love controls us. Since we believe that Christ died for all, we also believe that we have all died to our old life."
      }
