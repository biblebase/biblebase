    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "2co.10.3",
            "paragraphs": [
              "11:3",
              "你們的心或偏於邪：有些哥林多信徒面臨著上當受騙和接受錯謬福音的極大危險(4節)。他們因為聽從“假使徒”的異端教訓(13節)而偏離了起初全心委身基督的正道。如今在許多教會中也有一些表面上十分仁義的傳道人，積極散布違背聖經真理的異端假道，領人誤入歧途，致使信徒的靈命慘遭極大虧損(參下兩條註釋；太23:13註)。信徒必須警惕，積極防備這樣的人(參“假師傅”一文及“監督及其責任”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为我虽然活在血肉之躯里，但在属灵的争战上，却不是凭血气之勇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們雖然在世上行事，卻不是按照世俗的方式作戰，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為我們雖然在血氣中行事，卻不憑着血氣爭戰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For though we walk in the flesh, we do not war according to the flesh,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For though we live in the world, we do not wage war as the world does."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We are human, but we don’t wage war as humans do."
      }
    },
