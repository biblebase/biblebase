      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們留心行光明的事，不但在主面前，就在人面前也是這樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
