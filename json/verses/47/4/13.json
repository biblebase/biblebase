    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "圣经上说：“我相信，所以才说。”我们有同样的信心，我们也相信，所以才这样说。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "經上記著說：“我信，所以我說話。”我們既然有同樣的信心，也就信，所以也說話，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "照着经上所记的：“我信，故我说”，我们既然有同一个信仰的灵，我们也就信，所以也就说，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我們既有信心，正如經上記着說：「我因信，所以如此說話。」我們也信，所以也說話。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Since we have the same spirit of faith according to what has been written, “I believed, and so I spoke,” we also believe, and so we also speak,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But having the same spirit of faith, according to what is written, “I believed, therefore I spoke,” we also believe, therefore we also speak,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is written: “I believed; therefore I have spoken.” Since we have that same spirit of faith, we also believe and therefore speak,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And since we have the same spirit of faith, according to what is written, “I believed and therefore I spoke,” we also believe and therefore speak,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But we continue to preach because we have the same kind of faith the psalmist had when he said, “I believed in God, so I spoke.”"
      }
    },
