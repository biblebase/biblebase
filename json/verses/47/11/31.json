    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶稣的父——永受称颂的上帝知道我不撒谎。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶穌的父　神是應當永遠受稱頌的，他知道我沒有說謊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那永遠可稱頌之主耶穌的父神知道我不說謊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The God and Father of the Lord Jesus, He who is blessed forever, knows that I am not lying."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The God and Father of the Lord Jesus, who is to be praised forever, knows that I am not lying."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God, the Father of our Lord Jesus, who is worthy of eternal praise, knows I am not lying."
      }
    },
