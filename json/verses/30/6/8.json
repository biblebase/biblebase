    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之上帝——主耶和华凭自己起誓： “我憎恶雅各的骄傲， 厌弃他的城堡。 我要把城和城中的一切都交给敌人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶和華指著自己起誓說： “我痛恨雅各所誇耀的， 憎惡他的堡壘； 我要把城市和城中的一切， 都交給敵人。”這是耶和華萬軍的　神的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華－萬軍之神指着自己起誓說： 我憎惡雅各的榮華，厭棄他的宮殿； 因此，我必將城和其中所有的都交付敵人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord God has sworn by Himself, the Lord God of hosts has declared: “I loathe the arrogance of Jacob, And detest his citadels; Therefore I will deliver up the city and all it contains.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Sovereign Lord has sworn by himself—the Lord God Almighty declares: “I abhor the pride of Jacob and detest his fortresses; I will deliver up the city and everything in it.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Sovereign Lord has sworn by his own name, and this is what he, the Lord God of Heaven’s Armies, says: “I despise the arrogance of Israel, and I hate their fortresses. I will give this city and everything in it to their enemies.”"
      }
    },
