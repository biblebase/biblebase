    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “摩押人三番四次地犯罪， 我必不收回对他们的惩罚， 因为他们把以东王的骸骨焚烧成灰。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“耶和華這樣說： ‘摩押三番四次犯罪， 我必不收回懲罰他的命令； 因為他把以東王的骸骨焚燒成灰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華如此說： 摩押三番四次地犯罪， 我必不免去她的刑罰； 因為她將以東王的骸骨焚燒成灰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord, “For three transgressions of Moab and for four I will not revoke its punishment, Because he burned the bones of the king of Edom to lime."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord says: “For three sins of Moab, even for four, I will not relent. Because he burned to ashes the bones of Edom’s king,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord says: “The people of Moab have sinned again and again, and I will not let them go unpunished! They desecrated the bones of Edom’s king, burning them to ashes."
      }
    },
