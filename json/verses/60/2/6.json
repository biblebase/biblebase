    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为圣经上说： “看啊，我在锡安放了一块蒙拣选的宝贵房角石， 信靠祂的人必不会蒙羞。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為經上記著： “看哪，我在錫安放了一塊石頭， 就是所揀選所珍貴的房角石， 信靠他的人，必不致失望。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為經上說： 看哪，我把所揀選、 所寶貴的房角石安放在錫安； 信靠他的人必不至於羞愧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For this is contained in Scripture: “Behold, I lay in Zion a choice stone, a precious corner  stone, And he who believes in Him will not be disappointed.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For in Scripture it says: “See, I lay a stone in Zion, a chosen and precious cornerstone, and the one who trusts in him will never be put to shame.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As the Scriptures say, “I am placing a cornerstone in Jerusalem, chosen for great honor, and anyone who trusts in him will never be disgraced.”"
      }
    },
