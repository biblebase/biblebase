    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门王满足了示巴女王的一切要求，回赠她的礼物超过了她带来的。之后，女王和随从就回示巴去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門王按著示巴女王帶來給他的，回送她禮物；此外，還把女王所願所求的，都送給她，於是女王和她的臣僕都返回本國去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門王按示巴女王所帶來的，還她禮物，另外照她一切所要所求的，都送給她。於是女王和她臣僕轉回本國去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "King Solomon gave to the queen of Sheba all her desire which she requested besides a return for what she had brought to the king. Then she turned and went to her own land with her servants."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "King Solomon gave the queen of Sheba all she desired and asked for; he gave her more than she had brought to him. Then she left and returned with her retinue to her own country."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King Solomon gave the queen of Sheba whatever she asked for—gifts of greater value than the gifts she had given him. Then she and all her attendants returned to their own land."
      }
    },
