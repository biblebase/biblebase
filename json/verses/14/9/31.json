    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他与祖先同眠后，葬在他父亲大卫的城里。他儿子罗波安继位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門和他的列祖同睡，埋葬在他父親大衛的城裡；他的兒子羅波安接續他作王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門與他列祖同睡，葬在他父大衛城裏。他兒子羅波安接續他作王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Solomon slept with his fathers and was buried in the city of his father David; and his son Rehoboam reigned in his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he rested with his ancestors and was buried in the city of David his father. And Rehoboam his son succeeded him as king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When he died, he was buried in the City of David, named for his father. Then his son Rehoboam became the next king."
      }
