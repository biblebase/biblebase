      "leili": {
        "content": [
          {
            "verse": "2ch.9.10",
            "paragraphs": [
              "九10\"檀香木\"。參看列王紀上十章10、11節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希兰的仆人和所罗门的仆人从俄斐运来黄金、檀香木和宝石。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希蘭的僕人和所羅門的僕人從俄斐把黃金運來，也把檀香木和寶石運了來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希蘭的僕人和所羅門的僕人從俄斐運了金子來，也運了檀香木和寶石來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The servants of Huram and the servants of Solomon who brought gold from Ophir, also brought algum trees and precious stones."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "(The servants of Hiram and the servants of Solomon brought gold from Ophir; they also brought algumwood and precious stones."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "(In addition, the crews of Hiram and Solomon brought gold from Ophir, and they also brought red sandalwood and precious jewels."
      }
    },
