      "leili": {
        "content": [
          {
            "verse": "2ch.9.4",
            "paragraphs": [
              "九4\"他上耶和華殿的臺階\"。參看列王紀上十章5節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "席上的美味、入座的群臣、侍立一旁的仆人、他们的服装、酒政、以及他在耶和华殿里献的燔祭，感到万分惊奇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他席上的食物，群臣的座位，僕人的侍候和他們的服飾，酒政和酒政的服飾，以及所羅門在耶和華殿裡所獻的燔祭，就十分驚訝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "席上的珍饈美味、羣臣分列而坐、僕人兩旁侍立，以及他們的衣服裝飾、酒政，和酒政的衣服裝飾，又見他上耶和華殿的臺階，就詫異得神不守舍，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "the food at his table, the seating of his servants, the attendance of his ministers and their attire, his cupbearers and their attire, and his stairway by which he went up to the house of the Lord, she was breathless."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the food on his table, the seating of his officials, the attending servants in their robes, the cupbearers in their robes and the burnt offerings he made at the temple of the Lord, she was overwhelmed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "she was overwhelmed. She was also amazed at the food on his tables, the organization of his officials and their splendid clothing, the cup-bearers and their robes, and the burnt offerings Solomon made at the Temple of the Lord."
      }
    },
