    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若不是亲眼目睹，我不会相信。事实上，我听到的还不到一半！你的智慧远超过我听到的传闻。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我先前不信他們的話，等到我來了，親眼看見了，才知道你智慧的偉大，人告訴我的還不到一半。你實在是見面勝似聞名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我先不信那些話，及至我來親眼見了，才知道你的大智慧；人所告訴我的，還不到一半；你的實跡越過我所聽見的名聲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nevertheless I did not believe their reports until I came and my eyes had seen it. And behold, the half of the greatness of your wisdom was not told me. You surpass the report that I heard."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I did not believe what they said until I came and saw with my own eyes. Indeed, not even half the greatness of your wisdom was told me; you have far exceeded the report I heard."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I didn’t believe what was said until I arrived here and saw it with my own eyes. In fact, I had not heard the half of your great wisdom! It is far beyond what I was told."
      }
    },
