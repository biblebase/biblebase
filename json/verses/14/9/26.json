        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门统管从幼发拉底河到非利士地区，远至埃及边境的诸王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門統治列王，從幼發拉底河到非利士地，直到埃及的邊界。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門統管諸王，從大河到非利士地，直到埃及的邊界。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He was the ruler over all the kings from the Euphrates River even to the land of the Philistines, and as far as the border of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He ruled over all the kings from the Euphrates River to the land of the Philistines, as far as the border of Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He ruled over all the kings from the Euphrates River in the north to the land of the Philistines and the border of Egypt in the south."
      }
    },
