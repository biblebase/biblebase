    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门有四千个安置战车和马匹的棚，有一万二千名骑兵，驻扎在屯车城和他所在的耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門有四千馬廊的車馬，還有馬兵一萬二千，他把這些人馬安置在囤車城和耶路撒冷，就是與王在一起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門有套車的馬四千棚，有馬兵一萬二千，安置在屯車的城邑和耶路撒冷，就是王那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Solomon had 4,000 stalls for horses and chariots and 12,000 horsemen, and he stationed them in the chariot cities and with the king in Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Solomon had four thousand stalls for horses and chariots, and twelve thousand horses, which he kept in the chariot cities and also with him in Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon had 4,000 stalls for his horses and chariots, and he had 12,000 horses. He stationed some of them in the chariot cities, and some near him in Jerusalem."
      }
    },
