    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚她利雅听到人们奔走称颂王的声音，便走进耶和华的殿，众人都聚集在那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞他利雅聽見人民奔跑和讚美王的聲音，就進了耶和華的殿，到眾民那裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞她利雅聽見民奔走讚美王的聲音，就到民那裏，進耶和華的殿，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Athaliah heard the noise of the people running and praising the king, she came into the house of the Lord to the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Athaliah heard the noise of the people running and cheering the king, she went to them at the temple of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Athaliah heard the noise of the people running and the shouts of praise to the king, she hurried to the Lord’s Temple to see what was happening."
      }
    },
