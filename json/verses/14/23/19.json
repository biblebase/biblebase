    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶何耶大还派门卫把守耶和华殿的各门，禁止一切不洁净的人入内。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又設立守門的看守耶和華殿的各門，無論為了甚麼事，不潔淨的人，都不能進去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "且設立守門的把守耶和華殿的各門，無論為何事，不潔淨的人都不准進去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He stationed the gatekeepers of the house of the Lord, so that no one would enter who was in any way unclean."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He also stationed gatekeepers at the gates of the Lord’s temple so that no one who was in any way unclean might enter."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He also stationed gatekeepers at the gates of the Lord’s Temple to keep out those who for any reason were ceremonially unclean."
      }
    },
