    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他率领百夫长、贵族、首领和全体民众护送王从耶和华的殿里下来，经上门进入王宫，拥立王登上王位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他率領百夫長、貴族、民間的官長和國民，又把王從耶和華的殿領下來，從“上門”進入王宮，擁立王坐在王位上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又率領百夫長和貴冑，與民間的官長，並國中的眾民，請王從耶和華殿下來，由上門進入王宮，立王坐在國位上。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he took the captains, the nobles, the governors of the people, and all the people of the land, and they brought the king down from the house of the Lord, marching through the upper gate to the king’s house. And they set the king on the royal throne."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He took the captains of hundreds, the nobles, the rulers of the people and all the people of the land, and brought the king down from the house of the Lord, and came through the upper gate to the king’s house. And they placed the king upon the royal throne."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He took with him the commanders of hundreds, the nobles, the rulers of the people and all the people of the land and brought the king down from the temple of the Lord. They went into the palace through the Upper Gate and seated the king on the royal throne."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then he took the captains of hundreds, the nobles, the governors of the people, and all the people of the land, and brought the king down from the house of the Lord; and they went through the Upper Gate to the king’s house, and set the king on the throne of the kingdom."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the commanders, nobles, rulers, and all the people of the land escorted the king from the Temple of the Lord. They went through the upper gate and into the palace, and they seated the king on the royal throne."
      }
    },
