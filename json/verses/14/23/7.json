    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "利未人要手持兵器护卫在王周围。凡擅自进殿的，一律处死。无论王去哪里，你们都要紧随左右。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "利未人要在周圍環繞君王，各人手裡拿著武器；擅自進殿的，都必被處死；王出入的時候，你們都要隨著他。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利未人要手中各拿兵器，四圍護衛王；凡擅入殿宇的，必當治死。王出入的時候，你們當跟隨他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Levites will surround the king, each man with his weapons in his hand; and whoever enters the house, let him be killed. Thus be with the king when he comes in and when he goes out.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Levites are to station themselves around the king, each with weapon in hand. Anyone who enters the temple is to be put to death. Stay close to the king wherever he goes.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You Levites, form a bodyguard around the king and keep your weapons in hand. Kill anyone who tries to enter the Temple. Stay with the king wherever he goes.”"
      }
    },
