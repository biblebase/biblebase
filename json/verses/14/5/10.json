    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.5.10",
            "paragraphs": [
              "五10關於約櫃裏面放置的東西，參看列王紀上八章9節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约柜里除了两块石版，没有别的东西。石版是以色列人离开埃及后，耶和华在何烈山跟他们立约时，摩西放进去的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約櫃裡只有兩塊石版，就是以色列人出埃及以後，耶和華和他們立約的時候，摩西在何烈山放進去的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約櫃裏惟有兩塊石版，就是以色列人出埃及後，耶和華與他們立約的時候，摩西在何烈山所放的。除此以外，並無別物。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "There was nothing in the ark except the two tablets that Moses put there at Horeb, where the Lord made a covenant with the people of Israel, when they came out of Egypt."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There was nothing in the ark except the two tablets which Moses put there at Horeb, where the Lord made a covenant with the sons of Israel, when they came out of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There was nothing in the ark except the two tablets that Moses had placed in it at Horeb, where the Lord made a covenant with the Israelites after they came out of Egypt."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Nothing was in the ark except the two tablets which Moses put there at Horeb, when the Lord made a  covenant with the children of Israel, when they had come out of Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Nothing was in the Ark except the two stone tablets that Moses had placed in it at Mount Sinai, where the Lord made a covenant with the people of Israel when they left Egypt."
      }
    },
