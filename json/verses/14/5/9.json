    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.5.8-9",
            "paragraphs": [
              "五8,9關於約櫃的放置，參看列王紀上八章7、8節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "横杠非常长，在至圣所前面的圣所中也看得见横杠的末端，但在圣所外面看不见。横杠至今仍在那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那兩根槓很長，在內殿前可以看見約櫃那裡的槓頭，在殿外卻看不見；直到今日，那兩根槓還在那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這槓甚長，槓頭在內殿前可以看見，在殿外卻不能看見，直到如今還在那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The poles were so long that the ends of the poles of the ark could be seen in front of the inner sanctuary, but they could not be seen outside; and they are there to this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These poles were so long that their ends, extending from the ark, could be seen from in front of the inner sanctuary, but not from outside the Holy Place; and they are still there today."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These poles were so long that their ends could be seen from the Holy Place, which is in front of the Most Holy Place, but not from the outside. They are still there to this day."
      }
    },
