    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.5.3",
            "paragraphs": [
              "五3\"七月節\"。住棚節。參看利未記二十三章34至43節的腳註。\"七月\"（九、十月間）。參看列王紀上八章2節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在七月住棚节的时候，以色列人都聚集到所罗门王那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是，以色列眾人在七月的節期來到時，都聚集到王那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是以色列眾人在七月節前都聚集到王那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the men of Israel assembled themselves to the king at the feast, that is in the seventh month."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And all the Israelites came together to the king at the time of the festival in the seventh month."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So all the men of Israel assembled before the king at the annual Festival of Shelters, which is held in early autumn."
      }
