      "leili": {
        "content": [
          {
            "verse": "2ch.36.6-7",
            "paragraphs": [
              "三六6,7這是尼布甲尼撒第一次對耶路撒冷的進侵，在主前605年。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴比伦王尼布甲尼撒起兵攻打他，用铜链把他锁着带到巴比伦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴比倫王尼布甲尼撒上來攻打他，用銅鍊鎖住他，把他帶到巴比倫去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴比倫王尼布甲尼撒上來攻擊他，用銅鍊鎖着他，要將他帶到巴比倫去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nebuchadnezzar king of Babylon came up against him and bound him with bronze chains to take him to Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nebuchadnezzar king of Babylon attacked him and bound him with bronze shackles to take him to Babylon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then King Nebuchadnezzar of Babylon came to Jerusalem and captured it, and he bound Jehoiakim in bronze chains and led him away to Babylon."
      }
