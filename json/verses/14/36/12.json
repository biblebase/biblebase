    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他做他的上帝耶和华视为恶的事，耶利米先知向他传讲耶和华的话，他仍不肯在耶利米面前谦卑下来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他行耶和華他的　神看為惡的事；耶利米先知奉耶和華的命令警戒他，他仍不在耶利米面前謙卑下來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行耶和華－他神眼中看為惡的事。先知耶利米以耶和華的話勸他，他仍不在耶利米面前自卑。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He did what was evil in the sight of the Lord his God. He did not humble himself before Jeremiah the prophet, who spoke from the mouth of the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He did evil in the sight of the Lord his God; he did not humble himself before Jeremiah the prophet who spoke for the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He did evil in the eyes of the Lord his God and did not humble himself before Jeremiah the prophet, who spoke the word of the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He did evil in the sight of the Lord his God, and  did not humble himself before Jeremiah the prophet, who  spoke from the mouth of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Zedekiah did what was evil in the sight of the Lord his God, and he refused to humble himself when the prophet Jeremiah spoke to him directly from the Lord."
      }
    },
