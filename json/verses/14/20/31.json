    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约沙法三十五岁登基做犹大王，在耶路撒冷执政二十五年。他母亲叫阿苏巴，是示利希的女儿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約沙法登基統治猶大的時候，是三十五歲；他在耶路撒冷作王共二十五年。他的母親名叫阿蘇巴，是示利希的女兒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約沙法作猶大王，登基的時候年三十五歲，在耶路撒冷作王二十五年。他母親名叫阿蘇巴，乃示利希的女兒。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Thus Jehoshaphat reigned over Judah. He was thirty-five years old when he began to reign, and he reigned twenty-five years in Jerusalem. His mother’s name was Azubah the daughter of Shilhi."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Jehoshaphat reigned over Judah. He was thirty-five years old when he became king, and he reigned in Jerusalem twenty-five years. And his mother’s name was Azubah the daughter of Shilhi."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Jehoshaphat reigned over Judah. He was thirty-five years old when he became king of Judah, and he reigned in Jerusalem twenty-five years. His mother’s name was Azubah daughter of Shilhi."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So Jehoshaphat was king over Judah. He  was thirty-five years old when he became king, and he reigned twenty-five years in Jerusalem. His mother’s name was Azubah the daughter of Shilhi."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Jehoshaphat ruled over the land of Judah. He was thirty-five years old when he became king, and he reigned in Jerusalem twenty-five years. His mother was Azubah, the daughter of Shilhi."
      }
    },
