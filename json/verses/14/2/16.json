    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们会在黎巴嫩砍伐你需要的木材，扎成木筏，经海道运到约帕，你可以从那里将木材转运到耶路撒冷。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們必照著你所需要的，從黎巴嫩砍下樹木，紮成木筏，從海上運到約帕，你可以從那裡把它們運上耶路撒冷。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們必照你所需用的，從黎巴嫩砍伐樹木，紮成筏子，浮海運到約帕；你可以從那裏運到耶路撒冷。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And we will cut whatever timber you need from Lebanon and bring it to you in rafts by sea to Joppa, so that you may take it up to Jerusalem.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We will cut whatever timber you need from Lebanon and bring it to you on rafts by sea to Joppa, so that you may carry it up to Jerusalem.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and we will cut all the logs from Lebanon that you need and will float them as rafts by sea down to Joppa. You can then take them up to Jerusalem.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And we will cut wood from Lebanon, as much as you need; we will bring it to you in rafts by sea to Joppa, and you will carry it up to Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We will cut whatever timber you need from the Lebanon mountains and will float the logs in rafts down the coast of the Mediterranean Sea to Joppa. From there you can transport the logs up to Jerusalem.”"
      }
    },
