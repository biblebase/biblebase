    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他指派其中的七万人做搬运工，八万人在山上采凿石头，三千六百人做监工。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他選派七萬人作挑夫，八萬人在山上鑿石頭，三千六百人作監工，督促人民工作。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使七萬人扛擡材料，八萬人在山上鑿石頭，三千六百人督理工作。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He appointed 70,000 of them to carry loads and 80,000 to quarry stones in the mountains and 3,600 supervisors to make the people work."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He assigned 70,000 of them to be carriers and 80,000 to be stonecutters in the hills, with 3,600 foremen over them to keep the people working."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He assigned 70,000 of them as common laborers, 80,000 as quarry workers in the hill country, and 3,600 as foremen."
      }
    },
