    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.2.6",
            "paragraphs": [
              "二6所羅門心裏謙卑，深知道無所不在的神不會被囿限在一所聖殿裏，而聖殿只是一個供神子民敬拜祂的地方。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "诸天尚且容纳不下祂，谁能为祂建造殿宇呢？我是谁，怎能为祂建造殿宇？我不过是建个向祂烧香的地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但誰能為他建造殿宇呢？天和天上的天也容不下他；我是誰，怎能為他建造殿宇？我不過在他面前燒香罷了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天和天上的天，尚且不足他居住的，誰能為他建造殿宇呢？我是誰？能為他建造殿宇嗎？不過在他面前燒香而已。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But who is able to build him a house, since heaven, even highest heaven, cannot contain him? Who am I to build a house for him, except as a place to make offerings before him?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But who is able to build a house for Him, for the heavens and the highest heavens cannot contain Him? So who am I, that I should build a house for Him, except to burn incense before Him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But who is able to build a temple for him, since the heavens, even the highest heavens, cannot contain him? Who then am I to build a temple for him, except as a place to burn sacrifices before him?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But who is able to build Him a temple, since heaven and the heaven of heavens cannot contain Him? Who am I then, that I should build Him a temple, except to burn sacrifice before Him?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But who can really build him a worthy home? Not even the highest heavens can contain him! So who am I to consider building a Temple for him, except as a place to burn sacrifices to him?"
      }
    },
