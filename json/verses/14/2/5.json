        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要建造的殿宇极其宏伟，因为我们的上帝超越一切神明。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要建造的殿宇必須宏偉，因為我們的　神偉大，超過眾神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我所要建造的殿宇甚大；因為我們的神至大，超乎諸神。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The house that I am to build will be great, for our God is greater than all gods."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The house which I am about to build will be great, for greater is our God than all the gods."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The temple I am going to build will be great, because our God is greater than all other gods."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the temple which I build will  be great, for our God is greater than all gods."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This must be a magnificent Temple because our God is greater than all other gods."
      }
