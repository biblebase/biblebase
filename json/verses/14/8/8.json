    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人没有灭绝他们。所罗门让他们服劳役，至今如此。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的子孫仍然留在那地，以色列人沒有消滅他們。於是，所羅門徵召他們作苦工，直到今日。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是以色列人未曾滅絕的，所羅門挑取他們的後裔作服苦的奴僕，直到今日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "namely, from their descendants who were left after them in the land whom the sons of Israel had not destroyed, them Solomon raised as forced laborers to this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Solomon conscripted the descendants of all these people remaining in the land—whom the Israelites had not destroyed—to serve as slave labor, as it is to this day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These were descendants of the nations whom the people of Israel had not destroyed. So Solomon conscripted them for his labor force, and they serve as forced laborers to this day."
      }
