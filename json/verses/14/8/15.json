    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们没有违背大卫王就库房等工作对祭司和利未人的吩咐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王對祭司和利未人所吩咐的，無論是甚麼事務，或是有關庫房的事，他們都沒有違抗。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王所吩咐眾祭司和利未人的，無論是管府庫或辦別的事，他們都不違背。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they did not depart from the commandment of the king to the priests and Levites in any manner or concerning the storehouses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They did not deviate from the king’s commands to the priests or to the Levites in any matter, including that of the treasuries."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon did not deviate in any way from David’s commands concerning the priests and Levites and the treasuries."
      }
    },
