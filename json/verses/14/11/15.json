    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.11.15",
            "paragraphs": [
              "一一15\"為鬼魔\"。山羊模樣的偶像。參看利未記十七章1至7節的腳註。\"牛犢\"。參看列王紀上十二章28節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶罗波安却为丘坛、公山羊像和他造的牛犊像设立他自己的祭司。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶羅波安為邱壇、公山羊神，以及他鑄造的牛犢，自己設立了祭司。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶羅波安為邱壇、為鬼魔、為自己所鑄造的牛犢設立祭司。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He set up priests of his own for the high places, for the satyrs and for the calves which he had made."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "when he appointed his own priests for the high places and for the goat and calf idols he had made."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jeroboam appointed his own priests to serve at the pagan shrines, where they worshiped the goat and calf idols he had made."
      }
    },
