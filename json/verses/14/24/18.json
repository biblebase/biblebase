    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们离弃他们祖先的上帝耶和华的殿，去供奉亚舍拉神像及其他偶像。因他们所犯的罪，上帝的烈怒临到犹大和耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們離棄了耶和華他們列祖的　神的殿，去事奉亞舍拉和偶像。因為他們的罪，　神的忿怒臨到猶大和耶路撒冷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們離棄耶和華－他們列祖神的殿，去事奉亞舍拉和偶像；因他們這罪，就有忿怒臨到猶大和耶路撒冷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They abandoned the house of the Lord, the God of their fathers, and served the Asherim and the idols; so wrath came upon Judah and Jerusalem for this their guilt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They abandoned the temple of the Lord, the God of their ancestors, and worshiped Asherah poles and idols. Because of their guilt, God’s anger came on Judah and Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They decided to abandon the Temple of the Lord, the God of their ancestors, and they worshiped Asherah poles and idols instead! Because of this sin, divine anger fell on Judah and Jerusalem."
      }
    },
