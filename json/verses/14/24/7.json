    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为恶妇亚她利雅的爪牙曾闯入耶和华上帝的殿，拿殿里的圣物去供奉巴力。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "原來那惡婦亞他利雅的眾子曾經拆毀　神的殿，又把耶和華殿中一切分別為聖的物品用來供奉巴力。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為那惡婦亞她利雅的眾子曾拆毀神的殿，又用耶和華殿中分別為聖的物供奉巴力。）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the sons of the wicked Athaliah had broken into the house of God and even used the holy things of the house of the Lord for the Baals."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the sons of that wicked woman Athaliah had broken into the temple of God and had used even its sacred objects for the Baals."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Over the years the followers of wicked Athaliah had broken into the Temple of God, and they had used all the dedicated things from the Temple of the Lord to worship the images of Baal."
      }
    },
