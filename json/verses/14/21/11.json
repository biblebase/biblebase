    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.21.11",
            "paragraphs": [
              "二一11\"行邪淫\"。不是身體上而是屬靈上的不忠（雅四4）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约兰在犹大的山上修建丘坛，引诱耶路撒冷的居民和犹大人与假神苟合。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又在猶大眾山上建築邱壇，使耶路撒冷的居民行邪淫，引誘猶大人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又在猶大諸山建築邱壇，使耶路撒冷的居民行邪淫，誘惑猶大人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, he made high places in the mountains of Judah, and caused the inhabitants of Jerusalem to play the harlot and led Judah astray."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He had also built high places on the hills of Judah and had caused the people of Jerusalem to prostitute themselves and had led Judah astray."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He had built pagan shrines in the hill country of Judah and had led the people of Jerusalem and Judah to give themselves to pagan gods and to go astray."
      }
    },
