    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一切都准备好后，依照王的吩咐，祭司站在自己的地方，利未人按照班次侍立。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，事奉的職務安排好了以後，他們照著王的吩咐，祭司站在自己的崗位，利未人也按著自己的班次侍立。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，供獻的事齊備了。祭司站在自己的地方，利未人按着班次站立，都是照王所吩咐的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When the service had been prepared for, the priests stood in their place, and the Levites in their divisions according to the king’s command."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the service was prepared, and the priests stood at their stations and the Levites by their divisions according to the king’s command."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The service was arranged and the priests stood in their places with the Levites in their divisions as the king had ordered."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So the service was prepared, and the priests stood in their places, and the Levites in their divisions, according to the king’s command."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When everything was ready for the Passover celebration, the priests and the Levites took their places, organized by their divisions, as the king had commanded."
      }
    },
