    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.28.9-15",
            "paragraphs": [
              "二八9～15列王紀下沒有這部分的記載。這裏敘述一位只在這裏提及的先知俄德怎樣反對北國拘留他們從南國帶來的戰犯，原因是他們是兄弟。結果戰犯得以穿上衣服，獲得食物的供應，然後被送回耶利哥去。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，士兵们便把俘虏和战利品交给众首领和民众。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是，軍隊把俘虜和戰利品都留在眾領袖和全體會眾的面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是帶兵器的人將擄來的人口和掠來的財物都留在眾首領和會眾的面前。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So the armed men left the captives and the spoil before the princes and all the assembly."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the armed men left the captives and the spoil before the officers and all the assembly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the soldiers gave up the prisoners and plunder in the presence of the officials and all the assembly."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So the armed men left the captives and the spoil before the leaders and all the assembly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the warriors released the prisoners and handed over the plunder in the sight of the leaders and all the people."
      }
