    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚哈斯与祖先同眠后，葬在耶路撒冷城中，但没有葬在以色列的王陵。他儿子希西迦继位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞哈斯和他的列祖同睡，埋葬在耶路撒冷城裡，但沒有葬在以色列諸王的陵墓裡。他的兒子希西家接續他作王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈斯與他列祖同睡，葬在耶路撒冷城裏，沒有送入以色列諸王的墳墓中。他兒子希西家接續他作王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Ahaz slept with his fathers, and they buried him in the city, in Jerusalem, for they did not bring him into the tombs of the kings of Israel; and Hezekiah his son reigned in his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ahaz rested with his ancestors and was buried in the city of Jerusalem, but he was not placed in the tombs of the kings of Israel. And Hezekiah his son succeeded him as king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Ahaz died, he was buried in Jerusalem but not in the royal cemetery of the kings of Judah. Then his son Hezekiah became the next king."
      }
    },
