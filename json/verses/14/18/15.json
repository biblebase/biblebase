      "leili": {
        "content": [
          {
            "verse": "2ch.18.4-27",
            "paragraphs": [
              "一八4～27有關米該雅真正的預言，參看列王紀上二十二章6節至二十二章24、25節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王却说：“我要嘱咐你多少次，你才肯奉耶和华的名对我说实话呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王對他說：“我要囑咐你多少次，你才會奉耶和華的名對我只說真話呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王對他說：「我當囑咐你幾次，你才奉耶和華的名向我說實話呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king said to him, “How many times must I adjure you to speak to me nothing but the truth in the name of the Lord?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king said to him, “How many times must I make you swear to tell me nothing but the truth in the name of the Lord?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the king replied sharply, “How many times must I demand that you speak only the truth to me when you speak for the Lord?”"
      }
    },
