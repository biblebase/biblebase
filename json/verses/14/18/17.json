    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.18.4-27",
            "paragraphs": [
              "一八4～27有關米該雅真正的預言，參看列王紀上二十二章6節至二十二章24、25節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列王对约沙法说：“我不是告诉过你吗？他给我的预言都是有凶无吉。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列王對約沙法說：“我不是早告訴你，他對我說的預言，總沒有吉話，只說凶話嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列王對約沙法說：「我豈沒有告訴你，這人指着我所說的預言，不說吉語，單說凶言嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king of Israel said to Jehoshaphat, “Did I not tell you that he would not prophesy good concerning me, but evil?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king of Israel said to Jehoshaphat, “Didn’t I tell you that he never prophesies anything good about me, but only bad?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Didn’t I tell you?” the king of Israel exclaimed to Jehoshaphat. “He never prophesies anything but trouble for me.”"
      }
    },
