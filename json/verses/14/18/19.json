    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说，‘谁愿意去引诱以色列王亚哈到基列的拉末去送死呢？’众天军议论纷纷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：‘誰去引誘以色列王亞哈上去，好使他倒斃在基列的拉末呢？’有人這樣說，也有人那樣說。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：『誰去引誘以色列王亞哈上基列的拉末去陣亡呢？』這個就這樣說，那個就那樣說。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord said, ‘Who will entice Ahab king of Israel to go up and fall at Ramoth-gilead?’ And one said this while another said that."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Lord said, ‘Who will entice Ahab king of Israel into attacking Ramoth Gilead and going to his death there?’ “One suggested this, and another that."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the Lord said, ‘Who can entice King Ahab of Israel to go into battle against Ramoth-gilead so he can be killed?’ “There were many suggestions,"
      }
