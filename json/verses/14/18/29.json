    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列王对约沙法说：“我要改装上阵，你就穿王袍吧。”以色列王改装后，他们就上阵去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列王對約沙法說：“我要改裝上陣，你可以仍穿朝服。”於是以色列王改了裝，他們就上陣去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列王對約沙法說：「我要改裝上陣，你可以仍穿王服。」於是以色列王改裝，他們就上陣去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king of Israel said to Jehoshaphat, “I will disguise myself and go into battle, but you put on your robes.” So the king of Israel disguised himself, and they went into battle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king of Israel said to Jehoshaphat, “I will enter the battle in disguise, but you wear your royal robes.” So the king of Israel disguised himself and went into battle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king of Israel said to Jehoshaphat, “As we go into battle, I will disguise myself so no one will recognize me, but you wear your royal robes.” So the king of Israel disguised himself, and they went into battle."
      }
