    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华应允他的祷告，垂听他的恳求，使他返回耶路撒冷继续做王。玛拿西这才明白耶和华是上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他向耶和華禱告，耶和華應允他的懇求，垂聽他的祈求，使他歸回耶路撒冷，恢復他的王位。瑪拿西這才知道只有耶和華是　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他祈禱耶和華，耶和華就允准他的祈求，垂聽他的禱告，使他歸回耶路撒冷，仍坐國位。瑪拿西這才知道惟獨耶和華是神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he prayed to Him, He was moved by his entreaty and heard his supplication, and brought him again to Jerusalem to his kingdom. Then Manasseh knew that the Lord  was God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And when he prayed to him, the Lord was moved by his entreaty and listened to his plea; so he brought him back to Jerusalem and to his kingdom. Then Manasseh knew that the Lord is God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when he prayed, the Lord listened to him and was moved by his request. So the Lord brought Manasseh back to Jerusalem and to his kingdom. Then Manasseh finally realized that the Lord alone is God!"
      }
    },
