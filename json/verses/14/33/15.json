    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛拿西将偶像和外族人的神像从耶和华的殿中除去，又把他在圣殿山和耶路撒冷筑造的一切祭坛全部拆掉，扔在城外。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又從耶和華的殿中除掉外族人的神和偶像，又把他在耶和華殿的山和在耶路撒冷所築的一切壇，都拋出城外。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並除掉外邦人的神像與耶和華殿中的偶像，又將他在耶和華殿的山上和耶路撒冷所築的各壇都拆毀拋在城外；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also removed the foreign gods and the idol from the house of the Lord, as well as all the altars which he had built on the mountain of the house of the Lord and in Jerusalem, and he threw them outside the city."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He got rid of the foreign gods and removed the image from the temple of the Lord, as well as all the altars he had built on the temple hill and in Jerusalem; and he threw them out of the city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Manasseh also removed the foreign gods and the idol from the Lord’s Temple. He tore down all the altars he had built on the hill where the Temple stood and all the altars that were in Jerusalem, and he dumped them outside the city."
      }
    },
