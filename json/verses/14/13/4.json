    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚比雅站在以法莲山区的西玛莲山上说：“耶罗波安和全体以色列人啊，你们听我说，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞比雅起來站在以法蓮山地中的洗瑪臉山上，說：“耶羅波安和以色列眾民哪，你們要聽我的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞比雅站在以法蓮山地中的洗瑪臉山上，說：「耶羅波安和以色列眾人哪，要聽我說！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Abijah stood on Mount Zemaraim, which is in the hill country of Ephraim, and said, “Listen to me, Jeroboam and all Israel:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Abijah stood on Mount Zemaraim, in the hill country of Ephraim, and said, “Jeroboam and all Israel, listen to me!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the army of Judah arrived in the hill country of Ephraim, Abijah stood on Mount Zemaraim and shouted to Jeroboam and all Israel: “Listen to me!"
      }
