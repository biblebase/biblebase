    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "信差将这通告逐城传遍以法莲和玛拿西，直到西布伦，可是那些地方的人却嘲笑、戏弄他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "信差從這城到那城，走遍了以法蓮和瑪拿西各地，直到西布倫；那裡的人卻戲弄他們，譏笑他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "驛卒就由這城跑到那城，傳遍了以法蓮、瑪拿西，直到西布倫。那裏的人卻戲笑他們，譏誚他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the couriers passed from city to city through the country of Ephraim and Manasseh, and as far as Zebulun, but they laughed them to scorn and mocked them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The couriers went from town to town in Ephraim and Manasseh, as far as Zebulun, but people scorned and ridiculed them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The runners went from town to town throughout Ephraim and Manasseh and as far as the territory of Zebulun. But most of the people just laughed at the runners and made fun of them."
      }
    },
