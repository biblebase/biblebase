    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大人都为所起的誓而欢喜快乐，因他们诚心起誓、诚意寻求耶和华，耶和华就让他们寻见，并赐他们四境平安。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大眾人都為了所起的誓快樂，因為他們一心起誓，盡意尋求耶和華，耶和華就給他們尋見，並且使他們四境安寧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大眾人為所起的誓歡喜；因他們是盡心起誓，盡意尋求耶和華，耶和華就被他們尋見，且賜他們四境平安。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All Judah rejoiced concerning the oath, for they had sworn with their whole heart and had sought Him earnestly, and He let them find Him. So the Lord gave them rest on every side."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All Judah rejoiced about the oath because they had sworn it wholeheartedly. They sought God eagerly, and he was found by them. So the Lord gave them rest on every side."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All in Judah were happy about this covenant, for they had entered into it with all their heart. They earnestly sought after God, and they found him. And the Lord gave them rest from their enemies on every side."
      }
    },
