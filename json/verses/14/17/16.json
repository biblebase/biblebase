    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其次是自愿事奉耶和华的细基利的儿子亚玛斯雅，率领精兵二十万。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在約哈難底下，是細基利的兒子亞瑪斯雅，他是個甘願獻身給耶和華的人，率領英勇的戰士二十萬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其次是，細基利的兒子亞瑪斯雅（他為耶和華犧牲自己）率領大能的勇士－二十萬。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and next to him Amasiah the son of Zichri, who volunteered for the Lord, and with him 200,000 valiant warriors;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "next, Amasiah son of Zikri, who volunteered himself for the service of the Lord, with 200,000."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Next was Amasiah son of Zicri, who volunteered for the Lord’s service, with 200,000 troops under his command."
      }
    },
