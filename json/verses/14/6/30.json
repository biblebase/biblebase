    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你从天上的居所垂听，赦免他们。唯独你能洞察人心，你必按各人的行为施行赏罚，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你從天上你的居所垂聽和赦免他們，你是知道人心的，你必照著各人所行的報應各人；因為只有你知道世人的心。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你從天上你的居所垂聽赦免。你是知道人心的，要照各人所行的待他們（惟有你知道世人的心），"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then hear from heaven Your dwelling place, and forgive, and render to each according to all his ways, whose heart You know for You alone know the hearts of the sons of men,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "then hear from heaven, your dwelling place. Forgive, and deal with everyone according to all they do, since you know their hearts (for you alone know the human heart),"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "then hear from heaven where you live, and forgive. Give your people what their actions deserve, for you alone know each human heart."
      }
    },
