    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你在天上垂听，赦免你仆人——你以色列子民的罪，教导他们走正道，降下甘霖滋润你赐给你子民作产业的土地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你在天上垂聽，赦免你的僕人和你的子民以色列的罪，把他們當行的正道指教他們，降雨在你的地上，就是你賜給你的子民作產業的地上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你在天上垂聽，赦免你僕人和你民以色列的罪，將當行的善道指教他們，且降雨在你的地，就是你賜給你民為業之地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "then hear in heaven and forgive the sin of Your servants and Your people Israel, indeed, teach them the good way in which they should walk. And send rain on Your land which You have given to Your people for an inheritance."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "then hear from heaven and forgive the sin of your servants, your people Israel. Teach them the right way to live, and send rain on the land you gave your people for an inheritance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "then hear from heaven and forgive the sins of your servants, your people Israel. Teach them to follow the right path, and send rain on your land that you have given to your people as their special possession."
      }
    },
