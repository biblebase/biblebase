    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿你昼夜看顾这殿——你应许立你名的地方！你仆人向着这地方祷告的时候，求你垂听。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你張開眼睛，晝夜看顧這殿，就是看顧你所說要立你名的地方；求你垂聽你僕人向著這地方所發的禱告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願你晝夜看顧這殿，就是你應許立為你名的居所；求你垂聽僕人向此處禱告的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that Your eye may be open toward this house day and night, toward the place of which You have said that You would put Your name there, to listen to the prayer which Your servant shall pray toward this place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May your eyes be open toward this temple day and night, this place of which you said you would put your Name there. May you hear the prayer your servant prays toward this place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May you watch over this Temple day and night, this place where you have said you would put your name. May you always hear the prayers I make toward this place."
      }
    },
