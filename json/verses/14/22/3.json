      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他重蹈亚哈家的覆辙，因为他的母亲唆使他作恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞哈謝也隨從亞哈家的道路，因為他母親教唆他行惡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈謝也行亞哈家的道；因為他母親給他主謀，使他行惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also walked in the ways of the house of Ahab, for his mother was his counselor to do wickedly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He too followed the ways of the house of Ahab, for his mother encouraged him to act wickedly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ahaziah also followed the evil example of King Ahab’s family, for his mother encouraged him in doing wrong."
      }
