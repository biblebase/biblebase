    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶户讨伐亚哈家的时候，遇见犹大众首领和亚哈谢的众侄儿，他们都是服侍亚哈谢的，耶户就把他们全部杀掉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶戶討伐亞哈家的罪的時候，遇見了猶大的眾領袖和亞哈謝的兄弟的兒子，他們都是侍候亞哈謝的，耶戶把他們都殺了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶戶討亞哈家罪的時候，遇見猶大的眾首領和亞哈謝的眾姪子服事亞哈謝，就把他們都殺了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It came about when Jehu was executing judgment on the house of Ahab, he found the princes of Judah and the sons of Ahaziah’s brothers ministering to Ahaziah, and slew them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While Jehu was executing judgment on the house of Ahab, he found the officials of Judah and the sons of Ahaziah’s relatives, who had been attending Ahaziah, and he killed them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "While Jehu was executing judgment against the family of Ahab, he happened to meet some of Judah’s officials and Ahaziah’s relatives who were traveling with Ahaziah. So Jehu killed them all."
      }
    },
