    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他像亚哈家一样做耶和华视为恶的事，因为他父亲死后，亚哈家便唆使他作恶，使他走向灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他行耶和華看為惡的事，好像亞哈家一樣，因為他的父親死了以後，亞哈家的人就教唆他，好使他滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他行耶和華眼中看為惡的事，像亞哈家一樣；因他父親死後有亞哈家的人給他主謀，以致敗壞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He did evil in the sight of the Lord like the house of Ahab, for they were his counselors after the death of his father, to his destruction."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He did evil in the eyes of the Lord, as the house of Ahab had done, for after his father’s death they became his advisers, to his undoing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He did what was evil in the Lord’s sight, just as Ahab’s family had done. They even became his advisers after the death of his father, and they led him to ruin."
      }
    },
