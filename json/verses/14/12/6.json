    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王与以色列的众首领听了，就谦卑下来说：“耶和华是公义的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是以色列的眾領袖和君王，都謙卑下來，說：“耶和華是公義的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是王和以色列的眾首領都自卑說：「耶和華是公義的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the princes of Israel and the king humbled themselves and said, “The Lord is righteous.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The leaders of Israel and the king humbled themselves and said, “The Lord is just.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the leaders of Israel and the king humbled themselves and said, “The Lord is right in doing this to us!”"
      }
    },
