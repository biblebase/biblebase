    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "住在犹大各城的以色列人和犹大人也送来牛羊的十分之一，以及献给他们的上帝耶和华的圣物，就是十一奉献，把它们堆积成堆。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "住在猶大各城的以色列人和猶大人，也把牛羊的十分之一，和分別為聖歸耶和華他們的　神的聖物的十分之一，都送了來，囤積成堆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住猶大各城的以色列人和猶大人也將牛羊的十分之一，並分別為聖歸耶和華－他們神之物，就是十分取一之物，盡都送來，積成堆壘；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sons of Israel and Judah who lived in the cities of Judah also brought in the tithe of oxen and sheep, and the tithe of sacred gifts which were consecrated to the Lord their God, and placed them in heaps."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The people of Israel and Judah who lived in the towns of Judah also brought a tithe of their herds and flocks and a tithe of the holy things dedicated to the Lord their God, and they piled them in heaps."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people who had moved to Judah from Israel, and the people of Judah themselves, brought in the tithes of their cattle, sheep, and goats and a tithe of the things that had been dedicated to the Lord their God, and they piled them up in great heaps."
      }
    },
