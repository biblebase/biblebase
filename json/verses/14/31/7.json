    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.31.7-9",
            "paragraphs": [
              "三一7～9\"從三月〔五、六月間〕……到七月〔九、十月間〕\"，百姓把什一奉獻源源獻上，以至希西家命人準備好倉房（免得食物堆積在外面），並列好有權分取供物之人的名單。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们从三月开始堆积，到七月才结束。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從三月開始囤積，到七月才完畢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從三月積起，到七月才完。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In the third month they began to make the heaps, and finished them by the seventh month."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They began doing this in the third month and finished in the seventh month."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They began piling them up in late spring, and the heaps continued to grow until early autumn."
      }
    },
