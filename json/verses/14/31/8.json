        "lang": "en",
        "text": "When Hezekiah and the rulers came and saw the heaps, they blessed the Lord and His people Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Hezekiah and his officials came and saw the heaps, they praised the Lord and blessed his people Israel."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And when Hezekiah and the leaders came and saw the heaps, they blessed the Lord and His people Israel."
      },
      "nlt": {
        "version_id": "NLT",
