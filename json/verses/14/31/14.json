    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看守东门的利未人音拿的儿子可利，负责管理民众自愿献给上帝的礼物，以及分发献给耶和华的供物和至圣之物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看守東門的利未人音拿的兒子可利，掌管自願獻給　神的禮物，分發獻與耶和華的供物和至聖之物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "守東門的利未人音拿的兒子可利，掌管樂意獻與神的禮物，發放獻與耶和華的供物和至聖的物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Kore the son of Imnah the Levite, the keeper of the eastern gate, was over the freewill offerings of God, to apportion the contributions for the Lord and the most holy things."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Kore son of Imnah the Levite, keeper of the East Gate, was in charge of the freewill offerings given to God, distributing the contributions made to the Lord and also the consecrated gifts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Kore son of Imnah the Levite, who was the gatekeeper at the East Gate, was put in charge of distributing the voluntary offerings given to God, the gifts, and the things that had been dedicated to the Lord."
      }
    },
