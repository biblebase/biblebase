    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.29.21",
            "paragraphs": [
              "二九21\"贖罪祭\"。參看利未記四章2節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们牵来七头公牛、七只公绵羊、七只羊羔和七只公山羊，作国家、圣所和犹大人的赎罪祭。王吩咐做祭司的亚伦子孙把这些牲畜献在耶和华的坛上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們牽來了七頭公牛、七隻公綿羊、七隻羊羔和七隻公山羊，要為國家、為聖所和為猶大人作贖罪祭；王吩咐亞倫的子孫眾祭司把牠們獻在耶和華的壇上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牽了七隻公牛，七隻公羊，七隻羊羔，七隻公山羊，要為國、為殿、為猶大人作贖罪祭。王吩咐亞倫的子孫眾祭司，獻在耶和華的壇上，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They brought seven bulls, seven rams, seven lambs and seven male goats for a sin offering for the kingdom, the sanctuary, and Judah. And he ordered the priests, the sons of Aaron, to offer them on the altar of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They brought seven bulls, seven rams, seven male lambs and seven male goats as a sin offering for the kingdom, for the sanctuary and for Judah. The king commanded the priests, the descendants of Aaron, to offer these on the altar of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They brought seven bulls, seven rams, and seven male lambs as a burnt offering, together with seven male goats as a sin offering for the kingdom, for the Temple, and for Judah. The king commanded the priests, who were descendants of Aaron, to sacrifice the animals on the altar of the Lord."
      }
    },
