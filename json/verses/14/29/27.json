    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希西迦下令在坛上献燔祭。开始献燔祭时，伴随着号角声和以色列王大卫的乐器声，响起了颂赞耶和华的歌声。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希西家吩咐在祭壇上獻上燔祭。開始獻燔祭的時候，就開始向耶和華歌唱，並且用號筒和以色列王大衛的樂器伴奏。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希西家吩咐在壇上獻燔祭，燔祭一獻，就唱讚美耶和華的歌，用號，並用以色列王大衛的樂器相和。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Hezekiah commanded that the burnt offering be offered on the altar. And when the burnt offering began, the song to the Lord began also, and the trumpets, accompanied by the instruments of David king of Israel."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Hezekiah gave the order to offer the burnt offering on the altar. When the burnt offering began, the song to the Lord also began with the trumpets, accompanied by the instruments of David, king of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hezekiah gave the order to sacrifice the burnt offering on the altar. As the offering began, singing to the Lord began also, accompanied by trumpets and the instruments of David king of Israel."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Hezekiah commanded them to offer the burnt offering on the altar. And when the burnt offering began, the song of the Lord  also began, with the trumpets and with the instruments of David king of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Hezekiah ordered that the burnt offering be placed on the altar. As the burnt offering was presented, songs of praise to the Lord were begun, accompanied by the trumpets and other instruments of David, the former king of Israel."
      }
    },
