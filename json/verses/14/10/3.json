    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人派人去请他，他就和以色列众人去见罗波安，说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們派人去叫耶羅波安回來；他就和全體以色列人來見羅波安，對他說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人打發人去請他，他就和以色列眾人來見羅波安，對他說："
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they sent and called him. And Jeroboam and all Israel came and said to Rehoboam,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they sent and summoned him. When Jeroboam and all Israel came, they spoke to Rehoboam, saying,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So they sent for Jeroboam, and he and all Israel went to Rehoboam and said to him:"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then they sent for him and called him. And Jeroboam and all Israel came and spoke to Rehoboam, saying,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The leaders of Israel summoned him, and Jeroboam and all Israel went to speak with Rehoboam."
      }
    },
