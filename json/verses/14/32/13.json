    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.32.9-19",
            "paragraphs": [
              "三二9～19有關西拿基立仆人的辱駡和希西家的回應，參看列王紀下十八章17至32節和十九章8至13節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道你们不知道我和我的祖先怎样对付列国吗？列国的神明哪个能从我手中救自己的国家呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我和我列祖向各地的民族所行的，你們不知道嗎？各地列國的神能拯救他們的國脫離我的手嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我與我列祖向列邦所行的，你們豈不知道嗎？列邦的神何嘗能救自己的國脫離我手呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do you not know what I and my fathers have done to all the peoples of the lands? Were the gods of the nations of the lands able at all to deliver their land from my hand?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do you not know what I and my predecessors have done to all the peoples of the other lands? Were the gods of those nations ever able to deliver their land from my hand?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Surely you must realize what I and the other kings of Assyria before me have done to all the people of the earth! Were any of the gods of those nations able to rescue their people from my power?"
      }
    },
