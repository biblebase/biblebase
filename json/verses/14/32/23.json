    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "许多人带着祭物到耶路撒冷献给耶和华，也带许多名贵的礼物送给犹大王希西迦。此后，希西迦受到各国的敬重。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有很多人把供物帶到耶路撒冷獻給耶和華，又把很多寶物送給猶大王希西家；從此，希西家受到各國的尊重。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有許多人到耶路撒冷，將供物獻與耶和華，又將寶物送給猶大王希西家。此後，希西家在列邦人的眼中看為尊大。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And many were bringing gifts to the Lord at Jerusalem and choice presents to Hezekiah king of Judah, so that he was exalted in the sight of all nations thereafter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Many brought offerings to Jerusalem for the Lord and valuable gifts for Hezekiah king of Judah. From then on he was highly regarded by all the nations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From then on King Hezekiah became highly respected among all the surrounding nations, and many gifts for the Lord arrived at Jerusalem, with valuable presents for King Hezekiah, too."
      }
    },
