    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们要刚强勇敢，不要在亚述王和他的大军面前恐惧惊慌，因为与我们同在的比与他同在的更有能力。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們要堅強勇敢，不要因亞述王和他統領的大軍懼怕驚慌；因為和我們同在的，比和他們同在的更多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們當剛強壯膽，不要因亞述王和跟隨他的大軍恐懼、驚慌；因為與我們同在的，比與他們同在的更大。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Be strong and courageous, do not fear or be dismayed because of the king of Assyria nor because of all the horde that is with him; for the one with us is greater than the one with him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Be strong and courageous. Do not be afraid or discouraged because of the king of Assyria and the vast army with him, for there is a greater power with us than with him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Be strong and courageous! Don’t be afraid or discouraged because of the king of Assyria or his mighty army, for there is a power far greater on our side!"
      }
    },
