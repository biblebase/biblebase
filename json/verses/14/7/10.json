    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "七月二十三日，所罗门王让民众各回本乡。他们看见耶和华向大卫、所罗门和祂的以色列子民所施的恩惠，就满心欢喜地回家去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "七月二十三日，王遣散眾民回家；他們因看見耶和華向大衛、所羅門和他的子民以色列所施的恩惠，心裡都歡喜快樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "七月二十三日，王遣散眾民；他們因見耶和華向大衛和所羅門與他民以色列所施的恩惠，就都心中喜樂，各歸各家去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then on the twenty-third day of the seventh month he sent the people to their tents, rejoicing and happy of heart because of the goodness that the Lord had shown to David and to Solomon and to His people Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the twenty-third day of the seventh month he sent the people to their homes, joyful and glad in heart for the good things the Lord had done for David and Solomon and for his people Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then at the end of the celebration, Solomon sent the people home. They were all joyful and glad because the Lord had been so good to David and to Solomon and to his people Israel."
      }
    },
