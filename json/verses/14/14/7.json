    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.14.7",
            "paragraphs": [
              "一四7\"地還屬我們\"。即仍未受到敵人侵擾。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他对犹大人说：“我们要修筑城邑，在四周建造城墙和望楼，装置大门和门闩。我们仍拥有这片土地，是因为我们寻求我们的上帝耶和华；我们寻求祂，祂就赐我们四境平安。”于是，他们修筑城邑，凡事顺利。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他對猶大人說：“我們要建造這些城，在四周築牆，建造城樓，安設城門，製作門閂。這地還屬於我們，是因為我們尋求耶和華我們的　神；我們既然尋求他，他就使我們四境平安。”於是他們興工建造，事事順利。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對猶大人說：「我們要建造這些城邑，四圍築牆，蓋樓，安門，做閂；地還屬我們，是因尋求耶和華－我們的神；我們既尋求他，他就賜我們四境平安。」於是建造城邑，諸事亨通。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he said to Judah, “Let us build these cities and surround them with walls and towers, gates and bars. The land is still ours, because we have sought the Lord our God. We have sought him, and he has given us peace on every side.” So they built and prospered."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For he said to Judah, “Let us build these cities and surround them with walls and towers, gates and bars. The land is still ours because we have sought the Lord our God; we have sought Him, and He has given us rest on every side.” So they built and prospered."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Let us build up these towns,” he said to Judah, “and put walls around them, with towers, gates and bars. The land is still ours, because we have sought the Lord our God; we sought him and he has given us rest on every side.” So they built and prospered."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore he said to Judah, “Let us build these cities and make walls around them, and towers, gates, and bars, while the land is yet before us, because we have sought the Lord our God; we have sought Him, and He has given us rest on every side.” So they built and prospered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Asa told the people of Judah, “Let us build towns and fortify them with walls, towers, gates, and bars. The land is still ours because we sought the Lord our God, and he has given us peace on every side.” So they went ahead with these projects and brought them to completion."
      }
    },
