      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "保罗说完就回到楼上，与大家一同掰饼、用餐，又谈了很久，直到天亮才离去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "隨即回到樓上，擘餅吃了，又講了很久，直到天亮才走。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "保羅又上去，擘餅，吃了，談論許久，直到天亮，這才走了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he had gone back up and had broken the bread and eaten, he talked with them a long while until daybreak, and then left."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he went upstairs again and broke bread and ate. After talking until daylight, he left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they all went back upstairs, shared in the Lord’s Supper, and ate together. Paul continued talking to them until dawn, and then he left."
      }
