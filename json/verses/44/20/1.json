      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "騷亂平息以後，保羅派人把門徒請了來，勸勉一番，就辭別起行，往馬其頓去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亂定之後，保羅請門徒來，勸勉他們，就辭別起行，往馬其頓去。"
      },
