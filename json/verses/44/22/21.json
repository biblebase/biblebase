    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "act.22.21",
            "paragraphs": [
              "21.雖然有抗議，主的吩咐仍然特定，保羅離開，不只是為保守他自身的安全，更是達成神早先的心意，要差他到“萬人”那裏去，尤其是外邦人。徒九30提及有關保羅離開的“人之因素”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主却对我说，‘去吧！我要差遣你到遥远的外族人那里。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他對我說：‘你走吧，我要派你到遠方的外族人那裡去。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主向我說：『你去吧！我要差你遠遠地往外邦人那裏去。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to me, ‘Go! For I will send you far away to the Gentiles.’  ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Then the Lord said to me, ‘Go; I will send you far away to the Gentiles.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But the Lord said to me, ‘Go, for I will send you far away to the Gentiles!’”"
      }
