    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "act.22.4",
            "paragraphs": [
              "4.保羅的話，使他與聽眾們同一陣線，熱心守護律法。這時他繼續解釋，他怎樣更進一步，在宗教熱心上超越聽眾們。他一向逼迫基督教會（這兒稱為這道，參九2），甚至直到死地（九1）。這裏保羅心目中想起司提反，在廿六10則指另指其他好些死刑（見\\cf5註）。有的信徒被下在監裏，意下是後來被釋放了的。從林前十五9；加一13；腓三6；提後一13，都證實這情勢。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我曾经把信奉这道的男女信徒抓进监狱，迫害他们，置他们于死地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我曾經迫害信奉這道的人直至死地，把男男女女都捆綁起來，送進監獄，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我也曾逼迫奉這道的人，直到死地，無論男女都鎖拿下監。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I persecuted this Way to the death, binding and putting both men and women into prisons,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I persecuted the followers of this Way to their death, arresting both men and women and throwing them into prison,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I persecuted the followers of the Way, hounding some to death, arresting both men and women and throwing them in prison."
      }
    },
