    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人听见这阵响声便赶过来，听见门徒在说他们各自的语言，都十分纳闷，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這聲音一響，許多人都聚了來，人人都聽見門徒講出聽眾各人本鄉的話，就莫名其妙。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这声音一响，有一群人就聚集而来，非常慌乱，因为各个人都听见使徒们用各个人本地的语言说话。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這聲音一響，眾人都來聚集，各人聽見門徒用眾人的鄉談說話，就甚納悶；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And at this sound the multitude came together, and they were bewildered, because each one was hearing them speak in his own language."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And when this sound occurred, the crowd came together, and were bewildered because each one of them was hearing them speak in his own language."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they heard this sound, a crowd came together in bewilderment, because each one heard their own language being spoken."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And when this sound occurred, the multitude came together, and were confused, because everyone heard them speak in his own language."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they heard the loud noise, everyone came running, and they were bewildered to hear their own languages being spoken by the believers."
      }
    },
