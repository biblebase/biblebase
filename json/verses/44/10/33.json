    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是我立刻派人去请你。你能来，真是荣幸，现在我们都在上帝面前，想聆听主吩咐你传给我们的话。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以我立刻派人去請你，現在你來了，好極了！我們都在　神面前，要聽主吩咐你的一切話。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "所以我就立即派人到你那里。你来了真好！现在我们都来到这里，在神的面前，聆听主所吩咐你的一切话。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以我立時打發人去請你。你來了很好；現今我們都在神面前，要聽主所吩咐你的一切話。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So I sent for you at once, and you have been kind enough to come. Now therefore we are all here in the presence of God to hear all that you have been commanded by the Lord.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I sent for you immediately, and you have been kind enough to come. Now then, we are all here present before God to hear all that you have been commanded by the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I sent for you immediately, and it was good of you to come. Now we are all here in the presence of God to listen to everything the Lord has commanded you to tell us.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So I sent to you immediately, and you have done well to come. Now therefore, we are all present before God, to hear all the things commanded you by God.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I sent for you at once, and it was good of you to come. Now we are all here, waiting before God to hear the message the Lord has given you.”"
      }
    },
