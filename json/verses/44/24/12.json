        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們並沒有看見我在殿裏，或是在會堂裏，或是在城裏，和人辯論，聳動眾人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "and they did not find me disputing with anyone or stirring up a crowd, either in the temple or in the synagogues or in the city."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Neither in the temple, nor in the synagogues, nor in the city itself did they find me carrying on a discussion with anyone or causing a riot."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My accusers did not find me arguing with anyone at the temple, or stirring up a crowd in the synagogues or anywhere else in the city."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they neither found me in the temple disputing with anyone nor inciting the crowd, either in the synagogues or in the city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My accusers never found me arguing with anyone in the Temple, nor stirring up a riot in any synagogue or on the streets of the city."
      }
    },
