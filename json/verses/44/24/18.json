    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们看见我的时候，我已行过洁净礼，正在圣殿里献祭，没有聚众，也没有作乱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們看見我在殿裡的時候，我已經行完了潔淨禮，並沒有和一大群人在一起，也沒有生亂，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "正献上供物的时候，他们在圣殿里发现了我，那时我已经行完了洁净礼，既没有人群，也没有骚乱，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正獻的時候，他們看見我在殿裏已經潔淨了，並沒有聚眾，也沒有吵嚷，惟有幾個從亞細亞來的猶太人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "While I was doing this, they found me purified in the temple, without any crowd or tumult. But some Jews from Asia—"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "in which they found me occupied in the temple, having been purified, without any crowd or uproar. But there were some Jews from Asia—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I was ceremonially clean when they found me in the temple courts doing this. There was no crowd with me, nor was I involved in any disturbance."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "in the midst of which some Jews from Asia found me purified in the temple, neither with a mob nor with tumult."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My accusers saw me in the Temple as I was completing a purification ceremony. There was no crowd around me and no rioting."
      }
