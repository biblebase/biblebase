    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天使对他说：“束上腰带，穿好鞋子。”彼得一一照办。天使又说：“披上外衣，跟我来！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天使對他說：“束上帶子，穿上鞋！”他就這樣作了。天使又說：“披上外衣，跟我走！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天使對他說：「束上帶子，穿上鞋。」他就那樣做。天使又說：「披上外衣，跟着我來。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the angel said to him, “Gird yourself and put on your sandals.” And he did so. And he *said to him, “Wrap your cloak around you and follow me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the angel said to him, “Put on your clothes and sandals.” And Peter did so. “Wrap your cloak around you and follow me,” the angel told him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the angel told him, “Get dressed and put on your sandals.” And he did. “Now put on your coat and follow me,” the angel ordered."
      }
