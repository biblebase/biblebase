    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万事令人厌烦，人述说不尽。 眼看，看不饱； 耳听，听不够。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬事都令人厭倦， 人說，說不盡； 眼看，看不飽； 耳聽，聽不足。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬事令人厭煩， 人不能說盡。 眼看，看不飽； 耳聽，聽不足。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All things are wearisome; Man is not able to tell it. The eye is not satisfied with seeing, Nor is the ear filled with hearing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All things are wearisome, more than one can say. The eye never has enough of seeing, nor the ear its fill of hearing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everything is wearisome beyond description. No matter how much we see, we are never satisfied. No matter how much we hear, we are not content."
      }
