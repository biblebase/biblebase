    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "ecc.9.7",
            "paragraphs": [
              "9:7",
              "歡歡喜喜吃你的飯，心中快樂喝你的酒：盡管人人都有一死，時間和機會也一樣地臨到每一個人(11節)，但我們這些蒙神所喜悅的人(比較羅12:2)仍當常常快樂享受他賜我們的一切。這裏的“酒”(希伯來文yayin)無疑是指新鮮葡萄的甜汁(參“舊約中的酒”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "去欢欢喜喜、快快乐乐地吃喝吧！因为上帝已经悦纳你所做的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你只管快快樂樂地去吃你的飯，懷著高興的心去喝你的酒，因為　神已經悅納了你所作的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你只管去歡歡喜喜吃你的飯，心中快樂喝你的酒，因為神已經悅納你的作為。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Go then, eat your bread in happiness and drink your wine with a cheerful heart; for God has already approved your works."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Go, eat your food with gladness, and drink your wine with a joyful heart, for God has already approved what you do."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So go ahead. Eat your food with joy, and drink your wine with a happy heart, for God approves of this!"
      }
    },
