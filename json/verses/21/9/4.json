    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ecc.9.2-6",
            "paragraphs": [
              "九2～6不管人生中的事件看似多麽偶然，所有人都要面對一個必然的結局——死亡。"
            ]
          },
          {
            "verse": "ecc.9.4-6",
            "paragraphs": [
              "九4～6活人比死人更有優勢（4節）。死人再沒有借努力工作而得到回報的機會（5節），而且他們不能再分享這地上的生命（6節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有生命就有希望，一条活狗总比一头死狮子强。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡有生命的，就有盼望，因為活狗比死獅更好。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "要知道，谁在活人当中，谁就有希望，因为活着的狗好过死了的狮子；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "與一切活人相連的，那人還有指望，因為活着的狗比死了的獅子更強。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But he who is joined with all the living has hope, for a living dog is better than a dead lion."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For whoever is joined with all the living, there is hope; surely a live dog is better than a dead lion."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who is among the living has hope—even a live dog is better off than a dead lion!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But for him who is joined to all the living there is hope, for a living dog is better than a dead lion."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There is hope only for the living. As they say, “It’s better to be a live dog than a dead lion!”"
      }
    },
