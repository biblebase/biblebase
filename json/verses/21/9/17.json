    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智者的细语胜过官长在愚人中的喊叫。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "聽智慧人安安靜靜說出來的話，勝過聽管理愚昧者的人的喊叫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "寧可在安靜之中聽智慧人的言語，不聽掌管愚昧人的喊聲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The words of the wise heard in quietness are better than the shouting of a ruler among fools."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The quiet words of the wise are more to be heeded than the shouts of a ruler of fools."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Better to hear the quiet words of a wise person than the shouts of a foolish king."
      }
    },
