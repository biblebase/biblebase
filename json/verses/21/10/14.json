    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚人高谈阔论， 其实无人知道将来的事， 人死后，谁能告诉他世间的事呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚昧人多言多語。人不知道將來會有甚麼事，誰能告訴他死後會發生甚麼事呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚昧人多有言語， 人卻不知將來有甚麼事； 他身後的事誰能告訴他呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet the fool multiplies words. No man knows what will happen, and who can tell him what will come after him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and fools multiply words. No one knows what is coming— who can tell someone else what will happen after them?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "they chatter on and on. No one really knows what is going to happen; no one can predict the future."
      }
    },
