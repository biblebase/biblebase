    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "ecc.10.1",
            "paragraphs": [
              "10:1",
              "死蒼蠅使作香的膏油發出臭氣：正如死蒼蠅腐爛會糟蹋許多香膏一樣，一點愚昧也能敗壞極大的智慧。一人可籌劃出智慧的計謀，可是某人所犯愚昧的錯誤便可使一切前功盡棄(參王下20:12~19)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "死苍蝇会使芬芳的膏油发臭， 同样，一点点愚昧足以毁掉智慧和尊荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "幾隻死蒼蠅能使做香料者的膏油發霉變臭，一點點愚昧比智慧和榮譽的影響更大。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "几只死苍蝇使做香的膏油腐烂发臭， 一点点愚昧也能压过智慧和尊荣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "死蒼蠅使做香的膏油發出臭氣； 這樣，一點愚昧也能敗壞智慧和尊榮。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Dead flies make the perfumer’s ointment give off a stench; so a little folly outweighs wisdom and honor."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Dead flies make a perfumer’s oil stink, so a little foolishness is weightier than wisdom and honor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As dead flies give perfume a bad smell, so a little folly outweighs wisdom and honor."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Dead flies putrefy the perfumer’s ointment, And cause it to give off a foul odor; So  does a little folly to one respected for wisdom and honor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As dead flies cause even a bottle of perfume to stink, so a little foolishness spoils great wisdom and honor."
      }
    },
