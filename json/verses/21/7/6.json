      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For as the crackling of thorn bushes under a pot, So is the laughter of the fool; And this too is futility."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like the crackling of thorns under the pot, so is the laughter of fools. This too is meaningless."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A fool’s laughter is quickly gone, like thorns crackling in a fire. This also is meaningless."
      }
