    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无人能掌管生命，留住生命，也无人能掌管死期。无人能免于这场争战，邪恶也救不了作恶者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有人能支配風，把它留住；沒有人能控制死期；戰爭之時，沒有人能免役；邪惡救不了行邪惡的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無人有權力掌管生命，將生命留住；也無人有權力掌管死期；這場爭戰，無人能免；邪惡也不能救那好行邪惡的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No man has authority to restrain the wind with the wind, or authority over the day of death; and there is no discharge in the time of war, and evil will not deliver those who practice it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As no one has power over the wind to contain it, so no one has power over the time of their death. As no one is discharged in time of war, so wickedness will not release those who practice it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "None of us can hold back our spirit from departing. None of us has the power to prevent the day of our death. There is no escaping that obligation, that dark battle. And in the face of death, wickedness will certainly not rescue the wicked."
      }
    },
