    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然而，恶人得不到祝福，他们必像幻影一样不能长久，因为他们不敬畏上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人卻沒有福樂，也得不到長壽；他好像影子的短暫，因為他在　神面前不存敬畏的心。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人卻不得福樂，也不得長久的年日；這年日好像影兒，因他不敬畏神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But it will not be well for the evil man and he will not lengthen his days like a shadow, because he does not fear God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet because the wicked do not fear God, it will not go well with them, and their days will not lengthen like a shadow."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wicked will not prosper, for they do not fear God. Their days will never grow long like the evening shadows."
      }
