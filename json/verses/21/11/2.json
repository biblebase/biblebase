    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ecc.11.2",
            "paragraphs": [
              "一一2分散的投資是有智慧的，因為沒有人知道有什麽災難會臨到哪個企業。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当把你的资财分给多人， 因为你不知道日后会有什么灾难临到世上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要分為七份，或分為八份，因為你不知道將來會有甚麼災禍在地上發生。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要分給七人，或分給八人， 因為你不知道將來有甚麼災禍臨到地上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Divide your portion to seven, or even to eight, for you do not know what misfortune may occur on the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Invest in seven ventures, yes, in eight; you do not know what disaster may come upon the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But divide your investments among many places, for you do not know what risks might lie ahead."
      }
    },
