        "lang": "cht",
        "text": "看風的，不撒種；觀雲的，不收割。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看風的，必不撒種； 望雲的，必不收割。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who watches the wind will not sow and he who looks at the clouds will not reap."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever watches the wind will not plant; whoever looks at the clouds will not reap."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Farmers who wait for perfect weather never plant. If they watch every cloud, they never harvest."
      }
