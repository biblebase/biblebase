    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.46.1-15",
            "paragraphs": [
              "四六1～15這段詳述每周（安息日）、每月（月朔）和每日的獻祭。若千禧年期間也需要守逾越節和住棚節等重要節日，祭物就沒有理由不用獻上。那麽，這些獻祭當然是為了紀念基督已經完成的獻祭。參看利未記第二章的\"獻祭\"圖表和利未記第二十三章的\"節期\"圖表。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在安息日和朔日，以色列人要站在这门口敬拜耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "每逢安息日和月朔，國中的人民要在這門口，在耶和華面前敬拜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在安息日和月朔，國內的居民要在這門口，耶和華面前敬拜。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The people of the land shall bow down at the entrance of that gate before the Lord on the Sabbaths and on the new moons."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The people of the land shall also worship at the doorway of that gate before the Lord on the sabbaths and on the new moons."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the Sabbaths and New Moons the people of the land are to worship in the presence of the Lord at the entrance of that gateway."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Likewise the people of the land shall worship at the entrance to this gateway before the Lord on the Sabbaths and the New Moons."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The common people will bow down and worship the Lord in front of this gateway on Sabbath days and the days of new moon celebrations."
      }
    },
