      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This city will not be a pot for you, nor will you be the meat in it; I will execute judgment on you at the borders of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No, this city will not be an iron pot for you, and you will not be like meat safe inside it. I will judge you even to the borders of Israel,"
      }
