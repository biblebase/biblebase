      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "迦得的南界是从他玛经米利巴·加低斯泉和埃及小河，一直到地中海。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "迦得南面的邊界，是從他瑪到米利巴．加低斯水，經埃及小河到大海。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "迦得地的南界是從他瑪到米利巴‧加低斯的水，延到埃及小河，直到大海。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And beside the border of Gad, at the south side toward the south, the border shall be from Tamar to the waters of Meribath-kadesh, to the brook of Egypt, to the Great Sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The southern boundary of Gad will run south from Tamar to the waters of Meribah Kadesh, then along the Wadi of Egypt to the Mediterranean Sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The southern border of Gad runs from Tamar to the waters of Meribah at Kadesh and then follows the Brook of Egypt to the Mediterranean."
      }
    },
