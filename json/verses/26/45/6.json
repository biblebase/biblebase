    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.45.1-8",
            "paragraphs": [
              "四五1～8聖殿位於神聖區域（約8.3平方英裏或13.3平方公裏；而第6節提到的5,000x25,000肘面積也包括在內）的中央，由祭司的房屋包圍，借以保護它免受褻瀆。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘在圣地旁边要分出一块十二点五公里长、二点五公里宽的地建城，以色列人都可以到那里居住。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們要劃分一塊地作城的基業，寬兩公里半，長十二公里半，在所獻的聖區旁邊，歸給以色列全家。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也要分定屬城的地業，寬五千肘，長二萬五千肘，挨着那分聖供地，要歸以色列全家。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Alongside the portion set apart as the holy district you shall assign for the property of the city an area 5,000 cubits broad and 25,000 cubits long. It shall belong to the whole house of Israel."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall give the city possession of an area 5,000 cubits wide and 25,000 cubits long, alongside the allotment of the holy portion; it shall be for the whole house of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘You are to give the city as its property an area 5,000 cubits wide and 25,000 cubits long, adjoining the sacred portion; it will belong to all Israel."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“You shall appoint as the property of the city an  area five thousand cubits wide and twenty-five thousand long, adjacent to the district of the holy section; it shall belong to the whole house of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Adjacent to the larger sacred area will be a section of land 81/3 miles long and 12/3 miles wide. This will be set aside for a city where anyone in Israel can live."
      }
    },
