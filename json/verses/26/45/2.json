    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.45.1-8",
            "paragraphs": [
              "四五1～8聖殿位於神聖區域（約8.3平方英裏或13.3平方公裏；而第6節提到的5,000x25,000肘面積也包括在內）的中央，由祭司的房屋包圍，借以保護它免受褻瀆。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要在其中分出一块长宽各二百五十米的地建圣殿。殿的周围要有二十五米宽的空地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "其中要有一塊作為聖所之地，長二百五十公尺，寬二百五十公尺，四面見方；周圍再有二十五公尺作空地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其中有作為聖所之地，長五百肘，寬五百肘，四面見方。四圍再有五十肘為郊野之地。"
