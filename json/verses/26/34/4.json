    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们没有养壮瘦弱的，没有医治生病的，没有包扎受伤的，没有领回走散的，没有寻找迷失的。你们反以暴力来管辖他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瘦弱的，你們沒有養壯；患病的，你們沒有醫治；受傷的，你們沒有包紮；被趕散的，你們沒有領回；迷失的，你們沒有尋找；你們反而用強暴嚴嚴地管轄牠們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瘦弱的，你們沒有養壯；有病的，你們沒有醫治；受傷的，你們沒有纏裹；被逐的，你們沒有領回；失喪的，你們沒有尋找；但用強暴嚴嚴地轄制。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Those who are sickly you have not strengthened, the diseased you have not healed, the broken you have not bound up, the scattered you have not brought back, nor have you sought for the lost; but with force and with severity you have dominated them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have not strengthened the weak or healed the sick or bound up the injured. You have not brought back the strays or searched for the lost. You have ruled them harshly and brutally."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have not taken care of the weak. You have not tended the sick or bound up the injured. You have not gone looking for those who have wandered away and are lost. Instead, you have ruled them with harshness and cruelty."
      }
    },
