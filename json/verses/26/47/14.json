    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.47.13-20",
            "paragraphs": [
              "四七13～20以色列地的北界從推羅以北的地中海延展到大馬色附近（15～17節）；東界由約但河與死海形成（18節）；南界從死海以下直到埃及\"小河\"（19節；參賽二七12的腳註）；西界就是地中海。這地區將要分配給以色列人作為他們的住處，但他們顯然會控制從尼羅河至幼發拉底河的整遍土地（比較創一五18）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其他各支派都要均分，因为我曾起誓将这地方赐给你们祖先。这地方必成为你们的产业。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要均分這地為業，因為我曾舉手要把這地賜給你們的列祖，這地必歸給你們為業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們承受這地為業，要彼此均分；因為我曾起誓應許將這地賜與你們的列祖；這地必歸你們為業。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And you shall divide equally what I swore to give to your fathers. This land shall fall to you as your inheritance."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall divide it for an inheritance, each one equally with the other; for I swore to give it to your forefathers, and this land shall fall to you as an inheritance."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are to divide it equally among them. Because I swore with uplifted hand to give it to your ancestors, this land will become your inheritance."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You shall inherit it equally with one another; for I raised My hand in an oath to give it to your fathers, and this land shall fall to you as your inheritance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Otherwise each tribe will receive an equal share. I took a solemn oath and swore that I would give this land to your ancestors, and it will now come to you as your possession."
      }
    },
