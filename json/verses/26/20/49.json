      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是我说：“唉，主耶和华啊，他们在议论我说，‘这人岂不是在说比喻吗？’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是我說：“啊，主耶和華啊！人都指著我說：‘這人不是在說比喻嗎？’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是我說：「哎！主耶和華啊，人都指着我說：他豈不是說比喻的嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I said, “Ah Lord God! They are saying of me, ‘Is he not just speaking parables?’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I said, “Sovereign Lord, they are saying of me, ‘Isn’t he just telling parables?’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I said, “O Sovereign Lord, they are saying of me, ‘He only talks in riddles!’”"
      }
