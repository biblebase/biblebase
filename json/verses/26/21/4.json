    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.21.4",
            "paragraphs": [
              "二一4猶大全地將要接受審判，\"自南至北\"（另見二○47）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从南到北不留一人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我既然要把義人和惡人都從你那裡剪除，所以我的刀必要出鞘，從南到北攻擊所有的人；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我既要從你中間剪除義人和惡人，所以我的刀要出鞘，自南至北攻擊一切有血氣的；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because I will cut off from you the righteous and the wicked, therefore My sword will go forth from its sheath against all flesh from south to north."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because I am going to cut off the righteous and the wicked, my sword will be unsheathed against everyone from south to north."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, I will cut off both the righteous and the wicked! I will draw my sword against everyone in the land from south to north."
      }
    },
