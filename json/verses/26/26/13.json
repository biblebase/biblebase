        "lang": "chs",
        "text": "我必使你的歌声消逝，琴音断绝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必使你唱歌的喧聲止息；人也再聽不到你的琴聲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必使你唱歌的聲音止息；人也不再聽見你彈琴的聲音。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I will silence the sound of your songs, and the sound of your harps will be heard no more."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will put an end to your noisy songs, and the music of your harps will be heard no more."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will stop the music of your songs. No more will the sound of harps be heard among your people."
      }
