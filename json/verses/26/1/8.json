      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Under their wings on their four sides they had human hands. All four of them had faces and wings,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Under each of their four wings I could see human hands. So each of the four beings had four faces and four wings."
      }
