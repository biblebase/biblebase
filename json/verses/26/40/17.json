    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他带我到外院，外院靠墙的四周是铺石地，上面共有三十间厢房。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他帶我到了外院，我見有些房子，院子周圍有鋪石地，鋪石地上有三十間房子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他帶我到外院，見院的四圍有鋪石地；鋪石地上有屋子三十間。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then he brought me into the outer court. And behold, there were chambers and a pavement, all around the court. Thirty chambers faced the pavement."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he brought me into the outer court, and behold, there were chambers and a pavement made for the court all around; thirty chambers faced the pavement."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he brought me into the outer court. There I saw some rooms and a pavement that had been constructed all around the court; there were thirty rooms along the pavement."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then he brought me into the outer court; and there  were  chambers and a pavement made all around the court; thirty chambers faced the pavement."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the man brought me through the gateway into the outer courtyard of the Temple. A stone pavement ran along the walls of the courtyard, and thirty rooms were built against the walls, opening onto the pavement."
      }
    },
