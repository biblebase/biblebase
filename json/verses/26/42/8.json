    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.42.1-14",
            "paragraphs": [
              "四二1～14聖殿內院北面有兩座三層高的建築物，一座的長度是另一座的兩倍，是祭司用作吃祭物（四六20）和存放聖衣（四四19）的地方。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "靠近外院的厢房长二十五米，靠近圣殿的厢房则长五十米。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "朝著外院的房子，長二十五公尺；朝著聖殿的房子，長五十公尺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "靠着外院的聖屋長五十肘。殿北面的聖屋長一百肘。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For the chambers on the outer court were fifty cubits long, while those opposite the nave were a hundred cubits long."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the length of the chambers which were in the outer court was fifty cubits; and behold, the length of those facing the temple was a hundred cubits."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While the row of rooms on the side next to the outer court was fifty cubits long, the row on the side nearest the sanctuary was a hundred cubits long."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The length of the chambers toward the outer court was fifty cubits, whereas that facing the temple was one hundred cubits."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This wall added length to the outer block of rooms, which extended for only 871/2 feet, while the inner block—the rooms toward the Temple—extended for 175 feet."
      }
