    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.42.1-14",
            "paragraphs": [
              "四二1～14聖殿內院北面有兩座三層高的建築物，一座的長度是另一座的兩倍，是祭司用作吃祭物（四六20）和存放聖衣（四四19）的地方。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "内院十米宽的空地和外院的铺石地对面有三层高的走廊彼此相对。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些房子一排對著內院十公尺寬的空地，一排對著外院的鋪石地，在三層樓上，都有走廊對著走廊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對着內院那二十肘寬之空地，又對着外院的鋪石地，在第三層樓上有樓廊對着樓廊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Opposite the twenty cubits which belonged to the inner court, and opposite the pavement which belonged to the outer court, was gallery corresponding to gallery in three stories."
