    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.42.1-14",
            "paragraphs": [
              "四二1～14聖殿內院北面有兩座三層高的建築物，一座的長度是另一座的兩倍，是祭司用作吃祭物（四六20）和存放聖衣（四四19）的地方。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "厢房的东面通道的前端有一道门。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "南邊的房子的門，也與北面的一樣。在東面夾道的開端，與牆相對的地方，有門可以進入。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正在牆前、夾道的東頭，有門可以進入，與向南聖屋的門一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Corresponding to the openings of the chambers which were toward the south was an opening at the head of the way, the way in front of the wall toward the east, as one enters them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "were the doorways of the rooms on the south. There was a doorway at the beginning of the passageway that was parallel to the corresponding wall extending eastward, by which one enters the rooms."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So there was an entrance in the wall facing the doors of the inner block of rooms, and another on the east at the end of the interior walkway."
      }
    },
