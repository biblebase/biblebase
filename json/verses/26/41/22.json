    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.41.22",
            "paragraphs": [
              "四一22這\"壇\"只是聖殿本部內的一件家具（外面有另一座壇；四三13～17），每邊寬三英尺半（1米）的正方形，高約五英尺（1.5米）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "里面有一座木头做的祭坛，高一点五米，宽一米，坛的四角、底及四面都是木造的。他对我说：“这是耶和华面前的桌子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭壇是木做的，高一公尺半，長一公尺；壇角和壇的四邊，全都是木做的。他對我說：“這是擺在耶和華面前的桌子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "壇是木頭做的，高三肘，長二肘。壇角和壇面，並四旁，都是木頭做的。他對我說：「這是耶和華面前的桌子。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The altar was of wood, three cubits high and its length two cubits; its corners, its base and its sides were of wood. And he said to me, “This is the table that is before the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There was a wooden altar three cubits high and two cubits square; its corners, its base and its sides were of wood. The man said to me, “This is the table that is before the Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There was an altar made of wood, 51/4 feet high and 31/2 feet across. Its corners, base, and sides were all made of wood. “This,” the man told me, “is the table that stands in the Lord’s presence.”"
      }
    },
