    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "ezk.41.1",
            "paragraphs": [
              "41:1",
              "殿：以西結見到的殿與所羅門王建造的殿有所不同。這裏沒有提到香壇、燈臺或約櫃。其中的原因聖經中未作解釋。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那人带我进入殿堂，量两边的墙柱，各三米宽。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他領我到外殿，量了牆柱，這邊厚三公尺，那邊厚三公尺，寬窄與會幕相同。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他帶我到殿那裏量牆柱：這面厚六肘，那面厚六肘，寬窄與會幕相同。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he brought me to the nave and measured the side pillars; six cubits wide on each side was the width of the side pillar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the man brought me to the main hall and measured the jambs; the width of the jambs was six cubits on each side."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After that, the man brought me into the sanctuary of the Temple. He measured the walls on either side of its doorway, and they were 101/2 feet thick."
      }
    },
