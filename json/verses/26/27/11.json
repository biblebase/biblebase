    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "ezk.27.1-36",
            "paragraphs": [
              "ii.推羅船只沈沒（二十七1～36）",
              "本章包括了兩種不同的文體：一是用哀歌（qînâ）節韻寫成的長詩（3b～9節、25b～36節），一是穿插在詩中間的散文體清單（10～25a節）。該首詩是一則很長的比喻，提到推羅是一艘很好的船，精工建造、船員優秀，但是載貨過重，因此在滔天巨浪中沈入海底。所有在地中海航行的人都站在岸邊為她的滅亡而哀哭。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "ezk.27.10-25",
            "paragraphs": [
              "二七10～25這段描述從多國而來的許多貨物都運載在推羅的船上。"
            ]
          },
          {
            "verse": "ezk.27.11",
            "paragraphs": [
              "二七11\"亞發\"。參看第8節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚发人和你的军队都守卫在你的城墙上，歌玛底人把守你的城楼，他们都在墙上挂满盾牌，使你完美无瑕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在你四圍的城牆上有亞發人和赫勒克人；在你的城樓上有歌瑪底人。他們把自己的盾牌掛在你四圍的城牆上，使你全然美麗。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞發人和你的軍隊都在你四圍的牆上，你的望樓也有勇士；他們懸掛盾牌，成全你的美麗。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sons of Arvad and your army were on your walls, all around, and the Gammadim were in your towers. They hung their shields on your walls all around; they perfected your beauty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Men of Arvad and Helek guarded your walls on every side; men of Gammad were in your towers. They hung their shields around your walls; they brought your beauty to perfection."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Men from Arvad and Helech stood on your walls. Your towers were manned by men from Gammad. Their shields hung on your walls, completing your beauty."
      }
    },
