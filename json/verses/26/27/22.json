    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "ezk.27.1-36",
            "paragraphs": [
              "ii.推羅船只沈沒（二十七1～36）",
              "本章包括了兩種不同的文體：一是用哀歌（qînâ）節韻寫成的長詩（3b～9節、25b～36節），一是穿插在詩中間的散文體清單（10～25a節）。該首詩是一則很長的比喻，提到推羅是一艘很好的船，精工建造、船員優秀，但是載貨過重，因此在滔天巨浪中沈入海底。所有在地中海航行的人都站在岸邊為她的滅亡而哀哭。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "ezk.27.10-25",
            "paragraphs": [
              "二七10～25這段描述從多國而來的許多貨物都運載在推羅的船上。"
            ]
          },
          {
            "verse": "ezk.27.22",
            "paragraphs": [
              "二七22\"示巴和拉瑪\"。同位於亞拉伯。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "示巴和拉玛的商人用各类上等香料、宝石和黄金交换你的货物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "示巴和拉瑪的商人都與你交易；他們用各樣上等的香料，各種寶石和黃金，來換取你的貨物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "示巴和拉瑪的商人與你交易，他們用各類上好的香料、各類的寶石，和黃金兌換你的貨物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The traders of Sheba and Raamah, they traded with you; they paid for your wares with the best of all kinds of spices, and with all kinds of precious stones and gold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘The merchants of Sheba and Raamah traded with you; for your merchandise they exchanged the finest of all kinds of spices and precious stones, and gold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The merchants of Sheba and Raamah came with all kinds of spices, jewels, and gold in exchange for your wares."
      }
    },
