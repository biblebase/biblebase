    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "ezk.27.1-36",
            "paragraphs": [
              "ii.推羅船只沈沒（二十七1～36）",
              "本章包括了兩種不同的文體：一是用哀歌（qînâ）節韻寫成的長詩（3b～9節、25b～36節），一是穿插在詩中間的散文體清單（10～25a節）。該首詩是一則很長的比喻，提到推羅是一艘很好的船，精工建造、船員優秀，但是載貨過重，因此在滔天巨浪中沈入海底。所有在地中海航行的人都站在岸邊為她的滅亡而哀哭。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "ezk.27.26-36",
            "paragraphs": [
              "二七26～36這段描述推羅的\"沈\"船致使航海的人感到震驚。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你舵手的哀号声必震动海岸，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因你舵手的呼叫聲， 郊野都必震動。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你掌舵的呼號之聲一發， 郊野都必震動。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "At the sound of the cry of your pilots The pasture lands will shake."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The shorelands will quake when your sailors cry out."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your cities by the sea tremble as your pilots cry out in terror."
      }
    },
