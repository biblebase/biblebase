    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "ezk.27.1-36",
            "paragraphs": [
              "ii.推羅船只沈沒（二十七1～36）",
              "本章包括了兩種不同的文體：一是用哀歌（qînâ）節韻寫成的長詩（3b～9節、25b～36節），一是穿插在詩中間的散文體清單（10～25a節）。該首詩是一則很長的比喻，提到推羅是一艘很好的船，精工建造、船員優秀，但是載貨過重，因此在滔天巨浪中沈入海底。所有在地中海航行的人都站在岸邊為她的滅亡而哀哭。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "ezk.27.26-36",
            "paragraphs": [
              "二七26～36這段描述推羅的\"沈\"船致使航海的人感到震驚。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "船夫把你摇到汪洋之中， 但东风要把你击碎在海中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "給你蕩槳的已經把你蕩到水深之處； 東風在海的中心 把你吹破。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "盪槳的已經把你盪到大水之處， 東風在海中將你打破。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Your rowers have brought you Into great waters; The east wind has broken you In the heart of the seas."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your oarsmen take you out to the high seas. But the east wind will break you to pieces far out at sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But look! Your oarsmen have taken you into stormy seas! A mighty eastern gale has wrecked you in the heart of the sea!"
      }
    },
