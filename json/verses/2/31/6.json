    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我委派但支派亚希撒抹的儿子亚何利亚伯做他的助手。至于其他能工巧匠，我已赐给他们智慧，好照我对你的吩咐制造一切器具，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪，我已經指派但支派中亞希撒抹的兒子亞何利亞伯和他同工；所有心裡有智慧的人，我也把智慧賜給他們，使他們可以作我吩咐你的一切工作，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我分派但支派中、亞希撒抹的兒子亞何利亞伯與他同工。凡心裏有智慧的，我更使他們有智慧，能做我一切所吩咐的，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And behold, I Myself have appointed with him Oholiab, the son of Ahisamach, of the tribe of Dan; and in the hearts of all who are skillful I have put skill, that they may make all that I have commanded you:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moreover, I have appointed Oholiab son of Ahisamak, of the tribe of Dan, to help him. Also I have given ability to all the skilled workers to make everything I have commanded you:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And I have personally appointed Oholiab son of Ahisamach, of the tribe of Dan, to be his assistant. Moreover, I have given special skill to all the gifted craftsmen so they can make all the things I have commanded you to make:"
      }
    },
