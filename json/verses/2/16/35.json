    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "exo.16.35",
            "paragraphs": [
              "一六35\"四十年\"直到以色列人進入迦南。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列百姓就在旷野吃了四十年的吗哪，直到他们到达有人烟的迦南为止。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人吃嗎哪共四十年，直到進了有人居住的地方為止；他們吃嗎哪，直到進入迦南地的邊界為止。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人吃嗎哪共四十年，直到進了有人居住之地，就是迦南的境界。（"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sons of Israel ate the manna forty years, until they came to an inhabited land; they ate the manna until they came to the border of the land of Canaan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Israelites ate manna forty years, until they came to a land that was settled; they ate manna until they reached the border of Canaan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the people of Israel ate manna for forty years until they arrived at the land where they would settle. They ate manna until they came to the border of the land of Canaan."
      }
    },
