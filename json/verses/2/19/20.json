    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华降临在西奈山顶，召摩西到山顶，摩西就上去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華降臨在西奈山上，就是降臨到山頂上；耶和華呼召摩西上山頂，他就上去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華降臨在西奈山頂上，耶和華召摩西上山頂，摩西就上去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord came down on Mount Sinai, to the top of the mountain; and the Lord called Moses to the top of the mountain, and Moses went up."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord descended to the top of Mount Sinai and called Moses to the top of the mountain. So Moses went up"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord came down on the top of Mount Sinai and called Moses to the top of the mountain. So Moses climbed the mountain."
      }
    },
