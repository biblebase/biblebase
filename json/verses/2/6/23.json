    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "exo.6.14-27",
            "paragraphs": [
              "六14～27這裏插入以色列人的族譜，為的是識別摩西與亞倫的家系，好指出他們是剛被委派去肩負拯救以色列人的重任（比較29節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦与亚米拿达的女儿、拿顺的妹妹以利沙巴结婚，生了拿答、亚比户、以利亚撒和以他玛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫娶了亞米拿達的女兒拿順的妹妹以利沙巴作妻子；她給亞倫生了拿答、亞比戶、以利亞撒、以他瑪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫娶了亞米拿達的女兒，拿順的妹妹，以利沙巴為妻，她給他生了拿答、亞比戶、以利亞撒、以他瑪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Aaron married Elisheba, the daughter of Amminadab, the sister of Nahshon, and she bore him Nadab and Abihu, Eleazar and Ithamar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Aaron married Elisheba, daughter of Amminadab and sister of Nahshon, and she bore him Nadab and Abihu, Eleazar and Ithamar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Aaron married Elisheba, the daughter of Amminadab and sister of Nahshon, and she gave birth to his sons, Nadab, Abihu, Eleazar, and Ithamar."
      }
    },
