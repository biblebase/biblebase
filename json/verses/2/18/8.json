    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西把耶和华为拯救以色列人而向法老和埃及人所行的事，以及怎样救百姓脱离路上遇见的种种困难，都告诉了叶忒罗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西把耶和華為以色列的緣故，向法老和埃及人所作的一切，以及路上遭遇的一切困難，並且耶和華怎樣拯救他們，都向他的岳父述說。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西將耶和華為以色列的緣故向法老和埃及人所行的一切事，以及路上所遭遇的一切艱難，並耶和華怎樣搭救他們，都述說與他岳父聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moses told his father-in-law all that the Lord had done to Pharaoh and to the Egyptians for Israel’s sake, all the hardship that had befallen them on the journey, and how the Lord had delivered them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moses told his father-in-law about everything the Lord had done to Pharaoh and the Egyptians for Israel’s sake and about all the hardships they had met along the way and how the Lord had saved them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses told his father-in-law everything the Lord had done to Pharaoh and Egypt on behalf of Israel. He also told about all the hardships they had experienced along the way and how the Lord had rescued his people from all their troubles."
      }
    },
