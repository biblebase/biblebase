    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "强迫他们和泥造砖，并做田间一切的苦工，使他们痛苦不堪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "埃及人使他們因作苦工而覺得命苦，他們要和泥、做磚、作田間各樣的工；這一切苦工，埃及人都嚴嚴地驅使以色列人去作。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使他們因做苦工覺得命苦；無論是和泥，是做磚，是做田間各樣的工，在一切的工上都嚴嚴地待他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they made their lives bitter with hard labor in mortar and bricks and at all kinds of labor in the field, all their labors which they rigorously imposed on them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They made their lives bitter with harsh labor in brick and mortar and with all kinds of work in the fields; in all their harsh labor the Egyptians worked them ruthlessly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They made their lives bitter, forcing them to mix mortar and make bricks and do all the work in the fields. They were ruthless in all their demands."
      }
    },
