    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要对他说，‘希伯来人的上帝耶和华差遣我来告诉你，要让祂的子民到旷野去事奉祂，但到如今你还是不肯。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要對法老說：‘耶和華希伯來人的　神差我到你這裡來，說：“讓我的人民離開這裡，好使他們可以在曠野事奉我”，你看，直到現在你還是不聽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對他說：『耶和華－希伯來人的神打發我來見你，說：容我的百姓去，好在曠野事奉我。到如今你還是不聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall say to him, ‘The Lord, the God of the Hebrews, sent me to you, saying, “Let My people go, that they may serve Me in the wilderness. But behold, you have not listened until now.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then say to him, ‘The Lord, the God of the Hebrews, has sent me to say to you: Let my people go, so that they may worship me in the wilderness. But until now you have not listened."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then announce to him, ‘The Lord, the God of the Hebrews, has sent me to tell you, “Let my people go, so they can worship me in the wilderness.” Until now, you have refused to listen to him."
      }
    },
