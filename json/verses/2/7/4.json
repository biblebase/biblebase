    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "exo.7.4",
            "paragraphs": [
              "七4\"我的軍隊\"。即好象軍隊一樣有組織和裝備（比較一三18）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他将无动于衷。那时，我必伸手重重地惩罚埃及，然后领我的大队子民离开埃及。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可是法老不聽你們的話，我必下手擊打埃及；藉著嚴厲的刑罰把我的軍隊，我的人民以色列人，從埃及地領出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但法老必不聽你們；我要伸手重重地刑罰埃及，將我的軍隊以色列民從埃及地領出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Pharaoh does not listen to you, then I will lay My hand on Egypt and bring out My hosts, My people the sons of Israel, from the land of Egypt by great judgments."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "he will not listen to you. Then I will lay my hand on Egypt and with mighty acts of judgment I will bring out my divisions, my people the Israelites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even then Pharaoh will refuse to listen to you. So I will bring down my fist on Egypt. Then I will rescue my forces—my people, the Israelites—from the land of Egypt with great acts of judgment."
      }
    },
