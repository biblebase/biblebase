        "text": "我要使法老心硬，我也要在埃及地加增我的神蹟奇事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要使法老的心剛硬，也要在埃及地多行神蹟奇事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I will harden Pharaoh’s heart that I may multiply My signs and My wonders in the land of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I will harden Pharaoh’s heart, and though I multiply my signs and wonders in Egypt,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I will make Pharaoh’s heart stubborn so I can multiply my miraculous signs and wonders in the land of Egypt."
      }
    },
