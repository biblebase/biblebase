    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就在那一天，耶和华带领以色列大队人马离开了埃及。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就在這一天，耶和華按著他們的軍隊，把以色列人從埃及地領出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正當那日，耶和華將以色列人按着他們的軍隊，從埃及地領出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And on that same day the Lord brought the sons of Israel out of the land of Egypt by their hosts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And on that very day the Lord brought the Israelites out of Egypt by their divisions."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On that very day the Lord brought the people of Israel out of the land of Egypt like an army."
      }
