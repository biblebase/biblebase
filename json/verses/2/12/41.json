      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就在四百三十年屆滿的那一天，耶和華的全軍都從埃及地出來了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正滿了四百三十年的那一天，耶和華的軍隊都從埃及地出來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And at the end of four hundred and thirty years, to the very day, all the hosts of the Lord went out from the land of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At the end of the 430 years, to the very day, all the Lord’s divisions left Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In fact, it was on the last day of the 430th year that all the Lord’s forces left the land."
      }
