      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要向以色列人收赎命的款项，供会幕使用，可以使以色列人在耶和华面前蒙眷顾，赎性命。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要從以色列人收取這贖銀，指定作會幕事奉的使用，好使它在耶和華面前為以色列人作個記念，也贖回你們的性命。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要從以色列人收這贖罪銀，作為會幕的使用，可以在耶和華面前為以色列人作紀念，贖生命。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall take the atonement money from the sons of Israel and shall give it for the service of the tent of meeting, that it may be a memorial for the sons of Israel before the Lord, to make atonement for yourselves.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Receive the atonement money from the Israelites and use it for the service of the tent of meeting. It will be a memorial for the Israelites before the Lord, making atonement for your lives.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Receive this ransom money from the Israelites, and use it for the care of the Tabernacle. It will bring the Israelites to the Lord’s attention, and it will purify your lives.”"
      }
