    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡被统计的，年龄在二十岁以上的男子都要献此礼物给耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是屬於那些被數點的人，自二十歲以上的，要把禮物獻給耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡過去歸那些被數的人，從二十歲以外的，要將這禮物奉給耶和華。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Everyone who is numbered in the census, from twenty years old and upward, shall give the Lord’s offering."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Everyone who is numbered, from twenty years old and over, shall give the contribution to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All who cross over, those twenty years old or more, are to give an offering to the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Everyone included among those who are numbered, from twenty years old and above, shall give an offering to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All who have reached their twentieth birthday must give this sacred offering to the Lord."
      }
    },
