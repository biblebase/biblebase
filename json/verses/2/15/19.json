    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老的战车和骑兵追到海中，耶和华使海水回流淹没他们，以色列人却在海中踏着干地走到对岸。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為法老的馬匹、戰車和馬兵進到海中去的時候，耶和華使海水回流到他們身上；以色列人卻在海中走乾地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老的馬匹、車輛，和馬兵下到海中，耶和華使海水回流，淹沒他們；惟有以色列人在海中走乾地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the horses of Pharaoh with his chariots and his horsemen went into the sea, and the Lord brought back the waters of the sea on them, but the sons of Israel walked on dry land through the midst of the sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Pharaoh’s horses, chariots and horsemen went into the sea, the Lord brought the waters of the sea back over them, but the Israelites walked through the sea on dry ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Pharaoh’s horses, chariots, and charioteers rushed into the sea, the Lord brought the water crashing down on them. But the people of Israel had walked through the middle of the sea on dry ground!"
      }
    },
