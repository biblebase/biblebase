    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "exo.21.14",
            "paragraphs": [
              "二一14若是蓄意殺人，即使祭壇也不能作為一個安全的避難所。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若是蓄意杀人，就算他逃到我的祭坛那里，也要把他拉出来处死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "若有人任意待他的鄰舍，用詭計把他殺了，你要把他從我的祭壇那裡拿去處死。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人若任意用詭計殺了他的鄰舍，就是逃到我的壇那裏，也當捉去把他治死。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But if a man willfully attacks another to kill him by cunning, you shall take him from my altar, that he may die."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If, however, a man acts presumptuously toward his neighbor, so as to kill him craftily, you are to take him even from My altar, that he may die."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if anyone schemes and kills someone deliberately, that person is to be taken from my altar and put to death."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“But if a man acts with premeditation against his neighbor, to kill him by treachery, you shall take him from My altar, that he may die."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "However, if someone deliberately kills another person, then the slayer must be dragged even from my altar and be put to death."
      }
    },
