    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.21.7",
            "paragraphs": [
              "7.婢女不可像男仆那樣出去。希伯來妾婢的情況頗為不同。她不像男仆到時候便能自動離開，因為主人兼丈夫對她還有未盡的責任。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.21.7-11",
            "paragraphs": [
              "二一7～11女仆有機會嫁給主人或他的兒子，或由親屬買贖回來，或生活上受到照顧。主人絕對不能把她賣給外邦人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若有人把女儿卖为奴婢，她不可像男仆那样离开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果有人把女兒賣作婢女，婢女不可像男僕那樣出去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人若賣女兒作婢女，婢女不可像男僕那樣出去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If a man sells his daughter as a female slave, she is not to go free as the male slaves do."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If a man sells his daughter as a servant, she is not to go free as male servants do."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When a man sells his daughter as a slave, she will not be freed at the end of six years as the men are."
      }
    },
