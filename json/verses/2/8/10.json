    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老说：“明天。”摩西说：“就照你说的，这样你就知道没有神明能比得上我们的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老說：“明天。”摩西回答：“就照著你的話行吧，好叫你知道沒有神像耶和華我們的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：「明天。」摩西說：「可以照你的話吧，好叫你知道沒有像耶和華－我們神的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he said, “Tomorrow.” So he said, “May it be according to your word, that you may know that there is no one like the Lord our God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Tomorrow,” Pharaoh said. Moses replied, “It will be as you say, so that you may know there is no one like the Lord our God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do it tomorrow,” Pharaoh said. “All right,” Moses replied, “it will be as you have said. Then you will know that there is no one like the Lord our God."
      }
    },
