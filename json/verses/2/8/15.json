    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "exo.8.15",
            "paragraphs": [
              "八15短暫的磨難通常不能改變人的心。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老看见灾难已经过去，便又硬着心，不肯听从摩西和亚伦的话，正如耶和华所言。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老一見災禍平息了，就心裡剛硬，不肯聽他們的話，就像耶和華所說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但法老見災禍鬆緩，就硬着心，不肯聽他們，正如耶和華所說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when Pharaoh saw that there was relief, he hardened his heart and did not listen to them, as the Lord had said."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But when Pharaoh saw that there was relief, he hardened his heart and would not listen to Moses and Aaron, just as the Lord had said."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when Pharaoh saw that relief had come, he became stubborn. He refused to listen to Moses and Aaron, just as the Lord had predicted."
      }
    },
