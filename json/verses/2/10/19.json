    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华就把风向倒转，变成猛烈的西风，把蝗虫刮走，吹进红海。埃及境内一只蝗虫也没留下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華轉了非常強大的西風，把蝗蟲吹起來了，投進紅海裡；在埃及全境，連一隻蝗蟲也沒留下。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華轉了極大的西風，把蝗蟲颳起，吹入紅海；在埃及的四境連一個也沒有留下。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the Lord shifted the wind to a very strong west wind which took up the locusts and drove them into the Red Sea; not one locust was left in all the territory of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Lord changed the wind to a very strong west wind, which caught up the locusts and carried them into the Red Sea. Not a locust was left anywhere in Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord responded by shifting the wind, and the strong west wind blew the locusts into the Red Sea. Not a single locust remained in all the land of Egypt."
      }
    },
