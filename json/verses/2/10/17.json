    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在求你饶恕我的罪，就这一次，为我祈求你们的上帝耶和华撤去这场死亡之灾吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在求你只這一次赦免我的罪，也請你們求告耶和華你們的　神，使這一次的死亡離開我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在求你，只這一次，饒恕我的罪，求耶和華－你們的神使我脫離這一次的死亡。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now therefore, please forgive my sin only this once, and make supplication to the Lord your God, that He would only remove this death from me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now forgive my sin once more and pray to the Lord your God to take this deadly plague away from me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Forgive my sin, just this once, and plead with the Lord your God to take away this death from me.”"
      }
    },
