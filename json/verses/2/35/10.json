    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.35.10",
            "paragraphs": [
              "10.你們中間凡心裏有智慧的都要來作。凡樂意的人都要捐獻材料的經文（5節）和本節相仿佛，但此處呼召的內容比較廣泛。除了比撒列和亞何利亞伯有分以外，一切“能幹”的人都可以參加建築（英譯多作“能幹”；和合本直譯作“心裏有智慧”）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们当中的能工巧匠都要来建造耶和华所吩咐的物件，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘你們中間凡是心裡有智慧的都要來，做耶和華吩咐的一切，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們中間凡心裏有智慧的都要來做耶和華一切所吩咐的："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Let every skillful man among you come, and make all that the Lord has commanded:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“All who are skilled among you are to come and make everything the Lord has commanded:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Come, all of you who are gifted craftsmen. Construct everything that the Lord has commanded:"
      }
    },
