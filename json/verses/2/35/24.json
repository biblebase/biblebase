    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.35.24",
            "paragraphs": [
              "24.女人可以紡線（箴三十一13），官長比較富有，可以帶來寶石和香料（27、28節）。人人都有分為耶和華建造聖所，若不是各盡其職，工作也不能完成（參弗四16）。這也是今日信徒應當聽從的信息。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡奉献银器和铜器的，都拿来献给耶和华。凡有皂荚木的，只要用得上，都拿来奉献。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是奉獻銀子和銅為禮物的，都帶了來作耶和華的禮物；凡是有皂莢木可以用作工程上任何使用的，都帶了來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡獻銀子和銅給耶和華為禮物的都拿了來；凡有皂莢木可做甚麼使用的也拿了來。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Everyone who could make a contribution of silver or bronze brought it as the Lord’s contribution. And every one who possessed acacia wood of any use in the work brought it."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Everyone who could make a contribution of silver and bronze brought the Lord’s contribution; and every man who had in his possession acacia wood for any work of the service brought it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those presenting an offering of silver or bronze brought it as an offering to the Lord, and everyone who had acacia wood for any part of the work brought it."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Everyone who offered an offering of silver or bronze brought the Lord’s offering. And everyone with whom was found acacia wood for any work of the service, brought it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And all who had silver and bronze objects gave them as a sacred offering to the Lord. And those who had acacia wood brought it for use in the project."
      }
    },
