      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡懂得纺线的妇女，都把亲手纺成的细麻线和蓝色、紫色、朱红色的线拿来奉献。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是心中有智慧的婦女都親手紡織，把所紡的藍色紫色朱紅色線都帶了來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡心中有智慧的婦女親手紡線，把所紡的藍色、紫色、朱紅色線，和細麻都拿了來。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And every skillful woman spun with her hands, and they all brought what they had spun in blue and purple and scarlet yarns and fine twined linen."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the skilled women spun with their hands, and brought what they had spun, in blue and purple and scarlet material and in fine linen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every skilled woman spun with her hands and brought what she had spun—blue, purple or scarlet yarn or fine linen."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "All the women who  were  gifted artisans spun yarn with their hands, and brought what they had spun, of blue, purple, and scarlet, and fine linen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the women who were skilled in sewing and spinning prepared blue, purple, and scarlet thread, and fine linen cloth."
      }
