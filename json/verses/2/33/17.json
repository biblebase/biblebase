    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华回答说：“我答应你的请求，因为你在我面前蒙了恩，我凭名字认识你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對摩西說：“你所說的這件事我也要作，因為你在我眼前蒙恩，並且我按著你的名字認識了你。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對摩西說：「你這所求的我也要行；因為你在我眼前蒙了恩，並且我按你的名認識你。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord said to Moses, “I will also do this thing of which you have spoken; for you have found favor in My sight and I have known you by name.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Lord said to Moses, “I will do the very thing you have asked, because I am pleased with you and I know you by name.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord replied to Moses, “I will indeed do what you have asked, for I look favorably on you, and I know you by name.”"
      }
    },
