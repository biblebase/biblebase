    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "北面的帷幔也是长四十六米，柱子、铜底座、钩子、横杆的样式与南面的一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "北面，有幔幕四十四公尺，幔幕的柱子有二十根，柱座有二十個，是銅做的；柱鉤和橫柱閂是銀做的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "北面也有帷子，寬一百肘。帷子的柱子二十根，帶卯的銅座二十個；柱子上的鈎子和杆子都是用銀子做的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the north side there were one hundred cubits; their twenty pillars and their twenty sockets were of bronze, the hooks of the pillars and their bands were of silver."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The north side was also a hundred cubits long and had twenty posts and twenty bronze bases, with silver hooks and bands on the posts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He made a similar set of curtains for the north side—150 feet of curtains held up by twenty posts set securely in bronze bases. He hung the curtains with silver hooks and rings."
      }
    },
