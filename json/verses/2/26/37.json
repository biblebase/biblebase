    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要用皂荚木做五根挂门帘的柱子，柱子包上金，上面要有金钩，并要为柱子造五个带凹槽的铜底座。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你又要用皂莢木為簾子做五根柱子，柱子要包金，柱上有金鉤，你也要為柱腳鑄造五個銅插座。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要用皂莢木為簾子做五根柱子，用金子包裹。柱子上當有金鈎；又要為柱子用銅鑄造五個帶卯的座。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And you shall make for the screen five pillars of acacia, and overlay them with gold. Their hooks shall be of gold, and you shall cast five bases of bronze for them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall make five pillars of acacia for the screen and overlay them with gold, their hooks also being of gold; and you shall cast five sockets of bronze for them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Make gold hooks for this curtain and five posts of acacia wood overlaid with gold. And cast five bronze bases for them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And you shall make for the screen five pillars of acacia wood, and overlay them with gold; their hooks shall  be gold, and you shall cast five sockets of bronze for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Craft five posts from acacia wood. Overlay them with gold, and hang the curtain from them with gold hooks. Cast five bronze bases for the posts."
      }
    },
