    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.26.1-37",
            "paragraphs": [
              "ii.帳幕（二十六1～37）",
              "本段經文雖然也有隱誨之處，大體上仍是清楚明白的。讀者最好是參看聖經辭典的插圖。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.26.7-13",
            "paragraphs": [
              "二六7～13\"幔子\"。這些幔子覆蓋裏面的細麻布幔子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "两旁余下的幔子可垂在圣幕的两边，每边长五十厘米，盖着圣幕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "帳棚幔子多出的長度，要這邊五十公分，那邊五十公分，並且要在會幕的兩邊，遮蓋會幕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "罩棚的幔子所餘長的，這邊一肘，那邊一肘，要垂在帳幕的兩旁，遮蓋帳幕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The cubit on one side and the cubit on the other, of what is left over in the length of the curtains of the tent, shall lap over the sides of the tabernacle on one side and on the other, to cover it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The tent curtains will be a cubit longer on both sides; what is left will hang over the sides of the tabernacle so as to cover it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Allow 18 inches of remaining material to hang down over each side, so the Tabernacle is completely covered."
      }
    },
