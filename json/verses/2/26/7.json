    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.26.1-37",
            "paragraphs": [
              "ii.帳幕（二十六1～37）",
              "本段經文雖然也有隱誨之處，大體上仍是清楚明白的。讀者最好是參看聖經辭典的插圖。"
            ]
          },
          {
            "verse": "exo.26.7",
            "paragraphs": [
              "7.山羊毛……幔子。黑色山羊毛是通用的帳幕材料，能夠保護細麻不受外面日曬雨淋的打擊，又使裏面華麗的顏色不致外露。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.26.7-13",
            "paragraphs": [
              "二六7～13\"幔子\"。這些幔子覆蓋裏面的細麻布幔子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“要用山羊毛织十一幅幔子作圣幕的罩棚，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要用山羊毛做幔子，作會幕上面的帳棚，你要做十一幅幔子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你要用山羊毛織十一幅幔子，作為帳幕以上的罩棚。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Then you shall make curtains of goats’ hair for a tent over the tabernacle; you shall make eleven curtains in all."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Make curtains of goat hair for the tent over the tabernacle—eleven altogether."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Make eleven curtains of goat-hair cloth to serve as a tent covering for the Tabernacle."
      }
    },
