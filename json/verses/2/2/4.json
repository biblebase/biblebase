        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "婴儿的姐姐远远地站着，要看看她的弟弟究竟会怎样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "孩子的姊姊遠遠地站著，要知道孩子究竟怎樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "孩子的姊姊遠遠站着，要知道他究竟怎麼樣。"
      },
