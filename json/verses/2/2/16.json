      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "米甸祭司的七个女儿来打水，要把水倒进槽里饮她们父亲的羊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "米甸的祭司有七個女兒，她們來打水，打滿了水槽，要給父親的羊群喝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "一日，他在井旁坐下。米甸的祭司有七個女兒；她們來打水，打滿了槽，要飲父親的羣羊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the priest of Midian had seven daughters; and they came to draw water and filled the troughs to water their father’s flock."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now a priest of Midian had seven daughters, and they came to draw water and fill the troughs to water their father’s flock."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now the priest of Midian had seven daughters who came as usual to draw water and fill the water troughs for their father’s flocks."
      }
