    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若他偷的牲畜如牛、羊或驴在他手上还活着，他要偿还失主双倍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所偷之物，無論是牛、是驢，或是羊，如果發現在他手中仍然活著，他必須雙倍償還。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若他所偷的，或牛，或驢，或羊，仍在他手下存活，他就要加倍賠還。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If what he stole is actually found alive in his possession, whether an ox or a donkey or a sheep, he shall pay double."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If the stolen animal is found alive in their possession—whether ox or donkey or sheep—they must pay back double."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If someone steals an ox or a donkey or a sheep and it is found in the thief’s possession, then the thief must pay double the value of the stolen animal."
      }
    },
