    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.5.14",
            "paragraphs": [
              "14.官長，是以色列人，所以會被打。埃及人的督工，則當然不會受罰。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.5.14",
            "paragraphs": [
              "五14法老責打希伯來人的官長，是有意使他們與摩西和亞倫對抗（比較21節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们又鞭打以色列人的工头，说：“你们这两天为什么没有像从前一样完成规定的砖数？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老的督工責打他們派定的以色列人的首領，說：“你們昨天今天為甚麼沒有做完指定要你們做的磚，像往日一樣呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老督工的，責打他所派以色列人的官長，說：「你們昨天今天為甚麼沒有照向來的數目做磚、完你們的工作呢？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the foremen of the people of Israel, whom Pharaoh’s taskmasters had set over them, were beaten and were asked, “Why have you not done all your task of making bricks today and yesterday, as in the past?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, the foremen of the sons of Israel, whom Pharaoh’s taskmasters had set over them, were beaten and were asked, “Why have you not completed your required amount either yesterday or today in making brick as previously?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And Pharaoh’s slave drivers beat the Israelite overseers they had appointed, demanding, “Why haven’t you met your quota of bricks yesterday or today, as before?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Also the officers of the children of Israel, whom Pharaoh’s taskmasters had set over them, were beaten and were asked, “Why have you not fulfilled your task in making brick both yesterday and today, as before?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they whipped the Israelite foremen they had put in charge of the work crews. “Why haven’t you met your quotas either yesterday or today?” they demanded."
      }
    },
