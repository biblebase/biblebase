    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我会逐渐地赶走他们，直到你们人丁兴旺，可以占据那片土地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要漸漸地把他們從你面前驅逐出去，等到你的人數增多起來，承受那地為止。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要漸漸地將他們從你面前攆出去，等到你的人數加多，承受那地為業。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will drive them out before you little by little, until you become fruitful and take possession of the land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Little by little I will drive them out before you, until you have increased enough to take possession of the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will drive them out a little at a time until your population has increased enough to take possession of the land."
      }
