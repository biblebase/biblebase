    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.23.27",
            "paragraphs": [
              "27.我要在你面前差遣我的驚駭。這句話解釋了神為以色列制伏迦南人的方法（和合本作“我要使……在你面前驚駭”）：從神而來的恐懼會抓住他們。約書亞記二章11節便是一例，喇合描述：迦南全地的人在這群侵略者來到之時陷入全面恐慌。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你所到之处，我必使那里的人惶恐不安，使你一切的仇敌都在你面前转身逃跑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必在你面前展露我的威嚴；凡你所到的地方，我必使那裡的人民慌亂，又使你所有的仇敵都轉背而逃。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡你所到的地方，我要使那裏的眾民在你面前驚駭，擾亂，又要使你一切仇敵轉背逃跑。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I will send my terror before you and will throw into confusion all the people against whom you shall come, and I will make all your enemies turn their backs to you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will send My terror ahead of you, and throw into confusion all the people among whom you come, and I will make all your enemies turn their backs to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will send my terror ahead of you and throw into confusion every nation you encounter. I will make all your enemies turn their backs and run."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“I will send My fear before you, I will cause confusion among all the people to whom you come, and will make all your enemies turn their backs to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I will send my terror ahead of you and create panic among all the people whose lands you invade. I will make all your enemies turn and run."
      }
    },
