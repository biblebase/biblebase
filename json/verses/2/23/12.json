    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.23.12",
            "paragraphs": [
              "12.第七日要安息。命令和十誡相同，理由卻有分別。每星期安息一日不僅是為紀念神的創造大工（出二十11）或從埃及得拯救（申五15），更是讓以色列暫停工作，牲口藉此歇息，奴隸和半奴隸身分的工人亦可趁機舒暢。這些人大概沒有宗教上的責任。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "六天之内，你要工作，但第七天要休息。这样，牛、驴可以歇息，你家里的奴仆和寄居的也可以休息。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "六日之內，你要作你的工；但第七日你要安息，好使你的牛和驢可以歇息，並且使你婢女的兒子和寄居的也可以有時間喘息一下。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「六日你要做工，第七日要安息，使牛、驢可以歇息，並使你婢女的兒子和寄居的都可以舒暢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Six days you are to do your work, but on the seventh day you shall cease from labor so that your ox and your donkey may rest, and the son of your female slave, as well as your stranger, may refresh themselves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Six days do your work, but on the seventh day do not work, so that your ox and your donkey may rest, and so that the slave born in your household and the foreigner living among you may be refreshed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You have six days each week for your ordinary work, but on the seventh day you must stop working. This gives your ox and your donkey a chance to rest. It also allows your slaves and the foreigners living among you to be refreshed."
      }
    },
