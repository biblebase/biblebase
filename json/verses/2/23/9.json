      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不可壓迫寄居的，因為你們在埃及地也作過寄居的，知道寄居者的心情。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「不可欺壓寄居的；因為你們在埃及地作過寄居的，知道寄居的心。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall not oppress a stranger, since you yourselves know the feelings of a stranger, for you also were strangers in the land of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do not oppress a foreigner; you yourselves know how it feels to be foreigners, because you were foreigners in Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must not oppress foreigners. You know what it’s like to be a foreigner, for you yourselves were once foreigners in the land of Egypt."
      }
    },
