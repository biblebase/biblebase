    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“这样，他们就会相信他们祖先的上帝耶和华，就是亚伯拉罕、以撒和雅各的上帝，曾经向你显现。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“這樣就使他們相信耶和華他們祖宗的　神，就是亞伯拉罕的　神、以撒的　神、雅各的　神曾向你顯現了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "如此好叫他們信耶和華－他們祖宗的神，就是亞伯拉罕的神，以撒的神，雅各的神，是向你顯現了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“that they may believe that the Lord, the God of their fathers, the God of Abraham, the God of Isaac, and the God of Jacob, has appeared to you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This,” said the Lord, “is so that they may believe that the Lord, the God of their fathers—the God of Abraham, the God of Isaac and the God of Jacob—has appeared to you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Perform this sign,” the Lord told him. “Then they will believe that the Lord, the God of their ancestors—the God of Abraham, the God of Isaac, and the God of Jacob—really has appeared to you.”"
      }
