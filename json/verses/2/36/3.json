    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们从摩西那里领取了以色列百姓奉献出来建造圣所的一切礼物。每天早上，百姓仍然甘心乐意地来奉献礼物，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們當著摩西面前，接收了以色列人為供聖所使用的工程帶來的一切禮物。他們天天早晨還把自願奉獻的禮物帶到摩西那裡來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些人就從摩西收了以色列人為做聖所並聖所使用之工所拿來的禮物。百姓每早晨還把甘心獻的禮物拿來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They received from Moses all the contributions which the sons of Israel had brought to perform the work in the construction of the sanctuary. And they still continued bringing to him freewill offerings every morning."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They received from Moses all the offerings the Israelites had brought to carry out the work of constructing the sanctuary. And the people continued to bring freewill offerings morning after morning."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses gave them the materials donated by the people of Israel as sacred offerings for the completion of the sanctuary. But the people continued to bring additional gifts each morning."
      }
    },
