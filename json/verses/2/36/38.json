    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "用皂荚木做五根挂门帘的柱子，上面有金钩，柱顶和横杆包上金。柱子的五个底座是铜造的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又為門簾做了五根柱子和柱子上的釘子，用金子把柱頂和橫杆包裹。但那五個座是銅的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又為簾子做五根柱子和柱子上的鈎子，用金子把柱頂和柱子上的杆子包裹。柱子有五個帶卯的座，是銅的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and he made its five pillars with their hooks, and he overlaid their tops and their bands with gold; but their five sockets were of bronze."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and they made five posts with hooks for them. They overlaid the tops of the posts and their bands with gold and made their five bases of bronze."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This curtain was hung on gold hooks attached to five posts. The posts with their decorated tops and hooks were overlaid with gold, and the five bases were cast from bronze."
      }
    },
