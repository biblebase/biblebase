    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华使法老的心刚硬，法老就追赶毫无惧色地离开埃及的以色列人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華使埃及王法老的心剛硬，法老就追趕以色列人；因為以色列人是靠著耶和華高舉的手出來的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華使埃及王法老的心剛硬，他就追趕以色列人，因為以色列人是昂然無懼地出埃及。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord hardened the heart of Pharaoh, king of Egypt, and he chased after the sons of Israel as the sons of Israel were going out boldly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord hardened the heart of Pharaoh king of Egypt, so that he pursued the Israelites, who were marching out boldly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord hardened the heart of Pharaoh, the king of Egypt, so he chased after the people of Israel, who had left with fists raised in defiance."
      }
    },
