    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老硬着心，不让以色列人离开，正如耶和华借摩西所言。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老的心剛硬，不肯讓以色列人離開，就像耶和華藉著摩西所說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老的心剛硬，不容以色列人去，正如耶和華藉着摩西所說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Pharaoh’s heart was hardened, and he did not let the sons of Israel go, just as the Lord had spoken through Moses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Pharaoh’s heart was hard and he would not let the Israelites go, just as the Lord had said through Moses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because his heart was hard, Pharaoh refused to let the people leave, just as the Lord had predicted through Moses."
      }
    },
