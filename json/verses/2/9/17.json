    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.9.17",
            "paragraphs": [
              "17.自高。這個不尋常的形式只是在此出現一次。字根有“堆起攻城的土堆”（即“築壘”）之義，因此譯作“蓄意阻撓”似乎更佳。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你竟然在我的子民面前狂傲自大，不让他们离开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你仍然向我的人民自高，不讓他們離開嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你還向我的百姓自高，不容他們去嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You are still exalting yourself against my people and will not let them go."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Still you exalt yourself against My people by not letting them go."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You still set yourself against my people and will not let them go."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "As yet you exalt yourself against My people in that you will not let them go."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you still lord it over my people and refuse to let them go."
      }
