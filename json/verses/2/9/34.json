      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老看见灾祸停止，就又犯罪，他和他的臣仆都心里顽固。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老看見雨水、冰雹和雷聲都止住了，就更加犯罪，他和他的臣僕都心裡剛硬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老見雨和雹與雷止住，就越發犯罪；他和他的臣僕都硬着心。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when Pharaoh saw that the rain and the hail and the thunder had ceased, he sinned again and hardened his heart, he and his servants."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Pharaoh saw that the rain and hail and thunder had stopped, he sinned again: He and his officials hardened their hearts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when Pharaoh saw that the rain, hail, and thunder had stopped, he and his officials sinned again, and Pharaoh again became stubborn."
      }
    },
