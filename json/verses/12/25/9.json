        "text": "用火焚燒耶和華的殿和王宮，又焚燒耶路撒冷的房屋，就是各大戶家的房屋。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He burned the house of the Lord, the king’s house, and all the houses of Jerusalem; even every great house he burned with fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He set fire to the temple of the Lord, the royal palace and all the houses of Jerusalem. Every important building he burned down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He burned down the Temple of the Lord, the royal palace, and all the houses of Jerusalem. He destroyed all the important buildings in the city."
      }
