      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又在耶和華的殿內建造祭壇，耶和華曾指著這殿說：“我要立我的名在耶路撒冷。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在耶和華殿宇中築壇。耶和華曾指着這殿說：「我必立我的名在耶路撒冷。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He built altars in the house of the Lord, of which the Lord had said, “In Jerusalem I will put My name.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He built altars in the temple of the Lord, of which the Lord had said, “In Jerusalem I will put my Name.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He built pagan altars in the Temple of the Lord, the place where the Lord had said, “My name will remain in Jerusalem forever.”"
      }
