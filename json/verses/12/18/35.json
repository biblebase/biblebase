      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who of all the gods of these countries has been able to save his land from me? How then can the Lord deliver Jerusalem from my hand?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Who among all the gods of the lands have delivered their countries from my hand, that the Lord should deliver Jerusalem from my hand?’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What god of any nation has ever been able to save its people from my power? So what makes you think that the Lord can rescue Jerusalem from me?”"
      }
