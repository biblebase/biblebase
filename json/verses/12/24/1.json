    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ki.24.1",
            "paragraphs": [
              "二四1主前605年的迦基米施戰役（結束埃及的統治）之後，尼布甲尼撒進入耶路撒冷，使約雅敬作封臣，並把但以理和其它人擄到巴比倫去。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约雅敬执政期间，巴比伦王尼布甲尼撒进犯，约雅敬臣服于他，三年后又背叛了他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約雅敬在位的時候，巴比倫王尼布甲尼撒來攻打他，約雅敬就臣服他三年，後來卻反悔，背叛他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約雅敬年間，巴比倫王尼布甲尼撒上到猶大；約雅敬服事他三年，然後背叛他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In his days Nebuchadnezzar king of Babylon came up, and Jehoiakim became his servant for three years; then he turned and rebelled against him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "During Jehoiakim’s reign, Nebuchadnezzar king of Babylon invaded the land, and Jehoiakim became his vassal for three years. But then he turned against Nebuchadnezzar and rebelled."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "During Jehoiakim’s reign, King Nebuchadnezzar of Babylon invaded the land of Judah. Jehoiakim surrendered and paid him tribute for three years but then rebelled."
      }
    },
