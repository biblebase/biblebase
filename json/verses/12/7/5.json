    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "傍晚时分，他们动身去亚兰人的营地，到了营地外边，却发现人影全无。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他們在黃昏動身去到亞蘭人的軍營那裡；他們來到亞蘭人的軍營外邊，竟看不見一個人在那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "黃昏的時候，他們起來往亞蘭人的營盤去；到了營邊，不見一人在那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They arose at twilight to go to the camp of the Arameans; when they came to the outskirts of the camp of the Arameans, behold, there was no one there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At dusk they got up and went to the camp of the Arameans. When they reached the edge of the camp, no one was there,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So at twilight they set out for the camp of the Arameans. But when they came to the edge of the camp, no one was there!"
      }
    },
