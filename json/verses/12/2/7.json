      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "先知門徒中有五十人跟著他們，遠遠地站在他們對面；他們二人在約旦河河邊站著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有先知門徒去了五十人，遠遠地站在他們對面；二人在約旦河邊站住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now fifty men of the sons of the prophets went and stood opposite them at a distance, while the two of them stood by the Jordan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Fifty men from the company of the prophets went and stood at a distance, facing the place where Elijah and Elisha had stopped at the Jordan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fifty men from the group of prophets also went and watched from a distance as Elijah and Elisha stopped beside the Jordan River."
      }
