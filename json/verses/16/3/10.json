    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "接下来，哈路抹的儿子耶大雅修筑他房子对面的那一段。 再下一段由哈沙尼的儿子哈突修筑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "再接著的一段由哈路抹的兒子耶大雅修築，就是他自己房屋對面的那段。接著這一段的是由哈沙尼的兒子哈突修築。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其次是哈路抹的兒子耶大雅對着自己的房屋修造。其次是哈沙尼的兒子哈突修造。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Next to them Jedaiah the son of Harumaph made repairs opposite his house. And next to him Hattush the son of Hashabneiah made repairs."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Adjoining this, Jedaiah son of Harumaph made repairs opposite his house, and Hattush son of Hashabneiah made repairs next to him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Next Jedaiah son of Harumaph repaired the wall across from his own house, and next to him was Hattush son of Hashabneiah."
      }
    },
