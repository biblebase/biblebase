      "dde": {
        "content": [
          {
            "verse": "neh.3.21-22",
            "paragraphs": [
              "21～22.米利末像其他幾個人或小組一樣（例：5、27節中的提哥亞人），不只修造一段城墻。關於他的身分與重要性，見4節的註解。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "再下一段由哈哥斯的孙子、乌利亚的儿子米利末修筑，他从以利亚实家门口修到以利亚实家的尽头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "再下一段，就是哈哥斯的孫子、烏利亞的兒子米利末修築的另一段，從以利亞實住宅的門口，直到以利亞實住宅的盡頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其次是哈哥斯的孫子、烏利亞的兒子米利末修造一段，從以利亞實的府門，直到以利亞實府的盡頭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After him Meremoth the son of Uriah the son of Hakkoz repaired another section, from the doorway of Eliashib’s house even as far as the end of his house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Next to him, Meremoth son of Uriah, the son of Hakkoz, repaired another section, from the entrance of Eliashib’s house to the end of it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Meremoth son of Uriah and grandson of Hakkoz rebuilt another section of the wall extending from the door of Eliashib’s house to the end of the house."
      }
