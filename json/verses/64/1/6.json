      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把你的爱心告诉了教会。愿你照着上帝所喜悦的方式继续帮助他们，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在教會面前證實了你的愛；你照著　神所喜悅的，資助他們的旅程，這樣是好的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們在教會面前證明了你的愛；你若配得過神，幫助他們往前行，這就好了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they have testified to your love before the church. You will do well to send them on their way in a manner worthy of God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They have told the church about your love. Please send them on their way in a manner that honors God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They have told the church here of your loving friendship. Please continue providing for such teachers in a manner that pleases God."
      }
