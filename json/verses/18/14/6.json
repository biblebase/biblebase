    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.14.1-6",
            "paragraphs": [
              "1～6.約伯一再地停駐於人類存在的不幸與短暫上，“人的生命是孤單的、貧窮的、險惡的、殘酷的、與短暫的”273。我們已經註意過約伯的思想在這些點上的沖突。生命的重擔如此難以忍受，以致迅速的死亡是最值得羨慕的事；然而，約伯非但不因生命的迅速而歡欣，反倒從其短暫看出它終極的嘲弄意味。這種似非而是的想法將日子短少與多有患難這些片語並列。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你转身，别去管他， 好让他如雇工度完他的日子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就求你轉眼不看他，使他得歇息， 直等到他像雇工一樣享受他的日子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "便求你轉眼不看他，使他得歇息， 直等他像雇工人完畢他的日子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Turn Your gaze from him that he may rest, Until he fulfills his day like a hired man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So look away from him and let him alone, till he has put in his time like a hired laborer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So leave us alone and let us rest! We are like hired hands, so let us finish our work in peace."
      }
