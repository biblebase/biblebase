    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.32.19-20",
            "paragraphs": [
              "19～20.以利戶以煞費心機之想像的幫助，來衡量他想要說話之不可抵擋的沖動。盛酒之囊這個字只有出現在這裏，所以其意義取決於上下文，沒有人提出具說服力的其他解釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我里面像盛满了酒，却没有出口， 又如将要破裂的新酒袋。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我裡面如盛酒的囊，沒有出氣的地方， 又如新酒袋，快要破裂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的胸懷如盛酒之囊沒有出氣之縫， 又如新皮袋快要破裂。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Behold, my belly is like wine that has no vent; like new wineskins ready to burst."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, my belly is like unvented wine, Like new wineskins it is about to burst."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "inside I am like bottled-up wine, like new wineskins ready to burst."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Indeed my belly is like wine that has no vent; It is ready to burst like new wineskins."
      },
      "nlt": {
        "version_id": "NLT",
