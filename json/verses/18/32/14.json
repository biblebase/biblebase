    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.32.14",
            "paragraphs": [
              "14.連接之處並不清楚，除非以利戶的意義是：到目前為止，約伯還沒有把他包括在討論中，但如果他參與其中，他當然不會照著朋友們的方法了；諷刺的是：他的立場與他們的太相同了。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约伯没有与我辩论， 我也不会用你们的理论答复他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約伯沒有用他的話攻擊我， 我也不用你們的話回答他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約伯沒有向我爭辯； 我也不用你們的話回答他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He has not directed his words against me, and I will not answer him with your speeches."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For he has not arranged his words against me, Nor will I reply to him with your arguments."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Job has not marshaled his words against me, and I will not answer him with your arguments."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now he has not directed his words against me; So I will not answer him with your words."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If Job had been arguing with me, I would not answer with your kind of logic!"
      }
