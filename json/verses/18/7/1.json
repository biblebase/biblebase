    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "job.7.1",
            "paragraphs": [
              "7:1",
              "人在世上：約伯的三友人似乎無法理解他，於是他不再與他們分訴，轉向神禱告。約伯在一切談論中最註重的是神。即使在他以第三人稱稱呼神時，他也總是知道神的同在。約伯的心從未轉離他所愛的神。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "job.7.1-6",
            "paragraphs": [
              "七1～6約伯把他的存在比作（1）服兵役（1節），（2）\"雇工人\"（1,2節）和（3）\"奴仆\"（2節）那樣艱苦勞累。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“人生在世岂不像服劳役吗？ 他有生之年岂不像个雇工吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人在世上怎能沒有勞役呢？ 他的日子不像雇工的日子嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人在世上豈無爭戰嗎？ 他的日子不像雇工人的日子嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Is not man forced to labor on earth, And are not his days like the days of a hired man?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do not mortals have hard service on earth? Are not their days like those of hired laborers?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Is not all human life a struggle? Our lives are like that of a hired hand,"
      }
    },
