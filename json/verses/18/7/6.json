    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.7.6",
            "paragraphs": [
              "6.關於人生苦短的另一個比喻，請見十四1～2。這裏將之比擬成\\cs9梭，曾經受到質疑，因為這個字很少見，意義也不清楚。有許多變通的方法曾經被提出來，平行體支持將下半節的無指望解釋成從織梭之例證的另一部分，所以新英語聖經譯作“我的日子……到了盡頭，有如斷線”，這個提議早在伊斯拉（IbnEzra）時代就已出現。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.7.1-6",
            "paragraphs": [
              "七1～6約伯把他的存在比作（1）服兵役（1節），（2）\"雇工人\"（1,2節）和（3）\"奴仆\"（2節）那樣艱苦勞累。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的年日飞逝，比梭还快， 转眼结束，毫无盼望。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我過的日子比梭還要快， 在毫無盼望之中而結束。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的日子比梭更快， 都消耗在無指望之中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My days are swifter than a weaver’s shuttle, And come to an end without hope."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“My days are swifter than a weaver’s shuttle, and they come to an end without hope."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“My days fly faster than a weaver’s shuttle. They end without hope."
      }
    },
