    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.24.2-24",
            "paragraphs": [
              "二四2～24約伯描述世上邪惡的猖獗，而那情況顯然是神所容許的。"
            ]
          },
          {
            "verse": "job.24.2",
            "paragraphs": [
              "二四2\"挪移地界\"去偷取別人的產業。參看申命記十九章14節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人挪移界石， 抢夺羊群去牧养。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人挪移地界， 搶奪群畜去牧養。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人挪移地界， 搶奪羣畜而牧養。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Some remove the landmarks; They seize and devour flocks."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There are those who move boundary stones; they pasture flocks they have stolen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Evil people steal land by moving the boundary markers. They steal livestock and put them in their own pastures."
      }
    },
