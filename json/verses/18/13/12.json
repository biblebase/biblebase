    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.13.4-12",
            "paragraphs": [
              "一三4～12約伯嚴厲地批評友人的動機。箴言十七章28節那嘲諷的話跟這裏第5節相同。約伯提醒他們（9節），他們也面對一位監察人心的神。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的名言是无用的灰尘， 你们的雄辩是土筑的营垒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們背誦的格言都是爐灰的格言， 你們的辯護都是泥土的辯護。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們以為可記念的箴言是爐灰的箴言； 你們以為可靠的堅壘是淤泥的堅壘。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Your maxims are proverbs of ashes; your defenses are defenses of clay."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your memorable sayings are proverbs of ashes, Your defenses are defenses of clay."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your maxims are proverbs of ashes; your defenses are defenses of clay."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Your platitudes are proverbs of ashes, Your defenses are defenses of clay."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your platitudes are as valuable as ashes. Your defense is as fragile as a clay pot."
      }
    },
