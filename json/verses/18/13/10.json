    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.13.10",
            "paragraphs": [
              "10.責備這個字，是第3節譯作“案情”（呂譯）、第3節譯作“辯論”之字根的另一個譯法。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.13.4-12",
            "paragraphs": [
              "一三4～12約伯嚴厲地批評友人的動機。箴言十七章28節那嘲諷的話跟這裏第5節相同。約伯提醒他們（9節），他們也面對一位監察人心的神。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们若暗中偏袒， 祂必责备你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們若暗中徇情面， 他必然責備你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若暗中徇情， 他必要責備你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will surely reprove you If you secretly show partiality."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He would surely call you to account if you secretly showed partiality."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No, you will be in trouble with him if you secretly slant your testimony in his favor."
      }
