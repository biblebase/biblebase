    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.13.4-12",
            "paragraphs": [
              "一三4～12約伯嚴厲地批評友人的動機。箴言十七章28節那嘲諷的話跟這裏第5節相同。約伯提醒他們（9節），他們也面對一位監察人心的神。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要为上帝说谎， 为祂说诡诈的话吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要為　神說不義的言語嗎？ 你們要為他說詭詐的話嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要為神說不義的話嗎？ 為他說詭詐的言語嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Will you speak falsely for God and speak deceitfully for him?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will you speak what is unjust for God, And speak what is deceitful for Him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Will you speak wickedly on God’s behalf? Will you speak deceitfully for him?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Will you speak wickedly for God, And talk deceitfully for Him?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Are you defending God with lies? Do you make your dishonest arguments for his sake?"
      }
