    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.13.4-12",
            "paragraphs": [
              "一三4～12約伯嚴厲地批評友人的動機。箴言十七章28節那嘲諷的話跟這裏第5節相同。約伯提醒他們（9節），他們也面對一位監察人心的神。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂查问你们时岂会有好结果？ 你们岂能像欺骗人一样欺骗祂？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把你們查出來，這是好嗎？ 人怎樣哄騙人，你們也怎樣哄騙　神嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他查出你們來，這豈是好嗎？ 人欺哄人，你們也要照樣欺哄他嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will it be well when He examines you? Or will you deceive Him as one deceives a man?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Would it turn out well if he examined you? Could you deceive him as you might deceive a mortal?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What will happen when he finds out what you are doing? Can you fool him as easily as you fool people?"
      }
    },
