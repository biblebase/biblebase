    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "job.39.2",
            "paragraphs": [
              "39:2",
              "你能數算嗎：神若能引導約伯認識到人是何等有限，以至於無法明白神在天地間的作為，那麽他也就能使約伯相信，即使當他無法明白神在他生命中的作為時，神仍是大有公義，滿心憐憫的神。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你能算出它们怀胎的月数吗？ 你知道它们分娩的日期吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠們懷胎的月數你能計算嗎？ 牠們生產的日期你能曉得嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牠們懷胎的月數，你能數算嗎？ 牠們幾時生產，你能曉得嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Can you count the months they fulfill, Or do you know the time they give birth?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do you count the months till they bear? Do you know the time they give birth?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do you know how many months they carry their young? Are you aware of the time of their delivery?"
      }
    },
