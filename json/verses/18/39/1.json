    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "job.39.1",
            "paragraphs": [
              "39:1",
              "你知道嗎：神繼續以約伯無法回答的問題反問他。這樣，神讓約伯明白他想與神爭辯實在無知。約伯終於謙卑下來，不再言語，但他卻對那最重要的事有了確據——神沒有撇棄他。主仍在這裏，與他面對面。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你知道野山羊何时生产吗？ 你看过母鹿产仔吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“山巖間的野山羊的產期你能曉得嗎？ 母鹿下犢之期你能察出嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "山巖間的野山羊幾時生產，你知道嗎？ 母鹿下犢之期，你能察定嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Do you know the time the mountain goats give birth? Do you observe the calving of the deer?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do you know when the mountain goats give birth? Do you watch when the doe bears her fawn?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do you know when the wild goats give birth? Have you watched as deer are born in the wild?"
      }
    },
