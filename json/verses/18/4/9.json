    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.4.9-11",
            "paragraphs": [
              "9～11.以利法的論證在實質上所缺少的，他以修詞學來補足了。他以一個精心設計的圖畫來證明他的論點155。攫食的野獸可以暫時不受抑制地驚嚇世界，但神發怒的氣輕而易舉地就把它們消滅了。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.4.7-9",
            "paragraphs": [
              "四7～9以利法斷言滅亡的是惡人，而不是無辜的人。但事實不一定如此。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们被上帝的气息所毁， 被上帝的怒气所灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們因　神的氣息而滅亡， 因他的怒氣而消滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神一出氣，他們就滅亡； 神一發怒，他們就消沒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By the breath of God they perish, And by the blast of His anger they come to an end."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At the breath of God they perish; at the blast of his anger they are no more."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A breath from God destroys them. They vanish in a blast of his anger."
      }
