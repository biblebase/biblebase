    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.4.3-4",
            "paragraphs": [
              "3～4.以利法以令人喜悅的贊詞開始，稱贊約伯的名望，尤其是他幫助在困難中人的能力150。教導是“教育”的專門術語，在智慧圈中用來指藉著操練所作的生活訓練。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的话使人免于跌倒， 你使颤抖的膝硬朗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的話扶起了絆倒的人， 你使彎曲乏力的膝堅挺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的言語曾扶助那將要跌倒的人； 你又使軟弱的膝穩固。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Your words have upheld him who was stumbling, and you have made firm the feeble knees."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your words have helped the tottering to stand, And you have strengthened feeble knees."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your words have supported those who stumbled; you have strengthened faltering knees."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Your words have upheld him who was stumbling, And you have strengthened the feeble knees;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your words have supported those who were falling; you encouraged those with shaky knees."
      }
