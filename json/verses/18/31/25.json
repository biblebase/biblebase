    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.31.24-25",
            "paragraphs": [
              "24～25.貪婪在廿二24～25，以利法曾勸約伯以神為他的寶藏；他在這裏否認將任何信心放在他的財富上。這個條件子句（否認）沒有帶結果子句（咒詛），完整的格式是沒有必要的。但避免貪心可能與拜偶像（26～27節）有關，與第28節作為共通的罪。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因财富充裕、 或家产丰足而欢喜；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若因為財物豐裕， 或因為我多獲財利而歡喜；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若因財物豐裕， 因我手多得資財而歡喜；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I have gloated because my wealth was great, And because my hand had secured so much;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "if I have rejoiced over my great wealth, the fortune my hands had gained,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Have I gloated about my wealth and all that I own?"
      }
    },
