    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.6.26",
            "paragraphs": [
              "26.約伯在此似乎是要說：他的話──絕望人的講論，這是他在第3節已經憤怒地證明的──被他們當作是耳邊風一樣（直譯：“給風吹的”，見呂譯；另參思高）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.6.14-30",
            "paragraphs": [
              "六14～30約伯說出他因友人的不仁而受到深切的傷害，但對於他們的論點，他卻不為所動。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们既视绝望者的话如风， 还要来纠正吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "絕望人的話不過是風， 你們還想批判嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "絕望人的講論既然如風， 你們還想要駁正言語嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Do you think that you can reprove words, when the speech of a despairing man is wind?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do you intend to reprove my words, When the words of one in despair belong to the wind?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do you mean to correct what I say, and treat my desperate words as wind?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Do you intend to rebuke my words, And the speeches of a desperate one, which  are as wind?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do you think your words are convincing when you disregard my cry of desperation?"
      }
    },
