    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.41.30",
            "paragraphs": [
              "四一30鱷魚的鱗片在泥上留下印記，如釘著經過一樣。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它腹部有锋利的瓦片， 在淤泥上留下道道耙痕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠的腹下像銳利的瓦片， 牠在泥上碾過如同有利釘的耙犁田一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牠肚腹下如尖瓦片； 牠如釘耙經過淤泥。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His underparts are like sharp potsherds; He spreads out like a threshing sledge on the mire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Its undersides are jagged potsherds, leaving a trail in the mud like a threshing sledge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Its belly is covered with scales as sharp as glass. It plows up the ground as it drags through the mud."
      }
