        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠打的噴嚏閃出光來， 牠的眼睛如同清晨的陽光；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牠打噴嚏就發出光來； 牠眼睛好像早晨的光線。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His sneezes flash forth light, And his eyes are like the eyelids of the morning."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Its snorting throws out flashes of light; its eyes are like the rays of dawn."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When it sneezes, it flashes light! Its eyes are like the red of dawn."
      }
