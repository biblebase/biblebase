    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "再凶猛的人也不敢惹它。 这样，谁能在我面前站立得住？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人惹牠的時候，牠不是很兇猛嗎？ 這樣，有誰人在我面前能站立得住呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "沒有那麼兇猛的人敢惹牠。 這樣，誰能在我面前站立得住呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No one is so fierce that he dares to arouse him; Who then is he that can stand before Me?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one is fierce enough to rouse it. Who then is able to stand against me?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And since no one dares to disturb it, who then can stand up to me?"
      }
    },
