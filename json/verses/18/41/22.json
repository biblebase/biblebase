    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.41.12-25",
            "paragraphs": [
              "四一12～25這段經文描述鱷魚令人望而生畏的身體結構。"
            ]
          },
          {
            "verse": "job.41.22",
            "paragraphs": [
              "四一22\"恐嚇蹦跳\"。生動地描述其它動物包括人，在鱷魚面前的恐懼。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "颈项强而有力， 恐惧在它前面开路；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠的頸項上存著能力， 驚恐在牠面前跳躍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牠頸項中存着勁力； 在牠面前的都恐嚇蹦跳。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In his neck lodges strength, And dismay leaps before him."
      },
