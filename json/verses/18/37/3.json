        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂使闪电划过整个天空， 亮光直照到地极。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他發雷聲響遍天下， 也發電光閃到地極。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他發響聲震遍天下， 發電光閃到地極。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Under the whole heaven he lets it go, and his lightning to the corners of the earth."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Under the whole heaven He lets it loose, And His lightning to the ends of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He unleashes his lightning beneath the whole heaven and sends it to the ends of the earth."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He sends it forth under the whole heaven, His lightning to the ends of the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It rolls across the heavens, and his lightning flashes in every direction."
      }
    },
