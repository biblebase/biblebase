    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你还说，‘这与我何干？ 我不犯罪又有何益处？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你還說：‘你有甚麼益處呢？ 我不犯罪有甚麼好處？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "才說這與我有甚麼益處？ 我不犯罪比犯罪有甚麼好處呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "that you ask, ‘What advantage have I? How am I better off than if I had sinned?’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For you say, ‘What advantage will it be to You? What profit will I have, more than if I had sinned?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet you ask him, ‘What profit is it to me, and what do I gain by not sinning?’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For you say, ‘What advantage will it be to You? What profit shall I have, more than if I had sinned?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you also ask, ‘What’s in it for me? What’s the use of living a righteous life?’"
      }
    },
