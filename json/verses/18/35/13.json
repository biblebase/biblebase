    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.35.9-16",
            "paragraphs": [
              "三五9～16以利戶說明他的信念，那就是禱告得不回應--即使是義人的禱告--是因為人缺乏信心（10節），並因為禱告的虛妄（13節；比較雅四3）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝必不垂听虚妄的呼求， 全能者必不理会。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "真的，　神必不垂聽虛妄的呼求， 全能者也必不留意，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "虛妄的呼求，神必不垂聽； 全能者也必不眷顧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Surely God will not listen to an empty cry, Nor will the Almighty regard it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Indeed, God does not listen to their empty plea; the Almighty pays no attention to it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But it is wrong to say God doesn’t listen, to say the Almighty isn’t concerned."
      }
