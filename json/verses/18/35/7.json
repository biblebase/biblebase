      "leili": {
        "content": [
          {
            "verse": "job.35.5-8",
            "paragraphs": [
              "三五5～8以利戶斷言神是高超的，祂不會受到人的虔誠或不虔誠的影響；這些態度只會影響其它人（8節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你若为人公义，与祂何益？ 祂从你手上能得什么好处？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你若為人正義，你能給他甚麼呢？ 他從你手裡能領受甚麼呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你若是公義，還能加增他甚麼呢？ 他從你手裏還接受甚麼呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you are righteous, what do you give to Him, Or what does He receive from your hand?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you are righteous, what do you give to him, or what does he receive from your hand?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you are good, is this some great gift to him? What could you possibly give him?"
      }
    },
