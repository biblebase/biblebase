    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若论力量，祂甚强大； 若上公堂，谁敢传祂？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "若論力量，他多麼強大， 若論訴訟，他說：‘誰能把我傳來？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若論力量，他真有能力！ 若論審判，他說誰能將我傳來呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If it is a matter of power, behold, He is the strong one! And if it is a matter of justice, who can summon Him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If it is a matter of strength, he is mighty! And if it is a matter of justice, who can challenge him?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If it’s a question of strength, he’s the strong one. If it’s a matter of justice, who dares to summon him to court?"
      }
    },
