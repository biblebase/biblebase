    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.9.14",
            "paragraphs": [
              "14.約伯希望能夠對神說話；但是要做到這一點，他就必須對神的作為之目的有點概念。稍後（十9～13），他將會堅稱在創造中一定有著某種目的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“因此，我怎敢与祂辩驳？ 怎敢措辞与祂理论？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "何況我呢？我怎敢回答他， 措辭與他辯論呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "既是這樣，我怎敢回答他， 怎敢選擇言語與他辯論呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How then can I answer Him, And choose my words before Him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“How then can I dispute with him? How can I find words to argue with him?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“So who am I, that I should try to answer God or even reason with him?"
      }
    },
