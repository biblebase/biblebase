    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.9.12",
            "paragraphs": [
              "12.從出現相同問句的地方看來，問題的癥結不是“你做什麽？”而是“為什麽？”。前面的獨唱使人明白約伯相當清楚神做什麽，第24節將此撮要起來，徹底地斷言神是每一件事唯一的肇因。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.9.3-12",
            "paragraphs": [
              "九3～12面對神偉大的智慧與能力，喬布看到自己的不足。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂若夺取，谁能阻挡？ 谁敢问祂，‘你做什么？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他奪取，誰能攔阻他？ 誰敢問他：‘你幹甚麼？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他奪取，誰能阻擋？ 誰敢問他：你做甚麼？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Were He to snatch away, who could restrain Him? Who could say to Him, ‘What are You doing?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If he snatches away, who can stop him? Who can say to him, ‘What are you doing?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If he snatches someone in death, who can stop him? Who dares to ask, ‘What are you doing?’"
      }
    },
