        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "灾祸突然夺走人命时， 祂嘲笑无辜者的遭遇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "災禍忽然把人殺害的時候， 他就必嘲笑無辜人的遭遇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若忽然遭殺害之禍， 他必戲笑無辜的人遇難。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If the scourge kills suddenly, He mocks the despair of the innocent."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When a scourge brings sudden death, he mocks the despair of the innocent."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When a plague sweeps through, he laughs at the death of the innocent."
      }
    },
