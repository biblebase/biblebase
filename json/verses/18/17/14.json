    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.17.11-16",
            "paragraphs": [
              "11～16.第11節接續十六16與十七7的思想。他患病的身體正在死亡的邊緣。整潔的語文──三行，每行兩個字──跟第1節一樣，觀念也很接近。新英語聖經得了七十士譯本的支持，成功地將它改成兩行，每行三個字。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.17.13-14",
            "paragraphs": [
              "一七13,14約伯再次表達他對死的渴望。在墳墓裏，\"蟲\"與人是一家人！"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "称坟墓为父亲， 称蛆虫为母亲、姊妹，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若對深坑說：‘你是我的父親’， 對蟲說：‘你是我的母親，我的姊妹’，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若對朽壞說：你是我的父； 對蟲說：你是我的母親姊妹；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I call to the pit, ‘You are my father’; To the worm, ‘my mother and my sister’;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "if I say to corruption, ‘You are my father,’ and to the worm, ‘My mother’ or ‘My sister,’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What if I call the grave my father, and the maggot my mother or my sister?"
      }
    },
