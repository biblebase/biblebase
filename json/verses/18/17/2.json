      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无疑，嘲笑的人围着我， 我眼睁睁地看着他们戏弄我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "真有嘲笑人的在我這裡， 我的眼看著他們的悖逆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "真有戲笑我的在我這裏， 我眼常見他們惹動我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Surely mockers are with me, And my eye gazes on their provocation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely mockers surround me; my eyes must dwell on their hostility."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am surrounded by mockers. I watch how bitterly they taunt me."
      }
