    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.17.11-16",
            "paragraphs": [
              "11～16.第11節接續十六16與十七7的思想。他患病的身體正在死亡的邊緣。整潔的語文──三行，每行兩個字──跟第1節一樣，觀念也很接近。新英語聖經得了七十士譯本的支持，成功地將它改成兩行，每行三個字。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的年日已尽， 我的计划破灭， 我的心愿落空。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的日子已過， 我的謀算和我心中所想的都已粉碎。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的日子已經過了； 我的謀算、我心所想望的已經斷絕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My days are past, my plans are torn apart, Even the wishes of my heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My days have passed, my plans are shattered. Yet the desires of my heart"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My days are over. My hopes have disappeared. My heart’s desires are broken."
      }
    },
