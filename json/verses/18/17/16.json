    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.17.11-16",
            "paragraphs": [
              "11～16.第11節接續十六16與十七7的思想。他患病的身體正在死亡的邊緣。整潔的語文──三行，每行兩個字──跟第1節一樣，觀念也很接近。新英語聖經得了七十士譯本的支持，成功地將它改成兩行，每行三個字。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.17.16",
            "paragraphs": [
              "十七16\"陰間\"。參看創世記三十七章35節的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的希望会跟我进入阴间， 与我同归尘土吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "等到安息在塵土中的時候， 那些指望必下到陰間的門閂那裡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "等到安息在塵土中， 這指望必下到陰間的門閂那裏了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will it go down with me to Sheol? Shall we together go down into the dust?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Will it go down to the gates of death? Will we descend together into the dust?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No, my hope will go down with me to the grave. We will rest together in the dust!”"
      }
    },
