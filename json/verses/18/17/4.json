    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.17.3-5",
            "paragraphs": [
              "一七3～5約伯求神親自拿憑據（擔保）保證他的無辜。\"擊掌\"。作擔保的一種方法。"
            ]
          },
          {
            "verse": "job.17.4",
            "paragraphs": [
              "一七4\"他們〔的〕心\"。其指控者的心。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你蒙蔽了他们的心智， 因此你必不让他们得胜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你蒙蔽了他們的心，不讓他們明白； 因此你必不高舉他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因你使他們心不明理， 所以你必不高舉他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You have kept their heart from understanding, Therefore You will not exalt them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have closed their minds to understanding; therefore you will not let them triumph."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have closed their minds to understanding, but do not let them triumph."
      }
    },
