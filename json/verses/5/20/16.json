    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的上帝耶和华将把你们附近各族的城邑赐给你们，你们要将里面有气息的全部消灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只是在耶和華你的　神賜給你作產業的這些國民的城市裡，凡有氣息的，連一個你也不可讓他活著；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但這些國民的城，耶和華－你神既賜你為業，其中凡有氣息的，一個不可存留；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Only in the cities of these peoples that the Lord your God is giving you as an inheritance, you shall not leave alive anything that breathes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "However, in the cities of the nations the Lord your God is giving you as an inheritance, do not leave alive anything that breathes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In those towns that the Lord your God is giving you as a special possession, destroy every living thing."
      }
    },
