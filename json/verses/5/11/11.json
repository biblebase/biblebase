    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "而你们将要占领的那片土地却有山有谷，雨水充足，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但你們要過去得為業的地，卻是一個有山有谷的地，有天上雨水滋潤的地，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要過去得為業的那地乃是有山有谷、雨水滋潤之地，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the land into which you are about to cross to possess it, a land of hills and valleys, drinks water from the rain of heaven,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the land you are crossing the Jordan to take possession of is a land of mountains and valleys that drinks rain from heaven."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rather, the land you will soon take over is a land of hills and valleys with plenty of rain—"
      }
