    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "长久居住在耶和华起誓赐给你们祖先及其后裔的奶蜜之乡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又使你們在耶和華向你們的列祖起誓應許賜給他們和他們後裔的地上，就是流奶與蜜的地，可以長久居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並使你們的日子在耶和華向你們列祖起誓、應許給他們和他們後裔的地上得以長久；那是流奶與蜜之地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that you may prolong your days on the land which the Lord swore to your fathers to give to them and to their descendants, a land flowing with milk and honey."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and so that you may live long in the land the Lord swore to your ancestors to give to them and their descendants, a land flowing with milk and honey."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you obey, you will enjoy a long life in the land the Lord swore to give to your ancestors and to you, their descendants—a land flowing with milk and honey!"
      }
    },
