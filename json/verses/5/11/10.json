    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那片土地不像你们离开的埃及。在埃及你们撒种后，要像浇菜园一样辛勤灌溉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你要進去得的那地，並不像你出來的埃及地；在那裡你撒了種，還要用腳灌溉，像菜園一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要進去得為業的那地，本不像你出來的埃及地。你在那裏撒種，用腳澆灌，像澆灌菜園一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the land, into which you are entering to possess it, is not like the land of Egypt from which you came, where you used to sow your seed and water it with your foot like a vegetable garden."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The land you are entering to take over is not like the land of Egypt, from which you have come, where you planted your seed and irrigated it by foot as in a vegetable garden."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the land you are about to enter and take over is not like the land of Egypt from which you came, where you planted your seed and made irrigation ditches with your foot as in a vegetable garden."
      }
    },
