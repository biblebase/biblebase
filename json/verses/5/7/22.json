    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的上帝耶和华将在你们前面渐渐地赶走那些民族，你们不可急速地灭尽他们，否则你们周围的野兽会大增，危害你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華你的　神必把這些國的民，從你面前漸漸趕出去；你不可以把他們迅速地滅盡，恐怕田間的野獸多起來害你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－你神必將這些國的民從你面前漸漸趕出；你不可把他們速速滅盡，恐怕野地的獸多起來害你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord your God will clear away these nations before you little by little; you will not be able to put an end to them quickly, for the wild beasts would grow too numerous for you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord your God will drive out those nations before you, little by little. You will not be allowed to eliminate them all at once, or the wild animals will multiply around you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord your God will drive those nations out ahead of you little by little. You will not clear them away all at once, otherwise the wild animals would multiply too quickly for you."
      }
    },
