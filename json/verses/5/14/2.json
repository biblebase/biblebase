    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你们是属于你们的上帝耶和华的圣洁子民，祂从天下万族中拣选你们作祂宝贵的产业。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你是屬於耶和華你的　神的聖潔子民；耶和華從地上的萬民中揀選了你們，特作他自己的子民。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你歸耶和華－你神為聖潔的民，耶和華從地上的萬民中揀選你特作自己的子民。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For you are a holy people to the Lord your God, and the Lord has chosen you to be a people for His own possession out of all the peoples who are on the face of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for you are a people holy to the Lord your God. Out of all the peoples on the face of the earth, the Lord has chosen you to be his treasured possession."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have been set apart as holy to the Lord your God, and he has chosen you from all the nations of the earth to be his own special treasure."
      }
    },
