    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们是你们上帝耶和华的儿女，不可为死人割伤自己或剃光前额。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們是耶和華你們的　神的兒女，不可為了死人割傷自己的身體，也不可使你們的額上光禿；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們是耶和華－你們神的兒女。不可為死人用刀劃身，也不可將額上剃光；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You are the sons of the Lord your God; you shall not cut yourselves nor shave your forehead for the sake of the dead."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are the children of the Lord your God. Do not cut yourselves or shave the front of your heads for the dead,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Since you are the people of the Lord your God, never cut yourselves or shave the hair above your foreheads in mourning for the dead."
      }
    },
