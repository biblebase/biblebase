    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "deu.24.5",
            "paragraphs": [
              "5.免服兵役。這條律法屬於聖戰律法的一類。這樣的律法和現代國家似乎格格不入，但在申命記中以色列人保證有後，遠比能夠參加聖戰重要。這人可以有一年休假來陪伴妻子（參二十7），以確保她的幸福，並開始新的家庭327。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“不可叫新婚的人从军出征或办理公务。他可以留在家里一年，使新婚的妻子快乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果人娶了新婦，不可從軍出征，也不可派他擔任甚麼公務，他可以在家賦閒一年，使他所娶的妻子快樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「新娶妻之人不可從軍出征，也不可託他辦理甚麼公事，可以在家清閒一年，使他所娶的妻快活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“When a man takes a new wife, he shall not go out with the army nor be charged with any duty; he shall be free at home one year and shall give happiness to his wife whom he has taken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If a man has recently married, he must not be sent to war or have any other duty laid on him. For one year he is to be free to stay at home and bring happiness to the wife he has married."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“A newly married man must not be drafted into the army or be given any other official responsibilities. He must be free to spend one year at home, bringing happiness to the wife he has married."
      }
    },
