    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们犹如来自所多玛、 长在蛾摩拉的葡萄树， 结的葡萄粒粒有毒，串串苦涩；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們的葡萄樹，是所多瑪的葡萄樹所出的， 是蛾摩拉的田地所生的； 他們的葡萄是毒葡萄， 每一掛都是苦的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的葡萄樹是所多瑪的葡萄樹， 蛾摩拉田園所生的； 他們的葡萄是毒葡萄， 全掛都是苦的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For their vine is from the vine of Sodom, And from the fields of Gomorrah; Their grapes are grapes of poison, Their clusters, bitter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their vine comes from the vine of Sodom and from the fields of Gomorrah. Their grapes are filled with poison, and their clusters with bitterness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their vine grows from the vine of Sodom, from the vineyards of Gomorrah. Their grapes are poison, and their clusters are bitter."
      }
