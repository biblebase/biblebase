    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "deu.32.19-25",
            "paragraphs": [
              "19～25.判決在此宣告。本段談論臨到背約者身上的咒詛。耶和華看見以色列的悖逆（19節），便決定向她掩面──即止住恩寵，降下懲罰。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_sermon-2012-08-26",
        "title": "基督徒常犯的錯誤",
        "date": "2012-08-26",
        "preacher": "王文堂牧師",
        "verses": "申命記 32:1-52",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-08-26.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-08-26.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们必饱受饥荒之苦， 被瘟疫和灾祸吞噬， 被猛兽和毒蛇袭击。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們必因饑荒消瘦，被熱病和毒症消滅； 我也要打發野獸用牙齒咬他們， 和土中的蛇類用毒液害他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必因飢餓消瘦， 被炎熱苦毒吞滅。 我要打發野獸用牙齒咬他們， 並土中腹行的，用毒氣害他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will be wasted by famine, and consumed by plague And bitter destruction; And the teeth of beasts I will send upon them, With the venom of crawling things of the dust."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will send wasting famine against them, consuming pestilence and deadly plague; I will send against them the fangs of wild beasts, the venom of vipers that glide in the dust."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will weaken them with famine, burning fever, and deadly disease. I will send the fangs of wild beasts and poisonous snakes that glide in the dust."
      }
    },
