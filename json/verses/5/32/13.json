    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华使他们驰骋在大地的高处， 吃田间的五谷， 磐石中的蜂蜜， 石头地产的油；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華使他乘駕地的高處， 得吃田間的土產； 又使他從巖石中吸蜜， 從堅固的磐石中取油。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華使他乘駕地的高處， 得吃田間的土產； 又使他從磐石中咂蜜， 從堅石中吸油；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He made him ride on the high places of the earth, And he ate the produce of the field; And He made him suck honey from the rock, And oil from the flinty rock,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He made him ride on the heights of the land and fed him with the fruit of the fields. He nourished him with honey from the rock, and with oil from the flinty crag,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He let them ride over the highlands and feast on the crops of the fields. He nourished them with honey from the rock and olive oil from the stony ground."
      }
    },
