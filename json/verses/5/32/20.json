    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说，‘这堕落的子民， 不忠的儿女！ 我要掩面不理他们， 看他们的结局如何。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說：‘我必掩面不顧他們， 要看看他們的結局怎樣； 因為他們是乖曲的一代， 心裡沒有信實的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要向他們掩面， 看他們的結局如何。 他們本是極乖僻的族類， 心中無誠實的兒女。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He said, ‘I will hide My face from them, I will see what their end shall be; For they are a perverse generation, Sons in whom is no faithfulness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will hide my face from them,” he said, “and see what their end will be; for they are a perverse generation, children who are unfaithful."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He said, ‘I will abandon them; then see what becomes of them. For they are a twisted generation, children without integrity."
      }
    },
