    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚昧无知的人啊， 你们就这样报答耶和华吗？ 祂是你们的父，你们的创造主， 祂创造了你们，塑造了你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚昧無知的人民哪， 你們這樣報答耶和華嗎？ 他不是你的父，不是把你買回來的嗎？ 他造了你，建立了你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚昧無知的民哪，你們這樣報答耶和華嗎？ 他豈不是你的父、將你買來的嗎？ 他是製造你、建立你的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do you thus repay the Lord, O foolish and unwise people? Is not He your Father who has bought you? He has made you and established you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Is this the way you repay the Lord, you foolish and unwise people? Is he not your Father, your Creator, who made you and formed you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Is this the way you repay the Lord, you foolish and senseless people? Isn’t he your Father who created you? Has he not made you and established you?"
      }
    },
