      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他必在你那裏與你同住，在你的城邑中，要由他選擇一個所喜悅的地方居住；你不可欺負他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He shall live with you in your midst, in the place which he shall choose in one of your towns where it pleases him; you shall not mistreat him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let them live among you wherever they like and in whatever town they choose. Do not oppress them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let them live among you in any town they choose, and do not oppress them."
      }
