      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你借給你弟兄的，或是錢財或是糧食，無論甚麼可生利的物，都不可取利。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall not charge interest to your countrymen: interest on money, food, or anything that may be loaned at interest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not charge a fellow Israelite interest, whether on money or food or anything else that may earn interest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do not charge interest on the loans you make to a fellow Israelite, whether you loan money, or food, or anything else."
      }
    },
