    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "deu.23.9-14",
            "paragraphs": [
              "i.營中的潔凈（二十三9～14）",
              "本段的資料與二十章及其他討論聖戰的段落同屬一類。執行聖戰時，必須滿足某些祭禮上的要求。在營中與自己子民同在，又和他們一同出戰打敗敵人的耶和華，要求除盡一切不潔之物，免得祂離營而去。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果有人因梦遗而不洁净，他就要离开营地住在营外。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果你們中間有人因為夜間偶然夢遺而不潔淨，就要出到營外去，不可進入營中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們中間，若有人夜間偶然夢遺，不潔淨，就要出到營外，不可入營；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If there is among you any man who is unclean because of a nocturnal emission, then he must go outside the camp; he may not reenter the camp."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If one of your men is unclean because of a nocturnal emission, he is to go outside the camp and stay there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Any man who becomes ceremonially defiled because of a nocturnal emission must leave the camp and stay away all day."
      }
    },
