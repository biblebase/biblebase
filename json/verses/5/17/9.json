      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And you shall come to the Levitical priests and to the judge who is in office in those days, and you shall consult them, and they shall declare to you the decision."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So you shall come to the Levitical priest or the judge who is in office in those days, and you shall inquire of them and they will declare to you the verdict in the case."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Go to the Levitical priests and to the judge who is in office at that time. Inquire of them and they will give you the verdict."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And you shall come to the priests, the Levites, and to the judge there in those days, and inquire of  them;  they shall pronounce upon you the sentence of judgment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and present them to the Levitical priests or the judge on duty at that time. They will hear the case and declare the verdict."
      }
