    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "否则，今天我叫天地作证，你们必很快从约旦河对面将要占领的土地上灭亡，你们必被彻底消灭，不得在那里长久居住。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我今日就呼天喚地向你們作證，你們必在過約旦河取得為業的地上迅速滅亡；你們必不能在那地長久生存，必全被消滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我今日呼天喚地向你們作見證，你們必在過約旦河得為業的地上速速滅盡！你們不能在那地上長久，必盡行除滅。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I call heaven and earth to witness against you today, that you will soon utterly perish from the land that you are going over the Jordan to possess. You will not live long in it, but will be utterly destroyed."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I call heaven and earth to witness against you today, that you will surely perish quickly from the land where you are going over the Jordan to possess it. You shall not live long on it, but will be utterly destroyed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I call the heavens and the earth as witnesses against you this day that you will quickly perish from the land that you are crossing the Jordan to possess. You will not live there long but will certainly be destroyed."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I call heaven and earth to witness against you this day, that you will soon utterly perish from the land which you cross over the Jordan to possess; you will not prolong your days in it, but will be utterly destroyed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Today I call on heaven and earth as witnesses against you. If you break my covenant, you will quickly disappear from the land you are crossing the Jordan to occupy. You will live there only a short time; then you will be utterly destroyed."
      }
    },
