    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时，耶和华吩咐我把律例和典章传授给你们，好让你们在将要占领的土地上遵守。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，耶和華也吩咐我把律例和典章教訓你們，使你們在將要過去得為業的地上遵行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時耶和華又吩咐我將律例典章教訓你們，使你們在所要過去得為業的地上遵行。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the Lord commanded me at that time to teach you statutes and rules, that you might do them in the land that you are going over to possess."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord commanded me at that time to teach you statutes and judgments, that you might perform them in the land where you are going over to possess it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Lord directed me at that time to teach you the decrees and laws you are to follow in the land that you are crossing the Jordan to possess."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the Lord commanded me at that time to teach you statutes and judgments, that you might observe them in the land which you cross over to possess."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It was at that time that the Lord commanded me to teach you his decrees and regulations so you would obey them in the land you are about to enter and occupy."
      }
