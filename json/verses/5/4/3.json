    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们亲眼看见耶和华在巴力·毗珥事件中的作为，你们的上帝耶和华毁灭了你们中间所有拜巴力·毗珥的人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華因巴力．毘珥的事所行的，你們都親眼看見了；所有跟隨巴力．毘珥的人，耶和華你們的　神都從你們中間消滅了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華因巴力‧毗珥的事所行的，你們親眼看見了。凡隨從巴力‧毗珥的人，耶和華－你們的神都從你們中間除滅了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your eyes have seen what the Lord has done in the case of Baal-peor, for all the men who followed Baal-peor, the Lord your God has destroyed them from among you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You saw with your own eyes what the Lord did at Baal Peor. The Lord your God destroyed from among you everyone who followed the Baal of Peor,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You saw for yourself what the Lord did to you at Baal-peor. There the Lord your God destroyed everyone who had worshiped Baal, the god of Peor."
      }
    },
