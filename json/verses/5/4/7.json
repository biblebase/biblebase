    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的上帝耶和华与我们如此亲近，随时垂听我们的呼求，有哪个伟大民族的神明能与之相比？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "哪一個大國的人有神與他們這樣親近，像耶和華我們的　神，在我們每次呼求他的時候，與我們親近一樣呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "哪一大國的人有神與他們相近，像耶和華－我們的神、在我們求告他的時候與我們相近呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For what great nation is there that has a god so near to it as is the Lord our God whenever we call on Him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What other nation is so great as to have their gods near them the way the Lord our God is near us whenever we pray to him?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For what great nation has a god as near to them as the Lord our God is near to us whenever we call on him?"
      }
