    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华听见了你们的话，就对我说，‘我听见了众人对你说的话，他们所说的都对。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們對我說話的時候，耶和華聽見了你們說話的聲音；耶和華就對我說：‘這人民對你所說的話，我都聽見了；他們所說的都很好。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們對我說的話，耶和華都聽見了。耶和華對我說：『這百姓的話，我聽見了；他們所說的都是。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The Lord heard the voice of your words when you spoke to me, and the Lord said to me, ‘I have heard the voice of the words of this people which they have spoken to you. They have done well in all that they have spoken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord heard you when you spoke to me, and the Lord said to me, “I have heard what this people said to you. Everything they said was good."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The Lord heard the request you made to me. And he said, ‘I have heard what the people said to you, and they are right."
      }
    },
