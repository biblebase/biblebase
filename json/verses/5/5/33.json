    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，你们便得享长寿，并且可以在你们将要占领的土地上兴盛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要走在耶和華你們的　神吩咐你們走的路上，使你們可以活著，可以享福，並且使你在將要佔領為業的地上，可以長久居住。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－你們神所吩咐你們行的，你們都要去行，使你們可以存活得福，並使你們的日子在所要承受的地上得以長久。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall walk in all the way which the Lord your God has commanded you, that you may live and that it may be well with you, and that you may prolong your days in the land which you will possess."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Walk in obedience to all that the Lord your God has commanded you, so that you may live and prosper and prolong your days in the land that you will possess."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Stay on the path that the Lord your God has commanded you to follow. Then you will live long and prosperous lives in the land you are about to enter and occupy."
      }
    },
