    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘要遵照你的上帝耶和华的吩咐孝敬父母，以便在你的上帝耶和华要赐给你的土地上享长寿，蒙祝福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘要孝敬父母，照著耶和華你的　神吩咐你的，使你可以得享長壽，也使你在耶和華你的　神賜給你的地上，得享福樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「『當照耶和華－你神所吩咐的孝敬父母，使你得福，並使你的日子在耶和華－你神所賜你的地上得以長久。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Honor your father and your mother, as the Lord your God has commanded you, that your days may be prolonged and that it may go well with you on the land which the Lord your God gives you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Honor your father and your mother, as the Lord your God has commanded you, so that you may live long and that it may go well with you in the land the Lord your God is giving you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Honor your father and mother, as the Lord your God commanded you. Then you will live a long, full life in the land the Lord your God is giving you."
      }
