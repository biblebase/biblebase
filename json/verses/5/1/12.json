    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我怎能独自担当解决你们问题和争端的重任呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我獨自一人怎能擔得起你們的重擔、責任和你們爭訟的事呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你們的麻煩，和管理你們的重任，並你們的爭訟，我獨自一人怎能擔當得起呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How can I alone bear the load and burden of you and your strife?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But how can I bear your problems and your burdens and your disputes all by myself?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you are such a heavy load to carry! How can I deal with all your problems and bickering?"
      }
