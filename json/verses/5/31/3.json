    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的上帝耶和华要亲自带领你们过河，在你们面前消灭那里的各族，使你们占领他们的土地。约书亚将做你们的首领，正如耶和华所言。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華你的　神要親自領你們過去，從你們面前消滅這些國的民，使你們得著他們的地，照著耶和華說過的，約書亞要親自領你們過去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－你們的神必引導你們過去，將這些國民在你們面前滅絕，你們就得他們的地。約書亞必引導你們過去，正如耶和華所說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is the Lord your God who will cross ahead of you; He will destroy these nations before you, and you shall dispossess them. Joshua is the one who will cross ahead of you, just as the Lord has spoken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord your God himself will cross over ahead of you. He will destroy these nations before you, and you will take possession of their land. Joshua also will cross over ahead of you, as the Lord said."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Lord your God himself will cross over ahead of you. He will destroy the nations living there, and you will take possession of their land. Joshua will lead you across the river, just as the Lord promised."
      }
    },
