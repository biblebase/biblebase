    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "像对付希实本王西宏一样，我们将这些城邑夷为平地，城中的男女老幼一个不留，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們把這些都滅盡，像從前我們待希實本王西宏一樣，把各城中的男女和小孩子都滅盡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們將這些都毀滅了，像從前待希實本王西宏一樣，把有人煙的各城，連女人帶孩子，盡都毀滅；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And we devoted them to destruction, as we did to Sihon the king of Heshbon, devoting to destruction every city, men, women, and children."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We utterly destroyed them, as we did to Sihon king of Heshbon, utterly destroying the men, women and children of every city."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We completely destroyed them, as we had done with Sihon king of Heshbon, destroying every city—men, women and children."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And we utterly destroyed them, as we did to Sihon king of Heshbon, utterly destroying the men, women, and children of every city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We completely destroyed the kingdom of Bashan, just as we had destroyed King Sihon of Heshbon. We destroyed all the people in every town we conquered—men, women, and children alike."
      }
    },
