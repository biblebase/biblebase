    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西率利未祭司对全体以色列人说：“以色列人啊，要肃静聆听！你们今天已成为你们上帝耶和华的子民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西和利未支派的祭司對以色列眾人說：“以色列啊，要默默靜聽；今日你已經成為耶和華你的　神的子民了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西和祭司利未人曉諭以色列眾人說：「以色列啊，要默默靜聽。你今日成為耶和華－你神的百姓了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Moses and the Levitical priests spoke to all Israel, saying, “Be silent and listen, O Israel! This day you have become a people for the Lord your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Moses and the Levitical priests said to all Israel, “Be silent, Israel, and listen! You have now become the people of the Lord your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Moses and the Levitical priests addressed all Israel as follows: “O Israel, be quiet and listen! Today you have become the people of the Lord your God."
      }
    },
