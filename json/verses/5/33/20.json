    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西祝福迦得支派说： “扩张迦得疆界的那位当受称颂！ 迦得蹲伏如狮子， 撕碎头颅和臂膀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "論到迦得，他說： “那使迦得擴張的，是應該稱頌的， 迦得蹲下如母獅， 他撕裂膀臂，連頭頂也撕裂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論迦得說： 使迦得擴張的應當稱頌！ 迦得住如母獅； 他撕裂膀臂，連頭頂也撕裂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Of Gad he said, “Blessed is the one who enlarges Gad; He lies down as a lion, And tears the arm, also the crown of the head."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "About Gad he said: “Blessed is he who enlarges Gad’s domain! Gad lives there like a lion, tearing at arm or head."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses said this about the tribe of Gad: “Blessed is the one who enlarges Gad’s territory! Gad is poised there like a lion to tear off an arm or a head."
      }
    },
