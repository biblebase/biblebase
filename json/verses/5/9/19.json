    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华向你们发烈怒，要毁灭你们，我非常惧怕。但耶和华再次垂听了我的祈求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華向你們大發烈怒，要消滅你們，我就很懼怕；但是那一次耶和華也聽了我的懇求。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因耶和華向你們大發烈怒，要滅絕你們，就甚害怕；但那次耶和華又應允了我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I was afraid of the anger and hot displeasure with which the Lord was wrathful against you in order to destroy you, but the Lord listened to me that time also."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I feared the anger and wrath of the Lord, for he was angry enough with you to destroy you. But again the Lord listened to me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I feared that the furious anger of the Lord, which turned him against you, would drive him to destroy you. But again he listened to me."
      }
    },
