    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要知道，耶和华赐给你们那佳美之地不是因为你们有好品德。其实你们是顽固不化的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要知道，耶和華你的　神把這美地賜給你作產業，並不是因著你的義；你本來是頑固的民族。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你當知道，耶和華－你神將這美地賜你為業，並不是因你的義；你本是硬着頸項的百姓。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Know, then, it is not because of your righteousness that the Lord your God is giving you this good land to possess, for you are a stubborn people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Understand, then, that it is not because of your righteousness that the Lord your God is giving you this good land to possess, for you are a stiff-necked people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must recognize that the Lord your God is not giving you this good land because you are good, for you are not—you are a stubborn people."
      }
    },
