    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们遵照我们的上帝耶和华的吩咐，没有侵犯亚扪人的土地，即雅博河一带和山区的城邑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只有亞捫人的地，雅博河一帶的地方，和山地的城市，以及耶和華我們的　神吩咐我們不可去的地方，我們都沒有走近。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟有亞捫人之地，凡靠近雅博河的地，並山地的城邑，與耶和華－我們神所禁止我們去的地方，都沒有挨近。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Only you did not go near to the land of the sons of Ammon, all along the river Jabbok and the cities of the hill country, and wherever the Lord our God had commanded us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But in accordance with the command of the Lord our God, you did not encroach on any of the land of the Ammonites, neither the land along the course of the Jabbok nor that around the towns in the hills."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "However, we avoided the land of the Ammonites all along the Jabbok River and the towns in the hill country—all the places the Lord our God had commanded us to leave alone."
      }
    },
