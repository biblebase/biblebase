    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "deu.30.11-20",
            "paragraphs": [
              "Ｅ鄭重呼籲揀選生命（三十11～20）",
              "內容的發展，如今已經達到了呼籲百姓，在盟約之上作出決定的地步了。本段首先說明對他們的要求並不是「難行」或「離你遠」，而是顯然易明的（11～14節），然後呼召他們接納委身（15～20節）。"
            ]
          },
          {
            "verse": "deu.30.11-14",
            "paragraphs": [
              "i.神的盟約人人能得（三十11～14）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_sermon-2012-07-29",
        "title": "揀選生命",
        "date": "2012-07-29",
        "preacher": "王文堂牧師",
        "verses": "申命記 30:11-20",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-07-29.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-07-29.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这诫命并非在天上，以致你们说，‘谁替我们上天取下来，讲给我们听，让我们遵行呢？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這誡命不是在天上，以致你說：‘誰替我們上到天上去，為我們取下來，使我們聽見，就可以遵行呢？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不是在天上，使你說：『誰替我們上天取下來，使我們聽見可以遵行呢？』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is not in heaven, that you should say, ‘Who will go up to heaven for us to get it for us and make us hear it, that we may observe it?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is not up in heaven, so that you have to ask, “Who will ascend into heaven to get it and proclaim it to us so we may obey it?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is not kept in heaven, so distant that you must ask, ‘Who will go up to heaven and bring it down so we can hear it and obey?’"
      }
    },
