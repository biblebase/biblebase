    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的上帝耶和华必照祂的应许赐福给你们，你们必借贷给多国，却不需要向他们借贷。你们必统治多国，却不被他们统治。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華你的　神，必照著他應許你的，賜福給你；你必借貸給許多國的民，卻不會向他們借貸；你必統治許多國的民，他們卻不會統治你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為耶和華－你的神必照他所應許你的賜福與你。你必借給許多國民，卻不致向他們借貸；你必管轄許多國民，他們卻不能管轄你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord your God will bless you as He has promised you, and you will lend to many nations, but you will not borrow; and you will rule over many nations, but they will not rule over you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the Lord your God will bless you as he has promised, and you will lend to many nations but will borrow from none. You will rule over many nations but none will rule over you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord your God will bless you as he has promised. You will lend money to many nations but will never need to borrow. You will rule many nations, but they will not rule over you."
      }
