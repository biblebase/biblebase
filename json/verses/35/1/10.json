        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們戲弄君王，以掌權的為笑柄； 他們嗤笑一切城堡，築壘攻取；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們譏誚君王，笑話首領， 嗤笑一切保障，築壘攻取。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They mock at kings And rulers are a laughing matter to them. They laugh at every fortress And heap up rubble to capture it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They mock kings and scoff at rulers. They laugh at all fortified cities; by building earthen ramps they capture them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They scoff at kings and princes and scorn all their fortresses. They simply pile ramps of earth against their walls and capture them!"
      }
