    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要小心，谁都不可冤冤相报，总要彼此善待，也要善待众人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要注意，不管是誰都不要以惡報惡，卻要在彼此相處和對待眾人這方面，常常追求良善。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你们要注意，谁都不要以恶报恶；相反，无论是彼此之间，还是对待众人，总要追求美善。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要謹慎，無論是誰都不可以惡報惡；或是彼此相待，或是待眾人，常要追求良善。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "See that no one repays anyone evil for evil, but always seek to do good to one another and to everyone."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "See that no one repays another with evil for evil, but always seek after that which is good for one another and for all people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Make sure that nobody pays back wrong for wrong, but always strive to do what is good for each other and for everyone else."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "See that no one renders evil for evil to anyone, but always pursue what is good both for yourselves and for all."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "See that no one pays back evil for evil, but always try to do good to each other and to all people."
      }
