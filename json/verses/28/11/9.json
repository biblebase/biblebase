    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我不再发烈怒， 也不再毁灭以法莲。 因为我是上帝，不是世人， 是住在你们中间的圣者， 我必不带着烈怒来临。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必不讓我的烈怒發作， 必不再毀滅以法蓮； 因為我是　神，不是世人；我是你們中間的聖者， 必不會含怒而來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必不發猛烈的怒氣， 也不再毀滅以法蓮。 因我是神，並非世人， 是你們中間的聖者； 我必不在怒中臨到你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will not execute My fierce anger; I will not destroy Ephraim again. For I am God and not man, the Holy One in your midst, And I will not come in wrath."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will not carry out my fierce anger, nor will I devastate Ephraim again. For I am God, and not a man— the Holy One among you. I will not come against their cities."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No, I will not unleash my fierce anger. I will not completely destroy Israel, for I am God and not a mere mortal. I am the Holy One living among you, and I will not come to destroy."
      }
    },
