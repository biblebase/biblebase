    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.11.1",
            "paragraphs": [
              "一一1\"以色列\"。以色列民，神把她從埃及地拯救出來，並小心照顧（出四22），但她後來卻不忠於神。馬太福音使用以色列在埃及蒙召喚，來類比基督從埃及返回來（太二15）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “以色列年幼时，我就爱他， 视他为儿子，把他从埃及召出来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“以色列年幼的時候，我就愛他。 我從埃及召我的兒子出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列年幼的時候，我愛他， 就從埃及召出我的兒子來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Israel was a youth I loved him, And out of Egypt I called My son."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When Israel was a child, I loved him, and out of Egypt I called my son."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“When Israel was a child, I loved him, and I called my son out of Egypt."
      }
