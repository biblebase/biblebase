    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲本是驯服的母牛犊，喜欢打谷， 现在我要把轭套在它肥美的颈项上。 我要使犹大耕田，使雅各耙地。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮是一頭馴服的母牛犢，喜愛踹穀； 我卻要把軛扣在牠肥美的頸項上， 我要使以法蓮拖犁； 猶大要耕田，雅各必為自己耙地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮是馴良的母牛犢，喜愛踹穀， 我卻將軛加在牠肥美的頸項上， 我要使以法蓮拉套。 猶大必耕田； 雅各必耙地。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Ephraim was a trained calf that loved to thresh, and I spared her fair neck; but I will put Ephraim to the yoke; Judah must plow; Jacob must harrow for himself."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ephraim is a trained heifer that loves to thresh, But I will come over her fair neck with a yoke; I will harness Ephraim, Judah will plow, Jacob will harrow for himself."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ephraim is a trained heifer that loves to thresh; so I will put a yoke on her fair neck. I will drive Ephraim, Judah must plow, and Jacob must break up the ground."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Ephraim is  a trained heifer That loves to thresh grain; But I harnessed her fair neck, I will make Ephraim pull a  plow. Judah shall plow; Jacob shall break his clods.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Israel is like a trained heifer treading out the grain— an easy job she loves. But I will put a heavy yoke on her tender neck. I will force Judah to pull the plow and Israel to break up the hard ground."
      }
