    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "伯特利  啊，你罪大恶极， 必将遭遇同样的下场。 那日来临时，以色列的王将被彻底消灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "伯特利啊，因你們的罪大惡極， 我必這樣待你們。 在風暴中，以色列的王必全然被除滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他們的大惡， 伯特利必使你們遭遇如此。 到了黎明，以色列的王必全然滅絕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus it will be done to you at Bethel because of your great wickedness. At dawn the king of Israel will be completely cut off."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So will it happen to you, Bethel, because your wickedness is great. When that day dawns, the king of Israel will be completely destroyed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will share that fate, Bethel, because of your great wickedness. When the day of judgment dawns, the king of Israel will be completely destroyed."
      }
    },
