    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要按自己的心意惩罚你们。 列国要联合攻击你们， 惩罚你们的重重罪恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必來懲罰他們； 他們被自己那雙重的罪孽捆綁的時候， 列國就集結起來攻擊他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必隨意懲罰他們。 他們為兩樣的罪所纏； 列邦的民必聚集攻擊他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When it is My desire, I will chastise them; And the peoples will be gathered against them When they are bound for their double guilt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When I please, I will punish them; nations will be gathered against them to put them in bonds for their double sin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now whenever it fits my plan, I will attack you, too. I will call out the armies of the nations to punish you for your multiplied sins."
      }
    },
