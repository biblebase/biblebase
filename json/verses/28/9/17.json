    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.9.10-17",
            "paragraphs": [
              "九10～17雖然在以色列國開始的時候，神發現他的子民好象\"葡萄在曠野\"（10節）那樣使人心神爽快，但他們卻在\"巴力毗珥\"轉向邪惡（比較民二五），在\"吉甲\"背叛神（15節）；吉甲就是他們宣布掃羅為王的地方（撒上一一15）。巴力的膜拜，和渴望人類君王統治而非神統治兩者的結合，使百姓註定要\"飄流在列國中\"（17節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的上帝必弃绝他们， 因为他们不听从祂。 他们必在列国中流离飘泊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的　神必棄絕他們， 因為他們不聽從他； 他們必在列國中飄流。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的神必棄絕他們， 因為他們不聽從他； 他們也必飄流在列國中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My God will cast them away Because they have not listened to Him; And they will be wanderers among the nations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My God will reject them because they have not obeyed him; they will be wanderers among the nations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My God will reject the people of Israel because they will not listen or obey. They will be wanderers, homeless among the nations."
      }
