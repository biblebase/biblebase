    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.9.9",
            "paragraphs": [
              "九9何西阿把當日的邪惡與\"基比亞的日子\"相比（比較士一九～二一）；昔日便雅憫支派幾乎被滅絕。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们败坏至极， 恰如从前在基比亚的日子。 耶和华必记住你们的罪恶， 追讨你们的罪债。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮深深敗壞， 如同在基比亞的日子一般。 　神必記得他們的罪孽， 懲罰他們的罪惡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮深深地敗壞， 如在基比亞的日子一樣。 耶和華必記念他們的罪孽， 追討他們的罪惡。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They have deeply corrupted themselves as in the days of Gibeah: he will remember their iniquity; he will punish their sins."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
