    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.9.1",
            "paragraphs": [
              "九1\"賞賜\"。更可作：妓女的酬勞。以色列仿效那些信異教的鄰國，進行放肆的生殖物膜拜禮儀。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列啊， 不要欢喜快乐， 不要像外族人那样欢喜快乐！ 因为你对你的上帝不忠不贞， 在各麦场上喜爱卖淫得资财。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列啊！不要歡喜；不要像外族人一樣快樂， 因為你行了邪淫，離開你的　神， 在各打穀場上喜愛賣淫的賞賜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列啊，不要像外邦人歡喜快樂； 因為你行邪淫離棄你的神， 在各穀場上如妓女喜愛賞賜。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Rejoice not, O Israel! Exult not like the peoples; for you have played the whore, forsaking your God. You have loved a prostitute’s wages on all threshing floors."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not rejoice, O Israel, with exultation like the nations! For you have played the harlot, forsaking your God. You have loved harlots’ earnings on every threshing floor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not rejoice, Israel; do not be jubilant like the other nations. For you have been unfaithful to your God; you love the wages of a prostitute at every threshing floor."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Do not rejoice, O Israel, with joy like other peoples, For you have played the harlot against your God. You have made love for  hire on every threshing floor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O people of Israel, do not rejoice as other nations do. For you have been unfaithful to your God, hiring yourselves out like prostitutes, worshiping other gods on every threshing floor."
      }
    },
