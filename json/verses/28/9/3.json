    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.9.3",
            "paragraphs": [
              "九3有些人確實去了埃及（王下二五26）。許多人去了亞述。在這兩個不潔之地，他們會吃不潔的食物，而且不能守他們的節期（5節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们将不能再住在耶和华的土地上。 以法莲要返回埃及， 在亚述吃不洁之物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們必不得住在耶和華之地； 以法蓮要回到埃及去， 要在亞述吃不潔之物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必不得住耶和華的地； 以法蓮卻要歸回埃及， 必在亞述吃不潔淨的食物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will not remain in the Lord’s land, But Ephraim will return to Egypt, And in Assyria they will eat unclean food."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will not remain in the Lord’s land; Ephraim will return to Egypt and eat unclean food in Assyria."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You may no longer stay here in the Lord’s land. Instead, you will return to Egypt, and in Assyria you will eat food that is ceremonially unclean."
      }
    },
