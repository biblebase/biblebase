    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.4.2",
            "paragraphs": [
              "四2\"殺人流血，接連不斷\"。原文作：血觸及血，即一宗謀殺接另一宗，留下一條血的痕跡。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们咒诅、撒谎、凶杀、偷窃、通奸， 为所欲为，血案累累。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只有起假誓、撒謊、殘殺、偷盜、姦淫、行強暴，以至流人血事件相繼而來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但起假誓，不踐前言， 殺害，偷盜，姦淫，行強暴， 殺人流血，接連不斷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is swearing, deception, murder, stealing and adultery. They employ violence, so that bloodshed follows bloodshed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There is only cursing, lying and murder, stealing and adultery; they break all bounds, and bloodshed follows bloodshed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You make vows and break them; you kill and steal and commit adultery. There is violence everywhere— one murder after another."
      }
    },
