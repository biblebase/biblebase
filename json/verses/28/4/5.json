    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们白天黑夜都要跌倒， 先知也要跟你们一起跌倒， 所以我要毁灭你们的母亲以色列。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "白天你要跌倒， 晚上先知也必與你一同跌倒； 我要毀滅你的母親。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你這祭司必日間跌倒； 先知也必夜間與你一同跌倒； 我必滅絕你的母親。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You shall stumble by day; the prophet also shall stumble with you by night; and I will destroy your mother."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So you will stumble by day, And the prophet also will stumble with you by night; And I will destroy your mother."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You stumble day and night, and the prophets stumble with you. So I will destroy your mother—"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore you shall stumble in the day; The prophet also shall stumble with you in the night; And I will destroy your mother."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So you will stumble in broad daylight, and your false prophets will fall with you in the night. And I will destroy Israel, your mother."
      }
    },
