    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.2.21",
            "paragraphs": [
              "二21\"應允天\"。即回應那些上達於天的禱告。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “那时，我要应允， 我要应允天的祈求， 天要应允地的祈求，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：“到那日，我必應允， 我必應允天，天必應允地，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：那日我必應允， 我必應允天，天必應允地；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“It will come about in that day that I will respond,” declares the Lord. “I will respond to the heavens, and they will respond to the earth,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“In that day I will respond,” declares the Lord— “I will respond to the skies, and they will respond to the earth;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“In that day, I will answer,” says the Lord. “I will answer the sky as it pleads for clouds. And the sky will answer the earth with rain."
      }
