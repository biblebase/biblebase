    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.2.16",
            "paragraphs": [
              "二16\"伊施\"的意思是\"我的丈夫\"。\"巴力\"的意思是\"我的擁有者\"。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “那时，你要称我为丈夫， 不再称我为主人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：“到那日， 你必稱呼我‘伊施’， 不再稱呼我‘巴力’。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「那日你必稱呼我伊施，不再稱呼我巴力；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It will come about in that day,” declares the Lord, “That you will call Me Ishi And will no longer call Me Baali."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“In that day,” declares the Lord, “you will call me ‘my husband’; you will no longer call me ‘my master.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When that day comes,” says the Lord, “you will call me ‘my husband’ instead of ‘my master.’"
      }
    },
