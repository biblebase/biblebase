        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是，我用一百七十一克銀子，一百五十公斤大麥，買她回來歸我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我便用銀子十五舍客勒，大麥一賀梅珥半，買她歸我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So I bought her for myself for fifteen shekels of silver and a homer and a half of barley."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So I bought her for fifteen shekels of silver and about a homer and a lethek of barley."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I bought her back for fifteen pieces of silver and five bushels of barley and a measure of wine."
      }
