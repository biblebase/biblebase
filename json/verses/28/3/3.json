    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.3.3-5",
            "paragraphs": [
              "三3～5歌篾被迫獨居一段日子，象征以色列的被擄。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对她说：“你要和我住许多日子。这期间不可行淫，不可亲近别的男人，我也不与你同房。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我對她說：“你要給我獨居多日，不要行淫，不要有別的男人，我也必不與你同房。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我對她說：「你當多日為我獨居，不可行淫，不可歸別人為妻，我向你也必這樣。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And I said to her, “You must dwell as mine for many days. You shall not play the whore, or belong to another man; so will I also be to you.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I said to her, “You shall stay with me for many days. You shall not play the harlot, nor shall you have a man; so I will also be toward you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I told her, “You are to live with me many days; you must not be a prostitute or be intimate with any man, and I will behave the same way toward you.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And I said to her, “You shall stay with me many days; you shall not play the harlot, nor shall you have a man—so, too, will I be toward you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I said to her, “You must live in my house for many days and stop your prostitution. During this time, you will not have sexual relations with anyone, not even with me.”"
      }
    },
