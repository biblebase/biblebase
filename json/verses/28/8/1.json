    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.8.1",
            "paragraphs": [
              "八1亞述會如鷹到來攻打以色列。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“吹响号角警告众人吧！ 敌人要如鹰扑向耶和华的家， 因为百姓背弃我的约， 违背我的律法。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要把號角放在你的嘴上， 敵人如鷹一般來攻擊耶和華的家； 因為他們違背了我的約， 干犯了我的律法。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你用口吹角吧！ 敵人如鷹來攻打耶和華的家； 因為這民違背我的約， 干犯我的律法。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Put the trumpet to your lips! Like an eagle the enemy comes against the house of the Lord, Because they have transgressed My covenant And rebelled against My law."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Put the trumpet to your lips! An eagle is over the house of the Lord because the people have broken my covenant and rebelled against my law."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Sound the alarm! The enemy descends like an eagle on the people of the Lord, for they have broken my covenant and revolted against my law."
      }
    },
