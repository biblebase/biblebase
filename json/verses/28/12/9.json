    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.12.9",
            "paragraphs": [
              "一二9以色列人再次住在\"帳棚\"裏，因為她的城市都被摧毀了。住棚節紀念以色列人離開埃及後在曠野所過的那段日子。他們的被擄即將來臨，是另一次的曠野經歷。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “从你们在埃及的时候， 我就是你们的上帝耶和华。 我要使你们再次住在帐篷里， 就像从前守节期时一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“自從你出埃及地以來， 我就是耶和華你的　神； 我必使你再住在帳棚裡， 像在節日的日子一樣，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "自從你出埃及地以來， 我就是耶和華－你的神； 我必使你再住帳棚，如在大會的日子一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I have been the Lord your God since the land of Egypt; I will make you live in tents again, As in the days of the appointed festival."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I have been the Lord your God ever since you came out of Egypt; I will make you live in tents again, as in the days of your appointed festivals."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But I am the Lord your God, who rescued you from slavery in Egypt. And I will make you live in tents again, as you do each year at the Festival of Shelters."
      }
    },
