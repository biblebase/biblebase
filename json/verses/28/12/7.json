    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.12.7",
            "paragraphs": [
              "一二7\"商人\"。直譯作：迦南人。以色列人進入迦南，而迦南人的行事方式卻進入了以色列人之中。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲是个不诚实的商人， 手里拿着骗人的秤，喜欢压榨人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列是個商人，手裡拿著詭詐的天平，喜愛欺壓別人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮是商人， 手裏有詭詐的天平，愛行欺騙。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A merchant, in whose hands are false balances, He loves to oppress."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The merchant uses dishonest scales and loves to defraud."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But no, the people are like crafty merchants selling from dishonest scales— they love to cheat."
      }
    },
