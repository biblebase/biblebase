    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基列人充满罪恶，终必灭亡。 他们在吉甲用公牛献祭， 他们的祭坛像田间犁沟里的石堆。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在基列仍有罪孽， 他們實在是沒有價值的。 他們在吉甲獻牛犢為祭， 他們的祭壇好像田間犁溝中的石堆。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基列人沒有罪孽嗎？ 他們全然是虛假的。 人在吉甲獻牛犢為祭， 他們的祭壇好像田間犂溝中的亂堆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Is there iniquity in Gilead? Surely they are worthless. In Gilgal they sacrifice bulls, Yes, their altars are like the stone heaps Beside the furrows of the field."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Is Gilead wicked? Its people are worthless! Do they sacrifice bulls in Gilgal? Their altars will be like piles of stones on a plowed field."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the people of Gilead are worthless because of their idol worship. And in Gilgal, too, they sacrifice bulls; their altars are lined up like the heaps of stone along the edges of a plowed field."
      }
    },
