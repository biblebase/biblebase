    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.12.3-4",
            "paragraphs": [
              "一二3,4以色列和猶大象雅各一樣詭詐，但也象雅各在伯特利一樣，會轉向耶和華。參看創世記二十五章26節和三十二章24至28節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雅各在母胎之中曾抓住哥哥的脚跟， 壮年之时曾跟上帝角力。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他在母腹中抓住他哥哥的腳跟， 壯年的時候又與　神角力。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他在腹中抓住哥哥的腳跟， 壯年的時候與神較力，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In the womb he took his brother by the heel, And in his maturity he contended with God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the womb he grasped his brother’s heel; as a man he struggled with God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even in the womb, Jacob struggled with his brother; when he became a man, he even fought with God."
      }
    },
