    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们有祸了， 因为他们背弃我！ 他们要被毁灭了， 因为他们背叛我！ 我要救赎他们， 他们却向我撒谎。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們有禍了，因為他們離棄了我； 他們遭毀滅，因為他們背叛了我。 我雖然想救贖他們，他們卻對我說謊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們因離棄我，必定有禍； 因違背我，必被毀滅。 我雖要救贖他們，他們卻向我說謊。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Woe to them, for they have strayed from me! Destruction to them, for they have rebelled against me! I would redeem them, but they speak lies against me."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe to them, for they have strayed from Me! Destruction is theirs, for they have rebelled against Me! I would redeem them, but they speak lies against Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Woe to them, because they have strayed from me! Destruction to them, because they have rebelled against me! I long to redeem them but they speak about me falsely."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Woe to them, for they have fled from Me! Destruction to them, Because they have transgressed against Me! Though I redeemed them, Yet they have spoken lies against Me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What sorrow awaits those who have deserted me! Let them die, for they have rebelled against me. I wanted to redeem them, but they have told lies about me."
      }
    },
