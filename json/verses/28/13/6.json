    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你们吃得饱足以后， 就心高气傲，忘记了我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但他們得了餵養而飽足， 飽足以後，就心高氣傲， 因此忘記了我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些民照我所賜的食物得了飽足； 既得飽足，心就高傲， 忘記了我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As they had their pasture, they became satisfied, And being satisfied, their heart became proud; Therefore they forgot Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When I fed them, they were satisfied; when they were satisfied, they became proud; then they forgot me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when you had eaten and were satisfied, you became proud and forgot me."
      }
