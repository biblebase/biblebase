    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你曾求我为你设立君王和首领， 现在拯救你的君王在哪里呢？ 在各城管理你的首领在哪里呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在你的王在哪裡？ 讓他拯救你吧！ 你所有的領袖在哪裡？讓他們治理你吧！ 因為論到他們你曾請求：‘把王和領袖賜給我。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你曾求我說： 給我立王和首領。 現在你的王在哪裏呢？ 治理你的在哪裏呢？ 讓他在你所有的城中拯救你吧！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Where now is your king That he may save you in all your cities, And your judges of whom you requested, “Give me a king and princes”?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Where is your king, that he may save you? Where are your rulers in all your towns, of whom you said, ‘Give me a king and princes’?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now where is your king? Let him save you! Where are all the leaders of the land, the king and the officials you demanded of me?"
      }
