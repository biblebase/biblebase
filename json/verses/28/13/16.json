    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撒玛利亚人背叛了他们的上帝， 要担当自己的罪过。 他们将丧身刀下， 他们的孩子要被摔死， 孕妇要被剖腹。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒瑪利亞必承擔自己罪的刑罰， 因為他背叛了他的　神。 他們必倒在刀下。 他們的嬰孩被摔死， 他們的孕婦必被剖開肚腹。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "撒馬利亞必擔當自己的罪， 因為悖逆她的神。 她必倒在刀下； 嬰孩必被摔死； 孕婦必被剖開。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Samaria shall bear her guilt, because she has rebelled against her God; they shall fall by the sword; their little ones shall be dashed in pieces, and their pregnant women ripped open."
      },
      "nasb": {
