    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.6.2",
            "paragraphs": [
              "六2\"過兩天……第三天\"。即在短時間之內（參看路一三32,33；彼後三8）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "过两天，祂必复兴我们； 第三天，祂必使我们站立起来， 在祂面前存活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "兩天以後，他要使我們復原； 第三天，他使我們起來， 我們就可以活在他的面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "過兩天他必使我們甦醒， 第三天他必使我們興起， 我們就在他面前得以存活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will revive us after two days; He will raise us up on the third day, That we may live before Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After two days he will revive us; on the third day he will restore us, that we may live in his presence."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In just a short time he will restore us, so that we may live in his presence."
      }
    },
