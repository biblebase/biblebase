    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.6.6",
            "paragraphs": [
              "六6\"良善\"。希伯來文是hesed。參看第二章19節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我喜爱怜悯之心，而非祭物； 我喜爱人认识我，胜于燔祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我喜愛憐恤，不喜愛祭祀； 我喜愛人認識　神的知識，勝過人獻的燔祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我喜愛良善，不喜愛祭祀； 喜愛認識神，勝於燔祭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I delight in loyalty rather than sacrifice, And in the knowledge of God rather than burnt offerings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I desire mercy, not sacrifice, and acknowledgment of God rather than burnt offerings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I want you to show love, not offer sacrifices. I want you to know me more than I want burnt offerings."
      }
    },
