    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.5.9",
            "paragraphs": [
              "五9\"責罰的日子\"。即神審判百姓的日子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲啊，你要在受惩罚的日子沦为废墟！ 我要在以色列各支派中宣告将来必要发生的事！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在責罰的日子，以法蓮必成為荒場。 我要在以色列各支派中宣告一定會發生的事情。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在責罰的日子，以法蓮必變為荒場； 我在以色列支派中，指示將來必成的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ephraim will become a desolation in the day of rebuke; Among the tribes of Israel I declare what is sure."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ephraim will be laid waste on the day of reckoning. Among the tribes of Israel I proclaim what is certain."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One thing is certain, Israel: On your day of punishment, you will become a heap of rubble."
      }
    },
