    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列被自己的傲慢指控， 以色列和以法莲要跌倒在自己的罪中， 犹大也要一同跌倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列的驕傲當面指證自己， 以色列和以法蓮因自己的罪孽跌倒， 猶大也與他們一同跌倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列的驕傲當面見證自己。 故此，以色列和以法蓮必因自己的罪孽跌倒； 猶大也必與他們一同跌倒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, the pride of Israel testifies against him, And Israel and Ephraim stumble in their iniquity; Judah also has stumbled with them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Israel’s arrogance testifies against them; the Israelites, even Ephraim, stumble in their sin; Judah also stumbles with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The arrogance of Israel testifies against her; Israel and Ephraim will stumble under their load of guilt. Judah, too, will fall with them."
      }
    },
