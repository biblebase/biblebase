    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "hos.5.6",
            "paragraphs": [
              "5:6",
              "他已經轉去離開他們：以色列人牽著他們的牛羊牲畜來獻祭，尋求耶和華，但他們卻尋不見，因著他們的惡行，神已經轉離他們。他們的心中沒有愛，沒有信心，沒有忠心，也沒有真實的悔改，反而被各樣邪情私欲所占據。人想尋求神的幫助卻尋不到，往往都是因為他們對世界上各樣敗壞罪惡的誘惑戀戀不舍而造成的(比較雅4:1~4；參“有功效的禱告”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "hos.5.6",
            "paragraphs": [
              "五6虛偽的獻祭是無用的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们牵着牛羊去寻求耶和华， 却寻不到， 因为祂已离他们而去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然他們帶著牛群羊群 去尋求耶和華，卻尋不見； 他已經離開他們遠去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必牽着牛羊去尋求耶和華，卻尋不見； 他已經轉去離開他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will go with their flocks and herds To seek the Lord, but they will not find Him; He has withdrawn from them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they go with their flocks and herds to seek the Lord, they will not find him; he has withdrawn himself from them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they come with their flocks and herds to offer sacrifices to the Lord, they will not find him, because he has withdrawn from them."
      }
