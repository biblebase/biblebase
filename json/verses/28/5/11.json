    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.5.11",
            "paragraphs": [
              "五11指耶羅波安的\"命令\"；他帶領以色列進入罪中（王上一二28）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲因甘愿追随偶像而受欺压， 被审判压碎。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮受欺壓，被審判壓碎； 因為他們樂意隨從人的命令。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮因樂從人的命令， 就受欺壓，被審判壓碎。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ephraim is oppressed, crushed in judgment, Because he was determined to follow man’s command."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ephraim is oppressed, trampled in judgment, intent on pursuing idols."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people of Israel will be crushed and broken by my judgment because they are determined to worship idols."
      }
    },
