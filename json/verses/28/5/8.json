    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.5.8",
            "paragraphs": [
              "五8\"角\"。希伯來文是shophar。通常只是一只羊角。\"基比亞\"和\"拉瑪\"是巴勒斯坦中部的高山，是吹角發警報的最佳位置。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “要在基比亚吹角， 在拉玛鸣号， 在伯·亚文呐喊， 便雅悯啊，敌人就在你身后！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要在基比亞吹角， 在拉瑪吹號， 在伯．亞文大聲吶喊： “便雅憫啊！敵人就在你背後。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們當在基比亞吹角， 在拉瑪吹號， 在伯‧亞文吹出大聲，說： 便雅憫哪，有仇敵在你後頭！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Blow the horn in Gibeah, the trumpet in Ramah. Sound the alarm at Beth-aven; we follow you, O Benjamin!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Blow the horn in Gibeah, The trumpet in Ramah. Sound an alarm at Beth-aven: “Behind you, Benjamin!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Sound the trumpet in Gibeah, the horn in Ramah. Raise the battle cry in Beth Aven; lead on, Benjamin."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Blow the ram’s horn in Gibeah, The trumpet in Ramah! Cry aloud at  Beth Aven, ‘Look behind you, O Benjamin!’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Sound the alarm in Gibeah! Blow the trumpet in Ramah! Raise the battle cry in Beth-aven! Lead on into battle, O warriors of Benjamin!"
      }
    },
