    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，当地人阻挠犹大人，使他们不敢建殿，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是那地方的居民使猶大人的手發軟，驚擾他們的建築工程。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那地的民，就在猶大人建造的時候，使他們的手發軟，擾亂他們；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the people of the land discouraged the people of Judah, and frightened them from building,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the peoples around them set out to discourage the people of Judah and make them afraid to go on building."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the local residents tried to discourage and frighten the people of Judah to keep them from their work."
      }
