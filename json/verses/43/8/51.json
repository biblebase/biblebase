    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.51",
            "paragraphs": [
              "八51主耶穌再說出威嚴的話。這些話語，若不是由神親自說出，就是說謊了。主再用“我實實在在的告訴你們”，作開場白，加強語氣。耶穌應許每一個遵守祂的道的人，就永遠不見死。這裏“死”不是指身體的死亡，因為每天有很多信主的人與世長辭。這裏的死是指屬靈的死。主說耶些相信祂的人將脫離永遠的死亡，不在地獄裏受煎熬之苦。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我实实在在地告诉你们，人如果遵行我的道，必永远不死。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我實實在在告訴你們，人若遵守我的道，必定永遠不見死亡。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我确确实实地告诉你们：人如果遵守我的话语，就绝不见死亡，直到永远。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我實實在在地告訴你們，人若遵守我的道，就永遠不見死。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
