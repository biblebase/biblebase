    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.35",
            "paragraphs": [
              "八35耶穌繼而比較奴仆和兒子在家裏的位置。奴仆沒有保證能永遠住在家裏，而兒子卻常在家中。“兒子”一詞不論是解作神之子，或是那些相信基督而成為神兒女的人，主耶穌這句話的意思很清楚，是告訴猶太人他們不是兒子，而是隨時會被趕走的奴仆。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "奴隶不能永远留在主人的家里，只有儿子才可以。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "奴隸不能永遠住在家裡，唯有兒子才可以永遠住在家裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "奴僕不能永遠住在家裏；兒子是永遠住在家裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The slave does not remain in the house forever; the son does remain forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now a slave has no permanent place in the family, but a son belongs to it forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A slave is not a permanent member of the family, but a son is part of the family forever."
      }
    },
