    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.23",
            "paragraphs": [
              "八23主耶穌知道他們以為祂要自盡，便稱他們是從下頭來的。他們看事物，是從庸俗粗鄙的角度，他們的思想不能超越時間、感官及物質的局限，對屬靈的事一無所知。相反，基督是從上頭來的。祂的思想、言語和行為都是屬天。而那些猶太人所做的，卻是屬世的。基督的一生，顯明出祂不是從這世界來的，祂來的地方比這世界要更純凈。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对他们说：“你们是从下面来的，我是从上面来的。你们属于这世界，我不属于这世界。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說：“你們是從地上來的，我是從天上來的；你們屬這世界，我卻不屬這世界，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他們說：「你們是從下頭來的，我是從上頭來的；你們是屬這世界的，我不是屬這世界的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He was saying to them, “You are from below, I am from above; you are of this world, I am not of this world."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But he continued, “You are from below; I am from above. You are of this world; I am not of this world."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus continued, “You are from below; I am from above. You belong to this world; I do not."
