    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.54",
            "paragraphs": [
              "八54猶太人認為耶穌只是引人註意。耶穌提醒他們，祂不是。只有祂的父，就是他們聲稱所愛所服侍的神，榮耀祂。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“如果我为自己争取荣耀，那荣耀算不了什么。但使我得荣耀的是我的父，你们也称祂为你们的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌回答：“如果我榮耀自己，我的榮耀就算不得甚麼；那使我得榮耀的是我的父，就是你們說是你們的　神的那一位。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌回答說：「我若榮耀自己，我的榮耀就算不得甚麼；榮耀我的乃是我的父，就是你們所說是你們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus answered, “If I glorify Myself, My glory is nothing; it is My Father who glorifies Me, of whom you say, ‘He is our God’;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus replied, “If I glorify myself, my glory means nothing. My Father, whom you claim as your God, is the one who glorifies me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus answered, “If I want glory for myself, it doesn’t count. But it is my Father who will glorify me. You say, ‘He is our God,’"
      }
    },
