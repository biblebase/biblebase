    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对信祂的犹太人说：“你们若持守我的道，就真是我的门徒了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是耶穌對信了他的猶太人說：“你們若持守我的道，就真是我的門徒了；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對信他的猶太人說：「你們若常常遵守我的道，就真是我的門徒；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Jesus was saying to those Jews who had believed Him, “If you continue in My word, then you are truly disciples of Mine;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To the Jews who had believed him, Jesus said, “If you hold to my teaching, you are really my disciples."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus said to the people who believed in him, “You are truly my disciples if you remain faithful to my teachings."
      }
    },
