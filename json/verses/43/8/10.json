    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.1-11",
            "paragraphs": [
              "六．行淫時被拿的婦人（八1～11）"
            ]
          },
          {
            "verse": "jhn.8.10",
            "paragraphs": [
              "八10滿有恩典奇妙的主耶穌，對婦人指出所有前來指控她的人都走了，去得渺無蹤影，整群人中沒有一個留下，有膽量來指責她。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e7%bd%aa%e8%88%87%e6%81%a9%e5%85%b8",
        "title": "罪與恩典",
        "date": "2005-05-22",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 8:1-11",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-17-2005-05-22.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-17-2005-05-22.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣直起腰来，说：“妇人，他们到哪里去了？没有人定你的罪吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌挺起身來，問她：“婦人，他們在哪裡？沒有人定你的罪嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌就直起腰來，對她說：「婦人，那些人在哪裏呢？沒有人定你的罪嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Straightening up, Jesus said to her, “Woman, where are they? Did no one condemn you?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus straightened up and asked her, “Woman, where are they? Has no one condemned you?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus stood up again and said to the woman, “Where are your accusers? Didn’t even one of them condemn you?”"
      }
    },
