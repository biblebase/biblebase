    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.50",
            "paragraphs": [
              "八50他們應該知道耶穌不曾求自己的榮耀。祂所做的一切都是要將榮耀歸給父神。縱使耶穌指責他們輕慢祂，祂也不是在尋求自己的榮耀。主耶穌再說，“有一位為我求榮耀、定是非的。”祂所指的那位當然就是神。父神會為祂所愛的兒子求榮耀，並且會審判那些不榮耀祂的人。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我不为自己寻求荣耀，但有一位会为我寻求，祂也会断定谁是谁非。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我不求自己的榮耀，但有一位是為我求榮耀，並且是審判人的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我不求自己的榮耀，有一位為我求榮耀、定是非的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I do not seek My glory; there is One who seeks and judges."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am not seeking glory for myself; but there is one who seeks it, and he is the judge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And though I have no wish to glorify myself, God is going to glorify me. He is the true judge."
      }
    },
