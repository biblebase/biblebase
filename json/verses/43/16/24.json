    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.16.23-28",
            "paragraphs": [
              "二十一．奉耶穌的名禱告（一六23～28）"
            ]
          },
          {
            "verse": "jhn.16.24",
            "paragraphs": [
              "一六24在這以前，門徒從沒有奉主的名向父神禱告。這時候，主叫他們去求。因父應允禱告，門徒的喜樂可以滿足了。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%9f%ba%e7%9d%a3%e5%be%92%e7%9a%84%e5%96%9c%e6%a8%82",
        "title": "基督徒的喜樂",
        "date": "2006-07-23",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 16:20-24",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-37-2006-07-23.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-37-2006-07-23.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们从来没有奉我的名求过什么，你们现在求，就必得到，好使你们的喜乐满溢。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們向來沒有奉我的名求甚麼；現在你們祈求，就必定得著，讓你們的喜樂滿溢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "向來你們沒有奉我的名求甚麼，如今你們求，就必得着，叫你們的喜樂可以滿足。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Until now you have asked for nothing in My name; ask and you will receive, so that your joy may be made full."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Until now you have not asked for anything in my name. Ask and you will receive, and your joy will be complete."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You haven’t done this before. Ask, using my name, and you will receive, and you will have abundant joy."
      }
    },
