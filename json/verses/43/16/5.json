    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.16.5-15",
            "paragraphs": [
              "十九．真理聖靈的來臨（一六5～15）"
            ]
          },
          {
            "verse": "jhn.16.5",
            "paragraphs": [
              "一六5本節似乎道出主失望的心情，門徒不大關心祂前頭要走的路。雖然他們也順帶問過祂要往哪裏去，但他們似乎卻不大意。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“现在我要去差我来的父那里，你们没有人问我去哪里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在我要到那差我來的那裡去，你們中間並沒有人問：‘你到哪裡去？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現今我往差我來的父那裏去，你們中間並沒有人問我：『你往哪裏去？』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But now I am going to Him who sent Me; and none of you asks Me, ‘Where are You going?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but now I am going to him who sent me. None of you asks me, ‘Where are you going?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But now I am going away to the one who sent me, and not one of you is asking where I am going."
      }
