    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.16.16-22",
            "paragraphs": [
              "二十．憂愁變為喜樂（一六16～22）"
            ]
          },
          {
            "verse": "jhn.16.19-20",
            "paragraphs": [
              "一六19，20主耶穌是神，能體察他們的心思意念。祂問他們的問題，正反映出祂全知，明白他們的困惱。",
              "主沒有直接回答他們的問題，只就“不多時”的情形，多給一點資料。世人將會喜樂，因為他們釘了主耶穌十字架。而門徒要痛哭、哀號，但等不多時，他們的憂愁要變為喜樂。首先有主的復活，真次有聖靈的來臨。在主再來的那刻，所有歷世歷代的信徒也將轉憂為喜。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%9f%ba%e7%9d%a3%e5%be%92%e7%9a%84%e5%96%9c%e6%a8%82",
        "title": "基督徒的喜樂",
        "date": "2006-07-23",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 16:20-24",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-37-2006-07-23.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-37-2006-07-23.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我实实在在地告诉你们，你们将痛哭、哀号，世人却要欢喜；你们将忧愁，然而你们的忧愁将变为喜乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我實實在在告訴你們，你們要痛哭哀號，世人卻要歡喜；你們要憂愁，但你們的憂愁要變為喜樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我實實在在地告訴你們，你們將要痛哭、哀號，世人倒要喜樂；你們將要憂愁，然而你們的憂愁要變為喜樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Truly, truly, I say to you, that you will weep and lament, but the world will rejoice; you will grieve, but your grief will be turned into joy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Very truly I tell you, you will weep and mourn while the world rejoices. You will grieve, but your grief will turn to joy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I tell you the truth, you will weep and mourn over what is going to happen to me, but the world will rejoice. You will grieve, but your grief will suddenly turn to wonderful joy."
      }
    },
