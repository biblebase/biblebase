    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不用别人告诉祂，祂也深知人的内心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也不需要誰指證人是怎樣的，因為他知道人心裡存的是甚麼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也用不着誰見證人怎樣，因他知道人心裏所存的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and because He did not need anyone to testify concerning man, for He Himself knew what was in man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He did not need any testimony about mankind, for he knew what was in each person."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No one needed to tell him about human nature, for he knew what was in each person’s heart."
      }
