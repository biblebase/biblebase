    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当时，犹太人质问祂：“你给我们显什么神迹来证明你有权这样做？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶太人就問他：“你可以顯甚麼神蹟給我們看，證明你有權作這些事呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此猶太人問他說：「你既做這些事，還顯甚麼神蹟給我們看呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Jews then said to Him, “What sign do You show us as your authority for doing these things?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Jews then responded to him, “What sign can you show us to prove your authority to do all this?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Jewish leaders demanded, “What are you doing? If God gave you authority to do this, show us a miraculous sign to prove it.”"
      }
    },
