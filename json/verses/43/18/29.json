    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.18.28-40",
            "paragraphs": [
              "六．耶穌在彼拉多面前受審（一八28～40）"
            ]
          },
          {
            "verse": "jhn.18.29",
            "paragraphs": [
              "一八29彼拉多這位羅馬人的官也順了猶太人的宗教法規，出到了他們那裏。他開始審訊，問他們要告這犯人什麽。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%90%9b%e7%8e%8b%e5%8f%97%e5%af%a9",
        "title": "君王受審",
        "date": "2006-10-08",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 18:1-40",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-41-2006-10-08.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-41-2006-10-08.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼拉多出来问他们：“你们控告这个人什么罪？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是彼拉多走到外面見他們，說：“你們控告這個人甚麼呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼拉多就出來，到他們那裏，說：「你們告這人是為甚麼事呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore Pilate went out to them and *said, “What accusation do you bring against this Man?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Pilate came out to them and asked, “What charges are you bringing against this man?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Pilate, the governor, went out to them and asked, “What is your charge against this man?”"
      }
    },
