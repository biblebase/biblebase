    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.18.28-40",
            "paragraphs": [
              "六．耶穌在彼拉多面前受審（一八28～40）"
            ]
          },
          {
            "verse": "jhn.18.34",
            "paragraphs": [
              "一八34耶穌回答，仿佛是說：“你是巡撫，有否聽過我要顛覆羅馬政府呢？有否聽過我自稱為王，要反該撒的帝國呢？這指控是你要告我的，還是人雲亦雲，聽那些猶太人說的呢？”"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%90%9b%e7%8e%8b%e5%8f%97%e5%af%a9",
        "title": "君王受審",
        "date": "2006-10-08",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 18:1-40",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-41-2006-10-08.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-41-2006-10-08.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣回答说：“你这样问是你自己的意思还是听别人说的？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌回答：“這話是你自己說的，還是別人對你說到我的呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌回答說：「這話是你自己說的，還是別人論我對你說的呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus answered, “Are you saying this on your own initiative, or did others tell you about Me?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Is that your own idea,” Jesus asked, “or did others talk to you about me?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “Is this your own question, or did others tell you about me?”"
      }
