    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看门的女仆问彼得：“你不也是这个人的门徒吗？” 他说：“我不是。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那看門的婢女對彼得說：“你不也是這個人的門徒嗎？”他說：“我不是。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那看門的使女對彼得說：「你不也是這人的門徒嗎？」他說：「我不是。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the slave-girl who kept the door *said to Peter, “You are not also one of this man’s disciples, are you?” He *said, “I am not.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You aren’t one of this man’s disciples too, are you?” she asked Peter. He replied, “I am not.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The woman asked Peter, “You’re not one of that man’s disciples, are you?” “No,” he said, “I am not.”"
      }
