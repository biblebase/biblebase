    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣早就知道将要发生在自己身上的一切事，于是出来问他们：“你们找谁？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌知道快要臨到他身上的一切事，就出來對他們說：“你們找誰？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣知道就要临到自己的一切，于是上前对他们说：“你们找谁？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌知道將要臨到自己的一切事，就出來對他們說：「你們找誰？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Jesus, knowing all that would happen to him, came forward and said to them, “Whom do you seek?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Jesus, knowing all the things that were coming upon Him, went forth and *said to them, “Whom do you seek?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus, knowing all that was going to happen to him, went out and asked them, “Who is it you want?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Jesus therefore, knowing all things that would come upon Him, went forward and said to them, “Whom are you seeking?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus fully realized all that was going to happen to him, so he stepped forward to meet them. “Who are you looking for?” he asked."
      }
    },
