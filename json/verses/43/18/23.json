    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.18.19-24",
            "paragraphs": [
              "四．大祭司盤問耶穌（一八19～24）"
            ]
          },
          {
            "verse": "jhn.18.23",
            "paragraphs": [
              "一八23主耶穌以不卑不亢的神情，理直氣壯的邏輯，證明自己是含冤的，告祂的人是不實不正的。他們不能告祂說惡言，卻因祂說的是真話，就對祂動粗。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%90%9b%e7%8e%8b%e5%8f%97%e5%af%a9",
        "title": "君王受審",
        "date": "2006-10-08",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 18:1-40",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-41-2006-10-08.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-41-2006-10-08.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“如果我说错了，你可以指出我错在哪里。如果我说的对，你为什么打我呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對他說：“如果我講錯了，你可以指證錯在哪裡；如果我講對了，你為甚麼打我呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「我若說的不是，你可以指證那不是；我若說的是，你為甚麼打我呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus answered him, “If I have spoken wrongly, testify of the wrong; but if rightly, why do you strike Me?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If I said something wrong,” Jesus replied, “testify as to what is wrong. But if I spoke the truth, why did you strike me?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “If I said anything wrong, you must prove it. But if I’m speaking the truth, why are you beating me?”"
      }
    },
