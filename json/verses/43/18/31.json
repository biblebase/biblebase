    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼拉多说：“你们把祂带走，按照你们的律法去审理吧。” 犹太人说：“可是我们无权把人处死。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼拉多對他們說：“你們自己把他帶去，按著你們的律法審問他吧。”猶太人說：“我們沒有權去判人死罪。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "彼拉多对他们说：“你们自己把他带走，按照你们的律法审判他吧。” 那些犹太人回答说：“我们不可以处死人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼拉多說：「你們自己帶他去，按着你們的律法審問他吧。」猶太人說：「我們沒有殺人的權柄。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Pilate said to them, “Take him yourselves and judge him by your own law.” The Jews said to him, “It is not lawful for us to put anyone to death.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Pilate said to them, “Take Him yourselves, and judge Him according to your law.” The Jews said to him, “We are not permitted to put anyone to death,”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Pilate said, “Take him yourselves and judge him by your own law.” “But we have no right to execute anyone,” they objected."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Pilate said to them, “You take Him and judge Him according to your law.” Therefore the Jews said to him, “It is not lawful for us to put anyone to death,”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then take him away and judge him by your own law,” Pilate told them. “Only the Romans are permitted to execute someone,” the Jewish leaders replied."
      }
    },
