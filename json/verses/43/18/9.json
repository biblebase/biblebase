    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这是要应验祂以前说的：“你赐给我的人一个也没有失掉。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這應驗了耶穌說過的話：“你賜給我的人，我一個也沒有失落。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这是为要应验耶稣说过的话：“你所赐给我的人，我一个也没有丢失。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這要應驗耶穌從前的話，說：「你所賜給我的人，我沒有失落一個。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "This was to fulfill the word that he had spoken: “Of those whom you gave me I have lost not one.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "to fulfill the word which He spoke, “Of those whom You have given Me I lost not one.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This happened so that the words he had spoken would be fulfilled: “I have not lost one of those you gave me.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "that the saying might be fulfilled which He spoke, “Of those whom You gave Me I have lost none.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He did this to fulfill his own statement: “I did not lose a single one of those you have given me.”"
      }
