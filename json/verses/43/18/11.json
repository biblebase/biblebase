    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对彼得说：“收刀入鞘吧！我父赐给我的杯，我怎能不喝呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對彼得說：“把刀收入鞘裡去！父給我的杯，我怎能不喝呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌就對彼得說：「收刀入鞘吧，我父所給我的那杯，我豈可不喝呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Jesus said to Peter, “Put the sword into the sheath; the cup which the Father has given Me, shall I not drink it?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus commanded Peter, “Put your sword away! Shall I not drink the cup the Father has given me?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jesus said to Peter, “Put your sword back into its sheath. Shall I not drink from the cup of suffering the Father has given me?”"
      }
    },
