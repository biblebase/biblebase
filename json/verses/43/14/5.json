    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.14.1-14",
            "paragraphs": [
              "十三．耶穌是道路、真理、生命（一四1～14）"
            ]
          },
          {
            "verse": "jhn.14.4-5",
            "paragraphs": [
              "一四4，5主將要回到天上，他們也知道去天家那條路，因主已多次告訴他們。",
              "多馬顯然不明主所指，如彼得一樣，他以為主耶穌要到世上某處地方。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "多马说：“主啊，你要去哪里，我们还不知道，又怎么会知道路呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "多馬說：“主啊，我們不知道你去的地方，怎能知道那條路呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "多馬對他說：「主啊，我們不知道你往哪裏去，怎麼知道那條路呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thomas *said to Him, “Lord, we do not know where You are going, how do we know the way?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Thomas said to him, “Lord, we don’t know where you are going, so how can we know the way?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“No, we don’t know, Lord,” Thomas said. “We have no idea where you are going, so how can we know the way?”"
      }
    },
