    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.19.1-16",
            "paragraphs": [
              "七．彼拉多的判詞：無罪但要受刑（一九1～16）"
            ]
          },
          {
            "verse": "jhn.19.10",
            "paragraphs": [
              "一九10彼拉多試圖要脅主，叫祂回答，他恃自己是羅馬人的官，操放祂、或是釘祂十字架的權柄。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%8d%81%e5%ad%97%e6%9e%b6",
        "title": "十字架",
        "date": "2006-10-22",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 19:1-30",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-43-2006-10-22.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼拉多说：“你不回答我吗？难道你不知道我有权释放你，也有权把你钉在十字架上吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼拉多對他說：“你不對我說話嗎？你不知道我有權釋放你，也有權把你釘十字架嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼拉多說：「你不對我說話嗎？你豈不知我有權柄釋放你，也有權柄把你釘十字架嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Pilate *said to Him, “You do not speak to me? Do You not know that I have authority to release You, and I have authority to crucify You?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do you refuse to speak to me?” Pilate said. “Don’t you realize I have power either to free you or to crucify you?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why don’t you talk to me?” Pilate demanded. “Don’t you realize that I have the power to release you or crucify you?”"
      }
    },
