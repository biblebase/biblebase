    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹太人的祭司长对彼拉多说：“不要写‘犹太人的王’，应该写‘这人自称是犹太人的王’。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶太人的祭司長對彼拉多說：“不要寫‘猶太人的王’，要寫‘這個人自稱：我是猶太人的王’。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶太人的祭司長就對彼拉多說：「不要寫『猶太人的王』，要寫『他自己說：我是猶太人的王』。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the chief priests of the Jews were saying to Pilate, “Do not write, ‘The King of the Jews’; but that He said, ‘I am King of the Jews.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The chief priests of the Jews protested to Pilate, “Do not write ‘The King of the Jews,’ but that this man claimed to be king of the Jews.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the leading priests objected and said to Pilate, “Change it from ‘The King of the Jews’ to ‘He said, I am King of the Jews.’”"
      }
    },
