    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "永活的父差我来，我是靠祂而活。同样，吃我肉的人也靠我而活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正如永活的父差遣了我，我也因父活著；照樣，吃我肉的人也必因我而活。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "永生的父怎样差派了我，我又因父而活，照样，以我为食的人也将因我而活。"
