    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "永活的父差我来，我是靠祂而活。同样，吃我肉的人也靠我而活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正如永活的父差遣了我，我也因父活著；照樣，吃我肉的人也必因我而活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "永活的父怎樣差我來，我又因父活着；照樣，吃我肉的人也要因我活着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As the living Father sent Me, and I live because of the Father, so he who eats Me, he also will live because of Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Just as the living Father sent me and I live because of the Father, so the one who feeds on me will live because of me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I live because of the living Father who sent me; in the same way, anyone who feeds on me will live because of me."
      }
    },
