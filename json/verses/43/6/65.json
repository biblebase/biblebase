    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂继续说：“所以我曾对你们说，如果不是我父赐恩，没有人能到我这里来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌跟著說：“所以我對你們說過，如果不是父所賜的，沒有人能到我這裡來。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣又说：“所以我告诉过你们：如果不是蒙父所赐，没有人能到我这里来。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌又說：「所以我對你們說過，若不是蒙我父的恩賜，沒有人能到我這裏來。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he said, “This is why I told you that no one can come to me unless it is granted him by the Father.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He was saying, “For this reason I have said to you, that no one can come to Me unless it has been granted him from the Father.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He went on to say, “This is why I told you that no one can come to me unless the Father has enabled them.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And He said, “Therefore I have said to you that no one can come to Me unless it has been granted to him by My Father.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said, “That is why I said that people can’t come to me unless the Father gives them to me.”"
      }
    },
