    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹太人开始彼此争论，说：“这个人怎么能把自己的肉给我们吃呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是，猶太人彼此爭論，說：“這個人怎能把他的肉給我們吃呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，猶太人彼此爭論說：「這個人怎能把他的肉給我們吃呢？」"
      },
      "nasb": {
