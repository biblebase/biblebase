    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "先知书上这样说，‘他们都要受上帝的训诲’，这里是指凡听从父的教导又去效法的人，都会到我这里来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "先知書上記著：‘眾人都必受　神的教導。’凡聽見從父那裡來的教訓而又學習的，必到我這裡來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在先知書上寫着說：『他們都要蒙神的教訓。』凡聽見父之教訓又學習的，就到我這裏來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is written in the prophets, ‘And they shall all be taught of God.’ Everyone who has heard and learned from the Father, comes to Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is written in the Prophets: ‘They will all be taught by God.’  Everyone who has heard the Father and learned from him comes to me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As it is written in the Scriptures, ‘They will all be taught by God.’ Everyone who listens to the Father and learns from him comes to me."
      }
