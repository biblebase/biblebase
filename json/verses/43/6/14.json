    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大家看见耶稣行的这个神迹，都说：“这人真是那位要来到世上的先知！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人看見耶穌所行的神蹟，就說：“這真是那要到世上來的先知。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "众人看见耶稣所行的神迹，就说：“这个人确实是那位要来到世上的先知！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人看見耶穌所行的神蹟，就說：「這真是那要到世間來的先知！」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When the people saw the sign that he had done, they said, “This is indeed the Prophet who is to come into the world!”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore when the people saw the sign which He had performed, they said, “This is truly the Prophet who is to come into the world.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After the people saw the sign Jesus performed, they began to say, “Surely this is the Prophet who is to come into the world.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then those men, when they had seen the sign that Jesus did, said, “This is truly the Prophet who is to come into the world.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the people saw him do this miraculous sign, they exclaimed, “Surely, he is the Prophet we have been expecting!”"
      }
