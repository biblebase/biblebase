    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为我从天上下来，不是要成就自己的旨意，而是要成就差我来者的旨意。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為我從天上降下來，不是要行自己的意思，而是要行那差我來者的旨意。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為我從天上降下來，不是要按自己的意思行，乃是要按那差我來者的意思行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I have come down from heaven, not to do My own will, but the will of Him who sent Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I have come down from heaven not to do my will but to do the will of him who sent me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I have come down from heaven to do the will of God who sent me, not to do my own will."
      }
