    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.12.44-50",
            "paragraphs": [
              "七．不信的危機（一二44～50）"
            ]
          },
          {
            "verse": "jhn.12.48",
            "paragraphs": [
              "一二48主論到將來那些拒絕祂道的人將要在神面前受審判。那時，單是主耶穌所說的道已足夠定他們的罪。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%9f%ba%e7%9d%a3%e5%be%92%e7%94%9f%e6%b4%bb%e7%9a%84%e7%a7%98%e8%a8%a3",
        "title": "基督徒生活的秘訣",
        "date": "2005-10-09",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 12:20-50",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-25-2005-10-09.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-25-2005-10-09.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "弃绝我、不接受我话的人将受到审判，我讲过的道在末日要审判他，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "棄絕我又不接受我的話的人，自有審判他的。我所講的道在末日要定他的罪；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "棄絕我、不領受我話的人，有審判他的－就是我所講的道在末日要審判他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who rejects Me and does not receive My sayings, has one who judges him; the word I spoke is what will judge him at the last day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There is a judge for the one who rejects me and does not accept my words; the very words I have spoken will condemn them at the last day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But all who reject me and my message will be judged on the day of judgment by the truth I have spoken."
      }
    },
