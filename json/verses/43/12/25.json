        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "爱惜自己生命的，必会失掉生命；憎恶自己今世生命的，才能保住生命，直到永生。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愛惜自己生命的，就喪掉生命；在這世上恨惡自己生命的，必會保全生命到永遠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愛惜自己生命的，就失喪生命；在這世上恨惡自己生命的，就要保守生命到永生。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who loves his life loses it, and he who hates his life in this world will keep it to life eternal."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who loves their life will lose it, while anyone who hates their life in this world will keep it for eternal life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who love their life in this world will lose it. Those who care nothing for their life in this world will keep it for eternity."
      }
