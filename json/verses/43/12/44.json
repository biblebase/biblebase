    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣高声说：“信我的，其实不只是信我，而是信差我来的那位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌大聲說：“信我的，不單是信我，也是信那差我來的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌大聲說：「信我的，不是信我，乃是信那差我來的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus cried out and said, “He who believes in Me, does not believe in Me but in Him who sent Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jesus cried out, “Whoever believes in me does not believe in me only, but in the one who sent me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus shouted to the crowds, “If you trust me, you are trusting not only me, but also God who sent me."
      }
    },
