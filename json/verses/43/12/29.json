    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "站在那里的人群中有人听见就说：“打雷了！”也有人说：“是天使在跟祂说话。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "站在旁邊的群眾聽見了，就說：“打雷了。”另外有人說：“有天使對他說話。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "站在旁邊的眾人聽見，就說：「打雷了。」還有人說：「有天使對他說話。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the crowd of people who stood by and heard it were saying that it had thundered; others were saying, “An angel has spoken to Him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The crowd that was there and heard it said it had thundered; others said an angel had spoken to him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the crowd heard the voice, some thought it was thunder, while others declared an angel had spoken to him."
      }
