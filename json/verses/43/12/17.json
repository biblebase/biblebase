    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.12.12-19",
            "paragraphs": [
              "三．勝利地進耶路撒冷（一二12～19）"
            ]
          },
          {
            "verse": "jhn.12.17-18",
            "paragraphs": [
              "一二17，18看耶穌進耶路撒冷的眾人當中，有的曾看見祂呼喚拉撒路……從死復活。他們告訴身旁的人，這騎在驢駒上的就是當日叫拉撒路復活的那位。這件神跡，引人註目，眾人得知之後，便蜂擁出來迎接耶穌。可惜他們沒有真正相信祂，他們來見主，只因為好奇。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些亲眼看见耶稣叫拉撒路复活、走出坟墓的人不断传扬这件事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些和耶穌在一起，看見他叫拉撒路從墳墓出來，又使他從死人中復活的群眾，都為這事作見證。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "当耶稣呼唤拉撒路从坟墓中出来，使他从死人中复活的时候，那些与耶稣在一起的众人就见证了这件事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當耶穌呼喚拉撒路，叫他從死復活出墳墓的時候，同耶穌在那裏的眾人就作見證。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The crowd that had been with him when he called Lazarus out of the tomb and raised him from the dead continued to bear witness."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the people, who were with Him when He called Lazarus out of the tomb and raised him from the dead, continued to testify about Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the crowd that was with him when he called Lazarus from the tomb and raised him from the dead continued to spread the word."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore the people, who were with Him when He called Lazarus out of his tomb and raised him from the dead, bore witness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Many in the crowd had seen Jesus call Lazarus from the tomb, raising him from the dead, and they were telling others about it."
      }
    },
