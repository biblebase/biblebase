    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他回答说：“有一位叫耶稣的人和泥抹我的眼睛，叫我到西罗亚池子去洗。我照着去做，眼睛就能看见了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他回答：“那名叫耶穌的人和了一點泥，抹在我的眼上，對我說：‘你去西羅亞池洗一洗吧。’我去一洗，就看見了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他回答說：「有一個人，名叫耶穌，他和泥抹我的眼睛，對我說：『你往西羅亞池子去洗。』我去一洗，就看見了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He answered, “The man who is called Jesus made clay, and anointed my eyes, and said to me, ‘Go to Siloam and wash’; so I went away and washed, and I received sight.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He replied, “The man they call Jesus made some mud and put it on my eyes. He told me to go to Siloam and wash. So I went and washed, and then I could see.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He told them, “The man they call Jesus made mud and spread it over my eyes and told me, ‘Go to the pool of Siloam and wash yourself.’ So I went and washed, and now I can see!”"
      }
    },
