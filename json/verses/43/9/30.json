    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那人说：“祂开了我的眼睛，你们竟不知道祂从哪里来，真是奇怪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那人對他們說：“這就奇怪了，他開了我的眼睛，你們竟然不知道他從哪裡來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那人回答說：「他開了我的眼睛，你們竟不知道他從哪裏來，這真是奇怪！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The man answered and said to them, “Well, here is an amazing thing, that you do not know where He is from, and yet He opened my eyes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The man answered, “Now that is remarkable! You don’t know where he comes from, yet he opened my eyes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why, that’s very strange!” the man replied. “He healed my eyes, and yet you don’t know where he comes from?"
      }
    },
