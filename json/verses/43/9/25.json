    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他说：“祂是不是罪人，我不知道；我只知道从前我是瞎眼的，现在能看见了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那人回答：“他是不是個罪人，我不知道；我只知道一件事，就是我本來是瞎眼的，現在能看見了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：「他是個罪人不是，我不知道；有一件事我知道，從前我是眼瞎的，如今能看見了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He then answered, “Whether He is a sinner, I do not know; one thing I do know, that though I was blind, now I see.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He replied, “Whether he is a sinner or not, I don’t know. One thing I do know. I was blind but now I see!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I don’t know whether he is a sinner,” the man replied. “But I know this: I was blind, and now I can see!”"
      }
