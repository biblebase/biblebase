    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们知道上帝不听罪人的祷告，只听那些敬拜祂、遵行祂旨意者的祷告。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們知道　神不聽罪人的祈求，只聽那敬畏　神，遵行他旨意的人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我们知道神是不听罪人的，然而，如果有人敬畏神、遵行他的旨意，神就垂听他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們知道神不聽罪人，惟有敬奉神、遵行他旨意的，神才聽他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "We know that God does not listen to sinners, but if anyone is a worshiper of God and does his will, God listens to him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We know that God does not hear sinners; but if anyone is God-fearing and does His will, He hears him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We know that God does not listen to sinners. He listens to the godly person who does his will."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now we know that God does not hear sinners; but if anyone is a worshiper of God and does His will, He hears him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We know that God doesn’t listen to sinners, but he is ready to hear those who worship him and do his will."
      }
