    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.10-31",
            "paragraphs": [
              "二．耶穌在殿裏教訓人（七10～31）"
            ]
          },
          {
            "verse": "jhn.7.20",
            "paragraphs": [
              "七20眾人聽了主耶穌這番話後，如坐針氈，不但沒有承認祂的話是對的，反而更詆毀祂。他們稱祂是被鬼附，並反問當中有誰想要殺祂。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人回答说：“你一定是被鬼附身了！谁要杀你啊？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "群眾回答：“你是鬼附的，誰想要殺你呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人回答說：「你是被鬼附着了！誰想要殺你？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The crowd answered, “You have a demon! Who seeks to kill You?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You are demon-possessed,” the crowd answered. “Who is trying to kill you?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The crowd replied, “You’re demon possessed! Who’s trying to kill you?”"
      }
