    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.10-31",
            "paragraphs": [
              "二．耶穌在殿裏教訓人（七10～31）"
            ]
          },
          {
            "verse": "jhn.7.25",
            "paragraphs": [
              "七25當時，猶太領袖想要殺害耶穌的事在耶路撒冷眾所周知。這裏，一些平民查問這是否就是他們領袖想要殺的人。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有些住在耶路撒冷的人说：“这不是他们要杀的那个人吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當時，有些耶路撒冷人說：“這不是他們想要殺的人嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷人中有的說：「這不是他們想要殺的人嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So some of the people of Jerusalem were saying, “Is this not the man whom they are seeking to kill?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At that point some of the people of Jerusalem began to ask, “Isn’t this the man they are trying to kill?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some of the people who lived in Jerusalem started to ask each other, “Isn’t this the man they are trying to kill?"
      }
    },
