    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.10-31",
            "paragraphs": [
              "二．耶穌在殿裏教訓人（七10～31）"
            ]
          },
          {
            "verse": "jhn.7.13",
            "paragraphs": [
              "七13猶太人領袖極力反對耶穌，嚇得無人敢明明稱賞他。很多平民百姓無疑是相信了主耶穌是以色列真正的彌賽亞，但又不敢出來表白，因為怕這些領袖會逼迫他們。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e6%98%8e%e7%99%bd%e7%a5%9e%e7%9a%84%e6%97%a8%e6%84%8f",
        "title": "明白神的旨意",
        "date": "2005-05-15",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 7:11-18",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-16-2005-05-15.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-16-2005-05-15.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不过，没有人敢公开议论祂的事，因为害怕犹太人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但沒有人敢公開講論他，因為怕猶太人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是沒有人明明地講論他，因為怕猶太人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet no one was speaking openly of Him for fear of the Jews."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But no one would say anything publicly about him for fear of the leaders."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But no one had the courage to speak favorably about him in public, for they were afraid of getting in trouble with the Jewish leaders."
      }
    },
