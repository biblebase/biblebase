    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.40-53",
            "paragraphs": [
              "五．有關主耶穌的紛論（七40～53）"
            ]
          },
          {
            "verse": "jhn.7.46",
            "paragraphs": [
              "七46差役雖是罪人，也不接受耶穌，但此刻也不得不為祂講一番好說話。他們這番話，值得我們去記。“從來沒有像他這樣說話的。”這些差役已往一定聽過很多好人和偉人說話，但卻沒有一個說話時像耶穌一樣有權柄、恩典和智慧。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "差役答道：“从未有人像祂那样说话！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "差役回答：“從來沒有人像他這樣講話的！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "差役回答說：「從來沒有像他這樣說話的！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The officers answered, “Never has a man spoken the way this man speaks.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“No one ever spoke the way this man does,” the guards replied."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“We have never heard anyone speak like this!” the guards responded."
      }
