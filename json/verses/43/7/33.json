    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.32-36",
            "paragraphs": [
              "三．法利賽人的敵意（七32～36）"
            ]
          },
          {
            "verse": "jhn.7.33",
            "paragraphs": [
              "七33本節的那番話當然是對來捉拿耶穌的差役說的，同時也是對法利賽人和其他百姓說的。",
              "主耶穌並非作退讓，祂不過是更堅持。祂告訴他們，祂和他們同在還有不多的時候，然後祂會回到差祂來的父神那裏。這番話無疑會更惹法利賽人的怒氣。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对众人说：“我暂时还会跟你们在一起，不久我要回到差我来的那位那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是耶穌說：“我跟你們在一起的時候不多了，我要回到那差我來的那裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是耶穌說：「我還有不多的時候和你們同在，以後就回到差我來的那裏去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore Jesus said, “For a little while longer I am with you, then I go to Him who sent Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus said, “I am with you for only a short time, and then I am going to the one who sent me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jesus told them, “I will be with you only a little longer. Then I will return to the one who sent me."
      }
    },
