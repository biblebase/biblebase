    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.21.18-23",
            "paragraphs": [
              "三．耶穌預言彼得的死（二一18～23）"
            ]
          },
          {
            "verse": "jhn.21.21",
            "paragraphs": [
              "二一21彼得看見約翰，頓時想到：“約翰又如何呢？”他是否也會殉道呢？還是會活，直等到主再來呢？他這樣問主有關約翰的將來。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e4%b8%bb%e5%95%8a-%e4%bd%a0%e7%9f%a5%e9%81%93%e6%88%91%e6%84%9b%e4%bd%a0",
        "title": "主啊, 你知道我愛你!",
        "date": "2006-12-17",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 21:15-25",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-48-2006-12-17.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-48-2006-12-17.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼得问耶稣：“主啊！他将来会怎样呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼得看見他，就問耶穌：“主啊，這個人將來怎麼樣？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼得看見他，就問耶穌說：「主啊，這人將來如何？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Peter seeing him *said to Jesus, “Lord, and what about this man?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Peter saw him, he asked, “Lord, what about him?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Peter asked Jesus, “What about him, Lord?”"
      }
    },
