    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.21.18-23",
            "paragraphs": [
              "三．耶穌預言彼得的死（二一18～23）"
            ]
          },
          {
            "verse": "jhn.21.20",
            "paragraphs": [
              "二一20彼得似也起步跟從耶穌，轉過身來，看見約翰也跟。這裏約翰提筆表明自己的身分，是那位在逾越節晚飯的時候，靠耶穌的胸膛，問主賣祂的人是誰的那位。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e4%b8%bb%e5%95%8a-%e4%bd%a0%e7%9f%a5%e9%81%93%e6%88%91%e6%84%9b%e4%bd%a0",
        "title": "主啊, 你知道我愛你!",
        "date": "2006-12-17",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 21:15-25",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-48-2006-12-17.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-48-2006-12-17.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼得转身看见耶稣所爱的那个门徒跟在后面，就是吃最后的晚餐时靠在耶稣身边问“主啊！是谁要出卖你？”的那个门徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼得轉過身來，看見耶穌所愛的那門徒跟著，就是在晚飯的時候，靠著耶穌的胸膛，問“主啊，出賣你的是誰？”的那個人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼得轉過來，看見耶穌所愛的那門徒跟着，（就是在晚飯的時候，靠着耶穌胸膛說：「主啊，賣你的是誰？」的那門徒。）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Peter, turning around, *saw the disciple whom Jesus loved following them; the one who also had leaned back on His bosom at the supper and said, “Lord, who is the one who betrays You?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Peter turned and saw that the disciple whom Jesus loved was following them. (This was the one who had leaned back against Jesus at the supper and had said, “Lord, who is going to betray you?”)"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Peter turned around and saw behind them the disciple Jesus loved—the one who had leaned over to Jesus during supper and asked, “Lord, who will betray you?”"
      }
    },
