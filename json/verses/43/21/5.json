    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.21.1-14",
            "paragraphs": [
              "一．基督在加利利向祂的門徒顯現（二一1～14）"
            ]
          },
          {
            "verse": "jhn.21.5",
            "paragraphs": [
              "二一5主問門徒：“小子，你們有吃的沒有？”他們失望的門徒回答說：“沒有。”"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
