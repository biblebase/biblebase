      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对他们说：“孩子们，打到鱼没有？” 他们回答说：“没有！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對他們說：“孩子們，打到魚沒有？”他們回答：“沒有。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌就對他們說：「小子！你們有吃的沒有？」他們回答說：「沒有。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Jesus *said to them, “Children, you do not have any fish, do you?” They answered Him, “No.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He called out to them, “Friends, haven’t you any fish?” “No,” they answered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He called out, “Fellows, have you caught any fish?” “No,” they replied."
      }
