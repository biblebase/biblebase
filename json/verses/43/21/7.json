    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.21.1-14",
            "paragraphs": [
              "一．基督在加利利向祂的門徒顯現（二一1～14）"
            ]
          },
          {
            "verse": "jhn.21.7",
            "paragraphs": [
              "二一7約翰第一個認出是主，立即告訴彼得。彼得束上一件外衣，就急急回岸。我們不知他是遊，是涉，還是（如一些人所說）在水面上走回岸上的。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e6%b5%b7%e9%82%8a%e7%9a%84%e6%97%a9%e9%a4%90",
        "title": "海邊的早餐",
        "date": "2006-12-10",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 21:1-14",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-47-2006-12-10.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-47-2006-12-10.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣所爱的那个门徒对彼得说：“是主！”那时西门·彼得赤着身子，一听见是主，立刻束上外衣，跳进海里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌所愛的那門徒對彼得說：“是主！”西門．彼得一聽見是主，就立刻束上外衣，縱身跳進海裡。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣所爱的那门徒对彼得说：“那是主！” 当时，西门彼得赤着身子，一听说是主，就把外衣系上，跳进湖里。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌所愛的那門徒對彼得說：「是主！」那時西門‧彼得赤着身子，一聽見是主，就束上一件外衣，跳在海裏。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "That disciple whom Jesus loved therefore said to Peter, “It is the Lord!” When Simon Peter heard that it was the Lord, he put on his outer garment, for he was stripped for work, and threw himself into the sea."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore that disciple whom Jesus loved *said to Peter, “It is the Lord.” So when Simon Peter heard that it was the Lord, he put his outer garment on (for he was stripped for work), and threw himself into the sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the disciple whom Jesus loved said to Peter, “It is the Lord!” As soon as Simon Peter heard him say, “It is the Lord,” he wrapped his outer garment around him (for he had taken it off) and jumped into the water."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore that disciple whom Jesus loved said to Peter, “It is the Lord!” Now when Simon Peter heard that it was the Lord, he put on his outer garment (for he had removed it), and plunged into the sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the disciple Jesus loved said to Peter, “It’s the Lord!” When Simon Peter heard that it was the Lord, he put on his tunic (for he had stripped for work), jumped into the water, and headed to shore."
      }
    },
