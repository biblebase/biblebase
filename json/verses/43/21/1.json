    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jhn.21.1",
            "paragraphs": [
              "21:1",
              "又向門徒顯現：參太28:9，關於基督復活後顯現的註釋。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "matangna": {
        "content": [
          {
            "verse": "jhn.21.1-14",
            "paragraphs": [
              "一．基督在加利利向祂的門徒顯現（二一1～14）"
            ]
          },
          {
            "verse": "jhn.21.1",
            "paragraphs": [
              "二一1筆頭一轉，來到提比哩亞（加利利）海邊。門徒北返回加利利的家鄉，主耶穌在那裏會見他們。他怎樣顯現記在下面，是說約翰將要陳述的，就是基督向門徒顯現的情形。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e6%b5%b7%e9%82%8a%e7%9a%84%e6%97%a9%e9%a4%90",
        "title": "海邊的早餐",
        "date": "2006-12-10",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 21:1-14",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-47-2006-12-10.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-47-2006-12-10.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来，耶稣在提比哩亚海边又向门徒显现。当时的情形是这样："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些事以後，耶穌在提比里亞海邊再次向門徒顯現。他顯現的經過是這樣的："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些事以後，耶穌在提比哩亞海邊又向門徒顯現。他怎樣顯現記在下面："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After these things Jesus manifested Himself again to the disciples at the Sea of Tiberias, and He manifested Himself in this way."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Afterward Jesus appeared again to his disciples, by the Sea of Galilee. It happened this way:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Later, Jesus appeared again to the disciples beside the Sea of Galilee. This is how it happened."
      }
    },
