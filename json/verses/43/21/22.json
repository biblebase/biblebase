    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.21.18-23",
            "paragraphs": [
              "三．耶穌預言彼得的死（二一18～23）"
            ]
          },
          {
            "verse": "jhn.21.22",
            "paragraphs": [
              "二一22主叫彼得不要管約翰將來的事情，即使他會活，等到基督第二次來臨，也與彼得無關。彼得不會因此而有任何改變。現今多人事奉基督失敗，是因為他們太多管他人的事，而忽略了主。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e4%b8%bb%e5%95%8a-%e4%bd%a0%e7%9f%a5%e9%81%93%e6%88%91%e6%84%9b%e4%bd%a0",
        "title": "主啊, 你知道我愛你!",
        "date": "2006-12-17",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 21:15-25",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-48-2006-12-17.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-48-2006-12-17.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“如果我要他活到我再来，与你有什么关系？你只管跟从我吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌回答他：“如果我要他活到我來的時候，跟你有甚麼關係呢？你只管跟從我吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他說：「我若要他等到我來的時候，與你何干？你跟從我吧！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus *said to him, “If I want him to remain until I come, what is that to you? You follow Me!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus answered, “If I want him to remain alive until I return, what is that to you? You must follow me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “If I want him to remain alive until I return, what is that to you? As for you, follow me.”"
      }
    },
