    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一群犹太人围着祂说：“你让我们困惑、猜疑要到什么时候呢？如果你是基督，就清楚地告诉我们吧！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶太人圍著他，對他說：“你使我們心裡懸疑不定，要到幾時呢？如果你是基督，就公開地告訴我們吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶太人圍着他，說：「你叫我們猶疑不定到幾時呢？你若是基督，就明明地告訴我們。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Jews then gathered around Him, and were saying to Him, “How long will You keep us in suspense? If You are the Christ, tell us plainly.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Jews who were there gathered around him, saying, “How long will you keep us in suspense? If you are the Messiah, tell us plainly.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people surrounded him and asked, “How long are you going to keep us in suspense? If you are the Messiah, tell us plainly.”"
      }
    },
