    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "领出来之后，自己便走在前面，羊群也跟着他，因为羊认得他的声音。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把自己的羊領出來以後，就走在前頭，羊也跟隨他，因為認得他的聲音。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "既放出自己的羊來，就在前頭走，羊也跟着他，因為認得他的聲音。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he puts forth all his own, he goes ahead of them, and the sheep follow him because they know his voice."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he has brought out all his own, he goes on ahead of them, and his sheep follow him because they know his voice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After he has gathered his own flock, he walks ahead of them, and they follow him because they know his voice."
      }
    },
