    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.3.1-21",
            "paragraphs": [
              "八．耶穌教尼哥底母重生的道理（三1～21）"
            ]
          },
          {
            "verse": "jhn.3.21",
            "paragraphs": [
              "三21人若在神前全然誠實，他必來就光，到主耶穌面前，並察覺自己滿身罪惡，不配受任何恩典。然後他會將自己交托於救主，因信基督而重生。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e4%ba%ba%e7%94%9f%e7%9a%84%e4%b8%89%e9%a0%85%e6%8a%89%e6%93%87",
