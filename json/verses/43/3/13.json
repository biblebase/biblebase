        "lang": "cht",
        "text": "除了從天降下、仍舊在天的人子，沒有人升過天。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No one has ascended into heaven, but He who descended from heaven: the Son of Man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one has ever gone into heaven except the one who came from heaven—the Son of Man."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No one has ever gone to heaven and returned. But the Son of Man has come down from heaven."
      }
    },
