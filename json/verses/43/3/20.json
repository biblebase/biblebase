    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.3.1-21",
            "paragraphs": [
              "八．耶穌教尼哥底母重生的道理（三1～21）"
            ]
          },
          {
            "verse": "jhn.3.20",
            "paragraphs": [
              "三20愛罪的人恨光，惟恐真罪行敗露。當主耶穌在地上時，作惡的人見到祂就不自在，因為祂的聖潔顯出人的罪行。像一根筆直的桿子能顯出其他桿子的彎彎曲曲，主耶穌這個完全人來到世上，很容易便顯出人類的不正。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e4%ba%ba%e7%94%9f%e7%9a%84%e4%b8%89%e9%a0%85%e6%8a%89%e6%93%87",
        "title": "人生的三項抉擇",
        "date": "2005-01-09",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 3:17-35",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-06-2005-01-09.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-06-2005-01-09.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "作恶的人恨光，不肯接近光，恐怕他们的罪行暴露出来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡作惡的都恨光，不來接近光，免得他的惡行暴露出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡作惡的便恨光，並不來就光，恐怕他的行為受責備。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For everyone who does evil hates the Light, and does not come to the Light for fear that his deeds will be exposed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Everyone who does evil hates the light, and will not come into the light for fear that their deeds will be exposed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All who do evil hate the light and refuse to go near it for fear their sins will be exposed."
      }
