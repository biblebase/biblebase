    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.3.1-21",
            "paragraphs": [
              "八．耶穌教尼哥底母重生的道理（三1～21）"
            ]
          },
          {
            "verse": "jhn.3.7",
            "paragraphs": [
              "三7尼哥底母不應以主耶穌的教訓為希奇，他一定要知道人必須重生，否則，人是完全無法改變其敗壞的本質。況且，一個神國的子民一定要是神聖、純潔及屬靈的。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以我说你们必须重生，你不要惊奇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不要因為我對你說‘你們必須重生’而感到希奇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我說：『你們必須重生』，你不要以為希奇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not be amazed that I said to you, ‘You must be born again.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You should not be surprised at my saying, ‘You  must be born again.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So don’t be surprised when I say, ‘You must be born again.’"
      }
    },
