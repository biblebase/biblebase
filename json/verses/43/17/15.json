    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我不求你带他们离开这个世界，但求你保守他们脱离那恶者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我不求你使他們離開世界，只求你保守他們脫離那惡者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我不求你叫他們離開世界，只求你保守他們脫離那惡者。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I do not ask You to take them out of the world, but to keep them from the evil one."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My prayer is not that you take them out of the world but that you protect them from the evil one."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I’m not asking you to take them out of the world, but to keep them safe from the evil one."
      }
