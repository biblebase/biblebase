    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你把管理世人的权柄赐给了祂，使祂可以将永生赐予你交托给祂的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正如你把管理全人類的權柄給了他，使他賜永生給你所賜給他的人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "正如你赐给他权柄管辖所有的人    ， 好让他把永恒的生命给与一切你所赐给他的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正如你曾賜給他權柄管理凡有血氣的，叫他將永生賜給你所賜給他的人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "since you have given him authority over all flesh, to give eternal life to all whom you have given him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "even as You gave Him authority over all flesh, that to all whom You have given Him, He may give eternal life."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For you granted him authority over all people that he might give eternal life to all those you have given him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "as You have given Him authority over all flesh, that He should give eternal life to as many as You have given Him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you have given him authority over everyone. He gives eternal life to each one you have given him."
      }
