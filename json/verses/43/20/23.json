    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们赦免谁的罪，谁的罪就得到赦免；你们不赦免谁的罪，谁的罪就得不到赦免。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們赦免誰的罪，誰的罪就得赦免；你們不赦免誰的罪，誰的罪就不得赦免。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們赦免誰的罪，誰的罪就赦免了；你們留下誰的罪，誰的罪就留下了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you forgive the sins of any, their sins have been forgiven them; if you retain the sins of any, they have been retained.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you forgive anyone’s sins, their sins are forgiven; if you do not forgive them, they are not forgiven.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you forgive anyone’s sins, they are forgiven. If you do not forgive them, they are not forgiven.”"
      }
