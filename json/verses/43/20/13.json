    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天使对她说：“妇人，你为什么哭呢？” 她说：“有人把我的主搬走了，不知道放在什么地方。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天使問她：“婦人，你為甚麼哭？”她說：“有人把我的主搬走了，我不知道他們把他放在哪裡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天使對她說：「婦人，你為甚麼哭？」她說：「因為有人把我主挪了去，我不知道放在哪裏。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they *said to her, “Woman, why are you weeping?” She *said to them, “Because they have taken away my Lord, and I do not know where they have laid Him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They asked her, “Woman, why are you crying?” “They have taken my Lord away,” she said, “and I don’t know where they have put him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Dear woman, why are you crying?” the angels asked her. “Because they have taken away my Lord,” she replied, “and I don’t know where they have put him.”"
      }
    },
