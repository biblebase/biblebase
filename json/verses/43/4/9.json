    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撒玛利亚妇人说：“你是个犹太人，怎么向我这撒玛利亚妇人要水喝？”原来犹太人和撒玛利亚人互不往来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒瑪利亞婦人對耶穌說：“你是猶太人，怎麼向我，一個撒瑪利亞婦人要水喝呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "撒馬利亞的婦人對他說：「你既是猶太人，怎麼向我一個撒馬利亞婦人要水喝呢？」原來猶太人和撒馬利亞人沒有來往。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore the Samaritan woman *said to Him, “How is it that You, being a Jew, ask me for a drink since I am a Samaritan woman?” (For Jews have no dealings with Samaritans.)"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Samaritan woman said to him, “You are a Jew and I am a Samaritan woman. How can you ask me for a drink?” (For Jews do not associate with Samaritans.)"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The woman was surprised, for Jews refuse to have anything to do with Samaritans. She said to Jesus, “You are a Jew, and I am a Samaritan woman. Why are you asking me for a drink?”"
      }
