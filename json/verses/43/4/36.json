    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "收割的人不但可以得到报酬，而且可以为永生收获果实，叫撒种的和收割的一同快乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "收割的人得到工資，也積儲五穀直到永生，使撒種的和收割的一同快樂。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "收割的人得酬报，收集果实归入永恒的生命，好使撒种的和收割的一同快乐。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "收割的人得工價，積蓄五穀到永生，叫撒種的和收割的一同快樂。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Already the one who reaps is receiving wages and gathering fruit for eternal life, so that sower and reaper may rejoice together."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Already he who reaps is receiving wages and is gathering fruit for life eternal; so that he who sows and he who reaps may rejoice together."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even now the one who reaps draws a wage and harvests a crop for eternal life, so that the sower and the reaper may be glad together."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And he who reaps receives wages, and gathers fruit for eternal life, that both he who sows and he who reaps may rejoice together."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The harvesters are paid good wages, and the fruit they harvest is people brought to eternal life. What joy awaits both the planter and the harvester alike!"
      }
