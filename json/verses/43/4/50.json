    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.4.43-54",
            "paragraphs": [
              "十三．第二件神跡：治好大臣的兒子（四43～54）"
            ]
          },
          {
            "verse": "jhn.4.50",
            "paragraphs": [
              "四50本節叫我們看見那人的信心正不斷增加。他表現所有的信心，主給他更多。耶穌許下承諾，叫他回去。“你的兒子活了。”他的兒子已醫好了。那人雖然沒有眼見任何神跡發生，但他相信主耶穌所說的話，起程回家。這就是信心了！"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%a6%82%e4%bd%95%e6%89%8d%e8%83%bd%e7%b6%93%e6%ad%b7%e5%88%b0%e7%a5%9e%e8%b9%9f%ef%bc%9f",
        "title": "如何才能經歷到神蹟？",
        "date": "2005-02-06",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 4:43-54",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-10-2005-02-06.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-10-2005-02-06.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“回去吧，你的儿子好了。”他相信耶稣的话，就回去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌告訴他：“回去吧，你的兒子好了。”那人信耶穌對他說的話，就回去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他說：「回去吧，你的兒子活了！」那人信耶穌所說的話就回去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus *said to him, “Go; your son lives.” The man believed the word that Jesus spoke to him and started off."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Go,” Jesus replied, “your son will live.” The man took Jesus at his word and departed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus told him, “Go back home. Your son will live!” And the man believed what Jesus said and started home."
      }
    },
    "words": [
