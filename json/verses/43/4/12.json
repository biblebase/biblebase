    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的祖先雅各把这口井留给我们，他自己和他的儿女、牲畜都喝这井里的水，难道你比他更伟大吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們的祖先雅各把這口井留給我們，他自己和子孫以及牲畜都喝這井的水，難道你比他還大嗎？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "难道你比我们的先祖雅各更大吗？他给我们留下了这口井。他自己、他的子孙、他的牲畜都喝这井里的水！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們的祖宗雅各將這井留給我們，他自己和兒子並牲畜也都喝這井裏的水，難道你比他還大嗎？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Are you greater than our father Jacob? He gave us the well and drank from it himself, as did his sons and his livestock.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You are not greater than our father Jacob, are You, who gave us the well, and drank of it himself and his sons and his cattle?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Are you greater than our father Jacob, who gave us the well and drank from it himself, as did also his sons and his livestock?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Are You greater than our father Jacob, who gave us the well, and drank from it himself, as well as his sons and his livestock?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And besides, do you think you’re greater than our ancestor Jacob, who gave us this well? How can you offer better water than he and his sons and his animals enjoyed?”"
      }
    },
