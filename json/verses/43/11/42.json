    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我知道你常常垂听我的祷告。我这样说是为了周围站着的众人，好叫他们相信是你差了我来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我知道你常常聽我，但我說這話，是為了周圍站著的群眾，叫他們信是你差了我來。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我知道你总是在垂听我，但我这么说，是为了这些站在周围的民众，好让他们相信是你差派了我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我也知道你常聽我，但我說這話是為周圍站着的眾人，叫他們信是你差了我來。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I knew that you always hear me, but I said this on account of the people standing around, that they may believe that you sent me.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I knew that You always hear Me; but because of the people standing around I said it, so that they may believe that You sent Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I knew that you always hear me, but I said this for the benefit of the people standing here, that they may believe that you sent me.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And I know that You always hear Me, but because of the people who are standing by I said this, that they may believe that You sent Me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You always hear me, but I said it out loud for the sake of all these people standing here, so that they will believe you sent me.”"
      }
    },
