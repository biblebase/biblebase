    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其实耶稣是指他已经死了，门徒却以为他真的睡了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "其實耶穌是指著拉撒路的死說的，門徒卻以為他是指正常的睡眠說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌這話是指着他死說的，他們卻以為是說照常睡了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Jesus had spoken of his death, but they thought that He was speaking of literal sleep."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus had been speaking of his death, but his disciples thought he meant natural sleep."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They thought Jesus meant Lazarus was simply sleeping, but Jesus meant Lazarus had died."
      }
    },
