    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.15.21",
            "paragraphs": [
              "一五21信徒所遭受的憎恨和逼害都是“因我的名”而來。因為信徒與基督聯合，為基督而脫離世界，肩負基督的名，行為舉止也像祂，就必定有此遭遇。世人不認識父，不知祂差主耶穌來到世界成為救主；但無知不是藉口。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%9f%ba%e7%9d%a3%e5%be%92%e7%9a%84%e4%b8%96%e7%95%8c%e8%a7%80",
        "title": "基督徒的世界觀",
        "date": "2006-07-09",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 15:18-27, 17:14-19; 羅馬書 12:2; 約翰一書 2:15-17",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-35-2006-07-09.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-35-2006-07-09.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世人将因为我的名而这样对待你们，因为他们不认识差我来的那位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但他們因著我的名，要向你們行這一切，因為他們不認識那差我來的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "但是，因我名的缘故，他们会对你们做这一切事，因为他们不认识派我来的那一位。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但他們因我的名要向你們行這一切的事，因為他們不認識那差我來的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But all these things they will do to you on account of my name, because they do not know him who sent me."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But all these things they will do to you for My name’s sake, because they do not know the One who sent Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will treat you this way because of my name, for they do not know the one who sent me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But all these things they will do to you for My name’s sake, because they do not know Him who sent Me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will do all this to you because of me, for they have rejected the one who sent me."
      }
    },
