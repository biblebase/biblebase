    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们多结果子，证明自己是我的门徒，就会给我父带来荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，你們結出很多果子，我父就因此得榮耀，你們也就是我的門徒了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你们结出很多果子，就表明是我的门徒了；从这一点我父也就得荣耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們多結果子，我父就因此得榮耀，你們也就是我的門徒了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "By this my Father is glorified, that you bear much fruit and so prove to be my disciples."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My Father is glorified by this, that you bear much fruit, and so prove to be My disciples."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is to my Father’s glory, that you bear much fruit, showing yourselves to be my disciples."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "By this My Father is glorified, that you bear much fruit; so you will be My disciples."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When you produce much fruit, you are my true disciples. This brings great glory to my Father."
      }
    },
