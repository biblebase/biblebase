    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你既然不是基督，不是以利亚，也不是那位先知，那你为什么给人施洗呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們問約翰：“你既然不是基督，不是以利亞，也不是那位先知，那麼你為甚麼施洗呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們就問他說：「你既不是基督，不是以利亞，也不是那先知，為甚麼施洗呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They asked him, and said to him, “Why then are you baptizing, if you are not the Christ, nor Elijah, nor the Prophet?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "questioned him, “Why then do you baptize if you are not the Messiah, nor Elijah, nor the Prophet?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "asked him, “If you aren’t the Messiah or Elijah or the Prophet, what right do you have to baptize?”"
      }
