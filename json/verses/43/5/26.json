    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为正如父自己是生命的源头，祂也同样让子作生命的源头，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就如父是生命的源頭，照樣他也使子成為生命的源頭，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為父怎樣在自己有生命，就賜給他兒子也照樣在自己有生命，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For just as the Father has life in Himself, even so He gave to the Son also to have life in Himself;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For as the Father has life in himself, so he has granted the Son also to have life in himself."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Father has life in himself, and he has granted that same life-giving power to his Son."
      }
    },
