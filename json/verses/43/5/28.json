    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们不要因此而惊奇，时候一到，一切在坟墓里的死人都要听见上帝儿子的声音，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不要把這事看作希奇，因為時候將到，那時所有在墳墓裡的都要聽見他的聲音，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們不要把這事看作希奇。時候要到，凡在墳墓裏的，都要聽見他的聲音，就出來："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not marvel at this; for an hour is coming, in which all who are in the tombs will hear His voice,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do not be amazed at this, for a time is coming when all who are in their graves will hear his voice"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t be so surprised! Indeed, the time is coming when all the dead in their graves will hear the voice of God’s Son,"
      }
    },
