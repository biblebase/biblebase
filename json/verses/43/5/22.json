    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.5.19-29",
            "paragraphs": [
              "三．耶穌辯護自己是與神平等的（五19～29）"
            ]
          },
          {
            "verse": "jhn.5.22",
            "paragraphs": [
              "五22新約教導我們父神將審判的事全交與子。主耶穌要完成這任務，祂必須要有完全的知識及絕對的公義，能辨察人心裏的思想和動機。世界的判官站在這群猶太人面前，申明自己的權利和地位，這群人卻不認得祂，真是奇怪。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e7%94%9f%e5%91%bd%e8%88%87%e5%af%a9%e5%88%a4%e7%9a%84%e4%b8%bb",
        "title": "生命與審判的主",
        "date": "2005-03-20",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 5:18-29",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-12-2005-03-20.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-12-2005-03-20.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "父不审判人，祂将审判的事全交给子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "父不審判人，卻已經把審判的權柄完全交給子，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "父不審判甚麼人，乃將審判的事全交與子，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For not even the Father judges anyone, but He has given all judgment to the Son,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moreover, the Father judges no one, but has entrusted all judgment to the Son,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In addition, the Father judges no one. Instead, he has given the Son absolute authority to judge,"
      }
    },
