    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为耶稣知道谁要出卖祂，所以说：“你们不是每一个都干净。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "原來耶穌知道誰要出賣他，所以他說“你們不是人人都是潔淨的”。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌原知道要賣他的是誰，所以說：「你們不都是乾淨的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He knew the one who was betraying Him; for this reason He said, “Not all of you are clean.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For he knew who was going to betray him, and that was why he said not every one was clean."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For Jesus knew who would betray him. That is what he meant when he said, “Not all of you are clean.”"
      }
    },
