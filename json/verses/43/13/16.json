    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我实实在在地告诉你们，奴仆不能大过主人，受差遣的也不能大过差遣他的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我實實在在告訴你們，僕人不能大過主人，奉差遣的也不能大過差他的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我實實在在地告訴你們，僕人不能大於主人，差人也不能大於差他的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Truly, truly, I say to you, a slave is not greater than his master, nor is one who is sent greater than the one who sent him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Very truly I tell you, no servant is greater than his master, nor is a messenger greater than the one who sent him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I tell you the truth, slaves are not greater than their master. Nor is the messenger more important than the one who sends the message."
      }
    },
