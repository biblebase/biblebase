    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "西門．彼得向他示意，叫他問耶穌是指著誰說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "西門‧彼得點頭對他說：「你告訴我們，主是指着誰說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Simon Peter *gestured to him, and *said to him, “Tell us who it is of whom He is speaking.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Simon Peter motioned to this disciple and said, “Ask him which one he means.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Simon Peter motioned to him to ask, “Who’s he talking about?”"
      }
    },
