    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为圣经上说： “不生育、未生养的妇人啊， 要快乐； 未曾生产的女子啊， 要高声欢呼， 因为没有丈夫的比有丈夫的儿女更多。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為經上記著說： “不能生育、沒有生養的啊，你要歡欣！ 沒有受過生產痛苦的啊，你要呼喊，大聲呼叫！ 因為棄婦比有夫之婦有更多兒子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為經上記着： 不懷孕、不生養的，你要歡樂； 未曾經過產難的，你要高聲歡呼； 因為沒有丈夫的，比有丈夫的兒女更多。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it is written, “Rejoice, barren woman who does not bear; Break forth and shout, you who are not in labor; For more numerous are the children of the desolate Than of the one who has a husband.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For it is written: “Be glad, barren woman, you who never bore a child; shout for joy and cry aloud, you who were never in labor; because more are the children of the desolate woman than of her who has a husband.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As Isaiah said, “Rejoice, O childless woman, you who have never given birth! Break into a joyful shout, you who have never been in labor! For the desolate woman now has more children than the woman who lives with her husband!”"
      }
