    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "dan.2.1-49",
            "paragraphs": [
              "A.尼布甲尼撒夢見四帝國和神國（二1～49）"
            ]
          },
          {
            "verse": "dan.2.12-24",
            "paragraphs": [
              "b.但以理的反應（二12～24）"
            ]
          },
          {
            "verse": "dan.2.12",
            "paragraphs": [
              "12.尼布甲尼撒根本毫不隱藏他的忿怒。他將毫不遲延地運行他的懲罰，好讓臣仆尊重他下達的命令。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王大怒，下令处死巴比伦所有的智者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，王大發烈怒，下令殺絕巴比倫所有的智慧人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，王氣忿忿地大發烈怒，吩咐滅絕巴比倫所有的哲士。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Because of this the king was angry and very furious, and commanded that all the wise men of Babylon be destroyed."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because of this the king became indignant and very furious and gave orders to destroy all the wise men of Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This made the king so angry and furious that he ordered the execution of all the wise men of Babylon."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For this reason the king was angry and very furious, and gave the command to destroy all the wise men of Babylon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king was furious when he heard this, and he ordered that all the wise men of Babylon be executed."
      }
