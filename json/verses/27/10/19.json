    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他对我说：“倍受眷爱的人啊，不要怕，愿你平安！要刚强勇敢。”我听到他的话，便有了力量，于是对他说：“我主啊，请说，因为你使我有了力量。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“大蒙眷愛的人哪！不要懼怕，願你平安，你要大大剛強。”他和我說話的時候，我就剛強起來；於是我說：“我主請說，因為你剛強了我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：「大蒙眷愛的人哪，不要懼怕，願你平安！你總要堅強。」他一向我說話，我便覺得有力量，說：「我主請說，因你使我有力量。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said, “O man of high esteem, do not be afraid. Peace be with you; take courage and be courageous!” Now as soon as he spoke to me, I received strength and said, “May my lord speak, for you have strengthened me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do not be afraid, you who are highly esteemed,” he said. “Peace! Be strong now; be strong.” When he spoke to me, I was strengthened and said, “Speak, my lord, since you have given me strength.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Don’t be afraid,” he said, “for you are very precious to God. Peace! Be encouraged! Be strong!” As he spoke these words to me, I suddenly felt stronger and said to him, “Please speak to me, my lord, for you have strengthened me.”"
      }
