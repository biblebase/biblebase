    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "会众的长老说：“便雅悯支派的女子都死了，我们应该怎么办才能使余下的人有妻子呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "會眾中的長老說：“便雅憫的女子既然都被除滅了，我們應怎樣行，使那些餘下的人有妻子呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "會中的長老說：「便雅憫中的女子既然除滅了，我們當怎樣辦理、使那餘剩的人有妻呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the elders of the congregation said, “What shall we do for wives for those who are left, since the women are destroyed out of Benjamin?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the elders of the assembly said, “With the women of Benjamin destroyed, how shall we provide wives for the men who are left?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the elders of the assembly asked, “How can we find wives for the few who remain, since the women of the tribe of Benjamin are dead?"
      }
    },
