    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.21.1-7",
            "paragraphs": [
              "二一1～7由於幾乎所有的便雅憫人都被殺害了（比較二○47），又由於其它支派曾發誓不讓他們的處女嫁給便雅憫人，所以便雅憫支派就面對完全滅絕的危險。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们来到伯特利，坐在上帝面前放声痛哭，直到黄昏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人來到伯特利，在那裡坐在　神面前直到晚上，放聲大哭，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人來到伯特利，坐在神面前直到晚上，放聲痛哭，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the people came to Bethel and sat there before God until evening, and lifted up their voices and wept bitterly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The people went to Bethel, where they sat before God until evening, raising their voices and weeping bitterly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now the people went to Bethel and sat in the presence of God until evening, weeping loudly and bitterly."
      }
