    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是我们又不能把女儿嫁给他们，因为我们曾经起誓，‘把女儿嫁给便雅悯人的必受咒诅。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是我們不能把我們自己的女兒給他們作妻子。”因為以色列人曾經起誓說：“把女兒給便雅憫人作妻子的，是可咒詛的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是我們不能將自己的女兒給他們為妻；因為以色列人曾起誓說，有將女兒給便雅憫人為妻的，必受咒詛。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But we cannot give them wives of our daughters.” For the sons of Israel had sworn, saying, “Cursed is he who gives a wife to Benjamin.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We can’t give them our daughters as wives, since we Israelites have taken this oath: ‘Cursed be anyone who gives a wife to a Benjamite.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But we cannot give them our own daughters in marriage because we have sworn with a solemn oath that anyone who does this will fall under God’s curse.”"
      }
    },
