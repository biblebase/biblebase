    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要借此试验以色列人，看他们是否像他们祖先一样谨遵我的道。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為要藉著他們試驗以色列人，看他們肯不肯謹守遵行耶和華的道路，像他們的列祖一樣謹守。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為要藉此試驗以色列人，看他們肯照他們列祖謹守遵行我的道不肯。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "in order to test Israel by them, whether they will keep the way of the Lord to walk in it as their fathers did, or not.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will use them to test Israel and see whether they will keep the way of the Lord and walk in it as their ancestors did.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I did this to test Israel—to see whether or not they would follow the ways of the Lord as their ancestors did.”"
      }
