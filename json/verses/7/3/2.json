    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.3.1-5",
            "paragraphs": [
              "三1～5因為祂眷顧子民的性情，神利用以色列人不趕走迦南人的失敗，讓這些異國人把戰爭的藝術指教以色列人，因為他們不曾參與起初在約書亞帶領下的戰役。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "好使以色列人的後代，那些以前沒有經驗過這些戰爭的人，知道學習爭戰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "好叫以色列的後代又知道又學習未曾曉得的戰事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "only in order that the generations of the sons of Israel might be taught war, those who had not experienced it formerly)."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "(he did this only to teach warfare to the descendants of the Israelites who had not had previous battle experience):"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He did this to teach warfare to generations of Israelites who had no experience in battle."
      }
    },
