    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她回到丈夫住宿的房子门前，就扑倒在地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天快亮的時候，那女子回到留她主人住宿的那人的房子門口，就仆倒在那裡，直到天亮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天快亮的時候，婦人回到她主人住宿的房門前，就仆倒在地，直到天亮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As the day began to dawn, the woman came and fell down at the doorway of the man’s house where her master was, until full daylight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At daybreak the woman went back to the house where her master was staying, fell down at the door and lay there until daylight."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At daybreak the woman returned to the house where her husband was staying. She collapsed at the door of the house and lay there until it was light."
      }
