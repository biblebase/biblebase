    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.19.15-21",
            "paragraphs": [
              "一九15～21雖然他們在\"城裏的街上\"等候，但基比拉人沒有按照慣例接待他們。最後，一個旅居在基比拉的以法蓮人接他們進家裏。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们决定在那里过夜。因为没有人接待他们，他们只好在城中的广场露宿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們從那裡轉身，要進基比亞去住宿；他們進去，坐在城裡的廣場上，因為沒有人接待他們到家裡去住宿。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們進入基比亞要在那裏住宿，就坐在城裏的街上，因為無人接他們進家住宿。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "and they turned aside there, to go in and spend the night at Gibeah. And he went in and sat down in the open square of the city, for no one took them into his house to spend the night."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They turned aside there in order to enter and lodge in Gibeah. When they entered, they sat down in the open square of the city, for no one took them into his house to spend the night."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There they stopped to spend the night. They went and sat in the city square, but no one took them in for the night."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They turned aside there to go in to lodge in Gibeah. And when he went in, he sat down in the open square of the city, for no one would take them into his house to spend the night."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "so they stopped there to spend the night. They rested in the town square, but no one took them in for the night."
      }
    },
