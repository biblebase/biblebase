    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但求你先给我两个月的时间，让我和同伴到山上去为我终身未嫁哀哭。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她又對自己的父親說：“請你准我這件事吧：給我兩個月時間，讓我和我的同伴去漫遊山間，為我的童貞哀哭。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又對父親說：「有一件事求你允准：容我去兩個月，與同伴在山上，好哀哭我終為處女。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She said to her father, “Let this thing be done for me; let me alone two months, that I may go to the mountains and weep because of my virginity, I and my companions.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But grant me this one request,” she said. “Give me two months to roam the hills and weep with my friends, because I will never marry.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But first let me do this one thing: Let me go up and roam in the hills and weep with my friends for two months, because I will die a virgin.”"
      }
    },
