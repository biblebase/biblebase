      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "每年以色列的女子都去为基列人耶弗他的女儿哀哭四天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列的女子年年都去哀悼基列人耶弗他的女兒，每年四日。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "此後以色列中有個規矩，每年以色列的女子去為基列人耶弗他的女兒哀哭四天。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that the daughters of Israel went yearly to commemorate the daughter of Jephthah the Gileadite four days in the year."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "that each year the young women of Israel go out for four days to commemorate the daughter of Jephthah the Gileadite."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "for young Israelite women to go away for four days each year to lament the fate of Jephthah’s daughter."
      }
