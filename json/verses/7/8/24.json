    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.8.24",
            "paragraphs": [
              "八24\"以實瑪利人\"。指過遊牧生活的商人，米甸人就是這種商人。參看創世記三十七章25節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我有一个要求，请你们把抢来的耳环都给我。”原来米甸人属于以实玛利族，都戴金耳环。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基甸又對他們說：“我有一個要求：請把你們各人奪得的耳環給我。”原來敵人都戴金耳環，因為他們是以實瑪利人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基甸又對他們說：「我有一件事求你們：請你們各人將所奪的耳環給我。」（原來仇敵是以實瑪利人，都是戴金耳環的。）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet Gideon said to them, “I would request of you, that each of you give me an earring from his spoil.” (For they had gold earrings, because they were Ishmaelites.)"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he said, “I do have one request, that each of you give me an earring from your share of the plunder.” (It was the custom of the Ishmaelites to wear gold earrings.)"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "However, I do have one request—that each of you give me an earring from the plunder you collected from your fallen enemies.” (The enemies, being Ishmaelites, all wore gold earrings.)"
      }
    },
