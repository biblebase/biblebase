    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基甸说：“他们是我同母所生的兄弟，我凭永活的耶和华起誓，如果你们当初不杀他们，今天我就不杀你们了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基甸說：“他們是我的兄弟，是我母親的兒子；我指著永活的耶和華起誓，如果你們從前讓他們存活，我現在就不殺你們了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基甸說：「他們是我同母的弟兄，我指着永生的耶和華起誓，你們從前若存留他們的性命，我如今就不殺你們了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said, “They were my brothers, the sons of my mother. As the Lord lives, if only you had let them live, I would not kill you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Gideon replied, “Those were my brothers, the sons of my own mother. As surely as the Lord lives, if you had spared their lives, I would not kill you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“They were my brothers, the sons of my own mother!” Gideon exclaimed. “As surely as the Lord lives, I wouldn’t kill you if you hadn’t killed them.”"
      }
