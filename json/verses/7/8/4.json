      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基甸率领他的三百人渡过约旦河，虽然疲惫不堪，仍紧追敌人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基甸來到約旦河，就過了河；他和與他在一起的三百人雖然都很疲乏，仍然追趕敵人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基甸和跟隨他的三百人到約旦河過渡，雖然疲乏，還是追趕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Gideon and the 300 men who were with him came to the Jordan and crossed over, weary yet pursuing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Gideon and his three hundred men, exhausted yet keeping up the pursuit, came to the Jordan and crossed it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Gideon then crossed the Jordan River with his 300 men, and though exhausted, they continued to chase the enemy."
      }
