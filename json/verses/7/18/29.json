    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这座城原来叫拉亿，他们用祖先以色列的儿子但的名字为该城重新命名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "按著他們的祖宗，以色列所生的兒子但的名字，給那城起名叫但；其實那城原先的名字叫拉億。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "照着他們始祖以色列之子但的名字，給那城起名叫但；原先那城名叫拉億。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they named the city Dan, after the name of Dan their ancestor, who was born to Israel; but the name of the city was Laish at the first."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They called the name of the city Dan, after the name of Dan their father who was born in Israel; however, the name of the city formerly was Laish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They named it Dan after their ancestor Dan, who was born to Israel—though the city used to be called Laish."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they called the name of the city Dan, after the name of Dan their father, who was born to Israel. However, the name of the city formerly was Laish."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They renamed the town Dan after their ancestor, Israel’s son, but it had originally been called Laish."
      }
    },
