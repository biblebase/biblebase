    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "米甸人势力强盛，以色列人只好躲进山里，藏在洞穴和营寨中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "米甸人的勢力勝過以色列人；以色列人因米甸人的緣故，就為自己在山上挖穴、挖洞，建營寨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "米甸人壓制以色列人；以色列人因為米甸人，就在山中挖穴、挖洞、建造營寨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The power of Midian prevailed against Israel. Because of Midian the sons of Israel made for themselves the dens which were in the mountains and the caves and the strongholds."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because the power of Midian was so oppressive, the Israelites prepared shelters for themselves in mountain clefts, caves and strongholds."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
