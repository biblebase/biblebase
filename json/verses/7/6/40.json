    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那天晚上，耶和华就照他所求使地面布满露水，唯独羊毛是干的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那一夜，　神也這樣行了；單單羊毛是乾的，全地都有露水。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這夜神也如此行：獨羊毛上是乾的，別的地方都有露水。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God did so that night; for it was dry only on the fleece, and dew was on all the ground."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "That night God did so. Only the fleece was dry; all the ground was covered with dew."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So that night God did as Gideon asked. The fleece was dry in the morning, but the ground was covered with dew."
      }
    },
