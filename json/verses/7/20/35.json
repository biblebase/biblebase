    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华帮助以色列人战胜了便雅悯人。那天，以色列人杀了便雅悯两万五千一百刀兵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華在以色列人面前擊敗了便雅憫人；那天以色列人消滅便雅憫人，共二萬五千一百名，都是拿刀的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華使以色列人殺敗便雅憫人。那日，以色列人殺死便雅憫人二萬五千一百，都是拿刀的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the Lord struck Benjamin before Israel, so that the sons of Israel destroyed 25,100 men of Benjamin that day, all who draw the sword."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord defeated Benjamin before Israel, and on that day the Israelites struck down 25,100 Benjamites, all armed with swords."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the Lord helped Israel defeat Benjamin, and that day the Israelites killed 25,100 of Benjamin’s warriors, all of whom were experienced swordsmen."
      }
    },
