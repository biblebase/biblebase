    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛挪亚向耶和华祈求说：“主啊，求你再派你的仆人到这里来，教我们怎样照顾要出生的孩子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪挪亞就懇求耶和華，說：“啊，主啊，求你叫你派來的神人再到我們這裡吧！好指教我們要怎樣對待將要生下來的孩子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瑪挪亞就祈求耶和華說：「主啊，求你再差遣那神人到我們這裏來，好指教我們怎樣待這將要生的孩子。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Manoah entreated the Lord and said, “O Lord, please let the man of God whom You have sent come to us again that he may teach us what to do for the boy who is to be born.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Manoah prayed to the Lord: “Pardon your servant, Lord. I beg you to let the man of God you sent to us come again to teach us how to bring up the boy who is to be born.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Manoah prayed to the Lord, saying, “Lord, please let the man of God come back to us again and give us more instructions about this son who is to be born.”"
      }
    },
