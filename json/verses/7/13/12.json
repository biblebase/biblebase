    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛挪亚问道：“你的话应验以后，我们应该怎样抚养这孩子？他该做什么？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪挪亞說：“你的話應驗的時候，這孩子的生活方式怎樣？他要作甚麼呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瑪挪亞說：「願你的話應驗！我們當怎樣待這孩子，他後來當怎樣呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Manoah said, “Now when your words come to pass, what shall be the boy’s mode of life and his vocation?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Manoah asked him, “When your words are fulfilled, what is to be the rule that governs the boy’s life and work?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Manoah asked him, “When your words come true, what kind of rules should govern the boy’s life and work?”"
      }
    },
