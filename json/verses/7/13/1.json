      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the sons of Israel again did evil in the sight of the Lord, so that the Lord gave them into the hands of the Philistines forty years."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Again the Israelites did evil in the eyes of the Lord, so the Lord delivered them into the hands of the Philistines for forty years."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Again the Israelites did evil in the Lord’s sight, so the Lord handed them over to the Philistines, who oppressed them for forty years."
      }
