    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jdg.13.1",
            "paragraphs": [
              "13:1",
              "非利士人：大約主前1200年，非利士人大舉遷入迦南的南部海岸，他們可能來自克裏特島(耶47:4；摩9:7,其中的迦斐托即為克裏特)。在主前1200~1000年期間(也就是到大衛王時)，他們一直是希伯來人的主要敵人。因為善於制作鐵兵器，他們在與以色列人的戰爭中占了上風(撒上13:19~22)。“巴勒斯坦”一詞來自於“非利士”。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人又做耶和华视为恶的事，耶和华就把他们交在非利士人手中四十年。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人又行了耶和華看為惡的事，耶和華就把他們交在非利士人手中四十年。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人又行耶和華眼中看為惡的事，耶和華將他們交在非利士人手中四十年。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the people of Israel again did what was evil in the sight of the Lord, so the Lord gave them into the hand of the Philistines for forty years."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the sons of Israel again did evil in the sight of the Lord, so that the Lord gave them into the hands of the Philistines forty years."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Again the Israelites did evil in the eyes of the Lord, so the Lord delivered them into the hands of the Philistines for forty years."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Again the children of Israel did evil in the sight of the Lord, and the Lord delivered them into the hand of the Philistines for forty years."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Again the Israelites did evil in the Lord’s sight, so the Lord handed them over to the Philistines, who oppressed them for forty years."
      }
