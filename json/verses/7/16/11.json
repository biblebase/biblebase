    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "参孙回答说：“如果用从未用过的新绳子绑住我，我的力气就会和普通人一样。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "參孫對她說：“如果人用從未使用過的新繩子把我緊緊地捆綁起來，我就軟弱無力，像平常人一樣。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "參孫回答說：「人若用沒有使過的新繩捆綁我，我就軟弱像別人一樣。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to her, “If they bind me tightly with new ropes which have not been used, then I will become weak and be like any other man.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said, “If anyone ties me securely with new ropes that have never been used, I’ll become as weak as any other man.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Samson replied, “If I were tied up with brand-new ropes that had never been used, I would become as weak as anyone else.”"
      }
    },
