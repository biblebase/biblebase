    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.16.26",
            "paragraphs": [
              "一六26平頂房子由立在石基上的木柱子支撐，而房頂用許多平舖在墻上的圓木築成。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "参孙对拉着他手的童仆说：“带我到支撑这庙宇的柱子那里，我想靠一靠。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "參孫對拉著他手的童子說：“請你讓我摸摸支撐著這廟宇的柱子，我要在柱子上靠一靠。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "參孫向拉他手的童子說：「求你讓我摸着托房的柱子，我要靠一靠。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Samson said to the boy who was holding his hand, “Let me feel the pillars on which the house rests, that I may lean against them.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Samson said to the servant who held his hand, “Put me where I can feel the pillars that support the temple, so that I may lean against them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Samson said to the young servant who was leading him by the hand, “Place my hands against the pillars that hold up the temple. I want to rest against them.”"
      }
    },
