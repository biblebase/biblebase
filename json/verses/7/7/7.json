    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对基甸说：“我要用这三百舔水喝的人拯救你们，我要把米甸人交在你手中。让其余的人都回家吧。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對基甸說：“我要用這舔水的三百人拯救你們，把米甸人交在你們的手裡，所有其他的人都可以各回自己的地方去。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對基甸說：「我要用這舔水的三百人拯救你們，將米甸人交在你手中；其餘的人都可以各歸各處去。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the Lord said to Gideon, “With the 300 men who lapped I will save you and give the Midianites into your hand, and let all the others go every man to his home.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord said to Gideon, “I will deliver you with the 300 men who lapped and will give the Midianites into your hands; so let all the other people go, each man to his home.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said to Gideon, “With the three hundred men that lapped I will save you and give the Midianites into your hands. Let all the others go home.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then the Lord said to Gideon, “By the three hundred men who lapped I will save you, and deliver the Midianites into your hand. Let all the other people go, every man to his place.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord told Gideon, “With these 300 men I will rescue you and give you victory over the Midianites. Send all the others home.”"
      }
    },
