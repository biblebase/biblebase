    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.7.19",
            "paragraphs": [
              "七19\"三更之初\"。約為下午十點。打破\"瓶子\"不但造成噪音，而且造成忽然光亮起來的效果。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "午夜初，守卫刚换班，基甸带着一百人来到米甸营旁。他们吹响号角，打破手中的瓦瓶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基甸和所有與他在一起的一百人，在半夜三更的起初，換更的時候，來到了營的邊緣，就吹起角來，打破手中的瓦瓶。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基甸和跟隨他的一百人，在三更之初才換更的時候，來到營旁，就吹角，打破手中的瓶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Gideon and the hundred men who were with him came to the outskirts of the camp at the beginning of the middle watch, when they had just posted the watch; and they blew the trumpets and smashed the pitchers that were in their hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Gideon and the hundred men with him reached the edge of the camp at the beginning of the middle watch, just after they had changed the guard. They blew their trumpets and broke the jars that were in their hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It was just after midnight, after the changing of the guard, when Gideon and the 100 men with him reached the edge of the Midianite camp. Suddenly, they blew the rams’ horns and broke their clay jars."
      }
    },
