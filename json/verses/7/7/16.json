    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基甸把三百人分成三队，分给他们号角和藏有火把的瓦瓶，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他把三百人分作三隊，把角和空瓶交在各人手裡，又把火把放在瓶裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是基甸將三百人分作三隊，把角和空瓶交在各人手裏（瓶內都藏着火把），"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He divided the 300 men into three companies, and he put trumpets and empty pitchers into the hands of all of them, with torches inside the pitchers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Dividing the three hundred men into three companies, he placed trumpets and empty jars in the hands of all of them, with torches inside."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He divided the 300 men into three groups and gave each man a ram’s horn and a clay jar with a torch in it."
      }
    },
