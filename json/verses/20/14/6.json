    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.6",
            "paragraphs": [
              "智慧規避那自以為無所不知的人（十四6）",
              "從這一節開始所出現的褻慢人，不再單單是沒有禮貌的人，他可能是個敏銳的思想家：他之所以得到褻慢人這個名字，只是因為他拒絕神，但這卻是他的致命傷。本書的座右銘（一7）解釋了他的失敗，以及他的對手的成功（參，伯二十八28；約壹二27）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "嘲讽者徒然寻智慧， 明哲人轻易得知识。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "好譏笑人的尋找智慧，卻尋不著； 聰明人尋找知識，卻輕易得著。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "讥讽者寻求智慧却得不到； 有悟性的人轻易得知识。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "褻慢人尋智慧，卻尋不着； 聰明人易得知識。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "A scoffer seeks wisdom in vain, but knowledge is easy for a man of understanding."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A scoffer seeks wisdom and finds none, But knowledge is easy to one who has understanding."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The mocker seeks wisdom and finds none, but knowledge comes easily to the discerning."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "A scoffer seeks wisdom and does not find  it, But knowledge is easy to him who understands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A mocker seeks wisdom and never finds it, but knowledge comes easily to those with understanding."
      }
    },
