    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.14",
            "paragraphs": [
              "各有其份（十四14）",
              "令人驚奇的是，兩個掃羅（撒上二十六21b；提後四7）分別對這則箴言的前後兩半作了相對的見證。RSV、呂譯的修正是沒有必要的，也削弱了結論的力量。我們在第10、13節，與九12，也可以瞥見人類的孤立。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "背弃正道，自食恶果； 善人行善，必得善报。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "心中背道的，必飽嘗自己行為的惡果； 善人也因自己所行的，得到善報。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "心中背道的必饱尝他行为的恶果； 好人也必饱尝他行为的回报。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心中背道的，必滿得自己的結果； 善人必從自己的行為得以知足。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The backslider in heart will be filled with the fruit of his ways, and a good man will be filled with the fruit of his ways."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The backslider in heart will have his fill of his own ways, But a good man will be satisfied with his."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The faithless will be fully repaid for their ways, and the good rewarded for theirs."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The backslider in heart will be filled with his own ways, But a good man will  be  satisfied from above."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Backsliders get what they deserve; good people receive their reward."
      }
    },
