    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.21",
            "paragraphs": [
              "藐視之罪（十四21）",
              "箴言從許多角度來觀察一件事，從它對無憐憫所作的論述即可見一斑。在第20節只是就事論事地報導它──事情就是這樣子的。在十一12，則是將它視為通達人的反面。這裏比較深入，指出它乃是拒絕神的旨意與（第二行）祝福。關於富足與貧窮，見第31節所列的參考經文。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "藐视邻舍是罪过， 怜悯穷人蒙福乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "藐視鄰舍的，是為有罪； 恩待貧窮人的，是為有福。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "藐视自己邻人的，是有罪的； 恩待卑微人的，是蒙福的！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "藐視鄰舍的，這人有罪； 憐憫貧窮的，這人有福。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Whoever despises his neighbor is a sinner, but blessed is he who is generous to the poor."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who despises his neighbor sins, But happy is he who is gracious to the poor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is a sin to despise one’s neighbor, but blessed is the one who is kind to the needy."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He who despises his neighbor sins; But he who has mercy on the poor, happy is he."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is a sin to belittle one’s neighbor; blessed are those who help the poor."
      }
    },
