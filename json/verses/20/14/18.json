    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.18",
            "paragraphs": [
              "愚昧人與智慧人所得的獎賞（十四18）",
              "這是一則前瞻性的箴言。RSV的“得著”與現中的“自食……的惡果”比和合、AV、RV的得……為產業更清楚，因為這則箴言不是因著一個人的遺傳而譴責他的愚昧，而是警告他：積習成癖，他正為自己積存的，只是愈來愈多的愚昧，直到愚昧成為他所擁有的唯一事物，像在第24節一樣。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚昧人得愚昧作产业， 明哲人得知识为冠冕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚蒙人得愚妄為產業； 精明的人以知識為冠冕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚蒙人得愚昧為產業； 通達人得知識為冠冕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The naive inherit foolishness, But the sensible are crowned with knowledge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The simple inherit folly, but the prudent are crowned with knowledge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Simpletons are clothed with foolishness, but the prudent are crowned with knowledge."
      }
    },
