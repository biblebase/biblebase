    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.35",
            "paragraphs": [
              "報應的效力（十四35）",
              "這則令人振奮的箴言，所提醒我們的是：一個人若是不受人賞識，不要歸咎於運氣或他人偏袒，該怪的是自己的缺點。Moffatt將它的含義表達得相當好：“君王寵愛能幹的臣仆，無能的臣仆，遭其震怒。”參二十二29。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.14.35",
            "paragraphs": [
              "一四35\"貽羞\"。行為帶來羞恥。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "明智的臣子蒙王喜悦， 可耻的仆人惹王发怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "明慧的臣僕，為王喜悅； 貽羞的僕人，遭王震怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧的臣子蒙王恩惠； 貽羞的僕人遭其震怒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king’s favor is toward a servant who acts wisely, But his anger is toward him who acts shamefully."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A king delights in a wise servant, but a shameful servant arouses his fury."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A king rejoices in wise servants but is angry with those who disgrace him."
      }
