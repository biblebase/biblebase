    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.28",
            "paragraphs": [
              "君王的榮耀（十四28）",
              "這是提醒我們：孤立的光彩是會自己熄滅的。真實的領袖乃是以跟從他之人的活力為他的榮耀。",
              "安居度日（十四29、30）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人民众多，是君王的荣耀； 没有臣民，君主必然败亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "君王的榮耀在於人民眾多， 帝王的沒落由於國民寡少。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "帝王榮耀在乎民多； 君王衰敗在乎民少。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In a multitude of people is a king’s glory, But in the dearth of people is a prince’s ruin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A large population is a king’s glory, but without subjects a prince is ruined."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A growing population is a king’s glory; a prince without subjects has nothing."
      }
    },
