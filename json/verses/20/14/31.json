    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.31",
            "paragraphs": [
              "辱沒與尊敬（十四31）",
              "最佳的詮釋是約伯記三十一15，那裏也同樣是根據神是創造主的身分來論述的。還可以加上其他的理由：神的揀選（雅二5）、基督為人時的狀態（太二十五40）、基督徒的誠實（約壹三17、18）。也見本章20、21節，十七5，十九17，二十二2，二十八8，二十九13。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.14.31",
            "paragraphs": [
              "一四31\"憐憫\"。恩待。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "欺压穷人等于侮辱造物主， 怜悯贫弱就是尊敬造物主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "欺壓貧寒的，就是辱罵造他的主； 恩待窮苦人的，就是尊敬造他的主。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "欺壓貧寒的，是辱沒造他的主； 憐憫窮乏的，乃是尊敬主。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who oppresses the poor taunts his Maker, But he who is gracious to the needy honors Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever oppresses the poor shows contempt for their Maker, but whoever is kind to the needy honors God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who oppress the poor insult their Maker, but helping the poor honors him."
      }
    },
