    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.22",
            "paragraphs": [
              "比報復更為甘甜（二十22）",
              "確信神是救主（與審判者，21節），乃是應付自私魯莽的妙方。從十七13開始，經由二十22與二十四29，到二十五21，可以建立起一個逐漸上升的層次。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要说：“我要以恶报恶”， 要等候耶和华替你申冤。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不要說：“我要以惡報惡”； 要等候耶和華，他必拯救你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你不要說，我要以惡報惡； 要等候耶和華，他必拯救你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not say, “I will repay evil”; Wait for the Lord, and He will save you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not say, “I’ll pay you back for this wrong!” Wait for the Lord, and he will avenge you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t say, “I will get even for this wrong.” Wait for the Lord to handle the matter."
      }
    },
