    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.28",
            "paragraphs": [
              "嚴酷不足恃（二十28）",
              "“憐憫”（AV、RV；和合：仁慈），應代以“慈愛”（h]esed[，那些立約之人所保證的愛；參，十九22的註釋）。第二行重復使用這個重要的字（“而他以慈愛維系他的王位”）。這則箴言乃是第26節的補語，其原則同樣可以應用在較低形式之權柄上。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "君王靠慈爱和忠信护佑， 他的王位因慈爱而稳固。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "慈愛和信實保護君王， 他的王位也藉慈愛維持。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王因仁慈和誠實得以保全他的國位， 也因仁慈立穩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Loyalty and truth preserve the king, And he upholds his throne by righteousness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Love and faithfulness keep a king safe; through love his throne is made secure."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Unfailing love and faithfulness protect the king; his throne is made secure through love."
      }
    },
