    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.30",
            "paragraphs": [
              "肉體的刑罰（二十30）",
              "AV的“藍色”是來自Vulg，使希伯來文之“打傷（或割傷）的痕跡”──即“猛烈的擊打”──變得隱晦不明。最後一個片語模仿第27節（見該處註釋心（二十27）}），顯示出良心遲鈍時，可能就需要這樣的擊打。",
              "以賽亞書五十三5的吊詭強烈地突顯出這個背景：因著衪的鞭傷，我們得醫治。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "鞭伤能驱除罪恶， 责打能洁净人心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "鞭傷能除淨人的邪惡， 責打能洗淨人的臟腑。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "鞭伤能洁净人的邪恶； 责打能洁净人的内心深处。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "鞭傷除淨人的罪惡； 責打能入人的心腹。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Blows that wound cleanse away evil; strokes make clean the innermost parts."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Stripes that wound scour away evil, And strokes reach the innermost parts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Blows and wounds scrub away evil, and beatings purge the inmost being."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Blows that hurt cleanse away evil, As do stripes the inner depths of the heart."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Physical punishment cleanses away evil; such discipline purifies the heart."
      }
    },
