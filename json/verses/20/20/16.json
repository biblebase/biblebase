    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.16",
            "paragraphs": [
              "隨時會失去之物（二十16）",
              "“外女”（AV，第二行）：二十七13類似的箴言也是讀作這樣；但這一節經文的希伯來文子音讀作外人（陽性復數）；參和合、RV、RSV。就拿誰的衣服意思是：“不要毫無擔保就借給他（出二十二26）；他是個糟透了的被保人！”見六1～5的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁为陌生人作保，就拿去谁的外衣； 谁为外族人作保，就要承担责任。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "誰為陌生人作保證人，就拿去誰的衣服； 誰替妓女作保證人，誰就要作擔保。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誰為生人作保，就拿誰的衣服； 誰為外人作保，誰就要承當。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Take his garment when he becomes surety for a stranger; And for foreigners, hold him in pledge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Take the garment of one who puts up security for a stranger; hold it in pledge if it is done for an outsider."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Get security from someone who guarantees a stranger’s debt. Get a deposit if he does it for foreigners."
      }
    },
