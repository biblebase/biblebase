    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "骗来的饼觉得香甜， 最后却是满口泥沙。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "欺騙得來的食物，人總覺得甘甜； 事後他的口必充滿沙石。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以虛謊而得的食物，人覺甘甜； 但後來，他的口必充滿塵沙。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Bread obtained by falsehood is sweet to a man, But afterward his mouth will be filled with gravel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Food gained by fraud tastes sweet, but one ends up with a mouth full of gravel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Stolen bread tastes sweet, but it turns to gravel in the mouth."
      }
    },
