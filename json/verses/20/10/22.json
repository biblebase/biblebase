    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.10.22",
            "paragraphs": [
              "不朽的財富（十22）",
              "希伯來文加上一個強調的代名詞（像AV、RV一樣）：“它使人富足”──也就是說：別無他物能以如此。（呂譯、RV、RSV在附註中列出第二行的另一種讀法，現中則置於正文中，這讀法有點牽強，而且產生的含義頗值得懷疑。托伊指出：詩篇一二七2“所主張的並非勞碌本身是無用的，無用的只是沒有神的祝福伴隨在其中的勞碌”。）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的祝福使人富足， 祝福中不加任何忧愁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華所賜的福使人富足， 並不加上愁煩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華所賜的福使人富足， 並不加上憂慮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is the blessing of the Lord that makes rich, And He adds no sorrow to it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The blessing of the Lord brings wealth, without painful toil for it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The blessing of the Lord makes a person rich, and he adds no sorrow with it."
      }
    },
