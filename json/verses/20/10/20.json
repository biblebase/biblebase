    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.10.19-21",
            "paragraphs": [
              "言語──好話與壞話（十19～21）"
            ]
          },
          {
            "verse": "pro.10.20",
            "paragraphs": [
              "20.言語的價值，就是你的價值（註意舌與心之間的平行，和合、呂譯、思高、AV、RV）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人之舌似纯银， 恶人之心无价值。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的舌頭好像純銀， 惡人的心思毫無價值。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人的舌乃似高銀； 惡人的心所值無幾。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The tongue of the righteous is as choice silver, The heart of the wicked is worth little."
      },
