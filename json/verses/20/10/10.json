    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.10.10",
            "paragraphs": [
              "惹禍易，改過難（十10）",
              "照著現有的經文（和合、呂譯註、AV、RV、RSV邊註），本節的意思乃是說：小動作能夠造成大傷害，而喋喋不休的愚昧人則成了難以忍受的。但第二行似乎是受第8b節影響而來的，七十士譯本可能保存了真正的原文，而且形成令人驚奇的對句：“坦白勸責人將締造和平”（呂譯、Moffatt），思高、現中、RSV都接受此一譯法。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "挤眉弄眼，带来忧伤； 胡言乱语，导致灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以眼傳神的，使人受害； 嘴唇愚妄的，必自招滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以眼傳神的，使人憂患； 口裏愚妄的，必致傾倒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who winks the eye causes trouble, And a babbling fool will be ruined."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever winks maliciously causes grief, and a chattering fool comes to ruin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People who wink at wrong cause trouble, but a bold reproof promotes peace."
      }
    },
