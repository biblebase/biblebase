    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.10.19-21",
            "paragraphs": [
              "言語──好話與壞話（十19～21）"
            ]
          },
          {
            "verse": "pro.10.21",
            "paragraphs": [
              "21.言語是好是壞，完全在於你如何使用它們（義人所得的養分足以餵養別人；愚昧人甚至不足以餵養自己）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人的口滋养众人， 愚人因无知而死亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的嘴唇培育多人， 愚妄人因無知死亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人的口教養多人； 愚昧人因無知而死亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The lips of the righteous feed many, But fools die for lack of understanding."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The lips of the righteous nourish many, but fools die for lack of sense."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The words of the godly encourage many, but fools are destroyed by their lack of common sense."
      }
    },
