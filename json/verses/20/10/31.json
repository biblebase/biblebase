    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.10.31",
            "paragraphs": [
              "31.Moffatt譯作“發出智慧的芽苞”，將第一個動詞的意義表明出來。這裏與第32節之乖謬的（和合、AV、RV）是來自字根“轉變、扭曲”，以乖僻為它主要的觀念，同時也有歪曲不正的含義。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人的口发出智慧， 诡诈的舌必被割掉。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的口結出智慧的果子， 乖謬的舌頭必被割除。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "义人的口，发出智慧； 荒谬的舌，必被剪除。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人的口滋生智慧； 乖謬的舌必被割斷。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The mouth of the righteous brings forth wisdom, but the perverse tongue will be cut off."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The mouth of the righteous flows with wisdom, But the perverted tongue will be cut out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the mouth of the righteous comes the fruit of wisdom, but a perverse tongue will be silenced."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The mouth of the righteous brings forth wisdom, But the perverse tongue will be cut out."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The mouth of the godly person gives wise advice, but the tongue that deceives will be cut off."
      }
    },
