    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雇用懒惰人， 如醋倒牙，如烟熏目。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "醋怎樣使牙酸倒，煙怎樣薰目， 懶惰人也怎樣使差他的人難受。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "懶惰人叫差他的人 如醋倒牙，如煙薰目。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like vinegar to the teeth and smoke to the eyes, So is the lazy one to those who send him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As vinegar to the teeth and smoke to the eyes, so are sluggards to those who send them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lazy people irritate their employers, like vinegar to the teeth or smoke in the eyes."
      }
    },
