    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.23.22-25",
            "paragraphs": [
              "引以為榮的兒子（二十三22～25）",
              "這裏包含了第五條誡命的實際內容，直接的（22節）與間接的（23～25節）。也見第12～16節，並分題研究：“Ⅱ父母與兒女”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要听从生养你的父亲， 不可轻视年老的母亲。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要聽從生你的父親； 也不可因為母親老了，就藐視她。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要聽從生你的父親； 你母親老了，也不可藐視她。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Listen to your father who begot you, And do not despise your mother when she is old."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Listen to your father, who gave you life, and do not despise your mother when she is old."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen to your father, who gave you life, and don’t despise your mother when she is old."
      }
    },
