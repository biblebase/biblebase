      "leili": {
        "content": [
          {
            "verse": "pro.21.20",
            "paragraphs": [
              "二一20智慧人懂得計劃，為將來積蓄，愚昧人浪費他所有的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智者家中积存宝物和油， 愚人将所有的挥霍一空。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "智慧人的居所中，積存珍貴的財物和油； 愚昧人卻把他所有的揮霍耗盡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧人家中積蓄寶物膏油； 愚昧人隨得來隨吞下。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is precious treasure and oil in the dwelling of the wise, But a foolish man swallows it up."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wise store up choice food and olive oil, but fools gulp theirs down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wise have wealth and luxury, but fools spend whatever they get."
      }
