        "text": "智者家中积存宝物和油， 愚人将所有的挥霍一空。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "智慧人的居所中，積存珍貴的財物和油； 愚昧人卻把他所有的揮霍耗盡。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "智慧人的居所里积存珍贵的宝物和膏油； 愚昧人却吞尽自己所得的一切。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧人家中積蓄寶物膏油； 愚昧人隨得來隨吞下。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Precious treasure and oil are in a wise man’s dwelling, but a foolish man devours it."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is precious treasure and oil in the dwelling of the wise, But a foolish man swallows it up."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wise store up choice food and olive oil, but fools gulp theirs down."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "There  is desirable treasure, And oil in the dwelling of the wise, But a foolish man squanders it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wise have wealth and luxury, but fools spend whatever they get."
      }
