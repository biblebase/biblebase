    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.21.29",
            "paragraphs": [
              "虛張聲勢的人（二十一29）",
              "第二行的動詞（AV“修直”；和合：堅定）意思是“立定”〔在兩種含義上：就是預作準備（參，31a節）與獲得穩定（參，廿四3b）〕。RSV的“考慮”（LXX；作些微的改變）削弱了動詞之間的對比。這則箴言指出：大膽的面對，不能取代健全的原則。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.21.29",
            "paragraphs": [
              "二一29\"臉無羞恥\"。即以面容表示他的蔑視。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人虚张声势， 正直人谨慎行事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人厚顏無恥， 正直人卻堅定他的道路。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人臉無羞恥； 正直人行事堅定。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A wicked man displays a bold face, But as for the upright, he makes his way sure."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wicked put up a bold front, but the upright give thought to their ways."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wicked bluff their way through, but the virtuous think before they act."
      }
