      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is better to live in a corner of a roof Than in a house shared with a contentious woman."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Better to live on a corner of the roof than share a house with a quarrelsome wife."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It’s better to live alone in the corner of an attic than with a quarrelsome wife in a lovely home."
      }
