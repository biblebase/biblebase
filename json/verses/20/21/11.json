    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "惩罚嘲讽者，愚人得智慧； 训诫智者，他便长见识。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "好譏笑人的受刑罰的時候，愚蒙人就得著智慧； 智慧人受訓誨的時候，他就得著知識。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "褻慢的人受刑罰，愚蒙的人就得智慧； 智慧人受訓誨，便得知識。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the scoffer is punished, the naive becomes wise; But when the wise is instructed, he receives knowledge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When a mocker is punished, the simple gain wisdom; by paying attention to the wise they get knowledge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you punish a mocker, the simpleminded become wise; if you instruct the wise, they will be all the wiser."
      }
    },
