    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.21.7",
            "paragraphs": [
              "他們自己的劊子手（二十一7）",
              "見第5、6節。參亞比米勒與示劍人，被他們自己汲汲追求的權力手段所毀滅：士師記九23、24。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人的暴力必毁灭自己， 因为他们行事不公不义。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人的暴行必拖累自己； 因為他們不肯秉行公正。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人的強暴必將自己掃除， 因他們不肯按公平行事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The violence of the wicked will drag them away, Because they refuse to act with justice."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The violence of the wicked will drag them away, for they refuse to do what is right."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The violence of the wicked sweeps them away, because they refuse to do what is just."
      }
    },
