    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.25.16",
            "paragraphs": [
              "知道適可而止（二十五16）",
              "這個比喻是說明在健全的食欲與貪婪之間的重大差異。從伊甸園開始，人類就想要得到生命中最後的一丁點兒事物，仿佛在神的“夠了”之後乃是狂喜，而不是反胃一樣，也見第27節。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "找到蜂蜜要酌量而食， 免得吃多了呕吐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你找到蜂蜜，吃夠就好了； 免得吃得過多，就嘔吐出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你得了蜜嗎？只可吃夠而已， 恐怕你過飽就嘔吐出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Have you found honey? Eat only what you need, That you not have it in excess and vomit it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you find honey, eat just enough— too much of it, and you will vomit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do you like honey? Don’t eat too much, or it will make you sick!"
      }
    },
