    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.25.28",
            "paragraphs": [
              "懦弱的犧牲品（二十五28）",
              "性急把約束看作只是拘束而已，所以當敵人抵達時，發現墻垣已經都倒下了。相反的情形，見十六32。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.25.28",
            "paragraphs": [
              "二五28一個沒有自制能力的人，很容易受到自己的慾望和一時沖動的興致所攻擊。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无法自制的人就像被攻破的无墙之城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人若不控制自己的心， 就像沒有城牆，毀壞了的城一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人不制伏自己的心， 好像毀壞的城邑沒有牆垣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like a city that is broken into and without walls Is a man who has no control over his spirit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like a city whose walls are broken through is a person who lacks self-control."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A person without self-control is like a city with broken-down walls."
      }
    },
