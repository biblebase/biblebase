    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.25.13",
            "paragraphs": [
              "使人舒暢的忠實（二十五13）",
              "在二十六6有個沈痛的對比，也見十三17（論使者），與二十五25（論舒暢）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.25.13",
            "paragraphs": [
              "二五13\"冰雪的涼氣\"。即從雪山流下來的冰水，或一陣冰涼的山風。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "忠信的使者叫主人心里舒畅， 就像夏收之时有冰雪的凉气。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "忠信的使者對那些差遣他的人來說， 好像在收割的時候，有冰雪的涼氣， 使主人精神舒暢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "忠信的使者叫差他的人心裏舒暢， 就如在收割時有冰雪的涼氣。"
      },
