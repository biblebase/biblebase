    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.16.19",
            "paragraphs": [
              "一六19謙卑地與卑微的人來往，比透過不正當的手段與驕傲人一起致富為好。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "宁可谦卑地与贫寒人相处， 也不跟狂傲人共享战利品。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "存謙卑的心與窮乏人在一起， 勝過與驕傲人同分戰利品。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心裏謙卑與窮乏人來往， 強如將擄物與驕傲人同分。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is better to be humble in spirit with the lowly Than to divide the spoil with the proud."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Better to be lowly in spirit along with the oppressed than to share plunder with the proud."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Better to live humbly with the poor than to share plunder with the proud."
      }
    },
