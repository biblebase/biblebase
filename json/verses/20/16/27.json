    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.16.27-30",
            "paragraphs": [
              "惹是生非的人（十六27～30）",
              "陰謀、惡言漫罵（27節），或只是耳語傳言（28節）；粗暴（29節）、或奸滑（30節）──這些都是足以散播傷害的途徑。見分題研究：“言語的力量”第2點，第46頁。"
            ]
          },
          {
            "verse": "pro.16.27",
            "paragraphs": [
              "27.“揭發”（AV；和合：圖謀），更好是譯作挖掘：這是用來指陰謀的一個平凡字眼，是從挖坑而來（參，二十六27等）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不务正业的人图谋恶事， 他的口舌如烈焰般危险。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "無賴之徒挖出邪惡， 他口裡的話好像灼熱的火。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "匪徒圖謀奸惡， 嘴上彷彿有燒焦的火。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A worthless man digs up evil, While his words are like scorching fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A scoundrel plots evil, and on their lips it is like a scorching fire."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Scoundrels create trouble; their words are a destructive blaze."
      }
    },
