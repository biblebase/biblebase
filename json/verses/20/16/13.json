    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.16.10-15",
            "paragraphs": [
              "權勢的重擔（十六10～15）"
            ]
          },
          {
            "verse": "pro.16.12-13",
            "paragraphs": [
              "12～13.王上好的益虛。這兩則箴言是建立在一個假設上：王的心思是正確的──良心既末聾聵（12b節），也不昧於實話（13b節）。既然大部分人都在某個方面使用權柄，而且企圖將憎惡與喜悅的合適對象互換，這則箴言就不只具有理論上的益處而已，且是十分實用。",
              "第12b節重復出現在二十五5，見二十五1～7，十四34。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.16.10-15",
            "paragraphs": [
              "一六10～15君王的責任。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王喜爱公义的言词， 器重说话正直的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "公義的嘴唇是君王所喜悅的； 他喜愛說話正直的人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "公义的嘴唇，为君王所悦纳； 他喜爱说话正直的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "公義的嘴為王所喜悅； 說正直話的，為王所喜愛。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Righteous lips are the delight of a king, and he loves him who speaks what is right."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Righteous lips are the delight of kings, And he who speaks right is loved."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Kings take pleasure in honest lips; they value the one who speaks what is right."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Righteous lips are the delight of kings, And they love him who speaks what  is right."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king is pleased with words from righteous lips; he loves those who speak honestly."
      }
    },
