      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who is slow to anger is better than the mighty, And he who rules his spirit, than he who captures a city."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Better a patient person than a warrior, one with self-control than one who takes a city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Better to be patient than powerful; better to have self-control than to conquer a city."
      }
    },
