    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.16.1-9",
            "paragraphs": [
              "耶和華主宰的權柄（十六1～9）"
            ]
          },
          {
            "verse": "pro.16.8",
            "paragraphs": [
              "8.無論如何，總要諴實。與十五16相近的一個訓誨，以絕對道德化的詞語來陳述後者以福祉來說明之事，見分題研究：“神與人”，原書第27～32頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.16.1-9",
            "paragraphs": [
              "一六1～9本段的論題是神以及祂對人類活動最終的控制。"
            ]
          },
          {
            "verse": "pro.16.8",
            "paragraphs": [
              "一六8\"財利\"指收入。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "财物虽少但行事公义， 胜过家财万贯却不公义。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "收入少而有公義， 勝過收入多卻毫無正義。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "多有財利，行事不義， 不如少有財利，行事公義。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Better is a little with righteousness Than great income with injustice."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Better a little with righteousness than much gain with injustice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Better to have little, with godliness, than to be rich and dishonest."
      }
    },
