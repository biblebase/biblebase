    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚昧人背离正道， 自招灭亡； 愚顽人逍遥自在， 毁掉自己。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚蒙人的背道必殺害他們自己， 愚昧人的安逸必毀滅他們自己；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚昧人背道，必殺己身； 愚頑人安逸，必害己命。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the waywardness of the naive will kill them, And the complacency of fools will destroy them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the waywardness of the simple will kill them, and the complacency of fools will destroy them;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For simpletons turn away from me—to death. Fools are destroyed by their own complacency."
      }
    },
