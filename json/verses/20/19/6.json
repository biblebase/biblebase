    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.19.6",
            "paragraphs": [
              "一九6\"好施散的\"。更可作：慷慨的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大家都讨好慷慨的人， 人人都结交好施赠的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "尊貴的人，很多人求他的情面； 慷慨施贈的，人人都作他的朋友。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "好施散的，有多人求他的恩情； 愛送禮的，人都為他的朋友。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Many will seek the favor of a generous man, And every man is a friend to him who gives gifts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Many curry favor with a ruler, and everyone is the friend of one who gives gifts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Many seek favors from a ruler; everyone is the friend of a person who gives gifts!"
      }
    },
