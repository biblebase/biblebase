    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.12",
            "paragraphs": [
              "獅子與甘露（十九12）",
              "見十六14、15，二十2的註釋。下屬可以在此習得機智老練，上司則能習得和藹可親。這則箴言位在第11節之後，或許是要強調，有權之人的恩惠，乃是前一節的素養安靜結出的果子。參大衛對於統治者的理想，撒母耳記下二十三3、4，以及神給以色列人的保證，何西阿書十四5。",
              "家庭──地獄或天堂（十九13、14）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "君王的震怒像雄狮怒吼， 君王的恩泽如草上甘露。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王的忿怒，好像獅子的吼叫， 他的恩寵，如同草上的朝露。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王的忿怒好像獅子吼叫； 他的恩典卻如草上的甘露。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king’s wrath is like the roaring of a lion, But his favor is like dew on the grass."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A king’s rage is like the roar of a lion, but his favor is like dew on the grass."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king’s anger is like a lion’s roar, but his favor is like dew on the grass."
      }
    },
