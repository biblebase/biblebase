    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.18",
            "paragraphs": [
              "致命的寬容（十九18）",
              "第二行應該像RV、RSV一樣譯作“不可心中定意（參，何四8；結二十四25）叫他死亡”。禁止管教既不是禮貌上的致意，也不是仁慈；機會會迅速消逝，參列王紀上一5、6。見分題研究：“Ⅱ父母與兒女”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.19.18",
            "paragraphs": [
              "一九18\"你的心不可任他死亡\"。即不要忽略孩子的管教，這會導致他的死亡。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "管教孩子宜早不宜晚， 不可任由他走向灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "趁著還有指望的時候，要管教你的兒子； 不可存心使他死亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "趁有指望，管教你的兒子； 你的心不可任他死亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Discipline your son while there is hope, And do not desire his death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Discipline your children, for in that there is hope; do not be a willing party to their death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Discipline your children while there is hope. Otherwise you will ruin their lives."
      }
