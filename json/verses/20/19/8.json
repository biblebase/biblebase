    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.8",
            "paragraphs": [
              "見識是自給自足的（十九8）",
              "這裏的智慧乃是“見識”，與何西阿書七11和箴言十五32b所用的是同一個字（直譯“心”），這則箴言為後者提出了一個極佳的高峰。它在八35、36以更深的層次作類似的敘述。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "得到智慧的珍惜生命， 持守悟性的享受福乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "得著智慧的，愛惜自己的性命； 保持明達的，必得益處。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "得着智慧的，愛惜生命； 保守聰明的，必得好處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who gets wisdom loves his own soul; He who keeps understanding will find good."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The one who gets wisdom loves life; the one who cherishes understanding will soon prosper."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "To acquire wisdom is to love yourself; people who cherish understanding will prosper."
      }
