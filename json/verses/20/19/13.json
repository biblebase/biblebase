    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.13",
            "paragraphs": [
              "13.爭吵（和合、AV、RV）：Moffatt的譯法“嘮叨挑剔”較出色。論到滴水的明喻（在二十七15、16較充分地處理），德裏慈提及魏次坦（Wetzstein）所告訴他的一則亞拉伯格言，說到（用托伊較緊湊的概述來說）：“有三件事會使一棟屋子成為難以忍受的：tak（雨水滲進來的漏洞）、nak（妻子的嘮叨挑剔），與bak（病蟲害）。”"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚昧之子是父亲的灾殃， 争闹之妻如雨滴漏不止。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚昧的兒子是父親的禍患， 吵鬧的妻子好像雨水不停地滴漏。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚昧的兒子是父親的禍患； 妻子的爭吵如雨連連滴漏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A foolish son is destruction to his father, And the contentions of a wife are a constant dripping."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A foolish child is a father’s ruin, and a quarrelsome wife is like the constant dripping of a leaky roof."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A foolish child is a calamity to a father; a quarrelsome wife is as annoying as constant dripping."
      }
    },
