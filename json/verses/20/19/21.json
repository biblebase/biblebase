    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人心中有许多计划， 唯耶和华的旨意成就。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人心裡的謀算很多， 唯有耶和華的計劃能實現。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人心多有計謀； 惟有耶和華的籌算才能立定。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Many plans are in a man’s heart, But the counsel of the Lord will stand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Many are the plans in a person’s heart, but it is the Lord’s purpose that prevails."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You can make many plans, but the Lord’s purpose will prevail."
      }
    },
