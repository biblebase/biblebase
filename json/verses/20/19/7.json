    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "穷人被亲人厌弃， 朋友都远远躲避。 他苦苦哀求，也无人理会。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "貧窮人的兄弟都恨他； 他的朋友更疏遠他。 雖然他多多懇求他們，他們卻不理會。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "貧窮人，弟兄都恨他； 何況他的朋友，更遠離他！ 他用言語追隨，他們卻走了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the brothers of a poor man hate him; How much more do his friends abandon him! He pursues them with words, but they are gone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The poor are shunned by all their relatives— how much more do their friends avoid them! Though the poor pursue them with pleading, they are nowhere to be found."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The relatives of the poor despise them; how much more will their friends avoid them! Though the poor plead with them, their friends are gone."
      }
    },
