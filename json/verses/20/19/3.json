    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.3",
            "paragraphs": [
              "永遠是神錯了（十九3）",
              "現代譯本將希伯來文所暗示“反對耶和華”的重點表明出來，也就是說：我們自找的苦頭，卻怪罪於神。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.19.3",
            "paragraphs": [
              "一九3愚昧人因自己造成的失敗而埋怨耶和華。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人因愚昧而自毁前程， 他的心却抱怨耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人的愚妄毀滅自己的道路； 他的心卻惱怒耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人的愚昧傾敗他的道； 他的心也抱怨耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The foolishness of man ruins his way, And his heart rages against the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A person’s own folly leads to their ruin, yet their heart rages against the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People ruin their lives by their own foolishness and then are angry at the Lord."
      }
    },
