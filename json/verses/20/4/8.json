    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.4.1-27",
            "paragraphs": [
              "終身的旅程（四1～27）",
              "四1～9.尋求。三代之間的這個連鎖（1、3節及下），說明了對最好事物之喜愛是如何傳承的，主要是藉著本身的影響力，沿著情感的管道。進路是積極的：教師對於“不可”（雖然它們有它們的地位，見三27～31）的興趣，遠不如讓他的學生看見他所提供的乃是真正能以存活的秘訣（4b節），而智慧就是這個秘訣，因而使學生也能配合，來培養他自己對於智慧的愛好（6節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.4.1-27",
            "paragraphs": [
              "四1～27智慧提供許多的利益：關顧與榮譽（1～9節）；指教人走義人的路（10～19節）；以及生命、健康、正直（20～27節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "高举智慧，她必使你受尊崇； 拥抱智慧，她必使你得尊荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要高舉智慧，智慧就必使你高升； 你要懷抱智慧，智慧就必使你得尊榮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "高舉智慧，她就使你高升； 懷抱智慧，她就使你尊榮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Prize her, and she will exalt you; She will honor you if you embrace her."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Cherish her, and she will exalt you; embrace her, and she will honor you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you prize wisdom, she will make you great. Embrace her, and she will honor you."
      }
