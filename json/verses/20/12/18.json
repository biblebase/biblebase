    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.17-19",
            "paragraphs": [
              "言語──好的與壞的（十二17～19）",
              "見分題研究：“言語”及附屬部分，原書第45～48頁。"
            ]
          },
          {
            "verse": "pro.12.18",
            "paragraphs": [
              "18.言語的深入。第一行之動詞含有莽撞的觀念（差不多是“脫口而出”），是AV所疏漏的。它在詩篇一○六33用來指摩西的暴躁。Moffatt將第二行的對比表達得相當好：“但深思的言語含有醫治人的能力”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "出言不慎犹如利剑伤人， 智者之言却能医治创伤。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有人說話不慎，好像利刀刺人， 智慧人的舌頭卻能醫治人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說話浮躁的，如刀刺人； 智慧人的舌頭卻為醫人的良藥。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is one who speaks rashly like the thrusts of a sword, But the tongue of the wise brings healing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The words of the reckless pierce like swords, but the tongue of the wise brings healing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some people make cutting remarks, but the words of the wise bring healing."
      }
    },
