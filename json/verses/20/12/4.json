    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.4",
            "paragraphs": [
              "出色的妻子（十二4）",
              "才德（和合、呂譯、AV、RV）在現代的含義，不足以說明這個字的希伯來文字根中力量與價值的觀念（參，思高“賢能”；見三十一10的註釋；並參該節以後之經文所作之詳盡的描繪）。現代人所說：“她頗有分量”，稍微表達了它的意義。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.12.4",
            "paragraphs": [
              "一二4\"才德的婦人\"。品格尊貴而正直的妻子（比較三一10～31）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贤德的妻子是丈夫的冠冕， 无耻的妻子如丈夫的骨瘤。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有才德的妻子是丈夫的冠冕， 貽羞的婦人，如同丈夫骨中的朽爛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "才德的婦人是丈夫的冠冕； 貽羞的婦人如同朽爛在她丈夫的骨中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "An excellent wife is the crown of her husband, But she who shames him is like rottenness in his bones."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A wife of noble character is her husband’s crown, but a disgraceful wife is like decay in his bones."
