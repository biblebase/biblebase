    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.8",
            "paragraphs": [
              "正確的判斷力贏得尊敬（十二8）",
              "這裏所論及的智慧（和合、AV、RV）是亞比該所表現出來的那種（s*ekel，撒上二十五3；參，分題研究：“Ⅰ多面的智慧”第3點）；在第二行之相對的素質所暗示的，乃是沒有能力率直地思想，托伊的提議相當出色：“別扭的”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人有智慧受称赞， 心术不正遭唾弃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人因著自己的明慧必得稱讚， 但心中乖謬的必被藐視。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人必按自己的智慧被稱讚； 心中乖謬的，必被藐視。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A man will be praised according to his insight, But one of perverse mind will be despised."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A person is praised according to their prudence, and one with a warped mind is despised."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A sensible person wins admiration, but a warped mind is despised."
      }
    },
