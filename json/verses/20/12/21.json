    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.21",
            "paragraphs": [
              "得勝有余（十二21）",
              "用來指遭（和合、呂譯、AV、RV；參，詩九十一10）的動詞，所暗示的是奉差遣或被允許。這個定律精確的含義，乃是約伯的安慰者唯一的憑藉；但正確地應用，它乃是一個有激勵作用的真理，對保羅（羅八28；連同36、37節）與約瑟（創五十20）而言都是有效的──在亨通時是廉價的，在苦難中卻是難能可貴的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人无往不利， 恶人灾祸连连。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人不會遭遇災禍， 惡人卻飽經禍患。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人不遭災害；惡人滿受禍患。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No harm befalls the righteous, But the wicked are filled with trouble."
