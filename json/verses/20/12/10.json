      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人連自己牲畜的性命也顧惜； 但惡人的憐憫也是殘忍。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人顧惜他牲畜的命； 惡人的憐憫也是殘忍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A righteous man has regard for the life of his animal, But even the compassion of the wicked is cruel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The righteous care for the needs of their animals, but the kindest acts of the wicked are cruel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The godly care for their animals, but the wicked are always cruel."
      }
    },
