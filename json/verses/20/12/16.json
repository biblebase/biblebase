    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.16",
            "paragraphs": [
              "自制乃是辨別力（十二16）",
              "RSV正確地跟隨了Moffatt的第二行：“精明人無視於侮辱”（參，現中；AV、RV之“羞辱”的這種意義，參，九7a）。這一行的動詞與十12b的一樣，該處之類似的緘默甚至還有更好的動機。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.12.16",
            "paragraphs": [
              "一二16愚妄人被激怒便生氣，精明人卻不理會別人的侮辱。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚人难压怒气， 明哲忍辱负重。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚妄人的惱怒立時顯露， 精明的人卻能忍受羞辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚妄人的惱怒立時顯露； 通達人能忍辱藏羞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A fool’s anger is known at once, But a prudent man conceals dishonor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Fools show their annoyance at once, but the prudent overlook an insult."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A fool is quick-tempered, but a wise person stays calm when insulted."
      }
    },
