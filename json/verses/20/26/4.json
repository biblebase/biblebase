        "lang": "chs",
        "text": "别照愚人的愚昧回答他， 免得你像他一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要照著愚昧人的愚妄回答他， 免得你像他一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要照愚昧人的愚妄話回答他， 恐怕你與他一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not answer a fool according to his folly, Or you will also be like him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not answer a fool according to his folly, or you yourself will be just like him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t answer the foolish arguments of fools, or you will become as foolish as they are."
      }
    },
