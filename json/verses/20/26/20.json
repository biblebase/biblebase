    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.26.17-28",
            "paragraphs": [
              "惹是生非的人（二十六17～28）"
            ]
          },
          {
            "verse": "pro.26.20-28",
            "paragraphs": [
              "20～28.惡意的言談。這件惡事的不同層面引起了不同的描繪。",
              "20、21.炭。煽動余火的，乃是說悄悄話的人或好爭競的人自己，而不是（像他所說的）實話本身；因為他的心思把事實重新設計成燃料。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.26.17-28",
            "paragraphs": [
              "二六17～28這談及那卑鄙或胡鬧的人。"
            ]
          },
          {
            "verse": "pro.26.20",
            "paragraphs": [
              "二六20\"爭競便止息\"。爭辯就平靜下來。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "没有木柴，火自然熄灭； 没有闲话，争端便平息。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有柴，火就熄滅； 沒有搬弄是非的人，紛爭就平息。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "火缺了柴就必熄滅； 無人傳舌，爭競便止息。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For lack of wood the fire goes out, And where there is no whisperer, contention quiets down."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Without wood a fire goes out; without a gossip a quarrel dies down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fire goes out without wood, and quarrels disappear when gossip stops."
      }
    },
