    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.17.18",
            "paragraphs": [
              "無限的責任（十七18）",
              "這與第17節並無矛盾。它並不是反對人幫助有需要的朋友，而是反對人盲目地作保，那是有可能會使受保者流於輕率，使倆人都趨於破產的。進一步見六1～5。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.17.18",
            "paragraphs": [
              "一七18\"擊掌\"。即握手確證合約的生效。\"保\"。聯署人，借貸者若不履行責任，他便要負責、承擔債務。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无知的人才会为他人作保。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為鄰舍擊掌作保證人的， 是無知的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在鄰舍面前擊掌作保 乃是無知的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A man lacking in sense pledges And becomes guarantor in the presence of his neighbor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One who has no sense shakes hands in pledge and puts up security for a neighbor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It’s poor judgment to guarantee another person’s debt or put up security for a friend."
      }
    },
