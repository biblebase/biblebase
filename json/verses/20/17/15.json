    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.17.15",
            "paragraphs": [
              "不公平（十七15）",
              "Moffatt作：“永恒者厭惡他們的配對”。參律法書與先知書所說的（出二十三7；賽五23）。這就是新約聖經中羅馬書四5與三26那令人雀躍之信息有力的舊約背景：“……好使他為義，而義人……”（AV）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "放过罪人、冤枉义人， 都为耶和华所憎恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "宣判惡人為義，裁定義人有罪， 二者都是耶和華所厭惡的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "定惡人為義的，定義人為惡的， 這都為耶和華所憎惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who justifies the wicked and he who condemns the righteous, Both of them alike are an abomination to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Acquitting the guilty and condemning the innocent— the Lord detests them both."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Acquitting the guilty and condemning the innocent— both are detestable to the Lord."
      }
    },
