        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "嘲笑穷人等于侮辱造物主， 幸灾乐祸的人必难逃惩罚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "嘲笑窮人的就是辱罵造他的主； 幸災樂禍的必難免受懲罰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "戲笑窮人的，是辱沒造他的主； 幸災樂禍的，必不免受罰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who mocks the poor taunts his Maker; He who rejoices at calamity will not go unpunished."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever mocks the poor shows contempt for their Maker; whoever gloats over disaster will not go unpunished."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who mock the poor insult their Maker; those who rejoice at the misfortune of others will be punished."
      }
    },
