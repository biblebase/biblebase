      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "懲罰義人，已是不當； 擊打正直的官長，更是不妥。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "刑罰義人為不善； 責打君子為不義。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is also not good to fine the righteous, Nor to strike the noble for their uprightness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If imposing a fine on the innocent is not good, surely to flog honest officials is not right."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is wrong to punish the godly for being good or to flog leaders for being honest."
      }
    },
