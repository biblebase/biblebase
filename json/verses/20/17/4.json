    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.17.4",
            "paragraphs": [
              "難辭其咎的聽眾（十七4）",
              "邪惡的言語若沒人歡迎，自然就會平息下來；而有人歡迎，就會使我們原形畢露（見分題研究：“Ⅱ言語的弱點”第3點，原書第46～47頁）。聽眾另一面的角色出現在第10節。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.17.4",
            "paragraphs": [
              "一七4\"邪惡\"。更可作：破壞。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "作恶者留心听恶言， 说谎者侧耳听坏话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "作惡的人留心聽邪惡的話； 說謊的人側耳聽攻擊人的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行惡的，留心聽奸詐之言； 說謊的，側耳聽邪惡之語。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "An evildoer listens to wicked lips; A liar pays attention to a destructive tongue."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A wicked person listens to deceitful lips; a liar pays attention to a destructive tongue."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wrongdoers eagerly listen to gossip; liars pay close attention to slander."
      }
    },
