    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.17.21",
            "paragraphs": [
              "不肖子（十七21）",
              "參第25節，與十1，十五20，也見分題研究：“Ⅱ愚昧人”第1點，原書第37～39頁。第二行的愚頑人是na{b[a{l，見第7節。",
              "較廣的應用，見希伯來書十三7；約翰三書4。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "生愚昧子带来忧伤， 愚人之父毫无喜乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "生下愚昧的兒子，使父親憂愁； 愚頑人的父親毫無喜樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "生愚昧子的，必自愁苦； 愚頑人的父毫無喜樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who sires a fool does so to his sorrow, And the father of a fool has no joy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To have a fool for a child brings grief; there is no joy for the parent of a godless fool."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is painful to be the parent of a fool; there is no joy for the father of a rebel."
      }
    },
