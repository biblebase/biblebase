    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚人缄默可算为智慧， 闭口不言可算为明智。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚妄人默不作聲，也算是智慧； 閉口不言，也算是聰明。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚昧人若靜默不言也可算為智慧； 閉口不說也可算為聰明。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even a fool, when he keeps silent, is considered wise; When he closes his lips, he is considered prudent."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even fools are thought wise if they keep silent, and discerning if they hold their tongues."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even fools are thought wise when they keep silent; with their mouths shut, they seem intelligent."
      }
    },
