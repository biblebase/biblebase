    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.13.17",
            "paragraphs": [
              "使者──好的與壞的（十三17）",
              "根據第二行的類比，我們可能應該把這則箴言第一個動詞解釋為使役動詞（原封不動地保留希伯來文子音），像呂譯、思高、現中、RSV一樣：“……使人陷於禍患裏”。也見二十五13，二十六6。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "奸恶的使者陷入灾祸， 忠诚的使者带来医治。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "奸惡的使者，必陷在禍患裡； 忠心的使臣，使人復原。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "奸惡的使者必陷在禍患裏； 忠信的使臣乃醫人的良藥。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A wicked messenger falls into adversity, But a faithful envoy brings healing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A wicked messenger falls into trouble, but a trustworthy envoy brings healing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "An unreliable messenger stumbles into trouble, but a reliable messenger brings healing."
      }
