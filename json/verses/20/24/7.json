        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧極高，非愚昧人所能及， 所以在城門內不敢開口。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Wisdom is too exalted for a fool, He does not open his mouth in the gate."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
