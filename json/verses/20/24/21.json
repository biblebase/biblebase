      "leili": {
        "content": [
          {
            "verse": "pro.24.21",
            "paragraphs": [
              "二四21\"反復無常的人\"。即反對民權的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
