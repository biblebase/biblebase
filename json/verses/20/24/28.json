    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.24.23-34",
            "paragraphs": [
              "Ⅲｂ智慧人的言語──續（二十四23～34）"
            ]
          },
          {
            "verse": "pro.24.28",
            "paragraphs": [
              "無理的指控（二十四28）",
              "第二行（直譯為：“而你要欺騙……嗎？”）顯示出：這則箴言的目標不是只在於愛管閑事的人（如：三30，二十五8～10），而是在於虛假的控告者，這是神（六19）與法律（申十九18～21）所憎惡的。也見十四5所列的參考經文。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "别无故作证害邻舍， 也不可撒谎欺骗人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不可無故作證指控你的鄰舍； 不可誣告他人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可無故作見證陷害鄰舍， 也不可用嘴欺騙人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not be a witness against your neighbor without cause, And do not deceive with your lips."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not testify against your neighbor without cause— would you use your lips to mislead?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t testify against your neighbors without cause; don’t lie about them."
      }
    },
