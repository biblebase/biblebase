    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.29",
            "paragraphs": [
              "你正在傷害的，乃是你自己（十一29）",
              "這是第17節所引入之要旨的一個變通的說法；亦見該處對擾害這個字所作的解釋，它在這裏可以譯作“使……不安定”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祸害自家，必一无所有， 愚人必做智者的仆役。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "禍害自己家庭的，必承受清風； 愚妄人必作心思智慧的人的僕人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "擾害己家的，必承受清風； 愚昧人必作慧心人的僕人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who troubles his own house will inherit wind, And the foolish will be servant to the wisehearted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever brings ruin on their family will inherit only wind, and the fool will be servant to the wise."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who bring trouble on their families inherit the wind. The fool will be a servant to the wise."
      }
