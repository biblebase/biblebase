    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.12",
            "paragraphs": [
              "12.感受智慧，最容易誤導人的方式乃是自以為優越（十四21更往前一步：它乃是罪），因為這樣的人是在否認，神才是唯一有資格斷定人的價值之審判官。和合、呂譯、RSV，恰當地譯作藐視……的，像第二行一樣暗示著：說出這樣的判斷是更為愚昧的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.11.12",
            "paragraphs": [
              "一一12\"藐視\"。輕視或貶低別人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无知者轻视邻舍， 明哲人保持缄默。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "藐視鄰舍的，真是無知； 聰明人卻緘默不言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "藐視鄰舍的，毫無智慧； 明哲人卻靜默不言。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who despises his neighbor lacks sense, But a man of understanding keeps silent."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever derides their neighbor has no sense, but the one who has understanding holds their tongue."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is foolish to belittle one’s neighbor; a sensible person keeps quiet."
      }
    },
