    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.27",
            "paragraphs": [
              "你為別人所求的，你自己必得著（十一27）",
              "構成懇切求的只有一個字，所提示的意義是守夜等候黎明來到；見一28的註釋\\cf0。關於這則箴言一般的要旨，參第17節。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "寻求良善得恩惠， 追求罪恶遭祸患。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "殷切求善的，必得到愛顧； 追求邪惡的，邪惡必臨到他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "懇切求善的，就求得恩惠； 惟獨求惡的，惡必臨到他身。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who diligently seeks good seeks favor, But he who seeks evil, evil will come to him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever seeks good finds favor, but evil comes to one who searches for it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you search for good, you will find favor; but if you search for evil, it will find you!"
      }
