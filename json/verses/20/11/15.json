    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.15",
            "paragraphs": [
              "持久的保證（十一15）",
              "見六1～5，該處詳細地論述這一點；參十七18。安穩（和合、AV、RV，第二行），較好是譯作“安心無慮”（呂譯）。AV、RV中所使用的雙關語，在希伯來文中是沒有的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.11.15-16",
            "paragraphs": [
              "一一15,16比較第六章1節。安靜的婦女能贏得榮譽，而行使殘暴的武力只會得物質上的財產。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为他人作保，必受亏损； 厌恶当保人，得享平安。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為外人作保證人的，必受虧損； 厭惡替人擊掌擔保的，卻得著安穩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為外人作保的，必受虧損； 恨惡擊掌的，卻得安穩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who is guarantor for a stranger will surely suffer for it, But he who hates being a guarantor is secure."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever puts up security for a stranger will surely suffer, but whoever refuses to shake hands in pledge is safe."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There’s danger in putting up security for a stranger’s debt; it’s safer not to guarantee another person’s debt."
      }
    },
