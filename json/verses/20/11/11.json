    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.11.10-11",
            "paragraphs": [
              "一一10,11義人的生活（傳播道德）和惡人的死亡（排除某些罪）對全\"城\"的人都有好的影響。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "城因正直人的祝福而兴盛， 因恶人的口舌而倾覆。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因正直人的祝福，城的地位就提高； 因惡人的口，城就傾覆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "城因正直人祝福便高舉， 卻因邪惡人的口就傾覆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By the blessing of the upright a city is exalted, But by the mouth of the wicked it is torn down."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Through the blessing of the upright a city is exalted, but by the mouth of the wicked it is destroyed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Upright citizens are good for a city and make it prosper, but the talk of the wicked tears it apart."
      }
    },
