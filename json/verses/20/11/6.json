    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.3-9",
            "paragraphs": [
              "公義助人到底（十一3～9）",
              "第3～5節將公義與生命中復雜的抉擇連接起來，又將團體中的其余人與其危險連在一起。第3a、5a節一個驚人的例證乃是約瑟，他的純正乃是最佳的引導；但要緊的是：必須要做的誠實事必定是智慧的事（九10）。關於智慧所提供給人的安全（4、6～9節），見十2、3的註釋，並分題研究：“生命與死亡”及附屬部分，原書第53～57頁。"
            ]
          },
          {
            "verse": "pro.11.6",
            "paragraphs": [
              "6.“變節者”（RSV）或“叛徒”比AV的“罪人”更為準確（和合、呂譯、思高、現中皆作“奸詐人”）；而最後一句話可能應該譯作“陷落在自己的貪婪裏”（現中、呂譯、RSV）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.11.6",
            "paragraphs": [
              "一一6\"奸詐人\"。比較第十一章3節及其腳註。\"罪孽\"。直譯作：貪婪。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "正直人被义行所救， 奸诈者被私欲所掳。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正直人的公義必拯救自己， 但奸詐人必陷溺於自己的惡慾中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正直人的義必拯救自己； 奸詐人必陷在自己的罪孽中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The righteousness of the upright will deliver them, But the treacherous will be caught by their own greed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The righteousness of the upright delivers them, but the unfaithful are trapped by evil desires."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The godliness of good people rescues them; the ambition of treacherous people traps them."
      }
    },
