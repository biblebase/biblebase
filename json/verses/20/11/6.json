        "text": "正直人的公義必拯救自己， 但奸詐人必陷溺於自己的惡慾中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正直人的義必拯救自己； 奸詐人必陷在自己的罪孽中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The righteousness of the upright will deliver them, But the treacherous will be caught by their own greed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The righteousness of the upright delivers them, but the unfaithful are trapped by evil desires."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The godliness of good people rescues them; the ambition of treacherous people traps them."
      }
    },
