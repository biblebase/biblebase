    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.14",
            "paragraphs": [
              "人人效忠的君王（二十九14）",
              "參第4節，與十六12。要考驗一個掌權的人和他內在的力量，要看他對那些最不能向他施加壓力的人，是否仍舊誠實、忠心。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
