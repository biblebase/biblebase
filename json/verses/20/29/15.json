        "content": [
          {
            "verse": "pro.29.15",
            "paragraphs": [
              "杖打與責備（二十九15）",
              "參第17節，與分題研究：“Ⅱ父母與兒女”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "管教之杖使孩子得智慧， 放纵的子女让母亲蒙羞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "杖責和管教能使人有智慧， 放縱的孩子使母親蒙羞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "杖打和責備能加增智慧； 放縱的兒子使母親羞愧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The rod and reproof give wisdom, But a child who gets his own way brings shame to his mother."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A rod and a reprimand impart wisdom, but a child left undisciplined disgraces its mother."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "To discipline a child produces wisdom, but a mother is disgraced by an undisciplined child."
      }
    },
