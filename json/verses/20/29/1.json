      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "屡教不改、顽固不化者， 必突然灭亡，无可挽救。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人屢次受責備，仍然硬著頸項， 他必突然毀滅，無法挽救。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人屢次受責罰，仍然硬着頸項； 他必頃刻敗壞，無法可治。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A man who hardens his neck after much reproof Will suddenly be broken beyond remedy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever remains stiff-necked after many rebukes will suddenly be destroyed—without remedy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Whoever stubbornly refuses to accept criticism will suddenly be destroyed beyond recovery."
      }
    },
