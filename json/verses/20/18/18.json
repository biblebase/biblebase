    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.18.18",
            "paragraphs": [
              "我們的平安乃是祂的旨意（十八18）",
              "與這一則箴言所暗示之忠告同義的說法是：當利益或意見沖突時，要尋求神的引導，也要欣然接受它。論掣簽，見十六33註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.18.18",
            "paragraphs": [
              "一八18借\"掣簽\"來作決定，能分開敵對的雙方，使和諧得到恢復。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "抽签可以平息争端， 调解强者间的纠纷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "抽籤能止息紛爭， 排解二強之間的糾紛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掣籤能止息爭競， 也能解散強勝的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The cast lot puts an end to strife And decides between the mighty ones."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Casting the lot settles disputes and keeps strong opponents apart."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Flipping a coin can end arguments; it settles disputes between powerful opponents."
      }
