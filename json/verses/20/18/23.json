    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.18.23",
            "paragraphs": [
              "冷酷的現實（十八23）",
              "這樣立場超然的報導，連同它明確缺少解釋，使讀者面對他所居住之世界的醜陋（參，十九4、6、7的諂媚與無情；與神的法則相反），並且提醒他要安靜地接受其痛苦與斬獲（參，雅一9、10，五6、7）。詳加解說的評價，見十九1。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.18.23",
            "paragraphs": [
              "一八23這句箴言只是說出世界上醜陋的現實。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "穷人哀声恳求， 富人恶言相向。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "窮人乞恩哀求， 財主卻聲色俱厲地回答。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "貧窮人說哀求的話； 富足人用威嚇的話回答。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The poor man utters supplications, But the rich man answers roughly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The poor plead for mercy, but the rich answer harshly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The poor plead for mercy; the rich answer with insults."
      }
