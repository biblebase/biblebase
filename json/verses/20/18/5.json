    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.18.5",
            "paragraphs": [
              "偏愛徇私（十八5）",
              "見二十八21，十七26。",
              "多言惹禍（十八6、7）",
              "“進入”（6節，AV、RV）：希伯來文可以讀作動詞，也就是“帶給（他）”，七十士譯本即如此譯。Moffatt表達得很好：“愚昧人的談論使他陷入麻煩”（即爭端）。",
              "參分題研究：“Ⅱ愚昧人”第1點，原書第37～39頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "袒护恶人、冤枉义人都为不善。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在審判時，偏袒惡人、 屈枉義人，是不對的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瞻徇惡人的情面， 偏斷義人的案件，都為不善。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To show partiality to the wicked is not good, Nor to thrust aside the righteous in judgment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is not good to be partial to the wicked and so deprive the innocent of justice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is not right to acquit the guilty or deny justice to the innocent."
      }
    },
