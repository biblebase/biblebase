    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.18.20-21",
            "paragraphs": [
              "一八20,21人的舌頭能為他帶來滿足、生命或死亡。那些傾向使用舌頭的人必須接受言語所帶來的後果。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人因口中所说的话而饱足， 因嘴中所出的言语而满足。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人口中所結的果子，必使自己的肚腹飽足； 他嘴裡所出的，必使他飽足。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人口中所結的果子，必充滿肚腹； 他嘴所出的，必使他飽足。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "With the fruit of a man’s mouth his stomach will be satisfied; He will be satisfied with the product of his lips."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the fruit of their mouth a person’s stomach is filled; with the harvest of their lips they are satisfied."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wise words satisfy like a good meal; the right words bring satisfaction."
      }
    },
