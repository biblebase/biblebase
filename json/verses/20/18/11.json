    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.18.11",
            "paragraphs": [
              "一八11\"富足人\"幻想他的財富能保護他。\"心想\"。幻想。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "财富是富人的坚城， 在他幻想中是高墙。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "富翁的財物是他們的堅城； 在他們的想像中，有如高牆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "富足人的財物是他的堅城， 在他心想，猶如高牆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A rich man’s wealth is his strong city, And like a high wall in his own imagination."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wealth of the rich is their fortified city; they imagine it a wall too high to scale."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The rich think of their wealth as a strong defense; they imagine it to be a high wall of safety."
      }
    },
