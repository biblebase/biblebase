    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.18.20-21",
            "paragraphs": [
              "一八20,21人的舌頭能為他帶來滿足、生命或死亡。那些傾向使用舌頭的人必須接受言語所帶來的後果。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "口舌能够定生死， 多嘴多言食恶果。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "生與死都在舌頭的權下； 愛把弄這權柄的，必自食其果。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "生死在舌頭的權下， 喜愛它的，必吃它所結的果子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Death and life are in the power of the tongue, And those who love it will eat its fruit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The tongue has the power of life and death, and those who love it will eat its fruit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The tongue can bring death or life; those who love to talk will reap the consequences."
      }
