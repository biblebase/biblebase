    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.28.12",
            "paragraphs": [
              "一個人的快樂（二十八12）",
              "這主題重復出現在第28節與二十九2。見十一10的註釋。",
              "“歡欣”（AV）：即有理由高興；所以和合本作得誌，RV、RSV作“凱旋”。躲藏（和合、AV）：直譯為“願意（即必須？）被搜查”。參阿摩司五13。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.28.12",
            "paragraphs": [
              "二八12義人會躲避邪惡昏庸的君主。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人得胜，遍地欢腾； 恶人当道，人人躲藏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人得勝的時候，有極大的榮耀； 惡人興起的時候，人就躲避。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人得志，有大榮耀； 惡人興起，人就躲藏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the righteous triumph, there is great glory, But when the wicked rise, men hide themselves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the righteous triumph, there is great elation; but when the wicked rise to power, people go into hiding."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the godly succeed, everyone is glad. When the wicked take charge, people go into hiding."
      }
