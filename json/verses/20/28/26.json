    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.28.26",
            "paragraphs": [
              "行在智慧中（二十八26）",
              "第一行擺在這裏，從第25b節得著了加強的力量。憑智慧（和合；AV、RV：“有智慧地”）是“在智慧中”（RSV）；與第一行的對比清楚顯示出，這樣的智慧乃是神所教導的（在箴言中總是如此）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚人心中自以为是， 凭智慧行事的平安稳妥。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "自恃聰明的，是愚昧人； 憑著智慧行事的，必蒙拯救。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心中自是的，便是愚昧人； 憑智慧行事的，必蒙拯救。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who trusts in his own heart is a fool, But he who walks wisely will be delivered."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who trust in themselves are fools, but those who walk in wisdom are kept safe."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who trust their own insight are foolish, but anyone who walks in wisdom is safe."
      }
    },
