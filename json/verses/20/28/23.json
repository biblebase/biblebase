    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.28.23",
            "paragraphs": [
              "受歡迎之直言無諱（二十八23）",
              "參二十七5、6，二十九5。後來的希伯來文（ah]@ray）令人困惑。（a）它看起來好像是“在我之後”（即：“就如我所指示的”）；但是卻突然迸出個人的語氣來。（b）它可能是個罕見的副詞形式（參，ma{t[ay）。（c）德萊弗以之為巴比倫文ahurru（“普通人”）的一個同源形容詞，即：“身為責備人的普通人必蒙……”92。這是獨特的創見，但普通人似乎不可能與此有關。其他的提議都涉及經文的修正。",
              "92ZAW,1934,p.147."
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "责备人的至终比谄媚者更受爱戴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "責備人的，終必得人喜悅， 勝過那用舌頭諂媚人的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "責備人的，後來蒙人喜悅， 多於那用舌頭諂媚人的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who rebukes a man will afterward find more favor Than he who flatters with the tongue."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever rebukes a person will in the end gain favor rather than one who has a flattering tongue."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In the end, people appreciate honest criticism far more than flattery."
      }
    },
