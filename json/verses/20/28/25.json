    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贪得无厌的人挑起纷争， 信靠耶和华的富足昌盛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "貪心的人，必引起紛爭； 倚靠耶和華的，必得豐裕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心中貪婪的，挑起爭端； 倚靠耶和華的，必得豐裕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "An arrogant man stirs up strife, But he who trusts in the Lord will prosper."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The greedy stir up conflict, but those who trust in the Lord will prosper."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Greed causes fighting; trusting the Lord leads to prosperity."
      }
    },
