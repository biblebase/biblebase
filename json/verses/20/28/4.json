    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.28.4",
            "paragraphs": [
              "神的律法是人的堡壘（二十八4）",
              "若是沒有啟示，一切很快就都變成相對的；若是抱著道德的相對論態度，就沒有什麽事是十分值得去抨擊的。所以舉個例來說，暴君之所以能被人接受，是因為他使諸事得以推行；而背道者之所以被人接受，是因為他的處境令人關註。整個結果充分地出現在羅馬書一18～32。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.28.4",
            "paragraphs": [
              "二八4人對待律法的態度反映他對待惡人的態度。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "背弃律法的称赞恶人， 遵守律法的抗拒恶人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "離棄律法的，稱讚惡人； 遵守律法的，卻與惡人相爭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "違棄律法的，誇獎惡人； 遵守律法的，卻與惡人相爭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Those who forsake the law praise the wicked, But those who keep the law strive with them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
