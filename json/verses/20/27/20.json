        "text": "陰間和滅亡不會滿足， 人的眼目也是一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "陰間和滅亡永不滿足； 人的眼目也是如此。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Sheol and Abaddon are never satisfied, Nor are the eyes of man ever satisfied."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Death and Destruction are never satisfied, and neither are human eyes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Just as Death and Destruction are never satisfied, so human desire is never satisfied."
      }
