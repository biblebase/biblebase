    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.27.18",
            "paragraphs": [
              "服事的賞賜（二十七18）",
              "Moffatt清楚表達了兩個動詞之間的關聯：“照料（tends）……的人……；侍候（attendsto）……的人”。參提摩太後書二6、15。不成功的仆人可能會假裝輕視尊榮；但“追求地位之人”與“寵臣”有時候在不熱心的人眼中看來，只不過是一心一意鉆營的人罷了。參二十二29。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看无花果树的吃无花果， 照顾主人的也必得尊荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "照料無花果樹的，必吃樹上的果子； 事奉主人的，必得尊榮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看守無花果樹的，必吃樹上的果子； 敬奉主人的，必得尊榮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who tends the fig tree will eat its fruit, And he who cares for his master will be honored."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The one who guards a fig tree will eat its fruit, and whoever protects their master will be honored."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As workers who tend a fig tree are allowed to eat the fruit, so workers who protect their employer’s interests will be rewarded."
      }
