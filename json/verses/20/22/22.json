    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可仗势剥削贫穷人， 法庭上不可欺凌弱者，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不可因為貧窮人窮乏，就搶奪他的東西， 也不可在城門口欺壓窮苦人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "貧窮人，你不可因他貧窮就搶奪他的物， 也不可在城門口欺壓困苦人；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not rob the poor because he is poor, Or crush the afflicted at the gate;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not exploit the poor because they are poor and do not crush the needy in court,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t rob the poor just because you can, or exploit the needy in court."
      }
    },
