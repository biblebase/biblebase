    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.22.24",
            "paragraphs": [
              "二二24\"暴怒的人\"。直譯作：熱的人；即暴躁的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可结交脾气暴躁者， 不要跟易怒之人来往，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "容易發怒的人，不要與他為友； 脾氣暴躁的人，不要與他來往，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "好生氣的人，不可與他結交； 暴怒的人，不可與他來往；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not associate with a man given to anger; Or go with a hot-tempered man,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not make friends with a hot-tempered person, do not associate with one easily angered,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t befriend angry people or associate with hot-tempered people,"
      }
