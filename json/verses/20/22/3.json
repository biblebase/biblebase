    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.22.3",
            "paragraphs": [
              "走路要當心（二十二3）",
              "聖經所賦予盲目之樂觀主義的正確名字，不是信心，而是愚昧。參十四15、16。這句話甚至以更簡潔的希伯來文重復出現在二十七12。“受刑罰”（AV；和合：受害），Moffatt的譯法較好：“為它付上代價”。見分題研究：“Ⅰ多面的智慧”第4點，34頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.22.3",
            "paragraphs": [
              "二二3精明人遠離邪惡，愚蒙人卻沖進邪惡裏，並因此而接受懲罰。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "明哲人遇祸躲避， 愚昧人前往受害。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "精明人看見災禍，就躲藏起來； 愚蒙人卻往前走，自取禍害。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "通達人見禍藏躲； 愚蒙人前往受害。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The prudent sees the evil and hides himself, But the naive go on, and are punished for it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The prudent see danger and take refuge, but the simple keep going and pay the penalty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A prudent person foresees danger and takes precautions. The simpleton goes blindly on and suffers the consequences."
      }
    },
