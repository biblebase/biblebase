    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.22.9",
            "paragraphs": [
              "捐得樂意的人（二十二9）",
              "有人提出這樣的異議：“我不能夠給得太慷慨”，申命記十五9、10與哥林多後書九7、8回答了這個問題。參十九17，二十八27。“慷慨的”（AV）直譯為慈善的、“好的”，參馬太福音二十15。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "慷慨的人必蒙福， 因他给穷人食物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "心存良善看人的，必蒙賜福； 因為他把自己的食物分給窮人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眼目慈善的，就必蒙福， 因他將食物分給窮人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who is generous will be blessed, For he gives some of his food to the poor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The generous will themselves be blessed, for they share their food with the poor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Blessed are those who are generous, because they feed the poor."
      }
