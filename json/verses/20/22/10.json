    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.22.10",
            "paragraphs": [
              "惹是生非的人（二十二10）",
              "之所以會發生不和與敵意，有時候並不是因著某個處境中的一些事，而是因為某個人抱著錯誤的態度，表現出惡意來。那也就是說：一個組織有時候所需要的並不是改革，而是要革除某個成員，見馬太福音十八17。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "赶走嘲讽者，纷争平息， 争吵和羞辱也会消除。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "趕走好譏笑人的，紛爭就除去； 爭執和恥辱也必止息。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "趕出褻慢人，爭端就消除； 紛爭和羞辱也必止息。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Drive out the scoffer, and contention will go out, Even strife and dishonor will cease."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Drive out the mocker, and out goes strife; quarrels and insults are ended."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Throw out the mocker, and fighting goes, too. Quarrels and insults will disappear."
      }
