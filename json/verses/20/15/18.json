    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.18",
            "paragraphs": [
              "暴風中心（十五18）",
              "這裏的論點是：爭端的形成，人的因素遠比事物的因素強得多；與第二行一起，可參阿戚巴德（R.T.Archibald）論八福中“使人和睦的人”之特色時所說的：“……那人隨身帶著一種氛圍，爭端在其中自然就消逝無蹤。”（爭瑞……紛爭：希伯來文不像AV一樣犯了重復的毛病，把兩個都譯作“爭端”，和合本與RSV的譯法是比較好的。）也見第1節，與二十九22。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.15.18",
            "paragraphs": [
              "一五18\"暴怒的人\"。直譯作：憎恨的人，即脾氣暴躁的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "脾气暴躁，惹起争端； 忍耐克制，平息纠纷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "脾氣暴烈的人常引起紛爭； 不輕易動怒的可平息爭執。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "暴怒的人挑啟爭端； 忍怒的人止息紛爭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A hot-tempered man stirs up strife, But the slow to anger calms a dispute."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A hot-tempered person stirs up conflict, but the one who is patient calms a quarrel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A hot-tempered person starts fights; a cool-tempered person stops them."
      }
    },
