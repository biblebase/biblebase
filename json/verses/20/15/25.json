    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.25",
            "paragraphs": [
              "窮人的保護者（十五25）",
              "驕傲人這個字也出現在十六19，暗示出這些乃是高壓橫暴之人，寡婦就成了他們最自然的犧牲品。家與地界（RSV“邊界”）是極有力的對比。拿伯的故事（王上二十一）是這則箴言最佳的闡釋，但它也適用於所有種類的剝削。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华拆毁傲慢人的房屋， 祂使寡妇的地界完整无损。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必拆毀驕傲人的家， 卻要立定寡婦的地界。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華必拆毀驕傲人的家， 卻要立定寡婦的地界。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord will tear down the house of the proud, But He will establish the boundary of the widow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord tears down the house of the proud, but he sets the widow’s boundary stones in place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord tears down the house of the proud, but he protects the property of widows."
      }
