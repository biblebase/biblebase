    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.24",
            "paragraphs": [
              "找著的人少（十五24）",
              "呂譯與RSV在此譯得最好：“明智人的路徑是上升，到真生命的，故此他能躲避通往底下的陰間之路”。關於這兩條路在箴言中較充分的論述，見第二章，尤其是第10～22節；也見四18、19。AV的“地獄”=陰間；見分題研究：“生命與死亡”及附屬部分，原書第53～57頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智者循生命之路上升， 以免坠入阴间。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "生命之路領明慧人向上， 因此他可以離開在下面的陰間。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧人從生命的道上升， 使他遠離在下的陰間。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The path of life leads upward for the wise That he may keep away from Sheol below."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The path of life leads upward for the prudent to keep them from going down to the realm of the dead."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The path of life leads upward for the wise; they leave the grave behind."
      }
    },
