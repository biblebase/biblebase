    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.27",
            "paragraphs": [
              "萬惡之根（十五27）",
              "“饋贈”（思高、AV；和合：賄賂；matta{no{t[：見十八16註釋），就像財利一樣，可能是無罪的，但卻具有極大的危險。擾亂一個人的價值尺度（傳七7），威脅他的家庭（關於擾害，見第6節註釋）與他自己的性命（與必得存活相反，見分題研究：“生命與死亡”及附屬部分，第53～57頁），而且推翻他的神的權柄（弗五5）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.15.27",
            "paragraphs": [
              "一五27\"貪戀財利\"。更可作：用暴力取得不義之財。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贪爱财富，自害己家； 厌恶贿赂，安然存活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "貪愛不義之財的禍害自己的家； 恨惡賄賂的必可以存活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "貪戀財利的，擾害己家； 恨惡賄賂的，必得存活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who profits illicitly troubles his own house, But he who hates bribes will live."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The greedy bring ruin to their households, but the one who hates bribes will live."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Greed brings grief to the whole family, but those who hate bribes will live."
      }
    },
