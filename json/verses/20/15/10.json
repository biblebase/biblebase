    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.10",
            "paragraphs": [
              "挽救性命的責備（十五10）",
              "大部分的現代譯本在這一節都采逐字翻譯，像RV一樣：“有難以忍受的管教……”，暗示出某種每況愈下的發展：“剛愎任性是自討訓誡；不肯受教是自找死路。”AV同樣有可能，而且或許是比較可取的（“管教是令人痛苦的……”）；它暗示第一句話的立場到第二句話時有轉趨強硬之勢，因為那些最需要批評的人卻是對於批評最感不耐的，也是最危險。參第5、12節，與分題研究：“生命與死亡”及附屬部分，原書第53～57頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "背离正道，必遭严惩； 厌恶责备，必致死亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "離棄正路的必受嚴厲的管教； 恨惡責備的必致死亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "捨棄正路的，必受嚴刑； 恨惡責備的，必致死亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Grievous punishment is for him who forsakes the way; He who hates reproof will die."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Stern discipline awaits anyone who leaves the path; the one who hates correction will die."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Whoever abandons the right path will be severely disciplined; whoever hates correction will die."
      }
