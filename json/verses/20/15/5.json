        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚人蔑视父亲的管教， 接受责备的才算明智。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚妄人藐視父親的管教； 看重責備的是精明的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚妄人藐視父親的管教； 領受責備的，得着見識。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A fool rejects his father’s discipline, But he who regards reproof is sensible."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A fool spurns a parent’s discipline, but whoever heeds correction shows prudence."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Only a fool despises a parent’s discipline; whoever learns from correction is wise."
      }
