    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.19",
            "paragraphs": [
              "懶惰人困擾最多（十五19）",
              "稍微出乎意料之外的，懶惰人竟與正直人或率直人成為對比──使我們想起（a）懶惰中有著不誠實的成分（試著要逃避事實以及一個人所分配到的工作量）；（b）直路終究還是最容易的。參四25、26，也見分題研究：“懶惰人”及附屬部分，原書第40～42頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "懒惰人的路布满荆棘， 正直人的道平坦宽阔。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "懶惰人的道路好像荊棘籬笆； 正直人的路徑是平坦的大道。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "懒惰人的道路，如同荆棘的篱笆； 正直人的路途，是平坦的大道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "懶惰人的道像荊棘的籬笆； 正直人的路是平坦的大道。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The way of a sluggard is like a hedge of thorns, but the path of the upright is a level highway."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The way of the lazy is as a hedge of thorns, But the path of the upright is a highway."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The way of the sluggard is blocked with thorns, but the path of the upright is a highway."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The way of the lazy man  is like a hedge of thorns, But the way of the upright is a highway."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A lazy person’s way is blocked with briers, but the path of the upright is an open highway."
      }
    },
