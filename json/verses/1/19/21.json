    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天使回答说：“好吧，我答应你，不毁灭那座小城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一位天使對他說：“這事我也答應你，你所說的這城，我必不傾覆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天使對他說：「這事我也應允你；我不傾覆你所說的這城。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to him, “Behold, I grant you this request also, not to overthrow the town of which you have spoken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to him, “Very well, I will grant this request too; I will not overthrow the town you speak of."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“All right,” the angel said, “I will grant your request. I will not destroy the little village."
      }
