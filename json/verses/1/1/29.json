    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.1.24-31",
            "paragraphs": [
              "第六日（一24～31）"
            ]
          },
          {
            "verse": "gen.1.29-30",
            "paragraphs": [
              "29～30.將青羊賜……作食物，給一切生物之說，不可太過強解，說一切生物原來都是素食的，也不意味所有植物、一切生物都同樣可吃。這只是概說，指出所有生命都直接、間接仰仗植物菜蔬；本節的要點是說明，所有生物都從神的聖手中得到餵養。亦見九3的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "gen.1.29",
            "paragraphs": [
              "一29人類在洪水之後才以動物當作食物（九3）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝对人说：“看啊，我把地上所有结种子的菜蔬和所有树上有籽的果子都赐给你们作食物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神說：“看哪！我把全地上結種子的各樣蔬菜，和一切果樹上有種子的果子，都賜給你們作食物。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神说：“看哪，我把全地上一切结种子的蔬草和一切果实中带种子的树，都赐给你们；这些都可作为你们的食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神說：「看哪，我將遍地上一切結種子的菜蔬和一切樹上所結有核的果子全賜給你們作食物。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And God said, “Behold, I have given you every plant yielding seed that is on the face of all the earth, and every tree with seed in its fruit. You shall have them for food."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then God said, “Behold, I have given you every plant yielding seed that is on the surface of all the earth, and every tree which has fruit yielding seed; it shall be food for you;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then God said, “I give you every seed-bearing plant on the face of the whole earth and every tree that has fruit with seed in it. They will be yours for food."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And God said, “See, I have given you every herb that yields seed which is on the face of all the earth, and every tree whose fruit yields seed; to you it shall be for food."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then God said, “Look! I have given you every seed-bearing plant throughout the earth and all the fruit trees for your food."
      }
    },
