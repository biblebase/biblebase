    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝说：“天空下面的水要聚在一处，使干地露出来。”果然如此。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神說：“天下的水要聚在一處，使旱地露出來！”事就這樣成了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神說：「天下的水要聚在一處，使旱地露出來。」事就這樣成了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then God said, “Let the waters below the heavens be gathered into one place, and let the dry land appear”; and it was so."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And God said, “Let the water under the sky be gathered to one place, and let dry ground appear.” And it was so."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then God said, “Let the waters beneath the sky flow together into one place, so dry ground may appear.” And that is what happened."
      }
    },
