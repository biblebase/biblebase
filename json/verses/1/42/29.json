      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們回到迦南地他們的父親雅各那裡，把他們遭遇的一切事都告訴了他，說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們來到迦南地、他們的父親雅各那裏，將所遭遇的事都告訴他，說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they came to their father Jacob in the land of Canaan, they told him all that had happened to them, saying,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they came to their father Jacob in the land of Canaan, they told him all that had happened to them. They said,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the brothers came to their father, Jacob, in the land of Canaan, they told him everything that had happened to them."
      }
    },
