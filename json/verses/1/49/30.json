      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那洞穴在迦南幔利附近的麦比拉田间，是亚伯拉罕向赫人以弗仑买来作坟地的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這個洞在迦南地，在幔利前面的麥比拉的田間。那地方原是亞伯拉罕向赫人以弗崙買下來作墳地的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "就是在迦南地幔利对面的麦比拉田间的洞穴里；麦比拉田是亚伯拉罕向赫提人以弗仑买来的，作为他拥有的坟地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是在迦南地幔利前、麥比拉田間的洞；那洞和田是亞伯拉罕向赫人以弗崙買來為業，作墳地的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "in the cave that is in the field at Machpelah, to the east of Mamre, in the land of Canaan, which Abraham bought with the field from Ephron the Hittite to possess as a burying place."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "in the cave that is in the field of Machpelah, which is before Mamre, in the land of Canaan, which Abraham bought along with the field from Ephron the Hittite for a burial site."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the cave in the field of Machpelah, near Mamre in Canaan, which Abraham bought along with the field as a burial place from Ephron the Hittite."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "in the cave that is in the field of Machpelah, which is before Mamre in the land of Canaan, which Abraham bought with the field of Ephron the Hittite as a possession for a burial place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is the cave in the field of Machpelah, near Mamre in Canaan, that Abraham bought from Ephron the Hittite as a permanent burial site."
      }
