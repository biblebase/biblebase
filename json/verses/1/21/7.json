    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.21.1-7",
            "paragraphs": [
              "以撒誕生（二十一1～7）",
              "十二章開始的懸案，至此終告結束，而其最後的插曲，更是一個高潮。本段理所當然的語氣，以及強調神先前的話……所說的……所說的（1、2），表明在祂掌握中事情會靜悄悄地、準確地發生。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "gen.21.7",
            "paragraphs": [
              "二一7撒拉的身體得到復原，不但能生孩子，而且能乳養他。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁曾料到我会为亚伯拉罕哺养孩子呢？我竟然在丈夫年老的时候给他生了儿子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又說：“誰能告訴亞伯拉罕，撒拉要乳養兒女呢？在他年老的時候，我竟給他生了一個兒子。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "她又说：“当年会有人对亚伯拉罕说‘撒拉将乳养孩子’吗？我竟然在他年老的时候，为他生了一个儿子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又說：「誰能預先對亞伯拉罕說『撒拉要乳養嬰孩』呢？因為在他年老的時候，我給他生了一個兒子。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And she said, “Who would have said to Abraham that Sarah would nurse children? Yet I have borne him a son in his old age.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And she said, “Who would have said to Abraham that Sarah would nurse children? Yet I have borne him a son in his old age.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And she added, “Who would have said to Abraham that Sarah would nurse children? Yet I have borne him a son in his old age.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "She also said, “Who would have said to Abraham that Sarah would nurse children? For I have borne him a son in his old age.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Who would have said to Abraham that Sarah would nurse a baby? Yet I have given Abraham a son in his old age!”"
      }
    },
