    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.21.1-7",
            "paragraphs": [
              "以撒誕生（二十一1～7）",
              "十二章開始的懸案，至此終告結束，而其最後的插曲，更是一個高潮。本段理所當然的語氣，以及強調神先前的話……所說的……所說的（1、2），表明在祂掌握中事情會靜悄悄地、準確地發生。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华按着自己说过的话眷顾撒拉，实现给她的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華照著以前所說的，眷顧撒拉；耶和華按著他的應許，給撒拉作成。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華按着先前的話眷顧撒拉，便照他所說的給撒拉成就。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord took note of Sarah as He had said, and the Lord did for Sarah as He had promised."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the Lord was gracious to Sarah as he had said, and the Lord did for Sarah what he had promised."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord kept his word and did for Sarah exactly what he had promised."
      }
    },
