    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.6.9-12",
            "paragraphs": [
              "一人與神同行（六9～12）"
            ]
          },
          {
            "verse": "gen.6.10-12",
            "paragraphs": [
              "10～12.這裏所說的話，大部分都在家譜以及舊世界的總結記錄中出現過；重復則加深了嚴重性。不過敗壞與強暴等字，讓人對當時的強權有新的認識（看出第4節所提英武有名之人醜陋的一面）。希伯來文的敗壞（或“毀滅”）也顯明，神所定意要“毀滅”的世界（13節），事實上已經自我毀滅了。",
              "144用於形容人的態度，ta{mi^m（完整的）有此含意；若狹意用於指祭祀方面，則意為“沒有瑕疵”（如，出十二5）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝看见世界败坏了，因为世人行为败坏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神觀看大地，看見世界已經敗壞了；全人類在地上所行的都是敗壞的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神觀看世界，見是敗壞了；凡有血氣的人在地上都敗壞了行為。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God looked on the earth, and behold, it was corrupt; for all flesh had corrupted their way upon the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God saw how corrupt the earth had become, for all the people on earth had corrupted their ways."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God observed all this corruption in the world, for everyone on earth was corrupt."
      }
    },
