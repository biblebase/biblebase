    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到时候，他们的财产和牛羊等所有牲畜不全归我们了吗？我们同意他们吧，这样他们就会在我们这里定居。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣他們的牲畜、財產和所有的牲口，不都要歸我們所有嗎？只要我們應允他們的條件，他們就與我們同住了。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这样，他们的牲畜、财产和各样动物，难道不就都成为我们的吗？只要我们答应他们，他们就与我们一起居住。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的羣畜、貨財，和一切的牲口豈不都歸我們嗎？只要依從他們，他們就與我們同住。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Will not their livestock, their property and all their beasts be ours? Only let us agree with them, and they will dwell with us.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will not their livestock and their property and all their animals be ours? Only let us consent to them, and they will live with us.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Won’t their livestock, their property and all their other animals become ours? So let us agree to their terms, and they will settle among us.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Will not their livestock, their property, and every animal of theirs be ours? Only let us consent to them, and they will dwell with us.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if we do this, all their livestock and possessions will eventually be ours. Come, let’s agree to their terms and let them settle here among us.”"
      }
    },
