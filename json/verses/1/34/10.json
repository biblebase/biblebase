    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们可以在我们这里安顿下来，这片土地就在你们面前，住在这里做买卖、置产业吧。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，你們可以與我們同住；這地就擺在你們面前，只管在其中居住，來往作買賣，購置產業吧。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这样，你们可以与我们一起居住；这地就在你们面前，住在这里做买卖，置产业吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們與我們同住吧！這地都在你們面前，只管在此居住，做買賣，置產業。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You shall dwell with us, and the land shall be open to you. Dwell and trade in it, and get property in it.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus you shall live with us, and the land shall be open before you; live and trade in it and acquire property in it.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You can settle among us; the land is open to you. Live in it, trade in it, and acquire property in it.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So you shall dwell with us, and the land shall be before you. Dwell and trade in it, and acquire possessions for yourselves in it.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And you may live among us; the land is open to you! Settle here and trade with us. And feel free to buy property in the area.”"
      }
