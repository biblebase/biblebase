    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "只要你们答应把女儿嫁给我，无论你们要多少聘金和礼物，我必如数奉上。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "任由你們向我要多重的聘金和多大的禮物，我必照你們所說的給你們，只要你們把這少女給我為妻就是了。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你们尽管向我多多索要聘金和礼物，我都会照着你们所说的给，只求你们把这少女给我为妻。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "任憑向我要多重的聘金和禮物，我必照你們所說的給你們；只要把女子給我為妻。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Ask me for as great a bride-price and gift as you will, and I will give whatever you say to me. Only give me the young woman to be my wife.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ask me ever so much bridal payment and gift, and I will give according as you say to me; but give me the girl in marriage.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Make the price for the bride and the gift I am to bring as great as you like, and I’ll pay whatever you ask me. Only give me the young woman as my wife.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Ask me ever so much dowry and gift, and I will give according to what you say to me; but give me the young woman as a wife.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No matter what dowry or gift you demand, I will gladly pay it—just give me the girl as my wife.”"
      }
    },
