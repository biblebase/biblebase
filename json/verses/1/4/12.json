      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you cultivate the ground, it will no longer yield its strength to you; you will be a vagrant and a wanderer on the earth.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you work the ground, it will no longer yield its crops for you. You will be a restless wanderer on the earth.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No longer will the ground yield good crops for you, no matter how hard you work! From now on you will be a homeless wanderer on the earth.”"
      }
