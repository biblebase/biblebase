    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.30.8",
            "paragraphs": [
              "8.大大相爭，直譯為“神的爭鬥”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拉结说：“我跟姐姐相争，我得胜了。”她就给这孩子取名叫拿弗他利。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "拉結說：“我與姊姊大大地相爭，我得勝了。”於是給孩子起名叫拿弗他利。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拉結說：「我與我姊姊大大相爭，並且得勝」，於是給他起名叫拿弗他利。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Rachel said, “With mighty wrestlings I have wrestled with my sister, and I have indeed prevailed.” And she named him Naphtali."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Rachel said, “I have had a great struggle with my sister, and I have won.” So she named him Naphtali."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rachel named him Naphtali, for she said, “I have struggled hard with my sister, and I’m winning!”"
      }
    },
