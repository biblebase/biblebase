      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利亞說：「我有福啊，眾女子都要稱我是有福的」，於是給他起名叫亞設。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Leah said, “Happy am I! For women will call me happy.” So she named him Asher."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Leah said, “How happy I am! The women will call me happy.” So she named him Asher."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And Leah named him Asher, for she said, “What joy is mine! Now the other women will celebrate with me.”"
      }
