    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.47.28-31",
            "paragraphs": [
              "雅各指定葬埋在此（四十七28～31）",
              "正如馬丁阿卡德（R.Martin-Achard）所說：“這位垂死的以色列人，似乎對自己將要進入的未知世界不甚關心，反倒關註神子民的未來316。”以下兩章這一點清晰可見；在本段中，雅各對延續的感受，間接流露出這種情懷；他必須回到他所屬之地，那地不是約瑟的埃及，也不是祖先的米所波大米，而是神應許“給亞伯拉罕和他後裔，直到永遠”之地。參二十三章與四十八章的開頭註釋。"
            ]
          },
          {
            "verse": "gen.47.28",
            "paragraphs": [
              "28.列祖的生平歲數，見十二14的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雅各在埃及住了十七年，他一共活了一百四十七岁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雅各在埃及地住了十七年；他一生的壽數是一百四十七歲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雅各住在埃及地十七年，雅各平生的年日是一百四十七歲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jacob lived in the land of Egypt seventeen years; so the length of Jacob’s life was one hundred and forty-seven years."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jacob lived in Egypt seventeen years, and the years of his life were a hundred and forty-seven."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jacob lived for seventeen years after his arrival in Egypt, so he lived 147 years in all."
      }
