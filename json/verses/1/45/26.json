    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对父亲说：“约瑟还活着，并且做了埃及的宰相。”雅各惊呆了，不敢相信他们的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "告訴他，說：“約瑟還在，並且作了埃及全地的首相。”雅各的心冷淡麻木，因為他不相信他們。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们告诉他说：“约瑟还活着，而且作了埃及全地的管辖者。”雅各的心惊呆了，因为他简直不能相信他们。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "告訴他說：「約瑟還在，並且作埃及全地的宰相。」雅各心裏冰涼，因為不信他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they told him, “Joseph is still alive, and he is ruler over all the land of Egypt.” And his heart became numb, for he did not believe them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They told him, saying, “Joseph is still alive, and indeed he is ruler over all the land of Egypt.” But he was stunned, for he did not believe them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They told him, “Joseph is still alive! In fact, he is ruler of all Egypt.” Jacob was stunned; he did not believe them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they told him, saying, “Joseph is still alive, and he is governor over all the land of Egypt.” And Jacob’s heart stood still, because he did not believe them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Joseph is still alive!” they told him. “And he is governor of all the land of Egypt!” Jacob was stunned at the news—he couldn’t believe it."
      }
