    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.48.1-22",
            "paragraphs": [
              "雅各祝福以法蓮與瑪拿西（四十八1～22）",
              "希伯來書十一21從雅各一生漫長的經歷中，挑出這一段作為他信心的巔峰造極行動。那一章所稱道的，是這舉動所顯示抓住應許的品質，在臨死之前仍然不渝：“從遠處望見，且歡喜迎接”。當前的景況與他年輕時耍欺騙手段的情形一模一樣，略具諷刺意味。長子的福份再一度給了次子，不過這次並不是出於缺乏信心的計謀，後來也沒有產生苦毒的態度（參箴十22）。這是安靜回應與信心的實例和教訓。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对父亲说：“父亲，你弄错了，这才是长子，你应该把右手按在他的头上。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約瑟對他父親說：“爸爸啊，不是這樣，這個才是長子，請你把右手按在他的頭上。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約瑟對他父親說：「我父，不是這樣。這本是長子，求你把右手按在他的頭上。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joseph said to his father, “Not so, my father, for this one is the firstborn. Place your right hand on his head.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Joseph said to him, “No, my father, this one is the firstborn; put your right hand on his head.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“No, my father,” he said. “This one is the firstborn. Put your right hand on his head.”"
      }
