    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.43.1-34",
            "paragraphs": [
              "第二次訪問埃及（四十三1～34）"
            ]
          },
          {
            "verse": "gen.43.15-34",
            "paragraphs": [
              "15～34.約瑟的十一位客人。弟兄們的害怕（18節）透露出他們的無知，以為埃及人像氣量狹小的部落，要奪取他們的驢。這情采地刻劃出這住帳棚的人與周遭新奇環境之間的對比。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他洗过脸出来，控制着自己的感情，吩咐人摆饭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他洗了面，然後出來，抑制著自己的感情，吩咐人說：“開飯吧。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他洗了臉出來，勉強隱忍，吩咐人擺飯。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he washed his face and came out; and he controlled himself and said, “Serve the meal.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After he had washed his face, he came out and, controlling himself, said, “Serve the food.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After washing his face, he came back out, keeping himself under control. Then he ordered, “Bring out the food!”"
      }
    },
