    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.43.1-34",
            "paragraphs": [
              "第二次訪問埃及（四十三1～34）"
            ]
          },
          {
            "verse": "gen.43.15-34",
            "paragraphs": [
              "15～34.約瑟的十一位客人。弟兄們的害怕（18節）透露出他們的無知，以為埃及人像氣量狹小的部落，要奪取他們的驢。這情采地刻劃出這住帳棚的人與周遭新奇環境之間的對比。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "管家照约瑟的吩咐，带他们到约瑟家里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "管家就照著約瑟所吩咐的去作，把這些人帶到約瑟的家裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "家宰就遵着約瑟的命去行，領他們進約瑟的屋裏。"
      },
      "nasb": {
