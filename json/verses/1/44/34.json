    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这孩子不和我一同回去，我如何见我父亲呢？我不愿看到灾难临到我父亲。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為如果孩子沒有與我同去，我怎能上去見我的父親呢？恐怕我會看見災禍臨到我父親身上。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若童子不和我同去，我怎能上去見我父親呢？恐怕我看見災禍臨到我父親身上。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For how shall I go up to my father if the lad is not with me—for fear that I see the evil that would overtake my father?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How can I go back to my father if the boy is not with me? No! Do not let me see the misery that would come on my father.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For how can I return to my father if the boy is not with me? I couldn’t bear to see the anguish this would cause my father!”"
      }
    },
