      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大和他的兄弟來到約瑟的家裡，那時約瑟還在那裡；他們就在他面前俯伏在地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大和他弟兄們來到約瑟的屋中，約瑟還在那裏，他們就在他面前俯伏於地。"
      },
      "nasb": {
        "version_id": "NASB",
