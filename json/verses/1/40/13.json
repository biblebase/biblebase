    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.40.1-23",
            "paragraphs": [
              "酒政與膳長之夢（四十1～23）"
            ]
          },
          {
            "verse": "gen.40.13",
            "paragraphs": [
              "13.一個被貶絀的人得以頭，這生動的比喻，請參列王紀下二十五27；它與另一個希伯來文動詞同用，出現於詩篇三3，二十七6上。但見19節的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "三天之内，法老必定释放你，恢复你侍酒总管的职位。你仍要和从前一样伺候法老饮酒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "三天之內，法老必使你抬起頭來，恢復你原來的職位；你仍要把杯遞在法老的手裡，好像先前作他的酒政時一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "三天之內，法老必提你出監，叫你官復原職，你仍要遞杯在法老的手中，和先前作他的酒政一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "within three more days Pharaoh will lift up your head and restore you to your office; and you will put Pharaoh’s cup into his hand according to your former custom when you were his cupbearer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Within three days Pharaoh will lift up your head and restore you to your position, and you will put Pharaoh’s cup in his hand, just as you used to do when you were his cupbearer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Within three days Pharaoh will lift you up and restore you to your position as his chief cup-bearer."
      }
    },
