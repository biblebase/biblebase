    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.40.1-23",
            "paragraphs": [
              "酒政與膳長之夢（四十1～23）"
            ]
          },
          {
            "verse": "gen.40.6-8",
            "paragraphs": [
              "6～8.除了效率與正直外，約瑟對人迅速的關懷，披露出他性格的另一方面；他立刻提及神，聽來是出乎真誠：這是他思想的習慣（參三十九9，四十一16、51、52，四十五8，等）。至於這兩位作夢者，他們同樣確信夢是有意義的：在埃及一般人認為，它們具有預測性；有關如何解釋它們的著作愈來愈多。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第二天早上，约瑟过来见他们神情沮丧，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了早晨，約瑟進去到他們那裡，見他們神色不安，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了早晨，約瑟進到他們那裏，見他們有愁悶的樣子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Joseph came to them in the morning and observed them, behold, they were dejected."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Joseph came to them the next morning, he saw that they were dejected."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Joseph saw them the next morning, he noticed that they both looked upset."
      }
    },
