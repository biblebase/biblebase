    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，雅各从别示巴启程。以色列的儿子们带着父亲和妻儿，乘坐法老送来的车，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雅各就從別是巴起程。以色列的眾子，用法老為雅各送來的車輛，接載他們的父親雅各，以及他們的孩子和妻子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雅各就從別是巴起行。以色列的兒子們使他們的父親雅各和他們的妻子、兒女都坐在法老為雅各送來的車上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jacob arose from Beersheba; and the sons of Israel carried their father Jacob and their little ones and their wives in the wagons which Pharaoh had sent to carry him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jacob left Beersheba, and Israel’s sons took their father Jacob and their children and their wives in the carts that Pharaoh had sent to transport him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Jacob left Beersheba, and his sons took him to Egypt. They carried him and their little ones and their wives in the wagons Pharaoh had provided for them."
      }
    },
