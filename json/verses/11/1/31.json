        "content": [
          {
            "verse": "1ki.1.28-40",
            "paragraphs": [
              "Ｄ大衛印證所羅門為繼位人（一28～40）",
              "大衛依從拿單及拔示巴所求，指定所羅門為與他共同執政者（參43～44、47～48節），在他病時全權代理（46、53節）。這種作法當時在米索不達米亞及埃及全地已極為通行。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拔示芭就向王俯伏下拜说：“我主我王万岁！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是拔示巴面伏在地，向王叩拜，說：“願我主大衛王萬歲！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，拔示巴臉伏於地，向王下拜，說：「願我主大衛王萬歲！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Bathsheba bowed with her face to the ground, and prostrated herself before the king and said, “May my lord King David live forever.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Bathsheba bowed down with her face to the ground, prostrating herself before the king, and said, “May my lord King David live forever!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Bathsheba bowed down with her face to the ground before the king and exclaimed, “May my lord King David live forever!”"
      }
    },
