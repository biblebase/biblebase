    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.1.28-40",
            "paragraphs": [
              "Ｄ大衛印證所羅門為繼位人（一28～40）",
              "大衛依從拿單及拔示巴所求，指定所羅門為與他共同執政者（參43～44、47～48節），在他病時全權代理（46、53節）。這種作法當時在米索不達米亞及埃及全地已極為通行。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，大卫王召见拔示芭。拔示芭立刻晋见，侍立在王面前。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛回答說：“召拔示巴到我這裡來。”於是拔示巴來到王面前，站在那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛王吩咐說：「叫拔示巴來。」拔示巴就進來，站在王面前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then King David said, “Call Bathsheba to me.” And she came into the king’s presence and stood before the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then King David said, “Call in Bathsheba.” So she came into the king’s presence and stood before him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King David responded, “Call Bathsheba!” So she came back in and stood before the king."
      }
    },
