    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.13.11-32",
            "paragraphs": [
              "(2)伯特利的老先知（十三11～32）",
              "這個故事代表北國對神所宣告的審判持反抗的態度。此人從未宣稱自己從神直接得到命令（他說是藉著天使或帶信者，18節），但他所說的是謊話（18節），與真正的神的話相矛盾（9節）。但同時神卻也使用他來譴責神人（神人亦為一先知，18節）的不順服。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝的仆人说：“我不能跟你去，也不能跟你在这里吃饭喝水，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "神人說：“我不能與你回去，或是與你同行，也不能與你在這地方吃飯喝水，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神人說：「我不可同你回去進你的家，也不可在這裏同你吃飯喝水；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said, “I cannot return with you, nor go with you, nor will I eat bread or drink water with you in this place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The man of God said, “I cannot turn back and go with you, nor can I eat bread or drink water with you in this place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“No, I cannot,” he replied. “I am not allowed to eat or drink anything here in this place."
      }
