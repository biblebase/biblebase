    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.13.11-32",
            "paragraphs": [
              "(2)伯特利的老先知（十三11～32）",
              "這個故事代表北國對神所宣告的審判持反抗的態度。此人從未宣稱自己從神直接得到命令（他說是藉著天使或帶信者，18節），但他所說的是謊話（18節），與真正的神的話相矛盾（9節）。但同時神卻也使用他來譴責神人（神人亦為一先知，18節）的不順服。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "老先知把尸体驮在驴背上，运回城中为他哀悼，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "老先知把神人的屍體抱起來，放在驢子上，帶回自己的城裡，要為他舉哀，要埋葬他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "老先知就把神人的屍身馱在驢上，帶回自己的城裏，要哀哭他，葬埋他；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the prophet took up the body of the man of God and laid it on the donkey and brought it back to the city to mourn and to bury him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the prophet took up the body of the man of God and laid it on the donkey and brought it back, and he came to the city of the old prophet to mourn and to bury him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the prophet picked up the body of the man of God, laid it on the donkey, and brought it back to his own city to mourn for him and bury him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the prophet took up the corpse of the man of God, laid it on the donkey, and brought it back. So the old prophet came to the city to mourn, and to bury him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the prophet laid the body of the man of God on the donkey and took it back to the town to mourn over him and bury him."
      }
    },
