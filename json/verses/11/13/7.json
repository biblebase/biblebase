    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.13.1-10",
            "paragraphs": [
              "(1)伯特利來的神人（十三1～10）"
            ]
          },
          {
            "verse": "1ki.13.7-10",
            "paragraphs": [
              "7～10.若神人與王妥協或交往密切（“吃飯”，7、18節），便等於收回審判成命一樣。王的動機很可能是“為圖保險起見，與先知交往密切”（Robinson，p.161；參：Noth，p.298），同時冀望先知認可他新立為王的身分。有關回程道路的禁令可能是為了避免與受咒詛的地方及百姓有進一步接觸的機會。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王对上帝的仆人说：“请你跟我回宫，吃点东西吧！我要酬谢你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王對神人說：“請與我一起回王宮去，吃點東西，加添心力，我還要賜給你一份禮物。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王對神人說：「請你同我回去吃飯，加添心力，我也必給你賞賜。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king said to the man of God, “Come home with me and refresh yourself, and I will give you a reward.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king said to the man of God, “Come home with me for a meal, and I will give you a gift.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the king said to the man of God, “Come to the palace with me and have something to eat, and I will give you a gift.”"
      }
    },
