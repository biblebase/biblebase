    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.15.25-32",
            "paragraphs": [
              "iv.以色列的拿答（十五25～32）",
              "歷史敘事重新回到與亞撒同時代的以色列君王身上。巴沙的政變預示到耶羅波安家族會滅亡。資料來源包括當地有關叛亂的記錄（27～28節）以及先知所記載的歷史（29～30節）。"
            ]
          },
          {
            "verse": "1ki.15.25-26",
            "paragraphs": [
              "25～26.這裏的王朝總結提及拿達作王共二年，其中包括他登基那一年的部分時間以及第二年的一整年，因此總計可能少於兩年（24、33節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拿答像他父亲一样作恶多端，行耶和华视为恶的事，使以色列人陷入罪中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "拿答行耶和華看為惡的事，跟隨他父親的路，犯他父親使以色列人陷在罪中的那罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拿答行耶和華眼中看為惡的事，行他父親所行的，犯他父親使以色列人陷在罪裏的那罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He did evil in the sight of the Lord, and walked in the way of his father and in his sin which he made Israel sin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He did evil in the eyes of the Lord, following the ways of his father and committing the same sin his father had caused Israel to commit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But he did what was evil in the Lord’s sight and followed the example of his father, continuing the sins that Jeroboam had led Israel to commit."
      }
    },
