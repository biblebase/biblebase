    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他步耶罗波安的后尘，做耶和华视为恶的事，使以色列人陷入罪中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他行耶和華看為惡的事，行耶羅波安的路，犯了耶羅波安使以色列人陷在罪中的那罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他行耶和華眼中看為惡的事，行耶羅波安所行的道，犯他使以色列人陷在罪裏的那罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He did evil in the sight of the Lord, and walked in the way of Jeroboam and in his sin which he made Israel sin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He did evil in the eyes of the Lord, following the ways of Jeroboam and committing the same sin Jeroboam had caused Israel to commit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But he did what was evil in the Lord’s sight and followed the example of Jeroboam, continuing the sins that Jeroboam had led Israel to commit."
      }
    },
