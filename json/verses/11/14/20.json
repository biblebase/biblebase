    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他做王二十二年，然后与祖先同眠。他儿子拿答继位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶羅波安作王的日子共二十二年，就與他的列祖同睡。他的兒子拿答接續他作王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶羅波安作王二十二年，就與他列祖同睡。他兒子拿答接續他作王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The time that Jeroboam reigned was twenty-two years; and he slept with his fathers, and Nadab his son reigned in his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He reigned for twenty-two years and then rested with his ancestors. And Nadab his son succeeded him as king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jeroboam reigned in Israel twenty-two years. When Jeroboam died, his son Nadab became the next king."
      }
    },
