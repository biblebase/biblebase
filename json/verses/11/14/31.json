    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "罗波安与祖先同眠后，葬在大卫城的祖坟里。他母亲是亚扪人拿玛。他儿子亚比央继位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "羅波安與他的列祖同睡，與他的列祖一同埋葬在大衛的城裡。他的母親名叫拿瑪，是亞捫人；他的兒子亞比央接續他作王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "羅波安與他列祖同睡，葬在大衛城他列祖的墳地裏。他母親名叫拿瑪，是亞捫人。他兒子亞比央接續他作王。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And Rehoboam slept with his fathers and was buried with his fathers in the city of David. His mother’s name was Naamah the Ammonite. And Abijam his son reigned in his place."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Rehoboam slept with his fathers and was buried with his fathers in the city of David; and his mother’s name was Naamah the Ammonitess. And Abijam his son became king in his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And Rehoboam rested with his ancestors and was buried with them in the City of David. His mother’s name was Naamah; she was an Ammonite. And Abijah his son succeeded him as king."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So Rehoboam rested with his fathers, and was buried with his fathers in the City of David. His mother’s name was Naamah, an Ammonitess. Then Abijam his son reigned in his place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Rehoboam died, he was buried among his ancestors in the City of David. His mother was Naamah, an Ammonite woman. Then his son Abijam became the next king."
      }
