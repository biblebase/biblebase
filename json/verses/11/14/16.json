    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.14.1-18",
            "paragraphs": [
              "(4)亞希雅的預言（十四1～18）",
              "北國以色列得到的評語是一大失敗，這對整個歷史有決定性的影響，因此本段經文繼續預言耶羅波安家的盡頭，其重點是要顯示因他背道叛教，他將失去神所應許給他的地位。我們不需要視此為預言性神話。經文的用語及評語顯示這是由整卷歷史相一致的角度寫成，其原本的細節可能取自先知的記錄，原來可能是保存在示羅（Jones）或其他地方的（19節；代上二十九2；代下十三22）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为耶罗波安犯罪作孽，使以色列人陷入罪中，耶和华必抛弃以色列。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為了耶羅波安所犯的罪，又為了他使以色列人所犯的罪，耶和華必丟棄以色列人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因耶羅波安所犯的罪，又使以色列人陷在罪裏，耶和華必將以色列人交給仇敵。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he will give Israel up because of the sins of Jeroboam, which he sinned and made Israel to sin.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will give up Israel on account of the sins of Jeroboam, which he committed and with which he made Israel to sin.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he will give Israel up because of the sins Jeroboam has committed and has caused Israel to commit.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And He will give Israel up because of the sins of Jeroboam, who sinned and who made Israel sin.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will abandon Israel because Jeroboam sinned and made Israel sin along with him.”"
      }
    },
