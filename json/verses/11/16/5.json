    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ki.16.1-5",
            "paragraphs": [
              "一六1～5由於巴沙選擇象耶羅波安一樣活在罪中，所以他會得到耶羅波安家那種不名譽的下場（比較11,12節；一四10～13）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴沙其他的事及其一切所作所为和成就都记在以色列的列王史上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴沙其餘的事蹟，他所行的，以及他的英勇事蹟，不是都記在以色列諸王的年代誌上嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴沙其餘的事，凡他所行的和他的勇力，都寫在以色列諸王記上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the rest of the acts of Baasha and what he did and his might, are they not written in the Book of the Chronicles of the Kings of Israel?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As for the other events of Baasha’s reign, what he did and his achievements, are they not written in the book of the annals of the kings of Israel?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The rest of the events in Baasha’s reign and the extent of his power are recorded in The Book of the History of the Kings of Israel."
      }
    },
