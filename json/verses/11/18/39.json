    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人看见这情景，都俯伏在地高呼：“耶和华是上帝！耶和华是上帝！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人看見了，就面伏於地，說：“耶和華是　神！耶和華是　神！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾民看見了，就俯伏在地，說：「耶和華是神！耶和華是神！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When all the people saw it, they fell on their faces; and they said, “The Lord, He is God; the Lord, He is God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When all the people saw this, they fell prostrate and cried, “The Lord—he is God! The Lord—he is God!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when all the people saw it, they fell face down on the ground and cried out, “The Lord—he is God! Yes, the Lord is God!”"
      }
