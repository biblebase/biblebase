    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利亚继续对众人说：“耶和华的先知现在只剩下我一人，巴力的先知却有四百五十人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以利亞對眾人說：“耶和華的先知只剩下我一個，巴力的先知卻有四百五十個。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利亞對眾民說：「作耶和華先知的只剩下我一個人；巴力的先知卻有四百五十個人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Elijah said to the people, “I alone am left a prophet of the Lord, but Baal’s prophets are 450 men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Elijah said to them, “I am the only one of the Lord’s prophets left, but Baal has four hundred and fifty prophets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Elijah said to them, “I am the only prophet of the Lord who is left, but Baal has 450 prophets."
      }
    },
