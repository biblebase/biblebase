    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你应允我，应允我，好让众人知道你耶和华是上帝，你要使他们回心转意。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊，求你應允我，應允我！使這民知道你耶和華是　神，又知道是你使這民的心再回轉過來的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你應允我，應允我！使這民知道你－耶和華是神，又知道是你叫這民的心回轉。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Answer me, O Lord, answer me, that this people may know that You, O Lord, are God, and that You have turned their heart back again.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Answer me, Lord, answer me, so these people will know that you, Lord, are God, and that you are turning their hearts back again.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, answer me! Answer me so these people will know that you, O Lord, are God and that you have brought them back to yourself.”"
      }
    },
