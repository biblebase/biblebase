    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以利亚回答说：“给以色列带来灾祸的不是我，而是你和你父亲家，因为你们抛弃耶和华的诫命，去随从巴力。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以利亞說：“給以色列惹麻煩的，不是我，而是你和你父的家；因為你們離棄了耶和華的誡命，又去隨從巴力。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以利亞說：「使以色列遭災的不是我，乃是你和你父家；因為你們離棄耶和華的誡命，去隨從巴力。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said, “I have not troubled Israel, but you and your father’s house have, because you have forsaken the commandments of the Lord and you have followed the Baals."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I have not made trouble for Israel,” Elijah replied. “But you and your father’s family have. You have abandoned the Lord’s commands and have followed the Baals."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I have made no trouble for Israel,” Elijah replied. “You and your family are the troublemakers, for you have refused to obey the commands of the Lord and have worshiped the images of Baal instead."
      }
    },
