    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.17.1-16",
            "paragraphs": [
              "a.神保守以利亞（十七1～16）",
              "請註意神如何插手為祂的使者提供食物及安慰，以幫助他繼續他的使命。神使用人（15節）、自然（鳥，6節）及使者或天使（十九4～8）來執行這些任務。在創造之時（創一29～30，九3）、在曠野飄流（出十六31～35）、在耶穌基督的時候，神也同樣地為了人而插手幹預。在此神如何供應的細節不詳，但顯然是神跡，一如餵飽五千人一樣（約六10～14、30～31）。"
            ]
          },
          {
            "verse": "1ki.17.1-6",
            "paragraphs": [
              "(1)第一個神跡（十七1～6）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "1ki.17.3",
            "paragraphs": [
              "一七3\"基立溪\"。位於基列的一條小河，在約但河以東。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你离开这里，往东去，到约旦河以东的基立溪旁躲起来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你離開這裡，向東方去，躲藏在約旦河東面的基立溪旁。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你離開這裏往東去，藏在約旦河東邊的基立溪旁。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Go away from here and turn eastward, and hide yourself by the brook Cherith, which is east of the Jordan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Leave here, turn eastward and hide in the Kerith Ravine, east of the Jordan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Go to the east and hide by Kerith Brook, near where it enters the Jordan River."
      }
    },
