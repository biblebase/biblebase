        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "仆人住在你所拣选的子民中，这些子民多得不可胜数。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "僕人住在你揀選的子民中；這些子民很多，多得不能數算。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "僕人住在你所揀選的民中，這民多得不可勝數。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your servant is in the midst of Your people which You have chosen, a great people who are too many to be numbered or counted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your servant is here among the people you have chosen, a great people, too numerous to count or number."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And here I am in the midst of your own chosen people, a nation so great and numerous they cannot be counted!"
      }
    },
