    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你若像你父亲大卫一样遵行我的道，遵守我的律例和诫命，我必使你长寿。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你遵行了我的道，謹守我的律例、誡命，正如你父親大衛所行的，我就必延長你的壽數。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你若效法你父親大衛，遵行我的道，謹守我的律例、誡命，我必使你長壽。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you walk in My ways, keeping My statutes and commandments, as your father David walked, then I will prolong your days.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And if you walk in obedience to me and keep my decrees and commands as David your father did, I will give you a long life.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And if you follow me and obey my decrees and my commands as your father, David, did, I will give you a long life.”"
      }
    },
