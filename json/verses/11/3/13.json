    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你没有求富贵和尊荣，但我会一并赐给你，使你有生之年在列王中无人能比。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "連你所沒有求的，我也賜給你，就是財富、尊榮，以至你一生的年日，在列王之中沒有一人能像你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你所沒有求的，我也賜給你，就是富足、尊榮，使你在世的日子，列王中沒有一個能比你的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I give you also what you have not asked, both riches and honor, so that no other king shall compare with you, all your days."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have also given you what you have not asked, both riches and honor, so that there will not be any among the kings like you all your days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moreover, I will give you what you have not asked for—both wealth and honor—so that in your lifetime you will have no equal among kings."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And I have also given you what you have not asked: both riches and honor, so that there shall not be anyone like you among the kings all your days."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I will also give you what you did not ask for—riches and fame! No other king in all the world will be compared to you for the rest of your life!"
      }
    },
