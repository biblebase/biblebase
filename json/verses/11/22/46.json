      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約沙法將他父親亞撒在世所剩下的孌童都從國中除去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The remnant of the sodomites who remained in the days of his father Asa, he expelled from the land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He rid the land of the rest of the male shrine prostitutes who remained there even after the reign of his father Asa."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He banished from the land the rest of the male and female shrine prostitutes, who still continued their practices from the days of his father, Asa."
      }
    },
