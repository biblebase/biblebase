    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.9.10-28",
            "paragraphs": [
              "i.總論（九10～28）"
            ]
          },
          {
            "verse": "1ki.9.24",
            "paragraphs": [
              "c.建造米羅宮（九24）",
              "這節經文繼續三章1～4節的記載。臺地或作米羅（希伯來文意為“填平”），可能是宮殿東翼臺地及城墻的延伸，或是將錫安及俄斐勒之間的窪地填平而成（參15節；撒下五9；王上十一27；王下十二20）。然而，其他地方ml'一字的出現可能是指城堡圍墻內的平臺或是臺地，可能有輔助防禦的作用228。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "1ki.9.24",
            "paragraphs": [
              "九24\"米羅\"。在耶路撒冷城墻上的一個城堡。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老的女儿从大卫城迁到为她建造的宫殿以后，所罗门动工兴建米罗堡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老的女兒從大衛城搬到所羅門為她建造的宮裡以後，所羅門才重建米羅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老的女兒從大衛城搬到所羅門為她建造的宮裏。那時，所羅門才建造米羅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As soon as Pharaoh’s daughter came up from the city of David to her house which Solomon had built for her, then he built the Millo."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After Pharaoh’s daughter had come up from the City of David to the palace Solomon had built for her, he constructed the terraces."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon moved his wife, Pharaoh’s daughter, from the City of David to the new palace he had built for her. Then he constructed the supporting terraces."
      }
    },
