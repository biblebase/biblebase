    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "oba.1.15",
            "paragraphs": [
              "15.前面已詳細解釋，耶和華的日子便是萬國受審判的日子（見8節及導論，22頁）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "fengsheng": {
        "content": [
          {
            "verse": "oba.1.15",
            "paragraphs": [
              "1:15",
              "他也必照樣向你行：俄巴底亞預言說神將根據以東人和其他各國對待別人的方式報應他們。這條原則同樣適用於新約信徒(參西3:25註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华惩罚万国的日子近了。 必按你的所作所为报应你， 你的恶行必落到自己头上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的日子臨近萬國了， 人必照你所行的向你施行， 你的報應必歸到你的頭上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華降罰的日子臨近萬國。 你怎樣行，他也必照樣向你行； 你的報應必歸到你頭上。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For the day of the Lord is near upon all the nations. As you have done, it shall be done to you; your deeds shall return on your own head."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“For the day of the Lord draws near on all the nations. As you have done, it will be done to you. Your dealings will return on your own head."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The day of the Lord is near for all nations. As you have done, it will be done to you; your deeds will return upon your own head."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“For the day of the Lord upon all the nations is near; As you have done, it shall be done to you; Your reprisal shall return upon your own head."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The day is near when I, the Lord, will judge all godless nations! As you have done to Israel, so it will be done to you. All your evil deeds will fall back on your own heads."
      }
    },
