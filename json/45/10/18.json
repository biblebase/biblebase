    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我要问，以色列人没有听过吗？当然听过。因为 “他们的声音传遍天下， 他们的话语传到地极。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是我要說，他們沒有聽見嗎？他們的確聽見了，如經上所記： “他們的聲音傳遍全地， 他們的言語傳到地極。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "那么我要说，他们难道没有听过吗？当然听过—— “那声音传遍了天下， 那话语传到了地极。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我說，人沒有聽見嗎？誠然聽見了。 他們的聲音傳遍天下； 他們的言語傳到地極。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But I ask, have they not heard? Indeed they have, for “Their voice has gone out to all the earth, and their words to the ends of the world.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I say, surely they have never heard, have they? Indeed they have; “Their voice has gone out into all the earth, And their words to the ends of the world.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I ask: Did they not hear? Of course they did: “Their voice has gone out into all the earth, their words to the ends of the world.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But I say, have they not heard? Yes indeed: “Their sound has gone out to all the earth, And their words to the ends of the world.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I ask, have the people of Israel actually heard the message? Yes, they have: “The message has gone throughout the earth, and the words to all the world.”"
      }
