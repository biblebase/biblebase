    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "既然你们知道主是公义的，也该知道所有按公义行事的人都是上帝的儿女。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們既然曉得　神是公義的，就應當知道所有行公義的都是從他生的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你们如果知道他是公义的，也就知道所有行义的人，都是由他所生的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若知道他是公義的，就知道凡行公義之人都是他所生的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If you know that he is righteous, you may be sure that everyone who practices righteousness has been born of him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you know that He is righteous, you know that everyone also who practices righteousness is born of Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you know that he is righteous, you know that everyone who does what is right has been born of him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "If you know that He is righteous, you know that everyone who practices righteousness is born of Him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Since we know that Christ is righteous, we also know that all who do what is right are God’s children."
      }
    },
