    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.15.1-12",
            "paragraphs": [
              "一五1～12猶大的南界從\"鹽海\"（死海）的南端向西伸展至\"埃及小河\"。北界從死海北端伸展至地中海（這也包括東面和西面的邊界）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到伯曷拉，经过伯·亚拉巴北面，直到吕便的儿子波罕所立的大石；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "上到伯．曷拉，經過伯．亞拉巴的北邊，再上到流本的兒子波罕的磐石那裡；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "上到伯‧曷拉，過伯‧亞拉巴的北邊，上到呂便之子波罕的磐石；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the boundary goes up to Beth-hoglah and passes along north of Beth-arabah. And the boundary goes up to the stone of Bohan the son of Reuben."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the border went up to Beth-hoglah, and continued on the north of Beth-arabah, and the border went up to the stone of Bohan the son of Reuben."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "went up to Beth Hoglah and continued north of Beth Arabah to the Stone of Bohan son of Reuben."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The border went up to Beth Hoglah and passed north of Beth Arabah; and the border went up to the stone of Bohan the son of Reuben."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "went up from there to Beth-hoglah, then proceeded north of Beth-arabah to the Stone of Bohan. (Bohan was Reuben’s son.)"
      }
