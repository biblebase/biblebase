    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.15.1-12",
            "paragraphs": [
              "一五1～12猶大的南界從\"鹽海\"（死海）的南端向西伸展至\"埃及小河\"。北界從死海北端伸展至地中海（這也包括東面和西面的邊界）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "东面以盐海南端到约旦河口为边界。 北界从约旦河口的海湾开始，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "東方的疆界是從鹽海直到約旦河口。北方的疆界是從約旦河口的海灣起，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "東界是從鹽海南邊到約旦河口。北界是從約旦河口的海汊起，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the east boundary is the Salt Sea, to the mouth of the Jordan. And the boundary on the north side runs from the bay of the sea at the mouth of the Jordan."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The east border was the Salt Sea, as far as the mouth of the Jordan. And the border of the north side was from the bay of the sea at the mouth of the Jordan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The eastern boundary is the Dead Sea as far as the mouth of the Jordan. The northern boundary started from the bay of the sea at the mouth of the Jordan,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The east border was the Salt Sea as far as the mouth of the Jordan. And the border on the northern quarter began at the bay of the sea at the mouth of the Jordan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The eastern boundary extended along the Dead Sea to the mouth of the Jordan River. The northern boundary began at the bay where the Jordan River empties into the Dead Sea,"
      }
