    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.19.24-31",
            "paragraphs": [
              "一九24～31\"亞設\"定居在地中海沿岸，從迦密山向北伸展至推羅，即腓尼基人居住的地方。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "再转到拉玛、坚固的泰尔城，然后转到何萨和亚革悉一带，直到地中海。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又轉到拉瑪，直到設防的城推羅；再轉到何薩，直通到海岸靠近亞革悉的地區；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "轉到拉瑪和堅固城泰爾；又轉到何薩，靠近亞革悉一帶地方，直通到海；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then the boundary turns to Ramah, reaching to the fortified city of Tyre. Then the boundary turns to Hosah, and it ends at the sea; Mahalab, Achzib,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The border turned to Ramah and to the fortified city of Tyre; then the border turned to Hosah, and it ended at the sea by the region of Achzib."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The boundary then turned back toward Ramah and went to the fortified city of Tyre, turned toward Hosah and came out at the Mediterranean Sea in the region of Akzib,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the border turned to Ramah and to the fortified city of Tyre; then the border turned to Hosah, and ended at the sea by the region of Achzib."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the boundary turned toward Ramah and the fortress of Tyre, where it turned toward Hosah and came to the Mediterranean Sea. The territory also included Mehebel, Aczib,"
      }
    },
