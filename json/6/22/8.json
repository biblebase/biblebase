    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说：“现在你们带着大量的财物、牲畜、金银、铜、铁和衣物返回家园，要把从敌人那里得来的财物分给你们的弟兄。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對他們說：“你們帶著許多財物，許多牲畜和金、銀、銅、鐵，以及許多衣服，回自己的家去吧；也要把你們從仇敵奪得的戰利品與你們的兄弟同分。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對他們說：「你們帶許多財物，許多牲畜和金、銀、銅、鐵，並許多衣服，回你們的帳棚去，要將你們從仇敵奪來的物，與你們眾弟兄同分。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "he said to them, “Go back to your tents with much wealth and with very much livestock, with silver, gold, bronze, and iron, and with much clothing. Divide the spoil of your enemies with your brothers.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and said to them, “Return to your tents with great riches and with very much livestock, with silver, gold, bronze, iron, and with very many clothes; divide the spoil of your enemies with your brothers.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "saying, “Return to your homes with your great wealth—with large herds of livestock, with silver, gold, bronze and iron, and a great quantity of clothing—and divide the plunder from your enemies with your fellow Israelites.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and spoke to them, saying, “Return with much riches to your tents, with very much livestock, with silver, with gold, with bronze, with iron, and with very much clothing. Divide the spoil of your enemies with your brethren.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "he said to them, “Go back to your homes with the great wealth you have taken from your enemies—the vast herds of livestock, the silver, gold, bronze, and iron, and the large supply of clothing. Share the plunder with your relatives.”"
      }
    },
