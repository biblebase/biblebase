    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jos.13.1",
            "paragraphs": [
              "13:1",
              "約書亞年紀老邁：從這章起進入了約書亞記的第二部分。這時絕大部分土地已被占領，凡有組織的抵抗都被擊潰。盡管有些土地還未得到(2~6節)，這地上卻已“太平，沒有爭戰了”(11:23)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jos.13.1",
            "paragraphs": [
              "一三1\"年紀老邁\"。約書亞此時約有九十至一百歲（比較二三1；二四29）。\"未得之地\"。第2至6節所述眾支派仍未征服的地區，從南到北詳述出來了。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚年事已高，耶和华对他说：“你年纪大了，但还有许多地方有待征服，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞老了，年紀不小；耶和華對他說：“你已經老了，年紀不小；還有很多的地有待征服。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞年紀老邁，耶和華對他說：「你年紀老邁了，還有許多未得之地，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now Joshua was old and advanced in years, and the Lord said to him, “You are old and advanced in years, and there remains yet very much land to possess."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Joshua was old and advanced in years when the Lord said to him, “You are old and advanced in years, and very much of the land remains to be possessed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Joshua had grown old, the Lord said to him, “You are now very old, and there are still very large areas of land to be taken over."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now Joshua was old, advanced in years. And the Lord said to him: “You are old, advanced in years, and there remains very much land yet to be possessed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Joshua was an old man, the Lord said to him, “You are growing old, and much land remains to be conquered."
      }
    },
