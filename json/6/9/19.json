    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.9.3-27",
            "paragraphs": [
              "九3～27以色列人可以跟遠方的城市和平相處，但他們卻必須把迦南地的城全然毀滅（申七1,2；二○11～15）。由於\"基遍\"距離艾城西南面只有六英裏半（10.5公裏），所以它的特使必須裝作從遠方來，才可以逃過正在入侵的以色列人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "首领便对全体会众说：“我们曾凭以色列的上帝耶和华向他们起誓，我们现在不能动他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾首領對全體會眾說：“我們曾經指著耶和華以色列的　神向他們起了誓，現在我們不能傷害他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾首領對全會眾說：「我們已經指着耶和華－以色列的神向他們起誓，現在我們不能害他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But all the leaders said to all the congregation, “We have sworn to them by the Lord, the God of Israel, and now we may not touch them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But all the leaders said to the whole congregation, “We have sworn to them by the Lord, the God of Israel, and now we cannot touch them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but all the leaders answered, “We have given them our oath by the Lord, the God of Israel, and we cannot touch them now."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then all the rulers said to all the congregation, “We have sworn to them by the Lord God of Israel; now therefore, we may not touch them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the leaders replied, “Since we have sworn an oath in the presence of the Lord, the God of Israel, we cannot touch them."
      }
