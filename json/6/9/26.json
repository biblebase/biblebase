    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.9.3-27",
            "paragraphs": [
              "九3～27以色列人可以跟遠方的城市和平相處，但他們卻必須把迦南地的城全然毀滅（申七1,2；二○11～15）。由於\"基遍\"距離艾城西南面只有六英裏半（10.5公裏），所以它的特使必須裝作從遠方來，才可以逃過正在入侵的以色列人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚不让以色列人加害他们，因此他们没有被杀死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞就這樣處置他們，救了他們脫離以色列人的手，以色列人就沒有殺他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是約書亞這樣待他們，救他們脫離以色列人的手，以色列人就沒有殺他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So he did this to them and delivered them out of the hand of the people of Israel, and they did not kill them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus he did to them, and delivered them from the hands of the sons of Israel, and they did not kill them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Joshua saved them from the Israelites, and they did not kill them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So he did to them, and delivered them out of the hand of the children of Israel, so that they did not kill them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Joshua did not allow the people of Israel to kill them."
      }
    },
