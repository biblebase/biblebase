    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然而你还有善行，因为你铲除境内的亚舍拉神像，立志寻求上帝。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可是你還有善行，因為你從國中除掉亞舍拉，並且決心尋求　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "然而你還有善行，因你從國中除掉木偶，立定心意尋求神。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Nevertheless, some good is found in you, for you destroyed the Asheroth out of the land, and have set your heart to seek God.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But there is some good in you, for you have removed the Asheroth from the land and you have set your heart to seek God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There is, however, some good in you, for you have rid the land of the Asherah poles and have set your heart on seeking God.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Nevertheless good things are found in you, in that you have removed the wooden images from the land, and have prepared your heart to seek God.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even so, there is some good in you, for you have removed the Asherah poles throughout the land, and you have committed yourself to seeking God.”"
      }
    },
