    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "利玛利的儿子比加一天之内杀了犹大十二万勇士，因为他们背弃了他们祖先的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "利瑪利的兒子比加在猶大一天之內殺了十二萬人，全是勇士，因為他們離棄了耶和華他們列祖的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利瑪利的兒子比加一日殺了猶大人十二萬，都是勇士，因為他們離棄了耶和華－他們列祖的神。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For Pekah the son of Remaliah killed 120,000 from Judah in one day, all of them men of valor, because they had forsaken the Lord, the God of their fathers."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For Pekah the son of Remaliah slew in Judah 120,000 in one day, all valiant men, because they had forsaken the Lord God of their fathers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In one day Pekah son of Remaliah killed a hundred and twenty thousand soldiers in Judah—because Judah had forsaken the Lord, the God of their ancestors."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For Pekah the son of Remaliah killed one hundred and twenty thousand in Judah in one day, all valiant men, because they had forsaken the Lord God of their fathers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In a single day Pekah son of Remaliah, Israel’s king, killed 120,000 of Judah’s troops, all of them experienced warriors, because they had abandoned the Lord, the God of their ancestors."
      }
