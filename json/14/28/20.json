    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.28.16-21",
            "paragraphs": [
              "二八16～21有關這時期的更多事件，參看列王紀下十五章27至31節的腳註。\"欺淩他\"（20節）。要求他進貢。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚述王提革拉·毗列色来到犹大后，不但不救他，反而压迫他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞述王提革拉．毘尼色來到他那裡，不但沒有援助他，反倒欺壓他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞述王提革拉‧毗列色上來，卻沒有幫助他，反倒欺凌他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So Tiglath-pileser king of Assyria came against him and afflicted him instead of strengthening him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Tilgath-pilneser king of Assyria came against him and afflicted him instead of strengthening him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Tiglath-Pileser king of Assyria came to him, but he gave him trouble instead of help."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Also Tiglath-Pileser king of Assyria came to him and distressed him, and did not assist him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So when King Tiglath-pileser of Assyria arrived, he attacked Ahaz instead of helping him."
      }
    },
