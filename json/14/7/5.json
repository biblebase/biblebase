    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.7.5",
            "paragraphs": [
              "七5根據列王紀上八章63節，這些是平安祭，祭肉供應百姓在兩個星期的慶祝筵宴上的食用（9,10節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门王献上两万二千头牛和十二万只羊，王和全体民众为上帝的殿举行奉献典礼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門王獻上了二萬二千頭牛、十二萬隻羊為祭。這樣，王和全體人民為　神的殿舉行了奉獻禮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門王用牛二萬二千，羊十二萬獻祭。這樣，王和眾民為神的殿行奉獻之禮。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "King Solomon offered as a sacrifice 22,000 oxen and 120,000 sheep. So the king and all the people dedicated the house of God."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "King Solomon offered a sacrifice of 22,000 oxen and 120,000 sheep. Thus the king and all the people dedicated the house of God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And King Solomon offered a sacrifice of twenty-two thousand head of cattle and a hundred and twenty thousand sheep and goats. So the king and all the people dedicated the temple of God."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "King Solomon offered a sacrifice of twenty-two thousand bulls and one hundred and twenty thousand sheep. So the king and all the people dedicated the house of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King Solomon offered a sacrifice of 22,000 cattle and 120,000 sheep and goats. And so the king and all the people dedicated the Temple of God."
      }
    },
