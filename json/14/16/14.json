    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "葬在大卫城他为自己凿的坟墓里。民众把他放在堆满精心调制的各种香料的床上，又点了一堆大火向他致哀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人把他埋葬在大衛城裡他為自己所鑿的墳墓中。他們把他放在堆滿按著作香的方法調和的各樣香料的床上，又為他燒了很多香料。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "葬在大衛城自己所鑿的墳墓裏，放在牀上，其牀堆滿各樣馨香的香料，就是按做香的作法調和的香料，又為他燒了許多的物件。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They buried him in the tomb that he had cut for himself in the city of David. They laid him on a bier that had been filled with various kinds of spices prepared by the perfumer’s art, and they made a very great fire in his honor."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They buried him in his own tomb which he had cut out for himself in the city of David, and they laid him in the resting place which he had filled with spices of various kinds blended by the perfumers’ art; and they made a very great fire for him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They buried him in the tomb that he had cut out for himself in the City of David. They laid him on a bier covered with spices and various blended perfumes, and they made a huge fire in his honor."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They buried him in his own tomb, which he had made for himself in the City of David; and they laid him in the bed which was filled with spices and various ingredients prepared in a mixture of ointments. They made a very great burning for him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He was buried in the tomb he had carved out for himself in the City of David. He was laid on a bed perfumed with sweet spices and fragrant ointments, and the people built a huge funeral fire in his honor."
      }
