    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.29.31",
            "paragraphs": [
              "二九31\"甘心樂意\"。自由地。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希西迦说：“你们既然已经把自己献给了耶和华，现在就把祭物和感恩祭带到耶和华的殿里来吧。”于是，会众就把祭物和感恩祭送来，有些人甘心乐意地送来燔祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希西家說：“你們現在歸耶和華為聖了。請前來，把祭物和感恩祭帶到耶和華的殿裡來。”於是會眾把祭物和感恩祭帶來；凡是甘心樂意的，都把燔祭帶來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希西家說：「你們既然歸耶和華為聖，就要前來把祭物和感謝祭奉到耶和華殿裏。」會眾就把祭物和感謝祭奉來，凡甘心樂意的也將燔祭奉來。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Hezekiah said, “You have now consecrated yourselves to the Lord. Come near; bring sacrifices and thank offerings to the house of the Lord.” And the assembly brought sacrifices and thank offerings, and all who were of a willing heart brought burnt offerings."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Hezekiah said, “Now that you have consecrated yourselves to the Lord, come near and bring sacrifices and thank offerings to the house of the Lord.” And the assembly brought sacrifices and thank offerings, and all those who were willing brought burnt offerings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Hezekiah said, “You have now dedicated yourselves to the Lord. Come and bring sacrifices and thank offerings to the temple of the Lord.” So the assembly brought sacrifices and thank offerings, and all whose hearts were willing brought burnt offerings."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Hezekiah answered and said, “Now that you have consecrated yourselves to the Lord, come near, and bring sacrifices and thank offerings into the house of the Lord.” So the assembly brought in sacrifices and thank offerings, and as many as were of a willing heart brought burnt offerings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Hezekiah declared, “Now that you have consecrated yourselves to the Lord, bring your sacrifices and thanksgiving offerings to the Temple of the Lord.” So the people brought their sacrifices and thanksgiving offerings, and all whose hearts were willing brought burnt offerings, too."
      }
    },
