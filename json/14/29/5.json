    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.29.5",
            "paragraphs": [
              "二九5\"現在你們要潔凈自己\"。關於所包含的行動，參看歷代誌上十五章12節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对他们说：“利未人啊，请听我说！现在你们要洁净自己，也要洁净你们祖先的上帝耶和华的殿，除去圣所中的污秽之物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對他們說：“利未人哪，你們要聽我的話；現在你們要自潔，又要潔淨耶和華你們列祖的　神的殿，把污穢之物從聖所中除去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對他們說：「利未人哪，當聽我說：現在你們要潔淨自己，又潔淨耶和華－你們列祖神的殿，從聖所中除去污穢之物。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "and said to them, “Hear me, Levites! Now consecrate yourselves, and consecrate the house of the Lord, the God of your fathers, and carry out the filth from the Holy Place."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he said to them, “Listen to me, O Levites. Consecrate yourselves now, and consecrate the house of the Lord, the God of your fathers, and carry the uncleanness out from the holy place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and said: “Listen to me, Levites! Consecrate yourselves now and consecrate the temple of the Lord, the God of your ancestors. Remove all defilement from the sanctuary."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and said to them: “Hear me, Levites! Now sanctify yourselves, sanctify the house of the Lord God of your fathers, and carry out the rubbish from the holy place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He said to them, “Listen to me, you Levites! Purify yourselves, and purify the Temple of the Lord, the God of your ancestors. Remove all the defiled things from the sanctuary."
      }
    },
