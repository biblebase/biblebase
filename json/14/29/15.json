    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.29.15",
            "paragraphs": [
              "二九15\"照著……耶和華的命令\"，正如從前在律法書上的記載（申一二2～4）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们召集其他利未人一起洁净自己，并遵照王的吩咐和耶和华的命令，进去洁净耶和华的殿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "集合他們的親族，潔淨自己，照著王的吩咐，根據耶和華的命令，進去潔淨耶和華的殿。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "起來聚集他們的弟兄，潔淨自己，照着王的吩咐、耶和華的命令，進去潔淨耶和華的殿。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They gathered their brothers and consecrated themselves and went in as the king had commanded, by the words of the Lord, to cleanse the house of the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They assembled their brothers, consecrated themselves, and went in to cleanse the house of the Lord, according to the commandment of the king by the words of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they had assembled their fellow Levites and consecrated themselves, they went in to purify the temple of the Lord, as the king had ordered, following the word of the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they gathered their brethren, sanctified themselves, and went according to the commandment of the king, at the words of the Lord, to cleanse the house of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These men called together their fellow Levites, and they all purified themselves. Then they began to cleanse the Temple of the Lord, just as the king had commanded. They were careful to follow all the Lord’s instructions in their work."
      }
    },
