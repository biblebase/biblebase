    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大人转身，发现自己前后受敌，就呼求耶和华。祭司吹响号角，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大人轉身，看見自己前後受敵，就呼求耶和華，祭司也都吹號。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大人回頭觀看，見前後都有敵兵，就呼求耶和華，祭司也吹號。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And when Judah looked, behold, the battle was in front of and behind them. And they cried to the Lord, and the priests blew the trumpets."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Judah turned around, behold, they were attacked both front and rear; so they cried to the Lord, and the priests blew the trumpets."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Judah turned and saw that they were being attacked at both front and rear. Then they cried out to the Lord. The priests blew their trumpets"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And when Judah looked around, to their surprise the battle line was at both front and rear; and they cried out to the Lord, and the priests sounded the trumpets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Judah realized that they were being attacked from the front and the rear, they cried out to the Lord for help. Then the priests blew the trumpets,"
      }
    },
