    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚撒王废除了他祖母玛迦的太后之位，因为她造了可憎的亚舍拉神像。亚撒将她的神像砍倒、打碎，烧毁在汲沦溪旁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞撒王也廢掉他的祖母瑪迦太后的位，因為她曾給亞舍拉做了可憎的像；亞撒把她那可憎的像砍了下來，搗得粉碎，燒在汲淪溪旁。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞撒王貶了他祖母瑪迦太后的位，因她造了可憎的偶像亞舍拉。亞撒砍下她的偶像，搗得粉碎，燒在汲淪溪邊。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Even Maacah, his mother, King Asa removed from being queen mother because she had made a detestable image for Asherah. Asa cut down her image, crushed it, and burned it at the brook Kidron."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also removed Maacah, the mother of King Asa, from the position of queen mother, because she had made a horrid image as an Asherah, and Asa cut down her horrid image, crushed it and burned it at the brook Kidron."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "King Asa also deposed his grandmother Maakah from her position as queen mother, because she had made a repulsive image for the worship of Asherah. Asa cut it down, broke it up and burned it in the Kidron Valley."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Also he removed Maachah, the mother of Asa the king, from being queen mother, because she had made an obscene image of Asherah; and Asa cut down her obscene image, then crushed and burned it by the Brook Kidron."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King Asa even deposed his grandmother Maacah from her position as queen mother because she had made an obscene Asherah pole. He cut down her obscene pole, broke it up, and burned it in the Kidron Valley."
      }
    },
