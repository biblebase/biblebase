    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为会众中间有许多人没有洁净自己，利未人就为所有不洁之人宰逾越节的羊羔献给耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為會眾中有很多人還沒有自潔，所以利未人必須為所有不潔的人宰殺逾越節的羊羔，使他們在耶和華面前成為聖潔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "會中有許多人尚未自潔，所以利未人為一切不潔之人宰逾越節的羊羔，使他們在耶和華面前成為聖潔。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For there were many in the assembly who had not consecrated themselves. Therefore the Levites had to slaughter the Passover lamb for everyone who was not clean, to consecrate it to the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For there were many in the assembly who had not consecrated themselves; therefore, the Levites were over the slaughter of the Passover lambs for everyone who was unclean, in order to consecrate them to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Since many in the crowd had not consecrated themselves, the Levites had to kill the Passover lambs for all those who were not ceremonially clean and could not consecrate their lambs to the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For there  were many in the assembly who had not sanctified themselves; therefore the Levites had charge of the slaughter of the Passover lambs for everyone who  was not clean, to sanctify them to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Since many of the people had not purified themselves, the Levites had to slaughter their Passover lamb for them, to set them apart for the Lord."
      }
    },
