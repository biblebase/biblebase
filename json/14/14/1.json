    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚比雅与祖先同眠后，葬在大卫城中，他儿子亚撒继位。亚撒执政期间，国中太平十年。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞比雅和他的列祖同睡，埋葬在大衛城裡，他的兒子亞撒接續他作王。亞撒在世的日子，國中太平了十年。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞比雅與他列祖同睡，葬在大衛城裏。他兒子亞撒接續他作王。亞撒年間，國中太平十年。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Abijah slept with his fathers, and they buried him in the city of David. And Asa his son reigned in his place. In his days the land had rest for ten years."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Abijah slept with his fathers, and they buried him in the city of David, and his son Asa became king in his place. The land was undisturbed for ten years during his days."
      },
      "niv": {
        "version_id": "NIV",
