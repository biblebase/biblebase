    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无论是在上帝的殿中事奉，还是遵行律法和诫命，他都寻求他的上帝，尽心去做。因此，他行事顺利。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他經辦的一切事，無論是關於神殿的事奉，或是關於律法和誡命的遵行，他都尋求他的　神。因為他盡心去行，所以盡都順利。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡他所行的，無論是辦神殿的事，是遵律法守誡命，是尋求他的神，都是盡心去行，無不亨通。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And every work that he undertook in the service of the house of God and in accordance with the law and the commandments, seeking his God, he did with all his heart, and prospered."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every work which he began in the service of the house of God in law and in commandment, seeking his God, he did with all his heart and prospered."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In everything that he undertook in the service of God’s temple and in obedience to the law and the commands, he sought his God and worked wholeheartedly. And so he prospered."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And in every work that he began in the service of the house of God, in the law and in the commandment, to seek his God, he did it with all his heart. So he prospered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In all that he did in the service of the Temple of God and in his efforts to follow God’s laws and commands, Hezekiah sought his God wholeheartedly. As a result, he was very successful."
      }
    },
