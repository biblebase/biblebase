    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他说，‘我要做谎言之灵，进入他众先知的口中。’耶和华说，‘你必能成功，就这样做吧。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他回答：‘我要出去，在他眾先知的口裡成為謊言的靈。’耶和華說：‘你必能引誘他，並且可以成功；你去這樣行吧。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：『我去，要在他眾先知口中作謊言的靈。』耶和華說：『這樣，你必能引誘他，你去如此行吧！』"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he said, ‘I will go out, and will be a lying spirit in the mouth of all his prophets.’ And he said, ‘You are to entice him, and you shall succeed; go out and do so.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said, ‘I will go and be a deceiving spirit in the mouth of all his prophets.’ Then He said, ‘You are to entice him and prevail also. Go and do so.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘I will go and be a deceiving spirit in the mouths of all his prophets,’ he said. “ ‘You will succeed in enticing him,’ said the Lord. ‘Go and do it.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So he said, ‘I will go out and be a lying spirit in the mouth of all his prophets.’ And the  Lord said, ‘You shall persuade him and also prevail; go out and do so.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And the spirit replied, ‘I will go out and inspire all of Ahab’s prophets to speak lies.’ “‘You will succeed,’ said the Lord. ‘Go ahead and do it.’"
      }
    },
