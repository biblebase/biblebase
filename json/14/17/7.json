    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他在执政第三年委派官员便·亥伊勒、俄巴底、撒迦利雅、拿坦业和米该亚到犹大各城教导民众，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他作王第三年，就差派他的大臣便．亥伊勒、俄巴底、撒迦利雅、拿坦業、米該亞，到猶大各城去教訓人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他作王第三年，就差遣臣子便‧亥伊勒、俄巴底、撒迦利雅、拿坦業、米該亞往猶大各城去教訓百姓。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "In the third year of his reign he sent his officials, Ben-hail, Obadiah, Zechariah, Nethanel, and Micaiah, to teach in the cities of Judah;"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then in the third year of his reign he sent his officials, Ben-hail, Obadiah, Zechariah, Nethanel and Micaiah, to teach in the cities of Judah;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the third year of his reign he sent his officials Ben-Hail, Obadiah, Zechariah, Nethanel and Micaiah to teach in the towns of Judah."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Also in the third year of his reign he sent his leaders, Ben-Hail, Obadiah, Zechariah, Nethanel, and Michaiah, to teach in the cities of Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In the third year of his reign Jehoshaphat sent his officials to teach in all the towns of Judah. These officials included Ben-hail, Obadiah, Zechariah, Nethanel, and Micaiah."
      }
    },
