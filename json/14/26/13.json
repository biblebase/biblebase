    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们手下有三十万七千五百精兵，随时帮助王攻打敌人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在他們指揮下的軍隊，共有三十萬七千五百人，都是大有能力，善於作戰的，能幫助王攻打仇敵。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們手下的軍兵共有三十萬七千五百人，都有大能，善於爭戰，幫助王攻擊仇敵。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Under their command was an army of 307,500, who could make war with mighty power, to help the king against the enemy."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Under their direction was an elite army of 307,500, who could wage war with great power, to help the king against the enemy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Under their command was an army of 307,500 men trained for war, a powerful force to support the king against his enemies."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And under their authority was an army of three hundred and seven thousand five hundred, that made war with mighty power, to help the king against the enemy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The army consisted of 307,500 men, all elite troops. They were prepared to assist the king against any enemy."
      }
    },
