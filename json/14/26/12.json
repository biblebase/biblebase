      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "族長、大能勇士的總數共有二千六百人，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The whole number of the heads of fathers’ houses of mighty men of valor was 2,600."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The total number of the heads of the households, of valiant warriors, was 2,600."
      },
      "niv": {
