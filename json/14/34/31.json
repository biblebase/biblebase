    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王站在自己的位置上，在耶和华面前立约，要全心全意地跟随耶和华，遵从祂的一切诫命、法度和律例，履行约书上的规定。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王站在他的位上，在耶和華面前立約，要一心一意跟隨耶和華，謹守他的誡命、法度和律例，實行這書上所記立約的誓言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王站在他的地位上，在耶和華面前立約，要盡心盡性地順從耶和華，遵守他的誡命、法度、律例，成就這書上所記的約言；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the king stood in his place and made a covenant before the Lord, to walk after the Lord and to keep his commandments and his testimonies and his statutes, with all his heart and all his soul, to perform the words of the covenant that were written in this book."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king stood in his place and made a covenant before the Lord to walk after the Lord, and to keep His commandments and His testimonies and His statutes with all his heart and with all his soul, to perform the words of the covenant written in this book."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king stood by his pillar and renewed the covenant in the presence of the Lord—to follow the Lord and keep his commands, statutes and decrees with all his heart and all his soul, and to obey the words of the covenant written in this book."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then the king stood in his place and made a covenant before the Lord, to follow the Lord, and to keep His commandments and His testimonies and His statutes with all his heart and all his soul, to perform the words of the covenant that were written in this book."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king took his place of authority beside the pillar and renewed the covenant in the Lord’s presence. He pledged to obey the Lord by keeping all his commands, laws, and decrees with all his heart and soul. He promised to obey all the terms of the covenant that were written in the scroll."
      }
    },
