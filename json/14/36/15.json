    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.36.14-21",
            "paragraphs": [
              "三六14～21這裏說出猶大被巴比倫擄去的兩個原因：不斷拜偶像，以及沒有讓土地享受七十年的安息。參看利未記二十五章2至7節的腳註。關於七十年被擄的結束，參看以斯拉記三章8節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们祖先的上帝耶和华因为怜爱祂的子民和祂的居所，就不断派使者来劝告他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華他們列祖的　神，因為愛惜自己的子民和居所，就常常差派使者警戒他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－他們列祖的神因為愛惜自己的民和他的居所，從早起來差遣使者去警戒他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The Lord, the God of their fathers, sent persistently to them by his messengers, because he had compassion on his people and on his dwelling place."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord, the God of their fathers, sent word to them again and again by His messengers, because He had compassion on His people and on His dwelling place;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord, the God of their ancestors, sent word to them through his messengers again and again, because he had pity on his people and on his dwelling place."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the Lord God of their fathers sent warnings to them by His messengers, rising up early and sending them, because He had compassion on His people and on His dwelling place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord, the God of their ancestors, repeatedly sent his prophets to warn them, for he had compassion on his people and his Temple."
      }
    },
