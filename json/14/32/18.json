    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚述王的使者用希伯来语大声向城墙上的耶路撒冷人喊话，威吓他们，使他们惧怕，以便攻取城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞述王的臣僕用猶大語，向城牆上的耶路撒冷人民大聲呼叫，要驚嚇他們，使他們慌亂，好攻取那城。"
