    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然亚兰军只来了一小队，但耶和华把大队的犹大军兵交在他们手中，以惩罚约阿施，因为犹大人背弃了他们祖先的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞蘭的軍隊雖然只來了一小隊人，耶和華卻把一支龐大的軍隊交在他們手裡；因為猶大人離棄了耶和華他們列祖的　神，亞蘭人向約阿施執行了懲罰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞蘭的軍兵雖來了一小隊，耶和華卻將大隊的軍兵交在他們手裏，是因猶大人離棄耶和華－他們列祖的神，所以藉亞蘭人懲罰約阿施。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Though the army of the Syrians had come with few men, the Lord delivered into their hand a very great army, because Judah had forsaken the Lord, the God of their fathers. Thus they executed judgment on Joash."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Indeed the army of the Arameans came with a small number of men; yet the Lord delivered a very great army into their hands, because they had forsaken the Lord, the God of their fathers. Thus they executed judgment on Joash."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Although the Aramean army had come with only a few men, the Lord delivered into their hands a much larger army. Because Judah had forsaken the Lord, the God of their ancestors, judgment was executed on Joash."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For the army of the Syrians came with a small company of men; but the Lord  delivered a very great army into their hand, because they had forsaken the Lord God of their fathers. So they executed judgment against Joash."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Although the Arameans attacked with only a small army, the Lord helped them conquer the much larger army of Judah. The people of Judah had abandoned the Lord, the God of their ancestors, so judgment was carried out against Joash."
      }
    },
