    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.1.1",
            "paragraphs": [
              "一1\"國位堅固\"。所羅門緊緊地掌握統治的大權。他穩固掌權的詳情可見於列王紀上第一、二章（觀察王上二46）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫的儿子所罗门巩固了自己的王位。因为他的上帝耶和华与他同在，使他极其伟大。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛的兒子所羅門鞏固了他對以色列的統治，耶和華他的　神也和他同在，使他十分尊大。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛的兒子所羅門國位堅固；耶和華－他的神與他同在，使他甚為尊大。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Solomon the son of David established himself in his kingdom, and the Lord his God was with him and made him exceedingly great."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Solomon the son of David established himself securely over his kingdom, and the Lord his God was with him and exalted him greatly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Solomon son of David established himself firmly over his kingdom, for the Lord his God was with him and made him exceedingly great."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now Solomon the son of David was strengthened in his kingdom, and the Lord his God was with him and exalted him exceedingly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Solomon son of David took firm control of his kingdom, for the Lord his God was with him and made him very powerful."
      }
    },
