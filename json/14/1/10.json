    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "2ch.1.10",
            "paragraphs": [
              "1:10",
              "求你賜我智慧：智慧是一種審慎的洞察力，它使人能從神和神話語的觀點出發來看待和衡量所有事物；它是生命中最寶貴的財富之一(箴3:15)。神渴望將這智慧賜給每一個信徒。智慧往往借著我們的禱告(7節；雅1:5)和聖靈的感動(羅8:5~15；加5:16~25；弗5:17~18)，以及在我們勤奮考查聖經來學習神的道的過程中(詩25:4；119:2~3；徒17:11)臨到我們。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "2ch.1.10",
            "paragraphs": [
              "一10所羅門的\"智慧\"（正確地運用知識的能力）是獨特的（王上三12）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在，求你赐我智慧和知识以带领他们。不然，谁能治理这么多的百姓呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在求你賜我智慧和知識，使我可以領導這人民；否則，誰能治理你這眾多的人民呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你賜我智慧聰明，我好在這民前出入；不然，誰能判斷這眾多的民呢？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Give me now wisdom and knowledge to go out and come in before this people, for who can govern this people of yours, which is so great?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Give me now wisdom and knowledge, that I may go out and come in before this people, for who can rule this great people of Yours?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Give me wisdom and knowledge, that I may lead this people, for who is able to govern this great people of yours?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now give me wisdom and knowledge, that I may go out and come in before this people; for who can judge this great people of Yours?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give me the wisdom and knowledge to lead them properly, for who could possibly govern this great people of yours?”"
      }
    },
