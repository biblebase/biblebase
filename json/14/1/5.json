      "leili": {
        "content": [
          {
            "verse": "2ch.1.5",
            "paragraphs": [
              "一5\"銅壇\"。參看出埃及記二十七章1至8節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "户珥的孙子、乌利的儿子比撒列造的铜坛在基遍耶和华的会幕前，所罗门和会众就在那里求问耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "戶珥的孫子、烏利的兒子比撒列所做的銅祭壇也在那裡，在耶和華的會幕前面；所羅門和會眾同去求問耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且戶珥的孫子、烏利的兒子比撒列所造的銅壇也在基遍耶和華的會幕前。所羅門和會眾都就近壇前。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Moreover, the bronze altar that Bezalel the son of Uri, son of Hur, had made, was there before the tabernacle of the Lord. And Solomon and the assembly sought it out."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the bronze altar, which Bezalel the son of Uri, the son of Hur, had made, was there before the tabernacle of the Lord, and Solomon and the assembly sought it out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the bronze altar that Bezalel son of Uri, the son of Hur, had made was in Gibeon in front of the tabernacle of the Lord; so Solomon and the assembly inquired of him there."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now the bronze altar that Bezalel the son of Uri, the son of Hur, had made, he put before the tabernacle of the Lord; Solomon and the assembly sought Him there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the bronze altar made by Bezalel son of Uri and grandson of Hur was there at Gibeon in front of the Tabernacle of the Lord. So Solomon and the people gathered in front of it to consult the Lord."
      }
    },
