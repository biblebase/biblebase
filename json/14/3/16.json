    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他也造了链子，装饰在柱冠上。他又造了一百个石榴，安在链子上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又做了像項鍊的鍊子，安放在柱頭上；又做了石榴一百個，安放在鍊子上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又照聖所內鍊子的樣式做鍊子，安在柱頂上；又做一百石榴，安在鍊子上。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He made chains like a necklace and put them on the tops of the pillars, and he made a hundred pomegranates and put them on the chains."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He made chains in the inner sanctuary and placed them on the tops of the pillars; and he made one hundred pomegranates and placed them on the chains."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He made interwoven chains and put them on top of the pillars. He also made a hundred pomegranates and attached them to the chains."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He made wreaths of chainwork, as in the inner sanctuary, and put them on top of the pillars; and he made one hundred pomegranates, and put them on the wreaths of chainwork."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He made a network of interwoven chains and used them to decorate the tops of the pillars. He also made 100 decorative pomegranates and attached them to the chains."
      }
    },
