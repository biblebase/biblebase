    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.12.1",
            "paragraphs": [
              "一二1\"離棄耶和華的律法\"。實行迦南人的偶像膜拜（王上一四22,23）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "罗波安王位稳定、国势强盛后，就离弃耶和华的律法，以色列人也都随从他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "羅波安的王位穩定，他也強盛起來，就離棄了耶和華的律法，全體以色列人也都跟隨他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "羅波安的國堅立，他強盛的時候就離棄耶和華的律法，以色列人也都隨從他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When the rule of Rehoboam was established and he was strong, he abandoned the law of the Lord, and all Israel with him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the kingdom of Rehoboam was established and strong, he and all Israel with him forsook the law of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After Rehoboam’s position as king was established and he had become strong, he and all Israel with him abandoned the law of the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now it came to pass, when Rehoboam had established the kingdom and had strengthened himself, that he forsook the law of the Lord, and all Israel along with him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But when Rehoboam was firmly established and strong, he abandoned the Law of the Lord, and all Israel followed him in this sin."
      }
    },
