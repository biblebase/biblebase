    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.12.9",
            "paragraphs": [
              "一二9\"金盾牌\"。參看列王紀上十章16、17節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "埃及王示撒带兵攻陷了耶路撒冷，把耶和华殿里和王宫里的财宝及所罗门造的金盾牌掳掠一空。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是埃及王示撒上來攻打耶路撒冷，奪取了耶和華殿和王宮裡的寶物，把它們全部帶走，又奪去了所羅門所做的金盾牌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，埃及王示撒上來攻取耶路撒冷，奪了耶和華殿和王宮裏的寶物，盡都帶走，又奪去所羅門製造的金盾牌。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So Shishak king of Egypt came up against Jerusalem. He took away the treasures of the house of the Lord and the treasures of the king’s house. He took away everything. He also took away the shields of gold that Solomon had made,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Shishak king of Egypt came up against Jerusalem, and took the treasures of the house of the Lord and the treasures of the king’s palace. He took everything; he even took the golden shields which Solomon had made."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Shishak king of Egypt attacked Jerusalem, he carried off the treasures of the temple of the Lord and the treasures of the royal palace. He took everything, including the gold shields Solomon had made."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So Shishak king of Egypt came up against Jerusalem, and took away the treasures of the house of the Lord and the treasures of the king’s house; he took everything. He also carried away the gold shields which Solomon had made."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So King Shishak of Egypt came up and attacked Jerusalem. He ransacked the treasuries of the Lord’s Temple and the royal palace; he stole everything, including all the gold shields Solomon had made."
      }
    },
