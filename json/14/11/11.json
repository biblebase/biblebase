        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又鞏固這些設防城，在城裡設軍長，貯備糧食、油和酒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "羅波安又堅固各處的保障，在其中安置軍長，又預備下糧食、油、酒。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He made the fortresses strong, and put commanders in them, and stores of food, oil, and wine."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also strengthened the fortresses and put officers in them and stores of food, oil and wine."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He strengthened their defenses and put commanders in them, with supplies of food, olive oil and wine."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And he fortified the strongholds, and put captains in them, and stores of food, oil, and wine."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rehoboam strengthened their defenses and stationed commanders in them, and he stored supplies of food, olive oil, and wine."
      }
    },
