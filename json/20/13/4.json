    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.13.4",
            "paragraphs": [
              "怠惰使你無法滿足（十三4）",
              "見分題研究：“Ⅰ懶惰人的性格”，原書第40～42頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "懒惰人空有幻想， 勤快人心想事成。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "懶惰人渴求，卻一無所得； 殷勤人必得豐裕。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "懒惰人贪得无厌，但他的欲望一无所得； 殷勤人的欲求，却必得满足。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "懶惰人羨慕，卻無所得； 殷勤人必得豐裕。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The soul of the sluggard craves and gets nothing, while the soul of the diligent is richly supplied."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The soul of the sluggard craves and gets nothing, But the soul of the diligent is made fat."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A sluggard’s appetite is never filled, but the desires of the diligent are fully satisfied."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The soul of a lazy man desires, and has nothing; But the soul of the diligent shall be made rich."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lazy people want much but get little, but those who work hard will prosper."
      }
    },
