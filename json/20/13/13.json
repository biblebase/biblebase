        "text": "藐視訓言的，自取滅亡； 敬畏誡命的，必得善報。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Whoever despises the word brings destruction on himself, but he who reveres the commandment will be rewarded."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The one who despises the word will be in debt to it, But the one who fears the commandment will be rewarded."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever scorns instruction will pay for it, but whoever respects a command is rewarded."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He who despises the word will be destroyed, But he who fears the commandment will be rewarded."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People who despise advice are asking for trouble; those who respect a command will succeed."
      }
