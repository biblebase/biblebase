    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.20",
            "paragraphs": [
              "智慧──長期投資（十九20）",
              "論及具有特色的前瞻性，見五4的註釋。也見分題研究：“Ⅱ智慧的獲致”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.19.20",
            "paragraphs": [
              "一九20\"終久\"。即在你人生余下的時間裏。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
