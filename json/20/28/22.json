    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.28.22",
            "paragraphs": [
              "吝嗇鬼的失算（二十八22）",
              "吝嗇或貪婪的靈（惡眼，和合、AV、RV：參，二十三6；太二十15）確保了內在的貧窮（參25與27節），即使外在的財寶仍然持續。也見第20節。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.28.22",
            "paragraphs": [
              "二八22\"惡眼\"。自私的動機。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贪婪的人急于发财， 却不知贫穷即将临到。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "吝嗇的人急切求財， 卻不知窮乏快要臨到。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "贪婪的人，急切要发财， 但他不明白穷乏将临到他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人有惡眼想要急速發財， 卻不知窮乏必臨到他身。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "A stingy man  hastens after wealth and does not know that poverty will come upon him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A man with an evil eye hastens after wealth And does not know that want will come upon him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The stingy are eager to get rich and are unaware that poverty awaits them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "A man with an evil eye hastens after riches, And does not consider that poverty will come upon him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Greedy people try to get rich quick but don’t realize they’re headed for poverty."
      }
    },
