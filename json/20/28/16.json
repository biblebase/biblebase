    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "昏庸的君王残暴不仁， 恨不义之财的享长寿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "昏庸的君主必多行強暴， 憎惡不義之財的，必享長壽。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "无知的领袖是残酷的欺压者； 但恨恶私利的人必日子长久。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無知的君多行暴虐； 以貪財為可恨的，必年長日久。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "A ruler who lacks understanding is a cruel oppressor, but he who hates unjust gain will prolong his days."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A leader who is a great oppressor lacks understanding, But he who hates unjust gain will prolong his days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A tyrannical ruler practices extortion, but one who hates ill-gotten gain will enjoy a long reign."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "A ruler who lacks understanding is a great oppressor, But he who hates covetousness will prolong his days."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A ruler with no understanding will oppress his people, but one who hates corruption will have a long life."
      }
    },
