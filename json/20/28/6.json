    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.28.6",
            "paragraphs": [
              "他值多少？（二十八6）",
              "見十九1。這裏與第18節的“道路”（AV）是雙數字，強化了已經呈現在乖僻（或更好是譯作“邪曲”）中之言行不符的觀念。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "行为正直的穷人， 胜过行事邪僻的富人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "行為完全的窮人， 勝過行事狡詐的財主。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "行事纯全的穷人， 好过行事歪邪的富人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行為純正的窮乏人 勝過行事乖僻的富足人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Better is a poor man who walks in his integrity than a rich man who is crooked in his ways."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Better is the poor who walks in his integrity Than he who is crooked though he be rich."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Better the poor whose walk is blameless than the rich whose ways are perverse."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Better is the poor who walks in his integrity Than one perverse in  his ways, though he be rich."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Better to be poor and honest than to be dishonest and rich."
      }
