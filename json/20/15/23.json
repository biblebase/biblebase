    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "应对得体，心中愉快； 言语合宜，何等美好！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "應對得當，自己也覺喜樂； 合時的話，多麼美好！"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "口善应对，人有喜乐； 话合其时，多么美好！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "口善應對，自覺喜樂； 話合其時，何等美好。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "To make an apt answer is a joy to a man, and a word in season, how good it is!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A man has joy in an apt answer, And how delightful is a timely word!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A person finds joy in giving an apt reply— and how good is a timely word!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "A man has joy by the answer of his mouth, And a word spoken in due season, how good it  is!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone enjoys a fitting reply; it is wonderful to say the right thing at the right time!"
      }
    },
