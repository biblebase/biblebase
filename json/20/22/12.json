        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的眼目护卫真理， 祂必推翻奸徒的言论。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的眼目護衛知識， 卻傾覆奸詐人的話。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华的眼目守护知识， 他却推翻背信者的话。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的眼目眷顧聰明人， 卻傾敗奸詐人的言語。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The eyes of the Lord keep watch over knowledge, but he overthrows the words of the traitor."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The eyes of the Lord preserve knowledge, But He overthrows the words of the treacherous man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The eyes of the Lord keep watch over knowledge, but he frustrates the words of the unfaithful."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The eyes of the Lord preserve knowledge, But He overthrows the words of the faithless."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord preserves those with knowledge, but he ruins the plans of the treacherous."
      }
    },
