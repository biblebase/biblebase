    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.10.14",
            "paragraphs": [
              "適切的沈默（十14）",
              "積存（呂譯“含蓄著”；現中“吸收”；思高“隱諱”）在這裏的意思是：“儲存起來，留待適宜的時機”，也就是說：它是論及慎思明辨，而不是論及博學。第二行末尾字面的意思是：“是迫在眉睫的毀滅”；所以和合本作速致敗壞（參，呂譯“催促著敗落”；思高、RSV“招致逼近喪亡”）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智者储藏知识， 愚人口惹祸端。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "智慧人積存知識， 愚妄人的口招致毀滅。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "智慧人珍藏知识， 愚妄人的口速招败亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧人積存知識； 愚妄人的口速致敗壞。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The wise lay up knowledge, but the mouth of a fool brings ruin near."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Wise men store up knowledge, But with the mouth of the foolish, ruin is at hand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wise store up knowledge, but the mouth of a fool invites ruin."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Wise people store up knowledge, But the mouth of the foolish is near destruction."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wise people treasure knowledge, but the babbling of a fool invites disaster."
      }
