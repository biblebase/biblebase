    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "精明儿夏季时贮藏， 不肖子收成时酣睡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "夏天收聚的，是明慧的人， 收割時沉睡的，是貽羞的人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "在夏天收集的，是明达之子； 收割时沉睡的，是羞愧之子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "夏天聚斂的，是智慧之子； 收割時沉睡的，是貽羞之子。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He who gathers in summer is a prudent son, but he who sleeps in harvest is a son who brings shame."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who gathers in summer is a son who acts wisely, But he who sleeps in harvest is a son who acts shamefully."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He who gathers crops in summer is a prudent son, but he who sleeps during harvest is a disgraceful son."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He who gathers in summer is a wise son; He who sleeps in harvest is  a son who causes shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A wise youth harvests in the summer, but one who sleeps during harvest is a disgrace."
      }
    },
