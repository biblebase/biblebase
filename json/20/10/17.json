    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.10.17",
            "paragraphs": [
              "繼續受教，你就繼續長進（十17）",
              "“走向生命之路”（希伯來文，思高、RSV），是本節與前一節所共有的詞句，強調的事實是：生命不只是生存而已，而是要去達到的質素。註意在謹守與違棄之間的對比，也就是說：不僅必須聆聽訓誨，還必須長期地、堅定地持守這訓誨。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听从教诲的，走生命之路； 拒绝责备的，必步入歧途。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "遵守教訓的，走在生命的路上； 離棄責備的，卻是走上錯路。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "接受管教的，在生命路上； 离弃责备的，必导致迷失。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "謹守訓誨的，乃在生命的道上； 違棄責備的，便失迷了路。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Whoever heeds instruction is on the path to life, but he who rejects reproof leads others astray."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He is on the path of life who heeds instruction, But he who ignores reproof goes astray."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever heeds discipline shows the way to life, but whoever ignores correction leads others astray."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He who keeps instruction is  in the way of life, But he who refuses correction goes astray."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People who accept discipline are on the pathway to life, but those who ignore correction will go astray."
      }
    },
