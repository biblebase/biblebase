    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.23.22-25",
            "paragraphs": [
              "引以為榮的兒子（二十三22～25）",
              "這裏包含了第五條誡命的實際內容，直接的（22節）與間接的（23～25節）。也見第12～16節，並分題研究：“Ⅱ父母與兒女”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人的父亲喜乐无限， 智慧之子使父母欢欣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的父親必大有快樂； 生下智慧的兒子的，必因兒子歡喜。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "义人的父亲，必大有快乐； 生智慧儿子的，必因他欢喜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人的父親必大得快樂； 人生智慧的兒子，必因他歡喜。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The father of the righteous will greatly rejoice; he who fathers a wise son will be glad in him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The father of the righteous will greatly rejoice, And he who sires a wise son will be glad in him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The father of a righteous child has great joy; a man who fathers a wise son rejoices in him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The father of the righteous will greatly rejoice, And he who begets a wise child will delight in him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The father of godly children has cause for joy. What a pleasure to have children who are wise."
      }
    },
