    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.23.29-35",
            "paragraphs": [
              "酒（二十三29～35）",
              "對酒鬼作了令人難以忘懷的論述：他在別人眼中看起來的情形（29節），與他眼中所看見的情形（33～35節）。他的想像力就跟他的雙腿一樣無法控制（34節）；如果他初次所受的誘惑有悲傷感（31節），那麽他最後的虛張聲勢就更傷感了（35節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.23.29-35",
            "paragraphs": [
              "二三29～35智慧的導師警告人不要醉酒的時候，說出一個酒徒的個案。"
            ]
          },
          {
            "verse": "pro.23.35",
            "paragraphs": [
              "二三35人喝醉了可能會打架，但由於他不清醒，以至感覺不到打在他身上的拳頭。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你会说：“人打我，我不痛； 人揍我，我不知。 什么时候醒了，再干一杯！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你必說：“人打我，我不痛； 人鞭打我，我不曉得； 我甚麼時候清醒？ 我還要再去尋酒。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“他们打我，我不疼痛； 他们击打我，我也不知道。 我什么时候醒来呢？ 我还要继续寻找酒。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必說：人打我，我卻未受傷； 人鞭打我，我竟不覺得。 我幾時清醒，我仍去尋酒。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“They struck me,” you will say, “but I was not hurt; they beat me, but I did not feel it. When shall I awake? I must have another drink.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“They struck me, but I did not become ill; They beat me, but I did not know it. When shall I awake? I will seek another drink.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“They hit me,” you will say, “but I’m not hurt! They beat me, but I don’t feel it! When will I wake up so I can find another drink?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“They have struck me, but I was not hurt; They have beaten me, but I did not feel it. When shall I awake, that I may seek another drink?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And you will say, “They hit me, but I didn’t feel it. I didn’t even know it when they beat me up. When will I wake up so I can look for another drink?”"
      }
    },
