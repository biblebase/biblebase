    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.8",
            "paragraphs": [
              "鑒察人性（二十8）",
              "驅散主要的意義是“吹散糠秕”（RV邊註、RSV）；第26節的碌碡證實了這種譯法。真正統治者的老練眼目將糠秕與麥子篩選出來；耶和華的靈比此更為確定，以賽亞書十一3；哥林多前書二15。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王坐在宝座上审判， 他的眼目明察罪恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "君王坐在審判的位上， 他的眼目查察一切惡事。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "王坐在审判的宝座上， 以自己的眼目分辨出一切恶事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王坐在審判的位上， 以眼目驅散諸惡。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "A king who sits on the throne of judgment winnows all evil with his eyes."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A king who sits on the throne of justice Disperses all evil with his eyes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When a king sits on his throne to judge, he winnows out all evil with his eyes."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "A king who sits on the throne of judgment Scatters all evil with his eyes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When a king sits in judgment, he weighs all the evidence, distinguishing the bad from the good."
      }
