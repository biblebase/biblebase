        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "心灵是耶和华的灯， 鉴察人的肺腑心肠。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人的靈是耶和華的燈， 探照人的臟腑。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "人的灵是耶和华的灯， 探寻人内心深处的一切。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人的靈是耶和華的燈， 鑒察人的心腹。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The spirit of man is the lamp of the Lord, searching all his innermost parts."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The spirit of man is the lamp of the Lord, Searching all the innermost parts of his being."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The human spirit is the lamp of the Lord that sheds light on one’s inmost being."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The spirit of a man is the lamp of the Lord, Searching all the inner depths of his heart."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord’s light penetrates the human spirit, exposing every hidden motive."
      }
