      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要貪睡，免致貧窮； 眼要睜開，你就吃飽。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Love not sleep, lest you come to poverty; open your eyes, and you will have plenty of bread."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not love sleep, or you will become poor; Open your eyes, and you will be satisfied with food."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not love sleep or you will grow poor; stay awake and you will have food to spare."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Do not love sleep, lest you come to poverty; Open your eyes, and you will be satisfied with bread."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you love sleep, you will end in poverty. Keep your eyes open, and there will be plenty to eat!"
      }
