    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.17.20",
            "paragraphs": [
              "聰明反被聰明誤（十七20）",
              "十一20所警告的乃是神的憎惡，這則箴言（像第19節一樣）所警告的則是俗世的禍患。可與八8對比。關於邪僻（和合、AV、RV），見二14、15。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "心术不正，难觅幸福； 口吐谎言，陷入祸患。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "心存欺詐的得不著益處； 舌頭搬弄是非的必陷在禍患中。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "心里歪邪的，找不到福份； 话语善变的，陷在恶事中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心存邪僻的，尋不着好處； 舌弄是非的，陷在禍患中。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "A man of crooked heart does not discover good, and one with a dishonest tongue falls into calamity."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who has a crooked mind finds no good, And he who is perverted in his language falls into evil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One whose heart is corrupt does not prosper; one whose tongue is perverse falls into trouble."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He who has a deceitful heart finds no good, And he who has a perverse tongue falls into evil."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The crooked heart will not prosper; the lying tongue tumbles into trouble."
      }
    },
