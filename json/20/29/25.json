    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.25",
            "paragraphs": [
              "“神若幫助我們……”（二十九25）",
              "見十六7的註釋。安穩是“安置在高處”（即過於人所能構到的範圍），是十八10末了那個動詞的強化形式。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.29.25",
            "paragraphs": [
              "二九25\"必得安穩\"。更可作：在高處得安穩。人因信靠神而克服恐懼。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "惧怕人的必自陷网罗， 信靠耶和华的必安稳。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "懼怕人的，必陷入網羅； 倚靠耶和華的，必得安全。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "对人的惧怕，会成为陷阱； 依靠耶和华的，安稳在高处。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "懼怕人的，陷入網羅； 惟有倚靠耶和華的，必得安穩。"
      },
      "esv": {
        "version_id": "ESV",
