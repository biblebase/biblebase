    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.2",
            "paragraphs": [
              "人民的快樂（二十九2）",
              "參二十八12、28，二十九16。第一行的頭一個動詞（增多）與二十八28，二十九16一致，而且形成極佳的含義。AV與RSV毫無根據地改變了一個希伯來文子音，以加強與第二行較嚴謹之平行（“掌權”）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人增多，万众欢腾； 恶人得势，万民叹息。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人增多的時候，人民就喜樂； 惡人掌權的時候，人民就歎息。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "义人增多时，民众就欢喜； 恶人管辖时，民众就叹息。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人增多，民就喜樂； 惡人掌權，民就歎息。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When the righteous increase, the people rejoice, but when the wicked rule, the people groan."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the righteous increase, the people rejoice, But when a wicked man rules, people groan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the righteous thrive, the people rejoice; when the wicked rule, the people groan."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "When the righteous are in authority, the people rejoice; But when a wicked man rules, the people groan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the godly are in authority, the people rejoice. But when the wicked are in power, they groan."
      }
    },
