    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.21.19",
            "paragraphs": [
              "好爭吵的女人（二十一19）",
              "參第9節，並十九13的註釋。",
              "物質的與屬靈的寶藏（二十一20、21）",
              "見第17節的註釋。在第21節中，仁慈（AV、RV：“憐憫”）乃是hesed[，“專一的愛”、“忠貞”，就如神向拿俄米、路得向波阿斯所顯的（得二20，三1）。這一節的詞語，其內容有待馬太福音五\\cs166來“充實”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "宁愿独居旷野， 不跟吵闹易怒的女子同住。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "寧可住在野地， 也不跟吵鬧易怒的婦人同住。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "宁可住在旷野之地， 也好过与争吵易怒的妻子同住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "寧可住在曠野， 不與爭吵使氣的婦人同住。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "It is better to live in a desert land than with a quarrelsome and fretful woman."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is better to live in a desert land Than with a contentious and vexing woman."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Better to live in a desert than with a quarrelsome and nagging wife."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Better to dwell in the wilderness, Than with a contentious and angry woman."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It’s better to live alone in the desert than with a quarrelsome, complaining wife."
      }
    },
