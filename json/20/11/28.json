    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.28",
            "paragraphs": [
              "亨通──虛假的與真實的（十一28）",
              "第一個人有突然被提升的危險，第二個人則有生命與成長的韌力。",
              "RSV將跌倒（yippo{l）改成“雕落”（yibbo{l，參，現中），這是一個迷人的筆觸，但卻沒有經文的支持，事實上也沒有必要。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倚仗财势者必衰败， 义人必兴旺如绿叶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "倚賴自己財富的，必然衰落； 義人卻必繁茂，好像綠葉。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "依靠自己财富的必将倒下； 而义人却像绿叶那样茂盛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
