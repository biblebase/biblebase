      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "义人享福，全城欢庆； 恶人灭亡，必有欢呼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人享福，合城喜樂； 惡人滅亡，人都歡呼。"
      },
