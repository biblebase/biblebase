    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.24-26",
            "paragraphs": [
              "慷慨的獎賞（十一24～26）"
            ]
          },
          {
            "verse": "pro.11.26",
            "paragraphs": [
              "26.這一節經文藉著所激發的禱告或詛咒而特別引入屬天的層次。這裏的屯積（AV還是譯作“緊握不放”，但原文與第24節的那個字不同）是有計劃地哄擡物價──不只是第24節的吝嗇。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.11.26",
            "paragraphs": [
              "一一26\"屯糧不賣\"。他囤積糧食，為的是提高價格。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "囤粮不卖，惹人咒诅； 乐意卖粮，必蒙祝福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "屯積五穀的，必被人民咒詛； 出售糧食的，福祉必臨到他的頭上。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "屯粮不卖的，民将咒骂他； 出售五谷的，祝福将临到他头上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "屯糧不賣的，民必咒詛他； 情願出賣的，人必為他祝福。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The people curse him who holds back grain, but a blessing is on the head of him who sells it."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who withholds grain, the people will curse him, But blessing will be on the head of him who sells it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "People curse the one who hoards grain, but they pray God’s blessing on the one who is willing to sell."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The people will curse him who withholds grain, But blessing will  be on the head of him who sells it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People curse those who hoard their grain, but they bless the one who sells in time of need."
      }
    },
