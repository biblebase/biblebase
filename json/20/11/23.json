    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.23",
            "paragraphs": [
              "願望──與實現（十一23）",
              "第一行（以強烈的簡潔筆法）是與什麽有關呢？第二行給了我們一條線索，也就是說：那是論到我們所向往之事在神的審判底下的結局（參，RSV）。見十24的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人的愿望结出善果， 恶人的希望招致烈怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的願望，盡是美好， 惡人的希望，必招致忿怒。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "义人的愿望，必然美好； 恶人的期待，招致盛怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人的心願盡得好處； 惡人的指望致干忿怒。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The desire of the righteous ends only in good, the expectation of the wicked in wrath."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The desire of the righteous is only good, But the expectation of the wicked is wrath."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The desire of the righteous ends only in good, but the hope of the wicked only in wrath."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The desire of the righteous is only good, But the expectation of the wicked is wrath."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The godly can look forward to a reward, while the wicked can expect only judgment."
      }
