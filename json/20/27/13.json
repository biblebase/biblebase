    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.27.13",
            "paragraphs": [
              "抵押財產（二十七13）",
              "這幾乎是二十16的重復，見該處的註釋。關於長期的保證人，見六1～5。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.27.13",
            "paragraphs": [
              "二七13\"作保\"。參看第二十章16節的腳註。\"外女\"。即淫婦（比較二○16）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁为陌生人作保， 就拿谁的衣服； 谁为淫妇作保， 谁就得交抵押。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "誰為陌生人作保證人，就拿去誰的衣服； 誰替妓女作保證人，誰就要作擔保。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "谁为别人作保，就拿去谁的衣服； 谁替淫荡女子作保，就把谁当作抵押。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誰為生人作保，就拿誰的衣服； 誰為外女作保，誰就承當。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Take a man’s garment when he has put up security for a stranger, and hold it in pledge when he puts up security for an adulteress."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Take his garment when he becomes surety for a stranger; And for an adulterous woman hold him in pledge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Take the garment of one who puts up security for a stranger; hold it in pledge if it is done for an outsider."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Take the garment of him who is surety for a stranger, And hold it in pledge when he is surety for a seductress."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Get security from someone who guarantees a stranger’s debt. Get a deposit if he does it for foreigners."
      }
