    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ecc.11.10",
            "paragraphs": [
              "一一10\"虛空\"。短暫的，迅即消逝的。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，你要抛开心中的烦恼和肉体的痛苦，因为青春年华转瞬即逝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以你當除掉心中的煩惱，除去肉體的疾苦，因為無論是幼年或是壯年，都是虛空的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "因此你当从心中除掉烦恼， 从肉体中除去疾苦； 要知道，无论年少或青壮都转眼成空。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，你當從心中除掉愁煩，從肉體克去邪惡；因為一生的開端和幼年之時，都是虛空的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Remove vexation from your heart, and put away pain from your body, for youth and the dawn of life are vanity."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So, remove grief and anger from your heart and put away pain from your body, because childhood and the prime of life are fleeting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So then, banish anxiety from your heart and cast off the troubles of your body, for youth and vigor are meaningless."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore remove sorrow from your heart, And put away evil from your flesh, For childhood and youth are vanity."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So refuse to worry, and keep your body healthy. But remember that youth, with a whole life before you, is meaningless."
      }
    },
