    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ecc.9.2-6",
            "paragraphs": [
              "九2～6不管人生中的事件看似多麽偶然，所有人都要面對一個必然的結局——死亡。"
            ]
          },
          {
            "verse": "ecc.9.4-6",
            "paragraphs": [
              "九4～6活人比死人更有優勢（4節）。死人再沒有借努力工作而得到回報的機會（5節），而且他們不能再分享這地上的生命（6節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为活着的人还知道自己终有一死，但死了的人什么都不知道，再也得不到任何赏赐，他们被忘得一干二净。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "活人還知道自己必死，死人卻一無所知，而且再得不到酬勞，他們也被遺忘。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "因为活人知道自己必死；死人却毫无所知，他们也不会再得赏报，因为他们的名号已被遗忘；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "活着的人知道必死；死了的人毫無所知，也不再得賞賜；他們的名無人記念。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For the living know that they will die, but the dead know nothing, and they have no more reward, for the memory of them is forgotten."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the living know they will die; but the dead do not know anything, nor have they any longer a reward, for their memory is forgotten."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the living know that they will die, but the dead know nothing; they have no further reward, and even their name is forgotten."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For the living know that they will die; But the dead know nothing, And they have no more reward, For the memory of them is forgotten."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The living at least know they will die, but the dead know nothing. They have no further reward, nor are they remembered."
      }
    },
