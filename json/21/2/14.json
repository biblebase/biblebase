    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智者高瞻远瞩，愚人却在黑暗中摸索。但我知道两者终必有同样的命运。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "智慧人的眼目明亮，愚昧人卻走在黑暗中。我也知道同一的命運要臨到這兩種人身上。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "智慧人看得见他的道路， 愚昧人却在黑暗中行走。 不过我也明白，同样的命运会临到他们这两种人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧人的眼目光明，愚昧人在黑暗裏行。我卻看明有一件事，這兩等人都必遇見。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The wise person has his eyes in his head, but the fool walks in darkness. And yet I perceived that the same event happens to all of them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wise man’s eyes are in his head, but the fool walks in darkness. And yet I know that one fate befalls them both."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wise have eyes in their heads, while the fool walks in the darkness; but I came to realize that the same fate overtakes them both."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The wise man’s eyes are in his head, But the fool walks in darkness. Yet I myself perceived That the same event happens to them all."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the wise can see where they are going, but fools walk in the dark.” Yet I saw that the wise and the foolish share the same fate."
      }
