      "dde": {
        "content": [
          {
            "verse": "ecc.2.22",
            "paragraphs": [
              "22.傳道者開始將一2以來的整個討論作收場。實際的“勞碌”和感情──智識的掙紮（“累心”），同樣被證為無用。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世人在日光之下劳心劳力，究竟得到什么呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人的一切勞碌和操心，就是他在日光之下所勞碌的，對自己有甚麼益處呢？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "人一切的劳苦和他心里的追求，就是他在日光之下的劳碌所为，到底能为自己成就什么呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人在日光之下勞碌累心，在他一切的勞碌上得着甚麼呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "What has a man from all the toil and striving of heart with which he toils beneath the sun?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For what does a man get in all his labor and in his striving with which he labors under the sun?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "What do people get for all the toil and anxious striving with which they labor under the sun?"
      },
      "nkjv": {
