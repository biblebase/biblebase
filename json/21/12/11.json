    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "ecc.12.11",
            "paragraphs": [
              "12:11",
              "智慧人的言語，好像刺棍：我們的好牧人發出智慧的真道就如刺棍，督促我們行在正道上；又像釘子，將真理牢牢釘在我們心中。因此，神的話語遠比世上任何的智慧書都寶貴。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "ecc.12.8-14",
            "paragraphs": [
              "一二8～14本書的結語記載其主題（8節）：所羅門的教育計劃（9,10節）、教訓的價值（11,12節）和他結束的勸勉（13,14節）。"
            ]
          },
          {
            "verse": "ecc.12.11",
            "paragraphs": [
              "一二11所羅門的話語象刺激我們的\"刺棍\"和穩定我們的\"釘子\"。這裏提到\"一個牧者\"，證實所羅門的教訓是神所賜予的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智者的言语好像赶牛的刺棍，他们收集的箴言像钉稳的钉子一样牢靠，都是一位牧者所赐的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "智慧人的言語像刺棒，他們所彙集的箴言像釘牢的釘子，都是一位牧者所賜的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "智慧人的话语如同刺棒，编集者的语录像钉牢的钉子，都是那一位牧者所赐的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧人的言語好像刺棍；會中之師的言語又像釘穩的釘子，都是一個牧者所賜的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The words of the wise are like goads, and like nails firmly fixed are the collected sayings; they are given by one Shepherd."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The words of wise men are like goads, and masters of these collections are like well-driven nails; they are given by one Shepherd."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The words of the wise are like goads, their collected sayings like firmly embedded nails—given by one shepherd."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The words of the wise are like goads, and the words of scholars are like well-driven nails, given by one Shepherd."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The words of the wise are like cattle prods—painful but helpful. Their collected sayings are like a nail-studded stick with which a shepherd drives the sheep."
      }
    },
