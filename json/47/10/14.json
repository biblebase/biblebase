    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其实把你们包括在我们夸口的范围之内一点都不过分，因为我们曾把基督的福音传到你们那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我們沒有到過你們那裡，現在就自誇得過分了；但事實上，我們早就把基督的福音傳到你們那裡了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "的确，我们没有过于扩张自己，好像达不到你们那里，实际上我们带着基督的福音已经来了，甚至远到你们那里。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們並非過了自己的界限，好像搆不到你們那裏；因為我們早到你們那裏，傳了基督的福音。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For we are not overextending ourselves, as though we did not reach you. For we were the first to come all the way to you with the gospel of Christ."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For we are not overextending ourselves, as if we did not reach to you, for we were the first to come even as far as you in the gospel of Christ;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We are not going too far in our boasting, as would be the case if we had not come to you, for we did get as far as you with the gospel of Christ."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For we are not overextending ourselves (as though our  authority did not extend to you), for it was to you that we came with the gospel of Christ;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We are not reaching beyond these boundaries when we claim authority over you, as if we had never visited you. For we were the first to travel all the way to Corinth with the Good News of Christ."
      }
    },
