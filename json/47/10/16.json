    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "使我们可以把福音传到你们以外的地方，而不是在别人的工作范围内拿别人的成就夸口。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "使我們可以把福音傳到你們以外的地方。別人在他的界限以內所完成的工作，我們不會拿來誇口的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "以致把福音传到你们以外的地方；我们不是在别人的范围之内，为那些现成的事来夸耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "得以將福音傳到你們以外的地方；並不是在別人界限之內，藉着他現成的事誇口。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "so that we may preach the gospel in lands beyond you, without boasting of work already done in another’s area of influence."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so as to preach the gospel even to the regions beyond you, and not to boast in what has been accomplished in the sphere of another."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "so that we can preach the gospel in the regions beyond you. For we do not want to boast about work already done in someone else’s territory."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "to preach the gospel in the regions beyond you, and not to boast in another man’s sphere of accomplishment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then we will be able to go and preach the Good News in other places far beyond you, where no one else is working. Then there will be no question of our boasting about work done in someone else’s territory."
      }
    },
