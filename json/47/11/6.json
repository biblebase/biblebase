    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我虽然不善辞令，但我认识真理。这一点，我在各样的事上已经向你们证明了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然我不善於辭令，卻是有學問的，我們在各方面，已經向你們清楚顯明了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "即使我在言语上没有技巧，但在知识上并不是这样。其实我们在各个方面、在所有的事上都已经向你们显明了这一点。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的言語雖然粗俗，我的知識卻不粗俗。這是我們在凡事上向你們眾人顯明出來的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Even if I am unskilled in speaking, I am not so in knowledge; indeed, in every way we have made this plain to you in all things."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But even if I am unskilled in speech, yet I am not so in knowledge; in fact, in every way we have made this evident to you in all things."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I may indeed be untrained as a speaker, but I do have knowledge. We have made this perfectly clear to you in every way."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Even though I  am untrained in speech, yet I  am not in knowledge. But we have been thoroughly manifested among you in all things."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I may be unskilled as a speaker, but I’m not lacking in knowledge. We have made this clear to you in every possible way."
      }
    },
