    "interpretations": {
      "chenzhongdao": {
        "content": [
          {
            "verse": "2co.9.6-15",
            "paragraphs": [
              "六．樂捐的賞賜（9:6-15）"
            ]
          },
          {
            "verse": "2co.9.12-14",
            "paragraphs": [
              "５．可以叫神得到榮耀（9:12-14）"
            ]
          }
        ],
        "title": "新約書信讀經講義",
        "author": "陳終道"
      },
      "matangna": {
        "content": [
          {
            "verse": "2co.9.6-15",
            "paragraphs": [
              "六．慷慨捐獻的報酬（九6～15）"
            ]
          },
          {
            "verse": "2co.9.14",
            "paragraphs": [
              "九14還不止這些呢！本節再提出兩點好處。因從哥林多送到耶路撒冷的捐款，猶太的基督徒開始意地為哥林多的聖徒禱告，因而形成雙方強烈的感情維系。耶路撒冷的聖徒因神藉哥林多信徒顯出極大的恩賜，便切切的想念哥林多信徒。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们也会因上帝赐给你们的丰盛恩典而热切地为你们祷告。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因著　神在你們身上的厚恩，他們就為你們禱告，切切地想念你們。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "并且他们因着神赐给你们这无限的恩典，就切切地想念你们，为你们祈祷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們也因神極大的恩賜顯在你們心裏，就切切地想念你們，為你們祈禱。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "while they long for you and pray for you, because of the surpassing grace of God upon you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "while they also, by prayer on your behalf, yearn for you because of the surpassing grace of God in you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And in their prayers for you their hearts will go out to you, because of the surpassing grace God has given you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and by their prayer for you, who long for you because of the exceeding grace of God in you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And they will pray for you with deep affection because of the overflowing grace God has given to you."
      }
    },
