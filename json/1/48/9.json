    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.48.1-22",
            "paragraphs": [
              "雅各祝福以法蓮與瑪拿西（四十八1～22）",
              "希伯來書十一21從雅各一生漫長的經歷中，挑出這一段作為他信心的巔峰造極行動。那一章所稱道的，是這舉動所顯示抓住應許的品質，在臨死之前仍然不渝：“從遠處望見，且歡喜迎接”。當前的景況與他年輕時耍欺騙手段的情形一模一樣，略具諷刺意味。長子的福份再一度給了次子，不過這次並不是出於缺乏信心的計謀，後來也沒有產生苦毒的態度（參箴十22）。這是安靜回應與信心的實例和教訓。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约瑟说：“是上帝在这里赐给我的儿子。”以色列说：“把他们带过来，我要祝福他们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約瑟回答父親：“他們是我的兒子，是　神在這裡賜給我的。”以色列說：“把他們領到我這裡來，我要給他們祝福。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "约瑟对他父亲说：“他们是我的儿子，是神在这里赐给我的。” 以色列说：“把他们带到我这里，好让我祝福他们。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約瑟對他父親說：「這是神在這裏賜給我的兒子。」以色列說：「請你領他們到我跟前，我要給他們祝福。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Joseph said to his father, “They are my sons, whom God has given me here.” And he said, “Bring them to me, please, that I may bless them.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joseph said to his father, “They are my sons, whom God has given me here.” So he said, “Bring them to me, please, that I may bless them.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“They are the sons God has given me here,” Joseph said to his father. Then Israel said, “Bring them to me so I may bless them.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And Joseph said to his father, “They are my sons, whom God has given me in this place.” And he said, “Please bring them to me, and I will bless them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yes,” Joseph told him, “these are the sons God has given me here in Egypt.” And Jacob said, “Bring them closer to me, so I can bless them.”"
      }
    },
