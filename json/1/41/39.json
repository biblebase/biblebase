    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.41.1-45",
            "paragraphs": [
              "法老的夢與約瑟躍升為宰相（四十一1～45）",
              "1、2.RSV正確地指出此即尼羅河，如希伯來文的明示。我們可能順帶註意到，這個夢具埃及色彩，它描述母牛從河裏上來（弗格特指出，母牛喜歡沈浸在水中以躲避熱氣和蒼蠅），尋求蘆葦的草場（RV、RSV）或紙草的草圃。後者的希伯來文~a{h]u%^%%%，是從埃及借用來的字289；在約伯記八11中再次出現（參閱七十士譯本賽十九7和傳四十16）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老对约瑟说：“上帝既然把梦的意思指示给你，可见没人有你这样的见识和智慧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老對約瑟說：“　神既然把這事指示了你，就再沒有人像你這樣有見識有智慧了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "于是法老对约瑟说：“既然神把这一切都向你显明了，就没有像你这样有悟性有智慧的人了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老對約瑟說：「神既將這事都指示你，可見沒有人像你這樣有聰明有智慧。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Pharaoh said to Joseph, “Since God has shown you all this, there is none so discerning and wise as you are."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Pharaoh said to Joseph, “Since God has informed you of all this, there is no one so discerning and wise as you are."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Pharaoh said to Joseph, “Since God has made all this known to you, there is no one so discerning and wise as you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Pharaoh said to Joseph, “Inasmuch as God has shown you all this, there  is no one as discerning and wise as you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Pharaoh said to Joseph, “Since God has revealed the meaning of the dreams to you, clearly no one else is as intelligent or wise as you are."
      }
    },
