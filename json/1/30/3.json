    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拉结说：“你去跟我的婢女辟拉同房吧，这样她可以为我生孩子，我也可以留下后代。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "拉結說：“我的婢女辟拉在這裡，你可以與她親近，使她生子歸我，我就可以從她得到孩子。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "瑞秋就说：“我的女仆碧荷在这里，你与她同房，使她生孩子归于我膝下，好让我藉着她立后。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拉結說：「有我的使女辟拉在這裏，你可以與她同房，使她生子在我膝下，我便因她也得孩子。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then she said, “Here is my servant Bilhah; go in to her, so that she may give birth on my behalf, that even I may have children through her.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She said, “Here is my maid Bilhah, go in to her that she may bear on my knees, that through her I too may have children.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then she said, “Here is Bilhah, my servant. Sleep with her so that she can bear children for me and I too can build a family through her.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So she said, “Here is my maid Bilhah; go in to her, and she will bear a  child on my knees, that I also may have children by her.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Rachel told him, “Take my maid, Bilhah, and sleep with her. She will bear children for me, and through her I can have a family, too.”"
      }
    },
