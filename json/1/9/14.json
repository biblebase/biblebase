        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "当我聚起云遮盖大地时，彩虹会出现在云中，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我使雲彩蓋地的時候，必有虹現在雲彩中，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When I bring clouds over the earth and the bow is seen in the clouds,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It shall come about, when I bring a cloud over the earth, that the bow will be seen in the cloud,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whenever I bring clouds over the earth and the rainbow appears in the clouds,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "It shall be, when I bring a cloud over the earth, that the rainbow shall be seen in the cloud;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When I send clouds over the earth, the rainbow will appear in the clouds,"
      }
    },
