    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.40.1-23",
            "paragraphs": [
              "酒政與膳長之夢（四十1～23）"
            ]
          },
          {
            "verse": "gen.40.4",
            "paragraphs": [
              "4.護衛長親自284（想必仍舊是波堤乏，他會記得約瑟的可靠）對這些新來的貴客作了安排，而非由他的屬下“司獄285”來處理，像在三十九21以下所記對待約瑟的情形。因此，約瑟雖曾終於獲致了稍高一些的地位，此時卻再次落入谷底，成為囚犯的奴仆286。但他前途的開展，不是靠從前的擢升，反而是因著這一次的降卑。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "护卫长派约瑟去伺候他们。他们在监里待了一段日子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "軍長把他們交給約瑟，約瑟就侍候他們。他們在監房裡過了好些日子。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "护卫长指派约瑟与他们在一起，约瑟就侍候他们；他们在牢里过了一些日子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "護衛長把他們交給約瑟，約瑟便伺候他們；他們有些日子在監裏。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The captain of the guard appointed Joseph to be with them, and he attended them. They continued for some time in custody."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The captain of the bodyguard put Joseph in charge of them, and he took care of them; and they were in confinement for some time."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The captain of the guard assigned them to Joseph, and he attended them. After they had been in custody for some time,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the captain of the guard charged Joseph with them, and he served them; so they were in custody for a while."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They remained in prison for quite some time, and the captain of the guard assigned them to Joseph, who looked after them."
      }
