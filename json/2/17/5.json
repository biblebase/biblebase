    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "exo.17.5",
            "paragraphs": [
              "一七5摩西的\"杖\"象征神的能力，而且是給百姓活潑而清楚的提醒。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对摩西说：“你带着之前击打尼罗河水的杖，率领以色列的几个长老走在众人前面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華回答摩西：“你要到人民的面前，要帶著以色列的幾個長老和你一起，手裡要拿著你先前擊打河水的杖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對摩西說：「你手裏拿着你先前擊打河水的杖，帶領以色列的幾個長老，從百姓面前走過去。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the Lord said to Moses, “Pass on before the people, taking with you some of the elders of Israel, and take in your hand the staff with which you struck the Nile, and go."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord said to Moses, “Pass before the people and take with you some of the elders of Israel; and take in your hand your staff with which you struck the Nile, and go."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord answered Moses, “Go out in front of the people. Take with you some of the elders of Israel and take in your hand the staff with which you struck the Nile, and go."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the Lord said to Moses, “Go on before the people, and take with you some of the elders of Israel. Also take in your hand your rod with which you struck the river, and go."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord said to Moses, “Walk out in front of the people. Take your staff, the one you used when you struck the water of the Nile, and call some of the elders of Israel to join you."
      }
    },
