    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要把我吩咐你的话告诉你哥哥亚伦，由他要求法老让以色列人离开埃及。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我吩咐你的一切，你都要說；你哥哥亞倫要告訴法老，法老就讓以色列人離開他的地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡我所吩咐你的，你都要說。你的哥哥亞倫要對法老說，容以色列人出他的地。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You shall speak all that I command you, and your brother Aaron shall tell Pharaoh to let the people of Israel go out of his land."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall speak all that I command you, and your brother Aaron shall speak to Pharaoh that he let the sons of Israel go out of his land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are to say everything I command you, and your brother Aaron is to tell Pharaoh to let the Israelites go out of his country."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You shall speak all that I command you. And Aaron your brother shall tell Pharaoh to send the children of Israel out of his land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Tell Aaron everything I command you, and Aaron must command Pharaoh to let the people of Israel leave his country."
      }
    },
