      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若被野獸撕碎，看守的要帶來當作證據，所撕的不必賠還。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If it is torn by beasts, let him bring it as evidence. He shall not make restitution for what has been torn."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If it is all torn to pieces, let him bring it as evidence; he shall not make restitution for what has been torn to pieces."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If it was torn to pieces by a wild animal, the neighbor shall bring in the remains as evidence and shall not be required to pay for the torn animal."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "If it is torn to pieces by  a  beast, then he shall bring it as evidence, and he shall not make good what was torn."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If it was torn to pieces by a wild animal, the remains of the carcass must be shown as evidence, and no compensation will be required."
      }
    },
