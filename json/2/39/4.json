    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他做了两条肩带，固定在以弗得两边，用来连接以弗得。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又為以弗得做相連的肩帶，把以弗得的兩端連接起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又為以弗得做兩條相連的肩帶，接連在以弗得的兩頭。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They made for the ephod attaching shoulder pieces, joined to it at its two edges."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They made attaching shoulder pieces for the ephod; it was attached at its two upper ends."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They made shoulder pieces for the ephod, which were attached to two of its corners, so it could be fastened."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They made shoulder straps for it to couple it together; it was coupled together at its two edges."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The ephod consisted of two pieces, front and back, joined at the shoulders with two shoulder-pieces."
      }
    },
