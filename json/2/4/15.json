    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.4.15",
            "paragraphs": [
              "15.我……要指教你們。希伯來語“指教”一詞，和to^ra^“妥拉”有相同的字根。“妥拉”的意思是“教訓”，後來成為摩西律法（五經）的名稱。本節可能略帶後來的用法。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要把该说的话传给他，我会赐你们口才，教你们如何行事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要對他說話，把你要說的話放在他的口裡；我必與你的口同在，也與他的口同在；我必指教你們當行的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要將當說的話傳給他；我也要賜你和他口才，又要指教你們所當行的事。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You shall speak to him and put the words in his mouth, and I will be with your mouth and with his mouth and will teach you both what to do."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You are to speak to him and put the words in his mouth; and I, even I, will be with your mouth and his mouth, and I will teach you what you are to do."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You shall speak to him and put words in his mouth; I will help both of you speak and will teach you what to do."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now you shall speak to him and put the words in his mouth. And I will be with your mouth and with his mouth, and I will teach you what you shall do."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Talk to him, and put the words in his mouth. I will be with both of you as you speak, and I will instruct you both in what to do."
      }
    },
