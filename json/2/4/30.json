    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.4.30",
            "paragraphs": [
              "30.亞倫將……一切話述說了一遍。一如神的應許，亞倫是摩西的代言人。他大概以這身分在以色列面前施行神跡。經文暗示以色列起初不信，但沒有明言。後來他們又多次顯示了不信的心。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦把耶和华对摩西的吩咐详细地告诉他们。摩西又在百姓面前行了那些神迹，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫把耶和華對摩西所說的一切話都說了一遍，並且在人民的眼前行了這些神蹟。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫將耶和華對摩西所說的一切話述說了一遍，又在百姓眼前行了那些神蹟，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Aaron spoke all the words that the Lord had spoken to Moses and did the signs in the sight of the people."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and Aaron spoke all the words which the Lord had spoken to Moses. He then performed the signs in the sight of the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and Aaron told them everything the Lord had said to Moses. He also performed the signs before the people,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And Aaron spoke all the words which the Lord had spoken to Moses. Then he did the signs in the sight of the people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Aaron told them everything the Lord had told Moses, and Moses performed the miraculous signs as they watched."
      }
    },
