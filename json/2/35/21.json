    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.35.21",
            "paragraphs": [
              "21.模式再次擴闊：人人都有資格獻上禮物，即使本身沒技巧可言的人也不例外。從私人珠寶到各種原料，都可以獻上為禮物。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡心里有感动又甘愿奉献的，都把礼物拿来献给耶和华，作会幕、会幕中的一切器具及圣衣之用。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是心裡受感、靈裡樂意的，都來了；他們把耶和華的禮物都帶來了，用作會幕的工程和會幕中的一切使用，又用來做聖衣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡心裏受感和甘心樂意的都拿耶和華的禮物來，用以做會幕和其中一切的使用，又用以做聖衣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they came, everyone whose heart stirred him, and everyone whose spirit moved him, and brought the Lord’s contribution to be used for the tent of meeting, and for all its service, and for the holy garments."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Everyone whose heart stirred him and everyone whose spirit moved him came and brought the Lord’s contribution for the work of the tent of meeting and for all its service and for the holy garments."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and everyone who was willing and whose heart moved them came and brought an offering to the Lord for the work on the tent of meeting, for all its service, and for the sacred garments."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then everyone came whose heart was stirred, and everyone whose spirit was willing, and they brought the Lord’s offering for the work of the tabernacle of meeting, for all its service, and for the holy garments."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All whose hearts were stirred and whose spirits were moved came and brought their sacred offerings to the Lord. They brought all the materials needed for the Tabernacle, for the performance of its rituals, and for the sacred garments."
      }
    },
