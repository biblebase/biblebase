      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西对他们说：“你们今天就吃这些吧，因为今天是耶和华的安息日，地上不会有食物让你们拾取。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西說：“今天吃這個吧，因為今天是向耶和華守的安息日，今日你們在田野裡必找不著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西說：「你們今天吃這個吧！因為今天是向耶和華守的安息日；你們在田野必找不着了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Moses said, “Eat it today, for today is a Sabbath to the Lord; today you will not find it in the field."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moses said, “Eat it today, for today is a sabbath to the Lord; today you will not find it in the field."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Eat it today,” Moses said, “because today is a sabbath to the Lord. You will not find any of it on the ground today."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Moses said, “Eat that today, for today is a Sabbath to the Lord; today you will not find it in the field."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses said, “Eat this food today, for today is a Sabbath day dedicated to the Lord. There will be no food on the ground today."
      }
