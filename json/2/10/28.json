    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老对摩西说：“给我滚出去！不要再让我见到你，因为你再见我面之日必死。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老對摩西說：“你離開我去吧，你要小心，不要再見我的面；因為你再見我面的時候，你必定死。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老對摩西說：「你離開我去吧，你要小心，不要再見我的面！因為你見我面的那日你就必死！」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Pharaoh said to him, “Get away from me; take care never to see my face again, for on the day you see my face you shall die.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Pharaoh said to him, “Get away from me! Beware, do not see my face again, for in the day you see my face you shall die!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Pharaoh said to Moses, “Get out of my sight! Make sure you do not appear before me again! The day you see my face you will die.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Pharaoh said to him, “Get away from me! Take heed to yourself and see my face no more! For in the day you see my face you shall die!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Get out of here!” Pharaoh shouted at Moses. “I’m warning you. Never come back to see me again! The day you see my face, you will die!”"
      }
    },
