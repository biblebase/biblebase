    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.21.33",
            "paragraphs": [
              "33.坑（和合本作“井”），更有可能是谷物或水的儲藏庫。此外，坑還可以用作野獸的陷阱（撒下二十三20），或囚人的監牢（創三十七24）。這些坑是迦南動蕩時期的特色，因而也是社會安定的指標，越到後期則越少。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若有人打开井盖或挖了井后不把井口盖好，以致有牛或驴掉进井里，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果有人把井敞開，或是有人挖了井，而不把井口遮蓋，以致有牛或驢掉進裡面，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人若敞着井口，或挖井不遮蓋，有牛或驢掉在裏頭，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“When a man opens a pit, or when a man digs a pit and does not cover it, and an ox or a donkey falls into it,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If a man opens a pit, or digs a pit and does not cover it over, and an ox or a donkey falls into it,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If anyone uncovers a pit or digs one and fails to cover it and an ox or a donkey falls into it,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“And if a man opens a pit, or if a man digs a pit and does not cover it, and an ox or a donkey falls in it,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Suppose someone digs or uncovers a pit and fails to cover it, and then an ox or a donkey falls into it."
      }
    },
