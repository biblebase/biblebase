    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.40.1-38",
            "paragraphs": [
              "Ｄ帳幕分別為聖（四十1～38）",
              "四十1～15.分別為聖的指令",
              "這幾節和二十五至三十一章一樣，嚴格地說是期待將來的。神再次給予摩西的指示不但包括紮起帳幕、將一切聖物安放妥當，更包括了“膏立”帳幕、陳設、祭司，使之永遠歸神所用（15節）。參較出二十九章9節亞倫一脈的“永久性祭司職分”。這裏發出的指令將要在本章余下的部分執行。",
              "四十16～33.分別為聖的禮儀"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他把约版放在约柜里，横杠穿在约柜的两旁，施恩座放在约柜上，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西拿了法版，放在櫃裡，把槓穿在櫃的兩旁，把施恩座安放在約櫃上面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又把法版放在櫃裏，把槓穿在櫃的兩旁，把施恩座安在櫃上。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He took the testimony and put it into the ark, and put the poles on the ark and set the mercy seat above on the ark."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he took the testimony and put it into the ark, and attached the poles to the ark, and put the mercy seat on top of the ark."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He took the tablets of the covenant law and placed them in the ark, attached the poles to the ark and put the atonement cover over it."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He took the Testimony and put it into the ark, inserted the poles through the rings of the ark, and put the mercy seat on top of the ark."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He took the stone tablets inscribed with the terms of the covenant and placed them inside the Ark. Then he attached the carrying poles to the Ark, and he set the Ark’s cover—the place of atonement—on top of it."
      }
    },
