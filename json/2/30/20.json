    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们进会幕或在坛上献火祭之前，一定要自洁才可以供职，免得死亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們進會幕的時候，或是走近祭壇敬拜，給耶和華焚燒火祭的時候，都要用水洗濯，免得死亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們進會幕，或是就近壇前供職給耶和華獻火祭的時候，必用水洗濯，免得死亡。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When they go into the tent of meeting, or when they come near the altar to minister, to burn a food offering to the Lord, they shall wash with water, so that they may not die."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "when they enter the tent of meeting, they shall wash with water, so that they will not die; or when they approach the altar to minister, by offering up in smoke a fire sacrifice to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whenever they enter the tent of meeting, they shall wash with water so that they will not die. Also, when they approach the altar to minister by presenting a food offering to the Lord,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "When they go into the tabernacle of meeting, or when they come near the altar to minister, to burn an offering made by fire to the Lord, they shall wash with water, lest they die."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They must wash with water whenever they go into the Tabernacle to appear before the Lord and when they approach the altar to burn up their special gifts to the Lord—or they will die!"
      }
    },
