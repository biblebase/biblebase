    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "埃及人催促以色列人赶快离开埃及，因为他们说：“我们都要死了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "埃及人催促以色列人，要他們趕快離開此地，因為他們說：“我們都要死了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "埃及人催促百姓，打發他們快快出離那地，因為埃及人說：「我們都要死了。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The Egyptians were urgent with the people to send them out of the land in haste. For they said, “We shall all be dead.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Egyptians urged the people, to send them out of the land in haste, for they said, “We will all be dead.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Egyptians urged the people to hurry and leave the country. “For otherwise,” they said, “we will all die!”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the Egyptians urged the people, that they might send them out of the land in haste. For they said, “We shall all be dead.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the Egyptians urged the people of Israel to get out of the land as quickly as possible, for they thought, “We will all die!”"
      }
    },
