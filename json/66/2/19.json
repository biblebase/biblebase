    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘我知道你的行为、爱心、信心、事奉、坚忍，也知道你末后所做的善事比起初更多。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我知道你的行為，你的愛心、信心、服侍和忍耐，也知道你後來所作的比先前的還要多。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我知道你的行为，包括你的爱心、信仰、服事和忍耐，也知道你最近所做的比先前的更多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我知道你的行為、愛心、信心、勤勞、忍耐，又知道你末後所行的善事，比起初所行的更多。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“‘I know your works, your love and faith and service and patient endurance, and that your latter works exceed the first."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘I know your deeds, and your love and faith and service and perseverance, and that your deeds of late are greater than at first."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I know your deeds, your love and faith, your service and perseverance, and that you are now doing more than you did at first."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“I know your works, love, service, faith, and your patience; and as for your works, the last are more than the first."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I know all the things you do. I have seen your love, your faith, your service, and your patient endurance. And I can see your constant improvement in all these things."
      }
