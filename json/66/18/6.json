      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Pay her back as she herself has paid back others, and repay her double for her deeds; mix a double portion for her in the cup she mixed."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Pay her back even as she has paid, and give back to her double according to her deeds; in the cup which she has mixed, mix twice as much for her."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Give back to her as she has given; pay her back double for what she has done. Pour her a double portion from her own cup."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Render to her just as she rendered to you, and repay her double according to her works; in the cup which she has mixed, mix double for her."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do to her as she has done to others. Double her penalty for all her evil deeds. She brewed a cup of terror for others, so brew twice as much for her."
      }
