    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.48.8",
            "paragraphs": [
              "四八8\"萬軍之耶和華\"。關於這軍事術語的意思，參看撒母耳記上一章3節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们曾经听说，如今在万军之耶和华的城中， 在我们上帝的城中亲眼看见： 上帝要坚立这城，直到永远。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在萬軍之耶和華的城中， 就是在我們　神的城中， 我們所看見的，正如我們所聽見的： 　神必堅立這城，直到永遠。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "在万军之耶和华的城中，就是我们神的城中， 我们所看见的正如所听见的： 神必坚立这城，直到永远。细拉"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們在萬軍之耶和華的城中 －就是我們神的城中－所看見的， 正如我們所聽見的。 神必堅立這城，直到永遠。（細拉）"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "As we have heard, so have we seen in the city of the Lord of hosts, in the city of our God, which God will establish forever. Selah"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As we have heard, so have we seen In the city of the Lord of hosts, in the city of our God; God will establish her forever. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As we have heard, so we have seen in the city of the Lord Almighty, in the city of our God: God makes her secure forever."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "As we have heard, So we have seen In the city of the Lord of hosts, In the city of our God: God will establish it forever. Selah"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We had heard of the city’s glory, but now we have seen it ourselves— the city of the Lord of Heaven’s Armies. It is the city of our God; he will make it safe forever. Interlude"
      }
    },
