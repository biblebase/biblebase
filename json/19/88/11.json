    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "坟墓里的死人怎能宣扬你的慈爱呢？ 灭亡的人怎能传扬你的信实呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在墳墓裡有人述說你的慈愛嗎？ 在滅亡之地有人述說你的信實嗎？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "难道有人会在坟墓里述说你的慈爱， 在地狱里述说你的信实吗？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "豈能在墳墓裏述說你的慈愛嗎？ 豈能在滅亡中述說你的信實嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Is your steadfast love declared in the grave, or your faithfulness in Abaddon?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will Your lovingkindness be declared in the grave, Your faithfulness in Abaddon?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Is your love declared in the grave, your faithfulness in Destruction?"
