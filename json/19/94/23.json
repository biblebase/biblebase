    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.94.20-23",
            "paragraphs": [
              "九四20～23第20節的問題（神為何容許邪惡的統治者使用律法，屈枉正直？）的回答方法是：承認邪惡的存在（21節），神關懷屬祂的子民（22節），以及最終的懲罰（23節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂必使恶人自食恶果， 因他们的罪恶而毁灭他们。 我们的上帝耶和华必毁灭他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必使他們的罪孽歸到他們身上， 他必因他們的罪惡滅絕他們； 耶和華我們的　神必把他們滅絕。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他使他们的罪孽回到他们身上， 又因他们的罪恶除尽他们； 耶和华我们的神必除尽他们！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他叫他們的罪孽歸到他們身上。 他們正在行惡之中，他要剪除他們； 耶和華－我們的神要把他們剪除。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He will bring back on them their iniquity and wipe them out for their wickedness; the Lord our God will wipe them out."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has brought back their wickedness upon them And will destroy them in their evil; The Lord our God will destroy them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will repay them for their sins and destroy them for their wickedness; the Lord our God will destroy them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He has brought on them their own iniquity, And shall cut them off in their own wickedness; The Lord our God shall cut them off."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God will turn the sins of evil people back on them. He will destroy them for their sins. The Lord our God will destroy them."
      }
    },
