    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.144.12-15",
            "paragraphs": [
              "一四四12～15這幅和平與富饒的圖畫描寫茁壯的兒子、可靠的女兒、滿盈的谷倉、健壯的牲畜和神的同在。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿我们的牲口驮满货物， 我们的城墙固若金汤， 无人被掳，街上没有哭叫声。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願我們的牛群滿馱貨物； 城牆沒有缺口，沒有人出去爭戰， 在我們的街上也沒有呼叫的聲音。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我们的牛群满负货物， 城墙也没有破口，没有人流亡， 我们的广场上也没有哭声。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們的牛馱着滿馱， 沒有人闖進來搶奪， 也沒有人出去爭戰； 我們的街市上也沒有哭號的聲音。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "may our cattle be heavy with young, suffering no mishap or failure in bearing; may there be no cry of distress in our streets!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let our cattle bear Without mishap and without loss, Let there be no outcry in our streets!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "our oxen will draw heavy loads. There will be no breaching of walls, no going into captivity, no cry of distress in our streets."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "That our oxen may  be well laden; That  there  be no breaking in or going out; That  there  be no outcry in our streets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and may our oxen be loaded down with produce. May there be no enemy breaking through our walls, no going into captivity, no cries of alarm in our town squares."
      }
    },
