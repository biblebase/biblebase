    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.74.13-14",
            "paragraphs": [
              "七四13,14神毀滅\"大魚\"（海怪）和\"鱷魚\"（參看伯三8的腳註），是詩歌裏常用來形容神在出埃及時得勝埃及人的比喻。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你曾打碎海怪的头， 把它丢给旷野的禽兽吃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你砸碎了利未亞坦的頭， 把牠給住在曠野的人作食物。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你压碎了水怪的头， 把它赐给旷野中的人和兽作食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你曾砸碎鱷魚的頭， 把牠給曠野的禽獸為食物。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You crushed the heads of Leviathan; you gave him as food for the creatures of the wilderness."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You crushed the heads of Leviathan; You gave him as food for the creatures of the wilderness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It was you who crushed the heads of Leviathan and gave it as food to the creatures of the desert."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You broke the heads of Leviathan in pieces, And gave him as food to the people inhabiting the wilderness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You crushed the heads of Leviathan and let the desert animals eat him."
      }
