    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊！ 你恩待你子民的时候， 求你别忘了我； 你拯救他们的时候， 求你帮助我，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你恩待你子民的時候， 求你記念我； 你拯救他們的時候，求你眷念我；"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华啊，你悦纳你子民的时候， 求你记念我，以你的救恩看顾我，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你用恩惠待你的百姓； 求你也用這恩惠記念我，開你的救恩眷顧我，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Remember me, O Lord, when you show favor to your people; help me when you save them,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Remember me, O Lord, in Your favor toward Your people; Visit me with Your salvation,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember me, Lord, when you show favor to your people, come to my aid when you save them,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Remember me, O Lord, with the favor You  have  toward Your people. Oh, visit me with Your salvation,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Remember me, Lord, when you show favor to your people; come near and rescue me."
      }
