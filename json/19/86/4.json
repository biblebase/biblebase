      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主啊，我的心仰望你， 求你赐我欢乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主啊！求你使你的僕人心裡歡喜， 因為我的心仰望你。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "主啊，求你使你仆人的灵魂欢喜， 因为我的灵魂仰望你！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主啊，求你使僕人心裏歡喜， 因為我的心仰望你。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Gladden the soul of your servant, for to you, O Lord, do I lift up my soul."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Make glad the soul of Your servant, For to You, O Lord, I lift up my soul."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Bring joy to your servant, Lord, for I put my trust in you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Rejoice the soul of Your servant, For to You, O Lord, I lift up my soul."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give me happiness, O Lord, for I give myself to you."
      }
