    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.87.4",
            "paragraphs": [
              "八七4\"拉哈伯\"代表埃及。參看以賽亞書五十一章9節的腳註。到了千禧年，所有人都要做王國的民（\"生在那裏\"）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我要把埃及人、巴比伦人、非利士人、泰尔人和古实人列为认识我的民族， 视他们为锡安人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“在認識我的人中，我提到拉哈伯和巴比倫， 看哪！還有非利士、推羅和古實，我說： ‘這一個是生在那裡的。’”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“在认识我的人中，我要提起拉哈伯和巴比伦； 看哪，还有非利士、提尔与库实， 我说：‘这一个出生在那里！’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要提起拉哈伯和巴比倫人， 是在認識我之中的； 看哪，非利士和泰爾並古實人， 個個生在那裏。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Among those who know me I mention Rahab and Babylon; behold, Philistia and Tyre, with Cush— “This one was born there,” they say."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I shall mention Rahab and Babylon among those who know Me; Behold, Philistia and Tyre with Ethiopia: ‘This one was born there.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will record Rahab and Babylon among those who acknowledge me— Philistia too, and Tyre, along with Cush— and will say, ‘This one was born in Zion.’ ”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“I will make mention of Rahab and Babylon to those who know Me; Behold, O Philistia and Tyre, with Ethiopia: ‘This one was born there.’ ”"
      },
      "nlt": {
