      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你已经看见我的困苦， 知道我心中的愁烦， 你的恩慈使我欢喜快乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要因你的慈愛歡喜快樂， 因為你看見了我的困苦， 知道我心中的痛苦；"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我要在你的慈爱中快乐欢喜， 因你看到我的苦难，知道我心中的患难。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要為你的慈愛高興歡喜； 因為你見過我的困苦，知道我心中的艱難。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I will rejoice and be glad in your steadfast love, because you have seen my affliction; you have known the distress of my soul,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will rejoice and be glad in Your lovingkindness, Because You have seen my affliction; You have known the troubles of my soul,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will be glad and rejoice in your love, for you saw my affliction and knew the anguish of my soul."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I will be glad and rejoice in Your mercy, For You have considered my trouble; You have known my soul in adversities,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will be glad and rejoice in your unfailing love, for you have seen my troubles, and you care about the anguish of my soul."
      }
