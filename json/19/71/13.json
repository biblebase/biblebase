    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿我的仇敌在羞辱中灭亡， 愿那些害我的人抱愧蒙羞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願那些控告我的， 都羞愧滅亡； 願那些謀求害我的， 都蒙羞受辱。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愿我的死敌，蒙羞灭绝； 愿那些谋求害我的，披戴辱骂和羞愧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願那與我性命為敵的，羞愧被滅； 願那謀害我的，受辱蒙羞。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "May my accusers be put to shame and consumed; with scorn and disgrace may they be covered who seek my hurt."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let those who are adversaries of my soul be ashamed and consumed; Let them be covered with reproach and dishonor, who seek to injure me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May my accusers perish in shame; may those who want to harm me be covered with scorn and disgrace."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Let them be confounded and consumed Who are adversaries of my life; Let them be covered with reproach and dishonor Who seek my hurt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Bring disgrace and destruction on my accusers. Humiliate and shame those who want to harm me."
      }
    },
