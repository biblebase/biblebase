    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "虽然我无法测度你的公义和拯救之恩， 但我要终日传扬你的作为。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的口要述說你的公義， 終日講述你的救恩， 儘管我不知道它們的數量。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我的口要终日讲述你的公义和你的拯救—— 其数无法数算。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的口終日要述說你的公義和你的救恩， 因我不計其數。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "My mouth will tell of your righteous acts, of your deeds of salvation all the day, for their number is past my knowledge."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My mouth shall tell of Your righteousness And of Your salvation all day long; For I do not know the sum of them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My mouth will tell of your righteous deeds, of your saving acts all day long— though I know not how to relate them all."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "My mouth shall tell of Your righteousness And Your salvation all the day, For I do not know their limits."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will tell everyone about your righteousness. All day long I will proclaim your saving power, though I am not skilled with words."
      }
    },
