    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你成为保护我的磐石， 让我可以随时投靠你。 求你下令救我， 因为你是我的磐石和堡垒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你作我避難的磐石，使我可以常來投靠； 你已經下令救我， 因為你是我的巖石、我的堅壘。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "求你作我安身的磐石， 让我可以随时来投靠你； 你已经命定要拯救我， 因为你是我的岩石、我的要塞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你作我常住的磐石； 你已經命定要救我， 因為你是我的巖石，我的山寨。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Be to me a rock of refuge, to which I may continually come; you have given the command to save me, for you are my rock and my fortress."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be to me a rock of habitation to which I may continually come; You have given commandment to save me, For You are my rock and my fortress."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be my rock of refuge, to which I can always go; give the command to save me, for you are my rock and my fortress."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Be my strong refuge, To which I may resort continually; You have given the commandment to save me, For You are my rock and my fortress."
      },
