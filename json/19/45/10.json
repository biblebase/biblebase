    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "女子啊， 要侧耳倾听：不要再挂念家乡的父老，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王所愛的女子啊！你要聽，要看，要留心地聽； 你要忘記你的本族和你的父家；"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "女儿啊，你要听、要看、要侧耳听—— 你当忘记你的民和你父家！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "女子啊，你要聽，要想，要側耳而聽！ 不要記念你的民和你的父家，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Hear, O daughter, and consider, and incline your ear: forget your people and your father’s house,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Listen, O daughter, give attention and incline your ear: Forget your people and your father’s house;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Listen, daughter, and pay careful attention: Forget your people and your father’s house."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Listen, O daughter, Consider and incline your ear; Forget your own people also, and your father’s house;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen to me, O royal daughter; take to heart what I say. Forget your people and your family far away."
      }
