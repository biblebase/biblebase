    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.45.8-9",
            "paragraphs": [
              "四五8,9描述王之婚禮的華麗。\"沒藥\"、\"沈香\"、\"肉桂\"。全都是香料。\"俄斐金飾\"。參看列王紀上九章28節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的衣袍散发出没药、沉香和肉桂的芬芳， 你陶醉在象牙宫的弦乐中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的衣服都有沒藥、沉香和肉桂的香氣； 從象牙宮裡有絲弦的樂聲，使你歡喜。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你所有的衣服都有没药、沉香、桂皮的香气， 在象牙宫殿里有丝弦乐器声使你欢喜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的衣服都有沒藥、沉香、肉桂的香氣； 象牙宮中有絲弦樂器的聲音使你歡喜。"
      },
