        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Your testimonies are very sure; Holiness adorns Your house, O Lord, forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your royal laws cannot be changed. Your reign, O Lord, is holy forever and ever."
      }
