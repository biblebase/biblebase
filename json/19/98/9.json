    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.98.9",
            "paragraphs": [
              "98:9",
              "他來要審判遍地：當我們的主基督再來審判全地(參啟19~22章)、除滅罪惡和憂傷、使天地更新的時候，這節經文所預言的就必實現。那時，整個受造界都要歡呼(8節)，因為當基督以公義和正直統管全地的時候，它也要蒙那最終的救贖(參賽55:12~13；羅8:19~22)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为祂要来审判大地， 要公义地审判世界， 公正地审判万民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "它們都要在耶和華面前歡呼， 因為他來是要審判全地， 他要按著公義審判世界， 憑著公正審判萬民。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "因为他要来审判大地， 他将以公义审判世界， 以正直审判万民。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他來要審判遍地。 他要按公義審判世界， 按公正審判萬民。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "before the Lord, for he comes to judge the earth. He will judge the world with righteousness, and the peoples with equity."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Before the Lord, for He is coming to judge the earth; He will judge the world with righteousness And the peoples with equity."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "let them sing before the Lord, for he comes to judge the earth. He will judge the world in righteousness and the peoples with equity."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For He is coming to judge the earth. With righteousness He shall judge the world, And the peoples with equity."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "before the Lord, for he is coming to judge the earth. He will judge the world with justice, and the nations with fairness."
      }
    },
