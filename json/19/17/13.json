    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你起来迎头击倒他们， 求你持刀救我脱离恶人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你起來，迎面攻擊他們，把他們打倒； 用你的刀救我的命脫離惡人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华啊，求你起来， 迎面抵挡他们，使他们屈服！ 求你用你的刀剑，挽救我的性命脱离恶人！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你起來，前去迎敵，將他打倒！ 用你的刀救護我命脫離惡人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Arise, O Lord! Confront him, subdue him! Deliver my soul from the wicked by your sword,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Arise, O Lord, confront him, bring him low; Deliver my soul from the wicked with Your sword,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Rise up, Lord, confront them, bring them down; with your sword rescue me from the wicked."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Arise, O Lord, Confront him, cast him down; Deliver my life from the wicked with Your sword,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Arise, O Lord! Stand against them, and bring them to their knees! Rescue me from the wicked with your sword!"
      }
    },
