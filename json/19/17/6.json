    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.17.6-9",
            "paragraphs": [
              "因慈愛而求（十七6～9）",
              "這段是向一位朋友和保護者直求，而不是將案件呈給法官。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊！我向你祈祷， 因为你必应允我。 求你垂听我的祷告。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！我向你呼求，因為你必應允我； 求你側耳聽我，垂聽我的禱告。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，我呼求你，因为你必回应我； 求你向我侧耳听，垂听我的言语！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，我曾求告你，因為你必應允我； 求你向我側耳，聽我的言語。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I call upon you, for you will answer me, O God; incline your ear to me; hear my words."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have called upon You, for You will answer me, O God; Incline Your ear to me, hear my speech."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I call on you, my God, for you will answer me; turn your ear to me and hear my prayer."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I have called upon You, for You will hear me, O God; Incline Your ear to me, and hear my speech."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am praying to you because I know you will answer, O God. Bend down and listen as I pray."
      }
    },
