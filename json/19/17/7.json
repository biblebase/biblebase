        "lang": "cht",
        "text": "求你把你的慈愛奇妙地彰顯， 用右手拯救那些投靠你的， 脫離那些起來攻擊他們的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "求你奇妙地显明你的慈爱； 拯救者啊，你用右手使投靠你的人脱离那起来的敌对者！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你顯出你奇妙的慈愛來； 你是那用右手拯救投靠你的脫離起來攻擊他們的人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Wondrously show your steadfast love, O Savior of those who seek refuge from their adversaries at your right hand."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Wondrously show Your lovingkindness, O Savior of those who take refuge at Your right hand From those who rise up against them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Show me the wonders of your great love, you who save by your right hand those who take refuge in you from their foes."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Show Your marvelous lovingkindness by Your right hand, O You who save those who trust in  You From those who rise up against  them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Show me your unfailing love in wonderful ways. By your mighty power you rescue those who seek refuge from their enemies."
      }
