    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.30.6-10",
            "paragraphs": [
              "愚人的自誇（三十6～10）",
              "平順的希伯來文字根出現的時候，總離不開安舒的環境和粗心隨便，參，如：耶利米書二十二21的不肯聽從，與箴言一32令人遭災的自滿自大；而箴言的底下一節，則說明了粗心隨便與真正無憂無慮的分別。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华啊， 我被毁灭、落入坟墓有何益处？ 我归于尘土，还能赞美你、 宣扬你的信实吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我被害流血，下到深坑，有甚麼益處呢？ 塵土還能稱讚你，還能傳揚你的信實嗎？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“我死亡下到阴坑，有什么好处呢？ 难道尘土会称谢你，宣告你的信实吗？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我被害流血，下到坑中，有甚麼益處呢？ 塵土豈能稱讚你，傳說你的誠實嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“What profit is there in my death, if I go down to the pit? Will the dust praise you? Will it tell of your faithfulness?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“What profit is there in my blood, if I go down to the pit? Will the dust praise You? Will it declare Your faithfulness?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“What is gained if I am silenced, if I go down to the pit? Will the dust praise you? Will it proclaim your faithfulness?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“What profit is  there in my blood, When I go down to the pit? Will the dust praise You? Will it declare Your truth?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What will you gain if I die, if I sink into the grave? Can my dust praise you? Can it tell of your faithfulness?"
      }
    },
