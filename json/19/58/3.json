    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.58.3-5",
            "paragraphs": [
              "五八3～5“一出母胎”。參看第五十一篇5節的腳註。由於天生的罪性，人類說謊，毒害別人，而且不聽任何懇求或呼籲（象蛇不順服弄蛇的人一樣）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人生下来就步入歧途， 一出母胎就离开正道， 谎话连篇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人一出母胎，就走上歧路； 他們一離母腹，就走偏了路，常說謊話。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "恶人一出母胎就背弃神， 说谎的人一离母腹就迷失。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人一出母胎就與神疏遠， 一離母腹便走錯路，說謊話。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The wicked are estranged from the womb; they go astray from birth, speaking lies."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wicked are estranged from the womb; These who speak lies go astray from birth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even from birth the wicked go astray; from the womb they are wayward, spreading lies."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The wicked are estranged from the womb; They go astray as soon as they are born, speaking lies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These wicked people are born sinners; even from birth they have lied and gone their own way."
      }
    },
