    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.58.3-5",
            "paragraphs": [
              "五八3～5“一出母胎”。參看第五十一篇5節的腳註。由於天生的罪性，人類說謊，毒害別人，而且不聽任何懇求或呼籲（象蛇不順服弄蛇的人一樣）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们像喷出毒液的毒蛇， 又像耳聋的眼镜蛇，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的毒氣好像蛇的毒氣， 又像耳朵塞住的聾虺，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们的毒液好像蛇的毒液， 他们如同耳聋的毒蛇，耳朵封闭了，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的毒氣好像蛇的毒氣； 他們好像塞耳的聾虺，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They have venom like the venom of a serpent, like the deaf adder that stops its ear,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have venom like the venom of a serpent; Like a deaf cobra that stops up its ear,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their venom is like the venom of a snake, like that of a cobra that has stopped its ears,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Their poison is like the poison of a serpent; They  are like the deaf cobra that stops its ear,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They spit venom like deadly snakes; they are like cobras that refuse to listen,"
      }
    },
