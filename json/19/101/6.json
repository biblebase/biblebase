    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要看顾国中忠于上帝的人， 让他们与我同住。 行事正直的人才能服侍我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的眼目必看顧國中的誠實人， 使他們與我同住； 行為正直的， 必要侍候我。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我的眼目向着地上忠实的人， 好让他们与我一起居住； 走在完全道路上的人，他必事奉我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我眼要看國中的誠實人，叫他們與我同住； 行為完全的，他要伺候我。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I will look with favor on the faithful in the land, that they may dwell with me; he who walks in the way that is blameless shall minister to me."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My eyes shall be upon the faithful of the land, that they may dwell with me; He who walks in a blameless way is the one who will minister to me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My eyes will be on the faithful in the land, that they may dwell with me; the one whose walk is blameless will minister to me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "My eyes shall  be on the faithful of the land, That they may dwell with me; He who walks in a perfect way, He shall serve me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will search for faithful people to be my companions. Only those who are above reproach will be allowed to serve me."
      }
    },
