    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "即使羊圈里的看羊人也披金戴银， 就像鸽子镀银的翅膀和金光闪闪的羽毛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們躺臥在羊圈中， 如同鴿子的翅膀鍍上白銀， 羽毛鍍上黃金。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "——就算你们的男子在羊圈中间睡觉—— 作战利品的鸽子翅膀上镀着银子， 它的羽翼镀着闪亮的金子！”"
      },
