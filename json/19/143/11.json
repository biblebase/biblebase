    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.143.11-12",
            "paragraphs": [
              "一四三11,12大衛祈求耶和華基於祂的\"名\"、祂的\"公義\"和祂的\"慈愛\"，保全他的性命（\"將我救活\"）。參看何西阿書二章19節的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你为自己名的缘故拯救我， 凭你的公义救我脱离患难。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你為你名的緣故使我存活， 按著你的公義把我從患難中領出來。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华啊，求你为你名的缘故使我存活； 藉着你的公义，把我的灵魂从患难中领出来；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你為你的名將我救活， 憑你的公義，將我從患難中領出來，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For your name’s sake, O Lord, preserve my life! In your righteousness bring my soul out of trouble!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the sake of Your name, O Lord, revive me. In Your righteousness bring my soul out of trouble."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For your name’s sake, Lord, preserve my life; in your righteousness, bring me out of trouble."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Revive me, O Lord, for Your name’s sake! For Your righteousness’ sake bring my soul out of trouble."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the glory of your name, O Lord, preserve my life. Because of your faithfulness, bring me out of this distress."
      }
    },
