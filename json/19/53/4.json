    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人吞吃我的百姓如同吃饭， 他们不求告上帝。 难道他们无知吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "作惡的都是無知的嗎？ 他們吞吃我的子民好像吃飯一樣， 並不求告　神。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "难道作恶的都不明白吗？ 他们吞噬我的子民像吃饭那样， 他们不呼求神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "作孽的沒有知識嗎？ 他們吞吃我的百姓如同吃飯一樣，並不求告神。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Have those who work evil no knowledge, who eat up my people as they eat bread, and do not call upon God?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Have the workers of wickedness no knowledge, Who eat up My people as though they ate bread And have not called upon God?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do all these evildoers know nothing? They devour my people as though eating bread; they never call on God."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Have the workers of iniquity no knowledge, Who eat up my people as they eat bread, And do not call upon God?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Will those who do evil never learn? They eat up my people like bread and wouldn’t think of praying to God."
      }
    },
