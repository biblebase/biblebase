    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.4.2",
            "paragraphs": [
              "2.這一節將a、b兩部分互相參照，可明其義。因為第2a節羞辱，是從2b節的“謬見”（JB：delusion）和“謊言”（和合：虛妄、虛假）而來。亦即，大衛的權柄遭到藐視，因為仇敵散播虛假的應許和謠言（神親身經歷這滋味，參，瑪一6。保羅亦然；加四16以下）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世人啊！你们把我的荣耀变为羞辱要到何时呢？ 你们追求虚谎之事要到何时呢？（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "尊貴的人啊！你們把我的榮耀變為羞辱，要到幾時呢？ 你們喜愛虛妄，追求虛謊，要到幾時呢？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "尊贵人哪，你们把我的荣耀变为羞辱， 要到什么时候呢？ 你们喜爱虚空、寻求谎言， 要到什么时候呢？细拉"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們這上流人哪，你們將我的尊榮變為羞辱要到幾時呢？ 你們喜愛虛妄，尋找虛假，要到幾時呢？（細拉）"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "O men, how long shall my honor be turned into shame? How long will you love vain words and seek after lies? Selah"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O sons of men, how long will my honor become a reproach? How long will you love what is worthless and aim at deception? Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How long will you people turn my glory into shame? How long will you love delusions and seek false gods?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "How long, O you sons of men, Will  you  turn my glory to shame? How  long will you love worthlessness And seek falsehood? Selah"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How long will you people ruin my reputation? How long will you make groundless accusations? How long will you continue your lies? Interlude"
      }
    },
