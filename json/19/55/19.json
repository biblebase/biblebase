    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "永掌王权的上帝必鉴察并惩罚他们， 因为他们顽梗悖逆、不敬畏上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神必聽見， 那從亙古坐著為王的必使他們受苦， 因他們不肯改變， 也不敬畏　神。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "从太初就坐着为王的神听见了，就苦待他们。细拉 这是因他们不悔改，也不敬畏神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那沒有更變、不敬畏神的人， 從太古常存的神必聽見而苦待他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "God will give ear and humble them, he who is enthroned from of old, Selah because they do not change and do not fear God."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God will hear and answer them— Even the one who sits enthroned from of old— Selah. With whom there is no change, And who do not fear God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God, who is enthroned from of old, who does not change— he will hear them and humble them, because they have no fear of God."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "God will hear, and afflict them, Even He who abides from of old. Selah Because they do not change, Therefore they do not fear God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God, who has ruled forever, will hear me and humble them. Interlude For my enemies refuse to change their ways; they do not fear God."
      }
    },
