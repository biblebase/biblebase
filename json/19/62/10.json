    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要敲诈勒索， 不要妄想靠偷盗发财， 即使财富增多，也不要倚靠它。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要倚靠藉強暴得來的財物， 也不要妄想倚靠搶奪回來的錢財； 如果財寶加增， 你的心也不要倚靠它。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你不要依靠欺压，也不要妄想抢夺； 即使财富增加，也不要放在心上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要仗勢欺人， 也不要因搶奪而驕傲； 若財寶加增，不要放在心上。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Put no trust in extortion; set no vain hopes on robbery; if riches increase, set not your heart on them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not trust in oppression And do not vainly hope in robbery; If riches increase, do not set your heart upon them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not trust in extortion or put vain hope in stolen goods; though your riches increase, do not set your heart on them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Do not trust in oppression, Nor vainly hope in robbery; If riches increase, Do not set your heart on  them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t make your living by extortion or put your hope in stealing. And if your wealth increases, don’t make it the center of your life."
      }
    },
