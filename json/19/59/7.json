    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们出口伤人，舌如利剑， 还说：“谁听得见？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！他們口中吐出惡言， 他們嘴裡吐出利刀， 他們心裡說：“有誰聽見呢？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "看哪，他们口中吐出恶言， 嘴唇间有刀剑！ 他们说：“谁能听见呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們口中噴吐惡言，嘴裏有刀； 他們說：有誰聽見？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "There they are, bellowing with their mouths with swords in their lips— for “Who,” they think, “will hear us?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, they belch forth with their mouth; Swords are in their lips, For, they say, “Who hears?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See what they spew from their mouths— the words from their lips are sharp as swords, and they think, “Who can hear us?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Indeed, they belch with their mouth; Swords are in their lips; For they  say,  “Who hears?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen to the filth that comes from their mouths; their words cut like swords. “After all, who can hear us?” they sneer."
      }
    },
