    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但我要歌颂你的能力， 在清晨颂扬你的慈爱， 因为你是我的堡垒， 是我患难时的避难所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於我，我要歌頌你的能力， 每天早晨我要向你的慈愛歡呼， 因為你作了我的高臺， 在患難的日子，作了我的避難所。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "至于我，我要歌唱你的力量， 我要在清晨颂唱你的慈爱； 因为你是我的庇护所， 是我危难日子里的避难所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我要歌頌你的力量， 早晨要高唱你的慈愛； 因為你作過我的高臺， 在我急難的日子作過我的避難所。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But I will sing of your strength; I will sing aloud of your steadfast love in the morning. For you have been to me a fortress and a refuge in the day of my distress."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, I shall sing of Your strength; Yes, I shall joyfully sing of Your lovingkindness in the morning, For You have been my stronghold And a refuge in the day of my distress."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I will sing of your strength, in the morning I will sing of your love; for you are my fortress, my refuge in times of trouble."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But I will sing of Your power; Yes, I will sing aloud of Your mercy in the morning; For You have been my defense And refuge in the day of my trouble."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But as for me, I will sing about your power. Each morning I will sing with joy about your unfailing love. For you have been my refuge, a place of safety when I am in distress."
      }
    },
