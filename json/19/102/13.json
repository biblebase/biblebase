    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你必怜悯锡安， 因为现在是你恩待她的时候了，时候到了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要起來，憐憫錫安， 因為現在是恩待錫安的時候； 所定的日期已經到了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你必起来怜悯锡安， 因为这是恩待她的时候， 又因为所定的时期已经来到。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必起來憐恤錫安， 因現在是可憐她的時候， 日期已經到了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You will arise and have pity on Zion; it is the time to favor her; the appointed time has come."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will arise and have compassion on Zion; For it is time to be gracious to her, For the appointed time has come."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will arise and have compassion on Zion, for it is time to show favor to her; the appointed time has come."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You will arise and have mercy on Zion; For the time to favor her, Yes, the set time, has come."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will arise and have mercy on Jerusalem— and now is the time to pity her, now is the time you promised to help."
      }
    },
