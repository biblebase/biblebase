    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝必永远毁灭你， 祂必抓住你， 把你从家里拉出来， 从活人之地铲除。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神必把你永遠拆毀； 他必把你挪去，把你從帳棚中揪出來， 把你從活人之地連根拔起。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "为此神要把你永久拆毁， 他会抓住你，把你从帐篷里拖出来， 把你从活人之地连根拔出。细拉"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神也要毀滅你，直到永遠； 他要把你拿去，從你的帳棚中抽出， 從活人之地將你拔出。（細拉）"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But God will break you down forever; he will snatch and tear you from your tent; he will uproot you from the land of the living. Selah"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But God will break you down forever; He will snatch you up and tear you away from your tent, And uproot you from the land of the living. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely God will bring you down to everlasting ruin: He will snatch you up and pluck you from your tent; he will uproot you from the land of the living."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "God shall likewise destroy you forever; He shall take you away, and pluck you out of your dwelling place, And uproot you from the land of the living. Selah"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But God will strike you down once and for all. He will pull you from your home and uproot you from the land of the living. Interlude"
      }
    },
