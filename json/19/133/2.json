    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这就像珍贵的膏油倒在亚伦头上， 流到他的胡须， 又流到他的衣襟；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這好比那珍貴的油澆在亞倫的頭上， 流到鬍鬚， 又從亞倫的鬍鬚， 流到他的衣領上。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这就像浇在亚伦头上贵重的膏油， 流到他的胡须，又流到他的衣襟；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這好比那貴重的油澆在亞倫的頭上， 流到鬍鬚，又流到他的衣襟；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "It is like the precious oil on the head, running down on the beard, on the beard of Aaron, running down on the collar of his robes!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is like the precious oil upon the head, Coming down upon the beard, Even Aaron’s beard, Coming down upon the edge of his robes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is like precious oil poured on the head, running down on the beard, running down on Aaron’s beard, down on the collar of his robe."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "It  is like the precious oil upon the head, Running down on the beard, The beard of Aaron, Running down on the edge of his garments."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For harmony is as precious as the anointing oil that was poured over Aaron’s head, that ran down his beard and onto the border of his robe."
      }
