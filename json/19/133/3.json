    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又像黑门的甘露降在锡安山上。 那里有耶和华所赐的福气， 就是永远的生命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又好比黑門的甘露 落在錫安的眾山上， 在那裡有耶和華命定的福， 就是永遠的生命。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "又像黑门的甘露降在锡安的众山上， 因为在那里，耶和华命定了祝福， 就是永远的生命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又好比黑門的甘露降在錫安山； 因為在那裏有耶和華所命定的福， 就是永遠的生命。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "It is like the dew of Hermon, which falls on the mountains of Zion! For there the Lord  has commanded the blessing, life forevermore."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is like the dew of Hermon Coming down upon the mountains of Zion; For there the Lord commanded the blessing—life forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is as if the dew of Hermon were falling on Mount Zion. For there the Lord bestows his blessing, even life forevermore."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "It  is like the dew of Hermon, Descending upon the mountains of Zion; For there the Lord commanded the blessing— Life forevermore."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Harmony is as refreshing as the dew from Mount Hermon that falls on the mountains of Zion. And there the Lord has pronounced his blessing, even life everlasting."
      }
    },
