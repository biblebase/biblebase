    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.9",
            "paragraphs": [
              "七八9\"轉身退後\"。不是指在某次戰役當中懦弱的行為，而是他們象懦弱的人一樣，放棄遵從神的律法（10,11節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲人备上了弓箭， 却临阵逃命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮的子孫，雖然備有弓箭， 在爭戰的日子，卻轉身逃走。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "以法莲的子孙是装备好的弓箭手， 但他们在战斗之日转身而逃。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮的子孫帶着兵器，拿着弓， 臨陣之日轉身退後。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The Ephraimites, armed with the bow, turned back on the day of battle."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sons of Ephraim were archers equipped with bows, Yet they turned back in the day of battle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The men of Ephraim, though armed with bows, turned back on the day of battle;"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The children of Ephraim, being armed and carrying bows, Turned back in the day of battle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The warriors of Ephraim, though armed with bows, turned their backs and fled on the day of battle."
      }
    },
