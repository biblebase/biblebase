        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们忘记了祂的大能， 忘记了祂救他们脱离压迫的日子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們忘記了他的能力， 就是他救贖他們脫離敵人的日子，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们不记念他手的能力， 不记念他救赎他们脱离敌人的日子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們不追念他的能力 和贖他們脫離敵人的日子；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They did not remember his power or the day when he redeemed them from the foe,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They did not remember His power, The day when He redeemed them from the adversary,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They did not remember his power— the day he redeemed them from the oppressor,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They did not remember His power: The day when He redeemed them from the enemy,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They did not remember his power and how he rescued them from their enemies."
      }
