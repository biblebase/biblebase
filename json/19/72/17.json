    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他的名流芳百世，如日长存。 愿万国都因他而蒙福， 都称颂他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願他的名永遠常存， 願他的名延續像太陽的恆久； 願萬人都因他蒙福， 願萬國都稱他為有福的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愿他的名存到永远， 如太阳长久； 愿人们因他蒙福， 愿所有的国家都称他有福。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的名要存到永遠， 要留傳如日之久。 人要因他蒙福； 萬國要稱他有福。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "May his name endure forever, his fame continue as long as the sun! May people be blessed in him, all nations call him blessed!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May his name endure forever; May his name increase as long as the sun shines; And let men bless themselves by him; Let all nations call him blessed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May his name endure forever; may it continue as long as the sun. Then all nations will be blessed through him, and they will call him blessed."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "His name shall endure forever; His name shall continue as long as the sun. And men shall be blessed in Him; All nations shall call Him blessed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May the king’s name endure forever; may it continue as long as the sun shines. May all nations be blessed through him and bring him praise."
      }
    },
