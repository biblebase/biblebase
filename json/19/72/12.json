    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贫穷人呼求，他便搭救； 困苦人无助，他就伸出援手。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為貧窮人呼求的時候，他就搭救； 沒有人幫助的困苦人，他也搭救。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他解救那呼救的贫穷人， 以及那无人帮助的困苦人；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，窮乏人呼求的時候，他要搭救； 沒有人幫助的困苦人，他也要搭救。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For he delivers the needy when he calls, the poor and him who has no helper."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For he will deliver the needy when he cries for help, The afflicted also, and him who has no helper."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For he will deliver the needy who cry out, the afflicted who have no one to help."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For He will deliver the needy when he cries, The poor also, and him who has no helper."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will rescue the poor when they cry to him; he will help the oppressed, who have no one to defend them."
      }
    },
