    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的道路穿越海洋，经过洪涛， 但你的足迹无人看见。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的道路經過海洋， 你的路徑穿過大水， 但你的腳蹤無人知道。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你的道路在海洋中， 你的路途在众水上， 但你的脚踪没有人知道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的道在海中； 你的路在大水中； 你的腳蹤無人知道。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Your way was through the sea, your path through the great waters; yet your footprints were unseen."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your way was in the sea And Your paths in the mighty waters, And Your footprints may not be known."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your path led through the sea, your way through the mighty waters, though your footprints were not seen."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Your way was in the sea, Your path in the great waters, And Your footsteps were not known."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your road led through the sea, your pathway through the mighty waters— a pathway no one knew was there!"
      }
