    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道祂的慈爱永远消逝了吗？ 祂的应许永远落空了吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的慈愛永遠消失， 他的應許永久廢去嗎？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "难道他的慈爱永久消逝， 他的言语断绝，直到万代吗？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "難道他的慈愛永遠窮盡， 他的應許世世廢棄嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Has his steadfast love forever ceased? Are his promises at an end for all time?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Has His lovingkindness ceased forever? Has His promise come to an end forever?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Has his unfailing love vanished forever? Has his promise failed for all time?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Has His mercy ceased forever? Has His  promise failed forevermore?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Is his unfailing love gone forever? Have his promises permanently failed?"
      }
    },
