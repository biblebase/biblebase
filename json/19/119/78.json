    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿狂傲人受辱， 因他们诋毁我； 但我要思想你的法则。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願傲慢人蒙羞，因為他們以詭詐誣衊我； 至於我，我卻要默想你的訓詞。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愿傲慢人蒙羞，因为他们以虚谎屈枉我； 但我要默想你的训诫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願驕傲人蒙羞，因為他們無理地傾覆我； 但我要思想你的訓詞。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Let the insolent be put to shame, because they have wronged me with falsehood; as for me, I will meditate on your precepts."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May the arrogant be ashamed, for they subvert me with a lie; But I shall meditate on Your precepts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the arrogant be put to shame for wronging me without cause; but I will meditate on your precepts."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Let the proud be ashamed, For they treated me wrongfully with falsehood; But I will meditate on Your precepts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Bring disgrace upon the arrogant people who lied about me; meanwhile, I will concentrate on your commandments."
      }
    },
