        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "求你为仆人的福份作保， 不要让傲慢人欺压我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你為僕人作保，使我得好處， 不容驕傲人欺壓我！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Give your servant a pledge of good; let not the insolent oppress me."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be surety for Your servant for good; Do not let the arrogant oppress me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ensure your servant’s well-being; do not let the arrogant oppress me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Be surety for Your servant for good; Do not let the proud oppress me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Please guarantee a blessing for me. Don’t let the arrogant oppress me!"
      }
    },
