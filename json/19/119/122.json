      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你保障仆人的福祉， 不要让傲慢的人欺压我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
