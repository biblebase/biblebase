    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.113-120",
            "paragraphs": [
              "一一九113～120詩人懼怕神的審判。"
            ]
          },
          {
            "verse": "psa.119.119",
            "paragraphs": [
              "一一九119\"渣滓\"。從熔化的金屬除去的熔渣。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你铲除世上的恶人， 如同除掉渣滓， 因此我喜爱你的法度。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "世上所有的惡人，你都把他們除滅，如同除去渣滓， 因此我喜愛你的法度。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你把地上所有的恶人像渣滓那样除掉， 因此我爱你的法度。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡地上的惡人，你除掉他，好像除掉渣滓； 因此我愛你的法度。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "All the wicked of the earth you discard like dross, therefore I love your testimonies."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have removed all the wicked of the earth like dross; Therefore I love Your testimonies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the wicked of the earth you discard like dross; therefore I love your statutes."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You put away all the wicked of the earth like dross; Therefore I love Your testimonies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You skim off the wicked of the earth like scum; no wonder I love to obey your laws!"
      }
    },
