    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 我们亲耳听过祖先讲述你在古时， 在我们祖先时代的作为。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！你在古時， 在我們列祖的日子所作的事， 我們親耳聽見了， 我們的列祖也給我們述說過。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，你在古时，在我们列祖的日子里所做的事， 我们亲耳听见了，我们的祖先也向我们讲述过。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你在古時， 我們列祖的日子所行的事， 我們親耳聽見了； 我們的列祖也給我們述說過。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "O God, we have heard with our ears, our fathers have told us, what deeds you performed in their days, in the days of old:"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O God, we have heard with our ears, Our fathers have told us The work that You did in their days, In the days of old."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We have heard it with our ears, O God; our ancestors have told us what you did in their days, in days long ago."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "We have heard with our ears, O God, Our fathers have told us, The deeds You did in their days, In days of old:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O God, we have heard it with our own ears— our ancestors have told us of all you did in their day, in days long ago:"
      }
