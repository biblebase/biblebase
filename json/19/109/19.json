    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿咒诅不离其身， 如同身上的衣服和腰间的带子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願這咒詛像衣服一般給他披上， 並當作他常常束上的腰帶。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愿诅咒如同他披戴在身上的衣服， 又如他常常束上的腰带。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願這咒罵當他遮身的衣服， 當他常束的腰帶！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "May it be like a garment that he wraps around him, like a belt that he puts on every day!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let it be to him as a garment with which he covers himself, And for a belt with which he constantly girds himself."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May it be like a cloak wrapped about him, like a belt tied forever around him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Let it be to him like the garment which covers him, And for a belt with which he girds himself continually."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now may his curses return and cling to him like clothing; may they be tied around him like a belt.”"
      }
