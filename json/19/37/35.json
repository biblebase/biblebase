    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.37.27-40",
            "paragraphs": [
              "前瞻（三十七27～40）",
              "雖然在本詩中，這不算是新觀點，但在本段內它卻特別突顯，首先是永遠的重復出現，在第27、28（強調語）、29節（同義字）；然後是35、36節的小比方；最後則是藉37、38節所強調的\\cs9後裔表明。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.37.35",
            "paragraphs": [
              "三七35惡人看似不能毀壞，\"好象一根青翠樹在本土生發\"。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我见过邪恶残暴之人， 乍看如葱茏的黎巴嫩香柏树，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我曾看見強暴的惡人興旺， 像樹木在本土茂盛。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我曾看到恶人的强横， 像本地青翠的树那样蔓延；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我見過惡人大有勢力， 好像一棵青翠樹在本土生發。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I have seen a wicked, ruthless man, spreading himself like a green laurel tree."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have seen a wicked, violent man Spreading himself like a luxuriant tree in its native soil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have seen a wicked and ruthless man flourishing like a luxuriant native tree,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I have seen the wicked in great power, And spreading himself like a native green tree."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have seen wicked and ruthless people flourishing like a tree in its native soil."
      }
    },
