    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.37.12-26",
            "paragraphs": [
              "隱藏的幫助（三十七12～26）",
              "到目前為止，戰場乃在信徒心中，他們因惡人的明目張膽而激忿不平。現在詩人從外面觀察這兩種人，比較他們的道路與結局。直到本詩的末尾，幾乎每一節都講到義人或惡人，或類似用語。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们即使失脚，也不会跌倒， 因为耶和华的手扶持他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他雖然跌跤，卻不至仆倒； 因為耶和華用手扶持他。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他即使仆倒，也不被抛弃， 因为耶和华扶助他的手。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他雖失腳也不致全身仆倒， 因為耶和華用手攙扶他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "though he fall, he shall not be cast headlong, for the Lord  upholds his hand."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When he falls, he will not be hurled headlong, Because the Lord is the One who holds his hand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "though he may stumble, he will not fall, for the Lord upholds him with his hand."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Though he fall, he shall not be utterly cast down; For the Lord upholds him  with His hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Though they stumble, they will never fall, for the Lord holds them by the hand."
      }
    },
