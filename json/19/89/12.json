        "text": "南北為你所創造； 他泊和黑門都因你的名歡呼。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The north and the south, you have created them; Tabor and Hermon joyously praise your name."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The north and the south, You have created them; Tabor and Hermon shout for joy at Your name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You created the north and the south; Tabor and Hermon sing for joy at your name."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The north and the south, You have created them; Tabor and Hermon rejoice in Your name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You created north and south. Mount Tabor and Mount Hermon praise your name."
      }
