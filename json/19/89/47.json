    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿你顾念我的生命何其短暂！ 你创造的世人何其虚幻！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你記念我的一生多麼短暫， 你創造的世人是多麼虛幻呢！"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "求你记念我的一生是多么短暂， 你创造的所有世人是多么虚妄！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你想念我的時候是何等的短少； 你創造世人，要使他們歸何等的虛空呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Remember how short my time is! For what vanity you have created all the children of man!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Remember what my span of life is; For what vanity You have created all the sons of men!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember how fleeting is my life. For what futility you have created all humanity!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Remember how short my time is; For what futility have You created all the children of men?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Remember how short my life is, how empty and futile this human existence!"
      }
    },
