    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.60.1",
            "paragraphs": [
              "60:1",
              "1~12你丟棄了我們……你向我們發怒：大衛承認，神的百姓在戰鬥中被擊敗乃是因為他們不討神的喜悅，也不遵行他的旨意，因此神收回了他的幫助和保護。以色列人擺脫這種困境的惟一途徑就是迫切地禱告並尋求神，重新堅立敬畏神的心(4節)。同樣，我們這些新約信徒也會因為犯罪、驕傲、效法周圍的社會或背棄公義的聖經真理教訓而幹犯神的靈，以至於我們在教會、家庭以及個人生活中不斷遭遇屬靈的失敗(參“基督給7個教會的信息”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 你遗弃了我们， 使我们一败涂地； 你曾向我们发怒， 求你现在复兴我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！你丟棄了我們，擊碎了我們； 你曾向我們發怒，現在求你復興我們。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，你抛弃了我们，毁坏了我们； 你曾经发怒，现在求你复兴我们。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你丟棄了我們，使我們破敗； 你向我們發怒，求你使我們復興！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "O God, you have rejected us, broken our defenses; you have been angry; oh, restore us."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O God, You have rejected us. You have broken us; You have been angry; O, restore us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have rejected us, God, and burst upon us; you have been angry—now restore us!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "O God, You have cast us off; You have broken us down; You have been displeased; Oh, restore us again!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have rejected us, O God, and broken our defenses. You have been angry with us; now restore us to your favor."
      }
    },
