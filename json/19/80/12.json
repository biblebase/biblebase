      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你為何拆毀這樹的籬笆， 任憑一切過路的人摘取？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Why then have you broken down its walls, so that all who pass along the way pluck its fruit?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why have You broken down its hedges, So that all who pass that way pick its fruit?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why have you broken down its walls so that all who pass by pick its grapes?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Why have You broken down her hedges, So that all who pass by the way pluck her fruit?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But now, why have you broken down our walls so that all who pass by may steal our fruit?"
      }
