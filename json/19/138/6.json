    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华虽在高天之上， 仍看顾卑微的人， 祂对骄傲的人也了如指掌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華雖然高高在上，仍然看顧卑微的人； 他卻從遠處認出驕傲的人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华虽然是至高的，却看顾卑微的人； 他从远处就认出高傲的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華雖高，仍看顧低微的人； 他卻從遠處看出驕傲的人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For though the Lord is high, he regards the lowly, but the haughty he knows from afar."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For though the Lord is exalted, Yet He regards the lowly, But the haughty He knows from afar."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Though the Lord is exalted, he looks kindly on the lowly; though lofty, he sees them from afar."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Though the Lord  is on high, Yet He regards the lowly; But the proud He knows from afar."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Though the Lord is great, he cares for the humble, but he keeps his distance from the proud."
      }
    },
