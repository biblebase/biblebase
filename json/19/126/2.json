    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们笑声不止，欢呼歌唱。 列邦都说： “耶和华为他们成就了伟大的事。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，我們滿口喜笑， 滿舌歡呼； 那時列國中有人說： “耶和華為他們行了大事。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "那时我们的口充满欢笑， 我们的舌头充满欢呼； 人们在列国中说： “耶和华为他们行了大事！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們滿口喜笑、 滿舌歡呼的時候， 外邦中就有人說： 耶和華為他們行了大事！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then our mouth was filled with laughter, and our tongue with shouts of joy; then they said among the nations, “The Lord has done great things for them.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then our mouth was filled with laughter And our tongue with joyful shouting; Then they said among the nations, “The Lord has done great things for them.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Our mouths were filled with laughter, our tongues with songs of joy. Then it was said among the nations, “The Lord has done great things for them.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then our mouth was filled with laughter, And our tongue with singing. Then they said among the nations, “The Lord has done great things for them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We were filled with laughter, and we sang for joy. And the other nations said, “What amazing things the Lord has done for them.”"
      }
    },
