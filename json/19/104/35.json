    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿罪人从地上消逝， 愿恶人荡然无存。 我的心啊，要称颂耶和华！ 你们要赞美耶和华！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願罪人從世上滅絕， 也不再有惡人存在。 我的心哪！你要稱頌耶和華。 你們要讚美耶和華。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "愿罪人从地上被灭尽， 不再有恶人！ 我的灵魂哪，你当颂赞耶和华！ 哈利路亚！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願罪人從世上消滅！ 願惡人歸於無有！ 我的心哪，要稱頌耶和華！ 你們要讚美耶和華！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Let sinners be consumed from the earth, and let the wicked be no more! Bless the Lord, O my soul! Praise the Lord!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let sinners be consumed from the earth And let the wicked be no more. Bless the Lord, O my soul. Praise the Lord!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But may sinners vanish from the earth and the wicked be no more. Praise the Lord, my soul. Praise the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "May sinners be consumed from the earth, And the wicked be no more. Bless the Lord, O my soul! Praise the Lord!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let all sinners vanish from the face of the earth; let the wicked disappear forever. Let all that I am praise the Lord. Praise the Lord!"
      }
    },
