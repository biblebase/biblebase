        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂从天上的楼阁降雨在山间， 大地因祂的作为而丰美富饶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你從自己的樓閣中澆灌群山， 因你所作的事的果效，大地就豐足。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你从你的楼阁浇灌群山； 因你作工的成果，大地就饱足。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他從樓閣中澆灌山嶺； 因他作為的功效，地就豐足。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "From your lofty abode you water the mountains; the earth is satisfied with the fruit of your work."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He waters the mountains from His upper chambers; The earth is satisfied with the fruit of His works."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He waters the mountains from his upper chambers; the land is satisfied by the fruit of his work."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He waters the hills from His upper chambers; The earth is satisfied with the fruit of Your works."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You send rain on the mountains from your heavenly home, and you fill the earth with the fruit of your labor."
      }
    },
