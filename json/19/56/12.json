    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.56.12",
            "paragraphs": [
              "五六12\"感謝祭\"。可以是實在的獻祭（參看利七12～17的腳註）或獻上贊美之歌（詩二六7），或兩者同時獻上，如在這裏。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 我要恪守向你发的誓言， 献上感恩祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！我要償還我向你所許的願， 我要把感謝祭獻給你。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，我向你所许的愿在我身上， 我要用感谢祭向你偿还！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，我向你所許的願在我身上； 我要將感謝祭獻給你。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I must perform my vows to you, O God; I will render thank offerings to you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your vows are binding upon me, O God; I will render thank offerings to You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
