    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们如蜜蜂围住我， 然而他们必像燃烧的荆棘， 转瞬消逝； 我靠着耶和华必消灭他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們如同蜜蜂圍繞著我， 但他們要像燒荊棘的火熄滅； 我靠著耶和華的名必除滅他們。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们像蜂群那样围绕我， 却像荆棘火那样被熄灭； 我靠耶和华的名必要割除他们！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們如同蜂子圍繞我， 好像燒荊棘的火，必被熄滅； 我靠耶和華的名，必剿滅他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They surrounded me like bees; they went out like a fire among thorns; in the name of the Lord I cut them off!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They surrounded me like bees; They were extinguished as a fire of thorns; In the name of the Lord I will surely cut them off."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They swarmed around me like bees, but they were consumed as quickly as burning thorns; in the name of the Lord I cut them down."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They surrounded me like bees; They were quenched like a fire of thorns; For in the name of the Lord I will destroy them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They swarmed around me like bees; they blazed against me like a crackling fire. But I destroyed them all with the authority of the Lord."
      }
    },
