    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.118.6",
            "paragraphs": [
              "118:6",
              "有耶和華幫助我：凡以耶和華神為他們的避難所的人都確知他常與他們同在，做他們的幫助和力量(7,14節；書1:9；耶1:8；參出3:14註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有耶和华与我同在， 我必不惧怕， 人能把我怎么样？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華是我的幫助，我決不害怕， 人能把我怎麼樣呢？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华在我这边，我就不害怕； 人能对我做什么呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有耶和華幫助我，我必不懼怕， 人能把我怎麼樣呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The Lord is on my side; I will not fear. What can man do to me?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord is for me; I will not fear; What can man do to me?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord is with me; I will not be afraid. What can mere mortals do to me?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The Lord  is on my side; I will not fear. What can man do to me?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord is for me, so I will have no fear. What can mere people do to me?"
      }
