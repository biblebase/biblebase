    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华保护寄居异地的人， 看顾孤儿寡妇， 祂挫败恶人的阴谋诡计。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華保護寄居的， 扶持孤兒寡婦， 卻使惡人的行動挫敗。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华保护寄居者，扶助孤儿和寡妇， 却使恶人的道路弯曲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華保護寄居的， 扶持孤兒和寡婦， 卻使惡人的道路彎曲。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The Lord watches over the sojourners; he upholds the widow and the fatherless, but the way of the wicked he brings to ruin."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord protects the strangers; He supports the fatherless and the widow, But He thwarts the way of the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord watches over the foreigner and sustains the fatherless and the widow, but he frustrates the ways of the wicked."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The Lord watches over the strangers; He relieves the fatherless and widow; But the way of the wicked He turns upside down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord protects the foreigners among us. He cares for the orphans and widows, but he frustrates the plans of the wicked."
      }
    },
