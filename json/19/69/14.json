      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "求你解救我脱离泥泞，不要让我陷下去； 解救我脱离恨我的人，脱离众水的深处。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你搭救我出離淤泥， 不叫我陷在其中； 求你使我脫離那些恨我的人， 使我出離深水。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Deliver me from sinking in the mire; let me be delivered from my enemies and from the deep waters."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Deliver me from the mire and do not let me sink; May I be delivered from my foes and from the deep waters."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Rescue me from the mire, do not let me sink; deliver me from those who hate me, from the deep waters."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Deliver me out of the mire, And let me not sink; Let me be delivered from those who hate me, And out of the deep waters."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rescue me from the mud; don’t let me sink any deeper! Save me from those who hate me, and pull me from these deep waters."
      }
