    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的灵往哪里去，他们也往哪里去，移动时不用转身。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠們都是朝著臉孔的方向行走。靈往哪裡去，牠們就往哪裡去。牠們走動的時候，都不必轉身。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們俱各直往前行。靈往哪裏去，他們就往那裏去，行走並不轉身。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And each went straight forward. Wherever the spirit would go, they went, without turning as they went."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And each went straight forward; wherever the spirit was about to go, they would go, without turning as they went."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Each one went straight ahead. Wherever the spirit would go, they would go, without turning as they went."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And each one went straight forward; they went wherever the spirit wanted to go, and they did not turn when they went."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They went in whatever direction the spirit chose, and they moved straight forward in any direction without turning around."
      }
