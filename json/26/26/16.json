    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "ezk.26.15-18",
            "paragraphs": [
              "15～18.海上王子的狼狽。鄰近各國的統治者倚賴推羅的商業維持繁盛，如今推羅傾倒，他們不禁深感泄氣，而且驚駭如此大有勢力的君王就此毀滅。那些海島事實上是沿海地帶（呂譯、RSV），亦即地中海沿岸的眾多小城邦。他們的王子要坐下舉哀，唱首哀歌（qînâ）。這是傳統3：2節韻的挽歌，在RSV中翻譯的很好。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "ezk.26.15-16",
            "paragraphs": [
              "二六15,16\"靠海的君王\"。推羅的商業貿易夥伴，他們因推羅的陷落而哀悼。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "沿海诸王因你而恐惧，都从宝座上下来，除去王袍，脱掉锦衣，坐在地上发抖。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時所有沿海的君王都必由王座下來，除去朝服，脫下刺繡的衣服；披上戰兢，坐在地上，不停發抖，因你而驚駭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時靠海的君王必都下位，除去朝服，脫下花衣，披上戰兢，坐在地上，時刻發抖，為你驚駭。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then all the princes of the sea will step down from their thrones and remove their robes and strip off their embroidered garments. They will clothe themselves with trembling; they will sit on the ground and tremble every moment and be appalled at you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then all the princes of the sea will go down from their thrones, remove their robes and strip off their embroidered garments. They will clothe themselves with trembling; they will sit on the ground, tremble every moment and be appalled at you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then all the princes of the coast will step down from their thrones and lay aside their robes and take off their embroidered garments. Clothed with terror, they will sit on the ground, trembling every moment, appalled at you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then all the princes of the sea will come down from their thrones, lay aside their robes, and take off their embroidered garments; they will clothe themselves with trembling; they will sit on the ground, tremble every moment, and be astonished at you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the seaport rulers will step down from their thrones and take off their royal robes and beautiful clothing. They will sit on the ground trembling with horror at your destruction."
      }
    },
