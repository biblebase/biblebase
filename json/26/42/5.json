    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.42.1-14",
            "paragraphs": [
              "四二1～14聖殿內院北面有兩座三層高的建築物，一座的長度是另一座的兩倍，是祭司用作吃祭物（四六20）和存放聖衣（四四19）的地方。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上层的厢房比下层和中层的窄，因为上层走廊占去较大的面积。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "上層的房子最窄，因為上層的走廊所佔的地方，比下層和中層的為多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "聖屋因為樓廊佔去些地方，所以上層比中下兩層窄些。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now the upper chambers were narrower, for the galleries took more away from them than from the lower and middle chambers of the building."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the upper chambers were smaller because the galleries took more space away from them than from the lower and middle ones in the building."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the upper rooms were narrower, for the galleries took more space from them than from the rooms on the lower and middle floors of the building."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now the upper chambers were shorter, because the galleries took away space from them more than from the lower and middle stories of the building."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each of the two upper levels of rooms was narrower than the one beneath it because the upper levels had to allow space for walkways in front of them."
