    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那人带我出了圣殿，向北来到外院的厢房，一排在空地对面，一排在北边的建筑物对面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他領我出來，向著北面的外院去；帶我到了祭司的房子。這些房子一排對著殿院的空地，一排對著北面的建築物。"
      },
