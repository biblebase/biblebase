      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“我把以色列人驱逐到各国的时候，他们也要这样吃不洁净的食物。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：“以色列人在我趕他們去的列國中，也要這樣吃不潔淨的食物。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「以色列人在我所趕他們到的各國中，也必這樣吃不潔淨的食物。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the Lord said, “Thus shall the people of Israel eat their bread unclean, among the nations where I will drive them.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord said, “Thus will the sons of Israel eat their bread unclean among the nations where I will banish them.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said, “In this way the people of Israel will eat defiled food among the nations where I will drive them.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then the Lord said, “So shall the children of Israel eat their defiled bread among the Gentiles, where I will drive them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord said, “This is how Israel will eat defiled bread in the Gentile lands to which I will banish them!”"
      }
    },
