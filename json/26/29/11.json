      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Neither foot of man shall pass through it nor foot of beast pass through it, and it shall be uninhabited forty years."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For forty years not a soul will pass that way, neither people nor animals. It will be completely uninhabited."
      }
