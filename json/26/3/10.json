    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂又说：“人子啊，你要留心听我说的一切话，牢记在心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又對我說：“人子啊！我對你說的一切話，你都要留心聽，記在心裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他又對我說：「人子啊，我對你所說的一切話，要心裏領會，耳中聽聞。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Moreover, he said to me, “Son of man, all my words that I shall speak to you receive in your heart, and hear with your ears."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, He said to me, “Son of man, take into your heart all My words which I will speak to you and listen closely."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he said to me, “Son of man, listen carefully and take to heart all the words I speak to you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Moreover He said to me: “Son of man, receive into your heart all My words that I speak to you, and hear with your ears."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he added, “Son of man, let all my words sink deep into your own heart first. Listen to them carefully for yourself."
      }
