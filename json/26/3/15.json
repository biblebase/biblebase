    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我来到住在提勒·亚毕的被掳的人中，在迦巴鲁河边。我感到无比震惊，在他们中间坐了七天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是我來到提勒．亞畢被擄的人那裡，他們住在迦巴魯河邊。我在他們居住的地方坐了七天，在他們中間震驚得不知所措。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我就來到提勒‧亞畢，住在迦巴魯河邊被擄的人那裏，到他們所住的地方，在他們中間憂憂悶悶地坐了七日。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And I came to the exiles at Tel-abib, who were dwelling by the Chebar canal, and I sat where they were dwelling. And I sat there overwhelmed among them seven days."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I came to the exiles who lived beside the river Chebar at Tel-abib, and I sat there seven days where they were living, causing consternation among them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I came to the exiles who lived at Tel Aviv near the Kebar River. And there, where they were living, I sat among them for seven days—deeply distressed."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then I came to the captives at Tel Abib, who dwelt by the River Chebar; and I sat where they sat, and remained there astonished among them seven days."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I came to the colony of Judean exiles in Tel-abib, beside the Kebar River. I was overwhelmed and sat among them for seven days."
      }
