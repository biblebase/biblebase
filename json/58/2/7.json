    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使他暂时比天使低微一点， 赐他荣耀和尊贵作冠冕， 派他管理你所造的一切，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使他暫時成了比天使卑微， 卻賜給他榮耀尊貴作冠冕，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你使他暂时比天使低微    ； 你赐他荣耀和尊贵为冠冕；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你叫他比天使微小一點， 賜他榮耀尊貴為冠冕， 並將你手所造的都派他管理，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You made him for a little while lower than the angels; you have crowned him with glory and honor,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have made him for a little while lower than the angels; You have crowned him with glory and honor, And have appointed him over the works of Your hands;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You made them a little lower than the angels; you crowned them with glory and honor"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You have made him a little lower than the angels; You have crowned him with glory and honor, And set him over the works of Your hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet for a little while you made them a little lower than the angels and crowned them with glory and honor."
      }
