    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.13.33-34",
            "paragraphs": [
              "(3)耶羅波安的罪（十三33～34）",
              "這段經文記載他的“惡行”（參十二30～33），雖然經過上述重大事件及警告（“這事以後”），卻依然故我，不肯悔改。“這事叫耶羅波安的家陷在罪裏”（JB；此處的“罪”乃贖罪祭，h]at]t]a{~t[之意）。這可能是罔顧警告而犯下致死之罪的舊約例子（參：約壹五16）。耶羅波安的家族在他兒子死後也未幸存（十五29），但他的罪惡卻從未被人遺忘。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这罪使耶罗波安家走向没落，最终彻底灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這事成為耶羅波安的家的罪，以致他的家從地上被塗抹與除滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這事叫耶羅波安的家陷在罪裏，甚至他的家從地上除滅了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And this thing became sin to the house of Jeroboam, so as to cut it off and to destroy it from the face of the earth."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This event became sin to the house of Jeroboam, even to blot it out and destroy it from off the face of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This was the sin of the house of Jeroboam that led to its downfall and to its destruction from the face of the earth."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And this thing was the sin of the house of Jeroboam, so as to exterminate and destroy it from the face of the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This became a great sin and resulted in the utter destruction of Jeroboam’s dynasty from the face of the earth."
      }
    },
