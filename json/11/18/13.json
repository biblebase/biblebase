    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶洗别屠杀耶和华的众先知时，我把一百名先知分别藏在两个山洞里，每洞五十人，供应他们饮食。我主没有听说这事吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "難道沒有人把我所作的事告訴我主嗎？就是耶洗別殺害耶和華眾先知的時候，我把一百個先知，五十人一組的分別藏在山洞裡，又用餅和水供養他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶洗別殺耶和華眾先知的時候，我將耶和華的一百個先知藏了，每五十人藏在一個洞裏，拿餅和水供養他們，豈沒有人將這事告訴我主嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Has it not been told my lord what I did when Jezebel killed the prophets of the Lord, how I hid a hundred men of the Lord’s prophets by fifties in a cave and fed them with bread and water?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Has it not been told to my master what I did when Jezebel killed the prophets of the Lord, that I hid a hundred prophets of the Lord by fifties in a cave, and provided them with bread and water?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Haven’t you heard, my lord, what I did while Jezebel was killing the prophets of the Lord? I hid a hundred of the Lord’s prophets in two caves, fifty in each, and supplied them with food and water."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Was it not reported to my lord what I did when Jezebel killed the prophets of the Lord, how I hid one hundred men of the Lord’s prophets, fifty to a cave, and fed them with bread and water?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Has no one told you, my lord, about the time when Jezebel was trying to kill the Lord’s prophets? I hid 100 of them in two caves and supplied them with food and water."
      }
    },
