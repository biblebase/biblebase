    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你家人中，死在城中的必被狗吃，死在野外的必被鸟吃。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "屬巴沙的人，死在城裡的，必被狗吃掉；死在田野的，必被空中的飛鳥吃掉。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡屬巴沙的人，死在城中的必被狗吃，死在田野的必被空中的鳥吃。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Anyone belonging to Baasha who dies in the city the dogs shall eat, and anyone of his who dies in the field the birds of the heavens shall eat.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Anyone of Baasha who dies in the city the dogs will eat, and anyone of his who dies in the field the birds of the heavens will eat.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Dogs will eat those belonging to Baasha who die in the city, and birds will feed on those who die in the country.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The dogs shall eat whoever belongs to Baasha and dies in the city, and the birds of the air shall eat whoever dies in the fields.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The members of Baasha’s family who die in the city will be eaten by dogs, and those who die in the field will be eaten by vultures.”"
      }
