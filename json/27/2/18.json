    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "dan.2.1-49",
            "paragraphs": [
              "A.尼布甲尼撒夢見四帝國和神國（二1～49）"
            ]
          },
          {
            "verse": "dan.2.12-24",
            "paragraphs": [
              "b.但以理的反應（二12～24）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "dan.2.18",
            "paragraphs": [
              "二18\"奧秘的事\"。一些未知的事（這裏指異夢及其解釋），但後來要顯明並包含一些更高深的信息。參看以弗所書三章3節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要同伴祈求天上的上帝施怜悯，显明这奥秘，以免他们和其他巴比伦的智者一起被杀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要他們為這隱祕的事祈求天上的　神憐憫，免得但以理和他的同伴與巴比倫其餘的智慧人一同滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要他們祈求天上的神施憐憫，將這奧祕的事指明，免得但以理和他的同伴與巴比倫其餘的哲士一同滅亡。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "and told them to seek mercy from the God of heaven concerning this mystery, so that Daniel and his companions might not be destroyed with the rest of the wise men of Babylon."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that they might request compassion from the God of heaven concerning this mystery, so that Daniel and his friends would not be destroyed with the rest of the wise men of Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He urged them to plead for mercy from the God of heaven concerning this mystery, so that he and his friends might not be executed with the rest of the wise men of Babylon."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "that they might seek mercies from the God of heaven concerning this secret, so that Daniel and his companions might not perish with the rest of the wise men of Babylon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He urged them to ask the God of heaven to show them his mercy by telling them the secret, so they would not be executed along with the other wise men of Babylon."
      }
    },
