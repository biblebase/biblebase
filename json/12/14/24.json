      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他行耶和華看為惡的事，不轉離尼八的兒子耶羅波安使以色列人陷於罪中的一切罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他行耶和華眼中看為惡的事，不離開尼八的兒子耶羅波安使以色列人陷在罪裏的一切罪。"
      },
