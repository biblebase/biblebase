      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从外面进去的不会使人污秽，只有从人里面发出来的才会使人污秽。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從外面進去的，不能使人污穢，從裡面出來的，才能使人污穢。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "从人外面进到他里面的，没有一样能使人污秽；相反，从人里面出来的，才会使人污秽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從外面進去的不能污穢人，惟有從裏面出來的乃能污穢人。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "There is nothing outside a person that by going into him can defile him, but the things that come out of a person are what defile him.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "there is nothing outside the man which can defile him if it goes into him; but the things which proceed out of the man are what defile the man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nothing outside a person can defile them by going into them. Rather, it is what comes out of a person that defiles them.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "There is nothing that enters a man from outside which can defile him; but the things which come out of him, those are the things that defile a man."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It’s not what goes into your body that defiles you; you are defiled by what comes from your heart.”"
      }
    },
