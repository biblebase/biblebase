    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那位律法教师说：“老师，你说的对，上帝只有一位，除祂以外，别无他神。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那經學家對耶穌說：“老師，是的，你說的很對，　神是獨一的，除了他以外再沒有別的　神。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "那经文士就对耶稣说：“不错，老师，你真的讲了：神只有一位，除他以外没有别的神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那文士對耶穌說：「夫子說，神是一位，實在不錯；除了他以外，再沒有別的神；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the scribe said to him, “You are right, Teacher. You have truly said that he is one, and there is no other besides him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The scribe said to Him, “Right, Teacher; You have truly stated that He is One, and there is no one else besides Him;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Well said, teacher,” the man replied. “You are right in saying that God is one and there is no other but him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So the scribe said to Him, “Well said, Teacher. You have spoken the truth, for there is one God, and there is no other but He."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The teacher of religious law replied, “Well said, Teacher. You have spoken the truth by saying that there is only one God and no other."
      }
    },
