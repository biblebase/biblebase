    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王听了这个请求，感到十分为难，但因为在众宾客面前起了誓，就不好拒绝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希律王非常憂愁，但是因為他的誓言和在座的賓客，就不願拒絕她。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "王就变得很忧愁，但因所起的誓，又因坐席的人，就不好拒绝她。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王就甚憂愁；但因他所起的誓，又因同席的人，就不肯推辭，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the king was exceedingly sorry, but because of his oaths and his guests he did not want to break his word to her."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And although the king was very sorry, yet because of his oaths and because of his dinner guests, he was unwilling to refuse her."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king was greatly distressed, but because of his oaths and his dinner guests, he did not want to refuse her."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the king was exceedingly sorry; yet, because of the oaths and because of those who sat with him, he did not want to refuse her."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the king deeply regretted what he had said; but because of the vows he had made in front of his guests, he couldn’t refuse her."
      }
