    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "忽然，湖面上狂风大作，波浪撞击船身，船内几乎灌满了水。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "忽然起了狂風，波浪不斷地打進船來，艙裡積滿了水。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这时候，起了大风暴，以致波浪打进船里，船几乎灌满了水。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "忽然起了暴風，波浪打入船內，甚至船要滿了水。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And a great windstorm arose, and the waves were breaking into the boat, so that the boat was already filling."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And there *arose a fierce gale of wind, and the waves were breaking over the boat so much that the boat was already filling up."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A furious squall came up, and the waves broke over the boat, so that it was nearly swamped."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And a great windstorm arose, and the waves beat into the boat, so that it was already filling."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But soon a fierce storm came up. High waves were breaking into the boat, and it began to fill with water."
      }
