    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "出卖耶稣的犹大预先和他们定了暗号，说：“我亲吻谁，谁就是耶稣，你们把祂抓起来，小心押走。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "出賣耶穌的人給他們一個暗號，說：“我跟誰親吻，誰就是他。你們把他逮捕，小心帶去。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "那出卖耶稣的人已经和他们定了一个暗号，说：“我如果亲吻谁，谁就是那个人。你们把他抓住，小心地带走。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "賣耶穌的人曾給他們一個暗號，說：「我與誰親嘴，誰就是他。你們把他拿住，牢牢靠靠地帶去。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now the betrayer had given them a sign, saying, “The one I will kiss is the man. Seize him and lead him away under guard.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now he who was betraying Him had given them a signal, saying, “Whomever I kiss, He is the one; seize Him and lead Him away under guard.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the betrayer had arranged a signal with them: “The one I kiss is the man; arrest him and lead him away under guard.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now His betrayer had given them a signal, saying, “Whomever I kiss, He is the One; seize Him and lead Him away safely.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The traitor, Judas, had given them a prearranged signal: “You will know which one to arrest when I greet him with a kiss. Then you can take him away under guard.”"
      }
    },
