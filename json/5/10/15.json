    "sermons": [
      {
        "id": "Sermon-2011-10-09",
        "title": "神向你所要的是什麼？",
        "date": "2011-10-09",
        "preacher": "王文堂牧師",
        "verses": "申命記 10:1-22",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-10-09.pdf",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-10-09.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但耶和华只喜爱你们的祖先，从万族中拣选了他们的后裔——你们，正如今日的情形。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但耶和華只喜悅你的列祖，愛他們；他從萬民中揀選了他們的後裔，就是你們，像今日一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華但喜悅你的列祖，愛他們，從萬民中揀選他們的後裔，就是你們，像今日一樣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Yet the Lord set his heart in love on your fathers and chose their offspring after them, you above all peoples, as you are this day."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet on your fathers did the Lord set His affection to love them, and He chose their descendants after them, even you above all peoples, as it is this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet the Lord set his affection on your ancestors and loved them, and he chose you, their descendants, above all the nations—as it is today."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The Lord delighted only in your fathers, to love them; and He chose their descendants after them, you above all peoples, as it  is this day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet the Lord chose your ancestors as the objects of his love. And he chose you, their descendants, above all other nations, as is evident today."
      }
    },
