    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要赞美祂，祂是你们的上帝，你们亲眼目睹了祂为你们做的伟大而可畏的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他是你當頌讚的；他是你的　神，他為你作了那些大而可畏的事，就是你親眼見過的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他是你所讚美的，是你的神，為你做了那大而可畏的事，是你親眼所看見的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He is your praise. He is your God, who has done for you these great and terrifying things that your eyes have seen."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He is your praise and He is your God, who has done these great and awesome things for you which your eyes have seen."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He is the one you praise; he is your God, who performed for you those great and awesome wonders you saw with your own eyes."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He is your praise, and He is your God, who has done for you these great and awesome things which your eyes have seen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He alone is your God, the only one who is worthy of your praise, the one who has done these mighty miracles that you have seen with your own eyes."
      }
