    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西把所有的以色列人召来，对他们说：“以色列人啊，要留心听我今天对你们宣讲的律例和典章，要学习并谨慎遵行。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西把以色列人都召了來，對他們說：“以色列人哪，我今日講給你們聽的律例和典章，你們要聽從，要學習，要謹慎遵行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西將以色列眾人召了來，對他們說：「以色列人哪，我今日曉諭你們的律例典章，你們要聽，可以學習，謹守遵行。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And Moses summoned all Israel and said to them, “Hear, O Israel, the statutes and the rules that I speak in your hearing today, and you shall learn them and be careful to do them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Moses summoned all Israel and said to them: “Hear, O Israel, the statutes and the ordinances which I am speaking today in your hearing, that you may learn them and observe them carefully."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moses summoned all Israel and said: Hear, Israel, the decrees and laws I declare in your hearing today. Learn them and be sure to follow them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And Moses called all Israel, and said to them: “Hear, O Israel, the statutes and judgments which I speak in your hearing today, that you may learn them and be careful to observe them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses called all the people of Israel together and said, “Listen carefully, Israel. Hear the decrees and regulations I am giving you today, so you may learn them and obey them!"
      }
