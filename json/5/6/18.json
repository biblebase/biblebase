    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要做耶和华视为正与善的事，以便你们可以凡事顺利，得到耶和华起誓赐给你们祖先的佳美之地，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華看為正為好的事，你都要去行，使你可以享福，並且可以進去佔領耶和華向你的列祖起誓應許的那美地，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華眼中看為正、看為善的，你都要遵行，使你可以享福，並可以進去得耶和華向你列祖起誓應許的那美地，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And you shall do what is right and good in the sight of the Lord, that it may go well with you, and that you may go in and take possession of the good land that the Lord swore to give to your fathers"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall do what is right and good in the sight of the Lord, that it may be well with you and that you may go in and possess the good land which the Lord swore to give your fathers,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do what is right and good in the Lord’s sight, so that it may go well with you and you may go in and take over the good land the Lord promised on oath to your ancestors,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And you shall do what  is right and good in the sight of the Lord, that it may be well with you, and that you may go in and possess the good land of which the Lord swore to your fathers,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do what is right and good in the Lord’s sight, so all will go well with you. Then you will enter and occupy the good land that the Lord swore to give your ancestors."
      }
