        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那七天之内，你们境内不可有酵母。第一天傍晚献的祭肉不可留到第二天早晨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "七天之內，在你的全境不可見有酵。第一天晚上你獻的祭肉，一點也不可剩下，留到早晨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在你四境之內，七日不可見麵酵，頭一日晚上所獻的肉，一點不可留到早晨。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "No leaven shall be seen with you in all your territory for seven days, nor shall any of the flesh that you sacrifice on the evening of the first day remain all night until morning."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For seven days no leaven shall be seen with you in all your territory, and none of the flesh which you sacrifice on the evening of the first day shall remain overnight until morning."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let no yeast be found in your possession in all your land for seven days. Do not let any of the meat you sacrifice on the evening of the first day remain until morning."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And no leaven shall be seen among you in all your territory for seven days, nor shall any of the meat which you sacrifice the first day at twilight remain overnight until morning."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let no yeast be found in any house throughout your land for those seven days. And when you sacrifice the Passover lamb on the evening of the first day, do not let any of the meat remain until the next morning."
      }
