    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你们的上帝耶和华充满怜悯，祂不会抛弃你们，灭绝你们，也不会忘记祂与你们祖先所立的誓约。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華你的　神原是滿有憐憫的　神，他必不丟棄你，不毀滅你，也不忘記他起誓與你的列祖所立的約。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－你神原是有憐憫的神；他總不撇下你，不滅絕你，也不忘記他起誓與你列祖所立的約。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For the Lord your God is a merciful God. He will not leave you or destroy you or forget the covenant with your fathers that he swore to them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord your God is a compassionate God; He will not fail you nor destroy you nor forget the covenant with your fathers which He swore to them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the Lord your God is a merciful God; he will not abandon or destroy you or forget the covenant with your ancestors, which he confirmed to them by oath."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "(for the Lord your God is a merciful God), He will not forsake you nor destroy you, nor forget the covenant of your fathers which He swore to them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the Lord your God is a merciful God; he will not abandon you or destroy you or forget the solemn covenant he made with your ancestors."
      }
