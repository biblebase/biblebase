    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们不可按照他们的方式敬拜你们的上帝耶和华。他们祭拜自己神明的种种行为令耶和华憎恶，他们甚至焚烧自己的儿女献给他们的神明。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你不可向耶和華你的　神這樣行，因為耶和華恨惡的一切可厭惡的事，他們都向他們的神行了，甚至把自己的兒女用火焚燒，獻給他們的神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你不可向耶和華－你的神這樣行，因為他們向他們的神行了耶和華所憎嫌所恨惡的一切事，甚至將自己的兒女用火焚燒，獻與他們的神。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You shall not worship the Lord your God in that way, for every abominable thing that the Lord hates they have done for their gods, for they even burn their sons and their daughters in the fire to their gods."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not behave thus toward the Lord your God, for every abominable act which the Lord hates they have done for their gods; for they even burn their sons and daughters in the fire to their gods."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You must not worship the Lord your God in their way, because in worshiping their gods, they do all kinds of detestable things the Lord hates. They even burn their sons and daughters in the fire as sacrifices to their gods."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You shall not worship the Lord your God in that way; for every abomination to the Lord which He hates they have done to their gods; for they burn even their sons and daughters in the fire to their gods."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must not worship the Lord your God the way the other nations worship their gods, for they perform for their gods every detestable act that the Lord hates. They even burn their sons and daughters as sacrifices to their gods."
      }
    },
