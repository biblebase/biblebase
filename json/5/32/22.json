    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的怒火已经点燃， 要烧到阴间的尽头， 吞灭大地及其出产， 甚至烧毁山的根基。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為在我的怒中有火燃燒起來， 燒到陰間的深處， 把大地和地的出產盡都吞滅， 連山的根基也燒著了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為在我怒中有火燒起， 直燒到極深的陰間， 把地和地的出產盡都焚燒， 山的根基也燒着了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For a fire is kindled by my anger, and it burns to the depths of Sheol, devours the earth and its increase, and sets on fire the foundations of the mountains."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For a fire is kindled in My anger, And burns to the lowest part of Sheol, And consumes the earth with its yield, And sets on fire the foundations of the mountains."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For a fire will be kindled by my wrath, one that burns down to the realm of the dead below. It will devour the earth and its harvests and set afire the foundations of the mountains."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For a fire is kindled in My anger, And shall burn to the lowest hell; It shall consume the earth with her increase, And set on fire the foundations of the mountains."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For my anger blazes forth like fire and burns to the depths of the grave. It devours the earth and all its crops and ignites the foundations of the mountains."
      }
