    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "狡诈、堕落的一代啊， 你们却对祂不忠，行事败坏， 不配做祂的儿女。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這世代向他行事敗壞，就不是他的兒女，滿有弊病， 這實在是個乖僻彎曲的世代。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這乖僻彎曲的世代向他行事邪僻； 有這弊病就不是他的兒女。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They have dealt corruptly with him; they are no longer his children because they are blemished; they are a crooked and twisted generation."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have acted corruptly toward Him, They are not His children, because of their defect; But are a perverse and crooked generation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are corrupt and not his children; to their shame they are a warped and crooked generation."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“They have corrupted themselves; They  are not His children, Because of their blemish: A perverse and crooked generation."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But they have acted corruptly toward him; when they act so perversely, are they really his children? They are a deceitful and twisted generation."
      }
    },
