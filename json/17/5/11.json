    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "est.5.1-14",
            "paragraphs": [
              "Ⅵ以斯帖得恩寵（五1～14）"
            ]
          },
          {
            "verse": "est.5.9-14",
            "paragraphs": [
              "Ｂ哈曼受挫（五9～14）",
              "作為女主人的以斯帖，一直操縱著這次筵席的發展。這時，敘述者披露各個赴宴者在宴會後發生的事。"
            ]
          },
          {
            "verse": "est.5.11",
            "paragraphs": [
              "11.“愚昧人的心彰顯愚昧”（箴十二23），哈曼正是這樣，無法掩藏他大享富貴的光彩。他把財富放在首位，先於他的兒子〔從後面經文可知他有十個兒子（九7～10）〕。他心中充滿的，是他新近獲取的尊位，使他淩駕於全國人之上。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "est.5.11",
            "paragraphs": [
              "五11\"眾多的兒女\"。哈曼有十個兒子（九10）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "向他们夸耀自己的荣华富贵和儿孙满堂，以及王怎样擢升他，使他位极人臣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "哈曼把他的富貴榮華、兒女的眾多，以及王怎樣使他尊大，怎樣抬舉他在王的眾領袖和眾臣僕之上的一切事，都數說給他們聽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "哈曼將他富厚的榮耀、眾多的兒女，和王擡舉他使他超乎首領臣僕之上，都述說給他們聽。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And Haman recounted to them the splendor of his riches, the number of his sons, all the promotions with which the king had honored him, and how he had advanced him above the officials and the servants of the king."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Haman recounted to them the glory of his riches, and the number of his sons, and every instance where the king had magnified him and how he had promoted him above the princes and servants of the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Haman boasted to them about his vast wealth, his many sons, and all the ways the king had honored him and how he had elevated him above the other nobles and officials."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Haman told them of his great riches, the multitude of his children, everything in which the king had promoted him, and how he had advanced him above the officials and servants of the king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and boasted to them about his great wealth and his many children. He bragged about the honors the king had given him and how he had been promoted over all the other nobles and officials."
      }
    },
