    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "est.3.1-15",
            "paragraphs": [
              "Ⅳ哈曼向猶太人尋仇（三1～15）"
            ]
          },
          {
            "verse": "est.3.12-15",
            "paragraphs": [
              "Ｃ頒布禦旨（三12～15）",
              "禦旨草擬好了，譯成國中各種文字，最後正式頒布，在這一段中詳詳細細描述出來。"
            ]
          },
          {
            "verse": "est.3.14-15",
            "paragraphs": [
              "14～15.這禦旨隨即頒布，使人們準備妥當，抄錄（pat[s%egen）這字是波斯文，在聖經中只有以斯拉記及以斯帖記中出現過，共有六次，是以亞蘭文及希伯來文的形式出現。",
              "最後一句用了對比的技巧作襯托。設謀者慶祝之余，書珊城卻慌亂。作者對人民的反應很敏感，特別記載一般百姓對這駭人的禦旨滿肚狐疑，不知是怎麽一回事。",
              "130RobertGordis,\"Religion,WisdomandHistoryintheBookofEsther──ANewSolutiontoanAncientCrux\".JBL100.3(1981),p.377."
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谕旨的抄本作为法令颁布到各省，通知各族为那天做好准备。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "諭文抄本頒行各省，通告各族，使他們準備好這一天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "抄錄這旨意，頒行各省，宣告各族，使他們預備等候那日。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "A copy of the document was to be issued as a decree in every province by proclamation to all the peoples to be ready for that day."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A copy of the edict to be issued as law in every province was published to all the peoples so that they should be ready for this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A copy of the text of the edict was to be issued as law in every province and made known to the people of every nationality so they would be ready for that day."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "A copy of the document was to be issued as law in every province, being published for all people, that they should be ready for that day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A copy of this decree was to be issued as law in every province and proclaimed to all peoples, so that they would be ready to do their duty on the appointed day."
      }
    },
