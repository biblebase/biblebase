    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "俄巴底亚看到的异象。 论到以东， 主耶和华这样说： “我们从耶和华那里听到消息， 有位使者被派到各国，说， ‘来！我们去攻打以东吧！’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "俄巴底亞所見的異象。 主耶和華指著以東這樣說： 我們從耶和華那裡聽見了一個信息， 有一位使者被派往列國去，說： “起來吧！我們起來與以東爭戰。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "俄巴底亞得了耶和華的默示。 論以東說： 我從耶和華那裏聽見信息， 並有使者被差往列國去， 說：起來吧， 一同起來與以東爭戰！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The vision of Obadiah. Thus says the Lord God  concerning Edom: We have heard a report from the Lord, and a messenger has been sent among the nations: “Rise up! Let us rise against her for battle!”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The vision of Obadiah. Thus says the Lord God concerning Edom— We have heard a report from the Lord, And an envoy has been sent among the nations saying, “Arise and let us go against her for battle”—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The vision of Obadiah. This is what the Sovereign Lord says about Edom— We have heard a message from the Lord: An envoy was sent to the nations to say, “Rise, let us go against her for battle”—"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The vision of Obadiah. Thus says the Lord God  concerning Edom (We have heard a report from the Lord, And a messenger has been sent among the nations, saying, “Arise, and let us rise up against her for battle”):"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is the vision that the Sovereign Lord revealed to Obadiah concerning the land of Edom. We have heard a message from the Lord that an ambassador was sent to the nations to say, “Get ready, everyone! Let’s assemble our armies and attack Edom!”"
      }
    },
