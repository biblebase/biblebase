      "leili": {
        "content": [
          {
            "verse": "hos.7.7",
            "paragraphs": [
              "七7\"他們的君王都仆倒而死\"。以色列最後六位君王裏的四位都被謀殺而死。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们像炙热的烤炉， 吞灭他们的首领。 他们的君王都倒地而亡， 没有一个向我求救。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有的人都熱如火爐， 吞滅他們的官長。 他們所有的君王都倒斃了， 他們中間沒有一人向我呼求。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾民也熱如火爐， 燒滅他們的官長。 他們的君王都仆倒而死； 他們中間無一人求告我。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "All of them are hot as an oven, and they devour their rulers. All their kings have fallen, and none of them calls upon me."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All of them are hot like an oven, And they consume their rulers; All their kings have fallen. None of them calls on Me."
