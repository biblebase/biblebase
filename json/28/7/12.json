    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.7.11-12",
            "paragraphs": [
              "七11,12與\"埃及\"和\"亞述\"聯盟毫無益處，耶和華將要審判以色列。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们去的时候， 我要张网网住他们， 我要像打落飞鸟一样打落他们。 我要按他们的恶行惩罚他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但他們去的時候，我要把我的網撒在他們身上； 我要把他們如同空中的飛鳥打下來。 我要按著他們會眾所聽見的，懲罰他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們去的時候，我必將我的網撒在他們身上； 我要打下他們，如同空中的鳥。 我必按他們會眾所聽見的懲罰他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "As they go, I will spread over them my net; I will bring them down like birds of the heavens; I will discipline them according to the report made to their congregation."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they go, I will spread My net over them; I will bring them down like the birds of the sky. I will chastise them in accordance with the proclamation to their assembly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they go, I will throw my net over them; I will pull them down like the birds in the sky. When I hear them flocking together, I will catch them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Wherever they go, I will spread My net on them; I will bring them down like birds of the air; I will chastise them According to what their congregation has heard."
