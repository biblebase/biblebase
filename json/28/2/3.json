    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不然，我要剥光她的衣服， 使她像初生之时一样赤身露体； 我要使她像沙漠，如旱地， 干渴而死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "免得我把她剝光， 赤身露體，使她像剛生下來的那天一樣； 又使她像曠野， 像無水的旱地一般， 叫她因乾渴而死。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "免得我剝她的衣服， 使她赤體，與才生的時候一樣， 使她如曠野，如乾旱之地， 因渴而死。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "lest I strip her naked and make her as in the day she was born, and make her like a wilderness, and make her like a parched land, and kill her with thirst."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Or I will strip her naked And expose her as on the day when she was born. I will also make her like a wilderness, Make her like desert land And slay her with thirst."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Otherwise I will strip her naked and make her as bare as on the day she was born; I will make her like a desert, turn her into a parched land, and slay her with thirst."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Lest I strip her naked And expose her, as in the day she was born, And make her like a wilderness, And set her like a dry land, And slay her with thirst."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Otherwise, I will strip her as naked as she was on the day she was born. I will leave her to die of thirst, as in a dry and barren wilderness."
      }
    },
