    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "hos.2.8",
            "paragraphs": [
              "2:8",
              "不知道：以色列人認為他們五谷豐登的美福是巴力(迦南人的神祗)所賜，卻不知實際上是神的憐憫和恩典使他們享受各樣的美物。因此我們須隨時留意神賜給我們的恩惠和福氣，並心存感恩，若我們不如此行，就等於邁出了遠離神的第一步。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她不知道是我给她五谷、新酒和油， 又赐给她大量金银—— 她却用这些供奉巴力。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "其實她不知道，是我把五穀、新酒、橄欖油賜給了她； 甚至他們用來製造巴力的金銀， 也是我大量給她的；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她不知道是我給她五穀、新酒，和油， 又加增她的金銀； 她卻以此供奉巴力。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And she did not know that it was I who gave her the grain, the wine, and the oil, and who lavished on her silver and gold, which they used for Baal."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“For she does not know that it was I who gave her the grain, the new wine and the oil, And lavished on her silver and gold, Which they used for Baal."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She has not acknowledged that I was the one who gave her the grain, the new wine and oil, who lavished on her the silver and gold— which they used for Baal."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For she did not know That I gave her grain, new wine, and oil, And multiplied her silver and gold— Which they prepared for Baal."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She doesn’t realize it was I who gave her everything she has— the grain, the new wine, the olive oil; I even gave her silver and gold. But she gave all my gifts to Baal."
      }
    },
