    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.14.4-8",
            "paragraphs": [
              "一四4～8這裏談及以色列人將來在千禧年所得到的祝福。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以法莲啊，你要远离偶像。 我应允你的祷告，看顾你。 我像一棵青翠的松树， 你的果实都来自于我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮與偶像還有甚麼關係呢？ 答允他的是我，看顧他的也是我。 我像一棵青翠的松樹， 你的果子是從我而得。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮  必說： 我與偶像還有甚麼關涉呢？ 我－耶和華回答他，也必顧念他。 我如青翠的松樹； 你的果子從我而得。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "O Ephraim, what have I to do with idols? It is I who answer and look after you. I am like an evergreen cypress; from me comes your fruit."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Ephraim, what more have I to do with idols? It is I who answer and look after you. I am like a luxuriant cypress; From Me comes your fruit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ephraim, what more have I to do with idols? I will answer him and care for him. I am like a flourishing juniper; your fruitfulness comes from me.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Ephraim shall  say, ‘What have I to do anymore with idols?’ I have heard and observed him. I am like a green cypress tree; Your fruit is found in Me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O Israel, stay away from idols! I am the one who answers your prayers and cares for you. I am like a tree that is always green; all your fruit comes from me.”"
      }
    },
