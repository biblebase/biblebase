    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他要长出嫩枝， 他要像橄榄树一样荣美， 如黎巴嫩的香柏树一般芳香。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的幼枝必伸展， 他的榮美要像橄欖樹， 他的香氣必如黎巴嫩的香柏樹。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的枝條必延長； 他的榮華如橄欖樹； 他的香氣如黎巴嫩的香柏樹。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "his shoots shall spread out; his beauty shall be like the olive, and his fragrance like Lebanon."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His shoots will sprout, And his beauty will be like the olive tree And his fragrance like the cedars of Lebanon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "his young shoots will grow. His splendor will be like an olive tree, his fragrance like a cedar of Lebanon."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "His branches shall spread; His beauty shall be like an olive tree, And his fragrance like Lebanon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Its branches will spread out like beautiful olive trees, as fragrant as the cedars of Lebanon."
      }
    },
