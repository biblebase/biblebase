    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “我要医治他们的叛道， 心甘情愿地爱他们， 因为我的怒气已经转离他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我必醫治他們背道的病， 甘心樂意愛他們， 因為我的怒氣已經遠離他們了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必醫治他們背道的病， 甘心愛他們； 因為我的怒氣向他們轉消。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
