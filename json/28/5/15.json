    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "hos.5.15",
            "paragraphs": [
              "5:15",
              "我要回到原處：神要直等到以色列人承認己罪，擔當了刑罰，並誠心尋求他的幫助時，才會垂聽他們的禱告。下一章記錄了對將來悔改的一代人所作禱告的預言(6:1~3)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要撇下他们，返回我的居所， 直到他们知罪来寻求我。 在苦难的日子里， 他们必恳切地寻求我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要回到自己的地方去； 直到他們承認自己的罪過， 尋求我的面。 在痛苦中必懇切尋求我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要回到原處， 等他們自覺有罪，尋求我面； 他們在急難的時候必切切尋求我。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I will return again to my place, until they acknowledge their guilt and seek my face, and in their distress earnestly seek me."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will go away and return to My place Until they acknowledge their guilt and seek My face; In their affliction they will earnestly seek Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I will return to my lair until they have borne their guilt and seek my face— in their misery they will earnestly seek me.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I will return again to My place Till they acknowledge their offense. Then they will seek My face; In their affliction they will earnestly seek Me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I will return to my place until they admit their guilt and turn to me. For as soon as trouble comes, they will earnestly search for me.”"
      }
    },
