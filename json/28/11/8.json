    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲啊，我怎能舍弃你？ 以色列啊，我怎能弃绝你？ 我怎能使你的下场如押玛？ 我怎能对待你如对待洗扁？ 我的心意改变，我满怀怜爱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“以法蓮啊，我怎能捨棄你！ 以色列啊，我怎能把你交出來！ 我怎能對你如同押瑪， 待你好像洗扁呢！ 我的心腸翻轉， 我的憐憫發動。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮哪，我怎能捨棄你？ 以色列啊，我怎能棄絕你？ 我怎能使你如押瑪？ 怎能使你如洗扁？ 我回心轉意， 我的憐愛大大發動。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "How can I give you up, O Ephraim? How can I hand you over, O Israel? How can I make you like Admah? How can I treat you like Zeboiim? My heart recoils within me; my compassion grows warm and tender."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How can I give you up, O Ephraim? How can I surrender you, O Israel? How can I make you like Admah? How can I treat you like Zeboiim? My heart is turned over within Me, All My compassions are kindled."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“How can I give you up, Ephraim? How can I hand you over, Israel? How can I treat you like Admah? How can I make you like Zeboyim? My heart is changed within me; all my compassion is aroused."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“How can I give you up, Ephraim? How can I hand you over, Israel? How can I make you like Admah? How can I set you like Zeboiim? My heart churns within Me; My sympathy is stirred."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Oh, how can I give you up, Israel? How can I let you go? How can I destroy you like Admah or demolish you like Zeboiim? My heart is torn within me, and my compassion overflows."
      }
    },
