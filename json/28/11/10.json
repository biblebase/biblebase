    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.11.10",
            "paragraphs": [
              "一一10雖然審判即將來臨，但有一天神要\"如獅子吼叫\"，召集牠的幼兒。急切得顫抖的以色列，將會得到復興，重新聚集在她的土地上。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以色列子民必跟随耶和华， 祂要像狮子一样吼叫。 祂吼叫的时候， 他们要战战兢兢地从西方来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們必跟隨耶和華； 耶和華要像獅子吼叫； 他一吼叫， 他們就從西方戰戰兢兢而來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華必如獅子吼叫， 子民必跟隨他。 他一吼叫， 他們就從西方急速而來。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They shall go after the Lord; he will roar like a lion; when he roars, his children shall come trembling from the west;"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will walk after the Lord, He will roar like a lion; Indeed He will roar And His sons will come trembling from the west."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will follow the Lord; he will roar like a lion. When he roars, his children will come trembling from the west."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“They shall walk after the Lord. He will roar like a lion. When He roars, Then His sons shall come trembling from the west;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For someday the people will follow me. I, the Lord, will roar like a lion. And when I roar, my people will return trembling from the west."
      }
    },
