    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们祭司越增多， 所犯的罪也越多， 所以我要使你们的尊荣变为羞耻。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司越增多，就越發得罪我， 我必把他們的尊榮變為恥辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司越發增多，就越發得罪我； 我必使他們的榮耀變為羞辱。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The more they increased, the more they sinned against me; I will change their glory into shame."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The more they multiplied, the more they sinned against Me; I will change their glory into shame."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The more priests there were, the more they sinned against me; they exchanged their glorious God for something disgraceful."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“The more they increased, The more they sinned against Me; I will change their glory into shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The more priests there are, the more they sin against me. They have exchanged the glory of God for the shame of idols."
      }
    },
