    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我为他写下千万条的律法， 他却视若无睹。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我雖然為他們寫了律法萬條， 他們卻看作是為外族人寫的，與他們毫無關係一般。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我為他寫了律法萬條， 他卻以為與他毫無關涉。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Were I to write for him my laws by the ten thousands, they would be regarded as a strange thing."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Though I wrote for him ten thousand precepts of My law, They are regarded as a strange thing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I wrote for them the many things of my law, but they regarded them as something foreign."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I have written for him the great things of My law, But they were considered a strange thing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even though I gave them all my laws, they act as if those laws don’t apply to them."
      }
