        "lang": "cht",
        "text": "你們被囚仍有盼望的人哪！要歸回保障。 今天我要宣告：我必加倍補償給你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們被囚而有指望的人都要轉回保障。 我今日說明，我必加倍賜福給你們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Return to your stronghold, O prisoners of hope; today I declare that I will restore to you double."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Return to the stronghold, O prisoners who have the hope; This very day I am declaring that I will restore double to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Return to your fortress, you prisoners of hope; even now I announce that I will restore twice as much to you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Return to the stronghold, You prisoners of hope. Even today I declare That I will restore double to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Come back to the place of safety, all you prisoners who still have hope! I promise this very day that I will repay two blessings for each of your troubles."
      }
