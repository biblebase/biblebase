    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后对多马说：“把你的指头放在我手上摸摸看，伸出手来摸摸我的肋旁。不要怀疑，要信！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後對多馬說：“把你的指頭放在這裡，看看我的手吧！伸出你的手來，探探我的肋旁！不要疑惑，只要信！”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "然后他对多马说：“把你的指头伸到这里来，看看我的手吧。伸出你的手来，放进我的肋旁吧。不要不信，而要信！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就對多馬說：「伸過你的指頭來，摸我的手；伸出你的手來，探入我的肋旁。不要疑惑，總要信！」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then he said to Thomas, “Put your finger here, and see my hands; and put out your hand, and place it in my side. Do not disbelieve, but believe.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He *said to Thomas, “Reach here with your finger, and see My hands; and reach here your hand and put it into My side; and do not be unbelieving, but believing.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he said to Thomas, “Put your finger here; see my hands. Reach out your hand and put it into my side. Stop doubting and believe.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then He said to Thomas, “Reach your finger here, and look at My hands; and reach your hand here, and put it into My side. Do not be unbelieving, but believing.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said to Thomas, “Put your finger here, and look at my hands. Put your hand into the wound in my side. Don’t be faithless any longer. Believe!”"
      }
