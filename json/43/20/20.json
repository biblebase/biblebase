    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说完，让他们看祂的手和肋旁。门徒看见主，十分欢喜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說了這話，就把手和肋旁給他們看。門徒看見主，就歡喜了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "说了这话，他把手和肋旁给他们看。门徒们看见了主，就欢喜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說了這話，就把手和肋旁指給他們看。門徒看見主，就喜樂了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When he had said this, he showed them his hands and his side. Then the disciples were glad when they saw the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And when He had said this, He showed them both His hands and His side. The disciples then rejoiced when they saw the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After he said this, he showed them his hands and side. The disciples were overjoyed when they saw the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "When He had said this, He showed them His hands and His side. Then the disciples were glad when they saw the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As he spoke, he showed them the wounds in his hands and his side. They were filled with joy when they saw the Lord!"
      }
