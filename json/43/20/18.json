    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "抹大拉的玛丽亚就去向门徒报讯，说：“我看见主了！”又把主对她说的话告诉门徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "抹大拉的馬利亞就去，向門徒報信說：“我已經看見主了！”她又把主對她所說的話告訴他們。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "茉大拉的玛丽亚就去告诉门徒们：“我看到主了！”她又把耶稣对她说的这些话告诉他们。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "抹大拉的馬利亞就去告訴門徒說：「我已經看見了主。」她又將主對她說的這話告訴他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Mary Magdalene went and announced to the disciples, “I have seen the Lord”—and that he had said these things to her."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Mary Magdalene *came, announcing to the disciples, “I have seen the Lord,” and that He had said these things to her."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Mary Magdalene went to the disciples with the news: “I have seen the Lord!” And she told them that he had said these things to her."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Mary Magdalene came and told the disciples that she had seen the Lord, and that He had spoken these things to her."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Mary Magdalene found the disciples and told them, “I have seen the Lord!” Then she gave them his message."
      }
    },
