    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我实实在在地告诉你们，你们会看见天门敞开，上帝的天使以人子为梯上上下下。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又對他說：“我實實在在告訴你們，你們要看見天開了，　神的眾使者在人子的身上，上去下來。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣又对他说：“我确确实实地告诉你们：你们将会看见天敞开了，神的天使们在人子上头，上去下来。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又說：「我實實在在地告訴你們，你們將要看見天開了，神的使者上去下來在人子身上。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he said to him, “Truly, truly, I say to you,  you will see heaven opened, and the angels of God ascending and descending on the Son of Man.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He *said to him, “Truly, truly, I say to you, you will see the heavens opened and the angels of God ascending and descending on the Son of Man.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He then added, “Very truly I tell you,  you  will see ‘heaven open, and the angels of God ascending and descending on’  the Son of Man.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And He said to him, “Most assuredly, I say to you, hereafter you shall see heaven open, and the angels of God ascending and descending upon the Son of Man.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said, “I tell you the truth, you will all see heaven open and the angels of God going up and down on the Son of Man, the one who is the stairway between heaven and earth.”"
      }
    },
