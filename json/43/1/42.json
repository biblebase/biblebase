    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他带着西门去见耶稣。 耶稣看着西门，对他说：“约翰的儿子西门，你要改名为矶法。”矶法的意思是彼得。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "安得烈就帶他到耶穌那裡。耶穌定睛看著他，說：“你是約翰的兒子西門，你要稱為磯法。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他就带西门到耶稣那里。 耶稣注视着他，说：“你是约翰的儿子西门，你将被称为矶法。”——“矶法”翻译出来就是“彼得”。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是領他去見耶穌。耶穌看着他，說：「你是約翰的兒子西門，你要稱為磯法。」（磯法翻出來就是彼得。）"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He brought him to Jesus. Jesus looked at him and said, “You are Simon the son of John. You shall be called Cephas” (which means Peter)."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He brought him to Jesus. Jesus looked at him and said, “You are Simon the son of John; you shall be called Cephas” (which is translated Peter)."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he brought him to Jesus. Jesus looked at him and said, “You are Simon son of John. You will be called Cephas” (which, when translated, is Peter)."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And he brought him to Jesus. Now when Jesus looked at him, He said, “You are Simon the son of Jonah.  You shall be called Cephas” (which is translated, A Stone)."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Andrew brought Simon to meet Jesus. Looking intently at Simon, Jesus said, “Your name is Simon, son of John—but you will be called Cephas” (which means “Peter”)."
      }
