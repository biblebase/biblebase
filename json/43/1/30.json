    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这就是我以前所说的那位，‘有一个人在我以后来却比我位分高，因为祂在我之前已经存在了。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這一位就是我所說的：‘有一個人，是在我以後來的，位分比我高，因為他本來是在我以前的。’"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这一位就是我所说的‘那在我之后来的人，已成为在我前面的，因为在我之前他就存在了。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這就是我曾說：『有一位在我以後來、反成了在我以前的，因他本來在我以前。』"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "This is he of whom I said, ‘After me comes a man who ranks before me, because he was before me.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is He on behalf of whom I said, ‘After me comes a Man who has a higher rank than I, for He existed before me.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is the one I meant when I said, ‘A man who comes after me has surpassed me because he was before me.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "This is He of whom I said, ‘After me comes a Man who is preferred before me, for He was before me.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He is the one I was talking about when I said, ‘A man is coming after me who is far greater than I am, for he existed long before me.’"
      }
    },
