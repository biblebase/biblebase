      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣就对彼得说：“把刀收回鞘里！父所赐给我的杯，我难道能不喝吗？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌就對彼得說：「收刀入鞘吧，我父所給我的那杯，我豈可不喝呢？」"
      },
      "esv": {
        "version_id": "ESV",
