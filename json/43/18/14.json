    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这个该亚法以前曾对犹太人建议说：“让祂一个人替众人死对你们更好。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "該亞法就是從前向猶太人提議說“一個人代替人民死，這是有益的”那個人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "该亚法就是曾经向那些犹太人建议“一个人替民众死是有益处的”那个人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這該亞法就是從前向猶太人發議論說「一個人替百姓死是有益的」那位。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "It was Caiaphas who had advised the Jews that it would be expedient that one man should die for the people."
      },
      "nasb": {
        "version_id": "NASB",
