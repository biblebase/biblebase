    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为耶稣知道谁要出卖祂，所以说：“你们不是每一个都干净。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "原來耶穌知道誰要出賣他，所以他說“你們不是人人都是潔淨的”。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "原来，耶稣知道要出卖他的是谁，所以说“你们不是每个人都干净。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌原知道要賣他的是誰，所以說：「你們不都是乾淨的。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For he knew who was to betray him; that was why he said, “Not all of you are clean.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He knew the one who was betraying Him; for this reason He said, “Not all of you are clean.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For he knew who was going to betray him, and that was why he said not every one was clean."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For He knew who would betray Him; therefore He said, “You are not all clean.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For Jesus knew who would betray him. That is what he meant when he said, “Not all of you are clean.”"
      }
    },
