    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“洗过澡的人全身是干净的，只要洗洗脚就好了。你们是干净的，但不是每一个都干净。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說：“洗過澡的人，全身都潔淨，只需要洗腳就可以了。你們是潔淨的，但不是人人都是這樣。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣说：“洗过澡的人，除了脚以外，没有需要洗的；他是完全干净的。你们是干净的，但不是每个人都干净。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「凡洗過澡的人，只要把腳一洗，全身就乾淨了。你們是乾淨的，然而不都是乾淨的。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Jesus said to him, “The one who has bathed does not need to wash, except for his feet,  but is completely clean. And you  are clean, but not every one of you.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus *said to him, “He who has bathed needs only to wash his feet, but is completely clean; and you are clean, but not all of you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus answered, “Those who have had a bath need only to wash their feet; their whole body is clean. And you are clean, though not every one of you.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Jesus said to him, “He who is bathed needs only to wash his feet, but is completely clean; and you are clean, but not all of you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “A person who has bathed all over does not need to wash, except for the feet, to be entirely clean. And you disciples are clean, but not all of you.”"
      }
    },
