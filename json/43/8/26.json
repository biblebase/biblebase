    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.26",
            "paragraphs": [
              "八26本節的意思比較含糊。主似乎是說，祂有許多其他關於這些不信的猶太人的事要說，是判斷他們的。祂能揭穿他們惡毒的思想和心裏的謀算。但祂遵從父，只說父叫祂所說的話。因為父是真的，故此耶穌所說的話是當信、該聽的。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "关于你们，我有许多要说、要审判的，但差我来的那位是真实的，我把从祂那里听到的告诉世人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "關於你們，我有許多事要說，要判斷；但那差我來的是真實的，我從他那裡聽見的，就告訴世人。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "关于你们，我有很多事要说、要评断；不过派我来的那一位是真实的，我从他那里听了这些事，就向世人说出来。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我有許多事講論你們，判斷你們；但那差我來的是真的，我在他那裏所聽見的，我就傳給世人。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I have much to say about you and much to judge, but he who sent me is true, and I declare to the world what I have heard from him.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have many things to speak and to judge concerning you, but He who sent Me is true; and the things which I heard from Him, these I speak to the world.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I have much to say in judgment of you. But he who sent me is trustworthy, and what I have heard from him I tell the world.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I have many things to say and to judge concerning you, but He who sent Me is true; and I speak to the world those things which I heard from Him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have much to say about you and much to condemn, but I won’t. For I say only what I have heard from the one who sent me, and he is completely truthful.”"
      }
    },
