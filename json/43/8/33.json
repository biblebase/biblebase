    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们说：“我们是亚伯拉罕的子孙，从来没有做过奴隶，你为什么说我们可以得到自由呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們說：“我們是亞伯拉罕的後裔，從來沒有被誰奴役過，你怎麼說‘你們必得自由’呢？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们回答：“我们是亚伯拉罕的后裔，从来没有做过谁的奴仆。你怎么说‘你们将要得自由’呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們回答說：「我們是亞伯拉罕的後裔，從來沒有作過誰的奴僕。你怎麼說『你們必得以自由』呢？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They answered him, “We are offspring of Abraham and have never been enslaved to anyone. How is it that you say, ‘You will become free’?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They answered Him, “We are Abraham’s descendants and have never yet been enslaved to anyone; how is it that You say, ‘You will become free’?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They answered him, “We are Abraham’s descendants and have never been slaves of anyone. How can you say that we shall be set free?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They answered Him, “We are Abraham’s descendants, and have never been in bondage to anyone. How can You say, ‘You will be made free’?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But we are descendants of Abraham,” they said. “We have never been slaves to anyone. What do you mean, ‘You will be set free’?”"
      }
