      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "父啊，現在讓我在你自己面前得著榮耀，就是在創世以前我與你同享的榮耀。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "父啊， 现在愿你用那荣耀，与你自己一起荣耀我， 那荣耀是世界存在以前我与你一起就有的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "父啊，現在求你使我同你享榮耀，就是未有世界以先，我同你所有的榮耀。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And now, Father, glorify me in your own presence with the glory that I had with you before the world existed."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now, Father, glorify Me together with Yourself, with the glory which I had with You before the world was."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And now, Father, glorify me in your presence with the glory I had with you before the world began."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And now, O Father, glorify Me together with Yourself, with the glory which I had with You before the world was."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now, Father, bring me into the glory we shared before the world began."
      }
