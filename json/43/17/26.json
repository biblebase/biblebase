    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我使他们认识了你的名，我还要使他们更认识你，好让你对我的爱存在他们里面，我也在他们里面。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我已經把你的名指示他們，還要再指示，使你愛我的愛在他們裡面，我也在他們裡面。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我将你的名向他们显明了， 并且还要显明。 这样，你爱我的爱就能在他们里面， 我也能在他们里面。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我已將你的名指示他們，還要指示他們，使你所愛我的愛在他們裏面，我也在他們裏面。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I made known to them your name, and I will continue to make it known, that the love with which you have loved me may be in them, and I in them.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and I have made Your name known to them, and will make it known, so that the love with which You loved Me may be in them, and I in them.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have made you  known to them, and will continue to make you known in order that the love you have for me may be in them and that I myself may be in them.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And I have declared to them Your name, and will declare it, that the love with which You loved Me may be in them, and I in them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have revealed you to them, and I will continue to do so. Then your love for me will be in them, and I will be in them.”"
      }
    },
