    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.3.1-21",
            "paragraphs": [
              "八．耶穌教尼哥底母重生的道理（三1～21）"
            ]
          },
          {
            "verse": "jhn.3.4",
            "paragraphs": [
              "三4我們再次見到人要明白主耶穌所說的話是何等困難。尼哥底母將主所說的話照字面解釋。他不明白一個成年人怎能夠重生，認為人根本無法再進母腹生出來。",
              "尼哥底母是屬血氣的人，如哥林多前書二章14節所說：“屬血氣的人不領會神聖靈的事，反倒以為愚拙，並且不能知道，因為這些事惟有屬靈的人才能看透。”"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尼哥德慕说：“人老了，怎能重生呢？难道要再进母腹生一次吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "尼哥德慕說：“人老了，怎能重生呢？難道他能再進母腹生出來嗎？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "尼克迪莫问：“人老了，怎么能出生呢？他难道能再进母腹又出生吗？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "尼哥德慕說：「人已經老了，如何能重生呢？豈能再進母腹生出來嗎？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Nicodemus said to him, “How can a man be born when he is old? Can he enter a second time into his mother’s womb and be born?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nicodemus *said to Him, “How can a man be born when he is old? He cannot enter a second time into his mother’s womb and be born, can he?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“How can someone be born when they are old?” Nicodemus asked. “Surely they cannot enter a second time into their mother’s womb to be born!”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Nicodemus said to Him, “How can a man be born when he is old? Can he enter a second time into his mother’s womb and be born?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What do you mean?” exclaimed Nicodemus. “How can an old man go back into his mother’s womb and be born again?”"
      }
    },
