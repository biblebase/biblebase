    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我实实在在地告诉你，我们所说的是自己知道的，所见证的是自己见过的，可是你们不肯接受我们的见证。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我實實在在告訴你，我們知道的，才講論；見過的，就作證，然而你們卻不接受我們的見證。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我确确实实地告诉你：我们讲论我们所知道的，我们见证我们所看到的，而你们却不接受我们的见证。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我實實在在地告訴你，我們所說的是我們知道的；我們所見證的是我們見過的；你們卻不領受我們的見證。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Truly, truly, I say to you, we speak of what we know, and bear witness to what we have seen, but you  do not receive our testimony."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Truly, truly, I say to you, we speak of what we know and testify of what we have seen, and you do not accept our testimony."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Very truly I tell you, we speak of what we know, and we testify to what we have seen, but still you people do not accept our testimony."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Most assuredly, I say to you, We speak what We know and testify what We have seen, and you do not receive Our witness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I assure you, we tell you what we know and have seen, and yet you won’t believe our testimony."
      }
