    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但如果我做了，你们纵然不信我，也应该相信我所做的事，这样你们就会知道、领悟父在我里面，我也在父里面。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若作了，你們縱然不信我，也應當信這些事，好使你們確實知道，我父是在我裡面，我也在父裡面。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "但如果我做了，你们即使不相信我，也该相信这些事。这样，你们就会知道并体会父在我里面，我也在父里面。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若行了，你們縱然不信我，也當信這些事，叫你們又知道又明白父在我裏面，我也在父裏面。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "but if I do them, even though you do not believe me, believe the works, that you may know and understand that the Father is in me and I am in the Father.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but if I do them, though you do not believe Me, believe the works, so that you may know and understand that the Father is in Me, and I in the Father.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if I do them, even though you do not believe me, believe the works, that you may know and understand that the Father is in me, and I in the Father.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "but if I do, though you do not believe Me, believe the works, that you may know and believe  that the Father is in Me, and I in Him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if I do his work, believe in the evidence of the miraculous works I have done, even if you don’t believe me. Then you will know and understand that the Father is in me, and I am in the Father.”"
      }
    },
