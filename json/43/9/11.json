    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.9.1-12",
            "paragraphs": [
              "九．第六件神跡：治好生來瞎眼的人（九1～12）"
            ]
          },
          {
            "verse": "jhn.9.11",
            "paragraphs": [
              "九11他的見證雖簡單，但有說服力。他告訴他們得醫治的經過，並贊揚那位行神跡的人。但當時，他尚未知道主耶穌是誰，他只稱他為“有一個人，名叫耶穌”。但稍後，他明白多些，知道耶穌是誰了。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "John-19-2005-07-17",
        "title": "命運的轉捩點",
        "date": "2005-07-17",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 9:1-41",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-19-2005-07-17.pdf",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-19-2005-07-17.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他回答说：“有一位叫耶稣的人和泥抹我的眼睛，叫我到西罗亚池子去洗。我照着去做，眼睛就能看见了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他回答：“那名叫耶穌的人和了一點泥，抹在我的眼上，對我說：‘你去西羅亞池洗一洗吧。’我去一洗，就看見了。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他回答：“那个叫耶稣的人，和了些泥抹在我的眼睛上，吩咐我到西罗亚池子去洗。我去一洗，就得以看见。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他回答說：「有一個人，名叫耶穌，他和泥抹我的眼睛，對我說：『你往西羅亞池子去洗。』我去一洗，就看見了。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He answered, “The man called Jesus made mud and anointed my eyes and said to me, ‘Go to Siloam and wash.’ So I went and washed and received my sight.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He answered, “The man who is called Jesus made clay, and anointed my eyes, and said to me, ‘Go to Siloam and wash’; so I went away and washed, and I received sight.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He replied, “The man they call Jesus made some mud and put it on my eyes. He told me to go to Siloam and wash. So I went and washed, and then I could see.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He answered and said, “A Man called Jesus made clay and anointed my eyes and said to me, ‘Go to the pool of Siloam and wash.’ So I went and washed, and I received sight.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He told them, “The man they call Jesus made mud and spread it over my eyes and told me, ‘Go to the pool of Siloam and wash yourself.’ So I went and washed, and now I can see!”"
      }
    },
