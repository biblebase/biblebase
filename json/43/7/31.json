    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.10-31",
            "paragraphs": [
              "二．耶穌在殿裏教訓人（七10～31）"
            ]
          },
          {
            "verse": "jhn.7.31",
            "paragraphs": [
              "七31事實上眾人中間有好些相信了主耶穌。我們看他們的信是真的。他們的推論是這樣。耶穌還能做什麽來證明祂是彌賽亞呢？若耶穌不是彌賽亞，那麽基督來的時候，祂能比耶穌行更多、更奇妙的神跡嗎？他們的問題明顯說出他們因耶穌所行的神跡而相信祂真是彌賽亞。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人群中有许多人信了耶稣，他们说：“基督来的时候所行的神迹，难道会比这个人行得更多吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "群眾中有許多人信了他。他們說：“基督來的時候，他所行的神蹟，能比這人所行的更多嗎？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "但是人群中有许多人信了耶稣，他们说：“基督来的时候，难道会比这个人行更多的神迹吗？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但眾人中間有好些信他的，說：「基督來的時候，他所行的神蹟豈能比這人所行的更多嗎？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Yet many of the people believed in him. They said, “When the Christ appears, will he do more signs than this man has done?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But many of the crowd believed in Him; and they were saying, “When the Christ comes, He will not perform more signs than those which this man has, will He?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Still, many in the crowd believed in him. They said, “When the Messiah comes, will he perform more signs than this man?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And many of the people believed in Him, and said, “When the Christ comes, will He do more signs than these which this Man has done?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Many among the crowds at the Temple believed in him. “After all,” they said, “would you expect the Messiah to do more miraculous signs than this man has done?”"
      }
    },
