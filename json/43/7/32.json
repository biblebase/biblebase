    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.32-36",
            "paragraphs": [
              "三．法利賽人的敵意（七32～36）"
            ]
          },
          {
            "verse": "jhn.7.32",
            "paragraphs": [
              "七32法利賽人穿插於人群中間，聽見他們私下的議論。眾人正為耶穌紛紛議論，他們不是反對祂，而是偷偷的表示欣賞祂。法利賽人害怕情況會使愈來愈多的人接受耶穌，故打發差役去捉拿他。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法利赛人听见众人这样议论耶稣，就联合祭司长派差役去抓祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法利賽人聽見群眾紛紛議論這些關於耶穌的事，祭司長和法利賽人就派差役去逮捕耶穌。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "法利赛人听到了众人私下议论耶稣的这些事。于是祭司长们和法利赛人就派差役们去抓他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法利賽人聽見眾人為耶穌這樣紛紛議論，祭司長和法利賽人就打發差役去捉拿他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The Pharisees heard the crowd muttering these things about him, and the chief priests and Pharisees sent officers to arrest him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Pharisees heard the crowd muttering these things about Him, and the chief priests and the Pharisees sent officers to seize Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Pharisees heard the crowd whispering such things about him. Then the chief priests and the Pharisees sent temple guards to arrest him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The Pharisees heard the crowd murmuring these things concerning Him, and the Pharisees and the chief priests sent officers to take Him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the Pharisees heard that the crowds were whispering such things, they and the leading priests sent Temple guards to arrest Jesus."
      }
    },
