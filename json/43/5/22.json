    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "父不审判人，祂将审判的事全交给子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "父不審判人，卻已經把審判的權柄完全交給子，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "事实上，父并不审判任何人，而把一切审判的事都交给了子，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "父不審判甚麼人，乃將審判的事全交與子，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For the Father judges no one, but has given all judgment to the Son,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For not even the Father judges anyone, but He has given all judgment to the Son,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moreover, the Father judges no one, but has entrusted all judgment to the Son,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For the Father judges no one, but has committed all judgment to the Son,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In addition, the Father judges no one. Instead, he has given the Son absolute authority to judge,"
      }
