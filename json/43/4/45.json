    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然而，祂回到加利利却受到当地的人欢迎，因为他们上耶路撒冷过节时，看见了祂所做的一切。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌到了加利利的時候，加利利人都歡迎他，因為他們曾經上耶路撒冷去過節，見過他所行的一切。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "当耶稣来到加利利地区，加利利人就接受他，因为他们也曾经上耶路撒冷去过节，看到耶稣节日期间在那里所行的一切事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了加利利，加利利人既然看見他在耶路撒冷過節所行的一切事，就接待他，因為他們也是上去過節。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So when he came to Galilee, the Galileans welcomed him, having seen all that he had done in Jerusalem at the feast. For they too had gone to the feast."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So when He came to Galilee, the Galileans received Him, having seen all the things that He did in Jerusalem at the feast; for they themselves also went to the feast."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he arrived in Galilee, the Galileans welcomed him. They had seen all that he had done in Jerusalem at the Passover Festival, for they also had been there."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So when He came to Galilee, the Galileans received Him, having seen all the things He did in Jerusalem at the feast; for they also had gone to the feast."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet the Galileans welcomed him, for they had been in Jerusalem at the Passover celebration and had seen everything he did there."
      }
