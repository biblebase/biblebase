    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁要事奉我，就要跟从我。我在哪里，事奉我的人也要在哪里。我父必尊重事奉我的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果有人服事我，就應當跟從我；我在哪裡，服事我的人也會在哪裡；如果有人服事我，我父必尊重他。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "一个人如果要服事我，就当跟从我；我在哪里，我的仆人也将要在哪里。如果有人服事我，父将尊重他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若有人服事我，就當跟從我；我在哪裏，服事我的人也要在那裏；若有人服事我，我父必尊重他。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If anyone serves me, he must follow me; and where I am, there will my servant be also. If anyone serves me, the Father will honor him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If anyone serves Me, he must follow Me; and where I am, there My servant will be also; if anyone serves Me, the Father will honor him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever serves me must follow me; and where I am, my servant also will be. My Father will honor the one who serves me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "If anyone serves Me, let him follow Me; and where I am, there My servant will be also. If anyone serves Me, him My Father will honor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who wants to serve me must follow me, because my servants must be where I am. And the Father will honor anyone who serves me."
      }
    },
