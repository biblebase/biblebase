    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以赛亚看见了祂的荣耀，所以才这样说。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以賽亞說這些話，是因為看見了他的榮耀，就指著他說的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "以赛亚说了这些话，是因为看见了耶稣的荣耀，就讲论他的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以賽亞因為看見他的榮耀，就指着他說這話。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Isaiah said these things because he saw his glory and spoke of him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These things Isaiah said because he saw His glory, and he spoke of Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Isaiah said this because he saw Jesus’ glory and spoke about him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "These things Isaiah said when he saw His glory and spoke of Him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Isaiah was referring to Jesus when he said this, because he saw the future and spoke of the Messiah’s glory."
      }
