#!/usr/bin/env bash

ruby index-words.rb
ruby save-words.rb
ruby analyse-words.rb
ruby merge-jsons.rb
ruby formatters/base.rb

aws s3 sync --profile=me hhb/ s3://biblebase/hhb/
aws s3 sync --profile=me words/ s3://biblebase/words/
aws s3 sync --profile=me html/ s3://biblebase/html/
aws s3 sync --profile=me json/ s3://biblebase/json/
