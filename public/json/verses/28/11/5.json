    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“他们要返回埃及， 亚述要统治他们， 因为他们不肯归向我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“他們必返回埃及地， 亞述要作他們的王； 因為他們不肯回來歸我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們必不歸回埃及地， 亞述人卻要作他們的王， 因他們不肯歸向我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They will not return to the land of Egypt; But Assyria—he will be their king Because they refused to return to Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Will they not return to Egypt and will not Assyria rule over them because they refuse to repent?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But since my people refuse to return to me, they will return to Egypt and will be forced to serve Assyria."
      }
    },
