    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我用慈绳爱索牵引他们， 解开他们所负的重轭， 弯腰喂养他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我用慈繩愛索牽引他們， 我待他們像人拉高牛的軛在兩顎之上， 又向他們俯身，餵養他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我用慈繩愛索牽引他們； 我待他們如人放鬆牛的兩腮夾板， 把糧食放在他們面前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I led them with cords of a man, with bonds of love, And I became to them as one who lifts the yoke from their jaws; And I bent down and fed them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I led them with cords of human kindness, with ties of love. To them I was like one who lifts a little child to the cheek, and I bent down to feed them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I led Israel along with my ropes of kindness and love. I lifted the yoke from his neck, and I myself stooped to feed him."
      }
    },
