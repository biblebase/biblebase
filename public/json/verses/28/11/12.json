    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “以法莲用谎话包围我， 以色列家用诡诈环绕我； 犹大依然背离圣洁、信实的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮用虛謊， 以色列家用詭詐包圍著我； 猶大還是與　神對抗， 就是對抗那可信可靠的至聖者。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮用謊話， 以色列家用詭計圍繞我； 猶大卻靠神掌權， 向聖者有忠心。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ephraim surrounds Me with lies And the house of Israel with deceit; Judah is also unruly against God, Even against the Holy One who is faithful."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ephraim has surrounded me with lies, Israel with deceit. And Judah is unruly against God, even against the faithful Holy One."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Israel surrounds me with lies and deceit, but Judah still obeys God and is faithful to the Holy One."
      }
    },
