    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.9.10-17",
            "paragraphs": [
              "九10～17雖然在以色列國開始的時候，神發現他的子民好象\"葡萄在曠野\"（10節）那樣使人心神爽快，但他們卻在\"巴力毗珥\"轉向邪惡（比較民二五），在\"吉甲\"背叛神（15節）；吉甲就是他們宣布掃羅為王的地方（撒上一一15）。巴力的膜拜，和渴望人類君王統治而非神統治兩者的結合，使百姓註定要\"飄流在列國中\"（17節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你给他们—— 你要给他们什么呢？ 求你使他们子宫流产，乳房干瘪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊，求你賜給他們；你要賜給他們甚麼呢？ 求你使他們墜胎小產，乳房萎縮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你加給他們－ 加甚麼呢？ 要使他們胎墜乳乾。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Give them, O Lord— what will you give? Give them a miscarrying womb and dry breasts."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Give them, O Lord—what will You give? Give them a miscarrying womb and dry breasts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Give them, Lord— what will you give them? Give them wombs that miscarry and breasts that are dry."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Give them, O Lord— What will You give? Give them a miscarrying womb And dry breasts!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, what should I request for your people? I will ask for wombs that don’t give birth and breasts that give no milk."
      }
    },
