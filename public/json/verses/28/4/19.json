      "leili": {
        "content": [
          {
            "verse": "hos.4.19",
            "paragraphs": [
              "四19審判的\"風\"會突然和猛烈地吹來。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "风要把他们卷走， 他们必因所献的祭而蒙羞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "風要把他們裹在翅膀裡捲去， 他們必因自己所獻的祭祀而蒙受羞辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "風把他們裹在翅膀裏； 他們因所獻的祭必致蒙羞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wind wraps them in its wings, And they will be ashamed because of their sacrifices."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A whirlwind will sweep them away, and their sacrifices will bring them shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So a mighty wind will sweep them away. Their sacrifices to idols will bring them shame."
      }
