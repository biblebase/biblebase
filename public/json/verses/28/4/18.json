    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.4.18",
            "paragraphs": [
              "四18\"他們所喝的已經發酸\"。更可作：當他們的酒已經喝光了。他們喝完酒便轉向行淫。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们酒足兴尽之后， 便不住地行淫； 他们的首领恋慕羞耻之事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們醉意全消以後， 就不斷行淫； 他們的官長最喜愛恥辱的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們所喝的已經發酸， 他們時常行淫， 他們的官長最愛羞恥的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their liquor gone, They play the harlot continually; Their rulers dearly love shame."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Even when their drinks are gone, they continue their prostitution; their rulers dearly love shameful ways."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the rulers of Israel finish their drinking, off they go to find some prostitutes. They love shame more than honor."
      }
    },
