    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以，我差遣先知用我的话杀戮你们， 我的审判必如闪电临到你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，我藉眾先知砍碎他們， 用我口中的話殺戮他們； 我的審判必如光一般發出。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，我藉先知砍伐他們， 以我口中的話殺戮他們； 我施行的審判如光發出。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore I have hewn them in pieces by the prophets; I have slain them by the words of My mouth; And the judgments on you are like the light that goes forth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore I cut you in pieces with my prophets, I killed you with the words of my mouth— then my judgments go forth like the sun."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I sent my prophets to cut you to pieces— to slaughter you with my words, with judgments as inescapable as light."
      }
    },
