    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "来吧，让我们归向耶和华！ 祂撕裂我们，也必医治我们； 祂击打我们，也必为我们裹伤。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“來吧！讓我們回轉歸向耶和華； 因為，他雖然撕裂了我們，但必定醫治我們； 他雖然擊傷了我們，卻必替我們裹傷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "來吧，我們歸向耶和華！ 他撕裂我們，也必醫治； 他打傷我們，也必纏裹。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Come, let us return to the Lord. For He has torn us, but He will heal us; He has wounded us, but He will bandage us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Come, let us return to the Lord. He has torn us to pieces but he will heal us; he has injured us but he will bind up our wounds."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Come, let us return to the Lord. He has torn us to pieces; now he will heal us. He has injured us; now he will bandage our wounds."
      }
    },
