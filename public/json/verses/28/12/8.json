    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲说：“我发财了， 我依靠自己致富。 没有人能从我的劳碌中找出过错和罪恶。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮說：“我成了富翁， 我為自己發了財； 在我所有的收益中， 人不能找出我有甚麼罪孽，可算為罪惡的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮說： 我果然成了富足，得了財寶； 我所勞碌得來的， 人必不見有甚麼不義，可算為罪的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Ephraim said, “Surely I have become rich, I have found wealth for myself; In all my labors they will find in me No iniquity, which would be sin.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ephraim boasts, “I am very rich; I have become wealthy. With all my wealth they will not find in me any iniquity or sin.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Israel boasts, “I am rich! I’ve made a fortune all by myself! No one has caught me cheating! My record is spotless!”"
      }
    },
