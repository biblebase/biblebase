        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "雅各曾逃往亚兰， 以色列在那里为娶妻而服侍人， 为得到妻子而替人牧羊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從前雅各逃到亞蘭地， 以色列為得妻子而作工， 為得妻子而替人放羊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從前雅各逃到亞蘭地， 以色列為得妻服事人， 為得妻與人放羊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Jacob fled to the land of Aram, And Israel worked for a wife, And for a wife he kept sheep."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jacob fled to the country of Aram; Israel served to get a wife, and to pay for her he tended sheep."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jacob fled to the land of Aram, and there he earned a wife by tending sheep."
      }
