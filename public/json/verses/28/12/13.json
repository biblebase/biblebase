    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华借着先知带领以色列离开埃及， 也借着先知保护他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "後來，耶和華藉著一位先知把以色列從埃及領上來， 也藉著一位先知，以色列得蒙看顧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華藉先知領以色列從埃及上來； 以色列也藉先知而得保存。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "By a prophet the Lord brought Israel up from Egypt, and by a prophet he was guarded."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But by a prophet the Lord brought Israel from Egypt, And by a prophet he was kept."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord used a prophet to bring Israel up from Egypt, by a prophet he cared for him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "By a prophet the Lord brought Israel out of Egypt, And by a prophet he was preserved."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then by a prophet the Lord brought Jacob’s descendants out of Egypt; and by that prophet they were protected."
      }
