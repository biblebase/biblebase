    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "hos.2.11",
            "paragraphs": [
              "2:11",
              "我也必使她的宴樂……都止息了：神本希望以色列人在各聖日聖會中歡喜快樂，享受他的祝福；而他們卻將這種慶賀變成自娛自樂的歡聲和空洞無益的笑語。神所定的節期已不再是他的節期，反成了“她”的(也就是以色列的)，成了徒有其表的宗教儀式所包裹的純粹的社交聚會。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要终止她一切的欢庆， 终止她的年节、朔日、安息日及其他节期。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必使她的一切歡樂、 節期、新月、安息日， 或任何盛會都終止。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我也必使她的宴樂、節期、月朔、安息日， 並她的一切大會都止息了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will also put an end to all her gaiety, Her feasts, her new moons, her sabbaths And all her festal assemblies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will stop all her celebrations: her yearly festivals, her New Moons, her Sabbath days—all her appointed festivals."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will put an end to her annual festivals, her new moon celebrations, and her Sabbath days— all her appointed festivals."
      }
    },
