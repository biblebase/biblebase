    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.2.1-2",
            "paragraphs": [
              "二1,2\"弟兄……姐妹\"指個別的以色列人，而\"母親\"指以色列國。這裏用歌篾的行淫來說明以色列對神的不忠。第2節的用語跟猶太人用來表達休妻的說法相類似。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们要称你们的弟兄为阿米，要称你们的姊妹为路哈玛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們要對你們的兄弟說：‘我的子民’；要對你們的姊妹說：‘蒙憐憫的’。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要稱你們的弟兄為阿米，稱你們的姊妹為路哈瑪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Say to your brothers, “Ammi,” and to your sisters, “Ruhamah.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Say of your brothers, ‘My people,’ and of your sisters, ‘My loved one.’"
      }
