    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "hos.2.14",
            "paragraphs": [
              "2:14",
              "後來我必勸導她：何西阿書是把對審判的警告以及對盼望和復興的應許交織起來而成的信息。前兩次的“因此”(6,9節)都論到審判；現在神卻展示出一幅偉大奇妙的畫面，形成鮮明對比。他仍要照著他的恩典呼召以色列人回轉。正如神當年帶領以色列人出埃及，入曠野，在那裏頒布律法，並引導他們進入了應許之地，現在他要再次帶領以色列人脫離他們罪的埃及，進入新的曠野，並在那裏引導、指教他們，使他們復興。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "hos.2.14",
            "paragraphs": [
              "二14\"說安慰的話\"。直譯作：向她的心說話。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“然而，我要挽回她， 领她到旷野， 柔声安慰她。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“因此，我要誘導她， 領她到曠野， 對她說安慰的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "後來我必勸導她，領她到曠野， 對她說安慰的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Therefore, behold, I will allure her, Bring her into the wilderness And speak kindly to her."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Therefore I am now going to allure her; I will lead her into the wilderness and speak tenderly to her."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But then I will win her back once again. I will lead her into the desert and speak tenderly to her there."
      }
    },
