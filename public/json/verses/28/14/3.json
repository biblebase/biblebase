    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.14.3",
            "paragraphs": [
              "一四3\"亞述\"不會拯救以色列，埃及騎兵的\"馬\"也不能拯救以色列。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们不再向亚述求救， 不再依靠战马， 也不再称自己所造的神像为我们的上帝。 耶和华啊，只有在你那里， 孤儿才能得到怜悯。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞述不能救我們， 我們必不再騎馬； 我們親手所做的， 我們必不再稱為‘我們的　神’； 因為在你那裡，孤兒才得著憐憫。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們不向亞述求救， 不騎埃及的馬， 也不再對我們手所造的說： 你是我們的神。 因為孤兒在你－耶和華那裏得蒙憐憫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Assyria will not save us, We will not ride on horses; Nor will we say again, ‘Our god,’ To the work of our hands; For in You the orphan finds mercy.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Assyria cannot save us; we will not mount warhorses. We will never again say ‘Our gods’ to what our own hands have made, for in you the fatherless find compassion.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Assyria cannot save us, nor can our warhorses. Never again will we say to the idols we have made, ‘You are our gods.’ No, in you alone do the orphans find mercy.”"
      }
    },
