    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要归向耶和华， 向祂忏悔说： “求你赦免我们的罪， 施恩悦纳我们， 好叫我们以赞美为祭献给你！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們預備好要說的話，歸向耶和華， 對他說：“除去我們一切罪孽， 悅納我們的禱告。 這樣我們就把嘴唇的果子獻上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當歸向耶和華， 用言語禱告他說： 求你除淨罪孽，悅納善行； 這樣，我們就把嘴唇的祭代替牛犢獻上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Take words with you and return to the Lord. Say to Him, “Take away all iniquity And receive us graciously, That we may present the fruit of our lips."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Take words with you and return to the Lord. Say to him: “Forgive all our sins and receive us graciously, that we may offer the fruit of our lips."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Bring your confessions, and return to the Lord. Say to him, “Forgive all our sins and graciously receive us, so that we may offer you our praises."
      }
    },
