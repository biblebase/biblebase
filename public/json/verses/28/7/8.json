    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.7.8-9",
            "paragraphs": [
              "七8,9與外邦聯盟使以色列變得虛弱（\"沒有翻過的餅\"）和衰老（\"頭發斑白\"）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以法莲与外族人混杂， 就像没有翻过的饼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“以法蓮與外族混雜， 以法蓮是沒有翻過的餅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮與列邦人攙雜； 以法蓮是沒有翻過的餅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ephraim mixes himself with the nations; Ephraim has become a cake not turned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Ephraim mixes with the nations; Ephraim is a flat loaf not turned over."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The people of Israel mingle with godless foreigners, making themselves as worthless as a half-baked cake!"
      }
    },
