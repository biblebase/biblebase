    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们没有想到我记得他们的所有恶行。 如今他们的罪恶环绕他们， 都暴露在我面前。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們心裡並未想到， 我記得他們的一切罪惡。 他們所作的一切，現在都圍繞他們， 都在我面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們心裏並不思想我記念他們的一切惡； 他們所行的現在纏繞他們，都在我面前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And they do not consider in their hearts That I remember all their wickedness. Now their deeds are all around them; They are before My face."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "but they do not realize that I remember all their evil deeds. Their sins engulf them; they are always before me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Its people don’t realize that I am watching them. Their sinful deeds are all around them, and I see them all."
      }
    },
