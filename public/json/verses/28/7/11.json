    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.7.11-12",
            "paragraphs": [
              "七11,12與\"埃及\"和\"亞述\"聯盟毫無益處，耶和華將要審判以色列。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲像鸽子一样愚蠢无知， 他们向埃及求救，又投奔亚述。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮好像一隻愚蠢無知的鴿子。 他們向埃及求助，投奔亞述。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮好像鴿子愚蠢無知； 他們求告埃及，投奔亞述。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Ephraim has become like a silly dove, without sense; They call to Egypt, they go to Assyria."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Ephraim is like a dove, easily deceived and senseless— now calling to Egypt, now turning to Assyria."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The people of Israel have become like silly, witless doves, first calling to Egypt, then flying to Assyria for help."
      }
    },
