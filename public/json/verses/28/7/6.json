    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的心如烤炉，燃烧着阴谋。 他们的怒气如整夜闷烧的炉火， 早晨爆发出熊熊烈焰。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們陰謀埋伏的時候， 心中燒得熱如火爐。 他們整夜抑制怒氣， 一到早晨，卻像發出火燄的火燃燒起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "首領埋伏的時候，心中熱如火爐， 就如烤餅的整夜睡臥， 到了早晨火氣炎炎。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For their hearts are like an oven As they approach their plotting; Their anger smolders all night, In the morning it burns like a flaming fire."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their hearts are like an oven; they approach him with intrigue. Their passion smolders all night; in the morning it blazes like a flaming fire."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
