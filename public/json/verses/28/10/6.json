    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它要被带到亚述去， 作为贡物献给强大的亚述王。 以法莲因此而受人嘲笑， 以色列因自己的偶像而蒙羞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人要把它帶到亞述去， 作貢物獻給那裡的大王。 以法蓮必受羞辱， 以色列必因自己的計謀蒙羞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人必將牛犢帶到亞述當作禮物， 獻給耶雷布王。 以法蓮必蒙羞； 以色列必因自己的計謀慚愧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The thing itself will be carried to Assyria As tribute to King Jareb; Ephraim will be seized with shame And Israel will be ashamed of its own counsel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It will be carried to Assyria as tribute for the great king. Ephraim will be disgraced; Israel will be ashamed of its foreign alliances."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This idol will be carted away to Assyria, a gift to the great king there. Ephraim will be ridiculed and Israel will be shamed, because its people have trusted in this idol."
      }
    },
