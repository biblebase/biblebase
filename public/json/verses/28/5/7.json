    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.5.7",
            "paragraphs": [
              "五7\"到了月朔，他們與他們的地業必被吞滅\"。或指每個月的月朔是審判的時候，而不是歡樂的日子。參看阿摩司書八章5節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们对耶和华不忠， 所生的是私生子， 他们和他们的田地必在朔日被吞噬。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們以詭詐待耶和華， 因為他們生了私生子。 現在新月要吞滅他們和他們的地業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們向耶和華行事詭詐，生了私子。 到了月朔，他們與他們的地業必被吞滅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have dealt treacherously against the Lord, For they have borne illegitimate children. Now the new moon will devour them with their land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are unfaithful to the Lord; they give birth to illegitimate children. When they celebrate their New Moon feasts, he will devour their fields."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They have betrayed the honor of the Lord, bearing children that are not his. Now their false religion will devour them along with their wealth."
      }
    },
