    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "hos.5.10",
            "paragraphs": [
              "5:10",
              "挪移地界的人：挪移鄰舍地產的界石就等於偷竊他的一片土地(申19:14；27:17)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "hos.5.10",
            "paragraphs": [
              "五10\"挪移地界\"是一項罪（申一九14；二七17）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大的首领如同擅移地界的人， 侵吞以色列的领土， 我要将烈怒如洪水般倾泻在他们身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“猶大的領袖，好像挪移地界的人， 我要把我的忿怒傾倒在他們身上，如同倒水一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大的首領如同挪移地界的人， 我必將忿怒倒在他們身上，如水一般。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The princes of Judah have become like those who move a boundary; On them I will pour out My wrath like water."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Judah’s leaders are like those who move boundary stones. I will pour out my wrath on them like a flood of water."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The leaders of Judah have become like thieves. So I will pour my anger on them like a waterfall."
      }
    },
