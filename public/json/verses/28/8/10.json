    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "hos.8.10",
            "paragraphs": [
              "八10\"重擔\"。\"君王和首領\"指亞述的君主，強迫以色列獻上的貢物，但這樣做並未能買來安全。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尽管他们在各国贿买盟友， 如今我却要把他们聚集起来，审判他们。 他们必因君王和首领的欺压而日渐衰微。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們雖然在列國中賄買人， 我現在卻要招聚他們。 他們因著君王和領袖所加的重擔， 逐漸衰微。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們雖在列邦中賄買人， 現在我卻要聚集懲罰他們； 他們因君王和首領所加的重擔日漸衰微。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Though they hire allies among the nations, I will soon gather them up. And the king and princes shall soon writhe because of the tribute."
      },
