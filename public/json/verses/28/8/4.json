    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们拥立君王，未经我的同意； 选立首领，没有让我知道。 他们用金银为自己制造神像，自取灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“他們立了君王，卻不是出於我； 他們立了首領，我卻不知道。 他們用自己的金銀為自己做了偶像， 以致他們被剪除。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們立君王，卻不由我； 他們立首領，我卻不認。 他們用金銀為自己製造偶像， 以致被剪除。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have set up kings, but not by Me; They have appointed princes, but I did not know it. With their silver and gold they have made idols for themselves, That they might be cut off."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They set up kings without my consent; they choose princes without my approval. With their silver and gold they make idols for themselves to their own destruction."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people have appointed kings without my consent, and princes without my approval. By making idols for themselves from their silver and gold, they have brought about their own destruction."
      }
    },
