      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这牛犊是以色列的工匠造的， 并不是神，必被砸得粉碎。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這牛犢是出於以色列， 是匠人所做的，它並不是神， 撒瑪利亞的牛犢必被粉碎。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這牛犢出於以色列， 是匠人所造的，並不是神。 撒馬利亞的牛犢必被打碎。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For from Israel is even this! A craftsman made it, so it is not God; Surely the calf of Samaria will be broken to pieces."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They are from Israel! This calf—a metalworker has made it; it is not God. It will be broken in pieces, that calf of Samaria."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This calf you worship, O Israel, was crafted by your own hands! It is not God! Therefore, it must be smashed to bits."
      }
