    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果当权的人向你大发雷霆， 不要因此就离开岗位， 因为平心静气能避免大错。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果掌權者向你發怒，你不要離開原位，因為柔順能免大過。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "如果管辖者的怒气向你上腾，你不要离开你的本位， 因为平静能止住极大的罪过。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掌權者的心若向你發怒， 不要離開你的本位， 因為柔和能免大過。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If the anger of the ruler rises against you, do not leave your place, for calmness will lay great offenses to rest."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If the ruler’s temper rises against you, do not abandon your position, because composure allays great offenses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If a ruler’s anger rises against you, do not leave your post; calmness can lay great offenses to rest."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "If the spirit of the ruler rises against you, Do not leave your post; For conciliation pacifies great offenses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If your boss is angry at you, don’t quit! A quiet spirit can overcome even great mistakes."
      }
    },
