    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "ecc.10.8",
            "paragraphs": [
              "10:8",
              "8~10挖陷坑的：智慧使人思慮人生所遇的風險、艱難以及當承擔的基本責任。智慧人能不受損害是因為他知道將要發生的事，並謹慎不落入陷坑。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "挖掘陷阱的，自己必掉在其中； 拆围墙的，必被蛇咬；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "挖陷阱的，自己掉進去；拆牆垣的，有蛇來咬他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "挖陷坑的，自己必掉在其中； 拆牆垣的，必為蛇所咬。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who digs a pit may fall into it, and a serpent may bite him who breaks through a wall."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever digs a pit may fall into it; whoever breaks through a wall may be bitten by a snake."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When you dig a well, you might fall in. When you demolish an old wall, you could be bitten by a snake."
      }
    },
