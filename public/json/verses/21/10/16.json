    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "ecc.10.16",
            "paragraphs": [
              "10:16",
              "你的王若是孩童：若君王和領袖幼稚無知(參賽3:4~5,12)，而他們的王子群臣清早就吃喝宴樂(也許指開酒會)，這樣的事情實在令人悲哀。然而，可悲的是，歷世歷代以來，酒都肆意滲透在世界各國的京城。我們需要的是以身作則的敬虔好領袖(17節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一国之君若年幼无知， 他的臣宰从早到晚只顾宴乐， 那国就有祸了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "邦國啊，如果你的王是個孩童，而你的大臣又一早吃喝宴樂，你就有禍了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "邦國啊，你的王若是孩童， 你的羣臣早晨宴樂， 你就有禍了！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe to you, O land, whose king is a lad and whose princes feast in the morning."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Woe to the land whose king was a servant and whose princes feast in the morning."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What sorrow for the land ruled by a servant, the land whose leaders feast in the morning."
      }
