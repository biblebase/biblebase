    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一国之君若英明尊贵， 他的臣宰为了强身健体而节制饮食， 不酗酒宴乐，那国就有福了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "邦國啊，如果你的王是貴冑之子，而你的大臣又按時吃喝，為要得力，不為醉酒，你就有福了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "邦國啊，你的王若是貴冑之子， 你的羣臣按時吃喝， 為要補力，不為酒醉， 你就有福了！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Blessed are you, O land, whose king is of nobility and whose princes eat at the appropriate time—for strength and not for drunkenness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Blessed is the land whose king is of noble birth and whose princes eat at a proper time— for strength and not for drunkenness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Happy is the land whose king is a noble leader and whose leaders feast at the proper time to gain strength for their work, not to get drunk."
      }
    },
