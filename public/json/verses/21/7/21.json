    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "ecc.7.21",
            "paragraphs": [
              "21.人類的罪特別可從話語的不可靠看出（參雅三2）。結論是對別人的懷恨不必太過註意（參撒上廿四9），因為會擾亂我們的平靜。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "ecc.7.21-22",
            "paragraphs": [
              "七21,22面對恥辱的時候，人需要有自制。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你不要斤斤计较别人所说的每一句话，免得听见你的仆人咒诅你，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人所說的一切話，都不要放在心上，因你可能聽見僕人咒罵你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人所說的一切話，你不要放在心上，恐怕聽見你的僕人咒詛你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Also, do not take seriously all words which are spoken, so that you will not hear your servant cursing you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not pay attention to every word people say, or you may hear your servant cursing you—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t eavesdrop on others—you may hear your servant curse you."
      }
    },
