      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们一生充满痛苦，劳碌中尽是愁烦，即使夜间心里也不安宁。这也是虚空。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "其實終其一生，他的擔子既痛苦又煩惱，在夜間也不能安心。這也是虛空。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他日日憂慮，他的勞苦成為愁煩，連夜間心也不安。這也是虛空。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because all his days his task is painful and grievous; even at night his mind does not rest. This too is vanity."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All their days their work is grief and pain; even at night their minds do not rest. This too is meaningless."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their days of labor are filled with pain and grief; even at night their minds cannot rest. It is all meaningless."
      }
