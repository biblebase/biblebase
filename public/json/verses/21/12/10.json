    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ecc.12.8-14",
            "paragraphs": [
              "一二8～14本書的結語記載其主題（8節）：所羅門的教育計劃（9,10節）、教訓的價值（11,12節）和他結束的勸勉（13,14節）。"
            ]
          },
          {
            "verse": "ecc.12.10",
            "paragraphs": [
              "一二10\"憑正直寫的誠實話\"。所羅門保證他所寫的都是正直和真實的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "传道者费尽心思寻找金玉良言，所写的都是正直诚实的道理。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "傳道者搜尋可喜悅的話，正確地寫下真理。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "傳道者專心尋求可喜悅的言語，是憑正直寫的誠實話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Preacher sought to find delightful words and to write words of truth correctly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Teacher searched to find just the right words, and what he wrote was upright and true."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Teacher sought to find just the right words to express truths clearly."
      }
