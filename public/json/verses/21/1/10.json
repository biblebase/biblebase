    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人可以指着哪件事说： “看啊，这是新事”？ 所谓的新事在我们以前早就有了。"
      },
      "cnv": {
