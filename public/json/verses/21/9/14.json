    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有一个势力强大的君王来攻击一个人口不多的小城，他建造营垒围困这城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一座小城，人數不多；有一個大君王來進攻，把它包圍，建造巨大的高壘攻城。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是有一小城，其中的人數稀少，有大君王來攻擊，修築營壘，將城圍困。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There was a small city with few men in it and a great king came to it, surrounded it and constructed large siegeworks against it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There was once a small city with only a few people in it. And a powerful king came against it, surrounded it and built huge siege works against it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There was a small town with only a few people, and a great king came with his army and besieged it."
      }
