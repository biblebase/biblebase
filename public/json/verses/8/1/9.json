    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "rut.1.6-22",
            "paragraphs": [
              "Ⅱ回歸猶大地（一6～22）"
            ]
          },
          {
            "verse": "rut.1.6-18",
            "paragraphs": [
              "Ａ路得的忠誠（一6～18）",
              "摩押地對拿俄米而言，再無可留戀之處。她的全家因躲避猶大地的饑荒來到摩押，現在她聽說饑荒已經退出，因此起了返鄉的念頭。這使得她的兒媳們必須面對並作出抉擇。對路得而言，她因此作出了一偉大、忠誠的宣告。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "rut.1.9",
            "paragraphs": [
              "一9\"得平安\"。即找到另一個丈夫。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿耶和华使你们都有机会再嫁，找到好归宿。”拿俄米说完了，就亲吻她们。她们都放声大哭，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願耶和華使你們各人有機會再嫁，找到歸宿。”跟著親吻她們，她們就發聲大哭，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願耶和華使你們各在新夫家中得平安！」於是拿俄米與她們親嘴。她們就放聲而哭，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May the Lord grant that you may find rest, each in the house of her husband.” Then she kissed them, and they lifted up their voices and wept."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the Lord grant that each of you will find rest in the home of another husband.” Then she kissed them goodbye and they wept aloud"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May the Lord bless you with the security of another marriage.” Then she kissed them good-bye, and they all broke down and wept."
      }
    },
