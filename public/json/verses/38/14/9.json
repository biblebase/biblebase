    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华必做普世的王。到那天，必唯祂独尊，唯祂的名独尊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必作全地的王；到那日，人人都承認耶和華是獨一無二的，他的名也是獨一無二的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華必作全地的王。那日耶和華必為獨一無二的，他的名也是獨一無二的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the Lord will be king over all the earth; in that day the Lord will be the only one, and His name the only one."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will be king over the whole earth. On that day there will be one Lord, and his name the only name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the Lord will be king over all the earth. On that day there will be one Lord—his name alone will be worshiped."
      }
