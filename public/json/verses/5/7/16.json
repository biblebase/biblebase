    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们必须灭绝你们的上帝耶和华交在你们手里的各族，不可怜悯他们，不可拜他们的神明，因为那样会使你们陷入网罗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華你的　神交給你的所有民族，你都要把他們消滅；你的眼不可憐惜他們；你也不可事奉他們的神，因為這必成為你的網羅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－你神所要交給你的一切人民，你要將他們除滅；你眼不可顧惜他們。你也不可事奉他們的神，因這必成為你的網羅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall consume all the peoples whom the Lord your God will deliver to you; your eye shall not pity them, nor shall you serve their gods, for that would be a snare to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You must destroy all the peoples the Lord your God gives over to you. Do not look on them with pity and do not serve their gods, for that will be a snare to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must destroy all the nations the Lord your God hands over to you. Show them no mercy, and do not worship their gods, or they will trap you."
      }
