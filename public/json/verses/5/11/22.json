    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你们谨遵我吩咐你们的这一切诫命——爱你们的上帝耶和华，遵行祂的旨意，依靠祂，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們殷勤謹守遵行我吩咐你們的這一切誡命，愛耶和華你們的　神，行他的一切道路，又緊緊地依靠他，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若留意謹守遵行我所吩咐這一切的誡命，愛耶和華－你們的神，行他的道，專靠他，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if you are careful to keep all this commandment which I am commanding you to do, to love the Lord your God, to walk in all His ways and hold fast to Him,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you carefully observe all these commands I am giving you to follow—to love the Lord your God, to walk in obedience to him and to hold fast to him—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Be careful to obey all these commands I am giving you. Show love to the Lord your God by walking in his ways and holding tightly to him."
      }
    },
