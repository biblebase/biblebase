    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们攻占了西宏的所有城邑，将其夷为平地，杀光城中的男女老幼，一个不留，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時我們奪取了他所有的城邑，把每一座城，連男帶女，以及小孩子都全部毀滅，沒有留下一個存活的；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們奪了他的一切城邑，將有人煙的各城，連女人帶孩子，盡都毀滅，沒有留下一個。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And we captured all his cities at that time and devoted to destruction every city, men, women, and children. We left no survivors."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So we captured all his cities at that time and utterly destroyed the men, women and children of every city. We left no survivor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At that time we took all his towns and completely destroyed them—men, women and children. We left no survivors."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "We took all his cities at that time, and we utterly destroyed the men, women, and little ones of every city; we left none remaining."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We conquered all his towns and completely destroyed everyone—men, women, and children. Not a single person was spared."
      }
