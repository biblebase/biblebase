    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从亚嫩谷边的亚罗珥和谷中的城，直到基列的所有城邑全被我们攻克，我们的上帝耶和华把这一切都交给了我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從亞嫩谷旁邊的亞羅珥和谷中的城，直到基列，沒有一座城太高，是我們不能攻下的；耶和華我們的　神把這一切都交給我們了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從亞嫩谷邊的亞羅珥和谷中的城，直到基列，耶和華－我們的神都交給我們了，沒有一座城高得使我們不能攻取的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From Aroer which is on the edge of the valley of Arnon and from the city which is in the valley, even to Gilead, there was no city that was too high for us; the Lord our God delivered all over to us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From Aroer on the rim of the Arnon Gorge, and from the town in the gorge, even as far as Gilead, not one town was too strong for us. The Lord our God gave us all of them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The Lord our God also helped us conquer Aroer on the edge of the Arnon Gorge, and the town in the gorge, and the whole area as far as Gilead. No town had walls too strong for us."
      }
    },
