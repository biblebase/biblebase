      "dde": {
        "content": [
          {
            "verse": "deu.8.11-20",
            "paragraphs": [
              "b.對驕傲和忘恩的警告（八11～20）"
            ]
          },
          {
            "verse": "deu.8.11",
            "paragraphs": [
              "11.神既然這樣恩待以色列，以色列就有回報這恩情的重責。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们要谨慎，免得忘记你们的上帝耶和华，不遵守我今天吩咐你们的诫命、典章和律例。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要自己謹慎，免得忘記耶和華你的　神，不守他的誡命、典章、律例，就是我今日吩咐你的；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你要謹慎，免得忘記耶和華－你的神，不守他的誡命、典章、律例，就是我今日所吩咐你的；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Beware that you do not forget the Lord your God by not keeping His commandments and His ordinances and His statutes which I am commanding you today;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be careful that you do not forget the Lord your God, failing to observe his commands, his laws and his decrees that I am giving you this day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But that is the time to be careful! Beware that in your plenty you do not forget the Lord your God and disobey his commands, regulations, and decrees that I am giving you today."
      }
    },
