    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶和华见自己的子民力量衰竭， 奴隶和自由人所剩无几， 必为他们主持公道， 怜悯祂的仆人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華要為自己的子民伸冤， 為自己的僕人難過， 因為耶和華看見他們的能力已經消逝， 為奴的或自由的都沒有剩下一人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華見他百姓毫無能力， 無論困住的、自由的都沒有剩下， 就必為他們伸冤， 為他的僕人後悔。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord will vindicate His people, And will have compassion on His servants, When He sees that their strength is gone, And there is none remaining, bond or free."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will vindicate his people and relent concerning his servants when he sees their strength is gone and no one is left, slave or free."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Indeed, the Lord will give justice to his people, and he will change his mind about his servants, when he sees their strength is gone and no one is left, slave or free."
      }
    },
