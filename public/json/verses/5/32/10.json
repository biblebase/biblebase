    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“在荒凉的旷野， 在野兽吼叫的荒野， 耶和华找到他们， 庇护他们，看顾他们， 保护他们如同保护眼中的瞳仁，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華在曠野之地遇見了他， 在荒涼之地和野獸吼叫的荒野，遇見了他， 就環繞他，看顧他， 保護他，好像保護自己眼中的瞳人一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華遇見他在曠野－ 荒涼野獸吼叫之地， 就環繞他，看顧他， 保護他，如同保護眼中的瞳人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He found him in a desert land, And in the howling waste of a wilderness; He encircled him, He cared for him, He guarded him as the pupil of His eye."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In a desert land he found him, in a barren and howling waste. He shielded him and cared for him; he guarded him as the apple of his eye,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He found them in a desert land, in an empty, howling wasteland. He surrounded them and watched over them; he guarded them as he would guard his own eyes."
      }
    },
