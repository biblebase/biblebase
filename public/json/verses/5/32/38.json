    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它们吃你们祭牲的脂肪， 喝你们的奠酒。 让它们起来帮助你们， 庇护你们吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就是向來吃他們祭牲脂肪的， 喝他們奠祭之酒的神在哪裡呢？ 讓他們興起來幫助你們吧， 願他們保護你們吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是向來吃他們祭牲的脂油， 喝他們奠祭之酒的，在哪裏呢？ 他可以興起幫助你們， 護衛你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who ate the fat of their sacrifices, And drank the wine of their drink offering? Let them rise up and help you, Let them be your hiding place!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the gods who ate the fat of their sacrifices and drank the wine of their drink offerings? Let them rise up to help you! Let them give you shelter!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Where now are those gods, who ate the fat of their sacrifices and drank the wine of their offerings? Let those gods arise and help you! Let them provide you with shelter!"
      }
    },
