    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "世上有谁像我们一样听见永活上帝从火中说话后还能活着呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為有血肉生命的人，有誰像我們一樣，聽見永活的　神從火中說話的聲音，還能活著呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡屬血氣的，曾有何人聽見永生神的聲音從火中出來，像我們聽見還能存活呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For who is there of all flesh who has heard the voice of the living God speaking from the midst of the fire, as we have, and lived?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For what mortal has ever heard the voice of the living God speaking out of fire, as we have, and survived?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can any living thing hear the voice of the living God from the heart of the fire as we did and yet survive?"
      }
