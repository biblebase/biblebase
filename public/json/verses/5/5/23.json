    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当时，山上燃烧着烈焰，你们听见声音从黑暗中传来。你们各支派的首领和长老都到我跟前，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時山上被火燒著，你們聽見有聲音從黑暗中發出來的時候，你們各支派的首領和你們的長老，都走到我這裡來，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「那時，火焰燒山，你們聽見從黑暗中出來的聲音；你們支派中所有的首領和長老都來就近我，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And as soon as you heard the voice out of the midst of the darkness, while the mountain was burning with fire, you came near to me, all the heads of your tribes, and your elders."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And when you heard the voice from the midst of the darkness, while the mountain was burning with fire, you came near to me, all the heads of your tribes and your elders."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you heard the voice out of the darkness, while the mountain was ablaze with fire, all the leaders of your tribes and your elders came to me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“So it was, when you heard the voice from the midst of the darkness, while the mountain was burning with fire, that you came near to me, all the heads of your tribes and your elders."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But when you heard the voice from the heart of the darkness, while the mountain was blazing with fire, all your tribal leaders and elders came to me."
      }
