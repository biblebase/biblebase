    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们带回来一些那里的果子，禀告说，‘我们的上帝耶和华赐给我们的地方很好。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們手裡拿著那地的果子，帶下來到我們這裡，並且向我們回報，說：‘耶和華我們的　神要賜給我們的地是美地。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們手裏拿着那地的果子下來，到我們那裏，回報說：『耶和華－我們的神所賜給我們的是美地。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they took some of the fruit of the land in their hands and brought it down to us; and they brought us back a report and said, ‘It is a good land which the Lord our God is about to give us.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Taking with them some of the fruit of the land, they brought it down to us and reported, “It is a good land that the Lord our God is giving us.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They picked some of its fruit and brought it back to us. And they reported, ‘The land the Lord our God has given us is indeed a good land.’"
      }
