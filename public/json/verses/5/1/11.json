    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿你们祖先的上帝耶和华使你们人数更增千倍，照祂的应许赐福给你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願耶和華你們列祖的　神，使你們比現在更多千倍，又照著他應許你們的話，賜福給你們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟願耶和華－你們列祖的神使你們比如今更多千倍，照他所應許你們的話賜福與你們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May the Lord, the God of your fathers, increase you a thousand-fold more than you are and bless you, just as He has promised you!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the Lord, the God of your ancestors, increase you a thousand times and bless you as he has promised!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And may the Lord, the God of your ancestors, multiply you a thousand times more and bless you as he promised!"
      }
