      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the sons of Israel wept for Moses in the plains of Moab thirty days; then the days of weeping and mourning for Moses came to an end."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Israelites grieved for Moses in the plains of Moab thirty days, until the time of weeping and mourning was over."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people of Israel mourned for Moses on the plains of Moab for thirty days, until the customary period of mourning was over."
      }
