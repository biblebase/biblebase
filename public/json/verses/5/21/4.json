    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "去有溪流但无人耕种过的山谷，在那里把它的颈项打断。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那城的長老要把母牛犢牽下去，到一個有活水長流的山谷，就是未曾耕種過、未曾撒過種的地方，在那裡要打折母牛犢的頸項。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "把母牛犢牽到流水、未曾耕種的山谷去，在谷中打折母牛犢的頸項。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and the elders of that city shall bring the heifer down to a valley with running water, which has not been plowed or sown, and shall break the heifer’s neck there in the valley."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and lead it down to a valley that has not been plowed or planted and where there is a flowing stream. There in the valley they are to break the heifer’s neck."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They must lead it down to a valley that has not been plowed or planted and that has a stream running through it. There in the valley they must break the heifer’s neck."
      }
    },
