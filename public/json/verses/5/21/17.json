    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他必须承认他不爱之妻所生的长子的名分，分给他双份产业，因为长子是他壮年时所生的儿子，应享有长子的权利。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "卻要承認他不喜愛的妻子的兒子為長子，把所有的產業分兩分給他，因為這兒子是他強壯有力的時候所生的頭生子，長子的權利本是屬於他的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "卻要認所惡之妻生的兒子為長子，將產業多加一分給他；因這兒子是他力量強壯的時候生的，長子的名分本當歸他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But he shall acknowledge the firstborn, the son of the unloved, by giving him a double portion of all that he has, for he is the beginning of his strength; to him belongs the right of the firstborn."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He must acknowledge the son of his unloved wife as the firstborn by giving him a double share of all he has. That son is the first sign of his father’s strength. The right of the firstborn belongs to him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He must recognize the rights of his oldest son, the son of the wife he does not love, by giving him a double portion. He is the first son of his father’s virility, and the rights of the firstborn belong to him."
      }
