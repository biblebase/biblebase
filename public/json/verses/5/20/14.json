    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但可以留下城中的妇女、孩童、牲畜及其他一切作战利品。你们可以享用仇敌的财物，那是你们的上帝耶和华赐给你们的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只有婦人、小孩子、牲畜和城裡所有的一切，就是一切戰利品，你都可以據為己有，你的仇敵的戰利品，你都可以吃用，因為這是耶和華你的　神賜給你的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟有婦女、孩子、牲畜，和城內一切的財物，你可以取為自己的掠物。耶和華－你神把你仇敵的財物賜給你，你可以吃用。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "but the women and the little ones, the livestock, and everything else in the city, all its spoil, you shall take as plunder for yourselves. And you shall enjoy the spoil of your enemies, which the Lord your God has given you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Only the women and the children and the animals and all that is in the city, all its spoil, you shall take as booty for yourself; and you shall use the spoil of your enemies which the Lord your God has given you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As for the women, the children, the livestock and everything else in the city, you may take these as plunder for yourselves. And you may use the plunder the Lord your God gives you from your enemies."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But the women, the little ones, the livestock, and all that is in the city, all its spoil, you shall plunder for yourself; and you shall eat the enemies’ plunder which the Lord your God gives you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you may keep for yourselves all the women, children, livestock, and other plunder. You may enjoy the plunder from your enemies that the Lord your God has given you."
      }
