    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但耶和华至今还没有赐给你们能明白的心、能看见的眼、能听清的耳。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是直到今日，耶和華還沒有給你們一顆能明白的心，能看見的眼睛，能聽見的耳朵。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但耶和華到今日沒有使你們心能明白，眼能看見，耳能聽見。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet to this day the Lord has not given you a heart to know, nor eyes to see, nor ears to hear."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But to this day the Lord has not given you a mind that understands or eyes that see or ears that hear."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But to this day the Lord has not given you minds that understand, nor eyes that see, nor ears that hear!"
      }
