    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们中间，不论男人或女人，家族或支派，切不可有人背弃我们的上帝耶和华，去祭拜列国的神明，免得你们中间有残根长出苦毒的果实，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "恐怕你們中間有男人或女人，家族或支派，今日心裡偏離了耶和華我們的　神，去事奉那些國的神，又恐怕你們中間有惡根，生出毒草和苦堇來；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟恐你們中間，或男或女，或族長或支派長，今日心裏偏離耶和華－我們的神，去事奉那些國的神；又怕你們中間有惡根生出苦菜和茵蔯來，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "so that there will not be among you a man or woman, or family or tribe, whose heart turns away today from the Lord our God, to go and serve the gods of those nations; that there will not be among you a root bearing poisonous fruit and wormwood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Make sure there is no man or woman, clan or tribe among you today whose heart turns away from the Lord our God to go and worship the gods of those nations; make sure there is no root among you that produces such bitter poison."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am making this covenant with you so that no one among you—no man, woman, clan, or tribe—will turn away from the Lord our God to worship these gods of other nations, and so that no root among you bears bitter and poisonous fruit."
      }
