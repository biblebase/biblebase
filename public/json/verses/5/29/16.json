    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们知道我们如何在埃及生活，也知道我们如何穿越列国来到这里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們清楚知道我們怎樣在埃及地住過，怎樣從列國中經過；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我們曾住過埃及地，也從列國經過；這是你們知道的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "(for you know how we lived in the land of Egypt, and how we came through the midst of the nations through which you passed;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You yourselves know how we lived in Egypt and how we passed through the countries on the way here."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You remember how we lived in the land of Egypt and how we traveled through the lands of enemy nations as we left."
      }
