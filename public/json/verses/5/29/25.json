    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "必有人回答说，‘因为这地方的人背弃了他们祖先的上帝耶和华领他们出埃及时与他们所立的约，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人就必回答：‘是因為他們離棄了耶和華他們列祖的　神的約，就是耶和華把他們從埃及地領出來的時候，與他們所立的；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人必回答說：『是因這地的人離棄了耶和華－他們列祖的神，領他們出埃及地的時候與他們所立的約，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then men will say, ‘Because they forsook the covenant of the Lord, the God of their fathers, which He made with them when He brought them out of the land of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the answer will be: “It is because this people abandoned the covenant of the Lord, the God of their ancestors, the covenant he made with them when he brought them out of Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And the answer will be, ‘This happened because the people of the land abandoned the covenant that the Lord, the God of their ancestors, made with them when he brought them out of the land of Egypt."
      }
