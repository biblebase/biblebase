    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万国必问，‘耶和华为什么要这样对待这地方？祂为什么大发烈怒？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看見了這些的人和萬國的人都要問：‘耶和華為甚麼向這地這樣行呢？為甚麼大發烈怒呢？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所看見的人，連萬國人，都必問說：『耶和華為何向此地這樣行呢？這樣大發烈怒是甚麼意思呢？』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the nations will say, ‘Why has the Lord done thus to this land? Why this great outburst of anger?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the nations will ask: “Why has the Lord done this to this land? Why this fierce, burning anger?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And all the surrounding nations will ask, ‘Why has the Lord done this to this land? Why was he so angry?’"
      }
