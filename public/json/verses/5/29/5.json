    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "四十年来，我领你们走过旷野，你们身上的衣服没有穿破，脚上的鞋子没有穿坏；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我在曠野四十年之久領導你們；你們身上的衣服沒有穿破，腳上的鞋也沒有穿壞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我領你們在曠野四十年，你們身上的衣服並沒有穿破，腳上的鞋也沒有穿壞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have led you forty years in the wilderness; your clothes have not worn out on you, and your sandal has not worn out on your foot."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet the Lord says, “During the forty years that I led you through the wilderness, your clothes did not wear out, nor did the sandals on your feet."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For forty years I led you through the wilderness, yet your clothes and sandals did not wear out."
      }
    },
