    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们今天已经宣称耶和华是你们的上帝，答应要听从祂，行祂的道，遵守祂的一切律例、诫命和典章。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你今日承認了耶和華是你的　神，要遵行他的道，謹守他的律例、誡命和典章，聽從他的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你今日認耶和華為你的神，應許遵行他的道，謹守他的律例、誡命、典章，聽從他的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have today declared the Lord to be your God, and that you would walk in His ways and keep His statutes, His commandments and His ordinances, and listen to His voice."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have declared this day that the Lord is your God and that you will walk in obedience to him, that you will keep his decrees, commands and laws—that you will listen to him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have declared today that the Lord is your God. And you have promised to walk in his ways, and to obey his decrees, commands, and regulations, and to do everything he tells you."
      }
    },
