    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“如果你们的希伯来同胞，不论男女，卖身给你们做奴隶，他们要服侍你们六年，到第七年，你们要给他们自由。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你的兄弟，無論是希伯來男人，或是希伯來女人，如果賣身給你，要服事你六年；到第七年，就要讓他離開你得自由。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你弟兄中，若有一個希伯來男人或希伯來女人被賣給你，服事你六年，到第七年就要任他自由出去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If your kinsman, a Hebrew man or woman, is sold to you, then he shall serve you six years, but in the seventh year you shall set him free."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If any of your people—Hebrew men or women—sell themselves to you and serve you six years, in the seventh year you must let them go free."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If a fellow Hebrew sells himself or herself to be your servant and serves you for six years, in the seventh year you must set that servant free."
      }
    },
