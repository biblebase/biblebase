    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要慷慨给予，并且无怨无悔，因为耶和华必在你们所做的一切事上赐福给你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你必須給他，給他的時候，你不要心裡難受，因為為了這事，耶和華你的　神必在你一切工作上，和你所辦的一切事上，賜福給你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你總要給他，給他的時候心裏不可愁煩；因耶和華－你的神必在你這一切所行的，並你手裏所辦的事上，賜福與你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall generously give to him, and your heart shall not be grieved when you give to him, because for this thing the Lord your God will bless you in all your work and in all your undertakings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Give generously to them and do so without a grudging heart; then because of this the Lord your God will bless you in all your work and in everything you put your hand to."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give generously to the poor, not grudgingly, for the Lord your God will bless you in everything you do."
      }
    },
