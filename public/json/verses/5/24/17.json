    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "deu.24.17",
            "paragraphs": [
              "24:17",
              "寄居的和孤兒……寡婦：神特別眷顧孤兒和寡婦以及逃難寄居的人(出22:21~22；23:9)。神非常喜悅我們幫助那些處境不佳的人(參路7:13註；來13:2；雅1:27)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们要公正地对待寄居者和孤儿，不可拿寡妇的衣物作抵押。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你不可向寄居的和孤兒屈枉正直，也不可拿寡婦的衣服作抵押。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你不可向寄居的和孤兒屈枉正直，也不可拿寡婦的衣裳作當頭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall not pervert the justice due an alien or  an orphan, nor take a widow’s garment in pledge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not deprive the foreigner or the fatherless of justice, or take the cloak of the widow as a pledge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“True justice must be given to foreigners living among you and to orphans, and you must never accept a widow’s garment as security for her debt."
      }
    },
