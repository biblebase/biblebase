    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "七周后，要为你们的上帝耶和华守七七收获节。那时，你们要按照你们的上帝耶和华所赐的福分，甘心乐意地献上祭物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要向耶和華你的　神舉行七七節，照著耶和華你的　神賜福你的，盡你所能的獻上你手裡的甘心祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要照耶和華－你神所賜你的福，手裏拿着甘心祭，獻在耶和華－你的神面前，守七七節。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then you shall celebrate the Feast of Weeks to the Lord your God with a tribute of a freewill offering of your hand, which you shall give just as the Lord your God blesses you;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then celebrate the Festival of Weeks to the Lord your God by giving a freewill offering in proportion to the blessings the Lord your God has given you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then celebrate the Festival of Harvest to honor the Lord your God. Bring him a voluntary offering in proportion to the blessings you have received from him."
      }
