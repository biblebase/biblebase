        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞捫人或摩押人不可進耶和華的會；他們的後代直到第十代，也永遠不可進耶和華的會。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「亞捫人或是摩押人不可入耶和華的會；他們的子孫，雖過十代，也永不可入耶和華的會。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No Ammonite or Moabite shall enter the assembly of the Lord; none of their descendants, even to the tenth generation, shall ever enter the assembly of the Lord,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No Ammonite or Moabite or any of their descendants may enter the assembly of the Lord, not even in the tenth generation."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“No Ammonite or Moabite or any of their descendants for ten generations may be admitted to the assembly of the Lord."
      }
