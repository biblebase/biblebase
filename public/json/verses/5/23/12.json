    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "deu.23.9-14",
            "paragraphs": [
              "i.營中的潔凈（二十三9～14）",
              "本段的資料與二十章及其他討論聖戰的段落同屬一類。執行聖戰時，必須滿足某些祭禮上的要求。在營中與自己子民同在，又和他們一同出戰打敗敵人的耶和華，要求除盡一切不潔之物，免得祂離營而去。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要在营外指定一个地方作方便之处。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你在營外要有廁所，你可以出到那裡去便溺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你在營外也該定出一個地方作為便所。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall also have a place outside the camp and go out there,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Designate a place outside the camp where you can go to relieve yourself."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must have a designated area outside the camp where you can go to relieve yourself."
      }
