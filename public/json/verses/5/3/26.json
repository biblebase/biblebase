    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但因你们的缘故，耶和华向我发怒，不听我的恳求，祂对我说，‘够了，不要再跟我提这件事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但耶和華為了你們的緣故，向我發怒，不聽我的懇求；耶和華對我說：‘罷了，不要向我再提這事了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但耶和華因你們的緣故向我發怒，不應允我，對我說：『罷了！你不要向我再提這事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the Lord was angry with me on your account, and would not listen to me; and the Lord said to me, ‘Enough! Speak to Me no more of this matter."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But because of you the Lord was angry with me and would not listen to me. “That is enough,” the Lord said. “Do not speak to me anymore about this matter."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But the Lord was angry with me because of you, and he would not listen to me. ‘That’s enough!’ he declared. ‘Speak of it no more."
      }
    },
