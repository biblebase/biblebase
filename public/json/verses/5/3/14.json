    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛拿西的子孙雅珥得到亚珥歌伯全境，远至基述人和玛迦人的边界。他按自己的名字给那地方取名叫哈倭特·雅珥，沿用至今。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪拿西的兒子睚珥，佔了亞珥歌全境，直到基述人和瑪迦人的邊界，就按著自己的名字稱巴珊全境為哈倭特．睚珥，直到今日。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瑪拿西的子孫睚珥佔了亞珥歌伯全境，直到基述人和瑪迦人的交界，就按自己的名稱這巴珊地為哈倭特‧睚珥，直到今日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jair the son of Manasseh took all the region of Argob as far as the border of the Geshurites and the Maacathites, and called it, that is, Bashan, after his own name, Havvoth-jair, as it is to this day.)"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jair, a descendant of Manasseh, took the whole region of Argob as far as the border of the Geshurites and the Maakathites; it was named after him, so that to this day Bashan is called Havvoth Jair.)"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jair, a leader from the tribe of Manasseh, conquered the whole Argob region in Bashan, all the way to the border of the Geshurites and Maacathites. Jair renamed this region after himself, calling it the Towns of Jair, as it is still known today.)"
      }
    },
