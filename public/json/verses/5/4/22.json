    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我将死在约旦河这边，过不了约旦河。但你们必过去得到那片佳美之地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我只有死在此地，不得過約旦河；你們卻可以過去，取得那美地作產業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我只得死在這地，不能過約旦河；但你們必過去得那美地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I will die in this land, I shall not cross the Jordan, but you shall cross and take possession of this good land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will die in this land; I will not cross the Jordan; but you are about to cross over and take possession of that good land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will cross the Jordan to occupy the land, but I will not. Instead, I will die here on the east side of the river."
      }
