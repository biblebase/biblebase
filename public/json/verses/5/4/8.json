    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有哪个伟大的民族拥有我今天所传授给你们的如此公义的律例和典章？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又哪一個大國有這樣公義的律例和典章，像我今日在你面前頒布的這一切律法呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又哪一大國有這樣公義的律例典章、像我今日在你們面前所陳明的這一切律法呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And what great nation is there, that has statutes and rules so righteous as all this law that I set before you today?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Or what great nation is there that has statutes and judgments as righteous as this whole law which I am setting before you today?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And what other nation is so great as to have such righteous decrees and laws as this body of laws I am setting before you today?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And what great nation is  there that has such statutes and righteous judgments as are in all this law which I set before you this day?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And what great nation has decrees and regulations as righteous and fair as this body of instructions that I am giving you today?"
      }
