    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为祂爱你们的祖先，所以拣选你们，亲自用大能把你们从埃及领出来，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他愛你的列祖，所以才揀選了他們的後裔，用大能親自把你從埃及領出來，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他愛你的列祖，所以揀選他們的後裔，用大能親自領你出了埃及，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And because he loved your fathers and chose their offspring after them and brought you out of Egypt with his own presence, by his great power,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because He loved your fathers, therefore He chose their descendants after them. And He personally brought you from Egypt by His great power,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because he loved your ancestors and chose their descendants after them, he brought you out of Egypt by his Presence and his great strength,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And because He loved your fathers, therefore He chose their descendants after them; and He brought you out of Egypt with His Presence, with His mighty power,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because he loved your ancestors, he chose to bless their descendants, and he personally brought you out of Egypt with a great display of power."
      }
