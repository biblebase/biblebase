    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，遵照我的上帝耶和华的吩咐，我把律例和典章传授给你们，好让你们在将要占领的土地上遵守。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪，我照著耶和華我的　神吩咐我的，把律例和典章教訓你們，好讓你們在將要進去得為業的地上遵行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我照着耶和華－我神所吩咐的將律例典章教訓你們，使你們在所要進去得為業的地上遵行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“See, I have taught you statutes and judgments just as the Lord my God commanded me, that you should do thus in the land where you are entering to possess it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See, I have taught you decrees and laws as the Lord my God commanded me, so that you may follow them in the land you are entering to take possession of it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Look, I now teach you these decrees and regulations just as the Lord my God commanded me, so that you may obey them in the land you are about to enter and occupy."
      }
    },
