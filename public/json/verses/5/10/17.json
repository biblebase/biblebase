    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "deu.10.12-22",
            "paragraphs": [
              "a.耶和華你神向你所要的是甚麽呢？（十12～22）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_sermon-2011-10-09",
        "title": "神向你所要的是什麼？",
        "date": "2011-10-09",
        "preacher": "王文堂牧師",
        "verses": "申命記 10:1-22",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-10-09.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2011/Sermon-2011-10-09.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们的上帝耶和华是万神之神、万主之主，是伟大、全能、可畏的上帝。祂不徇情面，不受贿赂；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華你們的　神，他是萬神之神、萬主之主、偉大有力和可畏的　神；他不徇情面，也不受賄賂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為耶和華－你們的神－他是萬神之神，萬主之主，至大的神，大有能力，大而可畏，不以貌取人，也不受賄賂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the Lord your God is the God of gods and the Lord of lords, the great, the mighty, and the awesome God who does not show partiality nor take a bribe."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For the Lord your God is God of gods and Lord of lords, the great God, mighty and awesome, who shows no partiality and accepts no bribes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“For the Lord your God is the God of gods and Lord of lords. He is the great God, the mighty and awesome God, who shows no partiality and cannot be bribed."
      }
    },
