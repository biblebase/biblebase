        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要敬畏耶和華－你的神，事奉他，專靠他，也要指着他的名起誓。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall fear the Lord your God; you shall serve Him and cling to Him, and you shall swear by His name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Fear the Lord your God and serve him. Hold fast to him and take your oaths in his name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must fear the Lord your God and worship him and cling to him. Your oaths must be in his name alone."
      }
