    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以下是关于以东的预言。 有人从西珥大声问我： “守夜的啊，黑夜还有多长？ 守夜的啊，黑夜还有多长？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "關於以東默示： 有人從西珥呼叫我說： “守望的啊！黑夜還有多長？ 守望的啊！黑夜還有多長？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論度瑪的默示： 有人聲從西珥呼問我說： 守望的啊，夜裏如何？ 守望的啊，夜裏如何？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The oracle concerning Edom. One keeps calling to me from Seir, “Watchman, how far gone is the night? Watchman, how far gone is the night?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A prophecy against Dumah: Someone calls to me from Seir, “Watchman, what is left of the night? Watchman, what is left of the night?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This message came to me concerning Edom: Someone from Edom keeps calling to me, “Watchman, how much longer until morning? When will the night be over?”"
      }
