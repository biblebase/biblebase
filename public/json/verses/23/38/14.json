    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我像燕、鹤一样哀鸣， 像鸽子一样呻吟； 我举目望天，双眼疲倦。 耶和华啊！ 我困苦不堪，求你保护我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我像燕子呻吟，像白鶴鳴叫， 又像鴿子哀鳴。 我的眼睛因仰望高處而疲倦； 耶和華啊！我受欺壓，求你作我的保障。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我像燕子呢喃， 像白鶴鳴叫， 又像鴿子哀鳴； 我因仰觀，眼睛困倦。 耶和華啊，我受欺壓， 求你為我作保。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like a swallow, like a crane, so I twitter; I moan like a dove; My eyes look wistfully to the heights; O Lord, I am oppressed, be my security."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I cried like a swift or thrush, I moaned like a mourning dove. My eyes grew weak as I looked to the heavens. I am being threatened; Lord, come to my aid!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Delirious, I chattered like a swallow or a crane, and then I moaned like a mourning dove. My eyes grew tired of looking to heaven for help. I am in trouble, Lord. Help me!”"
      }
