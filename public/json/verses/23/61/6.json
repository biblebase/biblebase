    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要被称为耶和华的祭司， 我们上帝的臣仆。 你们必享用各国的财宝， 夸耀自己拥有他们的财富。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於你們，卻要稱為耶和華的祭司， 人必稱你們為我們　神的僕人； 你們要享用列國的財富， 因得著他們的榮耀而誇耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們倒要稱為耶和華的祭司； 人必稱你們為我們神的僕役。 你們必吃用列國的財物， 因得他們的榮耀自誇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But you will be called the priests of the Lord; You will be spoken of as ministers of our God. You will eat the wealth of nations, And in their riches you will boast."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And you will be called priests of the Lord, you will be named ministers of our God. You will feed on the wealth of nations, and in their riches you will boast."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will be called priests of the Lord, ministers of our God. You will feed on the treasures of the nations and boast in their riches."
      }
    },
