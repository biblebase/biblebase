    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她们的香气将变成臭味， 腰带将变成绳子， 美发将变成秃头， 华服将变成麻衣， 美丽的容颜将布满羞辱的烙印。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "必有腐臭代替馨香， 繩子代替腰帶， 光禿代替美髮， 麻衣繫腰代替美服， 烙痕代替美貌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "必有臭爛代替馨香， 繩子代替腰帶， 光禿代替美髮， 麻衣繫腰代替華服， 烙傷代替美容。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now it will come about that instead of sweet perfume there will be putrefaction; Instead of a belt, a rope; Instead of well-set hair, a plucked-out scalp; Instead of fine clothes, a donning of sackcloth; And branding instead of beauty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Instead of fragrance there will be a stench; instead of a sash, a rope; instead of well-dressed hair, baldness; instead of fine clothing, sackcloth; instead of beauty, branding."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Instead of smelling of sweet perfume, she will stink. She will wear a rope for a sash, and her elegant hair will fall out. She will wear rough burlap instead of rich robes. Shame will replace her beauty."
      }
