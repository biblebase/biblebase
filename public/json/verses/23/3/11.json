      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人有禍了！他必遭災難！ 因為要照自己手所行的受報應。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe to the wicked! It will go badly with him, For what he deserves will be done to him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Woe to the wicked! Disaster is upon them! They will be paid back for what their hands have done."
      },
