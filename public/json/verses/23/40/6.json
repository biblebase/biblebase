    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有声音说：“呼喊吧！” 我问道：“我呼喊什么呢？” 那声音说：“芸芸众生尽如草， 荣华富贵像野地的花。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有聲音說：“你呼喊吧！” 他問：“我呼喊甚麼呢？” “所有的人盡都如草； 他們的榮美都像野地的花。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人聲說：你喊叫吧！ 有一個說：我喊叫甚麼呢？ 說：凡有血氣的盡都如草； 他的美容都像野地的花。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A voice says, “Call out.” Then he answered, “What shall I call out?” All flesh is grass, and all its loveliness is like the flower of the field."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A voice says, “Cry out.” And I said, “What shall I cry?” “All people are like grass, and all their faithfulness is like the flowers of the field."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A voice said, “Shout!” I asked, “What should I shout?” “Shout that people are like the grass. Their beauty fades as quickly as the flowers in a field."
      }
    },
