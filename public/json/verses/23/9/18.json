    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "isa.9.18",
            "paragraphs": [
              "18.離開神的生活必然帶有毀滅性（焚燒），凡在它面前的，無論是無用的（荊棘）還是有用的（樹林），都一掃而空，連呼吸的空氣也遭到汙染（煙柱）。每樣東西都「化為上騰的灰燼」。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "邪恶像燃烧的火焰， 焚毁荆棘和蒺藜，点燃密林， 烟柱旋转上腾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "邪惡像火一般焚燒， 吞滅荊棘蒺藜， 連樹林中的密叢也燒起火來， 它們旋轉上騰，成了煙柱。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "邪恶如火焚烧， 吞没荆棘和蒺藜， 点燃树林中的密丛， 烟柱旋转上腾。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "邪惡像火焚燒， 燒滅荊棘和蒺藜， 在稠密的樹林中着起來， 就成為煙柱，旋轉上騰。"
      },
