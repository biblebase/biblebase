    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "isa.8.1",
            "paragraphs": [
              "8:1",
              "瑪黑珥沙拉勒哈施罷斯：以賽亞次子之名的意思(比較7:3註)是“擄掠速臨，搶奪快到”。這個名字預示了亞蘭國(主前732年)和以色列國(主前722年)的毀滅和傾覆，二國都先後被亞述帝國所滅。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "isa.8.1",
            "paragraphs": [
              "八1以賽亞第二個兒子\"瑪黑珥沙拉勒哈施罷斯\"（3節）的名意思是\"擄掠速臨，搶奪快到\"或\"快速搶奪，容易捕食\"。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对我说：“你去拿块大的写字板，用通用的文字在上面写上‘玛黑珥·沙拉勒·哈施·罢斯’。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對我說：“你拿一塊大版，用通用的文字寫上‘瑪黑珥．沙拉勒．哈施．罷斯’。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對我說：「你取一個大牌，拿人所用的筆，寫上『瑪黑珥‧沙拉勒‧哈施‧罷斯』。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord said to me, “Take for yourself a large tablet and write on it in ordinary letters: Swift is the booty, speedy is the prey."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said to me, “Take a large scroll and write on it with an ordinary pen: Maher-Shalal-Hash-Baz.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord said to me, “Make a large signboard and clearly write this name on it: Maher-shalal-hash-baz.”"
      }
    },
