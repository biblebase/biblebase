    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "isa.45.9-13",
            "paragraphs": [
              "a.陶匠與父母（四十五9～13）"
            ]
          },
          {
            "verse": "isa.45.12-13",
            "paragraphs": [
              "12～13.我們常只專註於創造是如何開始的，以致未能看見此一聲明的重要性。在舊約中，造物主不單是開始萬物的主，也是維系受造物運作的主，祂控制一切、引導一切。因此，如果祂定意要興起古列（13節），就沒有什麽可討論的！不是為工價：造物主並沒有和古列討價還價！他的出現（興起是「醒起／喚醒而起」，如四十一2、25）、他輕易的征服（修直一切道路）、他的功用（建造……釋放），都是出於全能的上主，「萬軍之上主」（一4），祂在自己裏面有一切的能力和潛力。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "是我造了大地，又造了地上的人； 我亲手铺展穹苍， 布置天上的万象。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我造了地，又造了人在地上； 我親手展開了諸天， 又命定了天上的萬象。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "是我造了地， 又在其上创造人； 是我亲手展开了诸天， 又命定其中的万象。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我造地，又造人在地上。 我親手鋪張諸天； 天上萬象也是我所命定的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I made the earth and created man on it; it was my hands that stretched out the heavens, and I commanded all their host."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is I who made the earth, and created man upon it. I stretched out the heavens with My hands And I ordained all their host."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is I who made the earth and created mankind on it. My own hands stretched out the heavens; I marshaled their starry hosts."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I have made the earth, And created man on it. I—My hands—stretched out the heavens, And all their host I have commanded."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am the one who made the earth and created people to live on it. With my hands I stretched out the heavens. All the stars are at my command."
      }
    },
