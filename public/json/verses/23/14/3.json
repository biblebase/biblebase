    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "isa.14.1-3",
            "paragraphs": [
              "一四1～3以色列人要從巴比倫的征服者手上逃生，並且返回自己的土地。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华使他们脱离痛苦、悲伤和残酷奴役的时候，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華使你脫離痛苦、愁煩、被迫作的苦工，得享安息的日子，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當耶和華使你脫離愁苦、煩惱，並人勉強你做的苦工，得享安息的日子，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And it will be in the day when the Lord gives you rest from your pain and turmoil and harsh service in which you have been enslaved,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the day the Lord gives you relief from your suffering and turmoil and from the harsh labor forced on you,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In that wonderful day when the Lord gives his people rest from sorrow and fear, from slavery and chains,"
      }
    },
