    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "isa.44.24-28",
            "paragraphs": [
              "四四24～28只有真神才能絕對準確地預言將來--在此是預言以色列人從被擄之地歸回，重建並重新居住在耶路撒冷，這預言在事情發生前的一百五十年透過以賽亞來傳達。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我对深海说，‘枯干吧！ 我要使你的河流干涸’；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我對深淵說：‘你乾了吧， 我必使你的江河乾涸！’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對深淵說：你乾了吧！ 我也要使你的江河乾涸。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is I who says to the depth of the sea, ‘Be dried up!’ And I will make your rivers dry."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who says to the watery deep, ‘Be dry, and I will dry up your streams,’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When I speak to the rivers and say, ‘Dry up!’ they will be dry."
      }
    },
