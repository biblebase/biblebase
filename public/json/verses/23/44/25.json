    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "isa.44.24-28",
            "paragraphs": [
              "四四24～28只有真神才能絕對準確地預言將來--在此是預言以色列人從被擄之地歸回，重建並重新居住在耶路撒冷，這預言在事情發生前的一百五十年透過以賽亞來傳達。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我使假先知的预兆无法应验， 使占卜者丑态百出； 我颠覆智者的知识， 使之愚不可及。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我使說假話者的兆頭失效， 使占卜者都變成愚昧人； 我使有智慧的人後退， 使他們的知識變為愚拙。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我破坏假先知的预兆， 使占卜者显为愚妄； 我颠覆了智慧人， 使他们的知识变为愚拙；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使說假話的兆頭失效， 使占卜的癲狂， 使智慧人退後， 使他的知識變為愚拙；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "who frustrates the signs of liars and makes fools of diviners, who turns wise men back and makes their knowledge foolish,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Causing the omens of boasters to fail, Making fools out of diviners, Causing wise men to draw back And turning their knowledge into foolishness,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who foils the signs of false prophets and makes fools of diviners, who overthrows the learning of the wise and turns it into nonsense,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Who frustrates the signs of the babblers, And drives diviners mad; Who turns wise men backward, And makes their knowledge foolishness;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I expose the false prophets as liars and make fools of fortune-tellers. I cause the wise to give bad advice, thus proving them to be fools."
      }
    },
