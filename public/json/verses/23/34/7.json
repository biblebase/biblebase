    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们像野牛、牛犊和公牛一样倒下。 他们的土地被血浸透， 土壤被脂肪覆盖。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "野牛、牛犢與壯牛要與他們一同倒下， 他們的地喝足了血， 他們的塵土因脂肪而肥潤。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "野牛、公牛和壮牛都将与他们一同倒下； 他们的地将喝足血， 他们的尘土将沾满脂肪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "野牛、牛犢，和公牛要一同下來。 他們的地喝醉了血； 他們的塵土因脂油肥潤。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Wild oxen shall fall with them, and young steers with the mighty bulls. Their land shall drink its fill of blood, and their soil shall be gorged with fat."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Wild oxen will also fall with them And young bulls with strong ones; Thus their land will be soaked with blood, And their dust become greasy with fat."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the wild oxen will fall with them, the bull calves and the great bulls. Their land will be drenched with blood, and the dust will be soaked with fat."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The wild oxen shall come down with them, And the young bulls with the mighty bulls; Their land shall be soaked with blood, And their dust saturated with fatness.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Even men as strong as wild oxen will die— the young men alongside the veterans. The land will be soaked with blood and the soil enriched with fat."
      }
    },
