    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "isa.34.5",
            "paragraphs": [
              "三四5\"以東\"。以色列疏遠的兄弟，代表所有遠離神的非信徒（來一二16）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的刀剑在天上饱饮了血后， 必降下来惩罚以东——祂决意要毁灭的民族。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為我的刀在天上已經喝足了， 看哪！它要落在以東， 以及我決定要滅絕的民身上，為要施行審判。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我耶和华的刀在天上已经喝足了， 看哪，这刀要落下施行审判， 落在我命定灭绝的民以东人身上！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為我的刀在天上已經喝足； 這刀必臨到以東和我所咒詛的民， 要施行審判。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For my sword has drunk its fill in the heavens; behold, it descends for judgment upon Edom, upon the people I have devoted to destruction."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For My sword is satiated in heaven, Behold it shall descend for judgment upon Edom And upon the people whom I have devoted to destruction."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My sword has drunk its fill in the heavens; see, it descends in judgment on Edom, the people I have totally destroyed."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“For My sword shall be bathed in heaven; Indeed it shall come down on Edom, And on the people of My curse, for judgment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when my sword has finished its work in the heavens, it will fall upon Edom, the nation I have marked for destruction."
      }
    },
