    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天上的万象必融化， 穹苍像书卷卷起； 星辰陨落， 像凋零的葡萄叶， 又像枯落的无花果。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "天上的萬象都必消殘； 天也像書卷一般被捲起， 所有星宿也必衰殘， 像葡萄樹的葉子衰殘一樣， 又像無花果樹的葉子凋落一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天上的萬象都要消沒； 天被捲起，好像書卷。 其上的萬象要殘敗， 像葡萄樹的葉子殘敗， 又像無花果樹的葉子殘敗一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And all the host of heaven will wear away, And the sky will be rolled up like a scroll; All their hosts will also wither away As a leaf withers from the vine, Or as one withers from the fig tree."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the stars in the sky will be dissolved and the heavens rolled up like a scroll; all the starry host will fall like withered leaves from the vine, like shriveled figs from the fig tree."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The heavens above will melt away and disappear like a rolled-up scroll. The stars will fall from the sky like withered leaves from a grapevine, or shriveled figs from a fig tree."
      }
    },
