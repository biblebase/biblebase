    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "isa.64.8",
            "paragraphs": [
              "六四8\"窰匠\"。直譯作：那造出我們的人；如在第二十九章16節和第四十五章9節。承認自己是\"泥\"，表示了完全的順服。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但耶和华啊，你是我们的父亲。 我们是陶泥，你是窑匠， 你亲手造了我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！現在你還是我們的父； 我們不過是泥土，你才是陶匠； 我們眾人都是你手所作的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，現在你仍是我們的父！ 我們是泥，你是窰匠； 我們都是你手的工作。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But now, O Lord, You are our Father, We are the clay, and You our potter; And all of us are the work of Your hand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet you, Lord, are our Father. We are the clay, you are the potter; we are all the work of your hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And yet, O Lord, you are our Father. We are the clay, and you are the potter. We all are formed by your hand."
      }
    },
