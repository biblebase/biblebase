    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们那圣洁、华美的殿——我们祖先颂赞你的地方已被焚毁， 我们珍爱的一切都被摧毁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們那聖潔和榮美的殿， 就是我們的列祖讚美你的所在， 被火燒了。 我們所喜愛的一切，都成了廢墟。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們聖潔華美的殿－ 就是我們列祖讚美你的所在被火焚燒； 我們所羨慕的美地盡都荒廢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Our holy and beautiful house, Where our fathers praised You, Has been burned by fire; And all our precious things have become a ruin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Our holy and glorious temple, where our ancestors praised you, has been burned with fire, and all that we treasured lies in ruins."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The holy and beautiful Temple where our ancestors praised you has been burned down, and all the things of beauty are destroyed."
      }
    },
