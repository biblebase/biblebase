    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把保罗绑起来正要鞭打，保罗问旁边的百夫长：“未经定罪就拷打罗马公民合法吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "士兵正用皮帶綁他的時候，保羅對站在旁邊的百夫長說：“你們鞭打一個還沒有定罪的羅馬公民，是合法的嗎？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "当士兵们用皮条捆上保罗的时候，他对站在旁边的百夫长说：“你们鞭打一个没有被定罪的罗马公民，可以吗？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "剛用皮條捆上，保羅對旁邊站着的百夫長說：「人是羅馬人，又沒有定罪，你們就鞭打他，有這個例嗎？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But when they had stretched him out for the whips, Paul said to the centurion who was standing by, “Is it lawful for you to flog a man who is a Roman citizen and uncondemned?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But when they stretched him out with thongs, Paul said to the centurion who was standing by, “Is it lawful for you to scourge a man who is a Roman and uncondemned?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As they stretched him out to flog him, Paul said to the centurion standing there, “Is it legal for you to flog a Roman citizen who hasn’t even been found guilty?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And as they bound him with thongs, Paul said to the centurion who stood by, “Is it lawful for you to scourge a man who is a Roman, and uncondemned?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they tied Paul down to lash him, Paul said to the officer standing there, “Is it legal for you to whip a Roman citizen who hasn’t even been tried?”"
      }
    },
