    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "act.22.15",
            "paragraphs": [
              "15.這啟示的目的，是要保羅作見證人，將所見所聽的向人傳述，萬人指猶太人及外邦人（九15，廿二21，廿六17，加一16）。因此，保羅這時受審，不只是被控汙穢聖殿，破壞猶太教（廿一28），更重要的，是作為耶穌的見證人。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你将做祂的见证人，把所见所闻告诉万民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你要把所看見所聽見的，向萬人為他作見證。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你要將所看見的，所聽見的，對着萬人為他作見證。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For you will be a witness for Him to all men of what you have seen and heard."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will be his witness to all people of what you have seen and heard."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you are to be his witness, telling everyone what you have seen and heard."
      }
    },
