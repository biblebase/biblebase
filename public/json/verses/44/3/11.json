    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那乞丐紧紧拉着彼得和约翰的手走到所罗门廊，众人都跑过来，啧啧称奇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那人拉著彼得和約翰的時候，群眾都很驚奇，跑到他們那裡，就是所羅門廊的下面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那人正在稱為所羅門的廊下，拉着彼得、約翰；眾百姓一齊跑到他們那裏，很覺希奇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While he was clinging to Peter and John, all the people ran together to them at the so-called portico of Solomon, full of amazement."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While the man held on to Peter and John, all the people were astonished and came running to them in the place called Solomon’s Colonnade."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They all rushed out in amazement to Solomon’s Colonnade, where the man was holding tightly to Peter and John."
      }
    },
