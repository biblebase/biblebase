    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "士兵们抬着保罗来到军营门口，保罗问千夫长：“我可以和你讲几句话吗？”千夫长说：“你也懂希腊话吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們帶著保羅快到營樓的時候，保羅對千夫長說：“我可以跟你講一句話嗎？”千夫長說：“你懂希臘話嗎？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "保罗快要被押进营楼的时候，就对千夫长说：“我是否可以对你说句话？” 千夫长说：“你懂希腊语吗？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將要帶他進營樓，保羅對千夫長說：「我對你說句話可以不可以？」他說：「你懂得希臘話嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "As Paul was about to be brought into the barracks, he said to the tribune, “May I say something to you?” And he said, “Do you know Greek?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As Paul was about to be brought into the barracks, he said to the commander, “May I say something to you?” And he *said, “Do you know Greek?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As the soldiers were about to take Paul into the barracks, he asked the commander, “May I say something to you?” “Do you speak Greek?” he replied."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then as Paul was about to be led into the barracks, he said to the commander, “May I speak to you?” He replied, “Can you speak Greek?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As Paul was about to be taken inside, he said to the commander, “May I have a word with you?” “Do you know Greek?” the commander asked, surprised."
      }
