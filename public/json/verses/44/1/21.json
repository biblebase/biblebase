      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore it is necessary to choose one of the men who have been with us the whole time the Lord Jesus was living among us,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“So now we must choose a replacement for Judas from among the men who were with us the entire time we were traveling with the Lord Jesus—"
      }
