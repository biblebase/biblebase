    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "act.26.4-5",
            "paragraphs": [
              "二六4，5保羅自幼為人便是個模範的猶太人。猶太人若肯作見證就必得承認，保羅遵循最嚴緊的正統，是個言行一致的法利賽人。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我从小在本族和耶路撒冷为人如何，犹太人都知道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我從幼年到現在，在本國、在耶路撒冷為人怎樣，猶太人都知道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我從起初在本國的民中，並在耶路撒冷，自幼為人如何，猶太人都知道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“So then, all Jews know my manner of life from my youth up, which from the beginning was spent among my own nation and at Jerusalem;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The Jewish people all know the way I have lived ever since I was a child, from the beginning of my life in my own country, and also in Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“As the Jewish leaders are well aware, I was given a thorough Jewish training from my earliest childhood among my own people and in Jerusalem."
      }
    },
