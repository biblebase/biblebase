    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所有先知也都为祂做见证说，‘谁信祂，谁的罪就能借着祂的名得到赦免。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有先知都為他作見證：所有信他的，都必藉著他的名，罪得赦免。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾先知也為他作見證說：『凡信他的人必因他的名得蒙赦罪。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Of Him all the prophets bear witness that through His name everyone who believes in Him receives forgiveness of sins.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the prophets testify about him that everyone who believes in him receives forgiveness of sins through his name.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He is the one all the prophets testified about, saying that everyone who believes in him will have their sins forgiven through his name.”"
      }
