    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们令使徒站在当中，质问道：“你们凭什么权柄、以谁的名义这样做？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們叫使徒都站在當中，查問說：“你們憑甚麼能力，奉誰的名作這事？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们叫彼得和约翰站在中间，质问他们，说：“你们凭着什么能力，以谁的名做这事？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "叫使徒站在當中，就問他們說：「你們用甚麼能力，奉誰的名做這事呢？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And when they had set them in the midst, they inquired, “By what power or by what name did you do this?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they had placed them in the center, they began to inquire, “By what power, or in what name, have you done this?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They had Peter and John brought before them and began to question them: “By what power or what name did you do this?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And when they had set them in the midst, they asked, “By what power or by what name have you done this?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They brought in the two disciples and demanded, “By what power, or in whose name, have you done this?”"
      }
