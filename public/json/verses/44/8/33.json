    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂忍受耻辱， 无人为祂主持公道， 谁能明白那个世代呢？ 因为祂的生命竟然被夺去。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他受屈辱的時候， 得不到公平的審判， 誰能說出他的身世呢？ 因為他的生命從地上被奪去。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他卑微的時候， 人不按公義審判他； 誰能述說他的世代？ 因為他的生命從地上奪去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In humiliation His judgment was taken away; Who will relate His generation? For His life is removed from the earth.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In his humiliation he was deprived of justice. Who can speak of his descendants? For his life was taken from the earth.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He was humiliated and received no justice. Who can speak of his descendants? For his life was taken from the earth.”"
      }
