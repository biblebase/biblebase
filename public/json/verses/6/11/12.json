    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚攻占了诸王所有的城池，擒获诸王。他照耶和华的仆人摩西的吩咐，将诸王全部杀死，毁掉城池。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞奪取了那些王的一切城鎮，又擒獲那些城鎮所有的王；他用刀擊殺了他們，把一切完全毀滅，正如耶和華僕人摩西所吩咐的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞奪了這些王的一切城邑，擒獲其中的諸王，用刀擊殺他們，將他們盡行殺滅，正如耶和華僕人摩西所吩咐的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joshua captured all the cities of these kings, and all their kings, and he struck them with the edge of the sword, and utterly destroyed them; just as Moses the servant of the Lord had commanded."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Joshua took all these royal cities and their kings and put them to the sword. He totally destroyed them, as Moses the servant of the Lord had commanded."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Joshua slaughtered all the other kings and their people, completely destroying them, just as Moses, the servant of the Lord, had commanded."
      }
    },
