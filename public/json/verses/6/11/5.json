    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.11.5",
            "paragraphs": [
              "一一5\"米倫水邊\"。明顯不是戶勒湖，而是在戶勒和加利利海中間的眾多水泉。北方聯盟的軍隊在這裏集合。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们在米伦河会师，一同扎营，要跟以色列人交战。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些王都聯合起來，來到米倫水邊，在那裡一起安營，要與以色列人作戰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這諸王會合，來到米倫水邊，一同安營，要與以色列人爭戰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So all of these kings having agreed to meet, came and encamped together at the waters of Merom, to fight against Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All these kings joined forces and made camp together at the Waters of Merom to fight against Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The kings joined forces and established their camp around the water near Merom to fight against Israel."
      }
    },
