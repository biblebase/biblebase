    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们杀掉城内所有的居民，一个没留，掳掠了城内所有的财物和牲畜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些城所有的戰利品和牲畜，以色列人都據為己有；只是把所有的人都用刀擊殺了，直到把他們殺盡；有氣息的，他們沒有留下一個。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那些城邑所有的財物和牲畜，以色列人都取為自己的掠物；惟有一切人口都用刀擊殺，直到殺盡；凡有氣息的沒有留下一個。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the spoil of these cities and the cattle, the sons of Israel took as their plunder; but they struck every man with the edge of the sword, until they had destroyed them. They left no one who breathed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Israelites carried off for themselves all the plunder and livestock of these cities, but all the people they put to the sword until they completely destroyed them, not sparing anyone that breathed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the Israelites took all the plunder and livestock of the ravaged towns for themselves. But they killed all the people, leaving no survivors."
      }
    },
