    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "好让那些误杀了人的可以逃到那里，免遭仇人追杀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "好使那出於錯誤、並非蓄意殺人的，可以逃到那裡去；這些城要成為你們逃避報血仇的人的避難所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使那無心而誤殺人的，可以逃到那裏。這些城可以作你們逃避報血仇人的地方。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that the manslayer who kills any person unintentionally, without premeditation, may flee there, and they shall become your refuge from the avenger of blood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "so that anyone who kills a person accidentally and unintentionally may flee there and find protection from the avenger of blood."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who kills another person accidentally and unintentionally can run to one of these cities; they will be places of refuge from relatives seeking revenge for the person who was killed."
      }
    },
