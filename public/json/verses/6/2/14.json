    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "二人便对她说：“如果你不泄露我们的事，我们愿意用性命担保，当耶和华把这地方赐给我们的时候，我们一定守信善待你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "兩個探子就對她說：“如果你不洩漏我們這次行動，我們願以性命作你們的保證；耶和華把這地賜給我們的時候，我們必定以慈愛和誠實待你們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "二人對她說：「你若不洩漏我們這件事，我們情願替你們死。耶和華將這地賜給我們的時候，我們必以慈愛誠實待你。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the men said to her, “Our life for yours if you do not tell this business of ours; and it shall come about when the Lord gives us the land that we will deal kindly and faithfully with you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Our lives for your lives!” the men assured her. “If you don’t tell what we are doing, we will treat you kindly and faithfully when the Lord gives us the land.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“We offer our own lives as a guarantee for your safety,” the men agreed. “If you don’t betray us, we will keep our promise and be kind to you when the Lord gives us the land.”"
      }
    },
