    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.18.2-9",
            "paragraphs": [
              "一八2～9約書亞派遣二十一人去勘察那仍未分配給猶大、瑪拿西和以法蓮的土地。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚对他们说：“你们等到什么时候才去占领你们祖先的上帝耶和华赐给你们的土地呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞對以色列人說：“耶和華你們列祖的　神賜給你們的地，你們遲遲不去佔領，要到幾時呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞對以色列人說：「耶和華－你們列祖的神所賜給你們的地，你們躭延不去得，要到幾時呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Joshua said to the sons of Israel, “How long will you put off entering to take possession of the land which the Lord, the God of your fathers, has given you?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Joshua said to the Israelites: “How long will you wait before you begin to take possession of the land that the Lord, the God of your ancestors, has given you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Joshua asked them, “How long are you going to wait before taking possession of the remaining land the Lord, the God of your ancestors, has given to you?"
      }
    },
