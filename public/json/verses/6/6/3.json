    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.6.2-5",
            "paragraphs": [
              "六2～5這段記載耶和華軍隊的元帥告訴約書亞的一些重要資料，那就是關乎怎樣攻取耶利哥城的資料。神吩咐以色列人圍繞耶利哥城七日的計劃，目的是試驗以色列人的信心，同時令已經惶恐的耶利哥城居民更加恐懼（二9）。該城只有八英畝半（3.4公頃），因此最後一天要圍繞\"七次\"也不太困難。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你所有的军队要每天绕城一圈，连续六天，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們所有能作戰的男丁要圍繞這城，一天圍繞一次，六天都要這樣行；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們的一切兵丁要圍繞這城，一日圍繞一次，六日都要這樣行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall march around the city, all the men of war circling the city once. You shall do so for six days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "March around the city once with all the armed men. Do this for six days."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You and your fighting men should march around the town once a day for six days."
      }
    },
