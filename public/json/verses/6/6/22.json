    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚对那两个探子说：“你们到那个妓女家里，照着你们向她起过的誓，把她和她的家人都带出来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞對窺探那地的兩個人說：“你們進那妓女的家裡去，照著你們向她所起的誓，把那女人和她所有的一切，都從家裡帶出來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞吩咐窺探地的兩個人說：「你們進那妓女的家，照着你們向她所起的誓，將那女人和她所有的都從那裏帶出來。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But to the two men who had spied out the land, Joshua said, “Go into the prostitute’s house and bring out from there the woman and all who belong to her, as you swore to her.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joshua said to the two men who had spied out the land, “Go into the harlot’s house and bring the woman and all she has out of there, as you have sworn to her.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Joshua said to the two men who had spied out the land, “Go into the prostitute’s house and bring her out and all who belong to her, in accordance with your oath to her.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But Joshua had said to the two men who had spied out the country, “Go into the harlot’s house, and from there bring out the woman and all that she has, as you swore to her.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Meanwhile, Joshua said to the two spies, “Keep your promise. Go to the prostitute’s house and bring her out, along with all her family.”"
      }
    },
