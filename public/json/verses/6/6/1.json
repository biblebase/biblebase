    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jos.6.1",
            "paragraphs": [
              "6:1",
              "耶利哥：耶利哥城占地約49畝。對城內和近郊的居民來說，這城是個要塞。它的城墻可能有9.2米高，6.1米厚。當時的人們認為耶利哥城有迦南諸神的保護，因而是無法攻克的。攻取耶利哥城是約書亞整個戰略中的關鍵，因為這將顯明以色列人的神勝過迦南人的神，這樣迦南人將必敗無疑。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jos.6.1",
            "paragraphs": [
              "六1\"嚴緊\"即堅固。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利哥人害怕以色列人，就把所有的城门都关得紧紧的，无人出入。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利哥的城門因以色列人的緣故，就緊緊地關閉，沒有人出入。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶利哥的城門因以色列人就關得嚴緊，無人出入。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Jericho was tightly shut because of the sons of Israel; no one went out and no one came in."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now the gates of Jericho were securely barred because of the Israelites. No one went out and no one came in."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now the gates of Jericho were tightly shut because the people were afraid of the Israelites. No one was allowed to go out or in."
      }
