    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他又对民众说：“你们要前去绕着城墙走，军队要走在耶和华的约柜前面。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又對人民說：“你們前去繞城吧，拿武器的人要走在耶和華約櫃的前面。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又對百姓說：「你們前去繞城，帶兵器的要走在耶和華的約櫃前。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he said to the people, “Go forward, and march around the city, and let the armed men go on before the ark of the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he ordered the army, “Advance! March around the city, with an armed guard going ahead of the ark of the Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he gave orders to the people: “March around the town, and the armed men will lead the way in front of the Ark of the Lord.”"
      }
