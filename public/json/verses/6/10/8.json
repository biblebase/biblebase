    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对约书亚说：“不要害怕，我已经把他们交在你手中，他们没有一人能抵挡你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對約書亞說：“你不要怕他們，因為我已經把他們交在你手裡了；他們必沒有一人能在你面前站立得住。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對約書亞說：「不要怕他們；因為我已將他們交在你手裏，他們無一人能在你面前站立得住。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord said to Joshua, “Do not fear them, for I have given them into your hands; not one of them shall stand before you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said to Joshua, “Do not be afraid of them; I have given them into your hand. Not one of them will be able to withstand you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do not be afraid of them,” the Lord said to Joshua, “for I have given you victory over them. Not a single one of them will be able to stand up to you.”"
      }
    },
