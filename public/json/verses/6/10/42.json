    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚能一鼓作气杀败诸王，征服他们的土地，都是因为有以色列的上帝耶和华为以色列人争战。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞在這一次戰役中擊敗了這些王和佔領了他們的地，是因為耶和華以色列的　神為以色列作戰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞一時殺敗了這些王，並奪了他們的地，因為耶和華－以色列的神為以色列爭戰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joshua captured all these kings and their lands at one time, because the Lord, the God of Israel, fought for Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All these kings and their lands Joshua conquered in one campaign, because the Lord, the God of Israel, fought for Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Joshua conquered all these kings and their land in a single campaign, for the Lord, the God of Israel, was fighting for his people."
      }
    },
