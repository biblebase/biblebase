    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华将拉吉交在以色列人手中，第二天约书亚便攻占了拉吉，杀了全城的人，就像在立拿所行的一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華把拉吉交在以色列人手裡，第二天約書亞就佔領了拉吉，用刀攻擊那城，殺了城中所有的人，跟他對立拿所行的一切一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華將拉吉交在以色列人的手裏。第二天約書亞就奪了拉吉，用刀擊殺了城中的一切人口，是照他向立拿一切所行的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the Lord gave Lachish into the hand of Israel, and he captured it on the second day and struck it with the edge of the sword, and every person in it, as he had done to Libnah."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord gave Lachish into the hands of Israel; and he captured it on the second day, and struck it and every person who was in it with the edge of the sword, according to all that he had done to Libnah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord gave Lachish into Israel’s hands, and Joshua took it on the second day. The city and everyone in it he put to the sword, just as he had done to Libnah."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the Lord delivered Lachish into the hand of Israel, who took it on the second day, and struck it and all the people who were in it with the edge of the sword, according to all that he had done to Libnah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Here again, the Lord gave them Lachish. Joshua took it on the second day and killed everyone in it, just as he had done at Libnah."
      }
    },
