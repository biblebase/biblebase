    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们如果违背你们的上帝耶和华吩咐你们遵守的约，去供奉、祭拜别的神明，祂必向你们发怒，使你们在祂所赐的这美好土地上迅速灭亡。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們違背了耶和華你們的　神吩咐你們遵守的約，去事奉別的神，敬拜他們，耶和華必向你們大發烈怒，使你們從他賜給你們的美地上迅速滅亡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若違背耶和華－你們神吩咐你們所守的約，去事奉別神，叩拜他，耶和華的怒氣必向你們發作，使你們在他所賜的美地上速速滅亡。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you transgress the covenant of the Lord your God, which He commanded you, and go and serve other gods and bow down to them, then the anger of the Lord will burn against you, and you will perish quickly from off the good land which He has given you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you violate the covenant of the Lord your God, which he commanded you, and go and serve other gods and bow down to them, the Lord’s anger will burn against you, and you will quickly perish from the good land he has given you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you break the covenant of the Lord your God by worshiping and serving other gods, his anger will burn against you, and you will quickly vanish from the good land he has given you.”"
      }
    },
