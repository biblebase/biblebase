      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚和以色列的长老便撕裂衣服，把灰撒在头上，俯伏在耶和华的约柜面前，直到晚上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞就撕裂自己的衣服，和以色列人的長老，在耶和華的約櫃前，一同俯伏在地，直到晚上，也把塵土撒在頭上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞便撕裂衣服；他和以色列的長老把灰撒在頭上，在耶和華的約櫃前俯伏在地，直到晚上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Joshua tore his clothes and fell to the earth on his face before the ark of the Lord until the evening, both he and the elders of Israel; and they put dust on their heads."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Joshua tore his clothes and fell facedown to the ground before the ark of the Lord, remaining there till evening. The elders of Israel did the same, and sprinkled dust on their heads."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Joshua and the elders of Israel tore their clothing in dismay, threw dust on their heads, and bowed face down to the ground before the Ark of the Lord until evening."
      }
    },
