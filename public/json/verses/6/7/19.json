    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚对亚干说：“孩子啊，我劝你把荣耀归给以色列的上帝耶和华，向祂认罪，把你所做的事告诉我，不要隐瞒。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞對亞干說：“我兒，我勸你把榮耀歸給耶和華以色列的　神，向他認罪，告訴我你作了甚麼事，不可向我隱瞞。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞對亞干說：「我兒，我勸你將榮耀歸給耶和華－以色列的神，在他面前認罪，將你所做的事告訴我，不要向我隱瞞。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Joshua said to Achan, “My son, give glory to the Lord God of Israel and give praise to him. And tell me now what you have done; do not hide it from me.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Joshua said to Achan, “My son, I implore you, give glory to the Lord, the God of Israel, and give praise to Him; and tell me now what you have done. Do not hide it from me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Joshua said to Achan, “My son, give glory to the Lord, the God of Israel, and honor him. Tell me what you have done; do not hide it from me.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now Joshua said to Achan, “My son, I beg you, give glory to the Lord God of Israel, and make confession to Him, and tell me now what you have done; do not hide it from me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Joshua said to Achan, “My son, give glory to the Lord, the God of Israel, by telling the truth. Make your confession and tell me what you have done. Don’t hide it from me.”"
      }
    },
