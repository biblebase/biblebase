    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.17.7-11",
            "paragraphs": [
              "一七7～11瑪拿西控制中央山地的北部，直到耶斯列谷。\"又有三處山岡\"（11節）。\"山岡\"（Napheth）一詞並未在別處出現。它可能指三個區域、城鎮或山岡，也可能要說明多珥（名單上的第三個城）是拿法特多珥（Naphathdor）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛拿西支派分到的产业从亚设开始，到示剑东面的密米他，再向南到隐·他普亚人居住的地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瑪拿西的疆界，是從亞設起，到示劍東面的密米他；又向南走，到隱．他普亞的居民那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瑪拿西的境界：從亞設起，到示劍前的密米他，往北到隱‧他普亞居民之地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The border of Manasseh ran from Asher to Michmethath which was east of Shechem; then the border went southward to the inhabitants of En-tappuah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The territory of Manasseh extended from Asher to Mikmethath east of Shechem. The boundary ran southward from there to include the people living at En Tappuah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The boundary of the tribe of Manasseh extended from the border of Asher to Micmethath, near Shechem. Then the boundary went south from Micmethath to the settlement near the spring of Tappuah."
      }
    },
