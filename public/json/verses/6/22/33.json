    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大家听后，都很满意，便颂赞耶和华，打消了出兵讨伐吕便人和迦得人、毁灭他们家园的念头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人對這事都很滿意，就稱頌　神，不再說要上去討伐流本人和迦得人，毀滅他們所住的地方了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人以這事為美，就稱頌神，不再提上去攻打呂便人、迦得人、毀壞他們所住的地了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The word pleased the sons of Israel, and the sons of Israel blessed God; and they did not speak of going up against them in war to destroy the land in which the sons of Reuben and the sons of Gad were living."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They were glad to hear the report and praised God. And they talked no more about going to war against them to devastate the country where the Reubenites and the Gadites lived."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And all the Israelites were satisfied and praised God and spoke no more of war against Reuben and Gad."
      }
    },
