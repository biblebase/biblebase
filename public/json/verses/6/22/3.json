    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这些日子以来，你们从没有离弃自己的弟兄，遵守了你们的上帝耶和华的吩咐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這許多日子以來，你們一直沒有撇棄你們的兄弟，直到今日；耶和華你們的　神吩咐你們的命令，你們也遵守了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們這許多日子，總沒有撇離你們的弟兄，直到今日，並守了耶和華－你們神所吩咐你們當守的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have not forsaken your brothers these many days to this day, but have kept the charge of the commandment of the Lord your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For a long time now—to this very day—you have not deserted your fellow Israelites but have carried out the mission the Lord your God gave you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "During all this time you have not deserted the other tribes. You have been careful to obey the commands of the Lord your God right up to the present day."
      }
    },
