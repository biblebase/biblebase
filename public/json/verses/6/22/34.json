    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jos.22.34",
            "paragraphs": [
              "22:34",
              "證壇……證明耶和華是神：這壇建立在約但河的東邊，作為一個見證和記念，提醒那些支派的後代們必須忠於神，並要單單服待他。同樣地，一些具體可見、代代相傳的信心紐帶，比如一本聖經、一件特殊的禮物、一張像片、一個紀念品或一個家庭的傳統，都可以提醒基督徒和他們的後代要忠於神。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jos.22.34",
            "paragraphs": [
              "二二34\"證壇\"的希伯來文是Ed，解作\"見證\"或\"證據\"（如在27,28節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "吕便和迦得人给那座祭坛取名叫“证坛”，意思是这坛在我们中间证明耶和华是上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "流本人和迦得人給那壇起名叫證壇，因為他們說：“這壇在我們中間證明耶和華是　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "呂便人、迦得人給壇起名叫證壇，意思說：這壇在我們中間證明耶和華是神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sons of Reuben and the sons of Gad called the altar Witness; “For,” they said, “it is a witness between us that the Lord is God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Reubenites and the Gadites gave the altar this name: A Witness Between Us—that the Lord is God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people of Reuben and Gad named the altar “Witness,” for they said, “It is a witness between us and them that the Lord is our God, too.”"
      }
    },
