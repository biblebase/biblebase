    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.22.2",
            "paragraphs": [
              "二二2\"摩西所吩咐你們的\"。參看民數記第三十二章的腳註。另參看他們的順從：約書亞記一章16至18節和四章12至14節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对他们说：“你们都遵守了耶和华的仆人摩西的一切吩咐，也遵守了我的一切吩咐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對他們說：“耶和華的僕人摩西吩咐你們的一切，你們都遵守了；我吩咐你們的一切，你們也都聽從了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對他們說：「耶和華僕人摩西所吩咐你們的，你們都遵守了；我所吩咐你們的，你們也都聽從了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and said to them, “You have kept all that Moses the servant of the Lord commanded you, and have listened to my voice in all that I commanded you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and said to them, “You have done all that Moses the servant of the Lord commanded, and you have obeyed me in everything I commanded."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He told them, “You have done as Moses, the servant of the Lord, commanded you, and you have obeyed every order I have given you."
      }
    },
