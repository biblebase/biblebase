    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "艾城的人都被召去追赶以色列人，他们都被诱出城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "城裡的眾民都被召集，去追趕他們；艾城的人追趕約書亞的時候，就都被誘離城。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "城內的眾民都被招聚，追趕他們；艾城人追趕的時候，就被引誘離開城。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And all the people who were in the city were called together to pursue them, and they pursued Joshua and were drawn away from the city."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the men of Ai were called to pursue them, and they pursued Joshua and were lured away from the city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then all the men in the town were called out to chase after them. In this way, they were lured away from the town."
      }
    },
