    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我会率领军队正面攻城，当敌人像上次那样出城应战时，我们便逃跑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我和所有與我一起的人，要向城推進。城裡的人像前一次出來攻擊我們的時候，我們就在他們面前逃跑；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我與我所帶領的眾民要向城前往。城裏的人像初次出來攻擊我們的時候，我們就在他們面前逃跑，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And I and all the people who are with me will approach the city. And when they come out against us just as before, we shall flee before them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I and all the people who are with me will approach the city. And when they come out to meet us as at the first, we will flee before them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I and all those with me will advance on the city, and when the men come out against us, as they did before, we will flee from them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then I and all the people who are with me will approach the city; and it will come about, when they come out against us as at the first, that we shall flee before them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When our main army attacks, the men of Ai will come out to fight as they did before, and we will run away from them."
      }
    },
