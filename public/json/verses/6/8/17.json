    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，伯特利和艾城的人倾巢而出，追杀以色列人，他们的城门大开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "艾城和伯特利城沒有留下一個人不出來追趕以色列人；他們撇下了敞開的城門，去追趕以色列人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "艾城和伯特利城沒有一人不出來追趕以色列人的，撇了敞開的城門，去追趕以色列人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So not a man was left in Ai or Bethel who had not gone out after Israel, and they left the city unguarded and pursued Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Not a man remained in Ai or Bethel who did not go after Israel. They left the city open and went in pursuit of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There was not a man left in Ai or Bethel who did not chase after the Israelites, and the town was left wide open."
      }
    },
