    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们攻陷艾城以后，要照耶和华的话放火焚城。这是我的命令。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們佔領了城以後，就要放火燒城；要照著耶和華的話行事；你們要謹慎，我已經吩咐你們了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們奪了城以後，就放火燒城，要照耶和華的話行。這是我吩咐你們的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then it will be when you have seized the city, that you shall set the city on fire. You shall do it according to the word of the Lord. See, I have commanded you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you have taken the city, set it on fire. Do what the Lord has commanded. See to it; you have my orders.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Set the town on fire, as the Lord has commanded. You have your orders.”"
      }
    },
