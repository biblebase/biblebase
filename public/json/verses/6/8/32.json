        "content": [
          {
            "verse": "jos.8.32",
            "paragraphs": [
              "八32\"石頭\"。不是壇的石頭，而是一根大柱子的石頭。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约书亚又在众人面前，将摩西所写的律法刻在石头上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約書亞在那裡，當著以色列人面前，把摩西所寫的摩西律法抄寫在石頭上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約書亞在那裏，當着以色列人面前，將摩西所寫的律法抄寫在石頭上。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And there, in the presence of the people of Israel, he wrote on the stones a copy of the law of Moses, which he had written."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He wrote there on the stones a copy of the law of Moses, which he had written, in the presence of the sons of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There, in the presence of the Israelites, Joshua wrote on stones a copy of the law of Moses."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And there, in the presence of the children of Israel, he wrote on the stones a copy of the law of Moses, which he had written."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And as the Israelites watched, Joshua copied onto the stones of the altar the instructions Moses had given them."
      }
