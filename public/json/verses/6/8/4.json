    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.8.4",
            "paragraphs": [
              "八4\"在城後\"。即在城的西面（9節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并吩咐他们说：“你们要在城后埋伏，不可离城太远，要随时准备行动。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "吩咐他們說：“你們注意，要在城後面埋伏攻城；不可離城太遠，人人都要作好準備。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "吩咐他們說：「你們要在城後埋伏，不可離城太遠，都要各自準備。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He commanded them, saying, “See, you are going to ambush the city from behind it. Do not go very far from the city, but all of you be ready."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "with these orders: “Listen carefully. You are to set an ambush behind the city. Don’t go very far from it. All of you be on the alert."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "with these orders: “Hide in ambush close behind the town and be ready for action."
      }
