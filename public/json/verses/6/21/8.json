    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人照耶和华对摩西的吩咐，把这些城邑及其草场用抽签的方式分给利未人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人照著耶和華吩咐摩西的，藉著抽籤，把這些城市和城的郊野分給利未人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人照着耶和華藉摩西所吩咐的，將這些城邑和城邑的郊野，按鬮分給利未人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "These cities and their pasturelands the people of Israel gave by lot to the Levites, as the Lord had commanded through Moses."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the sons of Israel gave by lot to the Levites these cities with their pasture lands, as the Lord had commanded through Moses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the Israelites allotted to the Levites these towns and their pasturelands, as the Lord had commanded through Moses."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the children of Israel gave these cities with their common-lands by lot to the Levites, as the Lord had commanded by the hand of Moses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the Israelites obeyed the Lord’s command to Moses and assigned these towns and pasturelands to the Levites by casting sacred lots."
      }
    },
