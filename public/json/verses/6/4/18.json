    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "抬耶和华约柜的祭司从河床上来，脚刚一踏上岸，约旦河的水立刻恢复了原状，像以往一样涨过两岸。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "抬耶和華約櫃的祭司從約旦河中間上來的時候，祭司的腳掌一提起來踏在乾地上，約旦河的水就流回原處，像以前一樣漲過兩岸。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "擡耶和華約櫃的祭司從約旦河裏上來，腳掌剛落旱地，約旦河的水就流到原處，仍舊漲過兩岸。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It came about when the priests who carried the ark of the covenant of the Lord had come up from the middle of the Jordan, and the soles of the priests’ feet were lifted up to the dry ground, that the waters of the Jordan returned to their place, and went over all its banks as before."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the priests came up out of the river carrying the ark of the covenant of the Lord. No sooner had they set their feet on the dry ground than the waters of the Jordan returned to their place and ran at flood stage as before."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As soon as the priests carrying the Ark of the Lord’s Covenant came up out of the riverbed and their feet were on high ground, the water of the Jordan returned and overflowed its banks as before."
      }
