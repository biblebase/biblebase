    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华把这地方的亚摩利人等外族人从我们面前赶走了，我们必事奉祂，因为祂是我们的上帝。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華又把所有的民族和住在這地的亞摩利人，都從我們面前趕走；所以我們必事奉耶和華，因為他是我們的　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華又把住此地的亞摩利人都從我們面前趕出去。所以，我們必事奉耶和華，因為他是我們的神。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord drove out from before us all the peoples, even the Amorites who lived in the land. We also will serve the Lord, for He is our God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Lord drove out before us all the nations, including the Amorites, who lived in the land. We too will serve the Lord, because he is our God.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It was the Lord who drove out the Amorites and the other nations living here in the land. So we, too, will serve the Lord, for he alone is our God.”"
      }
    },
