    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.15.1-12",
            "paragraphs": [
              "一五1～12猶大的南界從\"鹽海\"（死海）的南端向西伸展至\"埃及小河\"。北界從死海北端伸展至地中海（這也包括東面和西面的邊界）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "南面的边界始于盐海南端的海湾，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們南方的疆界是從鹽海的極南部分起，就是從南邊的海灣起；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的南界是從鹽海的儘邊，就是從朝南的海汊起，"
      },
