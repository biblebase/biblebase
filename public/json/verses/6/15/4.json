    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.15.1-12",
            "paragraphs": [
              "一五1～12猶大的南界從\"鹽海\"（死海）的南端向西伸展至\"埃及小河\"。北界從死海北端伸展至地中海（這也包括東面和西面的邊界）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "押们、埃及小河，直到地中海岸。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "經過押們，伸展到埃及河，疆界直通到海；這就是他們南方的疆界。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "接連到押們，通到埃及小河，直通到海為止。這就是他們的南界。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It continued to Azmon and proceeded to the brook of Egypt, and the border ended at the sea. This shall be your south border."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It then passed along to Azmon and joined the Wadi of Egypt, ending at the Mediterranean Sea. This is their southern boundary."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From there it passed to Azmon until it finally reached the Brook of Egypt, which it followed to the Mediterranean Sea. This was their southern boundary."
      }
    },
