    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.15.13-19",
            "paragraphs": [
              "一五13～19\"俄陀聶\"在約書亞的閃電攻擊（一○38,39）之後，向底璧進行第二次襲擊。他後來成為以色列的士師（士三9～11）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "迦勒的兄弟基纳斯的儿子俄陀聂攻取了那城，迦勒便把女儿押撒嫁给他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "迦勒的兄弟基納斯的兒子俄陀聶奪取了那城，迦勒就把女兒押撒給他作妻子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "迦勒兄弟基納斯的兒子俄陀聶奪取了那城，迦勒就把女兒押撒給他為妻。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Othniel the son of Kenaz, the brother of Caleb, captured it; so he gave him Achsah his daughter as a wife."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Othniel son of Kenaz, Caleb’s brother, took it; so Caleb gave his daughter Aksah to him in marriage."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Othniel, the son of Caleb’s brother Kenaz, was the one who conquered it, so Acsah became Othniel’s wife."
      }
    },
