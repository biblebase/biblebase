    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jos.15.13-19",
            "paragraphs": [
              "一五13～19\"俄陀聶\"在約書亞的閃電攻擊（一○38,39）之後，向底璧進行第二次襲擊。他後來成為以色列的士師（士三9～11）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "押撒出嫁的时候，劝丈夫向她父亲要一块田。她刚下驴，迦勒便问她：“你想要什么？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "押撒出嫁的時候，慫恿丈夫向她父親求一塊田地；押撒一下驢，迦勒問她：“你要甚麼？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "押撒過門的時候，勸丈夫向她父親求一塊田，押撒一下驢，迦勒問她說：「你要甚麼？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It came about that when she came to him, she persuaded him to ask her father for a field. So she alighted from the donkey, and Caleb said to her, “What do you want?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One day when she came to Othniel, she urged him to ask her father for a field. When she got off her donkey, Caleb asked her, “What can I do for you?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Acsah married Othniel, she urged him to ask her father for a field. As she got down off her donkey, Caleb asked her, “What’s the matter?”"
      }
    },
