    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人人都应该省察自己的行为，即使觉得有可夸之处，自己知道就好了，不必在别人面前炫耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "各人應該省察自己所作的，這樣，他引以為榮的，就只在自己而不在別人了；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "各人應當察驗自己的行為；這樣，他所誇的就專在自己，不在別人了，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But each one must examine his own work, and then he will have reason for boasting in regard to himself alone, and not in regard to another."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Each one should test their own actions. Then they can take pride in themselves alone, without comparing themselves to someone else,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Pay careful attention to your own work, for then you will get the satisfaction of a job well done, and you won’t need to compare yourself to anyone else."
      }
    },
