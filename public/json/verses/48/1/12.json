    "interpretations": {
      "chenzhongdao": {
        "content": [
          {
            "verse": "gal.1.11-12",
            "paragraphs": [
              "一．保羅如何從神領受啟示（1:11-12）"
            ]
          }
        ],
        "title": "新約書信讀經講義",
        "author": "陳終道"
      },
      "matangna": {
        "content": [
          {
            "verse": "gal.1.11-12",
            "paragraphs": [
              "一11，12使徒保羅提出了六個論點來為他的信息和事工抗辯。首先，福音是從神的啟示而來，不是由於人。不是出於人意思是非人創作出來的。我們只要稍為思想一下就能確定此說法。保羅的福音強調神而輕視人，這不會是人設計出來的救恩方法！保羅不是從人領受的，也不是從書本中得教導，乃是直接從耶穌基督啟示來的。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_sermon-2012-07-15",
        "title": "啟示的福音",
        "date": "2012-07-15",
        "preacher": "蘇文峰牧師",
        "verses": "加拉太書 1:11-16",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-07-15.mp3",
        "slides": "http://cbcwla.org/media/ch/SundaySermons/2012/Sermon-2012-07-15.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为这福音既不是我从人那里领受的，也不是我跟人学来的，而是耶稣基督亲自启示我的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為這福音我不是從人領受的，也不是人教導我的，而是藉著耶穌基督的啟示來的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為我不是從人領受的，也不是人教導我的，乃是從耶穌基督啟示來的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I neither received it from man, nor was I taught it, but I received it through a revelation of Jesus Christ."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I did not receive it from any man, nor was I taught it; rather, I received it by revelation from Jesus Christ."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I received my message from no human source, and no one taught me. Instead, I received it by direct revelation from Jesus Christ."
      }
    },
