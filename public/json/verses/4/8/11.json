      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦要把他们献给耶和华，作为以色列人献的摇祭，使他们可以事奉耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞倫要把利未人獻在耶和華面前，作以色列人中的搖祭，使他們辦理耶和華的事務。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫也將他們奉到耶和華面前，為以色列人當作搖祭，使他們好辦耶和華的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Aaron then shall present the Levites before the Lord as a wave offering from the sons of Israel, that they may qualify to perform the service of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Aaron is to present the Levites before the Lord as a wave offering from the Israelites, so that they may be ready to do the work of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Raising his hands, Aaron must then present the Levites to the Lord as a special offering from the people of Israel, thus dedicating them to the Lord’s service."
      }
