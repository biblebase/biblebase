      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘And this shall be your north border: you shall draw your border line from the Great Sea to Mount Hor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘For your northern boundary, run a line from the Mediterranean Sea to Mount Hor"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Your northern boundary will begin at the Mediterranean Sea and run east to Mount Hor,"
      }
