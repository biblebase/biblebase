    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "num.34.16-29",
            "paragraphs": [
              "iii.土地分配者受任命（三十四16～29）",
              "這些人責任何等重大，從他們由神直接指派，就可見一斑（16節；參一5）。這裏只提到了十個支派的首領，因為迦得和流便已經在外約但定居。這些都是古語形式的名字（參一5～15），但在別處出現過的，卻只有迦勒一人（19節；參十三6、30等）。各支派大約是依照定居的次序排名，從南面的猶太和西緬開始，以北面的亞設和拿弗他利作結（參：書十四～十九）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“负责给你们分地的是以利亚撒祭司和嫩的儿子约书亚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“這是要把地業分配給你們的人的名字：以利亞撒祭司和嫩的兒子約書亞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「要給你們分地為業之人的名字是祭司以利亞撒和嫩的兒子約書亞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“These are the names of the men who shall apportion the land to you for inheritance: Eleazar the priest and Joshua the son of Nun."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“These are the names of the men who are to assign the land for you as an inheritance: Eleazar the priest and Joshua son of Nun."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Eleazar the priest and Joshua son of Nun are the men designated to divide the grants of land among the people."
      }
    },
