    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "还要献上一只公山羊作赎罪祭。此外，要照常献上赎罪祭和日常献的燔祭、素祭和奠祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在為贖罪的贖罪祭、常獻的燔祭和同獻的素祭，以及同獻的奠祭以外，還要獻一隻公山羊作贖罪祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又獻一隻公山羊為贖罪祭。這是在贖罪祭和常獻的燔祭，與同獻的素祭並同獻的奠祭以外。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "one male goat for a sin offering, besides the sin offering of atonement and the continual burnt offering and its grain offering, and their drink offerings."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Include one male goat as a sin offering, in addition to the sin offering for atonement and the regular burnt offering with its grain offering, and their drink offerings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must also sacrifice one male goat for a sin offering. This is in addition to the sin offering of atonement and the regular daily burnt offering with its grain offering, and their accompanying liquid offerings."
      }
    },
