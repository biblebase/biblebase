      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只要給耶和華獻燔祭，作馨香的火祭，就是公牛一頭、公綿羊一隻、一歲沒有殘疾的公羊羔七隻；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只要將公牛一隻，公羊一隻，沒有殘疾、一歲的公羊羔七隻作火祭，獻給耶和華為馨香的燔祭；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "but you shall offer a burnt offering, a food offering, with a pleasing aroma to the Lord: one bull, one ram, seven male lambs a year old without blemish,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But you shall present a burnt offering, an offering by fire, as a soothing aroma to the Lord: one bull, one ram, seven male lambs one year old without defect;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Present as an aroma pleasing to the Lord a food offering consisting of a burnt offering of one bull, one ram and seven male lambs a year old, all without defect."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You shall present a burnt offering, an offering made by fire as a sweet aroma to the Lord: one bull, one ram, seven lambs in their first year without blemish,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You must present a burnt offering as a special gift, a pleasing aroma to the Lord. It will consist of one young bull, one ram, and seven one-year-old male lambs, all with no defects."
      }
