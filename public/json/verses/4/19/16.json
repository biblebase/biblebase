    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "num.19.16",
            "paragraphs": [
              "一九16-19人總不能避免偶爾被玷汙；因此，除汙穢的水常常都準備好，任何潔凈的人（不單是祭司）都可以把水灑在被玷汙的人身上。這儀式活畫出基督寶血的潔凈能力（比較來九13,14；約壹一7-9）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人若在野外碰过被杀或自然死亡之人的尸体，或碰过死人的骸骨或坟墓，就七天不洁净。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在田間摸了被刀殺死的，或是自己死的屍體，或是人的骨頭，或是墳墓，就必不潔淨七天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無論何人在田野裏摸了被刀殺的，或是屍首，或是人的骨頭，或是墳墓，就要七天不潔淨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Also, anyone who in the open field touches one who has been slain with a sword or who has died naturally, or a human bone or a grave, shall be unclean for seven days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Anyone out in the open who touches someone who has been killed with a sword or someone who has died a natural death, or anyone who touches a human bone or a grave, will be unclean for seven days."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And if someone in an open field touches the corpse of someone who was killed with a sword or who died a natural death, or if someone touches a human bone or a grave, that person will be defiled for seven days."
      }
    },
