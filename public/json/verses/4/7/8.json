    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "分给米拉利的子孙四辆车和八头牛。他们都在祭司亚伦的儿子以他玛的领导下任职。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又按著米拉利子孫所辦的事務，把四輛車和八頭牛交給他們；他們都受亞倫祭司的兒子以他瑪的管理；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又把四輛車，八隻牛，照米拉利子孫所辦的事交給他們；他們都在祭司亞倫的兒子以他瑪手下。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and four carts and eight oxen he gave to the sons of Merari, according to their service, under the direction of Ithamar the son of Aaron the priest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and he gave four carts and eight oxen to the Merarites, as their work required. They were all under the direction of Ithamar son of Aaron, the priest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and he gave four wagons and eight oxen to the Merarite division for their work. All their work was done under the leadership of Ithamar son of Aaron the priest."
      }
    },
