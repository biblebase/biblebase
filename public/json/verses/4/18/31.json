      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們和你們家屬隨處可以吃；這原是你們的賞賜，是酬你們在會幕裏辦事的勞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
