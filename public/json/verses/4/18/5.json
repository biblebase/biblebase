      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So you shall attend to the obligations of the sanctuary and the obligations of the altar, so that there will no longer be wrath on the sons of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You are to be responsible for the care of the sanctuary and the altar, so that my wrath will not fall on the Israelites again."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You yourselves must perform the sacred duties inside the sanctuary and at the altar. If you follow these instructions, the Lord’s anger will never again blaze against the people of Israel."
      }
