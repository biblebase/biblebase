    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "num.23.11-12",
            "paragraphs": [
              "11～12.巴蘭肯定以色列在神計畫中的特殊地位，當然不是巴勒想要聽的話。和以後的資訊相比，第一個資訊並沒有很詳細地解釋神怎樣幫助以色列；然而耶和華所支持的不是摩押而是以色列，卻是再清楚不過的事。故此巴勒暴跳如雷。巴蘭則請他不要忘記自己受聘時所約定的條件：耶和華傳給我的話，我能不謹慎傳說嗎？（12節；參5節；二十二38）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴勒对巴兰说：“你这是做什么？我请你来咒诅我的仇敌，你竟祝福他们！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴勒對巴蘭說：“你向我作的是甚麼事呢？我領你來咒詛我的仇敵，你反倒給他們祝福。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴勒對巴蘭說：「你向我做的是甚麼事呢？我領你來咒詛我的仇敵，不料，你竟為他們祝福。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Balak said to Balaam, “What have you done to me? I took you to curse my enemies, but behold, you have actually blessed them!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Balak said to Balaam, “What have you done to me? I brought you to curse my enemies, but you have done nothing but bless them!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then King Balak demanded of Balaam, “What have you done to me? I brought you to curse my enemies. Instead, you have blessed them!”"
      }
    },
