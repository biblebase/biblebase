      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要把掠奪得的分為兩半，一半給出去打仗的人，一半給全體會眾。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "把所擄來的分作兩半：一半歸與出去打仗的精兵，一半歸與全會眾。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and divide the booty between the warriors who went out to battle and all the congregation."
      },
