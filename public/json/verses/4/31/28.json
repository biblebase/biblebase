    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要从战士分得的俘虏、牛、驴和羊群中抽出五百分之一献给耶和华，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你又要從出去打仗的人所得的，無論是人、牛、驢，或羊群之中，抽出五百分之一作貢物奉給耶和華；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又要從出去打仗所得的人口、牛、驢、羊羣中，每五百取一，作為貢物奉給耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Levy a tax for the Lord from the men of war who went out to battle, one in five hundred of the persons and of the cattle and of the donkeys and of the sheep;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the soldiers who fought in the battle, set apart as tribute for the Lord one out of every five hundred, whether people, cattle, donkeys or sheep."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From the army’s portion, first give the Lord his share of the plunder—one of every 500 of the prisoners and of the cattle, donkeys, sheep, and goats."
      }
    },
