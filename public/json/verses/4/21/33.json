      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人回师前往巴珊，巴珊王噩率全军出动，在以得来迎战以色列人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人回頭，沿著到巴珊的路上走；巴珊王噩和他的眾民都出來迎著以色列人，在以得來與他們交戰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人轉回，向巴珊去。巴珊王噩和他的眾民都出來，在以得來與他們交戰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they turned and went up by the way of Bashan, and Og the king of Bashan went out with all his people, for battle at Edrei."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they turned and went up along the road toward Bashan, and Og king of Bashan and his whole army marched out to meet them in battle at Edrei."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they turned and marched up the road to Bashan, but King Og of Bashan and all his people attacked them at Edrei."
      }
