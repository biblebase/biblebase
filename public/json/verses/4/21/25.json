      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人奪取了這一切城，也住在亞摩利人的各城，就是希實本，與屬於希實本的一切村莊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人奪取這一切的城邑，也住亞摩利人的城邑，就是希實本與希實本的一切鄉村。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Israel took all these cities and Israel lived in all the cities of the Amorites, in Heshbon, and in all her villages."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Israel captured all the cities of the Amorites and occupied them, including Heshbon and all its surrounding settlements."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Israel captured all the towns of the Amorites and settled in them, including the city of Heshbon and its surrounding villages."
      }
