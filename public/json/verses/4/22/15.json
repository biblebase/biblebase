    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "num.22.15-20",
            "paragraphs": [
              "iii.巴蘭第二次遇見神（二十二15～20）",
              "情節漸加緊張。又一批使臣被差遣來懇求巴蘭，比先前的又多又尊貴（15節），更談到巨額的報酬（17～18節）。然而巴蘭堅持透過夜間異象尋求神的旨意。這個異象記敘得比較簡短，結論卻出乎意外地改變了本來的政策。巴蘭可以去，但他只能說神容許的話，做神容許的事（20節）。本段故意要令聽者或讀者覺得意外，自問表面看來，神怎麽會改變初衷？神至終會否批準巴蘭咒詛以色列？接下來一連串緊張刺激的事件毫不含糊地解答了這類問題。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴勒又派去更多、更尊贵的使臣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是，巴勒又派比先前更多、更尊貴的領袖去，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴勒又差遣使臣，比先前的又多又尊貴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Balak again sent leaders, more numerous and more distinguished than the former."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Balak sent other officials, more numerous and more distinguished than the first."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Balak tried again. This time he sent a larger number of even more distinguished officials than those he had sent the first time."
      }
