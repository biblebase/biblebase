    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要派利未人负责搬运和照料放约柜的圣幕及其所有器具，他们要在圣幕四围扎营。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只要派利未人管理法櫃的帳幕，和其中的一切器具，以及一切屬於帳幕的事物；他們要抬帳幕和其中的一切器具；他們要辦理帳幕的事，在帳幕四周安營。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只要派利未人管法櫃的帳幕和其中的器具，並屬乎帳幕的；他們要擡帳幕和其中的器具，並要辦理帳幕的事，在帳幕的四圍安營。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But you shall appoint the Levites over the tabernacle of the testimony, and over all its furnishings and over all that belongs to it. They shall carry the tabernacle and all its furnishings, and they shall take care of it; they shall also camp around the tabernacle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Instead, appoint the Levites to be in charge of the tabernacle of the covenant law—over all its furnishings and everything belonging to it. They are to carry the tabernacle and all its furnishings; they are to take care of it and encamp around it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Put the Levites in charge of the Tabernacle of the Covenant, along with all its furnishings and equipment. They must carry the Tabernacle and all its furnishings as you travel, and they must take care of it and camp around it."
      }
    },
