      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "統領以薩迦支派隊伍的，是蘇押的兒子拿坦業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "統領以薩迦支派軍隊的是蘇押的兒子拿坦業。"
