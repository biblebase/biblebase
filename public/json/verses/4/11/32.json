    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "民众花了两天一夜的时间去捕捉鹌鹑，每人至少捉了一吨。他们把鹌鹑摊在营地的周围。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人民起來，那一天終日終夜，並次日一整天，都在捕捉鵪鶉，最少的也捕捉了約二千公升；他們都擺在營地的四周。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "百姓起來，終日終夜，並次日一整天，捕取鵪鶉；至少的也取了十賀梅珥，為自己擺列在營的四圍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The people spent all day and all night and all the next day, and gathered the quail (he who gathered least gathered ten homers) and they spread them out for themselves all around the camp."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All that day and night and all the next day the people went out and gathered quail. No one gathered less than ten homers. Then they spread them out all around the camp."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the people went out and caught quail all that day and throughout the night and all the next day, too. No one gathered less than fifty bushels! They spread the quail all around the camp to dry."
      }
