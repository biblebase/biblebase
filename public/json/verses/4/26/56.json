    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "num.26.52-56",
            "paragraphs": [
              "52～56.土地必須按照各支派的大小比例來分配：支派大，得到的產業也會比較大。但為了避免爭論，確實地點則拈鬮決定。這通常視為發現神旨意的肯定有效方法（箴十六33，十八18）。藉拈鬮來決定土地的分配，從摩西之前直到如今，都可以在近東找得著例證。約書亞記十五章以下描述這些指令付諸實行。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无论大小支派，都要用抽签的方法分配产业。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要按著所抽的籤，照著人數的多少，把產業分配給他們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要按着所拈的鬮，看人數多，人數少，把產業分給他們。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Their inheritance shall be divided according to lot between the larger and the smaller.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "According to the selection by lot, their inheritance shall be divided between the larger and the smaller groups.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Each inheritance is to be distributed by lot among the larger and smaller groups.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "According to the lot their inheritance shall be divided between the larger and the smaller.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each grant of land must be assigned by lot among the larger and smaller tribal groups.”"
      }
    },
