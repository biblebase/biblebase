    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "num.26.2",
            "paragraphs": [
              "26:2",
              "你們要將以色列全會眾……計算總數：神第二次命令以色列人數點人數(參1:2~3)，以便預備民眾進入迦南時擔負爭戰的重任(2節)，奪得那地為業(53~56節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们要统计以色列全体会众，按宗族登记所有二十岁以上、有作战能力的男子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們要把以色列會眾，從二十歲以上，在以色列中能出去打仗的，按著他們的父家，計算數目。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們要將以色列全會眾，按他們的宗族，凡以色列中從二十歲以外、能出去打仗的，計算總數。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Take a census of all the congregation of the sons of Israel from twenty years old and upward, by their fathers’ households, whoever is able to go out to war in Israel.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Take a census of the whole Israelite community by families—all those twenty years old or more who are able to serve in the army of Israel.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“From the whole community of Israel, record the names of all the warriors by their families. List all the men twenty years old or older who are able to go to war.”"
      }
    },
