    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约雅斤十八岁登基，在耶路撒冷执政三个月零十天。他做耶和华视为恶的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約雅斤登基的時候，是八歲；他在耶路撒冷作王共三個月零十天，行耶和華看為惡的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約雅斤登基的時候年八歲，在耶路撒冷作王三個月零十天，行耶和華眼中看為惡的事。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Jehoiachin was eighteen years old when he became king, and he reigned three months and ten days in Jerusalem. He did what was evil in the sight of the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jehoiachin was eight years old when he became king, and he reigned three months and ten days in Jerusalem, and he did evil in the sight of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jehoiachin was eighteen years old when he became king, and he reigned in Jerusalem three months and ten days. He did evil in the eyes of the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Jehoiachin was eight years old when he became king, and he reigned in Jerusalem three months and ten days. And he did evil in the sight of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jehoiachin was eighteen years old when he became king, and he reigned in Jerusalem three months and ten days. Jehoiachin did what was evil in the Lord’s sight."
      }
    },
