    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.22.1",
            "paragraphs": [
              "二二1\"亞哈謝\"（即約哈斯，二一17），北國亞哈謝王的侄兒，亞哈的孫子，他跟隨亞哈邪惡的行徑。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶路撒冷的居民拥立约兰的小儿子亚哈谢为王。因为约兰的其他儿子都被那些与阿拉伯人同来攻营的匪徒杀了，于是犹大王约兰的儿子亚哈谢做了王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶路撒冷的居民立了約蘭的小兒子亞哈謝接續他作王；因為那些和阿拉伯人同來攻營的匪徒，把亞哈謝的哥哥全都殺了。這樣，猶大王約蘭的兒子亞哈謝就作了王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷的居民立約蘭的小兒子亞哈謝接續他作王；因為跟隨阿拉伯人來攻營的軍兵曾殺了亞哈謝的眾兄長。這樣，猶大王約蘭的兒子亞哈謝作了王。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the inhabitants of Jerusalem made Ahaziah, his youngest son, king in his place, for the band of men that came with the Arabians to the camp had killed all the older sons. So Ahaziah the son of Jehoram king of Judah reigned."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the inhabitants of Jerusalem made Ahaziah, his youngest son, king in his place, for the band of men who came with the Arabs to the camp had slain all the older sons. So Ahaziah the son of Jehoram king of Judah began to reign."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The people of Jerusalem made Ahaziah, Jehoram’s youngest son, king in his place, since the raiders, who came with the Arabs into the camp, had killed all the older sons. So Ahaziah son of Jehoram king of Judah began to reign."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then the inhabitants of Jerusalem made Ahaziah his youngest son king in his place, for the raiders who came with the Arabians into the camp had killed all the older sons. So Ahaziah the son of Jehoram, king of Judah, reigned."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the people of Jerusalem made Ahaziah, Jehoram’s youngest son, their next king, since the marauding bands who came with the Arabs had killed all the older sons. So Ahaziah son of Jehoram reigned as king of Judah."
      }
    },
