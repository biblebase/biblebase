    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门王所有的杯子都是金的，黎巴嫩林宫里所有的器皿都是纯金的，没有一件是用银子造的，因为所罗门年间银子不算什么。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所羅門王的一切杯爵都是金的；黎巴嫩林宮裡的一切器皿都是精金做的；在所羅門的時代，銀子算不得甚麼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所羅門王一切的飲器都是金的，黎巴嫩林宮裏的一切器皿都是精金的。所羅門年間，銀子算不了甚麼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All King Solomon’s drinking vessels were of gold, and all the vessels of the house of the forest of Lebanon were of pure gold; silver was not considered valuable in the days of Solomon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All King Solomon’s goblets were gold, and all the household articles in the Palace of the Forest of Lebanon were pure gold. Nothing was made of silver, because silver was considered of little value in Solomon’s day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All of King Solomon’s drinking cups were solid gold, as were all the utensils in the Palace of the Forest of Lebanon. They were not made of silver, for silver was considered worthless in Solomon’s day!"
      }
    },
