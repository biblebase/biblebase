    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "也就是木匠和石匠，让他们用来购买凿好的石头、木架和栋梁，整修犹大诸王毁坏的殿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們把銀子交給木匠和石匠，去購買鑿好的石頭和作木架與橫梁的木材，修補猶大列王毀壞了的殿宇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是交給木匠、石匠，買鑿成的石頭和架木與棟樑，修猶大王所毀壞的殿。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They gave it to the carpenters and the builders to buy quarried stone, and timber for binders and beams for the buildings that the kings of Judah had let go to ruin."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They in turn gave it to the carpenters and to the builders to buy quarried stone and timber for couplings and to make beams for the houses which the kings of Judah had let go to ruin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They also gave money to the carpenters and builders to purchase dressed stone, and timber for joists and beams for the buildings that the kings of Judah had allowed to fall into ruin."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They gave it to the craftsmen and builders to buy hewn stone and timber for beams, and to floor the houses which the kings of Judah had destroyed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They hired carpenters and builders, who purchased finished stone for the walls and timber for the rafters and beams. They restored what earlier kings of Judah had allowed to fall into ruin."
      }
    },
