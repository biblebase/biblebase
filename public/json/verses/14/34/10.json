    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把银子交给负责管理耶和华殿的督工，由他们转交给整修耶和华殿的工人，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們又把銀子交給在耶和華殿裡負責監察的人；他們又把銀子發給在耶和華殿裡作工的工人，去修補聖殿。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又將這銀子交給耶和華殿裏督工的，轉交修理耶和華殿的工匠，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they gave it into the hands of the workmen who had the oversight of the house of the Lord, and the workmen who were working in the house of the Lord used it to restore and repair the house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they entrusted it to the men appointed to supervise the work on the Lord’s temple. These men paid the workers who repaired and restored the temple."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He entrusted the money to the men assigned to supervise the restoration of the Lord’s Temple. Then they paid the workers who did the repairs and renovation of the Temple."
      }
    },
