    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他拆毁祭坛和亚舍拉神像，把偶像磨成粉末，砍掉以色列境内所有的香坛，然后回到耶路撒冷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他拆毀了祭壇，把亞舍拉和雕刻的偶像打碎，破碎了以色列全地所有的香壇，就回耶路撒冷去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又拆毀祭壇，把木偶和雕刻的像打碎成灰，砍斷以色列遍地所有的日像，就回耶路撒冷去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "he also tore down the altars and beat the Asherim and the carved images into powder, and chopped down all the incense altars throughout the land of Israel. Then he returned to Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "he tore down the altars and the Asherah poles and crushed the idols to powder and cut to pieces all the incense altars throughout Israel. Then he went back to Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He destroyed the pagan altars and the Asherah poles, and he crushed the idols into dust. He cut down all the incense altars throughout the land of Israel. Finally, he returned to Jerusalem."
      }
