    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说，‘我要照在犹大王面前所读的那书上的一切咒诅，降灾难给这地方及这里的居民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華這樣說：我要按著在猶大王面前誦讀的那卷書上所寫的一切咒詛，降災在這地和這地的居民身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華如此說：我必照着在猶大王面前所讀那書上的一切咒詛，降禍與這地和其上的居民；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Thus says the Lord, Behold, I will bring disaster upon this place and upon its inhabitants, all the curses that are written in the book that was read before the king of Judah."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "thus says the Lord, “Behold, I am bringing evil on this place and on its inhabitants, even all the curses written in the book which they have read in the presence of the king of Judah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘This is what the Lord says: I am going to bring disaster on this place and its people—all the curses written in the book that has been read in the presence of the king of Judah."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Thus says the Lord: ‘Behold, I will bring calamity on this place and on its inhabitants, all the curses that are written in the book which they have read before the king of Judah,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘This is what the Lord says: I am going to bring disaster on this city and its people. All the curses written in the scroll that was read to the king of Judah will come true."
      }
    },
