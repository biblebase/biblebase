      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华曾指着祂的殿说：“我的名必永远在耶路撒冷。”他却在耶和华的殿内建造异教的祭坛。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又在耶和華的殿中築壇，耶和華曾經指著這殿說：“我的名必永遠留在耶路撒冷。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在耶和華的殿宇中築壇－耶和華曾指着這殿說：「我的名必永遠在耶路撒冷。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He built altars in the house of the Lord of which the Lord had said, “My name shall be in Jerusalem forever.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He built altars in the temple of the Lord, of which the Lord had said, “My Name will remain in Jerusalem forever.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He built pagan altars in the Temple of the Lord, the place where the Lord had said, “My name will remain in Jerusalem forever.”"
      }
