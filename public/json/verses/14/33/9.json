    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛拿西诱使犹大人和耶路撒冷的居民作恶，比耶和华在以色列人面前所毁灭的各族更严重。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "可是瑪拿西卻引誘猶大人和耶路撒冷的居民行惡，比耶和華在以色列人面前除滅的列國更厲害。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "瑪拿西引誘猶大和耶路撒冷的居民，以致他們行惡比耶和華在以色列人面前所滅的列國更甚。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus Manasseh misled Judah and the inhabitants of Jerusalem to do more evil than the nations whom the Lord destroyed before the sons of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Manasseh led Judah and the people of Jerusalem astray, so that they did more evil than the nations the Lord had destroyed before the Israelites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Manasseh led the people of Judah and Jerusalem to do even more evil than the pagan nations that the Lord had destroyed when the people of Israel entered the land."
      }
    },
