    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大人来到旷野的瞭望塔俯瞰那支大军，发现敌军尸横遍野，无一幸免。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大人來到曠野的瞭望樓，向那大軍觀望，只見伏屍遍地，沒有一個逃脫的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大人來到曠野的望樓，向那大軍觀看，見屍橫遍地，沒有一個逃脫的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Judah came to the lookout of the wilderness, they looked toward the multitude, and behold, they were corpses lying on the ground, and no one had escaped."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the men of Judah came to the place that overlooks the desert and looked toward the vast army, they saw only dead bodies lying on the ground; no one had escaped."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So when the army of Judah arrived at the lookout point in the wilderness, all they saw were dead bodies lying on the ground as far as they could see. Not a single one of the enemy had escaped."
      }
    },
