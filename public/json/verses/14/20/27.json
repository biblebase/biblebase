    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大人和耶路撒冷人在约沙法的带领下，欢欢喜喜地返回耶路撒冷，因为耶和华使他们战胜了仇敌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "事後，猶大和耶路撒冷所有的人，在約沙法率領下，都歡歡喜喜回耶路撒冷去了，耶和華使他們因勝過仇敵歡喜快樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大人和耶路撒冷人都歡歡喜喜地回耶路撒冷，約沙法率領他們；因為耶和華使他們戰勝仇敵，就歡喜快樂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every man of Judah and Jerusalem returned with Jehoshaphat at their head, returning to Jerusalem with joy, for the Lord had made them to rejoice over their enemies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then, led by Jehoshaphat, all the men of Judah and Jerusalem returned joyfully to Jerusalem, for the Lord had given them cause to rejoice over their enemies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then all the men returned to Jerusalem, with Jehoshaphat leading them, overjoyed that the Lord had given them victory over their enemies."
      }
    },
