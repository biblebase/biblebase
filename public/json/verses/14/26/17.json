    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.26.16-20",
            "paragraphs": [
              "二六16～20神懲罰烏西雅，使他長大麻風，因為他侵奪祭司的職權。參看出埃及記三十章1至10節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司亚撒利雅率领八十位耶和华勇敢的祭司跟着进殿，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "跟隨烏西雅進去的，有亞撒利雅祭司，還有耶和華的祭司八十個，都是英勇的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司亞撒利雅率領耶和華勇敢的祭司八十人，跟隨他進去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Azariah the priest entered after him and with him eighty priests of the Lord, valiant men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Azariah the priest with eighty other courageous priests of the Lord followed him in."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Azariah the high priest went in after him with eighty other priests of the Lord, all brave men."
      }
    },
