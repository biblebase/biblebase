    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.26.5",
            "paragraphs": [
              "二六5\"撒迦利亞\"。不是被擄歸回時期的先知撒迦利亞，而是另一個同名但我們不認識的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撒迦利亚在世之日，乌西雅寻求上帝，撒迦利亚教导他敬畏上帝。当他寻求上帝时，上帝就使他亨通。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒迦利亞在世的日子，教導烏西雅敬畏　神，所以烏西雅常常尋求　神；烏西雅尋求　神的時候，　神就使他亨通。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "通曉神默示，撒迦利亞在世的時候，烏西雅定意尋求神；他尋求耶和華，神就使他亨通。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He continued to seek God in the days of Zechariah, who had understanding through the vision of God; and as long as he sought the Lord, God prospered him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He sought God during the days of Zechariah, who instructed him in the fear of God. As long as he sought the Lord, God gave him success."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Uzziah sought God during the days of Zechariah, who taught him to fear God. And as long as the king sought guidance from the Lord, God gave him success."
      }
    },
