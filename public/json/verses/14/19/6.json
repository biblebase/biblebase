    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对他们说：“你们办事必须谨慎，因为你们不是为人而是为耶和华断案，你们断案的时候，耶和华必与你们同在。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他對他們說：“你們行事，必須謹慎；因為你們審判，不是為人，而是為耶和華；你們審判的時候，耶和華必和你們同在。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對他們說：「你們辦事應當謹慎；因為你們判斷不是為人，乃是為耶和華。判斷的時候，他必與你們同在。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to the judges, “Consider what you are doing, for you do not judge for man but for the Lord who is with you when you render judgment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He told them, “Consider carefully what you do, because you are not judging for mere mortals but for the Lord, who is with you whenever you give a verdict."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and he said to them, “Always think carefully before pronouncing judgment. Remember that you do not judge to please people but to please the Lord. He will be with you when you render the verdict in each case."
      }
    },
