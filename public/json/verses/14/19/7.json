    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在你们应当敬畏耶和华，谨慎办事，因为我们的上帝耶和华毫无不义，不偏待人，不受贿赂。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在你們要敬畏耶和華；你們要謹慎行事，因為耶和華我們的　神沒有不義，不偏待人，也不受賄賂。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在你們應當敬畏耶和華，謹慎辦事；因為耶和華－我們的神沒有不義，不偏待人，也不受賄賂。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now then, let the fear of the Lord be upon you. Be careful what you do, for there is no injustice with the Lord our God, or partiality or taking bribes.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now then let the fear of the Lord be upon you; be very careful what you do, for the Lord our God will have no part in unrighteousness or partiality or the taking of a bribe.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now let the fear of the Lord be on you. Judge carefully, for with the Lord our God there is no injustice or partiality or bribery.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now therefore, let the fear of the Lord be upon you; take care and do it, for there  is no iniquity with the Lord our God, no partiality, nor taking of bribes.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fear the Lord and judge with integrity, for the Lord our God does not tolerate perverted justice, partiality, or the taking of bribes.”"
      }
    },
