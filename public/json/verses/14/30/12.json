    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华上帝感动犹大人，使他们一心遵从王与众官员奉祂的指示所下的命令。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神的能力也幫助猶大人，使他們一心遵行王和眾領袖奉耶和華的命令發出的吩咐。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神也感動猶大人，使他們一心遵行王與眾首領憑耶和華之言所發的命令。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The hand of God was also on Judah to give them one heart to do what the king and the princes commanded by the word of the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The hand of God was also on Judah to give them one heart to do what the king and the princes commanded by the word of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Also in Judah the hand of God was on the people to give them unity of mind to carry out what the king and his officials had ordered, following the word of the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Also the hand of God was on Judah to give them singleness of heart to obey the command of the king and the leaders, at the word of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At the same time, God’s hand was on the people in the land of Judah, giving them all one heart to obey the orders of the king and his officials, who were following the word of the Lord."
      }
    },
