    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后照上帝的仆人摩西的律法照常供职。祭司从利未人手中接过血，洒在坛上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們遵照神人摩西的律法，按著定例，站在自己的崗位上。祭司從利未人手裡把血接過來，灑在祭壇上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "遵着神人摩西的律法，照例站在自己的地方；祭司從利未人手裏接過血來，灑在壇上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They stood at their stations after their custom, according to the law of Moses the man of God; the priests sprinkled the blood which they received from the hand of the Levites."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they took up their regular positions as prescribed in the Law of Moses the man of God. The priests splashed against the altar the blood handed to them by the Levites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they took their places at the Temple as prescribed in the Law of Moses, the man of God. The Levites brought the sacrificial blood to the priests, who then sprinkled it on the altar."
      }
    },
