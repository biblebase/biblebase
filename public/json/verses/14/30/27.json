    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "利未祭司起来为民众祝福，他们的祷告得蒙垂听，达到天上的圣所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，利未支派的祭司起來，給人民祝福；他們的聲音蒙了垂聽，他們的禱告達到天上　神的聖所那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，祭司、利未人起來，為民祝福。他們的聲音蒙神垂聽，他們的禱告達到天上的聖所。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then the priests and the Levites arose and blessed the people, and their voice was heard, and their prayer came to his holy habitation in heaven."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Levitical priests arose and blessed the people; and their voice was heard and their prayer came to His holy dwelling place, to heaven."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priests and the Levites stood to bless the people, and God heard them, for their prayer reached heaven, his holy dwelling place."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then the priests, the Levites, arose and blessed the people, and their voice was heard; and their prayer came up to His holy dwelling place, to heaven."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the priests and Levites stood and blessed the people, and God heard their prayer from his holy dwelling in heaven."
      }
