    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希西迦慰劳所有善于事奉耶和华的利未人。于是，众人欢宴过节七天，又献上平安祭，称谢他们祖先的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希西家慰勉所有善於事奉耶和華的利未人；眾人吃節筵七日，又獻上平安祭，稱頌耶和華他們列祖的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希西家慰勞一切善於事奉耶和華的利未人。於是眾人吃節筵七日，又獻平安祭，且向耶和華－他們列祖的神認罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Hezekiah spoke encouragingly to all the Levites who showed good insight in the things of the Lord. So they ate for the appointed seven days, sacrificing peace offerings and giving thanks to the Lord God of their fathers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hezekiah spoke encouragingly to all the Levites, who showed good understanding of the service of the Lord. For the seven days they ate their assigned portion and offered fellowship offerings and praised the Lord, the God of their ancestors."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Hezekiah encouraged all the Levites regarding the skill they displayed as they served the Lord. The celebration continued for seven days. Peace offerings were sacrificed, and the people gave thanks to the Lord, the God of their ancestors."
      }
    },
