    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所罗门又动工建造至圣所，长九米，宽九米，与殿的宽度一样，里面共贴了二十一吨纯金。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又建造了至聖所，長九公尺，和殿的寬度相等，寬度也是九公尺；裡面都貼上精金，共用了二十公噸金子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又建造至聖所，長二十肘，與殿的寬窄一樣，寬也是二十肘；貼上精金，共用金子六百他連得。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now he made the room of the holy of holies: its length across the width of the house was twenty cubits, and its width was twenty cubits; and he overlaid it with fine gold, amounting to 600 talents."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He built the Most Holy Place, its length corresponding to the width of the temple—twenty cubits long and twenty cubits wide. He overlaid the inside with six hundred talents of fine gold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He made the Most Holy Place 30 feet wide, corresponding to the width of the Temple, and 30 feet deep. He overlaid its interior with 23 tons of fine gold."
      }
    },
