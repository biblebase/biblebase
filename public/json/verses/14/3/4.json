    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.3.4",
            "paragraphs": [
              "三4\"高一百二十肘\"，變成一座高約一百八十至二百零九英尺（55～64米）的高樓。但有人認為那是抄寫上的錯誤，應作\"高二十肘\"（30～35英尺，或9～10.6米）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "殿前的门廊长九米，与殿的宽度一样，高九米，里面都贴上纯金。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "殿前走廊長九公尺，與殿的寬度相等；高九公尺；裡面貼上純金。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "殿前的廊子長二十肘，與殿的寬窄一樣，高一百二十肘；裏面貼上精金。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The porch which was in front of the house was as long as the width of the house, twenty cubits, and the height 120; and inside he overlaid it with pure gold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The portico at the front of the temple was twenty cubits long across the width of the building and twenty cubits high. He overlaid the inside with pure gold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The entry room at the front of the Temple was 30 feet wide, running across the entire width of the Temple, and 30 feet high. He overlaid the inside with pure gold."
      }
    },
