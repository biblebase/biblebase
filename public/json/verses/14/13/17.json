    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.13.15-20",
            "paragraphs": [
              "一三15～20神顯然用了一些超乎自然的方法來幹預，使猶大得到勝利。八十萬士兵（一三3）死了五十萬，使北國嚴重地受創（17,20節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚比雅和他的军队大败以色列人，歼灭了他们五十万精兵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞比雅和他的人民大大地擊殺以色列人；以色列人中倒斃的精兵共有五十萬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞比雅和他的軍兵大大殺戮以色列人，以色列人仆倒死亡的精兵有五十萬。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Abijah and his people defeated them with a great slaughter, so that 500,000 chosen men of Israel fell slain."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Abijah and his troops inflicted heavy losses on them, so that there were five hundred thousand casualties among Israel’s able men."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Abijah and his army inflicted heavy losses on them; 500,000 of Israel’s select troops were killed that day."
      }
    },
