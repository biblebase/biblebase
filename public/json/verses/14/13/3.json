    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有一次，亚比雅率领四十万精兵出战，耶罗波安率领八十万精兵列阵迎战。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一次，亞比雅帶著精兵四十萬，都是英勇的戰士，準備開戰；耶羅波安也帶著八十萬精兵，都是英勇的戰士，列陣對抗亞比雅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一次亞比雅率領挑選的兵四十萬擺陣，都是勇敢的戰士；耶羅波安也挑選大能的勇士八十萬，對亞比雅擺陣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Abijah went out to battle, having an army of valiant men of war, 400,000 chosen men. And Jeroboam drew up his line of battle against him with 800,000 chosen mighty warriors."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Abijah began the battle with an army of valiant warriors, 400,000 chosen men, while Jeroboam drew up in battle formation against him with 800,000 chosen men who were valiant warriors."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Abijah went into battle with an army of four hundred thousand able fighting men, and Jeroboam drew up a battle line against him with eight hundred thousand able troops."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Abijah set the battle in order with an army of valiant warriors, four hundred thousand choice men. Jeroboam also drew up in battle formation against him with eight hundred thousand choice men, mighty men of valor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Judah, led by King Abijah, fielded 400,000 select warriors, while Jeroboam mustered 800,000 select troops from Israel."
      }
    },
