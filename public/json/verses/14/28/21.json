    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.28.16-21",
            "paragraphs": [
              "二八16～21有關這時期的更多事件，參看列王紀下十五章27至31節的腳註。\"欺淩他\"（20節）。要求他進貢。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚哈斯从耶和华的殿里、王宫和官员家中取财宝送给亚述王，但无济于事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞哈斯取了耶和華殿裡和王宮中，以及眾領袖家裡的財寶，給了亞述王，但也無濟於事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈斯從耶和華殿裏和王宮中，並首領家內所取的財寶給了亞述王，這也無濟於事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Although Ahaz took a portion out of the house of the Lord and out of the palace of the king and of the princes, and gave it to the king of Assyria, it did not help him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ahaz took some of the things from the temple of the Lord and from the royal palace and from the officials and presented them to the king of Assyria, but that did not help him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ahaz took valuable items from the Lord’s Temple, the royal palace, and from the homes of his officials and gave them to the king of Assyria as tribute. But this did not help him."
      }
    },
