      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚哈斯二十岁登基，在耶路撒冷执政十六年。他没有效法他祖先大卫做耶和华视为正的事，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞哈斯登基的時候，是二十歲；他在耶路撒冷作王十六年。他不像他的祖先大衛一樣，行耶和華看為正的事，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈斯登基的時候年二十歲，在耶路撒冷作王十六年；不像他祖大衛行耶和華眼中看為正的事，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ahaz was twenty years old when he became king, and he reigned sixteen years in Jerusalem; and he did not do right in the sight of the Lord as David his father had done."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ahaz was twenty years old when he became king, and he reigned in Jerusalem sixteen years. Unlike David his father, he did not do what was right in the eyes of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ahaz was twenty years old when he became king, and he reigned in Jerusalem sixteen years. He did not do what was pleasing in the sight of the Lord, as his ancestor David had done."
      }
