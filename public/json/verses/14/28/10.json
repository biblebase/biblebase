    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.28.9-15",
            "paragraphs": [
              "二八9～15列王紀下沒有這部分的記載。這裏敘述一位只在這裏提及的先知俄德怎樣反對北國拘留他們從南國帶來的戰犯，原因是他們是兄弟。結果戰犯得以穿上衣服，獲得食物的供應，然後被送回耶利哥去。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在你们竟还想让犹大和耶路撒冷的男女做你们的仆俾。你们岂不也得罪你们的上帝耶和华吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在你們又想強迫猶大人和耶路撒冷人作你們的奴婢。難道你們自己就沒有得罪耶和華你們的　神嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "如今你們又有意強逼猶大人和耶路撒冷人作你們的奴婢，你們豈不也有得罪耶和華－你們神的事嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And now you intend to subjugate the people of Judah and Jerusalem, male and female, as your slaves. Have you not sins of your own against the Lord your God?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now you are proposing to subjugate for yourselves the people of Judah and Jerusalem for male and female slaves. Surely, do you not have transgressions of your own against the Lord your God?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And now you intend to make the men and women of Judah and Jerusalem your slaves. But aren’t you also guilty of sins against the Lord your God?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And now you propose to force the children of Judah and Jerusalem to be your male and female slaves; but  are you not also guilty before the Lord your God?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And now you are planning to make slaves of these people from Judah and Jerusalem. What about your own sins against the Lord your God?"
      }
    },
