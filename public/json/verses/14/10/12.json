    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "过了三天，耶罗波安和百姓遵照罗波安王的话来见他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶羅波安和全體人民都遵照羅波安王所說“你們第三天再來見我”的話，第三天來見羅波安。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶羅波安和眾百姓遵着羅波安王所說「你們第三日再來見我」的那話，第三日他們果然來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Jeroboam and all the people came to Rehoboam on the third day as the king had directed, saying, “Return to me on the third day.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Three days later Jeroboam and all the people returned to Rehoboam, as the king had said, “Come back to me in three days.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Three days later Jeroboam and all the people returned to hear Rehoboam’s decision, just as the king had ordered."
      }
    },
