    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说：“百姓求我减轻我父亲加给他们的重担。你们认为我该怎样回复他们？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "問他們：“這人民對我說：求你減輕你父親加在我們身上的重軛吧。你們有甚麼提議，我好回覆他們呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：「這民對我說：『你父親使我們負重軛，求你使我們輕鬆些』；你們給我出個甚麼主意，我好回覆他們。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he said to them, “What counsel do you give that we may answer this people, who have spoken to me, saying, ‘Lighten the yoke which your father put on us’?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He asked them, “What is your advice? How should we answer these people who say to me, ‘Lighten the yoke your father put on us’?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What is your advice?” he asked them. “How should I answer these people who want me to lighten the burdens imposed by my father?”"
      }
    },
