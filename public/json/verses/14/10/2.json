    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尼八的儿子耶罗波安曾为了躲避所罗门王而逃往埃及，并一直住在那里。他听到消息便返回以色列。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，尼八的兒子耶羅波安，因為逃避所羅門王，住在埃及；他一聽見這事，就從埃及回來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "尼八的兒子耶羅波安先前躲避所羅門王，逃往埃及，住在那裏；他聽見這事，就從埃及回來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Jeroboam the son of Nebat heard of it (for he was in Egypt where he had fled from the presence of King Solomon), Jeroboam returned from Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jeroboam son of Nebat heard this (he was in Egypt, where he had fled from King Solomon), he returned from Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jeroboam son of Nebat heard of this, he returned from Egypt, for he had fled to Egypt to escape from King Solomon."
      }
    },
