    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们回禀说：“王若善待这些百姓，使他们喜悦，对他们好言相待，他们会永远做王的仆人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們對他說：“如果王善待人民，使他們喜悅，對他們說好話，他們就必永遠作你的僕人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "老年人對他說：「王若恩待這民，使他們喜悅，用好話回覆他們，他們就永遠作王的僕人。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they said to him, “If you will be good to this people and please them and speak good words to them, then they will be your servants forever.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They spoke to him, saying, “If you will be kind to this people and please them and speak good words to them, then they will be your servants forever.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They replied, “If you will be kind to these people and please them and give them a favorable answer, they will always be your servants.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they spoke to him, saying, “If you are kind to these people, and please them, and speak good words to them, they will be your servants forever.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The older counselors replied, “If you are good to these people and do your best to please them and give them a favorable answer, they will always be your loyal subjects.”"
      }
    },
