    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.35.12",
            "paragraphs": [
              "三五12有關\"燔祭\"這方面的詳情，參看利未記三章3至5節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们按照宗族把燔祭分给众人，好使他们按照摩西书上所写的，把祭物献给耶和华。献牛也是这样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "把作燔祭的部分拿去，按著各家族的系別分給眾民，使他們可以照著摩西的書上所寫的，奉獻給耶和華。奉獻公牛也是這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將燔祭搬來，按着宗族的班次分給眾民，好照摩西書上所寫的，獻給耶和華；獻牛也是這樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they removed the burnt offerings that they might give them to the sections of the fathers’ households of the lay people to present to the Lord, as it is written in the book of Moses. They did this also with the bulls."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They set aside the burnt offerings to give them to the subdivisions of the families of the people to offer to the Lord, as it is written in the Book of Moses. They did the same with the cattle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They divided the burnt offerings among the people by their family groups, so they could offer them to the Lord as prescribed in the Book of Moses. They did the same with the cattle."
      }
    },
