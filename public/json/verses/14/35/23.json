      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约西亚王在战场上中箭受伤，就对仆人说：“我受了重伤，带我离开吧。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "弓箭手射中了約西亞王；王對他的臣僕說：“你們把我帶走吧，因為我受了重傷。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "弓箭手射中約西亞王。王對他的臣僕說：「我受了重傷，你拉我出陣吧！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The archers shot King Josiah, and the king said to his servants, “Take me away, for I am badly wounded.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Archers shot King Josiah, and he told his officers, “Take me away; I am badly wounded.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the enemy archers hit King Josiah with their arrows and wounded him. He cried out to his men, “Take me from the battle, for I am badly wounded!”"
      }
