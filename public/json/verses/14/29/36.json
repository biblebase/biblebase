    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希西迦和民众因上帝为他们所做的而欢喜快乐，因为这事情很快成就了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為事情很快辦完，希西家和全體人民因　神為他的子民所預備的，都甚歡樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這事辦的甚速，希西家和眾民都喜樂，是因神為眾民所預備的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Hezekiah and all the people rejoiced over what God had prepared for the people, because the thing came about suddenly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hezekiah and all the people rejoiced at what God had brought about for his people, because it was done so quickly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And Hezekiah and all the people rejoiced because of what God had done for the people, for everything had been accomplished so quickly."
      }
