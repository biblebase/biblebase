    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的祖先行事不忠，做我们的上帝耶和华视为恶的事，背弃祂，转脸背向祂的居所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們的列祖冒犯了　神，行了耶和華我們的　神看為惡的事，離棄了他，轉臉不顧耶和華的居所，以背向他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們列祖犯了罪，行耶和華－我們神眼中看為惡的事，離棄他，轉臉背向他的居所，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For our fathers have been unfaithful and have done what was evil in the sight of the Lord our God. They have forsaken him and have turned away their faces from the habitation of the Lord and turned their backs."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For our fathers have been unfaithful and have done evil in the sight of the Lord our God, and have forsaken Him and turned their faces away from the dwelling place of the Lord, and have turned their backs."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Our parents were unfaithful; they did evil in the eyes of the Lord our God and forsook him. They turned their faces away from the Lord’s dwelling place and turned their backs on him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For our fathers have trespassed and done evil in the eyes of the Lord our God; they have forsaken Him, have turned their faces away from the dwelling place of the Lord, and turned their backs on  Him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Our ancestors were unfaithful and did what was evil in the sight of the Lord our God. They abandoned the Lord and his dwelling place; they turned their backs on him."
      }
    },
