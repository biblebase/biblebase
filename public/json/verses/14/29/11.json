    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的孩子们啊，不要懈怠！因为耶和华拣选你们站在祂面前事奉祂，向祂烧香。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的孩子們，你們現在不要懈怠，因為耶和華揀選了你們站在他面前事奉他，為要作他的僕人，向他燒香。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的眾子啊，現在不要懈怠；因為耶和華揀選你們站在他面前事奉他，與他燒香。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My sons, do not be negligent now, for the Lord has chosen you to stand before Him, to minister to Him, and to be His ministers and burn incense.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My sons, do not be negligent now, for the Lord has chosen you to stand before him and serve him, to minister before him and to burn incense.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My sons, do not neglect your duties any longer! The Lord has chosen you to stand in his presence, to minister to him, and to lead the people in worship and present offerings to him.”"
      }
    },
