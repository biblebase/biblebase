      "leili": {
        "content": [
          {
            "verse": "2ch.32.9-19",
            "paragraphs": [
              "三二9～19有關西拿基立仆人的辱駡和希西家的回應，參看列王紀下十八章17至32節和十九章8至13節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他把耶路撒冷的上帝与世间人手所造的神像相提并论。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們論到耶路撒冷的　神，就好像世上萬民的神一樣，把他當作是人手所做的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們論耶路撒冷的神，如同論世上人手所造的神一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They spoke of the God of Jerusalem as of the gods of the peoples of the earth, the work of men’s hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They spoke about the God of Jerusalem as they did about the gods of the other peoples of the world—the work of human hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These officers talked about the God of Jerusalem as though he were one of the pagan gods, made by human hands."
      }
    },
