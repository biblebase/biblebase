      "leili": {
        "content": [
          {
            "verse": "2ch.2.8",
            "paragraphs": [
              "二8\"檀香木\"。參看列王紀上十章10、11節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "也请你从黎巴嫩运些香柏木、松木和檀香木来，因为我知道你的仆人擅长砍伐黎巴嫩的树木。我会派人与你的仆人一起工作。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又請你從黎巴嫩運送香柏木、松木和檀香木到我這裡來，因為我知道你的臣僕善於砍伐黎巴嫩的樹木；我的臣僕可以和你的臣僕一同工作。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又求你從黎巴嫩運些香柏木、松木、檀香木到我這裏來，因我知道你的僕人善於砍伐黎巴嫩的樹木。我的僕人也必與你的僕人同工。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Send me also cedar, cypress, and algum timber from Lebanon, for I know that your servants know how to cut timber in Lebanon. And my servants will be with your servants,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Send me also cedar, cypress and algum timber from Lebanon, for I know that your servants know how to cut timber of Lebanon; and indeed my servants will work with your servants,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Send me also cedar, juniper and algum logs from Lebanon, for I know that your servants are skilled in cutting timber there. My servants will work with yours"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Also send me cedar and cypress and algum logs from Lebanon, for I know that your servants have skill to cut timber in Lebanon; and indeed my servants will  be with your servants,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Also send me cedar, cypress, and red sandalwood logs from Lebanon, for I know that your men are without equal at cutting timber in Lebanon. I will send my men to help them."
      }
    },
