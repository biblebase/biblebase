    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.11.20",
            "paragraphs": [
              "一一20\"瑪迦\"。押沙龍的孫女。參看列王紀上十五章1、2節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来，他又娶了押沙龙的女儿玛迦，生了亚比雅、亚太、细撒、示罗密。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "後來，羅波安又娶了押沙龍的外孫女瑪迦；瑪迦給他生了亞比雅、亞太、細撒和示羅密。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "後來又娶押沙龍的女兒瑪迦，從她生了亞比雅、亞太、細撒、示羅密。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After her he took Maacah the daughter of Absalom, and she bore him Abijah, Attai, Ziza and Shelomith."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he married Maakah daughter of Absalom, who bore him Abijah, Attai, Ziza and Shelomith."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Later Rehoboam married another cousin, Maacah, the granddaughter of Absalom. Maacah gave birth to Abijah, Attai, Ziza, and Shelomith."
      }
    },
