    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "罗波安回到耶路撒冷，从犹大和便雅悯支派召集了十八万精兵，要攻打以色列，收复全国。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "羅波安回到耶路撒冷，就召集猶大家和便雅憫家，共十八萬人，都是精兵，要和以色列人交戰，好把王國奪回，再歸自己。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "羅波安來到耶路撒冷，招聚猶大家和便雅憫家，共十八萬人，都是挑選的戰士，要與以色列人爭戰，好將國奪回再歸自己。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when Rehoboam had come to Jerusalem, he assembled the house of Judah and Benjamin, 180,000 chosen men who were warriors, to fight against Israel to restore the kingdom to Rehoboam."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Rehoboam arrived in Jerusalem, he mustered Judah and Benjamin—a hundred and eighty thousand able young men—to go to war against Israel and to regain the kingdom for Rehoboam."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Rehoboam arrived at Jerusalem, he mobilized the men of Judah and Benjamin—180,000 select troops—to fight against Israel and to restore the kingdom to himself."
      }
    },
