    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.11.13-14",
            "paragraphs": [
              "一一13,14耶羅波安登基後不久（參看王上一二31的腳註）及其後（留意14節的\"和他的兒子\"），忠心的祭司和利未人便從以色列移居到猶大。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列全境的祭司和利未人都从四面八方来投奔罗波安。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "全以色列的祭司和利未人，都從各地來歸向羅波安。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列全地的祭司和利未人都從四方來歸羅波安。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the priests and the Levites who were in all Israel presented themselves to him from all places where they lived."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, the priests and the Levites who were in all Israel stood with him from all their districts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priests and Levites from all their districts throughout Israel sided with him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And from all their territories the priests and the Levites who were in all Israel took their stand with him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But all the priests and Levites living among the northern tribes of Israel sided with Rehoboam."
      }
