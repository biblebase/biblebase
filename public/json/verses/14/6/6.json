    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.6.6",
            "paragraphs": [
              "六6關於神與戴維所立的約，參看塞繆爾記下七章12至16節的腳註。六13“銅臺”。就是他跪在其上的平臺（王上八54）。六14有關所羅門這偉大禱告的思路，參看列王紀上八章23至53節的腳註。六20“向此處”。猶太人從字義理解這話，於是向著耶路撒冷的方向來禱告（比較38節）。參看但以理書六章10節的腳註。六36這是舊約有關人類普遍罪性的聲明。六41“求你起來，和……約櫃同入安息之所”。指約櫃安放在聖殿裏。六42“慈愛”。希伯來文是hesed。即給予戴維的應許。參看何西阿書二章19節的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但如今，我拣选了耶路撒冷作为我名所在之地，又拣选了大卫来治理我的以色列子民。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我卻揀選了耶路撒冷，使我的名留在那裡；也揀選了大衛，統治我的子民以色列。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但選擇耶路撒冷為我名的居所，又揀選大衛治理我民以色列。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but I have chosen Jerusalem that My name might be there, and I have chosen David to be over My people Israel.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But now I have chosen Jerusalem for my Name to be there, and I have chosen David to rule my people Israel.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But now I have chosen Jerusalem as the place for my name to be honored, and I have chosen David to be king over my people Israel.’”"
      }
    },
