    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你的子民奉你的命令上阵与敌人作战，无论他们在哪里，如果他们向着你选择的这城和我为你建造的这殿祷告，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果你的子民在你派他們出去的路上，和仇敵作戰，他們向著你揀選的這城，和向著我為你的名建造的這殿向你禱告，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你的民若奉你的差遣，無論往何處去與仇敵爭戰，向你所選擇的城與我為你名所建造的殿禱告，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“When Your people go out to battle against their enemies, by whatever way You shall send them, and they pray to You toward this city which You have chosen and the house which I have built for Your name,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When your people go to war against their enemies, wherever you send them, and when they pray to you toward this city you have chosen and the temple I have built for your Name,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If your people go out where you send them to fight their enemies, and if they pray to you by turning toward this city you have chosen and toward this Temple I have built to honor your name,"
      }
    },
