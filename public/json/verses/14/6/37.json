    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若他们在那里回心转意，向你恳求，承认自己犯罪作恶了；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果他們在被擄去的地方回心轉意，在被擄去的地方祈求你說：‘我們犯了罪了，我們作了孽了，我們行了惡了。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們若在擄到之地想起罪來，回心轉意，懇求你說：『我們有罪了，我們悖逆了，我們作惡了』；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "yet if they turn their heart in the land to which they have been carried captive, and repent and plead with you in the land of their captivity, saying, ‘We have sinned and have acted perversely and wickedly,’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "if they take thought in the land where they are taken captive, and repent and make supplication to You in the land of their captivity, saying, ‘We have sinned, we have committed iniquity and have acted wickedly’;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and if they have a change of heart in the land where they are held captive, and repent and plead with you in the land of their captivity and say, ‘We have sinned, we have done wrong and acted wickedly’;"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "yet when they come to themselves in the land where they were carried captive, and repent, and make supplication to You in the land of their captivity, saying, ‘We have sinned, we have done wrong, and have committed wickedness’;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But in that land of exile, they might turn to you in repentance and pray, ‘We have sinned, done evil, and acted wickedly.’"
      }
    },
