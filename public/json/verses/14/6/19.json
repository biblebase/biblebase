    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是，我的上帝耶和华啊，求你垂顾你仆人的祷告和祈求，垂听你仆人在你面前的呼求和祷告。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華我的　神啊，求你垂注你僕人的禱告和祈求，聆聽你僕人在你面前所發的呼喊和禱告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟求耶和華－我的神垂顧僕人的禱告祈求，俯聽僕人在你面前的祈禱呼籲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet have regard to the prayer of Your servant and to his supplication, O Lord my God, to listen to the cry and to the prayer which Your servant prays before You;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet, Lord my God, give attention to your servant’s prayer and his plea for mercy. Hear the cry and the prayer that your servant is praying in your presence."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Nevertheless, listen to my prayer and my plea, O Lord my God. Hear the cry and the prayer that your servant is making to you."
      }
    },
