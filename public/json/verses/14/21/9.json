    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约兰率将领及所有战车前去讨伐，结果被以东人包围。但他们乘夜突出重围。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約蘭率領他的眾軍長和所有的戰車一同前去；他夜間起來，攻擊那些包圍他的以東人和戰車隊長。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約蘭就率領軍長和所有的戰車，夜間起來，攻擊圍困他的以東人和車兵長。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Jehoram passed over with his commanders and all his chariots, and he rose by night and struck the Edomites who had surrounded him and his chariot commanders."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jehoram crossed over with his commanders and all his chariots with him. And he arose by night and struck down the Edomites who were surrounding him and the commanders of the chariots."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Jehoram went there with his officers and all his chariots. The Edomites surrounded him and his chariot commanders, but he rose up and broke through by night."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So Jehoram went out with his officers, and all his chariots with him. And he rose by night and attacked the Edomites who had surrounded him and the captains of the chariots."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Jehoram went out with his full army and all his chariots. The Edomites surrounded him and his chariot commanders, but he went out at night and attacked them under cover of darkness."
      }
    },
