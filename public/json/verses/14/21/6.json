    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.21.6",
            "paragraphs": [
              "二一6約蘭不但娶了邪惡君王亞哈的女兒，而且還與她邪惡的行為聯合。參看瑪拉基書二章11節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他与亚哈家一样步以色列诸王的后尘，因为他娶了亚哈的女儿为妻，做耶和华视为恶的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他隨從眾以色列王的道路，好像亞哈家所行的一樣，因為他娶了亞哈的女兒作妻子，行耶和華看為惡的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他行以色列諸王的道，與亞哈家一樣；因他娶了亞哈的女兒為妻，行耶和華眼中看為惡的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He walked in the way of the kings of Israel, just as the house of Ahab did (for Ahab’s daughter was his wife), and he did evil in the sight of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He followed the ways of the kings of Israel, as the house of Ahab had done, for he married a daughter of Ahab. He did evil in the eyes of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jehoram followed the example of the kings of Israel and was as wicked as King Ahab, for he had married one of Ahab’s daughters. So Jehoram did what was evil in the Lord’s sight."
      }
    },
