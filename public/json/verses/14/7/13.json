    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若我使天不下雨，使蝗虫吞吃地上的出产，或叫瘟疫在我民中流行，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我使天閉塞不下雨，或吩咐蝗蟲吞吃這地的土產，或差遣瘟疫到我的子民中間來；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若使天閉塞不下雨，或使蝗蟲吃這地的出產，或使瘟疫流行在我民中，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I shut up the heavens so that there is no rain, or if I command the locust to devour the land, or if I send pestilence among My people,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“When I shut up the heavens so that there is no rain, or command locusts to devour the land or send a plague among my people,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At times I might shut up the heavens so that no rain falls, or command grasshoppers to devour your crops, or send plagues among you."
      }
