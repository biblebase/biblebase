    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "罗波安与祖先同眠后，葬在大卫城。他儿子亚比雅继位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "羅波安和他的列祖同睡，埋葬在大衛城裡。他的兒子亞比雅接續他作王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "羅波安與他列祖同睡，葬在大衛城裏。他兒子亞比雅接續他作王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Rehoboam slept with his fathers and was buried in the city of David; and his son Abijah became king in his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Rehoboam rested with his ancestors and was buried in the City of David. And Abijah his son succeeded him as king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Rehoboam died, he was buried in the City of David. Then his son Abijah became the next king."
      }
    },
