    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.12.2",
            "paragraphs": [
              "一二2\"示撒\"。參看列王紀上十一章40節的腳註。這次攻擊在主前925年進行，加納（Karnak）神廟的墻上也刻載此事。。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "罗波安王执政第五年，埃及王示撒上来攻打耶路撒冷，因为犹大君民对耶和华不忠。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "羅波安王第五年，埃及王示撒上來攻打耶路撒冷，因為他們得罪了耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "羅波安王第五年，埃及王示撒上來攻打耶路撒冷，因為王和民得罪了耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And it came about in King Rehoboam’s fifth year, because they had been unfaithful to the Lord, that Shishak king of Egypt came up against Jerusalem"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because they had been unfaithful to the Lord, Shishak king of Egypt attacked Jerusalem in the fifth year of King Rehoboam."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because they were unfaithful to the Lord, King Shishak of Egypt came up and attacked Jerusalem in the fifth year of King Rehoboam’s reign."
      }
    },
