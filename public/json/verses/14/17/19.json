    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以上都是服侍王的军兵，不包括王派去驻守犹大各坚城的军兵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些人都是服事王的；還有王在猶大全地各設防城裡安置的，沒有計算在內。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這都是伺候王的，還有王在猶大全地堅固城所安置的不在其內。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These are they who served the king, apart from those whom the king put in the fortified cities through all Judah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These were the men who served the king, besides those he stationed in the fortified cities throughout Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These were the troops stationed in Jerusalem to serve the king, besides those Jehoshaphat stationed in the fortified towns throughout Judah."
      }
    },
