        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他专心遵行耶和华的道，铲除犹大境内的丘坛和亚舍拉神像。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他勇敢地遵行耶和華的道路，並且從猶大地中除掉邱壇和亞舍拉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他高興遵行耶和華的道，並且從猶大除掉一切邱壇和木偶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He took great pride in the ways of the Lord and again removed the high places and the Asherim from Judah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His heart was devoted to the ways of the Lord; furthermore, he removed the high places and the Asherah poles from Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He was deeply committed to the ways of the Lord. He removed the pagan shrines and Asherah poles from Judah."
      }
    },
