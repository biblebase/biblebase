    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶何耶大安排众民手持兵器，从殿右到殿左，在祭坛和殿周围护卫王，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又分派眾民，手中各拿兵器，守在祭壇和殿的四周，從殿的右面到殿的左面，環繞著王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又分派眾民手中各拿兵器，在壇和殿那裏，從殿右直到殿左，站在王子的四圍；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He stationed all the people, each man with his weapon in his hand, from the right side of the house to the left side of the house, by the altar and by the house, around the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He stationed all the men, each with his weapon in his hand, around the king—near the altar and the temple, from the south side to the north side of the temple."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He stationed all the people around the king, with their weapons ready. They formed a line from the south side of the Temple around to the north side and all around the altar."
      }
    },
