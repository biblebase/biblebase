    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尽管他还没有把丘坛从以色列除去，但他一生对耶和华忠心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只是邱壇還沒有從以色列中除掉，但亞撒的心一生都完全忠於耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是邱壇還沒有從以色列中廢去，然而亞撒的心一生誠實。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the high places were not removed from Israel; nevertheless Asa’s heart was blameless all his days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Although he did not remove the high places from Israel, Asa’s heart was fully committed to the Lord all his life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Although the pagan shrines were not removed from Israel, Asa’s heart remained completely faithful throughout his life."
      }
    },
