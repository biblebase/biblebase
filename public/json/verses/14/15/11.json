    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当天，他们从掳物中取出七百头牛和七千只羊献给耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那一天，他們從奪回來的戰利品當中，把七百頭牛，七千隻羊，獻給耶和華為祭，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當日他們從所取的擄物中，將牛七百隻、羊七千隻獻給耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They sacrificed to the Lord that day 700 oxen and 7,000 sheep from the spoil they had brought."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At that time they sacrificed to the Lord seven hundred head of cattle and seven thousand sheep and goats from the plunder they had brought back."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On that day they sacrificed to the Lord 700 cattle and 7,000 sheep and goats from the plunder they had taken in the battle."
      }
