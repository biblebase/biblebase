    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "民众把他葬在大卫城的王陵里，因为他在以色列为上帝和上帝的殿做了美善的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他和列王一同埋葬在大衛城裡，因為他在以色列為　神和　神的殿作了美善的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "葬在大衛城列王的墳墓裏；因為他在以色列人中行善，又事奉神，修理神的殿。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they buried him in the city of David among the kings, because he had done good in Israel, and toward God and his house."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They buried him in the city of David among the kings, because he had done well in Israel and to God and His house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He was buried with the kings in the City of David, because of the good he had done in Israel for God and his temple."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they buried him in the City of David among the kings, because he had done good in Israel, both toward God and His house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He was buried among the kings in the City of David, because he had done so much good in Israel for God and his Temple."
      }
