    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶何耶大死后，犹大众首领来朝拜王，王对他们言听计从。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶何耶大死後，猶大的領袖們來朝拜王；王聽從了他們的主意。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶何耶大死後，猶大的眾首領來朝拜王；王就聽從他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But after the death of Jehoiada the officials of Judah came and bowed down to the king, and the king listened to them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After the death of Jehoiada, the officials of Judah came and paid homage to the king, and he listened to them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But after Jehoiada’s death, the leaders of Judah came and bowed before King Joash and persuaded him to listen to their advice."
      }
    },
