    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.25.23",
            "paragraphs": [
              "二五23亞瑪謝在自己的境內被打敗；\"伯示麥\"在耶路撒冷以西十五英裏（24公裏）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列王约阿施在伯·示麦擒获约哈斯的孙子、约阿施的儿子、犹大王亚玛谢，把他带到耶路撒冷，又拆毁从以法莲门到角门约一百八十米长的耶路撒冷城墙，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列王約阿施在伯．示麥捉住了約哈斯的孫子、約阿施的兒子、猶大王亞瑪謝，把他帶到耶路撒冷，又拆毀耶路撒冷的城牆，從以法蓮門直到角門，共兩百公尺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列王約阿施在伯‧示麥擒住約哈斯的孫子、約阿施的兒子、猶大王亞瑪謝，將他帶到耶路撒冷，又拆毀耶路撒冷的城牆，從以法蓮門直到角門，共四百肘；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Joash king of Israel captured Amaziah king of Judah, the son of Joash the son of Jehoahaz, at Beth-shemesh, and brought him to Jerusalem and tore down the wall of Jerusalem from the Gate of Ephraim to the Corner Gate, 400 cubits."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jehoash king of Israel captured Amaziah king of Judah, the son of Joash, the son of Ahaziah, at Beth Shemesh. Then Jehoash brought him to Jerusalem and broke down the wall of Jerusalem from the Ephraim Gate to the Corner Gate—a section about four hundred cubits long."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King Jehoash of Israel captured Judah’s king, Amaziah son of Joash and grandson of Ahaziah, at Beth-shemesh. Then he brought him to Jerusalem, where he demolished 600 feet of Jerusalem’s wall, from the Ephraim Gate to the Corner Gate."
      }
    },
