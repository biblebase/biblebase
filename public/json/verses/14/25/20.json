    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但亚玛谢不理会他的劝告。原来这事出于上帝，要把他们交在约阿施手中，因为他们求告以东的神明。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞瑪謝卻不聽從；這原是出於　神，好把他們交在約阿施手裡，因為他們尋求以東的神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞瑪謝卻不肯聽從。這是出乎神，好將他們交在敵人手裏，因為他們尋求以東的神。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But Amaziah would not listen, for it was of God, in order that he might give them into the hand of their enemies, because they had sought the gods of Edom."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Amaziah would not listen, for it was from God, that He might deliver them into the hand of Joash because they had sought the gods of Edom."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Amaziah, however, would not listen, for God so worked that he might deliver them into the hands of Jehoash, because they sought the gods of Edom."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But Amaziah would not heed, for it came from God, that He might give them into the hand of  their  enemies, because they sought the gods of Edom."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Amaziah refused to listen, for God was determined to destroy him for turning to the gods of Edom."
      }
    },
