    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你打败了以东人就趾高气扬。你还是待在家里吧，何必惹祸上身，使你和犹大一同灭亡呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你心裡說你擊敗了以東人，你就心裡驕傲，以這事誇口。現在你留在家裡吧，為甚麼要惹禍，使猶大和你一同覆亡呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你說：『看哪，我打敗了以東人』，你就心高氣傲，以致矜誇。你在家裏安居就罷了，為何要惹禍使自己和猶大國一同敗亡呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You said, ‘Behold, you have defeated Edom.’ And your heart has become proud in boasting. Now stay at home; for why should you provoke trouble so that you, even you, would fall and Judah with you?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You say to yourself that you have defeated Edom, and now you are arrogant and proud. But stay at home! Why ask for trouble and cause your own downfall and that of Judah also?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You are saying, ‘I have defeated Edom,’ and you are very proud of it. But my advice is to stay at home. Why stir up trouble that will only bring disaster on you and the people of Judah?”"
      }
    },
