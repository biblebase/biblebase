    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2ch.25.24",
            "paragraphs": [
              "二五24\"俄別以東\"的家族是負責看守殿門的（代上二六4～8）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "抢走俄别·以东在上帝殿中看守的所有金银和器皿以及王宫里的财宝，并带着人质返回撒玛利亚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又把俄別．以東負責看守神殿裡的一切金銀和一切器皿，以及王宮裡的寶物都掠去，又擄走人質，然後回撒瑪利亞去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又將俄別‧以東所看守神殿裏的一切金銀和器皿，與王宮裏的財寶都拿了去，並帶人去為質，就回撒馬利亞去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He took all the gold and silver and all the utensils which were found in the house of God with Obed-edom, and the treasures of the king’s house, the hostages also, and returned to Samaria."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He took all the gold and silver and all the articles found in the temple of God that had been in the care of Obed-Edom, together with the palace treasures and the hostages, and returned to Samaria."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He carried off all the gold and silver and all the articles from the Temple of God that had been in the care of Obed-edom. He also seized the treasures of the royal palace, along with hostages, and then returned to Samaria."
      }
    },
