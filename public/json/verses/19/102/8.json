    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的仇敌终日辱骂我， 嘲笑我的用我的名字咒诅人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的仇敵終日辱罵我； 嘲弄我的指著我賭咒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的仇敵終日辱罵我； 向我猖狂的人指着我賭咒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My enemies have reproached me all day long; Those who deride me have used my name as a curse."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All day long my enemies taunt me; those who rail against me use my name as a curse."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My enemies taunt me day after day. They mock and curse me."
      }
