    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.102.2",
            "paragraphs": [
              "102:2",
              "我在急難的日子：在人生急難的日子中，凡事似乎都與我們的意願背道而馳，而我們全然無助，無法挽救事態；這時我們惟一的盼望就是呼求神，將我們自己的生命和環境全然交托在他手中(比較39:12；54:2；61:1；64:1)。詩人正是存著這樣的指望，呼求神的憐憫，求神扭轉他的艱難處境，他也深信神必應允他的禱告，不丟棄他。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我在危难的时候， 求你不要掩面不理我。 求你垂听我的呼求， 赶快应允我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我在患難的日子， 求你不要向我掩面； 我呼求的時候， 求你留心聽我，並且迅速應允我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我在急難的日子，求你向我側耳； 不要向我掩面！ 我呼求的日子，求你快快應允我！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not hide Your face from me in the day of my distress; Incline Your ear to me; In the day when I call answer me quickly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not hide your face from me when I am in distress. Turn your ear to me; when I call, answer me quickly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t turn away from me in my time of distress. Bend down to listen, and answer me quickly when I call to you."
      }
    },
