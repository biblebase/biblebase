    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我贫穷困苦， 上帝啊，求你快来救我。 你是我的帮助，是我的拯救。 耶和华啊，求你不要迟延。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於我，我是困苦貧窮的； 　神啊！求你快快到我這裡來； 你是我的幫助，我的拯救； 耶和華啊！求你不要耽延。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "至于我，我困苦贫穷； 神哪，求你快到我这里来！ 你是我的帮助、我的救主， 耶和华啊，求你不要迟延！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我是困苦窮乏的； 神啊，求你速速到我這裏來！ 你是幫助我的，搭救我的。 耶和華啊，求你不要躭延！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But I am poor and needy; hasten to me, O God! You are my help and my deliverer; O Lord, do not delay!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But I am afflicted and needy; Hasten to me, O God! You are my help and my deliverer; O Lord, do not delay."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But as for me, I am poor and needy; come quickly to me, O God. You are my help and my deliverer; Lord, do not delay."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But I am poor and needy; Make haste to me, O God! You are my help and my deliverer; O Lord, do not delay."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But as for me, I am poor and needy; please hurry to my aid, O God. You are my helper and my savior; O Lord, do not delay."
      }
    },
