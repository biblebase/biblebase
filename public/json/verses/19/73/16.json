    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.73.15-17",
            "paragraphs": [
              "七三15～17若不考慮神的觀點和祂最後的審判--保證惡人必被毀滅（18～20,27節）--人就會被第3至14節所描述的情況所誤導。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
