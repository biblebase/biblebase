    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.73.27-28",
            "paragraphs": [
              "七三27,28\"離棄你行邪淫的\"。即在靈裏對神不忠的。亞薩正確地下結論說，惡人必然衰亡，而縱然面對逆境，亞薩仍在神裏找到避難所。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对于我，到上帝面前是何等美好。 我以主耶和华为我的避难所， 并宣扬祂的一切作为。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對我來說，親近　神是美好的， 我以主耶和華為我的避難所； 我要述說你的一切作為。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "至于我，在神的近旁对我是美好的； 我以主耶和华为我的避难所， 为要讲述他一切的作为。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我親近神是與我有益； 我以主耶和華為我的避難所， 好叫我述說你一切的作為。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But for me it is good to be near God; I have made the Lord God my refuge, that I may tell of all your works."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, the nearness of God is my good; I have made the Lord God my refuge, That I may tell of all Your works."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But as for me, it is good to be near God. I have made the Sovereign Lord my refuge; I will tell of all your deeds."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But it  is good for me to draw near to God; I have put my trust in the Lord God, That I may declare all Your works."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But as for me, how good it is to be near God! I have made the Sovereign Lord my shelter, and I will tell everyone about the wonderful things you do."
      }
    },
