    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.73.27-28",
            "paragraphs": [
              "七三27,28\"離棄你行邪淫的\"。即在靈裏對神不忠的。亞薩正確地下結論說，惡人必然衰亡，而縱然面對逆境，亞薩仍在神裏找到避難所。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些远离你的人必灭亡， 你必灭绝不忠于你的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！遠離你的，必定滅亡； 凡是對你不貞的，你都要滅絕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "遠離你的，必要死亡； 凡離棄你行邪淫的，你都滅絕了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For, behold, those who are far from You will perish; You have destroyed all those who are unfaithful to You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who are far from you will perish; you destroy all who are unfaithful to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who desert him will perish, for you destroy those who abandon you."
      }
    },
