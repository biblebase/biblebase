    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.73.2-3",
            "paragraphs": [
              "七三2,3\"我的腳幾乎失閃\"。由於嫉妒\"惡人……享平安\"，他對耶和華的信心幾乎失去。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我看见狂傲的恶人亨通就心怀不平。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我看見惡人興隆， 我就嫉妒狂傲的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我見惡人和狂傲人享平安就心懷不平。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I was envious of the arrogant As I saw the prosperity of the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I envied the arrogant when I saw the prosperity of the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I envied the proud when I saw them prosper despite their wickedness."
      }
    },
