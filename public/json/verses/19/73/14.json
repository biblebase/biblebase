    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.73.13-14",
            "paragraphs": [
              "七三13,14行公義的人得不到可見的賞賜，這使惡人終身的興旺令人更感困惑。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我天天遭灾，日日受苦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為我終日受傷害， 每天早晨受懲罰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，我終日遭災難； 每早晨受懲治。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I have been stricken all day long And chastened every morning."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All day long I have been afflicted, and every morning brings new punishments."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I get nothing but trouble all day long; every morning brings me pain."
      }
    },
