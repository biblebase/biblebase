    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.73.15-17",
            "paragraphs": [
              "七三15～17若不考慮神的觀點和祂最後的審判--保證惡人必被毀滅（18～20,27節）--人就會被第3至14節所描述的情況所誤導。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要是我这样说， 我就是背叛了你的子民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我心裡說：“我要說這樣的話”， 我就是對你這一代的眾兒女不忠了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若說，我要這樣講， 這就是以奸詐待你的眾子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I had said, “I will speak thus,” Behold, I would have betrayed the generation of Your children."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If I had spoken out like that, I would have betrayed your children."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If I had really spoken this way to others, I would have been a traitor to your people."
      }
