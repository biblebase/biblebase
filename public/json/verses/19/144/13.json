    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.144.12-15",
            "paragraphs": [
              "一四四12～15這幅和平與富饒的圖畫描寫茁壯的兒子、可靠的女兒、滿盈的谷倉、健壯的牲畜和神的同在。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿我们的仓库堆满各样的谷物， 我们的羊在田野繁衍众多， 成千上万。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願我們的倉庫滿溢， 各種糧食不缺； 願我們牧場上的羊群，孳生千萬。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們的倉盈滿，能出各樣的糧食； 我們的羊在田間孳生千萬。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let our garners be full, furnishing every kind of produce, And our flocks bring forth thousands and ten thousands in our fields;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Our barns will be filled with every kind of provision. Our sheep will increase by thousands, by tens of thousands in our fields;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May our barns be filled with crops of every kind. May the flocks in our fields multiply by the thousands, even tens of thousands,"
      }
    },
