    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 人算什么，你竟顾念他？ 世人算什么，你竟眷顾他？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！人算甚麼，你竟關懷他， 世人算甚麼，你竟眷念他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，人算甚麼，你竟認識他！ 世人算甚麼，你竟顧念他！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, what is man, that You take knowledge of him? Or the son of man, that You think of him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, what are human beings that you care for them, mere mortals that you think of them?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, what are human beings that you should notice them, mere mortals that you should think about them?"
      }
