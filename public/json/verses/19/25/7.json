        "content": [
          {
            "verse": "psa.25.6-7",
            "paragraphs": [
              "二五6,7要\"記念\"你長久存在的憐憫與慈愛，但\"不要記念\"我幼年的、很久以前的罪。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你饶恕我年轻时的罪恶和过犯， 以你的恩惠和慈爱待我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你不要記念我幼年的罪惡和我的過犯； 耶和華啊！求你因你的恩惠， 按著你的慈愛記念我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你不要記念我幼年的罪愆和我的過犯； 耶和華啊，求你因你的恩惠，按你的慈愛記念我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not remember the sins of my youth or my transgressions; According to Your lovingkindness remember me, For Your goodness’ sake, O Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not remember the sins of my youth and my rebellious ways; according to your love remember me, for you, Lord, are good."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not remember the rebellious sins of my youth. Remember me in the light of your unfailing love, for you are merciful, O Lord."
      }
    },
