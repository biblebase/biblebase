    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的上帝啊，我信靠你， 求你不要叫我蒙羞， 不要让我的仇敌胜过我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的　神啊！我倚靠你，求你不要使我羞愧， 也不要使我的仇敵勝過我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的神啊，我素來倚靠你； 求你不要叫我羞愧， 不要叫我的仇敵向我誇勝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O my God, in You I trust, Do not let me be ashamed; Do not let my enemies exult over me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I trust in you; do not let me be put to shame, nor let my enemies triumph over me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I trust in you, my God! Do not let me be disgraced, or let my enemies rejoice in my defeat."
      }
