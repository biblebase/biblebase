    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.37.12-26",
            "paragraphs": [
              "隱藏的幫助（三十七12～26）",
              "到目前為止，戰場乃在信徒心中，他們因惡人的明目張膽而激忿不平。現在詩人從外面觀察這兩種人，比較他們的道路與結局。直到本詩的末尾，幾乎每一節都講到義人或惡人，或類似用語。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人淡泊一生强过恶人财富如山。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "一個義人擁有的雖少， 勝過許多惡人的財富。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "一個義人所有的雖少， 強過許多惡人的富餘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Better is the little of the righteous Than the abundance of many wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Better the little that the righteous have than the wealth of many wicked;"
      },
      "nlt": {
