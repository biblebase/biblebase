    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.37.27-40",
            "paragraphs": [
              "前瞻（三十七27～40）",
              "雖然在本詩中，這不算是新觀點，但在本段內它卻特別突顯，首先是永遠的重復出現，在第27、28（強調語）、29節（同義字）；然後是35、36節的小比方；最後則是藉37、38節所強調的\\cs9後裔表明。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华拯救义人， 患难时作他们的庇护所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的拯救是由耶和華而來； 在患難的時候，他作他們的避難所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但義人得救是由於耶和華； 他在患難時作他們的營寨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the salvation of the righteous is from the Lord; He is their strength in time of trouble."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The salvation of the righteous comes from the Lord; he is their stronghold in time of trouble."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord rescues the godly; he is their fortress in times of trouble."
      }
    },
