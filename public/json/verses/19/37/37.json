    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你看那纯全正直、爱好和平的人， 他们前途美好。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
