      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Mark the blameless man, and behold the upright; For the man of peace will have a posterity."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Consider the blameless, observe the upright; a future awaits those who seek peace."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Look at those who are honest and good, for a wonderful future awaits those who love peace."
      }
    },
