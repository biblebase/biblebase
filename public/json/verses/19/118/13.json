    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.118.13",
            "paragraphs": [
              "一一八13\"你推我\"。更可作：你大力推我。這是指以色列的仇敵，這裏看為一個個體。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我受到猛烈的攻击， 几乎要倒下了， 但耶和华帮助了我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們極力推我，要我跌倒， 耶和華卻幫助了我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你推我，要叫我跌倒， 但耶和華幫助了我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You pushed me violently so that I was falling, But the Lord helped me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I was pushed back and about to fall, but the Lord helped me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My enemies did their best to kill me, but the Lord rescued me."
      }
