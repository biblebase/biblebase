      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华与我同在，帮助我， 我必看见那些恨我的人一败涂地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華是我的幫助， 我必看見恨我的人遭報。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在那幫助我的人中，有耶和華幫助我， 所以我要看見那恨我的人遭報。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord is for me among those who help me; Therefore I will look with satisfaction on those who hate me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord is with me; he is my helper. I look in triumph on my enemies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, the Lord is for me; he will help me. I will look in triumph at those who hate me."
      }
