    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.55.22",
            "paragraphs": [
              "55:22",
              "你要把你的重擔卸給耶和華：神願我們在經歷艱難試驗、重擔難當時，將自己的擔子和憂慮卸給他，他就會替我們分擔，隨時扶持我們。在漫長的救贖歷程中，聖靈已反復多次向我們發出了這呼喚；耶穌在馬太福音11:28~30中向我們伸出了邀請的雙手；使徒彼得告訴信徒要在神面前謙卑，並且“要將一切的憂慮卸給神，因為他顧念你們”(彼前5:7)；使徒保羅也勸勉我們要借著禱告，將我們的憂慮交托給神，並應許說神必賜出人意外的平安，保守我們的心懷意念(參腓4:7註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把你的重担卸给耶和华， 祂必扶持你。 祂必不让义人跌倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要把你的重擔卸給耶和華， 他必扶持你； 他永遠不會讓義人動搖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要把你的重擔卸給耶和華， 他必撫養你； 他永不叫義人動搖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Cast your burden upon the Lord and He will sustain you; He will never allow the righteous to be shaken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Cast your cares on the Lord and he will sustain you; he will never let the righteous be shaken."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give your burdens to the Lord, and he will take care of you. He will not permit the godly to slip and fall."
      }
    },
