    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.55.14",
            "paragraphs": [
              "五五14大衛與他的朋友曾有親密的相交，並且一起敬拜神。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从前我们情谊深厚， 与众人同去上帝的殿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們常在一起密談， 我們在　神的殿中與群眾同行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們素常彼此談論，以為甘甜； 我們與羣眾在神的殿中同行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We who had sweet fellowship together Walked in the house of God in the throng."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "with whom I once enjoyed sweet fellowship at the house of God, as we walked about among the worshipers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "What good fellowship we once enjoyed as we walked together to the house of God."
      }
    },
