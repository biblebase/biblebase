    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 你必把恶人送进灭亡的坑里。 嗜血成性的骗子必早早夭亡。 但我要信靠你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！你必使惡人墮入滅亡的深坑裡； 流人血和行詭詐的人必活不到半世； 至於我，我必倚靠你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你必使惡人下入滅亡的坑； 流人血、行詭詐的人必活不到半世， 但我要倚靠你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But You, O God, will bring them down to the pit of destruction; Men of bloodshed and deceit will not live out half their days. But I will trust in You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you, God, will bring down the wicked into the pit of decay; the bloodthirsty and deceitful will not live out half their days. But as for me, I trust in you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you, O God, will send the wicked down to the pit of destruction. Murderers and liars will die young, but I am trusting you to save me."
      }
    },
