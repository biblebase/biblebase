    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.55.2",
            "paragraphs": [
              "五五2\"我哀嘆不安，發聲唉哼\"。更可作：我哀嘆不安，而且心煩意亂。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你垂听、应允我的呼求。 我思绪烦乱，坐立不安。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你留心聽我，應允我； 我在苦惱中必不安寧，唉哼難過；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你側耳聽我，應允我。 我哀歎不安，發聲唉哼，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Give heed to me and answer me; I am restless in my complaint and am surely distracted,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "hear me and answer me. My thoughts trouble me and I am distraught"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Please listen and answer me, for I am overwhelmed by my troubles."
      }
    },
