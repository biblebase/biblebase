    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.55.15",
            "paragraphs": [
              "五五15\"願他們活活的下入陰間\"。即願神的審判是即時的和令人吃驚的（正如可拉的審判；民一六30～32）；不是為了個人的恩怨，而是因為那人叛逆神所揀選的領袖。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿死亡突然抓住我的仇敌， 愿他们活活地下阴间， 因为他们的内心和家中罪恶充斥。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願死亡忽然臨到他們身上， 願他們活活下到陰間去， 因為在他們中間，就是在他們的住所裡，盡是邪惡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願死亡忽然臨到他們！ 願他們活活地下入陰間！ 因為他們的住處，他們的心中，都是邪惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let death come deceitfully upon them; Let them go down alive to Sheol, For evil is in their dwelling, in their midst."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let death take my enemies by surprise; let them go down alive to the realm of the dead, for evil finds lodging among them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let death stalk my enemies; let the grave swallow them alive, for evil makes its home within them."
      }
    },
