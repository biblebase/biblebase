    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若是仇敌辱骂我， 我还能忍受； 倘若是恨我的人欺凌我， 我还可以躲开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "原來不是仇敵辱罵我， 如果是仇敵，我還可以忍受； 也不是恨我的人向我狂妄自大， 如果是恨我的人，我還可以躲避他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "原來不是仇敵辱罵我， 若是仇敵，還可忍耐； 也不是恨我的人向我狂大， 若是恨我的人就必躲避他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For it is not an enemy who reproaches me, Then I could bear it; Nor is it one who hates me who has exalted himself against me, Then I could hide myself from him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If an enemy were insulting me, I could endure it; if a foe were rising against me, I could hide."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is not an enemy who taunts me— I could bear that. It is not my foes who so arrogantly insult me— I could have hidden from them."
      }
