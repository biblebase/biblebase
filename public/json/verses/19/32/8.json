    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.32.8",
            "paragraphs": [
              "32:8",
              "我要教導你：蒙赦免的信徒，若有一顆受教的心，珍視神的同在和訓誨(比較7節)，又依靠他(10節)，以他為樂(11節)，保守內心的正直(11節)，主就必照自己的應許教導並指引他。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.32.8-10",
            "paragraphs": [
              "三二8～10說話的人是大衛，他根據自己的經驗來教導別人。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“我要教导你， 引领你走正路； 我要劝导你，看顾你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要教導你，指示你應走的路； 我要勸戒你，我的眼睛看顧你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要教導你，指示你當行的路； 我要定睛在你身上勸戒你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will instruct you and teach you in the way which you should go; I will counsel you with My eye upon you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will instruct you and teach you in the way you should go; I will counsel you with my loving eye on you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord says, “I will guide you along the best pathway for your life. I will advise you and watch over you."
      }
    },
