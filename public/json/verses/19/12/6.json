    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的应许纯全， 就像在炉中炼过七次的银子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的言語是純淨的言語， 好像銀子在泥爐中煉過，精煉過七次一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的言語是純淨的言語， 如同銀子在泥爐中煉過七次。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The words of the Lord are pure words; As silver tried in a furnace on the earth, refined seven times."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the words of the Lord are flawless, like silver purified in a crucible, like gold refined seven times."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord’s promises are pure, like silver refined in a furnace, purified seven times over."
      }
    },
