    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你知道我的哀伤， 你把我的眼泪收在袋中。 我的遭遇都记录在你的册子上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我多次流離，你都數算； 你把我的眼淚裝在你的皮袋裡。 這不都記在你的冊子上嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我幾次流離，你都記數； 求你把我眼淚裝在你的皮袋裏。 這不都記在你冊子上嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have taken account of my wanderings; Put my tears in Your bottle. Are they not in Your book?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Record my misery; list my tears on your scroll— are they not in your record?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You keep track of all my sorrows. You have collected all my tears in your bottle. You have recorded each one in your book."
      }
    },
