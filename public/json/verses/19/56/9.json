    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我向你求救的时候， 敌人都落荒而逃。 我知道上帝是我的帮助。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我呼求你的時候， 我的仇敵就都轉身退後； 因此我知道　神是幫助我的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "于是在我呼求的日子里， 我的仇敌必转身退后； 我明白这一点：神在我这边！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我呼求的日子，我的仇敵都要轉身退後。 神幫助我，這是我所知道的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then my enemies will turn back in the day when I call. This I know, that  God is for me."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then my enemies will turn back in the day when I call; This I know, that God is for me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then my enemies will turn back when I call for help. By this I will know that God is for me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "When I cry out to  You, Then my enemies will turn back; This I know, because God is for me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My enemies will retreat when I call to you for help. This I know: God is on my side!"
      }
