      "leili": {
        "content": [
          {
            "verse": "psa.131.1",
            "paragraphs": [
              "一三一1大衛宣示自己心不狂妄，也沒有過分的野心。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 我的心不狂傲， 我的眼目也不高傲； 我不敢涉猎太伟大、太奇妙的事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！我的心不驕傲， 我的眼不自高； 重大和超過我能力的事，我都不敢作。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我的心不狂傲， 我的眼不高大； 重大和測不透的事， 我也不敢行。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, my heart is not proud, nor my eyes haughty; Nor do I involve myself in great matters, Or in things too difficult for me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My heart is not proud, Lord, my eyes are not haughty; I do not concern myself with great matters or things too wonderful for me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, my heart is not proud; my eyes are not haughty. I don’t concern myself with matters too great or too awesome for me to grasp."
      }
    },
