    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.115.1",
            "paragraphs": [
              "115:1",
              "榮耀不要歸與我們……要……歸在你的名下：我們絕不可求自己的榮耀，而當渴望在世人面前彰顯神的榮耀並尊崇他的名。我們應當心意堅定，決不因自己的行事為人使基督和教會蒙羞受辱(比較太5:14~16；林前6:20)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 不要将荣耀归于我们， 不要归于我们。 愿荣耀归于你的名， 因为你慈爱、信实！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！不要把榮耀歸給我們，不要歸給我們； 要因你的慈愛和信實把榮耀歸在你的名下。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，榮耀不要歸與我們， 不要歸與我們； 要因你的慈愛和誠實歸在你的名下！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Not to us, O Lord, not to us, But to Your name give glory Because of Your lovingkindness, because of Your truth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Not to us, Lord, not to us but to your name be the glory, because of your love and faithfulness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Not to us, O Lord, not to us, but to your name goes all the glory for your unfailing love and faithfulness."
      }
    },
