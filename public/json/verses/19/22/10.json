      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我自出母胎就被交在你手裏； 從我母親生我，你就是我的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Upon You I was cast from birth; You have been my God from my mother’s womb."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From birth I was cast on you; from my mother’s womb you have been my God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I was thrust into your arms at my birth. You have been my God from the moment I was born."
      }
    },
