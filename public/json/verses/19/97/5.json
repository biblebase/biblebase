    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.97.2-6",
            "paragraphs": [
              "九七2～6千禧年國度的特色是道德和普及的。耶和華的顯現常伴有不尋常的大自然現象（出一九16；彌一4），主再次降臨的時候也這樣（徒二19,20）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在天下之主耶和华面前， 群山如蜡熔化。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在耶和華面前，就是在全地的主面前， 群山都像蠟一般融化。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "諸山見耶和華的面， 就是全地之主的面，便消化如蠟。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The mountains melted like wax at the presence of the Lord, At the presence of the Lord of the whole earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The mountains melt like wax before the Lord, before the Lord of all the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The mountains melt like wax before the Lord, before the Lord of all the earth."
      }
    },
