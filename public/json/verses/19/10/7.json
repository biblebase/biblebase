      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他滿口是咒罵、詭詐、欺壓， 舌底是毒害、奸惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His mouth is full of curses and deceit and oppression; Under his tongue is mischief and wickedness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His mouth is full of lies and threats; trouble and evil are under his tongue."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their mouths are full of cursing, lies, and threats. Trouble and evil are on the tips of their tongues."
      }
