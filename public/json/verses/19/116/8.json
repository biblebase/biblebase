    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂救我的性命脱离死亡， 止住我眼中的泪水， 使我双脚免于滑倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主啊！你救了我的性命脫離死亡， 你使我的眼睛不致流淚， 使我的雙腳不致跌倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主啊，你救我的命免了死亡， 救我的眼免了流淚， 救我的腳免了跌倒。"
      },
      "nasb": {
        "version_id": "NASB",
