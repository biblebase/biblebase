    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "死亡的绳索缠绕我， 阴间的恐怖笼罩我， 我被困苦和忧愁淹没。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "死亡的繩索纏繞著我， 陰間的痛苦抓住我； 我遭遇患難和愁苦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "死亡的繩索纏繞我； 陰間的痛苦抓住我； 我遭遇患難愁苦。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The cords of death encompassed me And the terrors of Sheol came upon me; I found distress and sorrow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The cords of death entangled me, the anguish of the grave came over me; I was overcome by distress and sorrow."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Death wrapped its ropes around me; the terrors of the grave overtook me. I saw only trouble and sorrow."
      }
    },
