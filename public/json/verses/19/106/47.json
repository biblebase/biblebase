    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的上帝耶和华啊， 求你拯救我们， 从各国招聚我们， 使我们可以称谢你的圣名， 欢然赞美你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華我們的　神啊！求你拯救我們， 把我們從萬國中招聚回來， 我們好稱謝你的聖名， 以讚美你為誇耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華－我們的神啊，求你拯救我們， 從外邦中招聚我們， 我們好稱讚你的聖名， 以讚美你為誇勝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Save us, O Lord our God, And gather us from among the nations, To give thanks to Your holy name And glory in Your praise."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Save us, Lord our God, and gather us from the nations, that we may give thanks to your holy name and glory in your praise."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Save us, O Lord our God! Gather us back from among the nations, so we can thank your holy name and rejoice and praise you."
      }
