    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.34.1-10",
            "paragraphs": [
              "與我一同喜樂！（三十四1～10）",
              "本詩的前一半，個人的見證（1～2a、4、6節）和重復呼召人同來贊美、更新信心，兩者交替出現。如此分享祝福的方式，請參二十二22以下的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要夸耀祂的作为， 困苦人听见必欢欣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的心要因耶和華誇耀， 困苦的人聽見了就喜樂。"
      },
      "cunp": {
