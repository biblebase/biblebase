    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.34.1-10",
            "paragraphs": [
              "與我一同喜樂！（三十四1～10）",
              "本詩的前一半，個人的見證（1～2a、4、6節）和重復呼召人同來贊美、更新信心，兩者交替出現。如此分享祝福的方式，請參二十二22以下的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "让我们一同颂赞祂的伟大， 尊崇祂的名！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要跟我一起尊耶和華為大， 我們來一同高舉他的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們和我當稱耶和華為大， 一同高舉他的名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O magnify the Lord with me, And let us exalt His name together."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Glorify the Lord with me; let us exalt his name together."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Come, let us tell of the Lord’s greatness; let us exalt his name together."
      }
