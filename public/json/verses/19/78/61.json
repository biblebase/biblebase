    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.61",
            "paragraphs": [
              "七八61\"將他的約櫃交與人擄去\"。指非利士人把約櫃擄去的事蹟（撒上四1～七1）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂任凭自己的约柜被人掳去， 让自己的荣耀落在敌人手中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他又把象徵他權能的約櫃交給人擄去， 把他的榮美交在敵人的手裡，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又將他的約櫃交與人擄去， 將他的榮耀交在敵人手中；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And gave up His strength to captivity And His glory into the hand of the adversary."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He sent the ark of his might into captivity, his splendor into the hands of the enemy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He allowed the Ark of his might to be captured; he surrendered his glory into enemy hands."
      }
    },
