    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.68-72",
            "paragraphs": [
              "七八68～72以色列民的盼望就在於神的新選擇：\"猶大\"（代替以法蓮），\"錫安\"（當時在敵人手中）和\"大衛\"（一個牧羊童）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂为自己建造高耸的圣所， 使它像大地一样长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他建造了自己的聖所好像在高天之上， 又像他所建立永存的大地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "蓋造他的聖所，好像高峯， 又像他建立永存之地；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He built His sanctuary like the heights, Like the earth which He has founded forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He built his sanctuary like the heights, like the earth that he established forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There he built his sanctuary as high as the heavens, as solid and enduring as the earth."
      }
