      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂使自己的子民被刀剑杀戮， 向自己的产业大发怒气。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "並且把自己的子民交給刀劍， 向自己的產業大發烈怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並將他的百姓交與刀劍， 向他的產業發怒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also delivered His people to the sword, And was filled with wrath at His inheritance."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He gave his people over to the sword; he was furious with his inheritance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He gave his people over to be butchered by the sword, because he was so angry with his own people—his special possession."
      }
