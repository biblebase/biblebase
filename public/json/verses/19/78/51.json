    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂击杀了埃及人所有的长子， 就是含帐篷中头生的儿子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他在埃及擊殺了所有的長子， 在含的帳棚中擊殺了他們強壯時生的頭胎子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在埃及擊殺一切長子， 在含的帳棚中擊殺他們強壯時頭生的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And smote all the firstborn in Egypt, The first issue of their virility in the tents of Ham."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He struck down all the firstborn of Egypt, the firstfruits of manhood in the tents of Ham."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He killed the oldest son in each Egyptian family, the flower of youth throughout the land of Egypt."
      }
