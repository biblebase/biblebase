    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.12-16",
            "paragraphs": [
              "七八12～16\"瑣安\"。埃及東北面之歌珊的省會。簡述神在拯救以色列人時，於埃及施行的、與水有關的災難，以及與水有關的神跡。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂曾在埃及，在琐安当着他们祖先的面行神迹。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他在埃及地，在瑣安的田野， 在他們列祖的眼前，行了奇事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他在埃及地，在瑣安田， 在他們祖宗的眼前施行奇事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He wrought wonders before their fathers In the land of Egypt, in the field of Zoan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He did miracles in the sight of their ancestors in the land of Egypt, in the region of Zoan."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "the miracles he did for their ancestors on the plain of Zoan in the land of Egypt."
      }
