    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.78.11",
            "paragraphs": [
              "78:11",
              "又忘記他所行的：以色列人在靈性上至少有些方面是失敗的，因為他們忘記了神為他們列祖所行的神跡和大能的作為。同樣，我們也不可忘記神在新約教會的忠心信徒中間所行的大能奇事。現在，聖靈仍願像當年那樣廣行神跡、奇事和異能，好叫救贖的信息像當年那樣大有能力、滿有功效地借著我們和教會傳揚出來(參徒1:8註；參“聖靈浸”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们忘记了祂的作为， 忘记了祂为他们所行的奇事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們忘記了　神的作為， 和他顯給他們看的奇事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又忘記他所行的 和他顯給他們奇妙的作為。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They forgot His deeds And His miracles that He had shown them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They forgot what he had done, the wonders he had shown them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They forgot what he had done— the great wonders he had shown them,"
      }
    },
