    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.17-39",
            "paragraphs": [
              "七八17～39概述以色列人在曠野飄流期間的反叛，以及神向他們所發的忿怒。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "直到上帝击杀他们的时候， 他们才回转，诚心寻求上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神擊殺他們的時候，他們就尋求他； 他們回轉過來，切切求問　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他殺他們的時候，他們才求問他， 回心轉意，切切地尋求神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He killed them, then they sought Him, And returned and searched diligently for God;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whenever God slew them, they would seek him; they eagerly turned to him again."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When God began killing them, they finally sought him. They repented and took God seriously."
      }
