    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.17-39",
            "paragraphs": [
              "七八17～39概述以色列人在曠野飄流期間的反叛，以及神向他們所發的忿怒。"
            ]
          },
          {
            "verse": "psa.78.33",
            "paragraphs": [
              "七八33\"虛空\"。指他們在曠野飄流的四十年。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "所以上帝使他们虚度一生， 让他们的岁月充满恐惧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以　神使他們的日子在虛空中消逝， 使他們的歲月在驚恐中完結。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，他叫他們的日子全歸虛空， 叫他們的年歲盡屬驚恐。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So He brought their days to an end in futility And their years in sudden terror."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So he ended their days in futility and their years in terror."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So he ended their lives in failure, their years in terror."
      }
    },
