    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.68-72",
            "paragraphs": [
              "七八68～72以色列民的盼望就在於神的新選擇：\"猶大\"（代替以法蓮），\"錫安\"（當時在敵人手中）和\"大衛\"（一個牧羊童）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，大卫以正直的心牧养他们， 用灵巧的手带领他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是大衛以正直的心牧養他們， 靈巧地引導他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，他按心中的純正牧養他們， 用手中的巧妙引導他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he shepherded them according to the integrity of his heart, And guided them with his skillful hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And David shepherded them with integrity of heart; with skillful hands he led them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He cared for them with a true heart and led them with skillful hands."
      }
    },
