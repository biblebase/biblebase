      "leili": {
        "content": [
          {
            "verse": "psa.78.17-39",
            "paragraphs": [
              "七八17～39概述以色列人在曠野飄流期間的反叛，以及神向他們所發的忿怒。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "使飞鸟像雨点一般降在他们当中，多如尘埃， 使他们有多如海沙的肉吃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他降肉食給他們，多如塵土； 又降飛鳥給他們，多如海沙。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他降肉，像雨在他們當中，多如塵土， 又降飛鳥，多如海沙，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He rained meat upon them like the dust, Even winged fowl like the sand of the seas,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He rained meat down on them like dust, birds like sand on the seashore."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He rained down meat as thick as dust— birds as plentiful as the sand on the seashore!"
      }
