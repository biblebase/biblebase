    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.17-39",
            "paragraphs": [
              "七八17～39概述以色列人在曠野飄流期間的反叛，以及神向他們所發的忿怒。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝就向他们发怒， 杀掉了他们当中最强壮的， 消灭了以色列的青年。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神的怒氣就向他們發作， 殺了他們中間最肥壯的人， 擊倒了以色列的年輕人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神的怒氣就向他們上騰， 殺了他們內中的肥壯人， 打倒以色列的少年人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The anger of God rose against them And killed some of their stoutest ones, And subdued the choice men of Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God’s anger rose against them; he put to death the sturdiest among them, cutting down the young men of Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "the anger of God rose against them, and he killed their strongest men. He struck down the finest of Israel’s young men."
      }
    },
