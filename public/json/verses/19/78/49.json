    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂把怒火、烈怒、愤恨和祸患倾倒在他们身上， 遣下一群降灾的天使。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他使猛烈的怒氣、忿怒、憤恨、患難， 好像一群降災的使者，臨到他們中間。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他向他们发出猛烈的怒气—— 盛怒、愤怒和患难， 像一队降祸的使者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他使猛烈的怒氣和忿怒、惱恨、苦難 成了一羣降災的使者，臨到他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He let loose on them his burning anger, wrath, indignation, and distress, a company of destroying angels."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He sent upon them His burning anger, Fury and indignation and trouble, A band of destroying angels."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He unleashed against them his hot anger, his wrath, indignation and hostility— a band of destroying angels."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He cast on them the fierceness of His anger, Wrath, indignation, and trouble, By sending angels of destruction among  them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He loosed on them his fierce anger— all his fury, rage, and hostility. He dispatched against them a band of destroying angels."
      }
    },
