    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.78.37",
            "paragraphs": [
              "78:37",
              "心向他不正，在他的約上不忠心：以色列人沒能終其一生向神完全忠心，並恒心跟從他。我們應當知道，要保守和神良好的關系，就務要立定心誌，向神和神的約忠心，直至我們完全得贖的日子。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.78.17-39",
            "paragraphs": [
              "七八17～39概述以色列人在曠野飄流期間的反叛，以及神向他們所發的忿怒。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们不忠于祂， 也不信守祂的约。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的心對他不堅定， 也不忠於和他所立的約。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他們的心向他不正， 在他的約上也不忠心。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For their heart was not steadfast toward Him, Nor were they faithful in His covenant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "their hearts were not loyal to him, they were not faithful to his covenant."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their hearts were not loyal to him. They did not keep his covenant."
      }
    },
