    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.78.17-39",
            "paragraphs": [
              "七八17～39概述以色列人在曠野飄流期間的反叛，以及神向他們所發的忿怒。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们抱怨上帝，说： “难道上帝可以在旷野摆宴席吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們妄論　神，說： “　神可以在曠野擺設筵席嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並且妄論神說： 神在曠野豈能擺設筵席嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then they spoke against God; They said, “Can God prepare a table in the wilderness?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They spoke against God; they said, “Can God really spread a table in the wilderness?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They even spoke against God himself, saying, “God can’t give us food in the wilderness."
      }
    },
