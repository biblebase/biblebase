      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从他们面前赶出外族人， 把外族人的土地分给他们， 作为他们的产业， 使以色列各支派安顿下来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他在他們面前把外族人趕出去， 用繩子量地，分給他們作為產業， 又使以色列眾支派的人居住在自己的帳棚裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他在他們面前趕出外邦人， 用繩子將外邦的地量給他們為業， 叫以色列支派的人住在他們的帳棚裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also drove out the nations before them And apportioned them for an inheritance by measurement, And made the tribes of Israel dwell in their tents."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He drove out nations before them and allotted their lands to them as an inheritance; he settled the tribes of Israel in their homes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He drove out the nations before them; he gave them their inheritance by lot. He settled the tribes of Israel into their homes."
      }
