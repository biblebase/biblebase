    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“看啊，这就是不依靠上帝的人， 他只倚仗自己的财富和以残暴手段获得的权势。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪！這就是那不以　神為自己保障的人， 他只倚靠自己豐盛的財富， 以毀滅別人來加強自己的力量。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：看哪，這就是那不以神為他力量的人， 只倚仗他豐富的財物，在邪惡上堅立自己。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, the man who would not make God his refuge, But trusted in the abundance of his riches And was strong in his evil desire.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Here now is the man who did not make God his stronghold but trusted in his great wealth and grew strong by destroying others!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Look what happens to mighty warriors who do not trust in God. They trust their wealth instead and grow more and more bold in their wickedness.”"
      }
    },
