    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "勇士啊，你为何以作恶为荣？ 上帝的慈爱永远长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "勇士啊！你為甚麼以作惡自誇呢？ 　神的慈愛是常存的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "勇士啊，你為何以作惡自誇？ 神的慈愛是常存的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why do you boast in evil, O mighty man? The lovingkindness of God endures all day long."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why do you boast of evil, you mighty hero? Why do you boast all day long, you who are a disgrace in the eyes of God?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why do you boast about your crimes, great warrior? Don’t you realize God’s justice continues forever?"
      }
    },
