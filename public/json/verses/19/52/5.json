    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.52.5",
            "paragraphs": [
              "五二5四幅驚人的圖畫描述惡人的最終的審判。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝必永远毁灭你， 祂必抓住你， 把你从家里拉出来， 从活人之地铲除。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神必把你永遠拆毀； 他必把你挪去，把你從帳棚中揪出來， 把你從活人之地連根拔起。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神也要毀滅你，直到永遠； 他要把你拿去，從你的帳棚中抽出， 從活人之地將你拔出。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But God will break you down forever; He will snatch you up and tear you away from your tent, And uproot you from the land of the living. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Surely God will bring you down to everlasting ruin: He will snatch you up and pluck you from your tent; he will uproot you from the land of the living."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But God will strike you down once and for all. He will pull you from your home and uproot you from the land of the living. Interlude"
      }
    },
