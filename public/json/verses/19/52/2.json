    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你这诡诈的人啊， 舌头利如剃刀， 尽是害人的奸计。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你圖謀毀滅， 你的舌頭鋒利像剃刀， 常弄詭詐。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的舌頭邪惡詭詐， 好像剃頭刀，快利傷人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your tongue devises destruction, Like a sharp razor, O worker of deceit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You who practice deceit, your tongue plots destruction; it is like a sharpened razor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All day long you plot destruction. Your tongue cuts like a sharp razor; you’re an expert at telling lies."
      }
