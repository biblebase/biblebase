    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.5.9",
            "paragraphs": [
              "五9\"敞開的墳墓\"。即腐敗和危險。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们口中毫无实话， 心里充满恶毒， 喉咙是敞开的坟墓， 舌头上尽是诡诈。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們的口裡沒有真話， 他們的內心充滿毀滅人的意圖， 他們的喉嚨是敞開的墳墓， 他們用舌頭弄詭詐。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "因为他们口中的话不可信， 他们里面都是奸恶， 他们的喉咙是敞开的坟墓， 他们用舌头谄媚人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，他們的口中沒有誠實； 他們的心裏滿有邪惡； 他們的喉嚨是敞開的墳墓； 他們用舌頭諂媚人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For there is no truth in their mouth; their inmost self is destruction; their throat is an open grave; they flatter with their tongue."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is nothing reliable in what they say; Their inward part is destruction itself. Their throat is an open grave; They flatter with their tongue."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Not a word from their mouth can be trusted; their heart is filled with malice. Their throat is an open grave; with their tongues they tell lies."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For there  is no faithfulness in their mouth; Their inward part is destruction; Their throat is an open tomb; They flatter with their tongue."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My enemies cannot speak a truthful word. Their deepest desire is to destroy others. Their talk is foul, like the stench from an open grave. Their tongues are filled with flattery."
      }
    },
