    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.38.9-14",
            "paragraphs": [
              "孤寂的受害者（三十八9～14）",
              "從開頭的主字（此處不是耶和華一字，而是如第22節，“我的主人”），以至最後在朋友離棄、仇敵惡言以向之時，都沒有回話（14節），可以看出他痛定思痛的謙卑。"
            ]
          },
          {
            "verse": "psa.38.11",
            "paragraphs": [
              "11.選用瘟疫（和合：災病）一字或許是因它可與痲瘋扯上關系（如：希伯來聖經，利十三3出現四次），而大衛的朋友待他的態度正是如此。或者這字代表皮膚上有審判的印記，如以賽亞書五十三4，那裏的“擊打”一字與此類似。可嘆的是，往往在一個人最需要別人扶持時（因他外形異常），他卻愈無法吸引人前來。而這種情況的改觀，福音有很大的貢獻。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.38.9-12",
            "paragraphs": [
              "三八9～12神知道大衛正在經受怎樣的痛苦（9,10節）；他的朋友都躲避他（11節），他的仇敵設計陷害他（12節）。"
            ]
          },
          {
            "verse": "psa.38.11",
            "paragraphs": [
              "三八11\"災病\"。這詞用來形容麻風病，可見大衛的朋友都躲避他，如同躲避麻風病人一樣。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因我的疾病，朋友同伴回避我， 亲人远离我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的良朋密友因我的災禍，都站到一旁去； 我的親人也都站得遠遠的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的良朋密友因我的災病都躲在旁邊站着； 我的親戚本家也遠遠地站立。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My loved ones and my friends stand aloof from my plague; And my kinsmen stand afar off."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My friends and companions avoid me because of my wounds; my neighbors stay far away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My loved ones and friends stay away, fearing my disease. Even my own family stands at a distance."
      }
    },
