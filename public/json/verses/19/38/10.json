    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.38.9-14",
            "paragraphs": [
              "孤寂的受害者（三十八9～14）",
              "從開頭的主字（此處不是耶和華一字，而是如第22節，“我的主人”），以至最後在朋友離棄、仇敵惡言以向之時，都沒有回話（14節），可以看出他痛定思痛的謙卑。"
            ]
          },
          {
            "verse": "psa.38.10",
            "paragraphs": [
              "10.此處沒有必要將我心跳動改為RP的“我心迷亂”（那是根據一亞喀得文字根而作的猜測）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.38.9-12",
            "paragraphs": [
              "三八9～12神知道大衛正在經受怎樣的痛苦（9,10節）；他的朋友都躲避他（11節），他的仇敵設計陷害他（12節）。"
            ]
          },
          {
            "verse": "psa.38.10",
            "paragraphs": [
              "三八10\"跳動\"。悸動。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的心砰砰直跳， 气力衰竭， 眼睛黯淡无光。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的心劇烈跳動，我的力量衰退； 連我眼中的光彩也消逝了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我心跳動，我力衰微， 連我眼中的光也沒有了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My heart throbs, my strength fails me; And the light of my eyes, even that has gone from me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My heart pounds, my strength fails me; even the light has gone from my eyes."
      },
