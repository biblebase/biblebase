    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大山啊，你为何如公羊一般跳跃？ 小山啊，你为何像羊羔一样蹦跳？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大山啊！你們為甚麼跳躍像公羊？ 小山啊！你們為甚麼蹦跳像小羊？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大山哪，你為何踴躍，如公羊？ 小山哪，你為何跳舞，如羊羔？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O mountains, that you skip like rams? O hills, like lambs?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why, mountains, did you leap like rams, you hills, like lambs?"
      },
