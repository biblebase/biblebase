    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.125.3",
            "paragraphs": [
              "一二五3\"不常落在義人的分上\"。即不會長期放在義人的土地上。換句話說，那試煉只限於他們所能忍受的。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人必不能长久统治义人的土地， 免得义人也去行恶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人的杖不會常留在義人的地業上， 免得義人伸手作惡。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "恶人的杖不能留在义人所分得的土地上， 免得义人把手伸进不义。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人的杖不常落在義人的分上， 免得義人伸手作惡。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For the scepter of wickedness shall not rest on the land allotted to the righteous, lest the righteous stretch out their hands to do wrong."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the scepter of wickedness shall not rest upon the land of the righteous, So that the righteous will not put forth their hands to do wrong."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The scepter of the wicked will not remain over the land allotted to the righteous, for then the righteous might use their hands to do evil."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For the scepter of wickedness shall not rest On the land allotted to the righteous, Lest the righteous reach out their hands to iniquity."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wicked will not rule the land of the godly, for then the godly might be tempted to do wrong."
      }
    },
