    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.86.2-15",
            "paragraphs": [
              "八六2～15\"虔誠人\"。即享受約關系和神的\"慈愛\"（希伯來文是hesed；5,13,15節）之民當中的忠貞者。參看何西阿書二章19節的腳註。"
            ]
          },
          {
            "verse": "psa.86.8-10",
            "paragraphs": [
              "八六8～10大衛在這三節經文裏贊美神的偉大--惟獨神是偉大的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主啊， 你创造的万国都要来敬拜你， 将荣耀归给你的名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主啊！你所造的萬國都要來，在你面前下拜， 他們必榮耀你的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主啊，你所造的萬民都要來敬拜你； 他們也要榮耀你的名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All nations whom You have made shall come and worship before You, O Lord, And they shall glorify Your name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the nations you have made will come and worship before you, Lord; they will bring glory to your name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the nations you made will come and bow before you, Lord; they will praise your holy name."
      }
