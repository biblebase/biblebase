    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.86.2-15",
            "paragraphs": [
              "八六2～15\"虔誠人\"。即享受約關系和神的\"慈愛\"（希伯來文是hesed；5,13,15節）之民當中的忠貞者。參看何西阿書二章19節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你深爱我， 从阴间的深处拯救了我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你向我大施慈愛， 你救了我的命，免入陰間的深處。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，你向我發的慈愛是大的； 你救了我的靈魂免入極深的陰間。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For Your lovingkindness toward me is great, And You have delivered my soul from the depths of Sheol."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For great is your love toward me; you have delivered me from the depths, from the realm of the dead."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "for your love for me is very great. You have rescued me from the depths of death."
      }
