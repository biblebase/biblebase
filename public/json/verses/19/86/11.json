    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.86.11",
            "paragraphs": [
              "86:11",
              "求你將你的道指教我：詩人在艱難困苦中謙卑己心，求神將他的道路和真理指教他，好使他真心敬畏神的名。當我們歷經艱難和試煉時，也必須呼求神賜我們智慧，好行在他的道上，並賜我們誠實敬畏他且喜愛他真理的心。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.86.2-15",
            "paragraphs": [
              "八六2～15\"虔誠人\"。即享受約關系和神的\"慈愛\"（希伯來文是hesed；5,13,15節）之民當中的忠貞者。參看何西阿書二章19節的腳註。"
            ]
          },
          {
            "verse": "psa.86.11",
            "paragraphs": [
              "八六11\"求你使我專心敬畏你的名\"。即願我專心致誌地尊崇你。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你教我行你的道， 我要行在你的真理中， 求你使我一心敬畏你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你把你的道路指教我， 使我行在你的真理中， 專心敬畏你的名。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华啊，求你把你的道路指示给我， 我要行走在你的真理中！ 求你使我一心敬畏你的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你將你的道指教我； 我要照你的真理行； 求你使我專心敬畏你的名！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Teach me your way, O Lord, that I may walk in your truth; unite my heart to fear your name."
      },
      "nasb": {
        "version_id": "NASB",
