    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.86.1",
            "paragraphs": [
              "86:1",
              "我是困苦窮乏的：因內心謙卑，遭遇患難或困苦窮乏而向神禱告，就必蒙神垂聽應允。因為他特別眷顧那些心靈憂傷、生命困乏的人(比較35:10；74:21；太6:25~34；參路11:3註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，我穷苦无助， 求你垂听我的祷告，应允我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你留心聽我、應允我， 因為我是困苦貧窮的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你側耳應允我， 因我是困苦窮乏的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Incline Your ear, O Lord, and answer me; For I am afflicted and needy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hear me, Lord, and answer me, for I am poor and needy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Bend down, O Lord, and hear my prayer; answer me, for I need your help."
      }
    },
