    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.58.3-5",
            "paragraphs": [
              "五八3～5“一出母胎”。參看第五十一篇5節的腳註。由於天生的罪性，人類說謊，毒害別人，而且不聽任何懇求或呼籲（象蛇不順服弄蛇的人一樣）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "任法师的邪术再高超， 也听不见他的咒语。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不聽行法術者的聲音， 就是極靈的咒語，也是無效。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不聽行法術的聲音， 雖用極靈的咒語也是不聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So that it does not hear the voice of charmers, Or a skillful caster of spells."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "that will not heed the tune of the charmer, however skillful the enchanter may be."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "ignoring the tunes of the snake charmers, no matter how skillfully they play."
      }
    },
