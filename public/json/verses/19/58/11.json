    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，人们必说： “义人终有善报， 确实有一位审判天下的上帝。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，人必說：“義人果然有善報； 在世上確有一位施行審判的　神。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，人必說：義人誠然有善報； 在地上果有施行判斷的神！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And men will say, “Surely there is a reward for the righteous; Surely there is a God who judges on earth!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then people will say, “Surely the righteous still are rewarded; surely there is a God who judges the earth.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then at last everyone will say, “There truly is a reward for those who live for God; surely there is a God who judges justly here on earth.”"
      }
