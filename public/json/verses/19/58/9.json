    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.58.9",
            "paragraphs": [
              "五八9“把青的和燒的一齊刮去”。“青的和燒的”指兩種柴，這裏描述神對惡人的審判。“荊棘”燒得很快，暗示神對惡人的審判也很快。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "刹那间， 荆棘还没有把锅烧热的工夫， 上帝必除灭他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們用荊棘燒火，鍋還未熱， 他要用旋風把燒著的和未燒著的一起颳去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們用荊棘燒火，鍋還未熱， 他要用旋風把青的和燒着的一齊颳去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Before your pots can feel the fire of thorns He will sweep them away with a whirlwind, the green and the burning alike."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Before your pots can feel the heat of the thorns— whether they be green or dry—the wicked will be swept away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God will sweep them away, both young and old, faster than a pot heats over burning thorns."
      }
    },
