    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.122.1",
            "paragraphs": [
              "122:1",
              "耶和華的殿：神的殿理當成為信徒歡心享受與神的親密相交、與聖靈的相交以及分享弟兄姊妹之愛的地方。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人们对我说： “让我们去耶和华的殿吧！” 我感到欢喜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人對我說：“我們到耶和華的殿那裡去”， 我就歡喜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人對我說：我們往耶和華的殿去， 我就歡喜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I was glad when they said to me, “Let us go to the house of the Lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I rejoiced with those who said to me, “Let us go to the house of the Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I was glad when they said to me, “Let us go to the house of the Lord.”"
      }
    },
