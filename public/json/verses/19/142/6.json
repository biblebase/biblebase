    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你垂听我的呼求， 因为我身陷绝境。 求你救我脱离迫害我的人， 因为他们太强大了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你留心聽我的呼求， 因為我落到極卑微的地步； 求你救我脫離逼迫我的人， 因為他們比我強大。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你側耳聽我的呼求， 因我落到極卑之地； 求你救我脫離逼迫我的人， 因為他們比我強盛。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Give heed to my cry, For I am brought very low; Deliver me from my persecutors, For they are too strong for me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Listen to my cry, for I am in desperate need; rescue me from those who pursue me, for they are too strong for me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Hear my cry, for I am very low. Rescue me from my persecutors, for they are too strong for me."
      }
