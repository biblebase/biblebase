    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，我心力交瘁的时候， 只有你知道我当走的路。 敌人已在我行的路上铺下网罗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的靈在我裡面軟弱的時候， 你知道我的道路。 在我所行的路上， 敵人暗設網羅陷害我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的靈在我裏面發昏的時候， 你知道我的道路。 在我行的路上， 敵人為我暗設網羅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When my spirit was overwhelmed within me, You knew my path. In the way where I walk They have hidden a trap for me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When my spirit grows faint within me, it is you who watch over my way. In the path where I walk people have hidden a snare for me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When I am overwhelmed, you alone know the way I should turn. Wherever I go, my enemies have set traps for me."
      }
    },
