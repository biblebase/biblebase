    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.21.13",
            "paragraphs": [
              "惟獨耶和華（二十一13）",
              "這一節將本詩作結束，與詩篇第二十篇的筆法相呼應。不過在這裏，不再看見任何人，只聞其聲而已；人的角色是站在一旁，欣賞並感恩。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，愿你彰显大能，受人尊崇！ 我们要歌颂、赞美你的权能。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！願你因自己的能力被尊崇， 好讓我們歌唱，頌讚你的大能。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，願你因自己的能力顯為至高！ 這樣，我們就唱詩，歌頌你的大能。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be exalted, O Lord, in Your strength; We will sing and praise Your power."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be exalted in your strength, Lord; we will sing and praise your might."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rise up, O Lord, in all your power. With music and singing we celebrate your mighty acts."
      }
    },
