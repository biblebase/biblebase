    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我说：“我要谨言慎行， 免犯口舌之罪。 只要身边有恶人， 我就用嚼环勒住自己的口。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我曾說：“我要謹慎我的行為， 不讓我的舌頭犯罪； 惡人在我面前的時候， 我總要用罩子約束我的嘴。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我曾說：我要謹慎我的言行， 免得我舌頭犯罪； 惡人在我面前的時候， 我要用嚼環勒住我的口。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I said, “I will guard my ways That I may not sin with my tongue; I will guard my mouth as with a muzzle While the wicked are in my presence.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I said, “I will watch my ways and keep my tongue from sin; I will put a muzzle on my mouth while in the presence of the wicked.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I said to myself, “I will watch what I do and not sin in what I say. I will hold my tongue when the ungodly are around me.”"
      }
    },
