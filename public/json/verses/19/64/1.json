    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.64.1",
            "paragraphs": [
              "64:1",
              "1~10保護我的性命：詩人祈求神保護他免受敵人的陰謀和暗算。我們在與撒但的屬靈爭戰中也可以如此禱告，切切求神救我們脫離那惡者和受撒但指使來攻擊我們的人(參太6:13,註)。在這一切爭戰中，我們都當堅信神必向我們施行拯救(9~10節；參“勝過撒但和鬼魔的能力”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，求你倾听我的苦诉， 求你保护我的性命不受仇敌的威胁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！我哀訴的時候，求你垂聽我的聲音； 求你保護我的性命，脫離仇敵的恐嚇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，我哀歎的時候，求你聽我的聲音！ 求你保護我的性命，不受仇敵的驚恐！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hear my voice, O God, in my complaint; Preserve my life from dread of the enemy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hear me, my God, as I voice my complaint; protect my life from the threat of the enemy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O God, listen to my complaint. Protect my life from my enemies’ threats."
      }
    },
