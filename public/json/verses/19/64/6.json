    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们图谋不义之事， 认为策划得无懈可击。 人心真是狡猾。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們圖謀奸惡，說： “我們設計了最精密的陰謀！” 人的意念和心思實在深不可測。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們圖謀奸惡， 說：我們是極力圖謀的。 他們各人的意念心思是深的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They devise injustices, saying, “We are ready with a well-conceived plot”; For the inward thought and the heart of a man are deep."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They plot injustice and say, “We have devised a perfect plan!” Surely the human mind and heart are cunning."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As they plot their crimes, they say, “We have devised the perfect plan!” Yes, the human heart and mind are cunning."
      }
