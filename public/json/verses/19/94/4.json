    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.94.4-7",
            "paragraphs": [
              "九四4～7惡人是狂傲（4節）和殘忍的（5,6節），他們愚蠢地以為神不知道他們所作的（7節；比較11節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们大放厥词，狂妄自大。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們不住地說驕傲的話， 所有作孽的人都誇耀自己。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們絮絮叨叨說傲慢的話； 一切作孽的人都自己誇張。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They pour forth words, they speak arrogantly; All who do wickedness vaunt themselves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They pour out arrogant words; all the evildoers are full of boasting."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How long will they speak with arrogance? How long will these evil people boast?"
      }
