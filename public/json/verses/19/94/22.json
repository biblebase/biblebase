    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.94.20-23",
            "paragraphs": [
              "九四20～23第20節的問題（神為何容許邪惡的統治者使用律法，屈枉正直？）的回答方法是：承認邪惡的存在（21節），神關懷屬祂的子民（22節），以及最終的懲罰（23節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但耶和华是我的堡垒， 我的上帝是保护我的磐石。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但耶和華作了我的高臺， 我的　神作了我避難的磐石。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但耶和華向來作了我的高臺； 我的神作了我投靠的磐石。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the Lord has been my stronghold, And my God the rock of my refuge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the Lord has become my fortress, and my God the rock in whom I take refuge."
      },
      "nlt": {
