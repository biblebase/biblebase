    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.94.17",
            "paragraphs": [
              "九四17“我就住在寂靜之中了”。即我不久就死去。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华若不帮助我， 我早已归入死亡的沉寂中了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果不是耶和華幫助我， 我早就不在人世了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若不是耶和華幫助我， 我就住在寂靜之中了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If the Lord had not been my help, My soul would soon have dwelt in the abode of silence."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Unless the Lord had given me help, I would soon have dwelt in the silence of death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Unless the Lord had helped me, I would soon have settled in the silence of the grave."
      }
    },
