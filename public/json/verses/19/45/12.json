      "leili": {
        "content": [
          {
            "verse": "psa.45.12",
            "paragraphs": [
              "四五12\"推羅的女子\"。把推羅的民人格化。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "泰尔人必来送礼， 富人必来取悦你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "推羅的居民必帶著禮物而來； 民間富有的人要向你求恩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "泰爾的民必來送禮； 民中的富足人也必向你求恩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The daughter of Tyre will come with a gift; The rich among the people will seek your favor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The city of Tyre will come with a gift, people of wealth will seek your favor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The princess of Tyre will shower you with gifts. The wealthy will beg your favor."
      }
