    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使你的名世代受尊崇， 万民必永远称谢你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必使你的名被萬代記念； 因此萬民都必稱讚你，直到永永遠遠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必叫你的名被萬代記念， 所以萬民要永永遠遠稱謝你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will cause Your name to be remembered in all generations; Therefore the peoples will give You thanks forever and ever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will perpetuate your memory through all generations; therefore the nations will praise you for ever and ever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will bring honor to your name in every generation. Therefore, the nations will praise you forever and ever."
      }
