      "leili": {
        "content": [
          {
            "verse": "psa.45.1",
            "paragraphs": [
              "四五1\"湧出\"。即為王寫作這首情歌。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我心中涌出美丽的诗章， 我要把它献给王， 我的舌头是诗人手上的妙笔。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我心裡湧出優美的言辭； 我要為王朗誦我的作品； 我的舌頭像經驗豐富的作家的筆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我心裏湧出美辭； 我論到我為王做的事， 我的舌頭是快手筆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My heart overflows with a good theme; I address my verses to the King; My tongue is the pen of a ready writer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My heart is stirred by a noble theme as I recite my verses for the king; my tongue is the pen of a skillful writer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Beautiful words stir my heart. I will recite a lovely poem about the king, for my tongue is like the pen of a skillful poet."
      }
    },
