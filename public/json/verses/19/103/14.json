    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.103.14",
            "paragraphs": [
              "103:14",
              "他知道我們的本體：神憐恤他的兒女，因為他深深了解他們的軟弱和缺乏。就是最出色的信徒也都時時靜候天父的憐恤。世上的父親怎樣深切憐恤他遭受惡待、失敗和痛苦的兒女，我們的天父也照樣為他兒女所受的傷害而痛心。所以，當我們遭遇患難、失敗和掙紮之苦的時候，切不要以為神對我們漠不關心；相反，我們當記住，他的眼目時常眷顧體恤我們，並且隨時準備照我們的需要幫助我們(比較路7:12~13)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为祂知道我们的本源， 顾念我们不过是尘土。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他知道我們的本體， 記得我們不過是塵土。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他知道我們的本體， 思念我們不過是塵土。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He Himself knows our frame; He is mindful that we are but dust."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for he knows how we are formed, he remembers that we are dust."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For he knows how weak we are; he remembers we are only dust."
      }
    },
