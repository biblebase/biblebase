    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华所造的万物啊， 要在祂掌管的各处称颂祂。 我的心啊，要称颂耶和华！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們一切被他所造的， 在他統治的各處，都要稱頌耶和華。 我的心哪，你要稱頌耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們一切被他造的， 在他所治理的各處， 都要稱頌耶和華！ 我的心哪，你要稱頌耶和華！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Bless the Lord, all you works of His, In all places of His dominion; Bless the Lord, O my soul!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Praise the Lord, all his works everywhere in his dominion. Praise the Lord, my soul."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Praise the Lord, everything he has created, everything in all his kingdom. Let all that I am praise the Lord."
      }
