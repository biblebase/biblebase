        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂以美物满足我的愿望， 使我如鹰一般恢复青春。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他以福樂使你的心願滿足， 以致你好像鷹一般恢復青春的活力。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他用美物使你所願的得以知足， 以致你如鷹返老還童。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who satisfies your years with good things, So that your youth is renewed like the eagle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "who satisfies your desires with good things so that your youth is renewed like the eagle’s."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He fills my life with good things. My youth is renewed like the eagle’s!"
      }
