    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华永永远远爱敬畏祂的人， 以公义待他们的子子孙孙，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但從亙古到永遠，耶和華的慈愛臨到敬畏他的人； 他的公義也歸於他們的子子孫孫，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但耶和華的慈愛歸於敬畏他的人， 從亙古到永遠； 他的公義也歸於子子孫孫－"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the lovingkindness of the Lord is from everlasting to everlasting on those who fear Him, And His righteousness to children’s children,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But from everlasting to everlasting the Lord’s love is with those who fear him, and his righteousness with their children’s children—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the love of the Lord remains forever with those who fear him. His salvation extends to the children’s children"
      }
    },
