    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听从耶和华的命令、遵行祂吩咐的大能天使啊， 你们要称颂祂！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們作他天使的， 就是那些大有能力，行他所吩咐， 以及聽從他命令的，都要稱頌耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "聽從他命令、成全他旨意、 有大能的天使，都要稱頌耶和華！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Bless the Lord, you His angels, Mighty in strength, who perform His word, Obeying the voice of His word!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Praise the Lord, you his angels, you mighty ones who do his bidding, who obey his word."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Praise the Lord, you angels, you mighty ones who carry out his plans, listening for each of his commands."
      }
