    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.33.13-19",
            "paragraphs": [
              "祂分辨的眼目（三十三13～19）",
              "現在進入眼簾的是審判與拯救；神雖有可畏的能力，但祂卻不是用極權來治理，而是根據完全的知識（13～15節）、完全的掌握（16、17節），與完全的愛（18、19節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "君王不能靠兵多取胜， 勇士不能凭力大获救。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "君王不是因兵多得勝， 勇士不是因力大得救。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "君王不能因兵多得勝； 勇士不能因力大得救。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king is not saved by a mighty army; A warrior is not delivered by great strength."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No king is saved by the size of his army; no warrior escapes by his great strength."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The best-equipped army cannot save a king, nor is great strength enough to save a warrior."
      }
