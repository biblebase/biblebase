    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.33.13-19",
            "paragraphs": [
              "祂分辨的眼目（三十三13～19）",
              "現在進入眼簾的是審判與拯救；神雖有可畏的能力，但祂卻不是用極權來治理，而是根據完全的知識（13～15節）、完全的掌握（16、17節），與完全的愛（18、19節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.33.17",
            "paragraphs": [
              "三三17\"靠馬得救是枉然的\"。更可作：靠馬得勝是錯誤的盼望。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "靠战马取胜实属妄想， 马虽力大也不能救人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "想靠馬得勝是枉然的； 馬雖然力大，也不能救人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "靠馬得救是枉然的； 馬也不能因力大救人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A horse is a false hope for victory; Nor does it deliver anyone by its great strength."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A horse is a vain hope for deliverance; despite all its great strength it cannot save."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t count on your warhorse to give you victory— for all its strength, it cannot save you."
      }
