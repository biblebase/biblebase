      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華破壞列國的謀略， 使萬民的計劃挫敗。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華使列國的籌算歸於無有， 使眾民的思念無有功效。"
