      "leili": {
        "content": [
          {
            "verse": "psa.112.4-9",
            "paragraphs": [
              "一一二4～9義人是別人的祝福（4節），他為人慷慨（5,9節），在逆境中也處變不驚（6～8節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他心里镇定自若，毫不害怕， 他终必战胜仇敌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的心堅決，毫不懼怕， 直到他看見他的敵人遭報。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他的心坚定，总不害怕， 直到看见敌人的下场。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他心確定，總不懼怕， 直到他看見敵人遭報。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "His heart is steady; he will not be afraid, until he looks in triumph on his adversaries."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His heart is upheld, he will not fear, Until he looks with satisfaction on his adversaries."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their hearts are secure, they will have no fear; in the end they will look in triumph on their foes."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "His heart is established; He will not be afraid, Until he sees his  desire upon his enemies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They are confident and fearless and can face their foes triumphantly."
      }
