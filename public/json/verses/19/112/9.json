    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.112.4-9",
            "paragraphs": [
              "一一二4～9義人是別人的祝福（4節），他為人慷慨（5,9節），在逆境中也處變不驚（6～8節）。"
            ]
          },
          {
            "verse": "psa.112.9",
            "paragraphs": [
              "一一二9\"角\"。力量的象征。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他慷慨施舍，周济穷人； 他的仁义永远长存； 他必充满力量，得享尊荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他廣施錢財，賙濟窮人； 他的仁義存到永遠； 他的角必被高舉，大有榮耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他施捨錢財，賙濟貧窮； 他的仁義存到永遠。 他的角必被高舉，大有榮耀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He has given freely to the poor, His righteousness endures forever; His horn will be exalted in honor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They have freely scattered their gifts to the poor, their righteousness endures forever; their horn will be lifted high in honor."
      },
