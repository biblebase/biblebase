    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人见状，必然恼怒， 咬牙切齿，气绝身亡。 恶人的盼望必破灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人看見就惱怒， 他必咬牙切齒，身心耗損； 惡人的心願必幻滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人看見便惱恨，必咬牙而消化； 惡人的心願要歸滅絕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wicked will see it and be vexed, He will gnash his teeth and melt away; The desire of the wicked will perish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wicked will see and be vexed, they will gnash their teeth and waste away; the longings of the wicked will come to nothing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wicked will see this and be infuriated. They will grind their teeth in anger; they will slink away, their hopes thwarted."
      }
