      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "每天早晨我要消灭国中所有的恶人， 从耶和华的城中铲除一切作恶的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我每日早晨必滅絕國中所有的惡人， 好把所有作孽的人都從耶和華的城裡剪除。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "每个清晨，我都要除尽地上的一切恶人， 为要从耶和华的城中剪除一切作恶者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我每日早晨要滅絕國中所有的惡人， 好把一切作孽的從耶和華的城裏剪除。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Morning by morning I will destroy all the wicked in the land, cutting off all the evildoers from the city of the Lord."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every morning I will destroy all the wicked of the land, So as to cut off from the city of the Lord all those who do iniquity."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every morning I will put to silence all the wicked in the land; I will cut off every evildoer from the city of the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Early I will destroy all the wicked of the land, That I may cut off all the evildoers from the city of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My daily task will be to ferret out the wicked and free the city of the Lord from their grip."
      }
