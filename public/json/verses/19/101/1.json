    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.101.1",
            "paragraphs": [
              "101:1",
              "1~8慈愛和公平：這首詩生動地描述了一個喜悅遵行神旨意的以色列君王當存怎樣的心來治理他的國。詩篇中所表明的態度也是治理神教會的人當有的心誌(比較徒20:28；24:16)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 我要歌颂你的慈爱和公正， 我要歌颂你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要歌唱慈愛和公正； 耶和華啊！我要向你歌頌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要歌唱慈愛和公平； 耶和華啊，我要向你歌頌！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will sing of lovingkindness and justice, To You, O Lord, I will sing praises."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will sing of your love and justice; to you, Lord, I will sing praise."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will sing of your love and justice, Lord. I will praise you with songs."
      }
