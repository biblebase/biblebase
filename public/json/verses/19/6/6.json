    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.6.6",
            "paragraphs": [
              "6:6",
              "我因唉哼而困乏：詩人心靈的愁煩持續已久。“要到幾時”(3節)的哀聲以及6~7節，都證實神並沒有立即恢復詩人心靈的平安，與他同在並向他施恩。8~9節告訴我們，神會按他自己的時間垂聽我們的哀哭懇求和悅納我們的禱告。我們不必絕望，要憑信心等候神(比較13:1；74:9)；到了時候他必應允我們。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我因哀叹心力交瘁， 夜夜哭泣，泪漂床榻， 湿透被褥。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我因悲歎而疲憊， 我夜夜流淚，把床漂起， 把床榻浸透。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因唉哼而困乏； 我每夜流淚，把牀榻漂起， 把褥子濕透。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am weary with my sighing; Every night I make my bed swim, I dissolve my couch with my tears."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am worn out from my groaning. All night long I flood my bed with weeping and drench my couch with tears."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am worn out from sobbing. All night I flood my bed with weeping, drenching it with my tears."
      }
    },
