    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.91.14",
            "paragraphs": [
              "91:14",
              "因為他專心愛我：在此，主親自向那些忠心跟從他的人說話。因為他們真正愛他，他就親自應許要在患難中幫助他們。得蒙神眷顧保護的秘訣在於以感恩愛慕的心與神親密相交。他認識誰是這樣的信徒，並要在患難中與他們同在，垂聽他們的禱告，使他們一生中滿有神的同在和豐富的供應(參約14:12~21；15:1~10)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“因为他爱我，我必拯救他； 因为他信靠我的名， 我必保护他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：“因為他戀慕我，我必搭救他； 因為他認識我的名，我必保護他。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“因为他爱慕我，我就挽救他； 因为他认识我的名，我就高举他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神說：因為他專心愛我，我就要搭救他； 因為他知道我的名，我要把他安置在高處。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Because he holds fast to me in love, I will deliver him; I will protect him, because he knows my name."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Because he has loved Me, therefore I will deliver him; I will set him securely on high, because he has known My name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Because he loves me,” says the Lord, “I will rescue him; I will protect him, for he acknowledges my name."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Because he has set his love upon Me, therefore I will deliver him; I will set him on high, because he has known My name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord says, “I will rescue those who love me. I will protect those who trust in my name."
      }
    },
