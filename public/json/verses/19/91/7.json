    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.91.5-7",
            "paragraphs": [
              "九一5～7在神裏面，我們無論何時（5節），無論在何等危險的情況中（6節）和在何種境況裏（7節），都必得安穩。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尽管千人扑倒在你左边， 万人扑倒在你右边， 你必安然无恙。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖有千人仆倒在你的左邊， 萬人仆倒在你的右邊， 但災害必不臨近你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "雖有千人仆倒在你旁邊， 萬人仆倒在你右邊， 這災卻不得臨近你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A thousand may fall at your side And ten thousand at your right hand, But it shall not approach you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A thousand may fall at your side, ten thousand at your right hand, but it will not come near you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Though a thousand fall at your side, though ten thousand are dying around you, these evils will not touch you."
      }
