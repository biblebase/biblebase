    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.91.1-2",
            "paragraphs": [
              "九一1,2留意四個神的名字：\"至高者\"（參看創一四18的腳註）、\"全能者\"（參看創一七1的腳註）、\"耶和華\"（參看創二4的腳註）、\"神\"（參看創一1的腳註）。這位神是祂子民的\"隱密處\"、藏匿與保護的地方（\"蔭下\"）、\"避難所\"和\"山寨\"。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要对耶和华说： “你是我的避难所，我的堡垒，是我所信靠的上帝。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要對耶和華說： “你是我的避難所，我的保障， 你是我的　神，我所倚靠的。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我要对耶和华说： “我的避难所、我的要塞、我的神， 我依靠你！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要論到耶和華說： 他是我的避難所，是我的山寨， 是我的神，是我所倚靠的。"
      },
      "esv": {
        "version_id": "ESV",
