    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.134.1",
            "paragraphs": [
              "134:1",
              "1~3夜間站在耶和華殿中的：這首詩篇使我們一睹神殿中他仆人徹夜敬拜、恒切代求的感人場面。新約時代神子民不也應當投身到這徹夜不眠的敬拜中，獻上我們的懇切代禱，以推進教會的復興和使更多失喪靈魂的得救嗎？"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.134.1",
            "paragraphs": [
              "一三四1\"稱頌\"。參看第一百三十篇1節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "夜间在耶和华殿中事奉的仆人们啊， 你们都要称颂耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的眾僕人哪！ 你們要稱頌耶和華； 夜間在耶和華殿中侍候的， 你們要稱頌耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的僕人夜間站在耶和華殿中的， 你們當稱頌耶和華！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, bless the Lord, all servants of the Lord, Who serve by night in the house of the Lord!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Praise the Lord, all you servants of the Lord who minister by night in the house of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Oh, praise the Lord, all you servants of the Lord, you who serve at night in the house of the Lord."
      }
