    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.111.10",
            "paragraphs": [
              "111:10",
              "敬畏耶和華是智慧的開端：這一真理是舊約聖經中智慧文學的根基(參伯28:28；箴1:7；9:10；傳12:13；參“敬畏神”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.111.10",
            "paragraphs": [
              "一一一10\"敬畏耶和華是智慧的開端\"。參看箴言一章7節的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智慧始于敬畏耶和华， 遵行祂命令者皆明睿。 祂永远当受赞美。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "敬畏耶和華是智慧的開端； 凡是這樣行的都是明智的。 耶和華該受讚美，直到永遠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "敬畏耶和華是智慧的開端； 凡遵行他命令的是聰明人。 耶和華是永遠當讚美的！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The fear of the Lord is the beginning of wisdom; A good understanding have all those who do His commandments; His praise endures forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The fear of the Lord is the beginning of wisdom; all who follow his precepts have good understanding. To him belongs eternal praise."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fear of the Lord is the foundation of true wisdom. All who obey his commandments will grow in wisdom. Praise him forever!"
      }
    },
