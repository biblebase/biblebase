    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使我们先前经历了多少苦难和不幸的岁月， 求你也赐给我们多少欢乐的岁月。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使我們受苦和遭難的年日有多久， 求你使我們喜樂的日子也有多久。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你苦待我们的日子和我们遭害的岁月是怎样， 求你使我们的欢喜也怎样。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你照着你使我們受苦的日子， 和我們遭難的年歲，叫我們喜樂。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Make us glad for as many days as you have afflicted us, and for as many years as we have seen evil."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Make us glad according to the days You have afflicted us, And the years we have seen evil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Make us glad for as many days as you have afflicted us, for as many years as we have seen trouble."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Make us glad according to the days in  which You have afflicted us, The years in  which we have seen evil."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give us gladness in proportion to our former misery! Replace the evil years with good."
      }
    },
