    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿主——我们的上帝恩待我们， 使我们所做的亨通， 使我们所做的亨通。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願主我們的　神的恩慈臨到我們身上； 願你為我們的緣故，堅立我們手所作的工； 我們手所作的工，願你堅立。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願主－我們神的榮美歸於我們身上。 願你堅立我們手所做的工； 我們手所做的工，願你堅立。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let the favor of the Lord our God be upon us; And confirm for us the work of our hands; Yes, confirm the work of our hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the favor of the Lord our God rest on us; establish the work of our hands for us— yes, establish the work of our hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And may the Lord our God show us his approval and make our efforts successful. Yes, make our efforts successful!"
      }
