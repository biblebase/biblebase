    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.61.4",
            "paragraphs": [
              "61:4",
              "你翅膀下的隱密處：參35:1~28註；57:1註。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.61.2-4",
            "paragraphs": [
              "六一2～4“那比我更高的盤石”。戴維不能靠自己的力量到達的庇護之所，而那裏能給他保護與安全。神就是那庇護之所，詩中用了四個比喻來描述：“避難所”、“堅固臺”（堡壘）、“帳幕”和“翅膀下的隱密處”。參看第五十七篇1節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要永远住在你的圣幕里， 在你翅膀下得荫庇！（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要永遠住在你的帳棚裡， 投靠在你翅膀下的隱密處。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要永遠住在你的帳幕裏！ 我要投靠在你翅膀下的隱密處！（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let me dwell in Your tent forever; Let me take refuge in the shelter of Your wings. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I long to dwell in your tent forever and take refuge in the shelter of your wings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let me live forever in your sanctuary, safe beneath the shelter of your wings! Interlude"
      }
    },
