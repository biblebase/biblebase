    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.139.8",
            "paragraphs": [
              "一三九8\"陰間\"（希伯來文是Sheol）。即使死人居住的地方，也不在神的全知以外（比較啟一四10）。有關陰間的含義，參看創世記三十七章35節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我若升到天上，你在那里； 我若下到阴间，你也在那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我升到天上，你在那裡； 如果我在陰間下榻，你也在那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若升到天上，你在那裏； 我若在陰間下榻，你也在那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I ascend to heaven, You are there; If I make my bed in Sheol, behold, You are there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If I go up to the heavens, you are there; if I make my bed in the depths, you are there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If I go up to heaven, you are there; if I go down to the grave, you are there."
      }
