    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.139.19-20",
            "paragraphs": [
              "一三九19,20\"好流人血的\"。即暴力的、兇狠的人。他們也妄稱耶和華的名。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，愿你杀戮恶人！ 嗜血成性的人啊，你们走开！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！甚願你殺戮惡人； 你們流人血的啊，離開我去吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你必要殺戮惡人； 所以，你們好流人血的，離開我去吧！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O that You would slay the wicked, O God; Depart from me, therefore, men of bloodshed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If only you, God, would slay the wicked! Away from me, you who are bloodthirsty!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O God, if only you would destroy the wicked! Get out of my life, you murderers!"
      }
    },
