    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.139.5",
            "paragraphs": [
              "一三九5\"曉得\"。這裏可能指人不能逃避神的全知，或指神總是知道我們的處境，並且能保護我們。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你在我前后护卫着我， 以大能的手保护我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你在我前後圍繞著我， 你的手按在我身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你在我前後環繞我， 按手在我身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have enclosed me behind and before, And laid Your hand upon me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You hem me in behind and before, and you lay your hand upon me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You go before me and follow me. You place your hand of blessing on my head."
      }
