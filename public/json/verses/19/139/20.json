    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.139.19-20",
            "paragraphs": [
              "一三九19,20\"好流人血的\"。即暴力的、兇狠的人。他們也妄稱耶和華的名。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们恶言顶撞你， 你的仇敌亵渎你的名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們惡意說話頂撞你， 你的仇敵妄稱你的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他們說惡言頂撞你； 你的仇敵也妄稱你的名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they speak against You wickedly, And Your enemies take Your name in vain."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They speak of you with evil intent; your adversaries misuse your name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They blaspheme you; your enemies misuse your name."
      }
