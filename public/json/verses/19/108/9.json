    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.108.9",
            "paragraphs": [
              "108:9",
              "摩押……以東……非利士：摩押、以東和非利士都是以色列的敵國，分別在以色列東、南、西邊與之交界。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押是我的洗脚盆， 我要把鞋扔给以东， 我要在非利士高唱凯歌。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩押是我的洗腳盆； 我要向以東拋鞋， 我要因戰勝非利士歡呼。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩押是我的沐浴盆； 我要向以東拋鞋； 我必因勝非利士呼喊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moab is My washbowl; Over Edom I shall throw My shoe; Over Philistia I will shout aloud.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moab is my washbasin, on Edom I toss my sandal; over Philistia I shout in triumph.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Moab, my washbasin, will become my servant, and I will wipe my feet on Edom and shout in triumph over Philistia.”"
      }
    },
