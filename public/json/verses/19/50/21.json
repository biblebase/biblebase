    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我对你的所作所为默然不语， 你就以为我与你是同道。 现在我要责备你， 当面指出你的罪状。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你作了這些事，我默不作聲； 你以為我和你一樣？ 其實我要責備你，要當面指控你。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你做了这些，我还保持沉默； 你就以为我完全像你一样； 其实我要责备你，并且把这些摆列在你眼前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你行了這些事，我還閉口不言， 你想我恰和你一樣； 其實我要責備你，將這些事擺在你眼前。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "These things you have done, and I have been silent; you thought that I was one like yourself. But now I rebuke you and lay the charge before you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These things you have done and I kept silence; You thought that I was just like you; I will reprove you and state the case in order before your eyes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you did these things and I kept silent, you thought I was exactly like you. But I now arraign you and set my accusations before you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "These things you have done, and I kept silent; You thought that I was altogether like you; But I will rebuke you, And set them in order before your eyes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "While you did all this, I remained silent, and you thought I didn’t care. But now I will rebuke you, listing all my charges against you."
      }
    },
