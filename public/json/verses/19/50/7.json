    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂说：“我的子民啊，听我说！ 以色列啊，我要指控你！ 我是上帝，是你的上帝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我的子民哪！你們要聽，我要說話； 以色列啊！我要控訴你； 我是　神，是你的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的民哪，你們當聽我的話！ 以色列啊，我要勸戒你； 我是神，是你的神！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Hear, O My people, and I will speak; O Israel, I will testify against you; I am God, your God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Listen, my people, and I will speak; I will testify against you, Israel: I am God, your God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O my people, listen as I speak. Here are my charges against you, O Israel: I am God, your God!"
      }
