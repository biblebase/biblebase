    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "向我献上感恩就是尊崇我， 我必拯救走正路的人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是以感謝為祭獻上的，就是尊敬我； 那預備道路的，我必使他得見　神的救恩。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡以感謝獻上為祭的便是榮耀我； 那按正路而行的，我必使他得着我的救恩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who offers a sacrifice of thanksgiving honors Me; And to him who orders his way aright I shall show the salvation of God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who sacrifice thank offerings honor me, and to the blameless I will show my salvation.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But giving thanks is a sacrifice that truly honors me. If you keep to my path, I will reveal to you the salvation of God.”"
      }
    },
