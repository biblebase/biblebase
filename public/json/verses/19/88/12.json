    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.88.10-12",
            "paragraphs": [
              "八八10～12死人不能贊美神。第六篇4、5節也說出相同的論點。參看那裏的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "黑暗之地有谁知道你的神迹呢？ 在被遗忘之地有谁知道你的公义作为呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的奇事在黑暗裡有人知道嗎？ 你的公義在那遺忘之地有人知道嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的奇事豈能在幽暗裏被知道嗎？ 你的公義豈能在忘記之地被知道嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will Your wonders be made known in the darkness? And Your righteousness in the land of forgetfulness?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Are your wonders known in the place of darkness, or your righteous deeds in the land of oblivion?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can the darkness speak of your wonderful deeds? Can anyone in the land of forgetfulness talk about your righteousness?"
      }
    },
