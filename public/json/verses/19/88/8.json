    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使我众叛亲离，遭人唾弃。 我陷入困境，无路可逃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使我的知己都遠離我， 使我成為他們所厭惡的。 我被囚禁，不能外出。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你使认识我的人远离我， 使我成为他们所憎恶的； 我被囚禁，不能出来。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你把我所認識的隔在遠處， 使我為他們所憎惡； 我被拘困，不得出來。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You have caused my companions to shun me; you have made me a horror to them. I am shut in so that I cannot escape;"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You have removed my acquaintances far from me; You have made me an object of loathing to them; I am shut up and cannot go out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have taken from me my closest friends and have made me repulsive to them. I am confined and cannot escape;"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You have put away my acquaintances far from me; You have made me an abomination to them; I  am shut up, and I cannot get out;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have driven my friends away by making me repulsive to them. I am in a trap with no way of escape."
      }
    },
