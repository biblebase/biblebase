    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的烈怒重重地压着我， 如汹涌波涛把我淹没。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的烈怒重重地壓著我， 你的波浪從四方八面把我淹蓋。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的忿怒重壓我身； 你用一切的波浪困住我。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your wrath has rested upon me, And You have afflicted me with all Your waves. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your wrath lies heavily on me; you have overwhelmed me with all your waves."
