    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为他们迫害你击打过的人， 嘲笑你所打伤之人的痛苦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們迫害你所擊打的人， 嘲笑你所擊傷的人的痛苦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，你所擊打的，他們就逼迫； 你所擊傷的，他們戲說他的愁苦。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they have persecuted him whom You Yourself have smitten, And they tell of the pain of those whom You have wounded."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For they persecute those you wound and talk about the pain of those you hurt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "To the one you have punished, they add insult to injury; they add to the pain of those you have hurt."
      }
    },
