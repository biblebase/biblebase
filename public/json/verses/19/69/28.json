    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.69.28",
            "paragraphs": [
              "六九28\"生命冊\"。從公民登記借用過來的比喻。大衛祈求神使他的敵人在\"義人\"的登記冊上沒有位置（啟二○15）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他们的名字从生命册上被抹去， 不得和义人的名字列在一起。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願他們的名字從生命冊上被塗抹， 不要讓他們和義人一同被記錄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願他們從生命冊上被塗抹， 不得記錄在義人之中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May they be blotted out of the book of life And may they not be recorded with the righteous."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May they be blotted out of the book of life and not be listed with the righteous."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Erase their names from the Book of Life; don’t let them be counted among the righteous."
      }
    },
