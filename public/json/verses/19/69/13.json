    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是，耶和华啊， 在你悦纳人的时候， 我向你祷告。 上帝啊， 求你以你的大爱和信实拯救我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但是，耶和華啊！ 在悅納的時候，我向你禱告； 　神啊！求你按著你豐盛的慈愛， 憑著你信實的拯救應允我。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "可是我，耶和华啊，我向你祷告！ 神哪，在悦纳的时候， 求你藉着你丰盛的慈爱， 藉着你信实的救恩，回应我吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我在悅納的時候向你－耶和華祈禱。 神啊，求你按你豐盛的慈愛， 憑你拯救的誠實應允我！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But as for me, my prayer is to you, O Lord. At an acceptable time, O God, in the abundance of your steadfast love answer me in your saving faithfulness."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for me, my prayer is to You, O Lord, at an acceptable time; O God, in the greatness of Your lovingkindness, Answer me with Your saving truth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But I pray to you, Lord, in the time of your favor; in your great love, O God, answer me with your sure salvation."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But as for me, my prayer is to You, O Lord, in the acceptable time; O God, in the multitude of Your mercy, Hear me in the truth of Your salvation."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I keep praying to you, Lord, hoping this time you will show me favor. In your unfailing love, O God, answer my prayer with your sure salvation."
      }
    },
