    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿他们眼目昏暗，无法看见； 愿他们哆哆嗦嗦，直不起腰来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願他們的眼睛昏花，不能看見； 願他們的腰不停地戰抖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願他們的眼睛昏矇，不得看見； 願你使他們的腰常常戰抖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May their eyes grow dim so that they cannot see, And make their loins shake continually."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May their eyes be darkened so they cannot see, and their backs be bent forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let their eyes go blind so they cannot see, and make their bodies shake continually."
      }
