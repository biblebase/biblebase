    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他喜欢咒诅人， 愿咒诅临到他； 他不喜欢祝福人， 愿祝福远离他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他愛咒詛，願咒詛臨到他； 他不喜愛祝福，願福樂遠離他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他愛咒罵，咒罵就臨到他； 他不喜愛福樂，福樂就與他遠離！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He also loved cursing, so it came to him; And he did not delight in blessing, so it was far from him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He loved to pronounce a curse— may it come back on him. He found no pleasure in blessing— may it be far from him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He loved to curse others; now you curse him. He never blessed others; now don’t you bless him."
      }
