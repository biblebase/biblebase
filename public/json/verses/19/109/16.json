    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为他毫无仁慈， 迫害困苦、贫穷和伤心的人， 置他们于死地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他從沒有想起要施慈愛， 只知道迫害困苦、貧窮和傷心的人， 要把他們置於死地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他不想施恩， 卻逼迫困苦窮乏的和傷心的人， 要把他們治死。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because he did not remember to show lovingkindness, But persecuted the afflicted and needy man, And the despondent in heart, to put them to death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For he never thought of doing a kindness, but hounded to death the poor and the needy and the brokenhearted."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For he refused all kindness to others; he persecuted the poor and needy, and he hounded the brokenhearted to death."
      }
    },
