    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华啊， 求你因自己的名恩待我， 因你的美善和慈爱拯救我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於你，主耶和華啊！求你為你名的緣故恩待我； 按著你美好的慈愛拯救我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主－耶和華啊，求你為你的名恩待我； 因你的慈愛美好，求你搭救我！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But You, O God, the Lord, deal kindly with me for Your name’s sake; Because Your lovingkindness is good, deliver me;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you, Sovereign Lord, help me for your name’s sake; out of the goodness of your love, deliver me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But deal well with me, O Sovereign Lord, for the sake of your own reputation! Rescue me because you are so faithful and good."
      }
    },
