    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华啊， 求你因自己的名恩待我， 因你的美善和慈爱拯救我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於你，主耶和華啊！求你為你名的緣故恩待我； 按著你美好的慈愛拯救我。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "可是你，主耶和华啊， 求你为你名的缘故恩待我； 因你的慈爱是美善的， 求你解救我，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主－耶和華啊，求你為你的名恩待我； 因你的慈愛美好，求你搭救我！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But you, O God my Lord, deal on my behalf for your name’s sake; because your steadfast love is good, deliver me!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But You, O God, the Lord, deal kindly with me for Your name’s sake; Because Your lovingkindness is good, deliver me;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you, Sovereign Lord, help me for your name’s sake; out of the goodness of your love, deliver me."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But You, O God the Lord, Deal with me for Your name’s sake; Because Your mercy is good, deliver me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But deal well with me, O Sovereign Lord, for the sake of your own reputation! Rescue me because you are so faithful and good."
      }
    },
