    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为祂保护贫穷的人， 拯救他们脱离置他们于死地的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他必站在貧窮人的右邊， 拯救他脫離定他死罪的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他必站在窮乏人的右邊， 要救他脫離審判他靈魂的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He stands at the right hand of the needy, To save him from those who judge his soul."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For he stands at the right hand of the needy, to save their lives from those who would condemn them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For he stands beside the needy, ready to save them from those who condemn them."
      }
    },
