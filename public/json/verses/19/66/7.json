    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂以大能永远掌权， 祂的眼目鉴察列国， 悖逆之徒不可在祂面前妄自尊大。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他以自己的權能永遠統管萬有， 他的眼睛鑒察萬國， 悖逆的人不可自高。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他用權能治理萬民，直到永遠。 他的眼睛鑒察列邦； 悖逆的人不可自高。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He rules by His might forever; His eyes keep watch on the nations; Let not the rebellious exalt themselves. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He rules forever by his power, his eyes watch the nations— let not the rebellious rise up against him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For by his great power he rules forever. He watches every movement of the nations; let no rebel rise in defiance. Interlude"
      }
    },
