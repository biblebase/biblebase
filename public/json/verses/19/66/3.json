    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要对上帝说：“你的作为令人敬畏！ 你的大能使敌人屈膝投降。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要對　神說：“你的作為多麼可畏！ 因你偉大的能力，你的仇敵必向你假意歸順。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當對神說：你的作為何等可畏！ 因你的大能，仇敵要投降你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Say to God, “How awesome are Your works! Because of the greatness of Your power Your enemies will give feigned obedience to You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Say to God, “How awesome are your deeds! So great is your power that your enemies cringe before you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Say to God, “How awesome are your deeds! Your enemies cringe before your mighty power."
      }
