    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.66.12",
            "paragraphs": [
              "六六12\"坐車軋我們的頭\"。形容敵人的勝利。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你让别人骑在我们的头上。 我们曾经历水火， 但你带我们到达丰盛之地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使人駕車軋我們的頭， 我們經過水火， 你卻把我們領到豐盛之地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你使人坐車軋我們的頭； 我們經過水火， 你卻使我們到豐富之地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You made men ride over our heads; We went through fire and through water, Yet You brought us out into a place of abundance."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You let people ride over our heads; we went through fire and water, but you brought us to a place of abundance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then you put a leader over us. We went through fire and flood, but you brought us to a place of great abundance."
      }
    },
