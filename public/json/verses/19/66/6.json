    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.66.6",
            "paragraphs": [
              "六六6提到從埃及地得釋放（出一四；一五），詩篇看為最奇妙的神蹟（一八15～19；六八7,8；七四13～15；七七16～20；七八13,52,53；八九7～10；一○六7～12；一三六10～15）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂将沧海变为干地， 让百姓步行经过。 让我们因祂的作为而欢欣吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把海變為乾地， 眾人就步行過河， 我們要在那裡因他歡喜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他將海變成乾地，眾民步行過河； 我們在那裏因他歡喜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He turned the sea into dry land; They passed through the river on foot; There let us rejoice in Him!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He turned the sea into dry land, they passed through the waters on foot— come, let us rejoice in him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He made a dry path through the Red Sea, and his people went across on foot. There we rejoiced in him."
      }
    },
