    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的心啊！要默默等候上帝， 因为我的盼望从祂而来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "唯獨等候　神，我的心才有安息； 因為我的盼望是從他而來的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的心哪，你當默默無聲，專等候神， 因為我的盼望是從他而來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My soul, wait in silence for God only, For my hope is from Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yes, my soul, find rest in God; my hope comes from him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let all that I am wait quietly before God, for my hope is in him."
      }
    },
