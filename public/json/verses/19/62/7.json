    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝是我的拯救者， 是我的荣耀， 祂是我的坚固磐石， 是我的避难所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的拯救，我的榮耀，都在於　神； 我堅固的磐石，我的避難所，都在於　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的拯救、我的榮耀都在乎神； 我力量的磐石、我的避難所都在乎神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On God my salvation and my glory rest; The rock of my strength, my refuge is in God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My salvation and my honor depend on God; he is my mighty rock, my refuge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My victory and honor come from God alone. He is my refuge, a rock where no enemy can reach me."
      }
