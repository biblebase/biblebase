    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，你降下沛雨， 滋润你干旱的产业——以色列，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！你降下大雨， 使你貧瘠的產業得到滋潤。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，你降下丰沛的大雨； 当你的继业疲倦时，是你坚立了他！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你降下大雨； 你產業以色列疲乏的時候，你使他堅固。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Rain in abundance, O God, you shed abroad; you restored your inheritance as it languished;"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shed abroad a plentiful rain, O God; You confirmed Your inheritance when it was parched."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You gave abundant showers, O God; you refreshed your weary inheritance."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You, O God, sent a plentiful rain, Whereby You confirmed Your inheritance, When it was weary."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You sent abundant rain, O God, to refresh the weary land."
      }
    },
