    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿你驱散他们， 如风把烟吹散。 愿恶人在上帝面前灭亡， 如蜡在火中熔化。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願你把他們趕散，如同煙被風吹散一樣； 願惡人在　神面前滅亡， 好像蠟在火前熔化。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们被神吹散， 如同烟被吹散； 恶人在神面前灭亡， 如同蜡在火面前熔化。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們被驅逐，如煙被風吹散； 惡人見神之面而消滅，如蠟被火鎔化。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "As smoke is driven away, so you shall drive them away; as wax melts before fire, so the wicked shall perish before God!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As smoke is driven away, so drive them away; As wax melts before the fire, So let the wicked perish before God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May you blow them away like smoke— as wax melts before the fire, may the wicked perish before God."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "As smoke is driven away, So drive them away; As wax melts before the fire, So let the wicked perish at the presence of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Blow them away like smoke. Melt them like wax in a fire. Let the wicked perish in the presence of God."
      }
    },
