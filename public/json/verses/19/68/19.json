    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.68.19",
            "paragraphs": [
              "68:19",
              "天天背負我們重擔的主：參55:22註。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.68.19",
            "paragraphs": [
              "六八19全以色列都有代表出席，從（南部的）小\"便雅憫\"和大\"猶大\"至北部的\"西布倫\"和\"拿弗他利\"。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要称颂主， 称颂我们的救主上帝， 祂天天背负我们的重担。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主，拯救我們的　神，是應當稱頌的， 他天天背負我們的重擔。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "天天背負我們重擔的主， 就是拯救我們的神， 是應當稱頌的！（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Blessed be the Lord, who daily bears our burden, The God who is our salvation. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Praise be to the Lord, to God our Savior, who daily bears our burdens."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Praise the Lord; praise God our savior! For each day he carries us in his arms. Interlude"
      }
    },
