    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你斥责那芦苇中的野兽， 斥责成群的公牛和列邦的牛犊， 使他们俯首献上贡银。 求你驱散好战的列邦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你斥責蘆葦中的野獸、 成群的公牛和萬民中的牛犢， 並且把貪愛銀塊的人踐踏在腳下； 求你趕散那些喜愛戰爭的民族。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "求你斥责芦苇中的野兽， 斥责壮牛群以及万民中的牛犊； 把贪爱银块的人践踏在脚下， 驱散那喜好战斗的民众。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你叱喝蘆葦中的野獸和羣公牛， 並列邦中的牛犢。 把銀塊踹在腳下； 神已經趕散好爭戰的列邦。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Rebuke the beasts that dwell among the reeds, the herd of bulls with the calves of the peoples. Trample underfoot those who lust after tribute; scatter the peoples who delight in war."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Rebuke the beasts in the reeds, The herd of bulls with the calves of the peoples, Trampling under foot the pieces of silver; He has scattered the peoples who delight in war."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Rebuke the beast among the reeds, the herd of bulls among the calves of the nations. Humbled, may the beast bring bars of silver. Scatter the nations who delight in war."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Rebuke the beasts of the reeds, The herd of bulls with the calves of the peoples, Till  everyone  submits himself with pieces of silver. Scatter the peoples who delight in war."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rebuke these enemy nations— these wild animals lurking in the reeds, this herd of bulls among the weaker calves. Make them bring bars of silver in humble tribute. Scatter the nations that delight in war."
      }
    },
