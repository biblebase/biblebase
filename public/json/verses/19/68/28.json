    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，求你施展你的权能； 上帝啊，求你像以前一样为我们彰显你的力量。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！召喚你的能力吧。 　神啊！堅立你為我們作成的事吧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列的能力是神所賜的； 神啊，求你堅固你為我們所成全的事！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your God has commanded your strength; Show Yourself strong, O God, who have acted on our behalf."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Summon your power, God; show us your strength, our God, as you have done before."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Summon your might, O God. Display your power, O God, as you have in the past."
      }
