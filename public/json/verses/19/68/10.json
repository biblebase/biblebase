    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "让你的子民得以安居在那里。 上帝啊， 你满怀恩慈地供养穷苦人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的子民住在其中； 　神啊，你滿有恩慈，為貧苦的人預備了一切。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的會眾住在其中； 神啊，你的恩惠是為困苦人預備的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your creatures settled in it; You provided in Your goodness for the poor, O God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your people settled in it, and from your bounty, God, you provided for the poor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There your people finally settled, and with a bountiful harvest, O God, you provided for your needy people."
      }
