    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.68.1",
            "paragraphs": [
              "68:1",
              "1~35願神興起：這是一首歡慶神戰勝仇敵、統管萬有，並眷顧以色列民的詩歌；它不單預表了基督要在末後敗壞那惡者，摧毀邪惡，而且預言了所有的信徒都將靠著基督誇勝，並要永遠與神同在，歡欣喜樂(啟19~21)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿上帝起来驱散祂的仇敌， 使恨祂的人四散奔逃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願　神興起，願他的仇敵四散； 願恨惡他的人從他面前逃跑。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願神興起，使他的仇敵四散， 叫那恨他的人從他面前逃跑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let God arise, let His enemies be scattered, And let those who hate Him flee before Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May God arise, may his enemies be scattered; may his foes flee before him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rise up, O God, and scatter your enemies. Let those who hate God run for their lives."
      }
    },
