    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.82.7",
            "paragraphs": [
              "八二7這些腐敗的審判者要\"死\"，象其它人一樣。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你们要跟世人一样死去， 像人间的王侯一样灭亡。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然而，你們要像世人一樣死亡， 像世上任何一位領袖一樣倒斃。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "然而，你們要死，與世人一樣， 要仆倒，像王子中的一位。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nevertheless you will die like men And fall like any one of the princes.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you will die like mere mortals; you will fall like every other ruler.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you will die like mere mortals and fall like every other ruler.’”"
      }
