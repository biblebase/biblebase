      "leili": {
        "content": [
          {
            "verse": "psa.44.17-22",
            "paragraphs": [
              "四四17～22為國民的忠心辯護，類似一些個人哀悼詩篇裏，詩人對自己無罪的辯解。參看第十七篇3節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你在豺狼出没的地方压碎我们， 使死亡的阴影笼罩我们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但你竟在野狗之地把我們壓傷了， 又以死亡的陰影籠罩我們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你在野狗之處壓傷我們， 用死蔭遮蔽我們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet You have crushed us in a place of jackals And covered us with the shadow of death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you crushed us and made us a haunt for jackals; you covered us over with deep darkness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet you have crushed us in the jackal’s desert home. You have covered us with darkness and death."
      }
