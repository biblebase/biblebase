    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们靠你击退敌人， 靠你的名践踏仇敌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們靠著你，必打倒我們的敵人； 靠著你的名，必踐踏那些起來攻擊我們的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "藉着你，我们冲垮我们的敌人； 奉你的名，我们践踏那些起来攻击我们的人；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們靠你要推倒我們的敵人， 靠你的名要踐踏那起來攻擊我們的人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Through you we push down our foes; through your name we tread down those who rise up against us."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Through You we will push back our adversaries; Through Your name we will trample down those who rise up against us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Through you we push back our enemies; through your name we trample our foes."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Through You we will push down our enemies; Through Your name we will trample those who rise up against us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Only by your power can we push back our enemies; only in your name can we trample our foes."
      }
    },
