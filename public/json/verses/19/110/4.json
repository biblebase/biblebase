    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.110.4",
            "paragraphs": [
              "110:4",
              "你……永遠為祭司：參來5:5~10；6:19~7:28中關於這句話如何應驗在基督身上的註釋。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华起了誓，永不反悔， 祂说：“你照麦基洗德的模式永远做祭司。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華起了誓，就決不反悔； 他說：“你永遠作祭司， 是照著麥基洗德的體系。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華起了誓，決不後悔，說： 你是照着麥基洗德的等次永遠為祭司。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord has sworn and will not change His mind, “You are a priest forever According to the order of Melchizedek.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord has sworn and will not change his mind: “You are a priest forever, in the order of Melchizedek.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord has taken an oath and will not break his vow: “You are a priest forever in the order of Melchizedek.”"
      }
    },
