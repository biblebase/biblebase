    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你跟仇敌作战的时候， 你的百姓必甘心跟从， 他们衣着圣洁， 如清晨的甘露。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在你征戰的日子，你的人民都樂意投身； 你的少年人以聖潔為裝飾， 好像清晨的甘露，到你那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當你掌權的日子， 你的民要以聖潔的妝飾為衣， 甘心犧牲自己； 你的民多如清晨的甘露。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your people will volunteer freely in the day of Your power; In holy array, from the womb of the dawn, Your youth are to You as the dew."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your troops will be willing on your day of battle. Arrayed in holy splendor, your young men will come to you like dew from the morning’s womb."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When you go to war, your people will serve you willingly. You are arrayed in holy garments, and your strength will be renewed each day like the morning dew."
      }
    },
