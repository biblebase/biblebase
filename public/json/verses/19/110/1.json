        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对我主说： “你坐在我的右边， 等我使你的仇敌成为你的脚凳。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對我主說： “你坐在我的右邊， 等我使你的仇敵作你的腳凳。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對我主說： 你坐在我的右邊， 等我使你仇敵作你的腳凳。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord says to my Lord: “Sit at My right hand Until I make Your enemies a footstool for Your feet.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord says to my lord: “Sit at my right hand until I make your enemies a footstool for your feet.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord said to my Lord, “Sit in the place of honor at my right hand until I humble your enemies, making them a footstool under your feet.”"
      }
