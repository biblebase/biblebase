    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.121.5-6",
            "paragraphs": [
              "一二一5,6耶和華在\"白日\"和\"夜間\"都\"蔭庇\"（保護）我們免受敵人的傷害。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
