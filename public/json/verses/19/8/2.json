    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.8.2-3",
            "paragraphs": [
              "八2,3神是萬有的主宰，從吃奶的嬰孩到天上的眾星都由祂管轄！"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使孩童和婴儿的口发出颂赞， 好叫你的仇敌哑口无言。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因你仇敵的緣故， 你從小孩和嬰兒的口中， 得著了讚美， 使仇敵和報仇的無話可說。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你因敵人的緣故， 從嬰孩和吃奶的口中， 建立了能力， 使仇敵和報仇的閉口無言。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the mouth of infants and nursing babes You have established strength Because of Your adversaries, To make the enemy and the revengeful cease."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Through the praise of children and infants you have established a stronghold against your enemies, to silence the foe and the avenger."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have taught children and infants to tell of your strength, silencing your enemies and all who oppose you."
      }
    },
