        "lang": "cht",
        "text": "耶和華啊！諸天要稱頌你的奇事； 在聖者的會中，你的信實也要被稱頌。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶和华啊，诸天要称颂你奇妙的事； 在圣者的会众里，也要称颂你的信实！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，諸天要稱讚你的奇事； 在聖者的會中，要稱讚你的信實。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Let the heavens praise your wonders, O Lord, your faithfulness in the assembly of the holy ones!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The heavens will praise Your wonders, O Lord; Your faithfulness also in the assembly of the holy ones."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The heavens praise your wonders, Lord, your faithfulness too, in the assembly of the holy ones."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the heavens will praise Your wonders, O Lord; Your faithfulness also in the assembly of the saints."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All heaven will praise your great wonders, Lord; myriads of angels will praise you for your faithfulness."
      }
