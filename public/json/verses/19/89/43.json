      "leili": {
        "content": [
          {
            "verse": "psa.89.38-45",
            "paragraphs": [
              "八九38～45這哀歌聲言，神由於大衛王朝的君王落敗蒙羞而摒棄這約。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使他的刀剑失去锋芒， 你使他败退沙场。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使他的刀劍捲刃， 使他在戰場上站立不住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你叫他的刀劍捲刃， 叫他在爭戰之中站立不住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You also turn back the edge of his sword And have not made him stand in battle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Indeed, you have turned back the edge of his sword and have not supported him in battle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have made his sword useless and refused to help him in battle."
      }
