    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 你的仇敌嘲笑你所膏立的王， 他们嘲笑他的一举一动。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你的仇敵用這羞辱羞辱了你的僕人， 羞辱了你受膏者的腳蹤。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你的仇敵用這羞辱羞辱了你的僕人， 羞辱了你受膏者的腳蹤。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "With which Your enemies have reproached, O Lord, With which they have reproached the footsteps of Your anointed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "the taunts with which your enemies, Lord, have mocked, with which they have mocked every step of your anointed one."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your enemies have mocked me, O Lord; they mock your anointed king wherever he goes."
      }
