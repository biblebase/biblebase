    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.89.5-14",
            "paragraphs": [
              "八九5～14詩人因神的屬性（5～8節）和祂的作為--尤其是在創世時的作為（10～14節）--而贊美祂。\"拉哈伯\"指埃及，如在第八十七篇4節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你击碎海怪， 以大能的臂膀驱散仇敌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你打碎了拉哈伯，好像已遭刺殺的人； 你用有力的膀臂，趕散了你的仇敵。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你打碎了拉哈伯，似乎是已殺的人； 你用有能的膀臂打散了你的仇敵。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You Yourself crushed Rahab like one who is slain; You scattered Your enemies with Your mighty arm."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You crushed Rahab like one of the slain; with your strong arm you scattered your enemies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You crushed the great sea monster. You scattered your enemies with your mighty arm."
      }
    },
