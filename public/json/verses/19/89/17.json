      "leili": {
        "content": [
          {
            "verse": "psa.89.17",
            "paragraphs": [
              "八九17\"我們的角\"。我們的力量。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你是他们的力量和荣耀， 你的恩惠使我们充满力量。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你是他們的榮耀和力量； 因你的恩惠，我們的角得以高舉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你是他們力量的榮耀； 因為你喜悅我們，我們的角必被高舉。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You are the glory of their strength, And by Your favor our horn is exalted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For you are their glory and strength, and by your favor you exalt our horn."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You are their glorious strength. It pleases you to make us strong."
      }
