    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.89.5-7",
            "paragraphs": [
              "八九5～7\"聖者……神的眾子……聖者\"。指天使。參看第二十九章1節的腳註。"
            ]
          },
          {
            "verse": "psa.89.5-14",
            "paragraphs": [
              "八九5～14詩人因神的屬性（5～8節）和祂的作為--尤其是在創世時的作為（10～14節）--而贊美祂。\"拉哈伯\"指埃及，如在第八十七篇4節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天上有谁能与耶和华相比， 众天使中谁能像祂？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在天上，誰能和耶和華相比呢？ 在全能者的眾子中，誰能像耶和華呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在天空誰能比耶和華呢？ 神的眾子中，誰能像耶和華呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For who in the skies is comparable to the Lord? Who among the sons of the mighty is like the Lord,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For who in the skies above can compare with the Lord? Who is like the Lord among the heavenly beings?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For who in all of heaven can compare with the Lord? What mightiest angel is anything like the Lord?"
      }
