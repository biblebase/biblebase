    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.89.5-14",
            "paragraphs": [
              "八九5～14詩人因神的屬性（5～8節）和祂的作為--尤其是在創世時的作為（10～14節）--而贊美祂。\"拉哈伯\"指埃及，如在第八十七篇4節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的宝座以公平和正义为根基， 你以慈爱和信实为先锋。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "公義和公正是你寶座的基礎， 慈愛和信實行在你前面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "公義和公平是你寶座的根基； 慈愛和誠實行在你前面。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Righteousness and justice are the foundation of Your throne; Lovingkindness and truth go before You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Righteousness and justice are the foundation of your throne; love and faithfulness go before you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Righteousness and justice are the foundation of your throne. Unfailing love and truth walk before you as attendants."
      }
    },
