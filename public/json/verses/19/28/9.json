      "leili": {
        "content": [
          {
            "verse": "psa.28.8-9",
            "paragraphs": [
              "二八8,9耶和華提供象堡壘一樣的\"力量\"，象牧人一樣的愛顧。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你拯救你的子民， 赐福给你拣选的人， 如牧人般照顾他们， 永远扶持他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你拯救你的子民， 賜福給你的產業， 牧養他們，懷抱他們，直到永遠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你拯救你的百姓，賜福給你的產業， 牧養他們，扶持他們，直到永遠。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Save Your people and bless Your inheritance; Be their shepherd also, and carry them forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Save your people and bless your inheritance; be their shepherd and carry them forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Save your people! Bless Israel, your special possession. Lead them like a shepherd, and carry them in your arms forever."
      }
