    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "想起自己夜间所唱的歌。 我沉思默想，扪心自问："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我想起我夜間的詩歌。 我的心沉思默想，我的靈仔細探究。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我想起我夜間的歌曲，捫心自問； 我心裏也仔細省察。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will remember my song in the night; I will meditate with my heart, And my spirit ponders:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I remembered my songs in the night. My heart meditated and my spirit asked:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "when my nights were filled with joyful songs. I search my soul and ponder the difference now."
      }
    },
