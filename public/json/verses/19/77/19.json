      "leili": {
        "content": [
          {
            "verse": "psa.77.16-20",
            "paragraphs": [
              "七七16～20描述神在拯救以色列人離開埃及時所彰顯的能力。參看第六十六篇6節的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的道路穿越海洋，经过洪涛， 但你的足迹无人看见。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的道路經過海洋， 你的路徑穿過大水， 但你的腳蹤無人知道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的道在海中； 你的路在大水中； 你的腳蹤無人知道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your way was in the sea And Your paths in the mighty waters, And Your footprints may not be known."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your path led through the sea, your way through the mighty waters, though your footprints were not seen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your road led through the sea, your pathway through the mighty waters— a pathway no one knew was there!"
      }
