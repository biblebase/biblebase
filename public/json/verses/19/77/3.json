    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我思想上帝，发出哀叹； 我默想，心灵疲惫不堪。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我想到　神，就哀怨唉哼； 我沉思默想，心靈就煩亂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我想念神，就煩燥不安； 我沉吟悲傷，心便發昏。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When I remember God, then I am disturbed; When I sigh, then my spirit grows faint. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I remembered you, God, and I groaned; I meditated, and my spirit grew faint."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I think of God, and I moan, overwhelmed with longing for his help. Interlude"
      }
    },
