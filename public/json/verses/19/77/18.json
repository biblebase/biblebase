    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "旋风中传来你的雷声， 你的闪电照亮世界， 大地颤抖震动。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的雷聲在旋風中響起來， 閃電照亮了世界， 大地戰抖震動。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的雷聲在旋風中； 電光照亮世界； 大地戰抖震動。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sound of Your thunder was in the whirlwind; The lightnings lit up the world; The earth trembled and shook."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your thunder was heard in the whirlwind, your lightning lit up the world; the earth trembled and quaked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your thunder roared from the whirlwind; the lightning lit up the world! The earth trembled and shook."
      }
    },
