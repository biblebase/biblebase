    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是我说：“我感到悲伤的是， 至高者已不再彰显大能。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此我說：“這是我的憂傷： 至高者的右手已經改變了！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我便說：這是我的懦弱， 但我要追念至高者顯出右手之年代。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then I said, “It is my grief, That the right hand of the Most High has changed.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then I thought, “To this I will appeal: the years when the Most High stretched out his right hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I said, “This is my fate; the Most High has turned his hand against me.”"
      }
    },
