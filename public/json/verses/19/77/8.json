    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道祂的慈爱永远消逝了吗？ 祂的应许永远落空了吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的慈愛永遠消失， 他的應許永久廢去嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "難道他的慈愛永遠窮盡， 他的應許世世廢棄嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Has His lovingkindness ceased forever? Has His promise come to an end forever?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Has his unfailing love vanished forever? Has his promise failed for all time?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Is his unfailing love gone forever? Have his promises permanently failed?"
      }
    },
