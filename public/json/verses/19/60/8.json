      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩押是我的沐浴盆； 我要向以東拋鞋。 非利士啊，你還能因我歡呼嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moab is My washbowl; Over Edom I shall throw My shoe; Shout loud, O Philistia, because of Me!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moab is my washbasin, on Edom I toss my sandal; over Philistia I shout in triumph.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Moab, my washbasin, will become my servant, and I will wipe my feet on Edom and shout in triumph over Philistia.”"
      }
    },
