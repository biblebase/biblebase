    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.95.8",
            "paragraphs": [
              "九五8\"在米利巴\"。參看出埃及記十七章2節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可像你们祖先在米利巴， 在旷野的玛撒那样心里顽固。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就不要硬著心，好像在米利巴， 就是在曠野的瑪撒的日子一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們不可硬着心，像當日在米利巴， 就是在曠野的瑪撒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not harden your hearts, as at Meribah, As in the day of Massah in the wilderness,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do not harden your hearts as you did at Meribah, as you did that day at Massah in the wilderness,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord says, “Don’t harden your hearts as Israel did at Meribah, as they did at Massah in the wilderness."
      }
    },
