    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.141.6-7",
            "paragraphs": [
              "一四一6,7直到不敬神之人的領袖（審判官）受審判，他們的\"骨頭散在墓旁\"，他們才聽大衛的話。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些恶人的骨头散落在墓旁， 好像耕田翻起的土块。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們說：“我們的骨頭被拋散在墓旁， 好像人犁田鋤地時掘起的土塊。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們的骨頭散在墓旁， 好像人耕田、刨地的土塊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As when one plows and breaks open the earth, Our bones have been scattered at the mouth of Sheol."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will say, “As one plows and breaks up the earth, so our bones have been scattered at the mouth of the grave.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Like rocks brought up by a plow, the bones of the wicked will lie scattered without burial."
      }
    },
