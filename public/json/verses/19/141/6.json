    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.141.6-7",
            "paragraphs": [
              "一四一6,7直到不敬神之人的領袖（審判官）受審判，他們的\"骨頭散在墓旁\"，他們才聽大衛的話。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的首领被抛下悬崖的时候， 他们就会知道我所言不虚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的官長被摔在山崖下的時候， 他們就要聽我的話，因為我的話甘美。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的審判官被扔在巖下； 眾人要聽我的話，因為這話甘甜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their judges are thrown down by the sides of the rock, And they hear my words, for they are pleasant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their rulers will be thrown down from the cliffs, and the wicked will learn that my words were well spoken."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When their leaders are thrown down from a cliff, the wicked will listen to my words and find them true."
      }
    },
