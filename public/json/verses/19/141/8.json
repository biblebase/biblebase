    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华啊， 我仰望你，我投靠你， 求你不要让我遭害。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶和華啊！我的眼睛仰望你； 我投靠你，求你不要使我喪命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主－耶和華啊，我的眼目仰望你； 我投靠你，求你不要將我撇得孤苦！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For my eyes are toward You, O God, the Lord; In You I take refuge; do not leave me defenseless."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But my eyes are fixed on you, Sovereign Lord; in you I take refuge—do not give me over to death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I look to you for help, O Sovereign Lord. You are my refuge; don’t let them kill me."
      }
