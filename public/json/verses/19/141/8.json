      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主－耶和華啊，我的眼目仰望你； 我投靠你，求你不要將我撇得孤苦！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For my eyes are toward You, O God, the Lord; In You I take refuge; do not leave me defenseless."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But my eyes are fixed on you, Sovereign Lord; in you I take refuge—do not give me over to death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I look to you for help, O Sovereign Lord. You are my refuge; don’t let them kill me."
      }
    },
