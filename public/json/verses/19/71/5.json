    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主耶和华啊，你是我的盼望， 我从小就信靠你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你是我的盼望； 主耶和華啊！你是我自幼以來所倚靠的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主－耶和華啊，你是我所盼望的； 從我年幼，你是我所倚靠的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You are my hope; O Lord God, You are my confidence from my youth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For you have been my hope, Sovereign Lord, my confidence since my youth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, you alone are my hope. I’ve trusted you, O Lord, from childhood."
      }
    },
