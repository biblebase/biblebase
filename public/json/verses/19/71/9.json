    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.71.9",
            "paragraphs": [
              "71:9",
              "我年老的時候：當我們力量衰微、年老體弱的時候，應當記念神在往昔如何信實地引領我們，並要認定他做我們的保護和幫助，堅信他必撫養我們到底。當死亡來臨，我們體力衰殘時，我們應當知道神必不離棄我們，卻要親近我們(12,18節)，他的聖天使也必要帶我們回到天家，永遠與他同在(路16:22)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我年老时，求你不要丢弃我； 我体力衰微时，求你不要离弃我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我年老的時候，求你不要丟棄我； 我氣力衰弱的時候，求你不要離棄我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我年老的時候，求你不要丟棄我！ 我力氣衰弱的時候，求你不要離棄我！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not cast me off in the time of old age; Do not forsake me when my strength fails."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not cast me away when I am old; do not forsake me when my strength is gone."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And now, in my old age, don’t set me aside. Don’t abandon me when my strength is failing."
      }
    },
