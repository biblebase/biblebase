      "leili": {
        "content": [
          {
            "verse": "psa.71.17-18",
            "paragraphs": [
              "七一17,18年老信徒的一個重要事奉是把神奇妙的作為向年輕的下一代述說。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 我从小就受你的教导， 直到今日我仍传扬你奇妙的作为。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！我自幼以來，你就教導我； 直到現在，我還是宣揚你奇妙的作為。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，自我年幼時，你就教訓我； 直到如今，我傳揚你奇妙的作為。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O God, You have taught me from my youth, And I still declare Your wondrous deeds."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Since my youth, God, you have taught me, and to this day I declare your marvelous deeds."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O God, you have taught me from my earliest childhood, and I constantly tell others about the wonderful things you do."
      }
