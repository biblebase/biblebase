    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使我经历了许多患难， 最终必使我重获新生， 救我脱离死亡的深渊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你使我們經歷了很多苦難， 你必使我們再活過來， 你必把我們從地的深處救上來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你是叫我們多經歷重大急難的， 必使我們復活， 從地的深處救上來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You who have shown me many troubles and distresses Will revive me again, And will bring me up again from the depths of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Though you have made me see troubles, many and bitter, you will restore my life again; from the depths of the earth you will again bring me up."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have allowed me to suffer much hardship, but you will restore me to life again and lift me up from the depths of the earth."
      }
