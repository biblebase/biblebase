    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使我出了母胎， 我从出生就依靠你， 我要永远赞美你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我自出母胎就倚賴你， 把我從母腹中領出來的就是你， 我要常常讚美你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我從出母胎被你扶持； 使我出母腹的是你。 我必常常讚美你！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By You I have been sustained from my birth; You are He who took me from my mother’s womb; My praise is continually of You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From birth I have relied on you; you brought me forth from my mother’s womb. I will ever praise you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, you have been with me from birth; from my mother’s womb you have cared for me. No wonder I am always praising you!"
      }
