      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿旷野的居民都来向他下拜， 敌人都扑倒在尘土中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "住在曠野的人必向他屈身， 他的仇敵必舔塵土。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住在曠野的，必在他面前下拜； 他的仇敵必要舔土。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let the nomads of the desert bow before him, And his enemies lick the dust."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the desert tribes bow before him and his enemies lick the dust."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Desert nomads will bow before him; his enemies will fall before him in the dust."
      }
    },
