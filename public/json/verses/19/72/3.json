    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.72.2-11",
            "paragraphs": [
              "七二2～11可以譯成一個一個的禱告（也見於15～17節）：\"願他按公義……\"，\"願大山小山……\"。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿大山给百姓带来繁荣， 小山带来公义。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因著公義， 願大山和小山都給人民帶來和平。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大山小山都要因公義使民得享平安。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let the mountains bring peace to the people, And the hills, in righteousness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the mountains bring prosperity to the people, the hills the fruit of righteousness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May the mountains yield prosperity for all, and may the hills be fruitful."
      }
