        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "太陽還存，月亮還在， 人要敬畏你，直到萬代！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let them fear You while the sun endures, And as long as the moon, throughout all generations."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May he endure as long as the sun, as long as the moon, through all generations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May they fear you as long as the sun shines, as long as the moon remains in the sky. Yes, forever!"
      }
