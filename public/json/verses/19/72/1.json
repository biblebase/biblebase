    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.72.1",
            "paragraphs": [
              "72:1",
              "1~19王：根據題註可知，本詩是所羅門做以色列王時的一篇禱告，他求神使他的國度滿有公平、公義、平安，並且懲治邪惡，救拔貧窮受壓的人。因為詩中多處經節都預表了耶穌基督(8,11,17節；比較賽11:1~5；60~62章)，所以這首詩也表述了基督做王、統管世界的美好景象(參啟20~22章)。這篇禱告類似於新約聖經所教導我們祈禱：“願你的國降臨。願你的旨意行在地上，如同行在天上”(太6:10)。凡是盼望基督做王掌權、切慕他在地上建立公義的信徒都應當常常用心靈獻上這樣的禱告(參啟21:1註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 求你将你的公正赐给王， 将你的公义赐给王的儿子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！求你把你的公正賜給王， 把你的公義賜給王子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，求你將判斷的權柄賜給王， 將公義賜給王的兒子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Give the king Your judgments, O God, And Your righteousness to the king’s son."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Endow the king with your justice, O God, the royal son with your righteousness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give your love of justice to the king, O God, and righteousness to the king’s son."
      }
    },
