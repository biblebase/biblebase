    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在他统治之下， 愿义人兴旺，国富民强， 如月长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他在世的日子，義人必興旺， 四境太平，直到月亮不再重現。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在他的日子，義人要發旺， 大有平安，好像月亮長存。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In his days may the righteous flourish, And abundance of peace till the moon is no more."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In his days may the righteous flourish and prosperity abound till the moon is no more."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May all the godly flourish during his reign. May there be abundant prosperity until the moon is no more."
      }
