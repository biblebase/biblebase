    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.135.16-18",
            "paragraphs": [
              "一三五16～18偶像不能說話、不能看、不能聽、不能呼吸、也不能拯救人。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些制造它们、信靠它们的人也会和它们一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "做偶像的必和它們一樣， 所有倚靠它們的也必這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "造他的要和他一樣， 凡靠他的也要如此。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Those who make them will be like them, Yes, everyone who trusts in them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who make them will be like them, and so will all who trust in them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And those who make idols are just like them, as are all who trust in them."
      }
    },
