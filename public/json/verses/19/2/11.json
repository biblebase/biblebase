    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.2.10-12",
            "paragraphs": [
              "召集世上君王（二10～12）",
              "根據以上經文，序言中背叛的諸王，現在只有一條出路，就是投降。但此處所提出的，不是最後通牒，乃是一項邀請；最後一行中，恩典溢於言辭。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要以敬畏的心事奉耶和华， 要喜乐也要战战兢兢。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要以敬畏的態度事奉耶和華， 又應當存戰兢的心而歡呼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當存畏懼事奉耶和華， 又當存戰兢而快樂。"
      },
