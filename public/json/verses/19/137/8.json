    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.137.8-9",
            "paragraphs": [
              "一三七8,9報仇法之嚴厲的條款要求巴比倫人承受他們對待耶路撒冷那樣的痛苦（包括殺孩童）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴比伦城啊， 你快要灭亡了， 那向你以牙还牙为我们复仇的人有福了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "將要被毀滅的巴比倫城啊！ 照著你待我們的行為報復你的， 那人有福了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將要被滅的巴比倫城啊， 報復你像你待我們的，那人便為有福！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O daughter of Babylon, you devastated one, How blessed will be the one who repays you With the recompense with which you have repaid us."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Daughter Babylon, doomed to destruction, happy is the one who repays you according to what you have done to us."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Babylon, you will be destroyed. Happy is the one who pays you back for what you have done to us."
      }
    },
