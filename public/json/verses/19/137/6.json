    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.137.6",
            "paragraphs": [
              "一三七6\"不看……過於……\"。不高舉。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若我忘了你， 不以你为我的至爱， 情愿我的舌头不能再歌唱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我不記念你， 如果我不高舉耶路撒冷 超過我最大的喜樂， 情願我的舌頭緊貼上膛。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "如果我不记念你， 如果我不将耶路撒冷置于我最高的喜乐之上， 情愿我的舌头紧贴着上颚！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若不記念你， 若不看耶路撒冷過於我所最喜樂的， 情願我的舌頭貼於上膛！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Let my tongue stick to the roof of my mouth, if I do not remember you, if I do not set Jerusalem above my highest joy!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May my tongue cling to the roof of my mouth If I do not remember you, If I do not exalt Jerusalem Above my chief joy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May my tongue cling to the roof of my mouth if I do not remember you, if I do not consider Jerusalem my highest joy."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "If I do not remember you, Let my tongue cling to the roof of my mouth— If I do not exalt Jerusalem Above my chief joy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "May my tongue stick to the roof of my mouth if I fail to remember you, if I don’t make Jerusalem my greatest joy."
      }
    },
