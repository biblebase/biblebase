    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.48.14",
            "paragraphs": [
              "48:14",
              "作我們引路的，直到死時：主應許要照著他的信實，盡心盡力、恒久如一地引領我們信徒度過此生、歷經死亡、步入我們永恒的天家，與他永遠在一起(約14:1~3；帖前4:17；啟21:3；參腓1:21註；參“死”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为上帝永永远远是我们的上帝， 祂必引领我们， 直到我们离世之日。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為這位　神就是我們的　神， 直到永永遠遠； 他必引導我們，直到我們死的時候。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為這神永永遠遠為我們的神； 他必作我們引路的，直到死時。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For such is God, Our God forever and ever; He will guide us until death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this God is our God for ever and ever; he will be our guide even to the end."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For that is what God is like. He is our God forever and ever, and he will guide us until we die."
      }
    },
