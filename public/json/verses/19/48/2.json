    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.48.2",
            "paragraphs": [
              "四八2\"在北面\"。異教徒認為北方是諸神的住所。參看以賽亞書十四章13、14節。因此，這裏斷言耶路撒冷是真神執政統治的地方。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "北面的锡安山雄伟壮丽， 令世人欢喜， 是伟大君王的城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在北面的錫安山、大君王的城， 美麗高聳，是全地所喜歡的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "錫安山－大君王的城， 在北面居高華美， 為全地所喜悅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Beautiful in elevation, the joy of the whole earth, Is Mount Zion in the far north, The city of the great King."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Beautiful in its loftiness, the joy of the whole earth, like the heights of Zaphon is Mount Zion, the city of the Great King."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is high and magnificent; the whole earth rejoices to see it! Mount Zion, the holy mountain, is the city of the great King!"
      }
    },
