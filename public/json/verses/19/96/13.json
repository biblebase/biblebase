    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.96.11-13",
            "paragraphs": [
              "九六11～13到了千禧年，咒詛得以逆轉的時候，天地萬物都要興旺。參看羅馬書八章20節的腳註。"
            ]
          },
          {
            "verse": "psa.96.13",
            "paragraphs": [
              "九六13\"審判\"。有統治的意思。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为祂要来审判大地， 祂要按公义审判世界， 凭祂的信实审判万民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "它們都要在耶和華面前歡呼，因為他來了； 他來要審判全地。 他要按著公義審判世界， 憑著他的信實審判萬民。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他來了，他來要審判全地。 他要按公義審判世界， 按他的信實審判萬民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Before the Lord, for He is coming, For He is coming to judge the earth. He will judge the world in righteousness And the peoples in His faithfulness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let all creation rejoice before the Lord, for he comes, he comes to judge the earth. He will judge the world in righteousness and the peoples in his faithfulness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "before the Lord, for he is coming! He is coming to judge the earth. He will judge the world with justice, and the nations with his truth."
      }
    },
