    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.63.6-8",
            "paragraphs": [
              "六三6～8大衛徹夜默想神，在祂的\"翅膀\"下找到保護，得到祂\"右手\"有力的扶持。參看第五十七篇1節和六十篇5節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你是我的帮助， 我在你翅膀的荫庇下欢歌。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你幫助了我， 我要在你翅膀的蔭下歡呼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你曾幫助我， 我就在你翅膀的蔭下歡呼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For You have been my help, And in the shadow of Your wings I sing for joy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Because you are my help, I sing in the shadow of your wings."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because you are my helper, I sing for joy in the shadow of your wings."
      }
