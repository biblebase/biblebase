    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.113.6-9",
            "paragraphs": [
              "一一三6～9神的恩慈可見於兩個例子，就是祂關懷\"貧寒人\"（7節）和\"不能生育的婦人\"（9節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂赐给不育的妇人儿女， 使她们成为快乐的母亲。 你们要赞美耶和华！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他使不能生育的婦人安居家中， 成了有許多兒女的快樂母親。 你們要讚美耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他使不能生育的婦人安居家中， 為多子的樂母。 你們要讚美耶和華！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He makes the barren woman abide in the house As a joyful mother of children. Praise the Lord!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He settles the childless woman in her home as a happy mother of children. Praise the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He gives the childless woman a family, making her a happy mother. Praise the Lord!"
      }
    },
