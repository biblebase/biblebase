    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.40.1-10",
            "paragraphs": [
              "等候得報償（四十1～10）",
              "大衛的“新歌”得到廣泛的回響，只要經歷過神的拯救──不論是從疾病、罪惡，或任何困境中出來，都可以同聲應和。我們對他“禍坑”的細節一無所知，正像對保羅“身上的刺”一般，為此反而更可感恩。"
            ]
          },
          {
            "verse": "psa.40.1-3",
            "paragraphs": [
              "1～3.拯救。耐性一字對這句開頭語顯得過分安靜，NEB的譯文表達得更好：“我等啊，等耶和華。”衪垂聽我的呼求，也嫌太優雅；可以譯為“祂彎下腰來就近我”（NEB），或“祂轉過來聽我的呼求”（摩法特）──就像一個人突然被某件事牢牢吸引。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我曾耐心等候耶和华， 祂倾听了我的呼求。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我曾切切等候耶和華； 他轉向我，聽了我的呼求。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我曾耐性等候耶和華； 他垂聽我的呼求。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I waited patiently for the Lord; And He inclined to me and heard my cry."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I waited patiently for the Lord; he turned to me and heard my cry."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I waited patiently for the Lord to help me, and he turned to me and heard my cry."
      }
    },
