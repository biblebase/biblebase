    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.40.1-10",
            "paragraphs": [
              "等候得報償（四十1～10）",
              "大衛的“新歌”得到廣泛的回響，只要經歷過神的拯救──不論是從疾病、罪惡，或任何困境中出來，都可以同聲應和。我們對他“禍坑”的細節一無所知，正像對保羅“身上的刺”一般，為此反而更可感恩。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你知道我在大会中宣扬你的公义， 没有闭口不言。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要在大會中傳揚公義的福音； 我必不禁止我的嘴唇； 耶和華啊！這是你知道的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我在大會中宣傳公義的佳音； 我必不止住我的嘴唇。 耶和華啊，這是你所知道的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have proclaimed glad tidings of righteousness in the great congregation; Behold, I will not restrain my lips, O Lord, You know."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I proclaim your saving acts in the great assembly; I do not seal my lips, Lord, as you know."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have told all your people about your justice. I have not been afraid to speak out, as you, O Lord, well know."
      }
    },
