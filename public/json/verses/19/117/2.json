    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.117.2",
            "paragraphs": [
              "一一七2\"大施慈愛\"。堅定、忠實的愛。參看何西阿書二章19節的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为祂对我们的慈爱是何等深厚， 祂的信实永远长存。 你们要赞美耶和华！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他向我們所施的慈愛極大； 耶和華的信實永遠長存。 你們要讚美耶和華。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "因为他的慈爱对我们何等大， 耶和华的信实存到永远！ 哈利路亚！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他向我們大施慈愛； 耶和華的誠實存到永遠。 你們要讚美耶和華！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For great is his steadfast love toward us, and the faithfulness of the Lord endures forever. Praise the Lord!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For His lovingkindness is great toward us, And the truth of the Lord is everlasting. Praise the Lord!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For great is his love toward us, and the faithfulness of the Lord endures forever. Praise the Lord."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For His merciful kindness is great toward us, And the truth of the Lord  endures forever. Praise the Lord!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For his unfailing love for us is powerful; the Lord’s faithfulness endures forever. Praise the Lord!"
      }
    },
