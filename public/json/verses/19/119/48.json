    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我尊崇你的命令， 我喜爱你的命令， 我要默想你的律例。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我又要向你的誡命舉手，這些誡命是我所愛的； 我也要默想你的律例。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我又要遵行你的命令， 這命令素來是我所愛的； 我也要思想你的律例。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And I shall lift up my hands to Your commandments, Which I love; And I will meditate on Your statutes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I reach out for your commands, which I love, that I may meditate on your decrees."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I honor and love your commands. I meditate on your decrees."
      }
