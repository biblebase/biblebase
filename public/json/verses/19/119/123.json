    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.121-128",
            "paragraphs": [
              "一一九121～128祈求神拯救他脫離壓迫他的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我望眼欲穿地期盼你拯救我， 实现你公义的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因盼望你的救恩和你公義的應許， 我的眼睛都昏花了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因盼望你的救恩 和你公義的話眼睛失明。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My eyes fail with longing for Your salvation And for Your righteous word."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My eyes fail, looking for your salvation, looking for your righteous promise."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My eyes strain to see your rescue, to see the truth of your promise fulfilled."
      }
