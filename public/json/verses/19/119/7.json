    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.1-8",
            "paragraphs": [
              "一一九1～8由於遵行耶和華律法的人都是蒙福的，所以詩人祈求神使他更加順服（5節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我学习你公义的法令时， 要存着正直的心来称谢你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我學會了你公義的法則， 就以正直的心稱謝你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我學了你公義的判語， 就要以正直的心稱謝你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I shall give thanks to You with uprightness of heart, When I learn Your righteous judgments."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will praise you with an upright heart as I learn your righteous laws."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As I learn your righteous regulations, I will thank you by living as I should!"
      }
