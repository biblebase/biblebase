      "leili": {
        "content": [
          {
            "verse": "psa.119.145-152",
            "paragraphs": [
              "一一九145～152詩人呼求神的拯救，因為他遵守神的道。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
