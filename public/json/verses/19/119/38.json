      "leili": {
        "content": [
          {
            "verse": "psa.119.33-40",
            "paragraphs": [
              "一一九33～40詩人渴望得到神之律法的引導。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你实现你给仆人的应许， 就是你给敬畏你之人的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你向你的僕人實踐你的諾言， 就是你向敬畏你的人所應許的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你向敬畏你的人所應許的話， 求你向僕人堅定！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Establish Your word to Your servant, As that which produces reverence for You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Fulfill your promise to your servant, so that you may be feared."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Reassure me of your promise, made to those who fear you."
      }
    },
