      "leili": {
        "content": [
          {
            "verse": "psa.119.57-64",
            "paragraphs": [
              "一一九57～64這段表達的思想是：我們不認識神的話，就不能經歷祂。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我与所有敬畏你、遵守你法则的人为友。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是敬畏你，謹守你訓詞的， 我都與他們為友。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡敬畏你、守你訓詞的人， 我都與他作伴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am a companion of all those who fear You, And of those who keep Your precepts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am a friend to all who fear you, to all who follow your precepts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am a friend to anyone who fears you— anyone who obeys your commandments."
      }
    },
