    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.145-152",
            "paragraphs": [
              "一一九145～152詩人呼求神的拯救，因為他遵守神的道。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你充满慈爱， 求你垂听我的呼求， 照你的法令保护我的性命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你照著你的慈愛垂聽我的聲音， 求你按著你的公正使我存活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你照你的慈愛聽我的聲音； 耶和華啊，求你照你的典章將我救活！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Hear my voice according to Your lovingkindness; Revive me, O Lord, according to Your ordinances."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hear my voice in accordance with your love; preserve my life, Lord, according to your laws."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In your faithful love, O Lord, hear my cry; let me be revived by following your regulations."
      }
    },
