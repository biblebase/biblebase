        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Remember your word to your servant, for you have given me hope."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
