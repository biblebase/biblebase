    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.129-136",
            "paragraphs": [
              "一一九129～136祈求神把他建立在祂的話語上。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你照你的应许引导我的脚步， 不要让罪恶辖制我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你用你的話引導我的腳步， 不容甚麼罪孽轄制我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你用你的話使我腳步穩當， 不許甚麼罪孽轄制我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Establish my footsteps in Your word, And do not let any iniquity have dominion over me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Direct my footsteps according to your word; let no sin rule over me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Guide my steps by your word, so I will not be overcome by evil."
      }
    },
