    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.161-168",
            "paragraphs": [
              "一一九161～168詩人因神的話而心生敬畏、喜樂、愛慕和贊美。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "权贵无故迫害我， 但我的心对你的话充满敬畏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "領袖們無緣無故迫害我， 我的心卻畏懼你的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "首領無故地逼迫我， 但我的心畏懼你的言語。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Princes persecute me without cause, But my heart stands in awe of Your words."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Rulers persecute me without cause, but my heart trembles at your word."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Powerful people harass me without cause, but my heart trembles only at your word."
      }
