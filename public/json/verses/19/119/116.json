    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.113-120",
            "paragraphs": [
              "一一九113～120詩人懼怕神的審判。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
