    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你按你的应许扶持我， 使我存活， 不要使我的盼望落空。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你照著你的應許扶持我，使我存活； 不要使我因仰望你而蒙羞。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你照你的話扶持我，使我存活， 也不叫我因失望而害羞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Sustain me according to Your word, that I may live; And do not let me be ashamed of my hope."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Sustain me, my God, according to your promise, and I will live; do not let my hopes be dashed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, sustain me as you promised, that I may live! Do not let my hope be crushed."
      }
    },
