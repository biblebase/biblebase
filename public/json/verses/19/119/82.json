    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我期盼你的应许实现， 望眼欲穿。 我说：“你何时才安慰我？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我仰望你的應許以致眼睛昏花， 我說：“你甚麼時候才安慰我呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因盼望你的應許眼睛失明，說： 你何時安慰我？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My eyes fail with longing for Your word, While I say, “When will You comfort me?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My eyes fail, looking for your promise; I say, “When will you comfort me?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My eyes are straining to see your promises come true. When will you comfort me?"
      }
