    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.119.67",
            "paragraphs": [
              "119:67",
              "受苦……現在卻遵守你的話：神有時允許我們經歷患難困苦，乃是為了吸引我們更親近他的話(比較94:12；箴3:11~12；參來12:5註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.119.65-72",
            "paragraphs": [
              "一一九65～72神的善有時包括管教。\"心蒙脂油\"（70節）。即冷淡和不能接受屬靈的真理。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从前我没有受苦的时候走迷了路， 现在我要遵行你的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我受苦以先，犯了錯誤； 現在我謹守你的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我未受苦以先走迷了路， 現在卻遵守你的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Before I was afflicted I went astray, But now I keep Your word."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Before I was afflicted I went astray, but now I obey your word."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I used to wander off until you disciplined me; but now I closely follow your word."
      }
    },
