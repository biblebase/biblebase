    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.119.2",
            "paragraphs": [
              "119:2",
              "法度：神的法度(希伯來文edot)代表神立約的條款或照他旨意所發布的要求。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.119.1-8",
            "paragraphs": [
              "一一九1～8由於遵行耶和華律法的人都是蒙福的，所以詩人祈求神使他更加順服（5節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "遵守祂的法度、全心寻求祂的人有福了！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "謹守他的法度， 全心尋求他的，都是有福的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "遵守他的法度、一心尋求他的， 這人便為有福！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How blessed are those who observe His testimonies, Who seek Him with all their heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Blessed are those who keep his statutes and seek him with all their heart—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Joyful are those who obey his laws and search for him with all their hearts."
      }
