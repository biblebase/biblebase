      "leili": {
        "content": [
          {
            "verse": "psa.119.121-128",
            "paragraphs": [
              "一一九121～128祈求神拯救他脫離壓迫他的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
