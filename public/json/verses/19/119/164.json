    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.161-168",
            "paragraphs": [
              "一一九161～168詩人因神的話而心生敬畏、喜樂、愛慕和贊美。"
            ]
          },
          {
            "verse": "psa.119.164",
            "paragraphs": [
              "一一九164\"七次\"。比第五十五篇17節的三次更多。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因你公义的法令， 我要每天七次赞美你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我因你公義的典章， 一天七次讚美你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我因你公義的典章一天七次讚美你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Seven times a day I praise You, Because of Your righteous ordinances."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Seven times a day I praise you for your righteous laws."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will praise you seven times a day because all your regulations are just."
      }
    },
