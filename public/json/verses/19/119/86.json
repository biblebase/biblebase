      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的命令盡都誠實； 他們無理地逼迫我，求你幫助我！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All Your commandments are faithful; They have persecuted me with a lie; help me!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All your commands are trustworthy; help me, for I am being persecuted without cause."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All your commands are trustworthy. Protect me from those who hunt me down without cause."
      }
