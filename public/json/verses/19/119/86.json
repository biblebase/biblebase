    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的一切命令都可靠， 他们无故地迫害我， 求你帮助我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
