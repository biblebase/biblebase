    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.119.161-168",
            "paragraphs": [
              "一一九161～168詩人因神的話而心生敬畏、喜樂、愛慕和贊美。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我遵守你的法则和法度， 你知道我做的每一件事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我謹守你的訓詞和法度， 因我所行的一切都在你面前。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我遵守了你的訓詞和法度， 因我一切所行的都在你面前。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I keep Your precepts and Your testimonies, For all my ways are before You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I obey your precepts and your statutes, for all my ways are known to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, I obey your commandments and laws because you know everything I do."
      }
    },
