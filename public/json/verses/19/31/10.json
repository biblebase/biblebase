    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.31.9-24",
            "paragraphs": [
              "碰壁的人（三十一9～24）"
            ]
          },
          {
            "verse": "psa.31.9-13",
            "paragraphs": [
              "9～13.他的隔離。這名受害者情緒一落千丈，從難受到絕望（12節），以至萬分恐懼（13節），由此可見仇恨的影響可以多強，特別是以拒絕來表明恨意時。詩篇第六篇同樣用了枯幹此一少見的字，表達相似的無助之愁苦，而導致沮喪的原因則未說明。根據第10節的希伯來文，此處的原因部分來自罪孽；不過從第14節可以明顯看出，一心要加以定罪的並不是神，而是人。耶利米曾親身置於如此殘酷的環境下，因此他借用了四圍都是驚嚇一語，作為慣用的形容（耶六25，二十3、10，四十六5，四十九29；參哀二22）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.31.9-12",
            "paragraphs": [
              "三一9～12大衛形容他由於受到攻擊而承受身體與情感上的極端痛苦。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的生命被愁苦吞噬， 岁月被哀伤耗尽， 力量因罪恶而消逝， 我成了枯骨一堆。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的生命因愁苦耗盡， 我的歲月在歎息中消逝， 我的力量因罪孽衰退， 我的骨頭也枯乾。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的生命為愁苦所消耗； 我的年歲為歎息所曠廢。 我的力量因我的罪孽衰敗； 我的骨頭也枯乾。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For my life is spent with sorrow And my years with sighing; My strength has failed because of my iniquity, And my body has wasted away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My life is consumed by anguish and my years by groaning; my strength fails because of my affliction, and my bones grow weak."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am dying from grief; my years are shortened by sadness. Sin has drained my strength; I am wasting away from within."
      }
    },
