    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.31.9-24",
            "paragraphs": [
              "碰壁的人（三十一9～24）"
            ]
          },
          {
            "verse": "psa.31.9-13",
            "paragraphs": [
              "9～13.他的隔離。這名受害者情緒一落千丈，從難受到絕望（12節），以至萬分恐懼（13節），由此可見仇恨的影響可以多強，特別是以拒絕來表明恨意時。詩篇第六篇同樣用了枯幹此一少見的字，表達相似的無助之愁苦，而導致沮喪的原因則未說明。根據第10節的希伯來文，此處的原因部分來自罪孽；不過從第14節可以明顯看出，一心要加以定罪的並不是神，而是人。耶利米曾親身置於如此殘酷的環境下，因此他借用了四圍都是驚嚇一語，作為慣用的形容（耶六25，二十3、10，四十六5，四十九29；參哀二22）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.31.9-12",
            "paragraphs": [
              "三一9～12大衛形容他由於受到攻擊而承受身體與情感上的極端痛苦。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，我落在苦难之中， 求你怜悯我， 我双眼哭肿，身心疲惫。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你恩待我，因為我在患難之中； 我的眼睛因愁煩昏花， 我的身、我的心也都衰殘。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你憐恤我， 因為我在急難之中； 我的眼睛因憂愁而乾癟， 連我的身心也不安舒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be gracious to me, O Lord, for I am in distress; My eye is wasted away from grief, my soul and my body also."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be merciful to me, Lord, for I am in distress; my eyes grow weak with sorrow, my soul and body with grief."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Have mercy on me, Lord, for I am in distress. Tears blur my eyes. My body and soul are withering away."
      }
    },
