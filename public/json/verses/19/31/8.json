    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你没有将我交给仇敌， 而是领我到宽阔之地。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你沒有把我交在仇敵的手裡， 你使我的腳站穩在寬闊之地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你未曾把我交在仇敵手裏； 你使我的腳站在寬闊之處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And You have not given me over into the hand of the enemy; You have set my feet in a large place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have not given me into the hands of the enemy but have set my feet in a spacious place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have not handed me over to my enemies but have set me in a safe place."
      }
    },
