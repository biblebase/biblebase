    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华掌权，万民当战抖； 祂坐在基路伯天使之上， 大地当战抖。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華作王，萬民都要戰慄； 他坐在兩個基路伯之上，大地也要震動。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華作王；萬民當戰抖！ 他坐在二基路伯上，地當動搖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord reigns, let the peoples tremble; He is enthroned above the cherubim, let the earth shake!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord reigns, let the nations tremble; he sits enthroned between the cherubim, let the earth shake."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord is king! Let the nations tremble! He sits on his throne between the cherubim. Let the whole earth quake!"
      }
