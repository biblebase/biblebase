    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.26.9-10",
            "paragraphs": [
              "二六9,10\"不要……一同除掉\"。大衛祈求能與罪人分開，以至到了審判日，他不會與他們一同被除掉。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "别把我的灵魂和罪人同毁， 别把我的性命与嗜血之徒同灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你不要把我的性命和罪人一同除掉， 也不要把我的生命和流人血的人一起消滅。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "不要把我的灵魂与罪人一同收回， 不要把我的命与杀人流血的一同收回。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要把我的靈魂和罪人一同除掉； 不要把我的性命和流人血的一同除掉。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Do not sweep my soul away with sinners, nor my life with bloodthirsty men,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not take my soul away along with sinners, Nor my life with men of bloodshed,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not take away my soul along with sinners, my life with those who are bloodthirsty,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Do not gather my soul with sinners, Nor my life with bloodthirsty men,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t let me suffer the fate of sinners. Don’t condemn me along with murderers."
      }
    },
