    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你不要让恶人的愿望实现， 不要让他们的阴谋得逞， 免得他们狂妄自大。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你不要容惡人的心願得償， 不要使他們的計謀成功， 免得他們自高自大。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你不要遂惡人的心願； 不要成就他們的計謀，恐怕他們自高。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not grant, O Lord, the desires of the wicked; Do not promote his evil device, that they not be exalted. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not grant the wicked their desires, Lord; do not let their plans succeed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, do not let evil people have their way. Do not let their evil schemes succeed, or they will become proud. Interlude"
      }
