    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊， 求你使我免遭恶人的毒手， 保护我脱离残暴之徒。 他们图谋害我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你保護我脫離惡人的手， 保護我脫離強暴的人， 因為他們圖謀推倒我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你拯救我脫離惡人的手， 保護我脫離強暴的人！ 他們圖謀推我跌倒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Keep me, O Lord, from the hands of the wicked; Preserve me from violent men Who have purposed to trip up my feet."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Keep me safe, Lord, from the hands of the wicked; protect me from the violent, who devise ways to trip my feet."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, keep me out of the hands of the wicked. Protect me from those who are violent, for they are plotting against me."
      }
    },
