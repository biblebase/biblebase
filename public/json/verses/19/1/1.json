    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有福的人： 不從惡人的計謀， 不站罪人的道路， 不坐好譏笑的人的座位。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不從惡人的計謀， 不站罪人的道路， 不坐褻慢人的座位，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How blessed is the man who does not walk in the counsel of the wicked, Nor stand in the path of sinners, Nor sit in the seat of scoffers!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Blessed is the one who does not walk in step with the wicked or stand in the way that sinners take or sit in the company of mockers,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Oh, the joys of those who do not follow the advice of the wicked, or stand around with sinners, or join in with mockers."
      }
    },
