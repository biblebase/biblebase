    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人在审判之日必无法逃脱， 罪人在义人的聚会中必站不住脚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此，在審判的時候，惡人必站立不住； 在義人的團體中，罪人也必這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因此，當審判的時候，惡人必站立不住； 罪人在義人的會中也是如此。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore the wicked will not stand in the judgment, Nor sinners in the assembly of the righteous."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore the wicked will not stand in the judgment, nor sinners in the assembly of the righteous."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They will be condemned at the time of judgment. Sinners will have no place among the godly."
      }
