    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.16.1-6",
            "paragraphs": [
              "忠心的仆人（十六1～6）",
              "這半篇詩幾乎每一節都講到忠貞專一的某方面；即如：將自己的安全（1節）、幸福（2節）、朋友（3節）、敬拜（4節）、抱負（5、6節）等各方面，都孤註一擲於神。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.16.5-6",
            "paragraphs": [
              "一六5,6大衛描述屬靈產業的美麗時，所用的詞藻跟神把應許地分配給以色列人的說法類似。\"繩\"。用來量度各個分地的繩子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你赐我佳美之地， 我的产业何其美！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "準繩量給我的是佳美之地， 我的產業實在令我喜悅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "用繩量給我的地界，坐落在佳美之處； 我的產業實在美好。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The lines have fallen to me in pleasant places; Indeed, my heritage is beautiful to me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The boundary lines have fallen for me in pleasant places; surely I have a delightful inheritance."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The land you have given me is a pleasant land. What a wonderful inheritance!"
      }
    },
