    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝住在城里， 城必屹立不倒。 天一亮，上帝必帮助这城。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神在城中，城必不動搖； 天一亮，　神必幫助它。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神在其中，城必不動搖； 到天一亮，神必幫助這城。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God is in the midst of her, she will not be moved; God will help her when morning dawns."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God is within her, she will not fall; God will help her at break of day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God dwells in that city; it cannot be destroyed. From the very break of day, God will protect it."
      }
