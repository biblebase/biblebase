    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们用诡计害你的子民， 他们一起谋害你所疼爱的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們密謀奸詐，陷害你的子民； 他們彼此商議，攻擊你所寶貴的人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们合伙密谋，对付你的子民； 他们彼此商议，对付你所珍爱的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們同謀奸詐要害你的百姓， 彼此商議要害你所隱藏的人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They lay crafty plans against your people; they consult together against your treasured ones."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They make shrewd plans against Your people, And conspire together against Your treasured ones."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "With cunning they conspire against your people; they plot against those you cherish."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They have taken crafty counsel against Your people, And consulted together against Your sheltered ones."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They devise crafty schemes against your people; they conspire against your precious ones."
      }
    },
