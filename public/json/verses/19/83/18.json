    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.83.18",
            "paragraphs": [
              "八三18\"耶和華\"（希伯來文是Yahweh）。參看創世記二章4節的腳註。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "让他们知道你是耶和华， 唯有你是普天下的至高者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "使他們知道只有你的名是耶和華， 唯有你是掌管全地的至高者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使他們知道：惟獨你－ 名為耶和華的－是全地以上的至高者！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "That they may know that You alone, whose name is the Lord, Are the Most High over all the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let them know that you, whose name is the Lord— that you alone are the Most High over all the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they will learn that you alone are called the Lord, that you alone are the Most High, supreme over all the earth."
      }
    },
