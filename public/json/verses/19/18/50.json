    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.18.46-50",
            "paragraphs": [
              "頌榮（十八46～50）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.18.49-50",
            "paragraphs": [
              "一八49,50保羅在這句話裏看見有關彌賽亞的暗示（羅一五9），因為當中有談論到王室家系的\"受膏者\"（希伯來文是彌賽亞）一詞，這家系在基督裏達到高峰。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使你立的王大获全胜， 向你膏立的大卫及其后代广施慈爱，直到永远。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華賜極大的救恩給他所立的王， 又向他的受膏者施慈愛， 就是向大衛和他的後裔施慈愛，直到永遠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華賜極大的救恩給他所立的王， 施慈愛給他的受膏者， 就是給大衛和他的後裔，直到永遠。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He gives great deliverance to His king, And shows lovingkindness to His anointed, To David and his descendants forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He gives his king great victories; he shows unfailing love to his anointed, to David and to his descendants forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You give great victories to your king; you show unfailing love to your anointed, to David and all his descendants forever."
      }
