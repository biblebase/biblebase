      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華按着我的公義報答我， 按着我手中的清潔賞賜我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord has rewarded me according to my righteousness; According to the cleanness of my hands He has recompensed me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord has dealt with me according to my righteousness; according to the cleanness of my hands he has rewarded me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord rewarded me for doing right; he restored me because of my innocence."
      }
    },
