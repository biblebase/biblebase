    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我被狮子包围， 躺卧在吃人的野兽中。 他们的牙齿是矛和箭， 舌头是尖刀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我躺臥在獅子中間， 就是在那些想吞滅人的世人中間； 他們的牙齒是槍和箭， 他們的舌頭是快刀。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我的性命在狮子中间， 我躺卧在吞灭人的世人当中； 他们的牙齿是长矛和箭矢， 他们的舌头是锐利的刀剑。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的性命在獅子中間； 我躺臥在性如烈火的世人當中。 他們的牙齒是槍、箭； 他們的舌頭是快刀。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "My soul is in the midst of lions; I lie down amid fiery beasts— the children of man, whose teeth are spears and arrows, whose tongues are sharp swords."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My soul is among lions; I must lie among those who breathe forth fire, Even the sons of men, whose teeth are spears and arrows And their tongue a sharp sword."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am in the midst of lions; I am forced to dwell among ravenous beasts— men whose teeth are spears and arrows, whose tongues are sharp swords."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "My soul is among lions; I lie among the sons of men Who are set on fire, Whose teeth are spears and arrows, And their tongue a sharp sword."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am surrounded by fierce lions who greedily devour human prey— whose teeth pierce like spears and arrows, and whose tongues cut like swords."
      }
    },
