    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.57.4",
            "paragraphs": [
              "五七4大衛要躲藏那心中充滿怒火的人。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我被狮子包围， 躺卧在吃人的野兽中。 他们的牙齿是矛和箭， 舌头是尖刀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我躺臥在獅子中間， 就是在那些想吞滅人的世人中間； 他們的牙齒是槍和箭， 他們的舌頭是快刀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的性命在獅子中間； 我躺臥在性如烈火的世人當中。 他們的牙齒是槍、箭； 他們的舌頭是快刀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My soul is among lions; I must lie among those who breathe forth fire, Even the sons of men, whose teeth are spears and arrows And their tongue a sharp sword."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am in the midst of lions; I am forced to dwell among ravenous beasts— men whose teeth are spears and arrows, whose tongues are sharp swords."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am surrounded by fierce lions who greedily devour human prey— whose teeth pierce like spears and arrows, and whose tongues cut like swords."
      }
    },
