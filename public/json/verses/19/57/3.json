    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.57.3",
            "paragraphs": [
              "五七3\"慈愛\"。神堅定不移的愛。參看何西阿書二章19節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝从天上施助、拯救我， 使迫害我的人蒙羞。 上帝必彰显祂的慈爱和信实。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神從天上施恩拯救我， 斥責那踐踏我的人； 　神必向我發出他的慈愛和信實。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他从天上赐下救恩拯救我， 他斥责那践踏我的人。细拉 神赐下了他的慈爱和信实。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那要吞我的人辱罵我的時候， 神從天上必施恩救我， 也必向我發出慈愛和誠實。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He will send from heaven and save me; he will put to shame him who tramples on me. Selah God will send out his steadfast love and his faithfulness!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will send from heaven and save me; He reproaches him who tramples upon me. Selah. God will send forth His lovingkindness and His truth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He sends from heaven and saves me, rebuking those who hotly pursue me— God sends forth his love and his faithfulness."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He shall send from heaven and save me; He reproaches the one who would swallow me up. Selah God shall send forth His mercy and His truth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will send help from heaven to rescue me, disgracing those who hound me. Interlude My God will send forth his unfailing love and faithfulness."
      }
