      "leili": {
        "content": [
          {
            "verse": "psa.65.5",
            "paragraphs": [
              "六五5\"威嚴\"。令人敬畏的作為。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拯救我们的上帝啊， 你凭公义、行可畏之事来应允我们的祈求。 你是普天下的盼望。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "拯救我們的　神啊！ 你必充滿威嚴，按公義應允我們； 你本是一切地極和海洋遠處的人所倚靠的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拯救我們的神啊，你必以威嚴秉公義應允我們； 你本是一切地極和海上遠處的人所倚靠的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By awesome deeds You answer us in righteousness, O God of our salvation, You who are the trust of all the ends of the earth and of the farthest sea;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You answer us with awesome and righteous deeds, God our Savior, the hope of all the ends of the earth and of the farthest seas,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You faithfully answer our prayers with awesome deeds, O God our savior. You are the hope of everyone on earth, even those who sail on distant seas."
      }
    },
