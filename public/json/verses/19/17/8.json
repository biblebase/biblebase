        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你保护我，像保护眼中的瞳仁， 用你的翅膀荫庇我，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你保護我，像保護眼中的瞳人， 把我隱藏在你的翅膀蔭下；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你保護我，如同保護眼中的瞳人； 將我隱藏在你翅膀的蔭下，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Keep me as the apple of the eye; Hide me in the shadow of Your wings"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Keep me as the apple of your eye; hide me in the shadow of your wings"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Guard me as you would guard your own eyes. Hide me in the shadow of your wings."
      }
