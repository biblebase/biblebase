      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願我的判語從你面前發出； 願你的眼睛觀看公正。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
