    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之上帝啊， 求你回来， 求你在天上垂顾我们这棵葡萄树，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍的　神啊！求你回心轉意， 從天上垂看鑒察， 眷顧這葡萄樹，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之神啊，求你回轉！ 從天上垂看，眷顧這葡萄樹，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O God of hosts, turn again now, we beseech You; Look down from heaven and see, and take care of this vine,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Return to us, God Almighty! Look down from heaven and see! Watch over this vine,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Come back, we beg you, O God of Heaven’s Armies. Look down from heaven and see our plight. Take care of this grapevine"
      }
