    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.51.14",
            "paragraphs": [
              "五一14\"流人血的罪\"。直譯是：流血；即因謀殺烏利亞而被判死刑。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拯救我的上帝啊， 求你赦免我杀人流血的罪， 使我颂扬你的公义。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！你是拯救我的　神， 求你救我脫離流人血的罪； 我的舌頭就必頌揚你的公義。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你是拯救我的神； 求你救我脫離流人血的罪！ 我的舌頭就高聲歌唱你的公義。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Deliver me from bloodguiltiness, O God, the God of my salvation; Then my tongue will joyfully sing of Your righteousness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Deliver me from the guilt of bloodshed, O God, you who are God my Savior, and my tongue will sing of your righteousness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Forgive me for shedding blood, O God who saves; then I will joyfully sing of your forgiveness."
      }
