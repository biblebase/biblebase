      "leili": {
        "content": [
          {
            "verse": "psa.51.8",
            "paragraphs": [
              "五一8\"壓傷的骨頭\"。大衛因拿單的指控而完全崩潰（撒下一二1～14）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你让我听到欢喜快乐的声音， 让我这被你压碎的骨头可以欢跳。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你使我聽見歡喜和快樂的聲音， 使你所壓傷的骨頭可以歡呼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你使我得聽歡喜快樂的聲音， 使你所壓傷的骨頭可以踴躍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Make me to hear joy and gladness, Let the bones which You have broken rejoice."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let me hear joy and gladness; let the bones you have crushed rejoice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Oh, give me back my joy again; you have broken me— now let me rejoice."
      }
    },
