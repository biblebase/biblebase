          {
            "verse": "psa.51.18",
            "paragraphs": [
              "五一18\"建造……城墻\"。鞏固國人的道德氣質，以至他們不會受到大衛之壞榜樣的感染。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你恩待锡安，重建耶路撒冷的城墙。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你按著你的美意善待錫安； 求你修築耶路撒冷的城牆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你隨你的美意善待錫安， 建造耶路撒冷的城牆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "By Your favor do good to Zion; Build the walls of Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May it please you to prosper Zion, to build up the walls of Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Look with favor on Zion and help her; rebuild the walls of Jerusalem."
      }
