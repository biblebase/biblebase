    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.51.10",
            "paragraphs": [
              "51:10",
              "求你為我造清潔的心：每位信徒都需要神的靈在他裏面造一顆清潔、恨惡罪惡的心和一個喜悅遵行神旨意的更新的靈。惟有神能使我們成為新造的人，使我們重新有誠實敬虔的心(約3:3；林後5:17；參“重生”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 求你为我造一颗纯洁的心， 使我里面重新有正直的灵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！求你為我造一顆清潔的心， 求你使我裡面重新有堅定的靈。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，求你為我造清潔的心， 使我裏面重新有正直的靈。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Create in me a clean heart, O God, And renew a steadfast spirit within me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Create in me a pure heart, O God, and renew a steadfast spirit within me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Create in me a clean heart, O God. Renew a loyal spirit within me."
      }
