    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.51.17",
            "paragraphs": [
              "51:17",
              "憂傷痛悔的心：神絕不會棄絕或輕看一顆破碎痛悔、為罪憂傷又哀慟沈重的心。當我們放下自私和驕傲，謙卑呼求神赦罪之恩時，就必蒙他悅納(比較賽57:15；路18:10~14)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你所要的祭是忧伤的心灵。 上帝啊，你必不轻看忧伤痛悔的心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神所要的祭，就是破碎的靈； 　神啊！破碎痛悔的心，你必不輕看。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神所要的祭就是憂傷的靈； 神啊，憂傷痛悔的心，你必不輕看。"
      },
