    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 求你为我伸张正义， 在不虔敬的人面前为我辩护， 救我脱离诡诈邪恶的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！求你為我伸冤， 為我的案件向不敬虔的國申辯； 求你救我脫離詭詐和不義的人；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，求你伸我的冤， 向不虔誠的國為我辨屈； 求你救我脫離詭詐不義的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Vindicate me, O God, and plead my case against an ungodly nation; O deliver me from the deceitful and unjust man!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Vindicate me, my God, and plead my cause against an unfaithful nation. Rescue me from those who are deceitful and wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Declare me innocent, O God! Defend me against these ungodly people. Rescue me from these unjust liars."
      }
    },
