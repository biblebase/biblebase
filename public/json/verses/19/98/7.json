    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.98.7-8",
            "paragraphs": [
              "九八7,8基督在千禧年的國度作王統治的時候，大自然因得釋放而歡呼。參看羅馬書八章20節的腳註。\"大水\"指河流。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大海和海中的一切要颂扬， 大地和地上的一切要欢呼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願海和海中充滿的，都翻騰響鬧， 願世界和住在世上的也都發聲；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願海和其中所充滿的澎湃； 世界和住在其間的也要發聲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let the sea roar and all it contains, The world and those who dwell in it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let the sea resound, and everything in it, the world, and all who live in it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let the sea and everything in it shout his praise! Let the earth and all living things join in."
      }
