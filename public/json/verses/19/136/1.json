    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.136.1",
            "paragraphs": [
              "136:1",
              "1~26他的慈愛永遠長存：這貫穿全詩的疊句教導我們，神的慈愛是他為我們行一切事的根由，也是我們一切感恩的源頭。他的慈愛包括了他的憐憫、信實和良善。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要称谢耶和华， 因为祂是美善的， 祂的慈爱永远长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要稱謝耶和華，因他本是良善的， 他的慈愛永遠長存。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要稱謝耶和華，因他本為善； 他的慈愛永遠長存。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Give thanks to the Lord, for He is good, For His lovingkindness is everlasting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Give thanks to the Lord, for he is good. His love endures forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give thanks to the Lord, for he is good! His faithful love endures forever."
      }
