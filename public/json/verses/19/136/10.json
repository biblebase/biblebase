    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要称谢那位击杀埃及人长子的， 因为祂的慈爱永远长存。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要稱謝那擊殺埃及所有頭生的， 因為他的慈愛永遠長存。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "稱謝那擊殺埃及人之長子的， 因他的慈愛永遠長存。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To Him who smote the Egyptians in their firstborn, For His lovingkindness is everlasting,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "to him who struck down the firstborn of Egypt His love endures forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give thanks to him who killed the firstborn of Egypt. His faithful love endures forever."
      }
