    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你起来！ 我的上帝啊，求你救我！ 求你掴我一切仇敌的脸， 打碎恶人的牙齿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你起來； 我的　神啊！求你拯救我， 你擊打了我所有仇敵的臉頰， 打碎了惡人的牙齒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你起來！ 我的神啊，求你救我！ 因為你打了我一切仇敵的腮骨， 敲碎了惡人的牙齒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Arise, O Lord; save me, O my God! For You have smitten all my enemies on the cheek; You have shattered the teeth of the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Arise, Lord! Deliver me, my God! Strike all my enemies on the jaw; break the teeth of the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Arise, O Lord! Rescue me, my God! Slap all my enemies in the face! Shatter the teeth of the wicked!"
      }
    },
