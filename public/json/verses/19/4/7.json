        "text": "Fill my heart with joy when their grain and new wine abound."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You have put gladness in my heart, More than in the season that their grain and wine increased."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You have given me greater joy than those who have abundant harvests of grain and new wine."
      }
