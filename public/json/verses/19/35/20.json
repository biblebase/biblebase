    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.35.19-28",
            "paragraphs": [
              "幸災樂禍（三十五19～28）",
              "無故的憎恨，是惡對善的基本反應（在第7節已經強調過），因此耶穌視第19節（及六十九4），不是指大衛意外的不幸，乃是指祂所預定的遭遇。這就是“他們律法上所寫的話”（約十五25），是有關即將發生之事的權威性啟示。這個模式在祂的案件上一展無遺，在大衛的一生中斷續可見，而我們也命定要如此經歷（約十五18以下）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.35.19-21",
            "paragraphs": [
              "三五19,21\"擠眼\"和\"大大張口攻擊我\"都是表示惡意和輕蔑的姿態。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们言语暴戾， 谋害安分守己的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們不說和睦的話， 卻計劃詭詐的事， 陷害世上的安靜人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "因为他们不说平安的话， 却针对地上安分的人谋算欺诈的事；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他們不說和平話， 倒想出詭詐的言語害地上的安靜人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For they do not speak peace, but against those who are quiet in the land they devise words of deceit."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they do not speak peace, But they devise deceitful words against those who are quiet in the land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They do not speak peaceably, but devise false accusations against those who live quietly in the land."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For they do not speak peace, But they devise deceitful matters Against the quiet ones in the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They don’t talk of peace; they plot against innocent people who mind their own business."
      }
