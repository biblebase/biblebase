    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.35.1-10",
            "paragraphs": [
              "詭計（三十五1～10）",
              "第1～3節的軍事用語是比喻，其實頭一個比方是借用法庭的狀況（相爭）。因此本詩適用於任何遭怨恨、被陷害的情形。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.35.7-8",
            "paragraphs": [
              "三五7,8詩中提到公義的經典例子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为他们无故设网罗， 挖陷阱要害我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們無故為我暗設網羅， 無故挖坑要陷害我的性命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他們無故地為我暗設網羅， 無故地挖坑，要害我的性命。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For without cause they hid their net for me; Without cause they dug a pit for my soul."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Since they hid their net for me without cause and without cause dug a pit for me,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I did them no wrong, but they laid a trap for me. I did them no wrong, but they dug a pit to catch me."
      }
    },
