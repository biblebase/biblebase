    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.35.1-10",
            "paragraphs": [
              "詭計（三十五1～10）",
              "第1～3節的軍事用語是比喻，其實頭一個比方是借用法庭的狀況（相爭）。因此本詩適用於任何遭怨恨、被陷害的情形。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.35.7-8",
            "paragraphs": [
              "三五7,8詩中提到公義的經典例子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
