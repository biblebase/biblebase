    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.35.1-10",
            "paragraphs": [
              "詭計（三十五1～10）",
              "第1～3節的軍事用語是比喻，其實頭一個比方是借用法庭的狀況（相爭）。因此本詩適用於任何遭怨恨、被陷害的情形。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我从心底发出赞叹： 耶和华啊，谁能像你？ 你拯救弱者免受欺压， 拯救穷人免遭掠夺。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我全身的骨頭都要說： “耶和華啊！有誰像你呢？ 你搭救困苦的人，脫離那些比他強盛的； 搭救困苦和窮乏的人，脫離那些搶奪他的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的骨頭都要說： 耶和華啊，誰能像你 救護困苦人脫離那比他強壯的， 救護困苦窮乏人脫離那搶奪他的？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All my bones will say, “Lord, who is like You, Who delivers the afflicted from him who is too strong for him, And the afflicted and the needy from him who robs him?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My whole being will exclaim, “Who is like you, Lord? You rescue the poor from those too strong for them, the poor and needy from those who rob them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "With every bone in my body I will praise him: “Lord, who can compare with you? Who else rescues the helpless from the strong? Who else protects the helpless and poor from those who rob them?”"
      }
    },
