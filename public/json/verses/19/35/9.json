    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.35.1-10",
            "paragraphs": [
              "詭計（三十五1～10）",
              "第1～3節的軍事用語是比喻，其實頭一個比方是借用法庭的狀況（相爭）。因此本詩適用於任何遭怨恨、被陷害的情形。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要因耶和华而欢欣， 因祂的拯救而快乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的心必因耶和華快樂， 因他的救恩高興。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的心必靠耶和華快樂， 靠他的救恩高興。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And my soul shall rejoice in the Lord; It shall exult in His salvation."
      },
