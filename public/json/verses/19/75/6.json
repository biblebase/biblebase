    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.75.6",
            "paragraphs": [
              "七五6沒有提到北面，因為亞述人正從那方向迫近。沒有人能指望從北方得到幫助，那裏只有侵擾（賽三六；三七可能是這首詩篇的背景）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人的尊贵不是来自东方， 不是来自西方， 也不是来自旷野。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "高升不是從東面來，不是從西面來， 也不是曠野來，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為高舉非從東，非從西， 也非從南而來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For not from the east, nor from the west, Nor from the desert comes exaltation;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one from the east or the west or from the desert can exalt themselves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For no one on earth—from east or west, or even from the wilderness— should raise a defiant fist."
      }
