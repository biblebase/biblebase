    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但上帝必救赎我的生命脱离死亡的权势， 接我到祂身边。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但　神必救贖我的靈魂脫離陰間的權勢， 因為他必把我接去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是神必救贖我的靈魂脫離陰間的權柄， 因他必收納我。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But God will redeem my soul from the power of Sheol, For He will receive me. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But God will redeem me from the realm of the dead; he will surely take me to himself."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But as for me, God will redeem my life. He will snatch me from the power of the grave. Interlude"
      }
