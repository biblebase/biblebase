    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.49.7-9",
            "paragraphs": [
              "四九7～9這裏意思是，人不能向神買贖日子，延長壽命。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "却不能救赎自己的生命， 也无法向上帝付生命的赎价。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但他們沒有一個能把他的兄弟贖回， 或把他的贖價交給　神，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "一個也無法贖自己的弟兄， 也不能替他將贖價給神，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No man can by any means redeem his brother Or give to God a ransom for him—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "No one can redeem the life of another or give to God a ransom for them—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yet they cannot redeem themselves from death by paying a ransom to God."
      }
    },
