    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.147.10-11",
            "paragraphs": [
              "一四七10,11耶和華不喜悅身體的力量，卻喜悅人出自尊敬的信靠。關於\"敬畏\"，參看第一百零三篇11節的腳註；關於\"慈愛\"，參看何西阿書二章19節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华所喜悦的不是强健的马匹， 也不是矫捷的战士，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他喜歡的不是馬的力大， 他喜悅的不是人的腿快。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他不喜悅馬的力大， 不喜愛人的腿快。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He does not delight in the strength of the horse; He does not take pleasure in the legs of a man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His pleasure is not in the strength of the horse, nor his delight in the legs of the warrior;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He takes no pleasure in the strength of a horse or in human might."
      }
