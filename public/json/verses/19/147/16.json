    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.147.15-20",
            "paragraphs": [
              "一四七15～20祂的話語能命令和操控大自然的力量（15～18節），但祂特別把祂的律法頒布給以色列，使她與其它各國有所不同（19,20節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂降下羊毛般的白雪， 撒下炉灰般的寒霜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他降下像羊毛一樣的雪， 撒下像爐灰一樣的霜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他降雪如羊毛， 撒霜如爐灰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He gives snow like wool; He scatters the frost like ashes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He spreads the snow like wool and scatters the frost like ashes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He sends the snow like white wool; he scatters frost upon the ground like ashes."
      }
