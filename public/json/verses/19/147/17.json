    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.147.15-20",
            "paragraphs": [
              "一四七15～20祂的話語能命令和操控大自然的力量（15～18節），但祂特別把祂的律法頒布給以色列，使她與其它各國有所不同（19,20節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂抛下碎石般的冰雹， 谁能经得住祂降下的严寒呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他拋下像碎屑一樣的冰雹； 面對他發出的寒冷，誰能承受得起呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他擲下冰雹如碎渣； 他發出寒冷，誰能當得起呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He casts forth His ice as fragments; Who can stand before His cold?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He hurls down his hail like pebbles. Who can withstand his icy blast?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He hurls the hail like stones. Who can stand against his freezing cold?"
      }
    },
