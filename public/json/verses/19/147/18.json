    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.147.15-20",
            "paragraphs": [
              "一四七15～20祂的話語能命令和操控大自然的力量（15～18節），但祂特別把祂的律法頒布給以色列，使她與其它各國有所不同（19,20節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂一声令下，冰雪便溶化， 微风便吹拂，河川便奔流。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他發出命令，這一切就都融化； 他使風颳起，水就流動。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他一出令，這些就都消化； 他使風颳起，水便流動。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He sends forth His word and melts them; He causes His wind to blow and the waters to flow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He sends his word and melts them; he stirs up his breezes, and the waters flow."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then, at his command, it all melts. He sends his winds, and the ice thaws."
      }
    },
