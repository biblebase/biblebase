          {
            "verse": "psa.147.2-5",
            "paragraphs": [
              "一四七2～5耶和華眷顧子民（2,3節）的事實，因祂的全能（4,5節）而得到保證。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们的主伟大无比，充满力量， 祂的智慧没有穷尽。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們的主偉大，大有能力； 他的智慧無法測度。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們的主為大，最有能力； 他的智慧無法測度。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Great is our Lord and abundant in strength; His understanding is infinite."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Great is our Lord and mighty in power; his understanding has no limit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How great is our Lord! His power is absolute! His understanding is beyond comprehension!"
      }
