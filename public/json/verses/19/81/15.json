    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.81.15",
            "paragraphs": [
              "八一15\"必來投降\"。更可作：假裝順服祂。\"祂的百姓必永久長存\"。或可作：他們（即恨耶和華的人）〔懲罰〕的日子必永久長存。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "憎恶我的人要在我面前屈膝投降，永远沉沦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "憎恨耶和華的人必向他假意歸順， 他們的刑期必永無止境。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "恨耶和華的人必來投降， 但他的百姓必永久長存。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Those who hate the Lord would pretend obedience to Him, And their time of punishment would be forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who hate the Lord would cringe before him, and their punishment would last forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who hate the Lord would cringe before him; they would be doomed forever."
      }
    },
