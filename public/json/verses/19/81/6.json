      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I relieved his shoulder of the burden, His hands were freed from the basket."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I removed the burden from their shoulders; their hands were set free from the basket."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Now I will take the load from your shoulders; I will free your hands from their heavy tasks."
      }
    },
