      "leili": {
        "content": [
          {
            "verse": "psa.79.1-5",
            "paragraphs": [
              "七九1～5這段經文描述耶路撒冷在主前586年毀滅於巴比倫軍隊手中。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "使耶路撒冷周围血流成河， 尸体无人埋葬。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在耶路撒冷的周圍把聖民的血像水一般倒出， 沒有人埋葬他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在耶路撒冷周圍流他們的血如水， 無人葬埋。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have poured out their blood like water round about Jerusalem; And there was no one to bury them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They have poured out blood like water all around Jerusalem, and there is no one to bury the dead."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Blood has flowed like water all around Jerusalem; no one is left to bury the dead."
      }
