    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.79.12",
            "paragraphs": [
              "七九12\"七倍\"。透徹",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "主啊，我们的邻邦羞辱你， 求你以七倍的羞辱来报应他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主啊！願你把我們鄰國羞辱你的羞辱， 七倍歸還在他們身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主啊，願你將我們鄰邦所羞辱你的羞辱 加七倍歸到他們身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And return to our neighbors sevenfold into their bosom The reproach with which they have reproached You, O Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Pay back into the laps of our neighbors seven times the contempt they have hurled at you, Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, pay back our neighbors seven times for the scorn they have hurled at you."
      }
    },
