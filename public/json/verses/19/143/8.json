    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你让我在清晨就听见你的慈言爱语， 因为我信靠你； 求你指示我当走的路， 因为我的心仰望你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你使我清晨得聞你的慈愛， 因為我倚靠你。 求你指示我當走的路， 因為我的心仰望你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你使我清晨得聽你慈愛之言， 因我倚靠你； 求你使我知道當行的路， 因我的心仰望你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let me hear Your lovingkindness in the morning; For I trust in You; Teach me the way in which I should walk; For to You I lift up my soul."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let the morning bring me word of your unfailing love, for I have put my trust in you. Show me the way I should go, for to you I entrust my life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let me hear of your unfailing love each morning, for I am trusting you. Show me where to walk, for I give myself to you."
      }
    },
