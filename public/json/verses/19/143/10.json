    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你教导我遵行你的旨意， 因为你是我的上帝， 愿你良善的灵引导我走平坦的路。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你教我遵行你的旨意， 因為你是我的　神； 願你至善的靈引導我到平坦的地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你指教我遵行你的旨意， 因你是我的神。 你的靈本為善； 求你引我到平坦之地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Teach me to do Your will, For You are my God; Let Your good Spirit lead me on level ground."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Teach me to do your will, for you are my God; may your good Spirit lead me on level ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Teach me to do your will, for you are my God. May your gracious Spirit lead me forward on a firm footing."
      }
