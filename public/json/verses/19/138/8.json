    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.138.8",
            "paragraphs": [
              "一三八8詩人表達自己對神有完全的信心；神的\"慈愛\"（堅定、忠實的愛）是永遠的。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华必成就祂为我所定的计划； 耶和华啊，你的慈爱永远长存。 求你不要撇弃你亲手所造的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華必成全關乎我的事； 耶和華啊！你的慈愛永遠長存， 求你不要離棄你手所造的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華必成全關乎我的事； 耶和華啊，你的慈愛永遠長存！ 求你不要離棄你手所造的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord will accomplish what concerns me; Your lovingkindness, O Lord, is everlasting; Do not forsake the works of Your hands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord will vindicate me; your love, Lord, endures forever— do not abandon the works of your hands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord will work out his plans for my life— for your faithful love, O Lord, endures forever. Don’t abandon me, for you made me."
      }
    },
