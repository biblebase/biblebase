    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.84.3-4",
            "paragraphs": [
              "八四3,4對於在聖殿築巢的麻雀和在聖殿裏供職的人，詩人既羨慕又妒忌。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_sermon-2010-10-24",
        "title": "從聖經看建堂",
        "date": "2010-10-24",
        "preacher": "寇順舉牧師",
        "verses": "詩篇 84:1-4",
