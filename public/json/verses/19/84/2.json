    "sermons": [
      {
        "id": "home_sermons_sermon-2010-10-24",
        "title": "從聖經看建堂",
        "date": "2010-10-24",
        "preacher": "寇順舉牧師",
        "verses": "詩篇 84:1-4",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2010/Sermon-2010-10-24.mp3"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我深愿进入耶和华的院宇， 我的身心向永活的上帝欢呼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的靈渴想切慕耶和華的院子， 我的心身向永活的　神歡呼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我羨慕渴想耶和華的院宇； 我的心腸，我的肉體向永生神呼籲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My soul longed and even yearned for the courts of the Lord; My heart and my flesh sing for joy to the living God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My soul yearns, even faints, for the courts of the Lord; my heart and my flesh cry out for the living God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I long, yes, I faint with longing to enter the courts of the Lord. With my whole being, body and soul, I will shout joyfully to the living God."
      }
    },
