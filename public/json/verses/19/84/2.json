    "sermons": [
      {
        "id": "home_sermons_sermon-2010-10-24",
        "title": "從聖經看建堂",
        "date": "2010-10-24",
        "preacher": "寇順舉牧師",
        "verses": "詩篇 84:1-4",
        "audio": "http://cbcwla.org/media/ch/SundaySermons/2010/Sermon-2010-10-24.mp3"
