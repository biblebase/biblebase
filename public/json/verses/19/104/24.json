    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你的创造多么繁多！ 你用智慧造了这一切， 大地充满了你创造的万物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你所造的真是眾多。 它們都是你用智慧造成的； 全地充滿你所造的東西。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你所造的何其多！ 都是你用智慧造成的； 遍地滿了你的豐富。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O Lord, how many are Your works! In wisdom You have made them all; The earth is full of Your possessions."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How many are your works, Lord! In wisdom you made them all; the earth is full of your creatures."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord, what a variety of things you have made! In wisdom you have made them all. The earth is full of your creatures."
      }
    },
