    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.104.15",
            "paragraphs": [
              "104:15",
              "酒能悅人心：“酒”(希伯來文yayin)在此指直接從“植物”(14節)果品提取的汁，而不是人發酵釀造的酒。天然葡萄汁味道甘美，營養豐富，是神所贊許的飲品(比較摩9:14；參“舊約中的酒”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有沁人心怀的醇酒、 滋润容颜的膏油、 增强活力的五谷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又有酒使人心歡喜， 有油使人容光煥發， 有糧加添人的心力。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又得酒能悅人心， 得油能潤人面， 得糧能養人心。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And wine which makes man’s heart glad, So that he may make his face glisten with oil, And food which sustains man’s heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "wine that gladdens human hearts, oil to make their faces shine, and bread that sustains their hearts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "wine to make them glad, olive oil to soothe their skin, and bread to give them strength."
      }
    },
