      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我一心等候耶和华， 比守夜的盼望天明还要迫切， 比守夜的盼望天明还要迫切。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的心等候主， 比守夜的等候天亮還迫切， 比守夜的等候天亮還迫切。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的心等候主， 勝於守夜的，等候天亮， 勝於守夜的，等候天亮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My soul waits for the Lord More than the watchmen for the morning; Indeed, more than the watchmen for the morning."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I wait for the Lord more than watchmen wait for the morning, more than watchmen wait for the morning."
      },
      "nlt": {
