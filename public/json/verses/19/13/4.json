    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.13.4",
            "paragraphs": [
              "一三4\"在我動搖的時候\"。即當我死的時候。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "免得我的仇敌说：“我们战胜了他！” 免得他们因我败落而欢喜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "免得我的仇敵說：“我勝過了他”； 免得我跌倒的時候我的敵人就歡呼。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "免得我的仇敌说“我胜过了他”； 免得我的敌人在我动摇时快乐。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "免得我的仇敵說：我勝了他； 免得我的敵人在我搖動的時候喜樂。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "lest my enemy say, “I have prevailed over him,” lest my foes rejoice because I am shaken."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And my enemy will say, “I have overcome him,” And my adversaries will rejoice when I am shaken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and my enemy will say, “I have overcome him,” and my foes will rejoice when I fall."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Lest my enemy say, “I have prevailed against him”; Lest those who trouble me rejoice when I am moved."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t let my enemies gloat, saying, “We have defeated him!” Don’t let them rejoice at my downfall."
      }
    },
