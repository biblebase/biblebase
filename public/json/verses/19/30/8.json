    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "psa.30.6-10",
            "paragraphs": [
              "愚人的自誇（三十6～10）",
              "平順的希伯來文字根出現的時候，總離不開安舒的環境和粗心隨便，參，如：耶利米書二十二21的不肯聽從，與箴言一32令人遭災的自滿自大；而箴言的底下一節，則說明了粗心隨便與真正無憂無慮的分別。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，我向你呼求， 恳求你怜悯，说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！我曾向你呼求； 我曾向我主懇求，說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，我曾求告你； 我向耶和華懇求，說："
      },
      "nasb": {
