    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华，以色列的上帝啊， 求你起来惩罚列国， 不要姑息奸诈的恶人。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍的　神耶和華啊！你是以色列的　神， 求你醒來，懲罰萬國； 求你不要恩待那些背信棄義的惡人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之神－耶和華以色列的神啊！ 求你興起，懲治萬邦！ 不要憐憫行詭詐的惡人！（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You, O Lord God of hosts, the God of Israel, Awake to punish all the nations; Do not be gracious to any who are treacherous in iniquity. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You, Lord God Almighty, you who are the God of Israel, rouse yourself to punish all the nations; show no mercy to wicked traitors."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O Lord God of Heaven’s Armies, the God of Israel, wake up and punish those hostile nations. Show no mercy to wicked traitors. Interlude"
      }
    },
