    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.59.13",
            "paragraphs": [
              "59:13",
              "求你發怒，使他們消滅：參35:1~28關於祈求神審判他子民之仇敵的註釋。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你发烈怒毁灭他们， 彻底铲除他们， 使普天下都知道上帝在雅各家掌权。（细拉）"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你在怒中消滅他們， 消滅他們，以致無一倖免， 好使他們知道　神在雅各中間掌權， 直到地極。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "求你發怒，使他們消滅， 以致歸於無有， 叫他們知道神在雅各中間掌權， 直到地極。（細拉）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Destroy them in wrath, destroy them that they may be no more; That men may know that God rules in Jacob To the ends of the earth. Selah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "consume them in your wrath, consume them till they are no more. Then it will be known to the ends of the earth that God rules over Jacob."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Destroy them in your anger! Wipe them out completely! Then the whole world will know that God reigns in Israel. Interlude"
      }
    },
