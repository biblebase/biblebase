    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我没有过错， 他们却准备攻击我。 求你起来帮助我， 顾念我的困境。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我雖然沒有過錯， 他們還是跑來，預備好攻擊我。 求你醒來，幫助我，鑒察我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我雖然無過，他們預備整齊，跑來攻擊我。 求你興起鑒察，幫助我！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For no guilt of mine, they run and set themselves against me. Arouse Yourself to help me, and see!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have done no wrong, yet they are ready to attack me. Arise to help me; look on my plight!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have done nothing wrong, yet they prepare to attack me. Wake up! See what is happening and help me!"
      }
