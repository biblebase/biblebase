    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要对你仇敌的喧嚷置之不理， 与你为敌的人不停地叫嚣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "求你不要忘記你敵人的聲音， 不要忘記那些起來與你為敵的人不住加劇地喧鬧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要忘記你敵人的聲音； 那起來敵你之人的喧嘩時常上升。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not forget the voice of Your adversaries, The uproar of those who rise against You which ascends continually."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not ignore the clamor of your adversaries, the uproar of your enemies, which rises continually."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t overlook what your enemies have said or their growing uproar."
      }
