    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "psa.74.15",
            "paragraphs": [
              "七四15\"使……幹了\"。相同的動詞用來形容紅海（書二10）和約但河（書四23；五1）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你曾开辟泉源和溪流， 也曾使滔滔河水枯干。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你曾開闢泉源和溪流， 又使長流不息的江河乾涸。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你曾分裂磐石，水便成了溪河； 你使長流的江河乾了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You broke open springs and torrents; You dried up ever-flowing streams."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It was you who opened up springs and streams; you dried up the ever-flowing rivers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You caused the springs and streams to gush forth, and you dried up rivers that never run dry."
      }
    },
