    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们纵火焚烧你的圣所， 把它夷为平地， 亵渎了你的居所。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們用火把你的聖所燒成焦土， 他們褻瀆你名的居所。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們用火焚燒你的聖所， 褻瀆你名的居所，拆毀到地。"
      },
