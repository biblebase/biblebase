    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "psa.74.1",
            "paragraphs": [
              "74:1",
              "1~23你為何……丟棄：詩人祈求神不要永遠懲戒他們。這篇禱告也提醒我們，神對罪的寬容忍耐不是無限量的；人終要為自己的罪而遭災哀痛。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "psa.74.1",
            "paragraphs": [
              "七四1\"永遠\"。他們的受苦似是永遠的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊， 你为何永远丢弃了我们？ 你为何对自己草场上的羊大发雷霆？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊！你為甚麼永遠丟棄我們呢？ 為甚麼你的怒氣向你草場上的羊群好像煙冒出呢？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神哪，你为什么永久抛弃了我们呢？ 为什么你的怒气向你牧场的羊群如烟冒出呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，你為何永遠丟棄我們呢？ 你為何向你草場的羊發怒，如煙冒出呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "O God, why do you cast us off forever? Why does your anger smoke against the sheep of your pasture?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O God, why have You rejected us forever? Why does Your anger smoke against the sheep of Your pasture?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "O God, why have you rejected us forever? Why does your anger smolder against the sheep of your pasture?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "O God, why have You cast us off forever? Why does Your anger smoke against the sheep of Your pasture?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "O God, why have you rejected us so long? Why is your anger so intense against the sheep of your own pasture?"
      }
    },
