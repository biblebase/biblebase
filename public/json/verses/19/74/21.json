    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "求你不要让受压迫的人羞愧而去。 愿贫穷困苦的人赞美你的名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要使受欺壓的人蒙羞回去， 要使困苦、貧窮的人讚美你的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要叫受欺壓的人蒙羞回去； 要叫困苦窮乏的人讚美你的名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let not the oppressed return dishonored; Let the afflicted and needy praise Your name."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not let the oppressed retreat in disgrace; may the poor and needy praise your name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t let the downtrodden be humiliated again. Instead, let the poor and needy praise your name."
      }
    },
