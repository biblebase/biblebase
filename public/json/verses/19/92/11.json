    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我亲眼看见我的敌人被击溃， 亲耳听见那些攻击我的恶人被打败。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我親眼看見那些窺伺我的人遭報， 我親耳聽見那些起來攻擊我的惡人受罰。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我的眼睛会看到我的仇敌灭亡， 我的耳朵会听到起来攻击我的作恶者被打散。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我眼睛看見仇敵遭報； 我耳朵聽見那些起來攻擊我的惡人受罰。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "My eyes have seen the downfall of my enemies; my ears have heard the doom of my evil assailants."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And my eye has looked exultantly upon my foes, My ears hear of the evildoers who rise up against me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My eyes have seen the defeat of my adversaries; my ears have heard the rout of my wicked foes."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "My eye also has seen my  desire on my enemies; My ears hear my  desire on the wicked Who rise up against me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My eyes have seen the downfall of my enemies; my ears have heard the defeat of my wicked opponents."
      }
    },
