      "leili": {
        "content": [
          {
            "verse": "psa.92.10",
            "paragraphs": [
              "九二10神高舉義人的\"角\"（力量），並把他奉獻（膏立）作事奉。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你使我强壮如野牛， 你用新油浇灌我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你卻高舉了我的角，像野牛的角； 你用新油膏抹我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你卻高舉了我的角，如野牛的角； 我是被新油膏了的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But You have exalted my horn like that of the wild ox; I have been anointed with fresh oil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You have exalted my horn like that of a wild ox; fine oils have been poured on me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you have made me as strong as a wild ox. You have anointed me with the finest oil."
      }
    },
