    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.21.1-7",
            "paragraphs": [
              "二一1～7由於幾乎所有的便雅憫人都被殺害了（比較二○47），又由於其它支派曾發誓不讓他們的處女嫁給便雅憫人，所以便雅憫支派就面對完全滅絕的危險。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们说：“以色列的上帝耶和华啊，以色列为何少了一个支派呢？为何发生了这样的事呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說：“耶和華以色列的　神啊，今日以色列中缺少了一個支派，為甚麼在以色列中發生這事呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：「耶和華－以色列的神啊，為何以色列中有這樣缺了一支派的事呢？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they said, “O Lord, the God of Israel, why has this happened in Israel, that today there should be one tribe lacking in Israel?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said, “Why, O Lord, God of Israel, has this come about in Israel, so that one tribe should be missing today in Israel?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Lord, God of Israel,” they cried, “why has this happened to Israel? Why should one tribe be missing from Israel today?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and said, “O Lord God of Israel, why has this come to pass in Israel, that today there should be one tribe missing in Israel?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O Lord, God of Israel,” they cried out, “why has this happened in Israel? Now one of our tribes is missing from Israel!”"
      }
    },
