    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.21.19",
            "paragraphs": [
              "二一19\"節期\"。大概是收割葡萄樹時期的住棚節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有人想起在示罗每年都有耶和华的节期。示罗在利波拿以南、伯特利以北至示剑的大路东面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們又說：“看哪，每年在示羅都舉行耶和華的節期；示羅就是在伯特利的北面，從伯特利上示劍的大路的東面，在利波拿的南面。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們又說：「在利波拿以南，伯特利以北，在示劍大路以東的示羅，年年有耶和華的節期」；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they said, “Behold, there is a feast of the Lord from year to year in Shiloh, which is on the north side of Bethel, on the east side of the highway that goes up from Bethel to Shechem, and on the south side of Lebonah.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But look, there is the annual festival of the Lord in Shiloh, which lies north of Bethel, east of the road that goes from Bethel to Shechem, and south of Lebonah.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then they thought of the annual festival of the Lord held in Shiloh, south of Lebonah and north of Bethel, along the east side of the road that goes from Bethel to Shechem."
      }
    },
