    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，二人坐下来一起吃喝。岳父对他说：“再住一夜，高兴高兴吧。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是二人坐下，一起吃喝；女子的父親對那人說：“請你答應再過一夜，暢快你的心。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是二人坐下一同吃喝。女子的父親對那人說：「請你再住一夜，暢快你的心。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So the two of them sat and ate and drank together. And the girl’s father said to the man, “Be pleased to spend the night, and let your heart be merry.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So both of them sat down and ate and drank together; and the girl’s father said to the man, “Please be willing to spend the night, and let your heart be merry.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the two of them sat down to eat and drink together. Afterward the woman’s father said, “Please stay tonight and enjoy yourself.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So they sat down, and the two of them ate and drank together. Then the young woman’s father said to the man, “Please be content to stay all night, and let your heart be merry.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the two men sat down together and had something to eat and drink. Then the woman’s father said, “Please stay another night and enjoy yourself.”"
      }
    },
