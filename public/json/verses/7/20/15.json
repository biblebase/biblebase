      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "便雅悯人从各城召集了两万六千刀兵，还从基比亚召集了七百精兵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時便雅憫人，從各城裡被數點的，共有二萬六千拿刀的，另外被數點的，還有基比亞的居民，共有七百精兵。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時便雅憫人從各城裏點出拿刀的，共有二萬六千；另外還有基比亞人點出七百精兵。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the cities on that day the sons of Benjamin were numbered, 26,000 men who draw the sword, besides the inhabitants of Gibeah who were numbered, 700 choice men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At once the Benjamites mobilized twenty-six thousand swordsmen from their towns, in addition to seven hundred able young men from those living in Gibeah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In all, 26,000 of their warriors armed with swords arrived in Gibeah to join the 700 elite troops who lived there."
      }
