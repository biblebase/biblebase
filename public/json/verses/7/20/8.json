    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人起来异口同声地说：“我们谁也不回帐篷，谁也不回家。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是眾人都起來如同一人，說：“我們當中誰都不回自己的帳棚，誰都不回自己的家。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾民都起來如同一人，說：「我們連一人都不回自己帳棚、自己房屋去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then all the people arose as one man, saying, “Not one of us will go to his tent, nor will any of us return to his house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the men rose up together as one, saying, “None of us will go home. No, not one of us will return to his house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And all the people rose to their feet in unison and declared, “None of us will return home! No, not even one of us!"
      }
