    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jdg.15.7",
            "paragraphs": [
              "15:7",
              "我必向你們報仇：參孫攻擊非利士人的主要動機是泄怒和報私仇，而不是為了按神的旨意去拯救希伯來人。這種自私和對神不忠的做法最終導致了他的毀滅(16:1,20~21)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "参孙说：“你们既然这样做，我不向你们报仇誓不罢休。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "參孫對他們說：“你們既然這樣行，我必在你們身上報仇，然後才肯罷休。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "參孫對非利士人說：「你們既然這樣行，我必向你們報仇才肯罷休。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Samson said to them, “Since you act like this, I will surely take revenge on you, but after that I will quit.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Samson said to them, “Since you’ve acted like this, I swear that I won’t stop until I get my revenge on you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Because you did this,” Samson vowed, “I won’t rest until I take my revenge on you!”"
      }
    },
