    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并对他说：“我以为你非常恨她，所以我把她改嫁给你的伴郎了。她妹子不是比她更美丽吗？你可以娶来代替她。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她父親說：“我以為你非常恨她，所以我把她給了你的一個同伴；她的妹妹不是比她更美麗嗎？你可以娶來代替她啊。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：「我估定你是極其恨她，因此我將她給了你的陪伴。她的妹子不是比她還美麗嗎？你可以娶來代替她吧！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Her father said, “I really thought that you hated her intensely; so I gave her to your companion. Is not her younger sister more beautiful than she? Please let her be yours instead.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I was so sure you hated her,” he said, “that I gave her to your companion. Isn’t her younger sister more attractive? Take her instead.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I truly thought you must hate her,” her father explained, “so I gave her in marriage to your best man. But look, her younger sister is even more beautiful than she is. Marry her instead.”"
      }
    },
