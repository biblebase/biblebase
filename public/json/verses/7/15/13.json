    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们说：“我们不会杀你，只将你绑起来交给非利士人。”于是，他们用两根新绳子把参孙捆绑起来带出岩洞。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們告訴他說：“我們不會殺害你，我們只要把你捆綁住，交在非利士人手裡，我們決不殺死你。”於是他們用兩條新繩子把參孫捆綁起來，從磐石的穴裡把他帶上來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們說：「我們斷不殺你，只要將你捆綁交在非利士人手中。」於是用兩條新繩捆綁參孫，將他從以坦磐帶上去。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "They said to him, “No; we will only bind you and give you into their hands. We will surely not kill you.” So they bound him with two new ropes and brought him up from the rock."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they said to him, “No, but we will bind you fast and give you into their hands; yet surely we will not kill you.” Then they bound him with two new ropes and brought him up from the rock."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Agreed,” they answered. “We will only tie you up and hand you over to them. We will not kill you.” So they bound him with two new ropes and led him up from the rock."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So they spoke to him, saying, “No, but we will tie you securely and deliver you into their hand; but we will surely not kill you.” And they bound him with two new ropes and brought him up from the rock."
      },
