    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.13.18",
            "paragraphs": [
              "一三18\"我名是奇妙的\"。\"奇妙\"一詞源於一個希伯來文字根，意思是\"超越\"或\"不可言喻的\"。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的天使说：“何必问我的名字呢？我的名字奇妙难测。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的使者對他說：“你為甚麼問我的名字呢？我的名是奇妙的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的使者對他說：「你何必問我的名，我名是奇妙的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the angel of the Lord said to him, “Why do you ask my name, seeing it is wonderful?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He replied, “Why do you ask my name? It is beyond understanding.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why do you ask my name?” the angel of the Lord replied. “It is too wonderful for you to understand.”"
      }
