    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基甸问西巴和撒慕拿：“你们在他泊山杀的人是什么样子？”他们回答说：“他们长得像你，都像王子一样。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基甸問西巴和撒慕拿：“你們在他泊山上所殺的人，是甚麼模樣的人？”他們回答：“他們像你一樣，每一個都像王子的模樣。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基甸問西巴和撒慕拿說：「你們在他泊山所殺的人是甚麼樣式？」回答說：「他們好像你，各人都有王子的樣式。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he said to Zebah and Zalmunna, “What kind of men were they whom you killed at Tabor?” And they said, “They were like you, each one resembling the son of a king.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he asked Zebah and Zalmunna, “What kind of men did you kill at Tabor?” “Men like you,” they answered, “each one with the bearing of a prince.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Gideon asked Zebah and Zalmunna, “The men you killed at Tabor—what were they like?” “Like you,” they replied. “They all had the look of a king’s son.”"
      }
    },
