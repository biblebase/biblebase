    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.8.1-3",
            "paragraphs": [
              "八1～3以法蓮人抱怨他們沒有參與最初打敗米甸人的戰鬥。基甸溫和的回答--指出他們已經捉拿了米甸人的兩個首領（七24,25）--使他們平靜下來。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝把两名米甸人的首领俄立和西伊伯交在了你们手中，我所做的有哪一件能与这事相比呢？我怎能比得上你们呢？”基甸这么一说，他们的怒气就平息了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神已經把米甸人的兩個首領：俄立和西伊伯，交在你們的手裡了；我所行的怎能與你們相比呢？”基甸說了這話，他們的怒氣就平息了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神已將米甸人的兩個首領俄立和西伊伯交在你們手中；我所行的豈能比你們所行的呢？」基甸說了這話，以法蓮人的怒氣就消了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God has given the leaders of Midian, Oreb and Zeeb into your hands; and what was I able to do in comparison with you?” Then their anger toward him subsided when he said that."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God gave Oreb and Zeeb, the Midianite leaders, into your hands. What was I able to do compared to you?” At this, their resentment against him subsided."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God gave you victory over Oreb and Zeeb, the commanders of the Midianite army. What have I accomplished compared to that?” When the men of Ephraim heard Gideon’s answer, their anger subsided."
      }
    },
