    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.8.8",
            "paragraphs": [
              "八8\"毗努伊勒\"在疏割以東四英裏（6.4公裏）。這些人也想保持中立。參看創世記三十二章30節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "基甸又去毗努伊勒提出同样的请求，也遭到拒绝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "基甸從那裡上到毘努伊勒，也向他們說了同樣的話；毘努伊勒人回答他也像疏割人回答的一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "基甸從那裏上到毗努伊勒，對那裏的人也是這樣說；毗努伊勒人也與疏割人回答他的話一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He went up from there to Penuel and spoke similarly to them; and the men of Penuel answered him just as the men of Succoth had answered."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From there he went up to Peniel and made the same request of them, but they answered as the men of Sukkoth had."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From there Gideon went up to Peniel and again asked for food, but he got the same answer."
      }
    },
