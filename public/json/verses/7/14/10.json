    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.14.10",
            "paragraphs": [
              "一四10\"筵宴\"。為期七天（12節），期間會喝酒，參孫那樣做也是有違他作為拿細耳人的身分（一三7）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "参孙的父亲去那女子家，参孙按习俗摆设宴席。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "參孫的父親下去見那女子，參孫在那裡擺設了筵席，因為青年人都慣常這樣行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他父親下去見女子。參孫在那裏設擺筵宴，因為向來少年人都有這個規矩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then his father went down to the woman; and Samson made a feast there, for the young men customarily did this."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now his father went down to see the woman. And there Samson held a feast, as was customary for young men."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As his father was making final arrangements for the marriage, Samson threw a party at Timnah, as was the custom for elite young men."
      }
    },
