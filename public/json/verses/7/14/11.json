    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.14.11",
            "paragraphs": [
              "一四11\"三十個\"陪伴的人。陪伴新郎的人（太九15），他們負責防止任何人進行幹擾。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人看见参孙，就安排了三十个人陪他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人看見參孫，就請了三十個人陪伴他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人看見參孫，就請了三十個人陪伴他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they saw him, they brought thirty companions to be with him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the people saw him, they chose thirty men to be his companions."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the bride’s parents saw him, they selected thirty young men from the town to be his companions."
      }
    },
