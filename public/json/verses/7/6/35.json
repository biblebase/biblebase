    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他派人走遍玛拿西、亚设、西布伦和拿弗他利各支派召集军队，得到了人们的响应。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他派使者到瑪拿西全地去，瑪拿西人也都應召來跟從他；他又派使者到亞設、西布倫、拿弗他利去，他們也都上來與他們會合。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他打發人走遍瑪拿西地，瑪拿西人也聚集跟隨他；又打發人去見亞設人、西布倫人、拿弗他利人，他們也都出來與他們會合。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He sent messengers throughout Manasseh, and they also were called together to follow him; and he sent messengers to Asher, Zebulun, and Naphtali, and they came up to meet them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He sent messengers throughout Manasseh, calling them to arms, and also into Asher, Zebulun and Naphtali, so that they too went up to meet them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He also sent messengers throughout Manasseh, Asher, Zebulun, and Naphtali, summoning their warriors, and all of them responded."
      }
    },
