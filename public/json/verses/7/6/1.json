    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jdg.6.1",
            "paragraphs": [
              "6:1",
              "以色列人又行……惡：以色列人又一次轉回那種信仰混雜的宗教，接受了迦南人的生活方式。其結果是在此後的七年時間裏，神允許米甸人在亞瑪力人和東方部落的協助下，進攻並欺壓以色列人(6:3)。以色列人被迫躲藏在山洞中，隱藏他們的糧食(2~5節)，最後在他們無法忍受的情況下，才開始向耶和華呼求(6節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jdg.6.1",
            "paragraphs": [
              "六1\"米甸人\"。參看出埃及記二章15節和民數記三十一章2節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来，以色列人又做耶和华视为恶的事，耶和华就把他们交在米甸人手中，受他们统治七年。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人又行了耶和華看為惡的事；耶和華就把他們交在米甸人手裡七年。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人又行耶和華眼中看為惡的事，耶和華就把他們交在米甸人手裏七年。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the sons of Israel did what was evil in the sight of the Lord; and the Lord gave them into the hands of Midian seven years."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Israelites did evil in the eyes of the Lord, and for seven years he gave them into the hands of the Midianites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Israelites did evil in the Lord’s sight. So the Lord handed them over to the Midianites for seven years."
      }
