    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "果然如此，基甸一早醒来，从羊毛中挤出满满一碗露水。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "第二天，基甸清早起來，情形果然是這樣；他把羊毛一擠，就從羊毛中擠出一滿盆的露水來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "次日早晨基甸起來，見果然是這樣；將羊毛擠一擠，從羊毛中擰出滿盆的露水來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And it was so. When he arose early the next morning and squeezed the fleece, he drained the dew from the fleece, a bowl full of water."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And that is what happened. Gideon rose early the next day; he squeezed the fleece and wrung out the dew—a bowlful of water."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And that is just what happened. When Gideon got up early the next morning, he squeezed the fleece and wrung out a whole bowlful of water."
      }
    },
