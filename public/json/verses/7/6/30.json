      "leili": {
        "content": [
          {
            "verse": "jdg.6.30",
            "paragraphs": [
              "六30這些人極度沈迷於拜偶像，以至他們要把那\"拆毀了巴力的壇\"的人治死。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "便对约阿施说：“把你儿子交出来！他拆毁了巴力的祭坛，砍倒了坛旁的亚舍拉神像，我们要处死他！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "城裡的人對約阿施說：“把你的兒子領出來，把他處死，因為他拆毀了巴力的祭壇，砍下了壇旁的亞舍拉。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "城裏的人對約阿施說：「將你兒子交出來，好治死他；因為他拆毀了巴力的壇，砍下壇旁的木偶。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the men of the city said to Joash, “Bring out your son, that he may die, for he has torn down the altar of Baal, and indeed, he has cut down the Asherah which was beside it.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The people of the town demanded of Joash, “Bring out your son. He must die, because he has broken down Baal’s altar and cut down the Asherah pole beside it.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Bring out your son,” the men of the town demanded of Joash. “He must die for destroying the altar of Baal and for cutting down the Asherah pole.”"
      }
    },
