    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我曾对你们说，‘我是你们的上帝耶和华。你们虽然住在亚摩利人的地方，但不可祭拜他们的神明。’你们却充耳不闻。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我又對你們說：我是耶和華你們的　神；你們住在亞摩利人的地，不可敬畏他們的神。但你們沒有聽從我的話。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又對你們說：『我是耶和華－你們的神。你們住在亞摩利人的地，不可敬畏他們的神。你們竟不聽從我的話。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and I said to you, “I am the Lord your God; you shall not fear the gods of the Amorites in whose land you live. But you have not obeyed Me.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I said to you, ‘I am the Lord your God; do not worship the gods of the Amorites, in whose land you live.’ But you have not listened to me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I told you, ‘I am the Lord your God. You must not worship the gods of the Amorites, in whose land you now live.’ But you have not listened to me.”"
      }
