    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jdg.3.6",
            "paragraphs": [
              "3:6",
              "娶他們的女兒為妻：兩種民族的文化因著以色列人與迦南人的通婚而摻雜在一起，這幾乎敗壞了他們屬神的特有身份。以色列人與耶和華所立的聖約中禁止這種通婚行為(出34:15~16；申7:3~4；書23:12~13)。士師記向我們敘述了以色列人因妥協而帶來的災難性後果。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "跟他们通婚，祭拜他们的神明。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們娶他們的女兒作妻子，也把自己的女兒嫁給他們的兒子，並且事奉他們的神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "娶他們的女兒為妻，將自己的女兒嫁給他們的兒子，並事奉他們的神。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they took their daughters for themselves as wives, and gave their own daughters to their sons, and served their gods."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They took their daughters in marriage and gave their own daughters to their sons, and served their gods."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and they intermarried with them. Israelite sons married their daughters, and Israelite daughters were given in marriage to their sons. And the Israelites served their gods."
      }
