    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华向他们发怒，使他们遭到掠夺者的攻击和四周仇敌的压制，毫无抵抗之力。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的怒氣向以色列人發作，就把他們交在搶掠的人手中，他們就搶掠他們；他又把他們完全交在他們四圍的仇敵手中，以致他們在仇敵面前再不能站立得住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的怒氣向以色列人發作，就把他們交在搶奪他們的人手中，又將他們付與四圍仇敵的手中，甚至他們在仇敵面前再不能站立得住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The anger of the Lord burned against Israel, and He gave them into the hands of plunderers who plundered them; and He sold them into the hands of their enemies around them, so that they could no longer stand before their enemies."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In his anger against Israel the Lord gave them into the hands of raiders who plundered them. He sold them into the hands of their enemies all around, whom they were no longer able to resist."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This made the Lord burn with anger against Israel, so he handed them over to raiders who stole their possessions. He turned them over to their enemies all around, and they were no longer able to resist them."
      }
    },
