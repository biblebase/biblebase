    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这就是为什么耶和华留下那些外族人，没有立刻把他们赶走，也没有把他们交在约书亚手中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，耶和華留下這些國的民，不迅速把他們趕走，也不把他們交在約書亞手裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣耶和華留下各族，不將他們速速趕出，也沒有交付約書亞的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the Lord allowed those nations to remain, not driving them out quickly; and He did not give them into the hand of Joshua."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord had allowed those nations to remain; he did not drive them out at once by giving them into the hands of Joshua."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That is why the Lord left those nations in place. He did not quickly drive them out or allow Joshua to conquer them all."
      }
    },
