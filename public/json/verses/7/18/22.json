    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.18.22-26",
            "paragraphs": [
              "一八22～26米迦與幾個朋友追趕但支派的人，希望取回敬拜的物品和他的祭司。然而，當但支派的人揚言他們的性命受到威脅，於是他們也只好回家了。留意米迦已經崩潰的屬靈狀況。他抱怨但支派的人取去他的神像時，說但支派的人把他的所有都帶走了（24節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们离开米迦的家一段路后，米迦召集邻居赶了上来，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們離開米迦的家很遠的時候，在米迦的家附近各家的人，都被召集出來，去追趕但人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "離米迦的住宅已遠，米迦的近鄰都聚集來，追趕但人，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they had gone some distance from the house of Micah, the men who were in the houses near Micah’s house assembled and overtook the sons of Dan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they had gone some distance from Micah’s house, the men who lived near Micah were called together and overtook the Danites."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the people from the tribe of Dan were quite a distance from Micah’s house, the people who lived near Micah came chasing after them."
      }
    },
