    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那五个人就进入米迦的家里，到年轻的利未人房间向他问安，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他們五人轉身，進入米迦的家，來到那青年利未人的屋裡，向他問安。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "五人就進入米迦的住宅，到了那少年利未人的房內問他好。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They turned aside there and came to the house of the young man, the Levite, to the house of Micah, and asked him of his welfare."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So they turned in there and went to the house of the young Levite at Micah’s place and greeted him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the five men turned off the road and went over to Micah’s house, where the young Levite lived, and greeted him kindly."
      }
