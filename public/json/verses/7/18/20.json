    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jdg.18.20",
            "paragraphs": [
              "18:20",
              "祭司心裏喜悅：本書第18~19章突出反映了士師時期祭司的生活，以此強調指出：當時不僅百姓，甚至連神聖的神職人員都一同敗壞墮落了。這個祭司僅僅為了金錢和地位就甘心情願地成為事奉和敬拜外邦假神的祭司(17:12)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祭司听了非常高兴，就带着以弗得、家庭神像及雕刻的偶像跟随了他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司心裡高興，就拿著以弗得、神像和雕像，來到眾人中間。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司心裏喜悅，便拿着以弗得和家中的神像，並雕刻的像，進入他們中間。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The priest’s heart was glad, and he took the ephod and household idols and the graven image and went among the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priest was very pleased. He took the ephod, the household gods and the idol and went along with the people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The young priest was quite happy to go with them, so he took along the sacred ephod, the household idols, and the carved image."
      }
    },
