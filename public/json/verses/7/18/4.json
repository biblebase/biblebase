    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那青年便把米迦聘他做祭司的事告诉了他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他回答他們：“米迦如此這般待我；他雇了我，我就作了他的祭司。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他回答說：「米迦待我如此如此，請我作祭司。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to them, “Thus and so has Micah done to me, and he has hired me and I have become his priest.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He told them what Micah had done for him, and said, “He has hired me and I am his priest.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He told them about his agreement with Micah and that he had been hired as Micah’s personal priest."
      }
    },
