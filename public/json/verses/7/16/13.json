    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jdg.16.13-14",
            "paragraphs": [
              "一六13,14由於參孫的頭躺在大利拉膝上，所以她很容易把他的頭發編成發綹。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大利拉对参孙说：“到现在你还是说谎捉弄我。请你告诉我，用什么方法才能绑住你？”参孙说：“你把我的七根发辨和织布机的线编在一起，用橛子钉牢就可以了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大利拉對參孫說：“到現在你還是作弄我，向我說謊；求你告訴我，人怎樣才能捆綁你。”參孫對她說：“如果你把我頭上的七條髮辮與織布機上的緯線編織在一起就可以了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大利拉對參孫說：「你到如今還是欺哄我，向我說謊言。求你告訴我，當用何法捆綁你。」參孫回答說：「你若將我頭上的七條髮綹，與緯線同織就可以了。」"
      },
      "nasb": {
