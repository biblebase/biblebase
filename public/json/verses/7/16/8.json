    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "非利士人的首领便把七根还没干的青绳子交给大利拉，她用那些绳子把参孙绑起来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是非利士人的領袖把七條未乾的青繩子，帶上來給那婦人，她就用那些繩子把參孫捆綁起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是非利士人的首領拿了七條未乾的青繩子來，交給婦人，她就用繩子捆綁參孫。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the lords of the Philistines brought up to her seven fresh cords that had not been dried, and she bound him with them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the rulers of the Philistines brought her seven fresh bowstrings that had not been dried, and she tied him with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the Philistine rulers brought Delilah seven new bowstrings, and she tied Samson up with them."
      }
    },
