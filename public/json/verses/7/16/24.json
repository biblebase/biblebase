    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人看见参孙，就赞美他们的神明，说：“我们的神把仇敌交在我们手中了，他曾毁坏我们的田地，杀死我们许多人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人看見了參孫，就讚美他們的神，說：“我們的神把我們的仇敵，就是那毀壞我們的地，又殺死我們很多人的，交在我們手中了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人看見參孫，就讚美他們的神說：「我們的神將毀壞我們地、殺害我們許多人的仇敵交在我們手中了。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the people saw him, they praised their god, for they said, “Our god has given our enemy into our hands, Even the destroyer of our country, Who has slain many of us.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the people saw him, they praised their god, saying, “Our god has delivered our enemy into our hands, the one who laid waste our land and multiplied our slain.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the people saw him, they praised their god, saying, “Our god has delivered our enemy to us! The one who killed so many of us is now in our power!”"
      }
    },
