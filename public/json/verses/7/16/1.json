      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "參孫到了迦薩，在那裏看見一個妓女，就與她親近。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Samson went to Gaza and saw a harlot there, and went in to her."
      },
