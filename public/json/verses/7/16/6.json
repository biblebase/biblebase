    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，大利拉问参孙：“求你告诉我，你为什么有这么大力气？用什么办法才能把你绑起来制服你？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大利拉對參孫說：“求你告訴我，你因甚麼緣故有這樣大的力氣，要用甚麼方法才能捆綁你，好把你制伏。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大利拉對參孫說：「求你告訴我，你因何有這麼大的力氣，當用何法捆綁剋制你。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So Delilah said to Samson, “Please tell me where your great strength lies, and how you might be bound, that one could subdue you.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Delilah said to Samson, “Please tell me where your great strength is and how you may be bound to afflict you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Delilah said to Samson, “Tell me the secret of your great strength and how you can be tied up and subdued.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So Delilah said to Samson, “Please tell me where your great strength lies, and with what you may be bound to afflict you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Delilah said to Samson, “Please tell me what makes you so strong and what it would take to tie you up securely.”"
      }
    },
