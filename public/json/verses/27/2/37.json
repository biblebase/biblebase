      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王啊，你是諸王之王。天上的神已將國度、權柄、能力、尊榮都賜給你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You, O king, are the king of kings, to whom the God of heaven has given the kingdom, the power, the strength and the glory;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your Majesty, you are the king of kings. The God of heaven has given you dominion and power and might and glory;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your Majesty, you are the greatest of kings. The God of heaven has given you sovereignty, power, strength, and honor."
      }
