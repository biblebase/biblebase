      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們第二次對王說：「請王將夢告訴僕人，僕人就可以講解。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
