    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他势力强大，却不是靠自己的力量。他必带来可怕的毁灭，而且凡事亨通，他必毁灭强者和圣民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的權勢強大，卻不是出於自己的能力； 他必造成驚人的毀滅； 他任意而行，凡事順利； 他必毀滅有強力的， 又毀滅聖民。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的權柄必大，卻不是因自己的能力；他必行非常的毀滅，事情順利，任意而行；又必毀滅有能力的和聖民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His power will be mighty, but not by his own power, And he will destroy to an extraordinary degree And prosper and perform his will; He will destroy mighty men and the holy people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will become very strong, but not by his own power. He will cause astounding devastation and will succeed in whatever he does. He will destroy those who are mighty, the holy people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He will become very strong, but not by his own power. He will cause a shocking amount of destruction and succeed in everything he does. He will destroy powerful leaders and devastate the holy people."
      }
    },
