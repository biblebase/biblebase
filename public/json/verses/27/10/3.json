    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "期间没有吃过美味，没有尝过酒肉，没有抹过膏油。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "美味的食物我沒有吃，肉和酒沒有入我的口，我也沒有用膏油抹身，整整三個星期。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "美味我沒有吃，酒肉沒有入我的口，也沒有用油抹我的身，直到滿了三個七日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I did not eat any tasty food, nor did meat or wine enter my mouth, nor did I use any ointment at all until the entire three weeks were completed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I ate no choice food; no meat or wine touched my lips; and I used no lotions at all until the three weeks were over."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All that time I had eaten no rich food. No meat or wine crossed my lips, and I used no fragrant lotions until those three weeks had passed."
      }
