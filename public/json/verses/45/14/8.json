    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们活是为主而活，死是为主而死。因此，我们无论生死都是属主的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們若死了，是為主而死。所以，我們無論活著或是死了，總是屬於主的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們若活着，是為主而活；若死了，是為主而死。所以，我們或活或死總是主的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "for if we live, we live for the Lord, or if we die, we die for the Lord; therefore whether we live or die, we are the Lord’s."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If we live, we live for the Lord; and if we die, we die for the Lord. So, whether we live or die, we belong to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If we live, it’s to honor the Lord. And if we die, it’s to honor the Lord. So whether we live or die, we belong to the Lord."
      }
    },
