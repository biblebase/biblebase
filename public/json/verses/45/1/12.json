    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "也可以说是借着我们彼此的信心互相激励。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也使我在你們中間，藉著你我彼此的信心，大家一同得到安慰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，我在你們中間，因你與我彼此的信心，就可以同得安慰。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that is, that I may be encouraged together with you while among you, each of us by the other’s faith, both yours and mine."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "that is, that you and I may be mutually encouraged by each other’s faith."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When we get together, I want to encourage you in your faith, but I also want to be encouraged by yours."
      }
