    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他率领部下袭击基述人、基色人和亚玛力人，这些人自古以来就住在远至书珥和埃及一带的地方。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛和跟隨他的人上去襲擊基述人、基色人和亞瑪力人，這些人自古以來就是這地的居民，居於書珥到埃及地之間。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛和跟隨他的人上去，侵奪基述人、基色人、亞瑪力人之地。這幾族歷來住在那地，從書珥直到埃及。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now David and his men went up and raided the Geshurites and the Girzites and the Amalekites; for they were the inhabitants of the land from ancient times, as you come to Shur even as far as the land of Egypt."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now David and his men went up and raided the Geshurites, the Girzites and the Amalekites. (From ancient times these peoples had lived in the land extending to Shur and Egypt.)"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "David and his men spent their time raiding the Geshurites, the Girzites, and the Amalekites—people who had lived near Shur, toward the land of Egypt, since ancient times."
      }
