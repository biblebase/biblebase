    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撒母耳渐渐长大，耶和华与他同在，使他所说的话没有一句落空。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒母耳漸漸長大，耶和華與他同在，使他所說的一切話，沒有一句落空。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "撒母耳長大了，耶和華與他同在，使他所說的話一句都不落空。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus Samuel grew and the Lord was with him and let none of his words fail."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord was with Samuel as he grew up, and he let none of Samuel’s words fall to the ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As Samuel grew up, the Lord was with him, and everything Samuel said proved to be reliable."
      }
