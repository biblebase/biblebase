      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，时候将到，我要终结你和你家族的力量，你们家族必没有一个老人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "日子快到了，我要砍下你和你父親的膀臂，使你家中再沒有有權勢的長者。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "日子必到，我要折斷你的膀臂和你父家的膀臂，使你家中沒有一個老年人。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Behold, the days are coming when I will cut off your strength and the strength of your father’s house, so that there will not be an old man in your house."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, the days are coming when I will break your strength and the strength of your father’s house so that there will not be an old man in your house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The time is coming when I will cut short your strength and the strength of your priestly house, so that no one in it will reach old age,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Behold, the days are coming that I will cut off your arm and the arm of your father’s house, so that there will not be an old man in your house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The time is coming when I will put an end to your family, so it will no longer serve as my priests. All the members of your family will die before their time. None will reach old age."
      }
