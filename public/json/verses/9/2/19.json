      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒母耳的母親每年為他做一件小外袍，她和丈夫一同上來獻年祭的時候，就帶來給他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他母親每年為他做一件小外袍，同着丈夫上來獻年祭的時候帶來給他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And his mother would make him a little robe and bring it to him from year to year when she would come up with her husband to offer the yearly sacrifice."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Each year his mother made him a little robe and took it to him when she went up with her husband to offer the annual sacrifice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each year his mother made a small coat for him and brought it to him when she came with her husband for the sacrifice."
      }
