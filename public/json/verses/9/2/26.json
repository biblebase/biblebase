    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.2.11-36",
            "paragraphs": [
              "iii.撒母耳面對示羅的敗落（二11～36）"
            ]
          },
          {
            "verse": "1sa.2.26",
            "paragraphs": [
              "26.耶和華與人愈發喜愛他，對撒母耳的成長所作的這個註解，也為著孩童耶穌的成長，提供了描述的方式（路二52）；祂與撒母耳一樣在邪惡的世界中，認定了神的道路，並且抵擋試探。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "小撒母耳渐渐长大，深受耶和华和民众的喜爱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那孩子撒母耳的身量，耶和華和眾人對他的喜愛，都不斷增長。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "孩子撒母耳漸漸長大，耶和華與人越發喜愛他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the boy Samuel was growing in stature and in favor both with the Lord and with men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the boy Samuel continued to grow in stature and in favor with the Lord and with people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Meanwhile, the boy Samuel grew taller and grew in favor with the Lord and with the people."
      }
