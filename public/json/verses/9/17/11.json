    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.17.8-11",
            "paragraphs": [
              "8～11.巨人對他優越的裝備及高大的身材充滿信心，向以色列人挑釁，要他們找出一個可堪匹配的人與他單打獨鬥。他如此有把握贏得戰鬥，甚至拿同胞當賭註，說他若輸了，他們就作以色列人的奴隸；然而當發生的事未如他所料，以色列人得勝時，非利士人並沒有服事以色列人。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗和所有以色列人听了非利士人的这些话，都吓得惊恐不已。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅和以色列眾人聽見了那非利士人所說的這些話，就都驚惶，非常害怕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅和以色列眾人聽見非利士人的這些話，就驚惶，極其害怕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Saul and all Israel heard these words of the Philistine, they were dismayed and greatly afraid."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On hearing the Philistine’s words, Saul and all the Israelites were dismayed and terrified."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Saul and the Israelites heard this, they were terrified and deeply shaken."
      }
    },
