    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.17.28-30",
            "paragraphs": [
              "28～30.以利押憤怒地責備這位「來自老家的男孩」，表現他年長的氣慨，這是可以理解的。以利押氣憤他這位最年輕弟弟，因為他竟然擅自進入軍事區，還暗示他可以替代他們。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他又去问其他人同样的问题，得到了同样的答复。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛離開他的哥哥，轉向另一個人，問同樣的問題。眾民還是用先前的話回答他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛就離開他轉向別人，照先前的話而問；百姓仍照先前的話回答他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he turned away from him to another and said the same thing; and the people answered the same thing as before."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He then turned away to someone else and brought up the same matter, and the men answered him as before."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He walked over to some others and asked them the same thing and received the same answer."
      }
    },
