    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.7.3-17",
            "paragraphs": [
              "iv.在米斯巴悔改並重新委身（七3～17）"
            ]
          },
          {
            "verse": "1sa.7.7",
            "paragraphs": [
              "7.米斯巴這名字的意義是「守望塔」──在軍事上是個有利的地點，從遠方就可以看見它。非利士人聽見一大群以色列人聚集，以為以色列人很快就要發動攻擊，所以就主動上來攻擊，想在戰爭中制敵機先（參：士二十3a、14）。對以色列人而言，這是考驗的時刻，他們在先前的會戰中曾經遭受重大的挫敗。（撒上四10～11）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "非利士人听说以色列人在米斯巴聚集，他们的首领就率领军队来攻打以色列人。以色列人听见消息后，非常害怕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "非利士人聽見以色列人集合在米斯巴，各城的領袖就上來攻打以色列人；以色列人聽見了，都害怕起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "非利士人聽見以色列人聚集在米斯巴，非利士的首領就上來要攻擊以色列人。以色列人聽見，就懼怕非利士人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when the Philistines heard that the sons of Israel had gathered to Mizpah, the lords of the Philistines went up against Israel. And when the sons of Israel heard it, they were afraid of the Philistines."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the Philistines heard that Israel had assembled at Mizpah, the rulers of the Philistines came up to attack them. When the Israelites heard of it, they were afraid because of the Philistines."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the Philistine rulers heard that Israel had gathered at Mizpah, they mobilized their army and advanced. The Israelites were badly frightened when they learned that the Philistines were approaching."
      }
    },
