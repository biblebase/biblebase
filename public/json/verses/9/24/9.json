      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David said to Saul, “Why do you listen to the words of men, saying, ‘Behold, David seeks to harm you’?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to Saul, “Why do you listen when men say, ‘David is bent on harming you’?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he shouted to Saul, “Why do you listen to the people who say I am trying to harm you?"
      }
