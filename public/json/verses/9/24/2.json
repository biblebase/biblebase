      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅就從以色列人中選出三千精兵，帶領他們到野羊磐石去搜索大衛和跟隨他的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅就從以色列人中挑選三千精兵，率領他們往野羊的磐石去，尋索大衛和跟隨他的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Saul took three thousand chosen men from all Israel and went to seek David and his men in front of the Rocks of the Wild Goats."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Saul took three thousand able young men from all Israel and set out to look for David and his men near the Crags of the Wild Goats."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Saul chose 3,000 elite troops from all Israel and went to search for David and his men near the rocks of the wild goats."
      }
