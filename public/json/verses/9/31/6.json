      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，扫罗和他三个儿子，以及为他拿兵器的人和他的军队都死了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣，掃羅和他的三個兒子，以及替他拿兵器的人，還有所有跟隨他的人，都在那一天一同死了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，掃羅和他三個兒子，與拿他兵器的人，以及跟隨他的人，都一同死亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus Saul died with his three sons, his armor bearer, and all his men on that day together."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Saul and his three sons and his armor-bearer and all his men died together that same day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Saul, his three sons, his armor bearer, and his troops all died together that same day."
      }
