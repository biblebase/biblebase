    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对拿八说：“愿你和你全家平安，愿你一切顺利！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要這樣說：‘願你長壽！願你平安！願你的家平安！願你所有的一切都平安！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要對那富戶如此說：『願你平安，願你家平安，願你一切所有的都平安。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and thus you shall say, ‘Have a long life, peace be to you, and peace be to your house, and peace be to all that you have."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Say to him: ‘Long life to you! Good health to you and your household! And good health to all that is yours!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Peace and prosperity to you, your family, and everything you own!"
      }
