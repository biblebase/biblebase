    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "迦得先知吩咐大卫离开堡垒回犹大去，大卫就动身前往哈列的树林。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "迦得先知對大衛說：“你不要留在山寨，要到猶大地去。”大衛就離開那裡，往哈列的森林去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "先知迦得對大衛說：「你不要住在山寨，要往猶大地去。」大衛就離開那裏，進入哈列的樹林。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The prophet Gad said to David, “Do not stay in the stronghold; depart, and go into the land of Judah.” So David departed and went into the forest of Hereth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the prophet Gad said to David, “Do not stay in the stronghold. Go into the land of Judah.” So David left and went to the forest of Hereth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One day the prophet Gad told David, “Leave the stronghold and return to the land of Judah.” So David went to the forest of Hereth."
      }
