    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.22.6-23",
            "paragraphs": [
              "vi.幫助大衛的代價（二十二6～23）"
            ]
          },
          {
            "verse": "1sa.22.20-23",
            "paragraphs": [
              "20～23.在神的眷顧之下，亞希米勒的一個兒子亞比亞他躲過了大屠殺，把以弗得搶救出來（撒上二十三6），逃去投奔大衛，大衛因而能夠得著祭司的神諭，而掃羅卻使自己全然不能得著這類的幫助。碩果僅存的祭司與命定的君王一起被迫藏匿，彼此互相扶持，在大衛作王的整個期間，他們的友誼未曾中斷（亦參：王上二26～27）。大衛變成了祭司職分的保護者。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "1sa.22.23",
            "paragraphs": [
              "二二23\"得保全\"指安全。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你就放心留在我这里，不要害怕，谁想杀你，就是想杀我。你在我这里很安全。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你可以住在我這裡。不要懼怕，因為那尋索你命的，正在尋索我的命，你與我在一起就可保安全。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你可以住在我這裏，不要懼怕。因為尋索你命的就是尋索我的命；你在我這裏可得保全。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Stay with me; do not be afraid, for he who seeks my life seeks your life. With me you shall be in safekeeping.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Stay with me; do not be afraid, for he who seeks my life seeks your life, for you are safe with me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Stay with me; don’t be afraid. The man who wants to kill you is trying to kill me too. You will be safe with me.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Stay with me; do not fear. For he who seeks my life seeks your life, but with me you shall  be safe.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Stay here with me, and don’t be afraid. I will protect you with my own life, for the same person wants to kill us both.”"
      }
    },
