    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1sa.22.6-23",
            "paragraphs": [
              "vi.幫助大衛的代價（二十二6～23）"
            ]
          },
          {
            "verse": "1sa.22.11-15",
            "paragraphs": [
              "11～15.王召喚亞希米勒和一切祭司一起到基比亞的宮廷；亞希米勒對此似乎毫無戒心，他的良心是清潔的，也為大衛滔滔而辯，強調大衛的可靠、在王家與朝廷中的地位，以及大衛習慣向他諮商，以明白耶和華的引導。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "扫罗说：“亚希突的儿子，听着！”亚希米勒答道：“我主啊，我在这里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "掃羅說：“亞希突的兒子啊！你要聽。”他回答：“我主啊！我在這裡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "掃羅說：「亞希突的兒子，要聽我的話！」他回答說：「主啊，我在這裏。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Saul said, “Listen now, son of Ahitub.” And he answered, “Here I am, my lord.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Saul said, “Listen now, son of Ahitub.” “Yes, my lord,” he answered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When they arrived, Saul shouted at him, “Listen to me, you son of Ahitub!” “What is it, my king?” Ahimelech asked."
      }
    },
