    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "自从我把他们从埃及领出来以后，他们就常常背弃我，去供奉其他神明。现在，他们也这样对待你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "自從我把他們從埃及領上來的那天起，直到現在，他們怎樣離開我，事奉別的神，他們對我所行的一切，也照樣對你行。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "自從我領他們出埃及到如今，他們常常離棄我，事奉別神。現在他們向你所行的，是照他們素來所行的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "According to all the deeds that they have done, from the day I brought them up out of Egypt even to this day, forsaking me and serving other gods, so they are also doing to you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like all the deeds which they have done since the day that I brought them up from Egypt even to this day—in that they have forsaken Me and served other gods—so they are doing to you also."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As they have done from the day I brought them up out of Egypt until this day, forsaking me and serving other gods, so they are doing to you."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "According to all the works which they have done since the day that I brought them up out of Egypt, even to this day—with which they have forsaken Me and served other gods—so they are doing to you also."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ever since I brought them from Egypt they have continually abandoned me and followed other gods. And now they are giving you the same treatment."
      }
    },
