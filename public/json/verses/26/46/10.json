    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.46.1-15",
            "paragraphs": [
              "四六1～15這段詳述每周（安息日）、每月（月朔）和每日的獻祭。若千禧年期間也需要守逾越節和住棚節等重要節日，祭物就沒有理由不用獻上。那麽，這些獻祭當然是為了紀念基督已經完成的獻祭。參看利未記第二章的\"獻祭\"圖表和利未記第二十三章的\"節期\"圖表。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在这些节期君王应和百姓一同进殿敬拜，百姓离开的时候，君王也要一同离开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "君王要在人民當中，人民進入的時候，他也進入；人民出去的時候，他也出去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "民進入，王也要在民中進入；民出去，王也要一同出去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they go in, the prince shall go in among them; and when they go out, he shall go out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The prince is to be among them, going in when they go in and going out when they go out."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The prince will enter and leave with the people on these occasions."
      }
    },
