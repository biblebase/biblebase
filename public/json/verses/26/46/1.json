    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "ezk.46.1",
            "paragraphs": [
              "46:1",
              "1~24安息日和月朔：本章講到了安息日的獻祭和敬拜、月朔(1~8節)、各節期(9~12節)以及每日獻祭(13~15節)等方面的條例。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "ezk.46.1-15",
            "paragraphs": [
              "四六1～15這段詳述每周（安息日）、每月（月朔）和每日的獻祭。若千禧年期間也需要守逾越節和住棚節等重要節日，祭物就沒有理由不用獻上。那麽，這些獻祭當然是為了紀念基督已經完成的獻祭。參看利未記第二章的\"獻祭\"圖表和利未記第二十三章的\"節期\"圖表。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“主耶和华说，‘圣殿内院的东门在平时工作的六日内都要关闭，只在安息日和朔日才打开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主耶和華這樣說：“內院朝東的門，在工作的六天裡，必須關閉，在安息日卻要打開，在月朔之日也要打開。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華如此說：「內院朝東的門，在辦理事務的六日內必須關閉；惟有安息日和月朔必須敞開。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Thus says the Lord God, “The gate of the inner court facing east shall be shut the six working days; but it shall be opened on the sabbath day and opened on the day of the new moon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘This is what the Sovereign Lord says: The gate of the inner court facing east is to be shut on the six working days, but on the Sabbath day and on the day of the New Moon it is to be opened."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Sovereign Lord says: The east gateway of the inner courtyard will be closed during the six workdays each week, but it will be open on Sabbath days and the days of new moon celebrations."
      }
    },
