    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘因为你曾说，犹大和以色列必归我，即使上帝在那里，我们也要吞并他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘因為你曾說：這兩國這兩邦都必歸我，我必得它們為業，但其實耶和華還是在那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「因為你曾說：『這二國這二邦必歸於我，我必得為業』（其實耶和華仍在那裏），"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Because you have said, ‘These two nations and these two lands will be mine, and we will possess them,’ although the Lord was there,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Because you have said, “These two nations and countries will be ours and we will take possession of them,” even though I the Lord was there,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“For you said, ‘The lands of Israel and Judah will be ours. We will take possession of them. What do we care that the Lord is there!’"
      }
