    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“人子啊，你要告诉他们，主耶和华这样说，‘你们是来求问我吗？我凭我的永恒起誓，我决不让你们求问。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“人子啊！你要告訴以色列的長老，對他們說：‘主耶和華這樣說：你們來求問我嗎？我指著我的永生起誓，我必不讓你們求問我。這是主耶和華的宣告。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人子啊，你要告訴以色列的長老說，主耶和華如此說：你們來是求問我嗎？主耶和華說：我指着我的永生起誓，我必不被你們求問。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Son of man, speak to the elders of Israel and say to them, ‘Thus says the Lord God, “Do you come to inquire of Me? As I live,” declares the Lord God, “I will not be inquired of by you.” ’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Son of man, speak to the elders of Israel and say to them, ‘This is what the Sovereign Lord says: Have you come to inquire of me? As surely as I live, I will not let you inquire of me, declares the Sovereign Lord.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Son of man, tell the leaders of Israel, ‘This is what the Sovereign Lord says: How dare you come to ask me for a message? As surely as I live, says the Sovereign Lord, I will tell you nothing!’"
      }
    },
