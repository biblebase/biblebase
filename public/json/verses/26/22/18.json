      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Son of man, the house of Israel has become dross to Me; all of them are bronze and tin and iron and lead in the furnace; they are the dross of silver."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Son of man, the people of Israel have become dross to me; all of them are the copper, tin, iron and lead left inside a furnace. They are but the dross of silver."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Son of man, the people of Israel are the worthless slag that remains after silver is smelted. They are the dross that is left over—a useless mixture of copper, tin, iron, and lead."
      }
