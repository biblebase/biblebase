    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "ezk.27.1-36",
            "paragraphs": [
              "ii.推羅船只沈沒（二十七1～36）",
              "本章包括了兩種不同的文體：一是用哀歌（qînâ）節韻寫成的長詩（3b～9節、25b～36節），一是穿插在詩中間的散文體清單（10～25a節）。該首詩是一則很長的比喻，提到推羅是一艘很好的船，精工建造、船員優秀，但是載貨過重，因此在滔天巨浪中沈入海底。所有在地中海航行的人都站在岸邊為她的滅亡而哀哭。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "ezk.27.10-25",
            "paragraphs": [
              "二七10～25這段描述從多國而來的許多貨物都運載在推羅的船上。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘他施的船只替你运货， 你在海上载满沉甸甸的货物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他施的船隻運載你的商品， 你在海的中心，滿載貨物， 極其沉重。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他施的船隻接連成幫為你運貨， 你便在海中豐富極其榮華。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The ships of Tarshish traveled for you with your merchandise. So you were filled and heavily laden in the heart of the seas."
      },
      "nasb": {
        "version_id": "NASB",
