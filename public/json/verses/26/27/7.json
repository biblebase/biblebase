    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "用埃及来的绣花细麻布做你的帆， 成为你的旗号， 用伊利沙岛来的蓝布和紫布做船篷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的帆是用埃及刺繡的細麻布做的， 可作你的旗幟； 你的篷是用以利沙沿岸的藍色和紫色布做的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的篷帆是用埃及繡花細麻布做的， 可以做你的大旗； 你的涼棚是用以利沙島的藍色、紫色布做的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your sail was of fine embroidered linen from Egypt So that it became your distinguishing mark; Your awning was blue and purple from the coastlands of Elishah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Fine embroidered linen from Egypt was your sail and served as your banner; your awnings were of blue and purple from the coasts of Elishah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your sails were made of Egypt’s finest linen, and they flew as a banner above you. You stood beneath blue and purple awnings made bright with dyes from the coasts of Elishah."
      }
