    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "ezk.42.1",
            "paragraphs": [
              "42:1",
              "本章沒有帶註解"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "ezk.42.1-14",
            "paragraphs": [
              "四二1～14聖殿內院北面有兩座三層高的建築物，一座的長度是另一座的兩倍，是祭司用作吃祭物（四六20）和存放聖衣（四四19）的地方。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那人带我出了圣殿，向北来到外院的厢房，一排在空地对面，一排在北边的建筑物对面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他領我出來，向著北面的外院去；帶我到了祭司的房子。這些房子一排對著殿院的空地，一排對著北面的建築物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他帶我出來向北，到外院，又帶我進入聖屋；這聖屋一排順着空地，一排與北邊鋪石地之屋相對。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he brought me out into the outer court, the way toward the north; and he brought me to the chamber which was opposite the separate area and opposite the building toward the north."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the man led me northward into the outer court and brought me to the rooms opposite the temple courtyard and opposite the outer wall on the north side."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the man led me out of the Temple courtyard by way of the north gateway. We entered the outer courtyard and came to a group of rooms against the north wall of the inner courtyard."
      }
    },
