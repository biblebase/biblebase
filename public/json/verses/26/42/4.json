      "leili": {
        "content": [
          {
            "verse": "ezk.42.1-14",
            "paragraphs": [
              "四二1～14聖殿內院北面有兩座三層高的建築物，一座的長度是另一座的兩倍，是祭司用作吃祭物（四六20）和存放聖衣（四四19）的地方。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在两排厢房之间有一条五米宽、五十米长的通道，厢房的门都向北。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "房子前面有一條夾道，寬五公尺，長五十公尺。房子的門都朝向北面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在聖屋前有一條夾道，寬十肘，長一百肘。房門都向北。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Before the chambers was an inner walk ten cubits wide, a way of one hundred cubits; and their openings were on the north."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In front of the rooms was an inner passageway ten cubits wide and a hundred cubits long. Their doors were on the north."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Between the two blocks of rooms ran a walkway 171/2 feet wide. It extended the entire 175 feet of the complex, and all the doors faced north."
      }
    },
