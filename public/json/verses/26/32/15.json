    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使埃及的土地荒凉， 毁灭其中的居民， 那时他们就知道我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我使埃及地荒涼，使這地失去它所充滿的，又擊殺其中所有的居民。那時，他們就知道我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我使埃及地變為荒廢淒涼； 這地缺少從前所充滿的， 又擊殺其中一切的居民。 那時，他們就知道我是耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“When I make the land of Egypt a desolation, And the land is destitute of that which filled it, When I smite all those who live in it, Then they shall know that I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When I make Egypt desolate and strip the land of everything in it, when I strike down all who live there, then they will know that I am the Lord.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when I destroy Egypt and strip you of everything you own and strike down all your people, then you will know that I am the Lord."
      }
    },
