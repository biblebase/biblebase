    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘这是列国为埃及唱的哀歌，各国的妇女要为埃及和她的人民唱这哀歌。这是主耶和华说的。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這就是人為埃及所唱的哀歌，列國的女子必唱這哀歌；她們必為埃及和它的眾民唱這哀歌。這是主耶和華的宣告。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人必用這哀歌去哀哭，列國的女子為埃及和她的羣眾也必以此悲哀。這是主耶和華說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is a lamentation and they shall chant it. The daughters of the nations shall chant it. Over Egypt and over all her hordes they shall chant it,” declares the Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is the lament they will chant for her. The daughters of the nations will chant it; for Egypt and all her hordes they will chant it, declares the Sovereign Lord.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, this is the funeral song they will sing for Egypt. Let all the nations mourn. Let them mourn for Egypt and its hordes. I, the Sovereign Lord, have spoken!”"
      }
    },
