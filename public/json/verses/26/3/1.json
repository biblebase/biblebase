    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "ezk.3.1",
            "paragraphs": [
              "3:1",
              "要吃這書卷：書卷記載了神賜給他子民的話語以及有關審判的信息。以西結吃這書卷的舉動表明他必須首先接受神的信息並且全心委身於它，然後才能向民眾宣講。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂对我说：“人子啊，你要把得到的吃下去，要吃下这书卷，然后去向以色列人传讲。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他對我說：“人子啊，把你得到的吃下去吧！吃下這書卷，然後去向以色列家說話。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對我說：「人子啊，要吃你所得的，要吃這書卷，好去對以色列家講說。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He said to me, “Son of man, eat what you find; eat this scroll, and go, speak to the house of Israel.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And he said to me, “Son of man, eat what is before you, eat this scroll; then go and speak to the people of Israel.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The voice said to me, “Son of man, eat what I am giving you—eat this scroll! Then go and give its message to the people of Israel.”"
      }
    },
