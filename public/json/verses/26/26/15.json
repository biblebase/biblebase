    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "ezk.26.15-18",
            "paragraphs": [
              "15～18.海上王子的狼狽。鄰近各國的統治者倚賴推羅的商業維持繁盛，如今推羅傾倒，他們不禁深感泄氣，而且驚駭如此大有勢力的君王就此毀滅。那些海島事實上是沿海地帶（呂譯、RSV），亦即地中海沿岸的眾多小城邦。他們的王子要坐下舉哀，唱首哀歌（qînâ）。這是傳統3：2節韻的挽歌，在RSV中翻譯的很好。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "ezk.26.15-16",
            "paragraphs": [
              "二六15,16\"靠海的君王\"。推羅的商業貿易夥伴，他們因推羅的陷落而哀悼。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“主耶和华对泰尔这样说，‘你倾覆的时候，你中间的杀戮声和受伤之人的惨叫声岂不震动沿海地区吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“主耶和華對推羅這樣說：‘殺戮在你中間發生，受傷的人都在唉哼的時候，眾海島豈不因你傾倒的聲音而震動嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主耶和華對泰爾如此說：「在你中間行殺戮，受傷之人唉哼的時候，因你傾倒的響聲，海島豈不都震動嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Thus says the Lord God to Tyre: Will not the coastlands shake at the sound of your fall, when the wounded groan, when slaughter is made in your midst?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord God to Tyre, “Shall not the coastlands shake at the sound of your fall when the wounded groan, when the slaughter occurs in your midst?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“This is what the Sovereign Lord says to Tyre: Will not the coastlands tremble at the sound of your fall, when the wounded groan and the slaughter takes place in you?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Thus says the Lord God to Tyre: ‘Will the coastlands not shake at the sound of your fall, when the wounded cry, when slaughter is made in the midst of you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“This is what the Sovereign Lord says to Tyre: The whole coastline will tremble at the sound of your fall, as the screams of the wounded echo in the continuing slaughter."
      }
