    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必转脸不看他们，任由外族人亵渎我的殿，强盗必进入殿里大肆亵渎。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必轉臉不看他們，讓他們褻瀆我的聖所。 強盜要進那裡去，褻瀆那地方。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必轉臉不顧以色列人，他們褻瀆我隱密之所，強盜也必進去褻瀆。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will also turn My face from them, and they will profane My secret place; then robbers will enter and profane it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will turn my face away from the people, and robbers will desecrate the place I treasure. They will enter it and will defile it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will turn my eyes from them as these robbers invade and defile my treasured land."
      }
