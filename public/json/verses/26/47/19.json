    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "ezk.47.13-20",
            "paragraphs": [
              "四七13～20以色列地的北界從推羅以北的地中海延展到大馬色附近（15～17節）；東界由約但河與死海形成（18節）；南界從死海以下直到埃及\"小河\"（19節；參賽二七12的腳註）；西界就是地中海。這地區將要分配給以色列人作為他們的住處，但他們顯然會控制從尼羅河至幼發拉底河的整遍土地（比較創一五18）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“南界从他玛西经米利巴·加低斯泉和埃及小河直到地中海。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "南面的地界，從他瑪到米利巴．加低斯水，經埃及小河，到大海。這是南面的地界。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「南界是從他瑪到米利巴‧加低斯的水，延到埃及小河，直到大海。這是南界。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The south side toward the south shall extend from Tamar as far as the waters of Meribath-kadesh, to the brook of Egypt and to the Great Sea. This is the south side toward the south."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“On the south side it will run from Tamar as far as the waters of Meribah Kadesh, then along the Wadi of Egypt to the Mediterranean Sea. This will be the southern boundary."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The southern border will go west from Tamar to the waters of Meribah at Kadesh and then follow the course of the Brook of Egypt to the Mediterranean. This will be the southern border."
      }
    },
