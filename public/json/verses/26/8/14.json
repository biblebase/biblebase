      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂带我来到耶和华的殿外院的北门，看见一些妇女坐在那里为搭模斯神像哭泣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他領我到耶和華殿朝北的門口，在那裡有些婦女坐著，為搭模斯哭泣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他領我到耶和華殿外院朝北的門口。誰知，在那裏有婦女坐着，為搭模斯哭泣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then He brought me to the entrance of the gate of the Lord’s house which was toward the north; and behold, women were sitting there weeping for Tammuz."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he brought me to the entrance of the north gate of the house of the Lord, and I saw women sitting there, mourning the god Tammuz."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He brought me to the north gate of the Lord’s Temple, and some women were sitting there, weeping for the god Tammuz."
      }
    },
