      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "舍客勒是标准的重量单位，一舍客勒是二十季拉。六十舍客勒等于一弥那。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "重量單位的一‘舍客勒’是二十‘季拉’；二十‘舍客勒’，加二十五‘舍客勒’和十五‘舍客勒’、就是你們的一‘彌那’。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "舍客勒是二十季拉；二十舍客勒，二十五舍客勒，十五舍客勒，為你們的彌那。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The shekel shall be twenty gerahs; twenty shekels, twenty-five shekels, and fifteen shekels shall be your maneh."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The shekel is to consist of twenty gerahs. Twenty shekels plus twenty-five shekels plus fifteen shekels equal one mina."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The standard unit for weight will be the silver shekel. One shekel will consist of twenty gerahs, and sixty shekels will be equal to one mina."
      }
    },
