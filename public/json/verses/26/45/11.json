    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贺梅珥是标准的容量单位，量固体的伊法是十分之一贺梅珥，量液体的罢特也是十分之一贺梅珥。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "量固體的‘伊法’和量液體的‘罷特’的容量要相同：一‘罷特’等於‘賀梅珥’的十分之一；一‘伊法’也等於‘賀梅珥’的十分之一，都以‘賀梅珥’的容量為準。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "伊法與罷特大小要一樣。罷特可盛賀梅珥十分之一，伊法也可盛賀梅珥十分之一，都以賀梅珥的大小為準。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The ephah and the bath shall be the same quantity, so that the bath will contain a tenth of a homer and the ephah a tenth of a homer; their standard shall be according to the homer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The ephah and the bath are to be the same size, the bath containing a tenth of a homer and the ephah a tenth of a homer; the homer is to be the standard measure for both."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The homer will be your standard unit for measuring volume. The ephah and the bath will each measure one-tenth of a homer."
      }
    },
