    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这样，你戴的是金银首饰，穿的是绫罗绸缎，吃的是细面粉、蜂蜜和油。你变得美艳绝伦，晋升为王后。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這樣你就有了金銀的裝飾，你穿的是細麻、絲綢和刺繡的衣服；你吃的是細麵、蜂蜜和油；你實在非常美麗，配登王后的位。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這樣，你就有金銀的妝飾，穿的是細麻衣和絲綢，並繡花衣；吃的是細麵、蜂蜜，並油。你也極其美貌，發達到王后的尊榮。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Thus you were adorned with gold and silver, and your clothing was of fine linen and silk and embroidered cloth. You ate fine flour and honey and oil. You grew exceedingly beautiful and advanced to royalty."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus you were adorned with gold and silver, and your dress was of fine linen, silk and embroidered cloth. You ate fine flour, honey and oil; so you were exceedingly beautiful and advanced to royalty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So you were adorned with gold and silver; your clothes were of fine linen and costly fabric and embroidered cloth. Your food was honey, olive oil and the finest flour. You became very beautiful and rose to be a queen."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Thus you were adorned with gold and silver, and your clothing was  of fine linen, silk, and embroidered cloth. You ate pastry  of fine flour, honey, and oil. You were exceedingly beautiful, and succeeded to royalty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And so you were adorned with gold and silver. Your clothes were made of fine linen and costly fabric and were beautifully embroidered. You ate the finest foods—choice flour, honey, and olive oil—and became more beautiful than ever. You looked like a queen, and so you were!"
      }
