      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的美貌名扬四方，因为我给你的华贵使你美貌无比。这是主耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的名聲因你的美麗傳遍列國，你全然美麗，是因為我加在你身上的榮美。這是主耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你美貌的名聲傳在列邦中，你十分美貌，是因我加在你身上的威榮。這是主耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then your fame went forth among the nations on account of your beauty, for it was perfect because of My splendor which I bestowed on you,” declares the Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And your fame spread among the nations on account of your beauty, because the splendor I had given you made your beauty perfect, declares the Sovereign Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your fame soon spread throughout the world because of your beauty. I dressed you in my splendor and perfected your beauty, says the Sovereign Lord."
      }
