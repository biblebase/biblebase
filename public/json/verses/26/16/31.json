    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你在街头巷尾筑祭坛，在广场建高高的神庙，甚至不收淫资，实在连妓女都不如。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你在各路口建造你的高岡，又在各廣場上築起你的高壇；然而你並不像一個妓女，因為你藐視賣淫所得的賞賜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因你在一切市口上建造圓頂花樓，在各街上做了高臺，你卻藐視賞賜，不像妓女。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When you built your shrine at the beginning of every street and made your high place in every square, in disdaining money, you were not like a harlot."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When you built your mounds at every street corner and made your lofty shrines in every public square, you were unlike a prostitute, because you scorned payment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You build your pagan shrines on every street corner and your altars to idols in every square. In fact, you have been worse than a prostitute, so eager for sin that you have not even demanded payment."
      }
    },
