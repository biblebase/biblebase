    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要告诉她，主耶和华这样对她说，‘你祖居迦南，生于迦南。你的父亲是亚摩利人，母亲是赫人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要說：‘主耶和華對耶路撒冷這樣說：你的根源和出生地是在迦南地；你的父親是亞摩利人，你的母親是赫人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說主耶和華對耶路撒冷如此說：你根本，你出世，是在迦南地；你父親是亞摩利人，你母親是赫人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and say, ‘Thus says the Lord God to Jerusalem, “Your origin and your birth are from the land of the Canaanite, your father was an Amorite and your mother a Hittite."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and say, ‘This is what the Sovereign Lord says to Jerusalem: Your ancestry and birth were in the land of the Canaanites; your father was an Amorite and your mother a Hittite."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give her this message from the Sovereign Lord: You are nothing but a Canaanite! Your father was an Amorite and your mother a Hittite."
      }
    },
