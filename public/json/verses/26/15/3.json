      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Is wood taken from it to make anything? Do people take a peg from it to hang any vessel on it?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Can wood be taken from it to make anything, or can men take a peg from it on which to hang any vessel?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Is wood ever taken from it to make anything useful? Do they make pegs from it to hang things on?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Is wood taken from it to make any object? Or can men make a peg from it to hang any vessel on?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can its wood be used for making things, like pegs to hang up pots and pans?"
      }
