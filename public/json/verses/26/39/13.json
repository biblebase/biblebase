    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人都来埋葬他们。我彰显荣耀的那天将是以色列人扬眉吐气的日子。这是主耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那地所有的人民都必埋葬他們。這是我得榮耀的日子，以色列人也必因此得享美名。這是主耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "全地的居民都必葬埋他們。當我得榮耀的日子，這事必叫他們得名聲。這是主耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Even all the people of the land will bury them; and it will be to their renown on the day that I glorify Myself,” declares the Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the people of the land will bury them, and the day I display my glory will be a memorable day for them, declares the Sovereign Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone in Israel will help, for it will be a glorious victory for Israel when I demonstrate my glory on that day, says the Sovereign Lord."
      }
    },
