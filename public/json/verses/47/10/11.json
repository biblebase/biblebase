    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说这话的人要留心：我们不在你们那里的时候，在信上怎么说，与你们见面时也会怎么做。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說這話的人要想一想，我們不在你們那裡的時候，信上怎樣寫，我們來到的時候也會怎樣作。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這等人當想，我們不在那裏的時候，信上的言語如何，見面的時候，行事也必如何。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let such a person consider this, that what we are in word by letters when absent, such persons we are also in deed when present."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Such people should realize that what we are in our letters when we are absent, we will be in our actions when we are present."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those people should realize that our actions when we arrive in person will be as forceful as what we say in our letters from far away."
      }
    },
