    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "圣经上说：“我相信，所以才说。”我们有同样的信心，我们也相信，所以才这样说。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "經上記著說：“我信，所以我說話。”我們既然有同樣的信心，也就信，所以也說話，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但我們既有信心，正如經上記着說：「我因信，所以如此說話。」我們也信，所以也說話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But having the same spirit of faith, according to what is written, “I believed, therefore I spoke,” we also believe, therefore we also speak,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is written: “I believed; therefore I have spoken.” Since we have that same spirit of faith, we also believe and therefore speak,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But we continue to preach because we have the same kind of faith the psalmist had when he said, “I believed in God, so I spoke.”"
      }
    },
