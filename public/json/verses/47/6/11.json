      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We have spoken freely to you, Corinthians, and opened wide our hearts to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
