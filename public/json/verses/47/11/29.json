    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有谁软弱，我不感同身受呢？有谁失足犯罪，我不心急如焚呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有誰軟弱，我不軟弱呢？有誰陷在罪裡，我不焦急呢？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "有谁软弱，我不软弱呢？有谁被绊倒，我不心如火烧呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有誰軟弱，我不軟弱呢？有誰跌倒，我不焦急呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Who is weak, and I am not weak? Who is made to fall, and I am not indignant?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who is weak without my being weak? Who is led into sin without my intense concern?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who is weak, and I do not feel weak? Who is led into sin, and I do not inwardly burn?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Who is weak, and I am not weak? Who is made to stumble, and I do not burn with  indignation?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Who is weak without my feeling that weakness? Who is led astray, and I do not burn with anger?"
      }
