      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有谁软弱，我不感同身受呢？有谁失足犯罪，我不心急如焚呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有誰軟弱，我不軟弱呢？有誰陷在罪裡，我不焦急呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有誰軟弱，我不軟弱呢？有誰跌倒，我不焦急呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who is weak without my being weak? Who is led into sin without my intense concern?"
      },
