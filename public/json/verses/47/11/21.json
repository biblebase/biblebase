    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "惭愧得很，我必须说，我们太软弱了，做不出那样的事！ 然而，别人敢夸口的，让我再讲一句傻话，我也敢。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說來慚愧，我們太軟弱了。說句愚昧的話，如果有人在甚麼事上是勇敢的，我也是勇敢的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "说来惭愧，我们倒好像是软弱的。 但如果有人在什么事上大胆——我说句愚妄的话——我也大胆！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我說這話是羞辱自己，好像我們從前是軟弱的。然而，人在何事上勇敢，（我說句愚妄話，）我也勇敢。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "To my shame, I must say, we were too weak for that! But whatever anyone else dares to boast of—I am speaking as a fool—I also dare to boast of that."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To my shame I must say that we have been weak by comparison. But in whatever respect anyone else is bold—I speak in foolishness—I am just as bold myself."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To my shame I admit that we were too weak for that! Whatever anyone else dares to boast about—I am speaking as a fool—I also dare to boast about."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "To our shame I say that we were too weak for that! But in whatever anyone is bold—I speak foolishly—I am bold also."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I’m ashamed to say that we’ve been too “weak” to do that! But whatever they dare to boast about—I’m talking like a fool again—I dare to boast about it, too."
      }
    },
