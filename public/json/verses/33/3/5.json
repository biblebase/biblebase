    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “使我的子民步入歧途的先知啊， 你们向供养你们的人报平安， 出言攻击那不供养你们的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "關於那些使我民走迷路的先知， 耶和華這樣說： “他們的牙齒有所咀嚼的時候， 就大叫‘平安’； 誰不把食物給他們吃， 他們就攻擊那人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論到使我民走差路的先知－ 他們牙齒有所嚼的， 他們就呼喊說：平安了！ 凡不供給他們吃的， 他們就預備攻擊他－ 耶和華如此說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord concerning the prophets who lead my people astray; When they have something to bite with their teeth, They cry, “Peace,” But against him who puts nothing in their mouths They declare holy war."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord says: “As for the prophets who lead my people astray, they proclaim ‘peace’ if they have something to eat, but prepare to wage war against anyone who refuses to feed them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord says: “You false prophets are leading my people astray! You promise peace for those who give you food, but you declare war on those who refuse to feed you."
      }
    },
