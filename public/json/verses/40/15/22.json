    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那地方有个迦南的妇人前来大声恳求耶稣：“主啊！大卫的后裔啊！可怜我吧！我的女儿被鬼附身，受尽折磨！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一個迦南的婦人從那地區出來，喊著說：“主啊，大衛的子孫，可憐我吧！我的女兒被鬼附得很苦。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有一個迦南婦人，從那地方出來，喊着說：「主啊，大衛的子孫，可憐我！我女兒被鬼附得甚苦。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And a Canaanite woman from that region came out and began to cry out, saying, “Have mercy on me, Lord, Son of David; my daughter is cruelly demon-possessed.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A Canaanite woman from that vicinity came to him, crying out, “Lord, Son of David, have mercy on me! My daughter is demon-possessed and suffering terribly.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A Gentile woman who lived there came to him, pleading, “Have mercy on me, O Lord, Son of David! For my daughter is possessed by a demon that torments her severely.”"
      }
    },
