    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“妇人，你的信心真大！我答应你的要求。”就在那一刻，她女儿就好了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是耶穌對她說：“婦人，你的信心真大，照你所想的給你成就吧！”從那時起，她的女兒就好了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「婦人，你的信心是大的！照你所要的，給你成全了吧。」從那時候，她女兒就好了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jesus said to her, “O woman, your faith is great; it shall be done for you as you wish.” And her daughter was healed at once."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jesus said to her, “Woman, you have great faith! Your request is granted.” And her daughter was healed at that moment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Dear woman,” Jesus said to her, “your faith is great. Your request is granted.” And her daughter was instantly healed."
      }
    },
