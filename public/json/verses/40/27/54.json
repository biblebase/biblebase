    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看守耶稣的百夫长和士兵看见地震及所发生的一切，都很害怕，说：“这人真是上帝的儿子啊！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "百夫長和跟他一起看守耶穌的士兵，看見了地震和所發生的事情，就十分懼怕，說：“這個人真是　神的兒子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "百夫長和一同看守耶穌的人看見地震並所經歷的事，就極其害怕，說：「這真是神的兒子了！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the centurion, and those who were with him keeping guard over Jesus, when they saw the earthquake and the things that were happening, became very frightened and said, “Truly this was the Son of God!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the centurion and those with him who were guarding Jesus saw the earthquake and all that had happened, they were terrified, and exclaimed, “Surely he was the Son of God!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Roman officer and the other soldiers at the crucifixion were terrified by the earthquake and all that had happened. They said, “This man truly was the Son of God!”"
      }
    },
