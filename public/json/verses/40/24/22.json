    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果不缩短灾期，恐怕没有人能活命。但为了选民的缘故，灾期必被缩短。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果那些日子不減少，沒有一個人可以存活；但是為了選民，那些日子必會減少。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若不減少那日子，凡有血氣的總沒有一個得救的；只是為選民，那日子必減少了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Unless those days had been cut short, no life would have been saved; but for the sake of the elect those days will be cut short."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If those days had not been cut short, no one would survive, but for the sake of the elect those days will be shortened."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In fact, unless that time of calamity is shortened, not a single person will survive. But it will be shortened for the sake of God’s chosen ones."
      }
    },
