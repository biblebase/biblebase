    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“因为摩西知道你们铁石心肠，所以才准你们休妻。但起初并不是这样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“摩西因為你們的心硬，才准許你們休妻，但起初並不是這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「摩西因為你們的心硬，所以許你們休妻，但起初並不是這樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He *said to them, “Because of your hardness of heart Moses permitted you to divorce your wives; but from the beginning it has not been this way."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus replied, “Moses permitted you to divorce your wives because your hearts were hard. But it was not this way from the beginning."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “Moses permitted divorce only as a concession to your hard hearts, but it was not what God had originally intended."
      }
    },
