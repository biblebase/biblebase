    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣问她：“你要什么呢？” 她说：“在你的国度，请让我这两个儿子一个坐在你右边，一个坐在你左边。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌問她：“你想要甚麼？”她說：“求你下令，使我這兩個兒子在你的國裡，一個坐在你的右邊，一個坐在你的左邊。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「你要甚麼呢？」她說：「願你叫我這兩個兒子在你國裏，一個坐在你右邊，一個坐在你左邊。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to her, “What do you wish?” She *said to Him, “Command that in Your kingdom these two sons of mine may sit one on Your right and one on Your left.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“What is it you want?” he asked. She said, “Grant that one of these two sons of mine may sit at your right and the other at your left in your kingdom.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What is your request?” he asked. She replied, “In your Kingdom, please let my two sons sit in places of honor next to you, one on your right and the other on your left.”"
      }
