    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，耶稣把他们叫来，说：“外族人有君王统治他们，有大臣管理他们。但你们不可这样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌把他們叫過來，說：“你們知道各國都有元首統治他們，也有官長管轄他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌叫了他們來，說：「你們知道外邦人有君王為主治理他們，有大臣操權管束他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Jesus called them to Himself and said, “You know that the rulers of the Gentiles lord it over them, and their great men exercise authority over them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus called them together and said, “You know that the rulers of the Gentiles lord it over them, and their high officials exercise authority over them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jesus called them together and said, “You know that the rulers in this world lord it over their people, and officials flaunt their authority over those under them."
      }
