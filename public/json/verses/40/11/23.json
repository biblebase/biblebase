    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“迦百农啊，你将被提升到天上吗？不！你将被打落到阴间。因为若把在你那里所行的神迹行在所多玛，它肯定会存留到今天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "迦百農啊！ 你會被高舉到天上嗎？ 你必降到陰間。 在你那裡行過的神蹟，如果行在所多瑪，那城還會存留到今天。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "而你——迦百农啊，你将被高举到天上吗？不，你将下到阴间。因为在你中间行过的神迹，如果行在所多玛，它就会存留到今天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "迦百農啊，你已經升到天上，將來必墜落陰間；因為在你那裏所行的異能，若行在所多瑪，它還可以存到今日。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And you, Capernaum, will you be exalted to heaven? You will be brought down to Hades. For if the mighty works done in you had been done in Sodom, it would have remained until this day."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And you, Capernaum, will not be exalted to heaven, will you? You will descend to Hades; for if the miracles had occurred in Sodom which occurred in you, it would have remained to this day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And you, Capernaum, will you be lifted to the heavens? No, you will go down to Hades.  For if the miracles that were performed in you had been performed in Sodom, it would have remained to this day."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And you, Capernaum, who are exalted to heaven, will be brought down to Hades; for if the mighty works which were done in you had been done in Sodom, it would have remained until this day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And you people of Capernaum, will you be honored in heaven? No, you will go down to the place of the dead. For if the miracles I did for you had been done in wicked Sodom, it would still be here today."
      }
    },
