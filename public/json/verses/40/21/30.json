    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“那父亲又对小儿子说，‘你今天到葡萄园工作吧！’小儿子回答说，‘好的，父亲。’他答应了，却没有去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "父親又照樣去吩咐小兒子。小兒子說：‘父親，我會去的。’後來卻沒有去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又來對小兒子也是這樣說。他回答說：『父啊，我去』，他卻不去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The man came to the second and said the same thing; and he answered, ‘I will, sir’; but he did not go."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Then the father went to the other son and said the same thing. He answered, ‘I will, sir,’ but he did not go."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the father told the other son, ‘You go,’ and he said, ‘Yes, sir, I will.’ But he didn’t go."
      }
