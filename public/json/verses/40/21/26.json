    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但如果我们说‘是从人来的’，又怕触怒百姓，因为他们相信约翰是个先知。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我們說‘是從人來的’，我們又怕群眾，因為他們都認為約翰是先知。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若說『從人間來』，我們又怕百姓，因為他們都以約翰為先知。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But if we say, ‘From men,’ we fear the people; for they all regard John as a prophet.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if we say, ‘Of human origin’—we are afraid of the people, for they all hold that John was a prophet.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if we say it was merely human, we’ll be mobbed because the people believe John was a prophet.”"
      }
