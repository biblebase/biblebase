    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“你们进城去，到某人那里对他说，‘老师说祂的时候到了，祂要与门徒在你家中过逾越节。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“你們到城裡去見某人，對他說，老師說：‘我的時候快到了；我要到你那裡和我的門徒守逾越節。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「你們進城去，到某人那裏，對他說：『夫子說：我的時候快到了，我與門徒要在你家裏守逾越節。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said, “Go into the city to a certain man, and say to him, ‘The Teacher says, “My time is near; I am to keep the Passover at your house with My disciples.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He replied, “Go into the city to a certain man and tell him, ‘The Teacher says: My appointed time is near. I am going to celebrate the Passover with my disciples at your house.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“As you go into the city,” he told them, “you will see a certain man. Tell him, ‘The Teacher says: My time has come, and I will eat the Passover meal with my disciples at your house.’”"
      }
    },
