    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "mal.3.13",
            "paragraphs": [
              "13.瑪拉基以同樣的方法開始∶神指控那些對事奉失望的人，他們用強硬（NEB∶難聽）的話頂撞祂；這些話，顯然是無心之言。然而口無遮攔會導致士氣低落。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "mal.3.13",
            "paragraphs": [
              "三13\"頂撞\"。傲慢或倔強。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“你们出言毁谤我，还问，‘我们怎么毁谤你了？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：“你們用無禮的話頂撞我。你們還問：‘我們用甚麼話頂撞了你呢？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「你們用話頂撞我，你們還說：『我們用甚麼話頂撞了你呢？』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Your words have been arrogant against Me,” says the Lord. “Yet you say, ‘What have we spoken against You?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You have spoken arrogantly against me,” says the Lord. “Yet you ask, ‘What have we said against you?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You have said terrible things about me,” says the Lord. “But you say, ‘What do you mean? What have we said against you?’"
      }
