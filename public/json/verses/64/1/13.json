    "interpretations": {
      "chenzhongdao": {
        "content": [
          {
            "verse": "3jn.1.13",
            "paragraphs": [
              "13-14“我原有許多事要……”雖然有許多事要寫，老約翰卻不想用筆墨寫，這可能因為：",
              "１．既然就快見面，當然不用多寫。",
              "２．年老的約翰，可能執筆並不很方便。",
              "３．老約翰對受書人有很親切的情感，總覺得當面談論，比較遠遠隔開而用筆寫親切得多。",
              "二．問安（15）"
            ]
          }
        ],
        "title": "新約書信讀經講義",
        "author": "陳終道"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我本来有很多事想跟你谈，但不想用笔墨写给你，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我還有許多話要寫給你，可是我不願借用筆墨。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我原有許多事要寫給你，卻不願意用筆墨寫給你，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I had many things to write to you, but I am not willing to write them to you with pen and ink;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have much to write you, but I do not want to do so with pen and ink."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have much more to say to you, but I don’t want to write it with pen and ink."
      }
    },
