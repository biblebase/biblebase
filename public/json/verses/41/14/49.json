    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我天天和你们在一起，在圣殿里教导人，你们没有来抓我。你们现在这样做是要应验圣经的话。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我天天在殿裡教導人，跟你們在一起，你們卻沒有捉拿我；但這是為了要應驗經上的話。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我天天教訓人，同你們在殿裏，你們並沒有拿我。但這事成就，為要應驗經上的話。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every day I was with you in the temple teaching, and you did not seize Me; but this has taken place to fulfill the Scriptures.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every day I was with you, teaching in the temple courts, and you did not arrest me. But the Scriptures must be fulfilled.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why didn’t you arrest me in the Temple? I was there among you teaching every day. But these things are happening to fulfill what the Scriptures say about me.”"
      }
    },
