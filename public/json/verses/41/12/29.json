    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣回答道：“最重要的诫命是，‘听啊，以色列！主——我们的上帝是独一的主。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌回答：“第一重要的是：‘以色列啊，你要聽！主我們的　神是獨一的主。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌回答說：「第一要緊的就是說：『以色列啊，你要聽，主－我們神是獨一的主。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus answered, “The foremost is, ‘Hear, O Israel! The Lord our God is one Lord;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The most important one,” answered Jesus, “is this: ‘Hear, O Israel: The Lord our God, the Lord is one."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “The most important commandment is this: ‘Listen, O Israel! The Lord our God is the one and only Lord."
      }
    },
