    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "最后只剩下园主的爱子，园主就派他去，以为那些佃户会尊重他的儿子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "還有一個，就是園主的愛子，最後園主派他到那裡去，說：‘他們必尊敬我的兒子。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "園主還有一位是他的愛子，末後又打發他去，意思說：『他們必尊敬我的兒子。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He had one more to send, a beloved son; he sent him last of all to them, saying, ‘They will respect my son.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“He had one left to send, a son, whom he loved. He sent him last of all, saying, ‘They will respect my son.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "until there was only one left—his son whom he loved dearly. The owner finally sent him, thinking, ‘Surely they will respect my son.’"
      }
