    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这班佃户却彼此商量说，‘这个就是园主的继承人。来吧！我们杀掉他，产业就归我们了。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些佃戶卻彼此說：‘這是繼承產業的；來，我們殺了他，產業就是我們的了。’"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“可是那些农夫彼此说：‘这是继承人。来，我们把他杀了，那继业就归我们了。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不料，那些園戶彼此說：『這是承受產業的。來吧，我們殺他，產業就歸我們了！』"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But those tenants said to one another, ‘This is the heir. Come, let us kill him, and the inheritance will be ours.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But those vine-growers said to one another, ‘This is the heir; come, let us kill him, and the inheritance will be ours!’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But the tenants said to one another, ‘This is the heir. Come, let’s kill him, and the inheritance will be ours.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But those vinedressers said among themselves, ‘This is the heir. Come, let us kill him, and the inheritance will be ours.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But the tenant farmers said to one another, ‘Here comes the heir to this estate. Let’s kill him and get the estate for ourselves!’"
      }
    },
