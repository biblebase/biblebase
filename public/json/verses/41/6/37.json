    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“你们给他们吃的吧。” 门徒说：“我们哪来这么多钱买东西给他们吃啊？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌回答他們：“你們給他們吃吧！”門徒說：“要我們去買兩百銀幣的餅給他們吃嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌回答說：「你們給他們吃吧。」門徒說：「我們可以去買二十兩銀子的餅給他們吃嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But He answered them, “You give them something to eat!” And they *said to Him, “Shall we go and spend two hundred denarii on bread and give them something to eat?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But he answered, “You give them something to eat.” They said to him, “That would take more than half a year’s wages! Are we to go and spend that much on bread and give it to them to eat?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Jesus said, “You feed them.” “With what?” they asked. “We’d have to work for months to earn enough money to buy food for all these people!”"
      }
    },
