    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "放猪的人都吓跑了。他们把这消息传遍了城里乡间，于是人人都跑来看个究竟。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "放豬的人都逃跑了，到城裡和各鄉村去報告，大家就來看發生了甚麼事。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "那些放猪的人就逃跑了，去把这事传到城里城外。于是人们就来看发生了什么事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "放豬的就逃跑了，去告訴城裏和鄉下的人。眾人就來，要看是甚麼事。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The herdsmen fled and told it in the city and in the country. And people came to see what it was that had happened."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their herdsmen ran away and reported it in the city and in the country. And the people came to see what it was that had happened."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those tending the pigs ran off and reported this in the town and countryside, and the people went out to see what had happened."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So those who fed the swine fled, and they told it in the city and in the country. And they went out to see what it was that had happened."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The herdsmen fled to the nearby town and the surrounding countryside, spreading the news as they ran. People rushed out to see what had happened."
      }
    },
