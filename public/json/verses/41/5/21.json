    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣再坐船回到对岸，立刻有一大群人在岸边围着祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌又坐船渡到那邊；還在海邊的時候，有一大群人向他圍攏過來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌坐船又渡到那邊去，就有許多人到他那裏聚集；他正在海邊上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Jesus had crossed over again in the boat to the other side, a large crowd gathered around Him; and so He stayed by the seashore."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jesus had again crossed over by boat to the other side of the lake, a large crowd gathered around him while he was by the lake."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus got into the boat again and went back to the other side of the lake, where a large crowd gathered around him on the shore."
      }
    },
