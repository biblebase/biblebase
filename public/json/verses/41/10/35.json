    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "西庇太的两个儿子雅各和约翰上前对耶稣说：“老师，请你答应我们一个要求。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "西庇太的兒子雅各、約翰，來到耶穌跟前，對他說：“老師，我們無論向你求甚麼，願你為我們作成。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "西庇太的兒子雅各、約翰進前來，對耶穌說：「夫子，我們無論求你甚麼，願你給我們做。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "James and John, the two sons of Zebedee, *came up to Jesus, saying, “Teacher, we want You to do for us whatever we ask of You.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then James and John, the sons of Zebedee, came to him. “Teacher,” they said, “we want you to do for us whatever we ask.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then James and John, the sons of Zebedee, came over and spoke to him. “Teacher,” they said, “we want you to do us a favor.”"
      }
    },
