    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣看着他们说：“对人而言，这不可能，但对上帝而言，凡事都可能。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌看著他們，說：“在人不能，在　神卻不然，因為在　神凡事都能。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌看着他們，說：「在人是不能，在神卻不然，因為神凡事都能。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Looking at them, Jesus *said, “With people it is impossible, but not with God; for all things are possible with God.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus looked at them and said, “With man this is impossible, but not with God; all things are possible with God.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus looked at them intently and said, “Humanly speaking, it is impossible. But not with God. Everything is possible with God.”"
      }
