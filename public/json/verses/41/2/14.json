    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣往前走的时候，看见亚勒腓的儿子利未坐在收税站里工作，就对他说：“跟从我！”利未就起来跟从了耶稣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他往前走的時候，看見亞勒腓的兒子利未坐在稅關那裡，就對他說：“來跟從我！”他就起來跟從了耶穌。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌經過的時候，看見亞勒腓的兒子利未坐在稅關上，就對他說：「你跟從我來。」他就起來，跟從了耶穌。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As He passed by, He saw Levi the son of Alphaeus sitting in the tax booth, and He *said to him, “Follow Me!” And he got up and followed Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As he walked along, he saw Levi son of Alphaeus sitting at the tax collector’s booth. “Follow me,” Jesus told him, and Levi got up and followed him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As he walked along, he saw Levi son of Alphaeus sitting at his tax collector’s booth. “Follow me and be my disciple,” Jesus said to him. So Levi got up and followed him."
      }
