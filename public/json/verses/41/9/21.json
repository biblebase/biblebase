    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣问孩子的父亲：“他这样子多久了？”孩子的父亲回答道：“他从小就这样，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌問他父親：“這事臨到他有多久了？”他說：“從小就是這樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌問他父親說：「他得這病有多少日子呢？」回答說：「從小的時候。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He asked his father, “How long has this been happening to him?” And he said, “From childhood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus asked the boy’s father, “How long has he been like this?” “From childhood,” he answered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“How long has this been happening?” Jesus asked the boy’s father. He replied, “Since he was a little boy."
      }
