    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们回到迦百农的住所，耶稣问门徒：“你们一路上争论些什么？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們來到迦百農。在屋裡的時候，耶穌問他們：“你們在路上爭論甚麼？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們來到迦百農。耶穌在屋裏問門徒說：「你們在路上議論的是甚麼？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They came to Capernaum; and when He was in the house, He began to question them, “What were you discussing on the way?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They came to Capernaum. When he was in the house, he asked them, “What were you arguing about on the road?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After they arrived at Capernaum and settled in a house, Jesus asked his disciples, “What were you discussing out on the road?”"
      }
