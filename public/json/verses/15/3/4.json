    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们照律法书的记载守住棚节，每天按规定的数目献上燔祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們又按照律法上所記載的守住棚節，每天照著規定的數目獻上當天的燔祭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又照律法書上所寫的守住棚節，按數照例獻每日所當獻的燔祭；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And they kept the Feast of Booths, as it is written, and offered the daily burnt offerings by number according to the rule, as each day required,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They celebrated the Feast of Booths, as it is written, and offered the fixed number of burnt offerings daily, according to the ordinance, as each day required;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then in accordance with what is written, they celebrated the Festival of Tabernacles with the required number of burnt offerings prescribed for each day."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "They also kept the Feast of Tabernacles, as it  is written, and offered the daily burnt offerings in the number required by ordinance for each day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They celebrated the Festival of Shelters as prescribed in the Law, sacrificing the number of burnt offerings specified for each day of the festival."
      }
    },
