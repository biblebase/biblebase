    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尽管耶和华殿的根基还没有立好，从七月一日起，他们就开始向耶和华献燔祭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從七月一日起，他們就開始向耶和華獻燔祭，但是那時耶和華的殿還沒有立定根基，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從七月初一日起，他們就向耶和華獻燔祭。但耶和華殿的根基尚未立定。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From the first day of the seventh month they began to offer burnt offerings to the Lord, but the foundation of the temple of the Lord had not been laid."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the first day of the seventh month they began to offer burnt offerings to the Lord, though the foundation of the Lord’s temple had not yet been laid."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fifteen days before the Festival of Shelters began, the priests had begun to sacrifice burnt offerings to the Lord. This was even before they had started to lay the foundation of the Lord’s Temple."
      }
    },
