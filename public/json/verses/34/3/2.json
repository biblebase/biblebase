    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "nam.3.1-19",
            "paragraphs": [
              "Ⅶ禍哉尼尼微（三1～19）",
              "尼尼微的敗落從幾個不同時間的觀點來描繪∶前瞻敵對的局面（5～12節）、圍城與戰爭的預備（14～17節）、實際戰爭的預示（2～4節）、挫敗的回顧（13、18～19節）。"
            ]
          },
          {
            "verse": "nam.3.2-3",
            "paragraphs": [
              "Ｂ戰鬥與死亡（三2～3）",
              "用攻擊者行動的景象與響聲（2節），以及戰爭的余殃──無數的屍首（3節），重述二章3～10節所描繪之生動的戰爭情景。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "nam.3.2",
            "paragraphs": [
              "三2\"鞭聲響亮\"。驅策瑪代和巴比倫的馬匹。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "鞭声啪啪，车轮隆隆， 战马奔腾，战车疾驰，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "鞭聲颼颼， 輪聲轔轔， 駿馬奔馳， 戰車顛簸跳動。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "鞭聲響亮，車輪轟轟， 馬匹踢跳，車輛奔騰，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The noise of the whip, The noise of the rattling of the wheel, Galloping horses And bounding chariots!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The crack of whips, the clatter of wheels, galloping horses and jolting chariots!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Hear the crack of whips, the rumble of wheels! Horses’ hooves pound, and chariots clatter wildly."
      }
    },
