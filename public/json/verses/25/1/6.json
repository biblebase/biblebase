    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "锡安的尊荣消逝， 她的首领像找不到草吃的鹿， 无力逃脱敌人的追赶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "錫安所有的尊榮，都離開了她的居民。 她的眾領袖都像找不著牧場的鹿； 在追趕的人面前無力逃走。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "錫安城的威榮全都失去。 她的首領像找不着草場的鹿； 在追趕的人前無力行走。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All her majesty Has departed from the daughter of Zion; Her princes have become like deer That have found no pasture; And they have fled without strength Before the pursuer."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the splendor has departed from Daughter Zion. Her princes are like deer that find no pasture; in weakness they have fled before the pursuer."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the majesty of beautiful Jerusalem has been stripped away. Her princes are like starving deer searching for pasture. They are too weak to run from the pursuing enemy."
      }
    },
