    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们要牢记人子所说的话，因为祂将要被交到人的手里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你們要把這些話存在心裡：人子將要被交在人的手裡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們要把這些話存在耳中，因為人子將要被交在人手裏。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Let these words sink into your ears; for the Son of Man is going to be delivered into the hands of men.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Listen carefully to what I am about to tell you: The Son of Man is going to be delivered into the hands of men.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Listen to me and remember what I say. The Son of Man is going to be betrayed into the hands of his enemies.”"
      }
    },
