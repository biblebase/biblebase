    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "树的好坏从果子就可以分辨出来。人不会从荆棘中采集无花果，也不会在蒺藜上摘取葡萄。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "憑著果子就可以認出樹來。人不能從荊棘上採無花果，也不能從蒺藜裡摘葡萄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡樹木看果子，就可以認出它來。人不是從荊棘上摘無花果，也不是從蒺藜裏摘葡萄。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For each tree is known by its own fruit. For men do not gather figs from thorns, nor do they pick grapes from a briar bush."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Each tree is recognized by its own fruit. People do not pick figs from thornbushes, or grapes from briers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A tree is identified by its fruit. Figs are never gathered from thornbushes, and grapes are not picked from bramble bushes."
      }
