    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到了黄昏，人们把患各种疾病的人带到耶稣面前。耶稣把手按在病人身上，一一治好他们，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "日落的時候，不論害甚麼病的人，都被帶到耶穌那裡；他一一為他們按手，醫好他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "日落的時候，凡有病人的，不論害甚麼病，都帶到耶穌那裏。耶穌按手在他們各人身上，醫好他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "While the sun was setting, all those who had any who were sick with various diseases brought them to Him; and laying His hands on each one of them, He was healing them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At sunset, the people brought to Jesus all who had various kinds of sickness, and laying his hands on each one, he healed them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As the sun went down that evening, people throughout the village brought sick family members to Jesus. No matter what their diseases were, the touch of his hand healed every one."
      }
    },
