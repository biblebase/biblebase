    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "律法教师想证明自己有理，就问：“那么，谁是我的邻居呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那人想證明自己有理，就對耶穌說：“誰是我的鄰舍呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那人要顯明自己有理，就對耶穌說：「誰是我的鄰舍呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But wishing to justify himself, he said to Jesus, “And who is my neighbor?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But he wanted to justify himself, so he asked Jesus, “And who is my neighbor?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The man wanted to justify his actions, so he asked Jesus, “And who is my neighbor?”"
      }
