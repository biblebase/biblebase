    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“一个人在小事上忠心，在大事上也会忠心；在小事上靠不住，在大事上更靠不住。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在最小的事上忠心的，在大事上也忠心；在最小的事上不義的，在大事上也不義。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "在最小的事上忠心的人，在很多事上也忠心；在最小的事上不义的人，在很多事上也不义。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人在最小的事上忠心，在大事上也忠心；在最小的事上不義，在大事上也不義。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“One who is     faithful in a very little is also faithful in much, and one who is dishonest in a very little is also dishonest in much."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“He who is faithful in a very little thing is faithful also in much; and he who is unrighteous in a very little thing is unrighteous also in much."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Whoever can be trusted with very little can also be trusted with much, and whoever is dishonest with very little will also be dishonest with much."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He who is faithful in what is least is faithful also in much; and he who is unjust in what is least is unjust also in much."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If you are faithful in little things, you will be faithful in large ones. But if you are dishonest in little things, you won’t be honest with greater responsibilities."
      }
    },
