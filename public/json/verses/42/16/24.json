    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就喊着说，‘我的先祖亚伯拉罕啊！求你可怜我，派拉撒路用指尖蘸点水来润润我的舌头吧！我在这火中实在痛苦不堪。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就喊著說：‘我祖亞伯拉罕啊，可憐我吧！打發拉撒路來用指頭蘸點水，涼涼我的舌頭吧！因為我在這火燄裡非常痛苦。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就喊着說：『我祖亞伯拉罕哪，可憐我吧！打發拉撒路來，用指頭尖蘸點水，涼涼我的舌頭；因為我在這火焰裏，極其痛苦。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he cried out and said, ‘Father Abraham, have mercy on me, and send Lazarus so that he may dip the tip of his finger in water and cool off my tongue, for I am in agony in this flame.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So he called to him, ‘Father Abraham, have pity on me and send Lazarus to dip the tip of his finger in water and cool my tongue, because I am in agony in this fire.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The rich man shouted, ‘Father Abraham, have some pity! Send Lazarus over here to dip the tip of his finger in water and cool my tongue. I am in anguish in these flames.’"
      }
    },
