    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“财主回答说，‘我的先祖亚伯拉罕啊！他们不会听的。但如果有一个死而复活的人去警告他们，他们肯定会悔改！’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：‘不然，我祖亞伯拉罕啊，如果有人從死人中復活，到他們那裡去，他們必定會悔改。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：『我祖亞伯拉罕哪，不是的，若有一個從死裏復活的，到他們那裏去的，他們必要悔改。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But he said, ‘No, father Abraham, but if someone goes to them from the dead, they will repent!’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘No, father Abraham,’ he said, ‘but if someone from the dead goes to them, they will repent.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The rich man replied, ‘No, Father Abraham! But if someone is sent to them from the dead, then they will repent of their sins and turn to God.’"
      }
