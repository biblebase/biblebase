    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“亚伯拉罕说，‘孩子啊！你要想想，你生前享福，而拉撒路受苦；如今他在这里得到安慰，而你受折磨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "亞伯拉罕說：‘孩子，你應該回想你生前享過福，同樣拉撒路受過苦，現在他在這裡得安慰，你卻要受苦了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞伯拉罕說：『兒啊，你該回想你生前享過福，拉撒路也受過苦；如今他在這裏得安慰，你倒受痛苦。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Abraham said, ‘Child, remember that during your life you received your good things, and likewise Lazarus bad things; but now he is being comforted here, and you are in agony."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But Abraham replied, ‘Son, remember that in your lifetime you received your good things, while Lazarus received bad things, but now he is comforted here and you are in agony."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But Abraham said to him, ‘Son, remember that during your lifetime you had everything you wanted, and Lazarus had nothing. So now he is here being comforted, and you are in anguish."
      }
    },
