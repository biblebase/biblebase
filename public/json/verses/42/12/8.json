    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我告诉你们，凡在人面前承认我的，人子在上帝的天使面前也必承认他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我告訴你們，凡在人面前承認我的，人子在　神的使者面前也承認他；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我又告訴你們，凡在人面前認我的，人子在神的使者面前也必認他；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“And I say to you, everyone who confesses Me before men, the Son of Man will confess him also before the angels of God;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I tell you, whoever publicly acknowledges me before others, the Son of Man will also acknowledge before the angels of God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I tell you the truth, everyone who acknowledges me publicly here on earth, the Son of Man will also acknowledge in the presence of God’s angels."
      }
    },
