    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听见的人都在想：“这孩子将来会怎样呢？因为主与他同在。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡聽見的就把這些事放在心裡，說：“這孩子將會成為怎樣的人呢？因為主的手與他同在。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡聽見的人都將這事放在心裏，說：「這個孩子將來怎麼樣呢？因為有主與他同在。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All who heard them kept them in mind, saying, “What then will this child turn out to be?” For the hand of the Lord was certainly with him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Everyone who heard this wondered about it, asking, “What then is this child going to be?” For the Lord’s hand was with him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Everyone who heard about it reflected on these events and asked, “What will this child turn out to be?” For the hand of the Lord was surely upon him in a special way."
      }
    },
