    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撒迦利亚对天使说：“我已经老了，我的妻子也上了年纪，我如何知道这是真的呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒迦利亞對天使說：“我怎麼能知道這事呢？我已經老了，我妻子也上了年紀。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "撒迦利亚对天使说：“我凭着什么能知道这事呢？我是个老人，我的妻子也上了年纪。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "撒迦利亞對天使說：「我憑着甚麼可知道這事呢？我已經老了，我的妻子也年紀老邁了。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And Zechariah said to the angel, “How shall I know this? For I am an old man, and my wife is advanced in years.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Zacharias said to the angel, “How will I know this for certain? For I am an old man and my wife is advanced in years.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Zechariah asked the angel, “How can I be sure of this? I am an old man and my wife is well along in years.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And Zacharias said to the angel, “How shall I know this? For I am an old man, and my wife is well advanced in years.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Zechariah said to the angel, “How can I be sure this will happen? I’m an old man now, and my wife is also well along in years.”"
      }
