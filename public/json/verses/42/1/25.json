    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她说：“主真是眷顾我，除掉了我不生育的羞耻。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“主在眷顧的日子，這樣看待我，要把我在人間的羞恥除掉。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：「主在眷顧我的日子，這樣看待我，要把我在人間的羞恥除掉。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“This is the way the Lord has dealt with me in the days when He looked with favor upon me, to take away my disgrace among men.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The Lord has done this for me,” she said. “In these days he has shown his favor and taken away my disgrace among the people.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“How kind the Lord is!” she exclaimed. “He has taken away my disgrace of having no children.”"
      }
