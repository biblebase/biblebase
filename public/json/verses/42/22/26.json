    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你们不可这样。相反，你们中间地位最高的，要像最卑微的；做首领的，要像服侍人的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但你們卻不要這樣；你們中間最大的，應當像最小的；作首領的，應當像服事人的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你們不可這樣；你們裏頭為大的，倒要像年幼的，為首領的，倒要像服事人的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But it is not this way with you, but the one who is the greatest among you must become like the youngest, and the leader like the servant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But you are not to be like that. Instead, the greatest among you should be like the youngest, and the one who rules like the one who serves."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But among you it will be different. Those who are the greatest among you should take the lowest rank, and the leader should be like a servant."
      }
    },
