    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "围观的人见状，无不捶胸顿足黯然离去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "聚集觀看的群眾，看見所發生的事，都捶著胸回去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "聚集觀看的眾人見了這所成的事都捶着胸回去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And all the crowds who came together for this spectacle, when they observed what had happened, began to return, beating their breasts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When all the people who had gathered to witness this sight saw what took place, they beat their breasts and went away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when all the crowd that came to see the crucifixion saw what had happened, they went home in deep sorrow."
      }
