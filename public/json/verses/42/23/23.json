      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人却继续大声喊叫，执意要求把耶稣钉在十字架上。最后，他们的声势占了上风。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但他們大聲吵鬧，要他把耶穌釘十字架，他們的聲音就得了勝。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们催逼彼拉多，大声要求把耶稣钉上十字架，他们的喊声终于得胜了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們大聲催逼彼拉多，求他把耶穌釘在十字架上。他們的聲音就得了勝。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But they were urgent, demanding with loud cries that he should be crucified. And their voices prevailed."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But they were insistent, with loud voices asking that He be crucified. And their voices began to prevail."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But with loud shouts they insistently demanded that he be crucified, and their shouts prevailed."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But they were insistent, demanding with loud voices that He be crucified. And the voices of these men and of the chief priests prevailed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the mob shouted louder and louder, demanding that Jesus be crucified, and their voices prevailed."
      }
    },
