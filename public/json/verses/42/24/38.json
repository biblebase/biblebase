    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“你们为什么忧心忡忡？为什么心存疑惑呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“你們為甚麼驚慌，為甚麼心裡疑惑呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「你們為甚麼愁煩？為甚麼心裏起疑念呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to them, “Why are you troubled, and why do doubts arise in your hearts?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to them, “Why are you troubled, and why do doubts rise in your minds?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why are you frightened?” he asked. “Why are your hearts filled with doubt?"
      }
    },
