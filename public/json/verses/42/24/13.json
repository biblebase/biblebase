      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And behold, two of them were going that very day to a village named Emmaus, which was about seven miles from Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now that same day two of them were going to a village called Emmaus, about seven miles from Jerusalem."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now behold, two of them were traveling that same day to a village called Emmaus, which was seven miles from Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "That same day two of Jesus’ followers were walking to the village of Emmaus, seven miles from Jerusalem."
