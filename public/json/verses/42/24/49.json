    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要把我父所应许的赐给你们。不过，你们要留在城里，直到你们得到天上来的能力。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要使我父的應許臨到你們身上，你們當在城裡等候，直到得著從上面來的能力。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "看哪！我要将我父所应许的差派到你们那里。不过你们要留在城里，直到领受了从上面来的能力。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要將我父所應許的降在你們身上，你們要在城裏等候，直到你們領受從上頭來的能力。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And behold, I am sending the promise of my Father upon you. But stay in the city until you are clothed with power from on high.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And behold, I am sending forth the promise of My Father upon you; but you are to stay in the city until you are clothed with power from on high.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am going to send you what my Father has promised; but stay in the city until you have been clothed with power from on high.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Behold, I send the Promise of My Father upon you; but tarry in the city of Jerusalem until you are endued with power from on high.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And now I will send the Holy Spirit, just as my Father promised. But stay here in the city until the Holy Spirit comes and fills you with power from heaven.”"
      }
