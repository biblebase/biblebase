    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣对他们说：“无知的人啊！为什么迟迟不肯相信先知的话呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說：“無知的人哪，先知所說的一切話，你們心裡信得太遲鈍了！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌對他們說：「無知的人哪，先知所說的一切話，你們的心信得太遲鈍了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said to them, “O foolish men and slow of heart to believe in all that the prophets have spoken!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to them, “How foolish you are, and how slow to believe all that the prophets have spoken!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus said to them, “You foolish people! You find it so hard to believe all that the prophets wrote in the Scriptures."
      }
    },
