    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣接着从摩西和众先知的记载开始，把有关自己的经文都向他们讲解明白。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他從摩西和眾先知起，把所有關於自己的經文，都給他們解釋明白了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "于是耶稣从摩西和所有的先知开始，向他们解释了经上一切关于自己的话。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是從摩西和眾先知起，凡經上所指着自己的話都給他們講解明白了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And beginning with Moses and all the Prophets, he interpreted to them in all the Scriptures the things concerning himself."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then beginning with Moses and with all the prophets, He explained to them the things concerning Himself in all the Scriptures."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And beginning with Moses and all the Prophets, he explained to them what was said in all the Scriptures concerning himself."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And beginning at Moses and all the Prophets, He expounded to them in all the Scriptures the things concerning Himself."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jesus took them through the writings of Moses and all the prophets, explaining from all the Scriptures the things concerning himself."
      }
