    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁不与我为友，就是与我为敌；谁不和我一起收聚，就是故意拆散。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不站在我這一邊的就是反對我的；不跟從一起收聚的就是分散的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不與我相合的，就是敵我的；不同我收聚的，就是分散的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who is not with Me is against Me; and he who does not gather with Me, scatters."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Whoever is not with me is against me, and whoever does not gather with me scatters."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Anyone who isn’t with me opposes me, and anyone who isn’t working with me is actually working against me."
      }
    },
