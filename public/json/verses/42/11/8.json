    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我告诉你们，那人虽不念朋友之情，但是倘若你不肯罢休，继续请求，他最终会起来满足你的愿望。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我告訴你們，即使不因為他是朋友而起來給他，也會因為他厚顏地直求，而起來照他所需要的給他。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我告诉你们：即使不因着是朋友而起来拿给他，还是会因着他毫无羞愧地求，就起来把他所要的拿给他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我告訴你們，雖不因他是朋友起來給他，但因他情詞迫切地直求，就必起來照他所需用的給他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I tell you, though he will not get up and give him anything because he is his friend, yet because of his impudence  he will rise and give him whatever he needs."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I tell you, even though he will not get up and give him anything because he is his friend, yet because of his persistence he will get up and give him as much as he needs."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I tell you, even though he will not get up and give you the bread because of friendship, yet because of your shameless audacity  he will surely get up and give you as much as you need."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "I say to you, though he will not rise and give to him because he is his friend, yet because of his persistence he will rise and give him as many as he needs."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I tell you this—though he won’t do it for friendship’s sake, if you keep knocking long enough, he will get up and give you whatever you need because of your shameless persistence."
      }
    },
