    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "若我是靠别西卜赶鬼，你们的子弟又是靠谁赶鬼呢？因此，他们要审判你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若靠別西卜趕鬼，你們的子孫趕鬼又靠誰呢？因此，他們必要作你們的審判官。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若靠着別西卜趕鬼，你們的子弟趕鬼又靠着誰呢？這樣，他們就要斷定你們的是非。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And if I by Beelzebul cast out demons, by whom do your sons cast them out? So they will be your judges."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now if I drive out demons by Beelzebul, by whom do your followers drive them out? So then, they will be your judges."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And if I am empowered by Satan, what about your own exorcists? They cast out demons, too, so they will condemn you for what you have said."
      }
    },
