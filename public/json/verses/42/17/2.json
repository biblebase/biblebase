    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁使一个卑微的弟兄失足犯罪，他的下场比把大磨石拴在他脖子上扔到海里还要惨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就算拿一塊大磨石拴在他的頸項上，把他沉在深海裡，比他使這小子中的一個犯罪還好。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "对他来说，就算脖子上拴着大磨石，被丢到海里，也比他使这些卑微人中的一个绊倒更好。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是把磨石拴在這人的頸項上，丟在海裏，還強如他把這小子裏的一個絆倒了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "It would be better for him if a millstone were hung around his neck and he were cast into the sea than that he should cause one of these little ones to sin."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It would be better for him if a millstone were hung around his neck and he were thrown into the sea, than that he would cause one of these little ones to stumble."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It would be better for them to be thrown into the sea with a millstone tied around their neck than to cause one of these little ones to stumble."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "It would be better for him if a millstone were hung around his neck, and he were thrown into the sea, than that he should offend one of these little ones."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It would be better to be thrown into the sea with a millstone hung around your neck than to cause one of these little ones to fall into sin."
      }
    },
