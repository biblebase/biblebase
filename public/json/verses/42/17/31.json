    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那天，在自己屋顶上的，不要下来收拾行李；在田里工作的，也不要回家。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當那日，人在房頂上，器具在屋裡，不要下來拿；在田裡的，照樣不要回家。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當那日，人在房上，器具在屋裏，不要下來拿；人在田裏，也不要回家。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "On that day, the one who is on the housetop and whose goods are in the house must not go down to take them out; and likewise the one who is in the field must not turn back."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On that day no one who is on the housetop, with possessions inside, should go down to get them. Likewise, no one in the field should go back for anything."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "On that day a person out on the deck of a roof must not go down into the house to pack. A person out in the field must not return home."
      }
    },
