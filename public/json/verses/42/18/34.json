    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "使徒却一点也不明白，因为这番话的含意是隐藏的，他们听不明白。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些話的含義對門徒是隱藏的。他們聽了，一點也不明白，不知道他說的是甚麼。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "但门徒们一点也不领悟这些话。这话对他们是隐藏的，他们不知道他在说什么。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些事門徒一樣也不懂得，意思乃是隱藏的；他們不曉得所說的是甚麼。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But they understood none of these things. This saying was hidden from them, and they did not grasp what was said."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the disciples understood none of these things, and the meaning of this statement was hidden from them, and they did not comprehend the things that were said."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The disciples did not understand any of this. Its meaning was hidden from them, and they did not know what he was talking about."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But they understood none of these things; this saying was hidden from them, and they did not know the things which were spoken."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But they didn’t understand any of this. The significance of his words was hidden from them, and they failed to grasp what he was talking about."
      }
    },
