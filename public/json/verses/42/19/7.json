    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "百姓见状，都埋怨说：“祂怎么到一个罪人家里作客？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人看見就紛紛議論說：“他竟到罪人家裡去住宿！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人看見，都私下議論說：「他竟到罪人家裏去住宿。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When they saw it, they all began to grumble, saying, “He has gone to be the guest of a man who is a sinner.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the people saw this and began to mutter, “He has gone to be the guest of a sinner.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the people were displeased. “He has gone to be the guest of a notorious sinner,” they grumbled."
      }
