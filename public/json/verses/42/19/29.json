    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "快到橄榄山附近的伯法其和伯大尼时，耶稣派了两个门徒，说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "將到伯法其和伯大尼，就在橄欖山那裡，他差派兩個門徒，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "快到伯法其和伯大尼，那叫做“橄榄园”的山，耶稣派了两个门徒，"
      },
      "cunp": {
        "version_id": "CUNP",
