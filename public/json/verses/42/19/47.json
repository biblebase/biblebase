      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂天天在圣殿教导人，祭司长、律法教师和百姓的官长都想杀祂，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他天天在聖殿裡教導人，祭司長、經學家和民間的首領，都想殺害他；"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣天天在圣殿里教导人。祭司长们、经文士们和民间的首领们想要杀害他，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌天天在殿裏教訓人。祭司長和文士與百姓的尊長都想要殺他，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And he was teaching daily in the temple. The chief priests and the scribes and the principal men of the people were seeking to destroy him,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He was teaching daily in the temple; but the chief priests and the scribes and the leading men among the people were trying to destroy Him,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every day he was teaching at the temple. But the chief priests, the teachers of the law and the leaders among the people were trying to kill him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And He was teaching daily in the temple. But the chief priests, the scribes, and the leaders of the people sought to destroy Him,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After that, he taught daily in the Temple, but the leading priests, the teachers of religious law, and the other leaders of the people began planning how to kill him."
      }
    },
