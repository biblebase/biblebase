    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那些法利赛人和律法教师开始议论起来：“这个说话亵渎上帝的人是谁啊？除了上帝以外，谁能够赦罪？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "經學家和法利賽人就議論起來，說：“這人是誰，竟然說僭妄的話？除　神一位以外，誰能赦罪呢？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "那些经文士和法利赛人就开始想：“这个人是谁？竟说亵渎的话。除了神一位之外，谁能赦免罪呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "文士和法利賽人就議論說：「這說僭妄話的是誰？除了神以外，誰能赦罪呢？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the scribes and the Pharisees began to question, saying, “Who is this who speaks blasphemies? Who can forgive sins but God alone?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The scribes and the Pharisees began to reason, saying, “Who is this man who speaks blasphemies? Who can forgive sins, but God alone?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Pharisees and the teachers of the law began thinking to themselves, “Who is this fellow who speaks blasphemy? Who can forgive sins but God alone?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the scribes and the Pharisees began to reason, saying, “Who is this who speaks blasphemies? Who can forgive sins but God alone?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Pharisees and teachers of religious law said to themselves, “Who does he think he is? That’s blasphemy! Only God can forgive sins!”"
      }
