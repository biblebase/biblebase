    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在第八天，婴孩接受了割礼，祂的名字叫耶稣，是玛丽亚怀孕前天使取的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "滿了八天，替孩子行割禮的時候，就給他起名叫耶穌，就是他成胎之前，天使所起的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "滿了八天，就給孩子行割禮，與他起名叫耶穌；這就是沒有成胎以前，天使所起的名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And when eight days had passed, before His circumcision, His name was then called Jesus, the name given by the angel before He was conceived in the womb."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On the eighth day, when it was time to circumcise the child, he was named Jesus, the name the angel had given him before he was conceived."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Eight days later, when the baby was circumcised, he was named Jesus, the name given him by the angel even before he was conceived."
      }
