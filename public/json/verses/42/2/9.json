      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "主的一位使者站在他們旁邊，主的榮光四面照著他們，他們就非常害怕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有主的使者站在他們旁邊，主的榮光四面照着他們；牧羊的人就甚懼怕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And an angel of the Lord suddenly stood before them, and the glory of the Lord shone around them; and they were terribly frightened."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "An angel of the Lord appeared to them, and the glory of the Lord shone around them, and they were terrified."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Suddenly, an angel of the Lord appeared among them, and the radiance of the Lord’s glory surrounded them. They were terrified,"
      }
    },
