    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣问：“谁摸我？”没有人说摸过祂。 彼得说：“老师，众人都在拥挤你。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說：“摸我的是誰？”眾人都不承認。彼得說：“主啊，眾人都擁擠著你。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「摸我的是誰？」眾人都不承認。彼得和同行的人都說：「夫子，眾人擁擁擠擠緊靠着你。 」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus said, “Who is the one who touched Me?” And while they were all denying it, Peter said, “Master, the people are crowding and pressing in on You.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Who touched me?” Jesus asked. When they all denied it, Peter said, “Master, the people are crowding and pressing against you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Who touched me?” Jesus asked. Everyone denied it, and Peter said, “Master, this whole crowd is pressing up against you.”"
      }
