    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣便说：“上帝国的奥秘只让你们知道，对其他人，我就用比喻，使他们看却看不见，听却听不明白。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說：“　神的國的奧祕，只給你們知道，對別人就用比喻，叫他們看卻看不見，聽卻聽不明白。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他說：「神國的奧祕只叫你們知道；至於別人，就用比喻，叫他們看也看不見，聽也聽不明。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He said, “To you it has been granted to know the mysteries of the kingdom of God, but to the rest it is in parables, so that seeing they may not see, and hearing they may not understand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said, “The knowledge of the secrets of the kingdom of God has been given to you, but to others I speak in parables, so that, “ ‘though seeing, they may not see; though hearing, they may not understand.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He replied, “You are permitted to understand the secrets of the Kingdom of God. But I use parables to teach the others so that the Scriptures might be fulfilled: ‘When they look, they won’t really see. When they hear, they won’t understand.’"
      }
