    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣问他：“你叫什么名字？” 他说：“我叫‘群’。”因为有一大群鬼附在那人身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌問他：“你叫甚麼名字？”他說：“群。”因為進到那人裡面的鬼很多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌問他說：「你名叫甚麼？」他說：「我名叫『羣』」；這是因為附着他的鬼多。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Jesus asked him, “What is your name?” And he said, “Legion”; for many demons had entered him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus asked him, “What is your name?” “Legion,” he replied, because many demons had gone into him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus demanded, “What is your name?” “Legion,” he replied, for he was filled with many demons."
      }
