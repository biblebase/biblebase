    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.21.25",
            "paragraphs": [
              "二一25總額約為二百四十安士（15磅或6.8千克）金子，是即將用來建聖殿之整塊土地的地價。撒母耳記下二十四章24節只記載禾場與牛的購入。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，大卫就用七公斤金子买了阿珥楠的麦场。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是大衛稱了六千八百四十克金子給阿珥楠，買了那塊地。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是大衛為那塊地平了六百舍客勒金子給阿珥楠。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So David gave Ornan 600 shekels of gold by weight for the site."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So David paid Araunah six hundred shekels of gold for the site."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So David gave Araunah 600 pieces of gold in payment for the threshing floor."
      }
    },
