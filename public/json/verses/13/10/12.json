    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们所有的勇士就去把扫罗和他儿子们的尸体运到雅比，葬在雅比的橡树下，并禁食七天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有的勇士就動身，把掃羅的屍體和他兒子的屍體收殮起來，帶回雅比，把他們的骸骨埋葬在雅比的橡樹下，並且禁食七天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們中間所有的勇士就起身前去，將掃羅和他兒子的屍身送到雅比，將他們的屍骨葬在雅比的橡樹下，就禁食七日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "all the valiant men arose and took away the body of Saul and the bodies of his sons and brought them to Jabesh, and they buried their bones under the oak in Jabesh, and fasted seven days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "all their valiant men went and took the bodies of Saul and his sons and brought them to Jabesh. Then they buried their bones under the great tree in Jabesh, and they fasted seven days."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "all their mighty warriors brought the bodies of Saul and his sons back to Jabesh. Then they buried their bones beneath the great tree at Jabesh, and they fasted for seven days."
      }
    },
