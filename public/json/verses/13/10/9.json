    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就剥下扫罗的盔甲，割下他的头颅，并派人到非利士四境向他们的偶像和民众通告消息。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們就剝了他的衣服，拿了他的頭和兵器，又派人到非利士地的各處，去向他們的偶像和人民報信。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就剝了他的軍裝，割下他的首級，打發人到   非利士地的四境報信與他們的偶像和眾民，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they stripped him and took his head and his armor and sent messengers around the land of the Philistines to carry the good news to their idols and to the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They stripped him and took his head and his armor, and sent messengers throughout the land of the Philistines to proclaim the news among their idols and their people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So they stripped off Saul’s armor and cut off his head. Then they proclaimed the good news of Saul’s death before their idols and to the people throughout the land of Philistia."
      }
    },
