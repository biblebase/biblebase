    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚伦的子孙得到避难城希伯仑、立拿及其周围的草场、雅提珥、以实提莫及其周围的草场、"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人把避難城希伯崙給了亞倫的子孫，還有立拿和屬於立拿的草場，雅提珥、以實提莫和屬於以實提莫的草場，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞倫的子孫得了逃城希伯崙，又得了立拿與其郊野，雅提珥、以實提莫與其郊野；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To the sons of Aaron they gave the following cities of refuge: Hebron, Libnah also with its pasture lands, Jattir, Eshtemoa with its pasture lands,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So the descendants of Aaron were given Hebron (a city of refuge), and Libnah, Jattir, Eshtemoa,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the descendants of Aaron were given the following towns, each with its pasturelands: Hebron (a city of refuge), Libnah, Jattir, Eshtemoa,"
      }
    },
