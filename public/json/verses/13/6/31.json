    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约柜安放在耶和华的殿中之后，大卫便派人在那里负责歌乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "自約櫃安放妥當以後，大衛指派在耶和華殿裡負責歌唱事奉的就是以下這些人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約櫃安設之後，大衛派人在耶和華殿中管理歌唱的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now these are those whom David appointed over the service of song in the house of the Lord, after the ark rested there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These are the men David put in charge of the music in the house of the Lord after the ark came to rest there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "David assigned the following men to lead the music at the house of the Lord after the Ark was placed there."
      }
    },
