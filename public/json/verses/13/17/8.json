    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "无论你去哪里，我都与你同在，为你铲除所有仇敌。现在我要使你声名远播，与世上的伟人齐名。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "無論你到哪裡去，我總與你同在；在你面前消滅你所有的仇敵；我必使你獲得名聲，好像世上偉人的名聲一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你無論往哪裏去，我常與你同在，剪除你的一切仇敵；我必使你得大名，好像世上大大有名的人一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have been with you wherever you have gone, and have cut off all your enemies from before you; and I will make you a name like the name of the great ones who are in the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have been with you wherever you have gone, and I have cut off all your enemies from before you. Now I will make your name like the names of the greatest men on earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have been with you wherever you have gone, and I have destroyed all your enemies before your eyes. Now I will make your name as famous as anyone who has ever lived on the earth!"
      }
    },
