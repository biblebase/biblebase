    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你将尊荣加于仆人，我还能说什么？因为你了解仆人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你把榮耀加在你僕人身上，我還有甚麼話可以對你說呢？你認識你的僕人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你加於僕人的尊榮，我還有何言可說呢？因為你知道你的僕人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "What more can David still say to You concerning the honor bestowed on Your servant? For You know Your servant."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“What more can David say to you for honoring your servant? For you know your servant,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What more can I say to you about the way you have honored me? You know what your servant is really like."
      }
