    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要為我的子民以色列人選定一個地方，栽培他們，使他們住在自己的地方，不再受攪擾，也不再像從前被惡徒苦害他們，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必為我民以色列選定一個地方，栽培他們，使他們住自己的地方，不再遷移；凶惡之子也不像從前擾害他們，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will appoint a place for My people Israel, and will plant them, so that they may dwell in their own place and not be moved again; and the wicked will not waste them anymore as formerly,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And I will provide a place for my people Israel and will plant them so that they can have a home of their own and no longer be disturbed. Wicked people will not oppress them anymore, as they did at the beginning"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I will provide a homeland for my people Israel, planting them in a secure place where they will never be disturbed. Evil nations won’t oppress them as they’ve done in the past,"
      }
    },
