    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿你赐福仆人的王朝，使它在你面前永远坚立。耶和华啊，你已赐下祝福，你的祝福是永远的！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在請你賜福你僕人的家，使它在你面前永遠存留。耶和華啊，因為你已經賜福了你僕人的家，它必永遠蒙福。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在你喜悅賜福與僕人的家，可以永存在你面前。耶和華啊，你已經賜福，還要賜福到永遠。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And now it has pleased You to bless the house of Your servant, that it may continue forever before You; for You, O Lord, have blessed, and it is blessed forever.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now you have been pleased to bless the house of your servant, that it may continue forever in your sight; for you, Lord, have blessed it, and it will be blessed forever.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And now, it has pleased you to bless the house of your servant, so that it will continue forever before you. For when you grant a blessing, O Lord, it is an eternal blessing!”"
      }
