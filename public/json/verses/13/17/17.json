    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝啊，不止如此，你还应许将来要恩待仆人的后裔。耶和华上帝啊，你视我为尊贵的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神啊，這在你眼中看為小事，你對你僕人的家的未來發出應許！耶和華　神啊，你看我好像一個高貴的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神啊，這在你眼中還看為小，又應許你僕人的家至於久遠。耶和華神啊，你看顧我好像看顧高貴的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This was a small thing in Your eyes, O God; but You have spoken of Your servant’s house for a great while to come, and have regarded me according to the standard of a man of high degree, O Lord God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And as if this were not enough in your sight, my God, you have spoken about the future of the house of your servant. You, Lord God, have looked on me as though I were the most exalted of men."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And now, O God, in addition to everything else, you speak of giving your servant a lasting dynasty! You speak as though I were someone very great, O Lord God!"
      }
    },
