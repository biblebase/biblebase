      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一些犹大人、便雅悯人、以法莲人和玛拿西人住在耶路撒冷，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在耶路撒冷居住的，有猶大支派、便雅憫支派、以法蓮支派和瑪拿西支派的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "住在耶路撒冷的有猶大人、便雅憫人、以法蓮人、瑪拿西人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Some of the sons of Judah, of the sons of Benjamin and of the sons of Ephraim and Manasseh lived in Jerusalem:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those from Judah, from Benjamin, and from Ephraim and Manasseh who lived in Jerusalem were:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some of the people from the tribes of Judah, Benjamin, Ephraim, and Manasseh came and settled in Jerusalem."
      }
    },
