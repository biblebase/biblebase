    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ch.28.19",
            "paragraphs": [
              "二八19顯然大衛在異象中已經看見整座建築物，並且為所羅門劃下來。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫说：“以上这一切都是耶和华亲手指示我的，让我明白工作的细节。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛說：“以上這一切，就是一切工作的樣式，都是耶和華用手寫給我，使我明白的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛說：「這一切工作的樣式都是耶和華用手劃出來使我明白的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“All this,” said David, “the Lord made me understand in writing by His hand upon me, all the details of this pattern.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“All this,” David said, “I have in writing as a result of the Lord’s hand on me, and he enabled me to understand all the details of the plan.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Every part of this plan,” David told Solomon, “was given to me in writing from the hand of the Lord.”"
      }
    },
