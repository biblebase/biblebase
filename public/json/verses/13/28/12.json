    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又将心里构想的耶和华殿的院子、四周的房屋、殿的库房和圣物库房的图样都交给他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又把他心中所想的圖樣，就是耶和華殿的院子、四周所有的房屋、神殿裡的庫房和聖物庫房的圖樣，都指示了他；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又將被靈感動所得的樣式，就是耶和華神殿的院子、周圍的房屋、殿的府庫，和聖物府庫的一切樣式都指示他；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and the plan of all that he had in mind, for the courts of the house of the Lord, and for all the surrounding rooms, for the storehouses of the house of God and for the storehouses of the dedicated things;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He gave him the plans of all that the Spirit had put in his mind for the courts of the temple of the Lord and all the surrounding rooms, for the treasuries of the temple of God and for the treasuries for the dedicated things."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "David also gave Solomon all the plans he had in mind for the courtyards of the Lord’s Temple, the outside rooms, the treasuries, and the rooms for the gifts dedicated to the Lord."
      }
    },
