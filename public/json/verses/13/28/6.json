    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华对我说，‘你儿子所罗门必建造我的殿宇和庭院，因为我拣选他做我的儿子，我也必做他的父亲。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華對我說：‘你的兒子所羅門要建造我的殿宇和庭院；因為我已經揀選他作我的兒子，我也要作他的父親。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華對我說：『你兒子所羅門必建造我的殿和院宇；因為我揀選他作我的子，我也必作他的父。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to me, ‘Your son Solomon is the one who shall build My house and My courts; for I have chosen him to be a son to Me, and I will be a father to him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He said to me: ‘Solomon your son is the one who will build my house and my courts, for I have chosen him to be my son, and I will be his father."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He said to me, ‘Your son Solomon will build my Temple and its courtyards, for I have chosen him as my son, and I will be his father."
      }
    },
