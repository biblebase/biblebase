    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "1ch.16.29",
            "paragraphs": [
              "16:29",
              "當以聖潔的妝飾：真正的敬拜必須以聖潔為妝飾(比較代下20:21)。只有我們心存敬畏和清潔，切切地渴慕親近神，堅決抵制一切冒犯神的聖潔性情之事時，我們從靈裏發出的歡喜敬拜(15:28)才能蒙神悅納(參7節註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要将耶和华当得的荣耀归给祂， 要将供物献给祂， 要穿上圣洁的衣服敬拜祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要把耶和華的名應得的榮耀歸給他， 拿著禮物到他面前來， 要以聖潔的裝飾敬拜耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要將耶和華的名所當得的榮耀歸給他， 拿供物來奉到他面前； 當以聖潔的妝飾敬拜耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ascribe to the Lord the glory due His name; Bring an offering, and come before Him; Worship the Lord in holy array."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ascribe to the Lord the glory due his name; bring an offering and come before him. Worship the Lord in the splendor of his holiness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Give to the Lord the glory he deserves! Bring your offering and come into his presence. Worship the Lord in all his holy splendor."
      }
    },
