    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫指派一些利未人在耶和华的约柜前事奉，颂扬、称谢和赞美以色列的上帝耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛派了一部分利未人，在耶和華的約櫃前事奉、頌揚、稱謝和讚美耶和華以色列的　神。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛派幾個利未人在耶和華的約櫃前事奉，頌揚，稱謝，讚美耶和華－以色列的神："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He appointed some of the Levites as ministers before the ark of the Lord, even to celebrate and to thank and praise the Lord God of Israel:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He appointed some of the Levites to minister before the ark of the Lord, to extol, thank, and praise the Lord, the God of Israel:"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "David appointed the following Levites to lead the people in worship before the Ark of the Lord—to invoke his blessings, to give thanks, and to praise the Lord, the God of Israel."
      }
    },
