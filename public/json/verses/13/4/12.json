    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "伊施屯生伯拉巴、巴西亚和提欣拿，提欣拿是珥拿辖的父亲。这些都是利迦人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "伊施屯生伯拉巴、巴西亞和珥拿轄的父親提欣拿；這些都是利迦人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "伊施屯生伯拉巴、巴西亞，並珥拿轄之祖提欣拿，這都是利迦人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Eshton became the father of Beth-rapha and Paseah, and Tehinnah the father of Ir-nahash. These are the men of Recah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Eshton was the father of Beth Rapha, Paseah and Tehinnah the father of Ir Nahash. These were the men of Rekah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Eshton was the father of Beth-rapha, Paseah, and Tehinnah. Tehinnah was the father of Ir-nahash. These were the descendants of Recah."
      }
