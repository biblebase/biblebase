    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "1ch.27.1",
            "paragraphs": [
              "27:1",
              "本章沒有帶註解"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "1ch.27.1-15",
            "paragraphs": [
              "二七1～15大衛的軍隊有二十八萬八千人，分成十二隊，每隊二萬四千人，每隊每年輪流服役一個月。在十二名將領中，有八名來自猶大支派。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人的族长、千夫长、百夫长及其他官长都分定班次，每班两万四千人，每年按月轮流服侍王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人各家族的首領、千夫長、百夫長和服事王的所有官長，都按著他們的數目分定班次，每年按月輪班，每班有二萬四千人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人的族長、千夫長、百夫長，和官長都分定班次，每班是二萬四千人，週年按月輪流，替換出入服事王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now this is the enumeration of the sons of Israel, the heads of fathers’ households, the commanders of thousands and of hundreds, and their officers who served the king in all the affairs of the divisions which came in and went out month by month throughout all the months of the year, each division numbering 24,000:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is the list of the Israelites—heads of families, commanders of thousands and commanders of hundreds, and their officers, who served the king in all that concerned the army divisions that were on duty month by month throughout the year. Each division consisted of 24,000 men."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is the list of Israelite generals and captains, and their officers, who served the king by supervising the army divisions that were on duty each month of the year. Each division served for one month and had 24,000 troops."
      }
    },
