    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他对所罗门说：“儿啊，我本想为我的上帝耶和华的名建造一座殿宇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只是耶和華的話臨到我說：‘你流了許多人的血，打了很多大仗；你不可為我的名建造殿宇，因為你在我面前流了許多人的血在地上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "對所羅門說：「我兒啊，我心裏本想為耶和華－我神的名建造殿宇，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "David said to Solomon, “My son, I had it in my heart to build a house to the name of the Lord my God."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David said to Solomon, “My son, I had intended to build a house to the name of the Lord my God."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "David said to Solomon: “My son, I had it in my heart to build a house for the Name of the Lord my God."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And David said to Solomon: “My son, as for me, it was in my mind to build a house to the name of the Lord my God;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“My son, I wanted to build a Temple to honor the name of the Lord my God,” David told him."
      }
