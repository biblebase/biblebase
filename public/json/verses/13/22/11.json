      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“儿啊，愿耶和华与你同在，使你亨通，照你的上帝耶和华的吩咐去建造祂的殿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "深願耶和華賜你聰明和智慧，使你治理以色列的時候，謹守耶和華你的　神的律法。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我兒啊，現今願耶和華與你同在，使你亨通，照他指着你說的話，建造耶和華－你神的殿。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Now, my son, the Lord be with you, so that you may succeed in building the house of the Lord your God, as he has spoken concerning you."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now, my son, the Lord be with you that you may be successful, and build the house of the Lord your God just as He has spoken concerning you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Now, my son, the Lord be with you, and may you have success and build the house of the Lord your God, as he said you would."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now, my son, may the Lord be with you; and may you prosper, and build the house of the Lord your God, as He has said to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Now, my son, may the Lord be with you and give you success as you follow his directions in building the Temple of the Lord your God."
      }
    },
