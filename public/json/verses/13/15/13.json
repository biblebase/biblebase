    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "1ch.15.13",
            "paragraphs": [
              "15:13",
              "先前沒有……求問耶和華：參13:10註。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "先前你们没有抬约柜，我们也没有按规矩求问我们的上帝耶和华，以致祂向我们发怒。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "先前沒有你們，耶和華我們的　神就擊打我們，因為我們沒有按照定例尋求他。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因你們先前沒有擡這約櫃，按定例求問耶和華－我們的神，所以他刑罰我們。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because you did not carry it at the first, the Lord our God made an outburst on us, for we did not seek Him according to the ordinance.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It was because you, the Levites, did not bring it up the first time that the Lord our God broke out in anger against us. We did not inquire of him about how to do it in the prescribed way.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because you Levites did not carry the Ark the first time, the anger of the Lord our God burst out against us. We failed to ask God how to move it properly.”"
      }
    },
