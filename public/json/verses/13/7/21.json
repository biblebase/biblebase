    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他哈的儿子是撒拔，撒拔的儿子是书提拉。以法莲的另外两个儿子以谢和以列因为偷迦特人的牲畜而被当地人杀死了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他哈的兒子是撒拔，撒拔的兒子是書提拉。以謝和以列因為下去奪取迦特人的牲畜，被當地出生的迦特人殺了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他哈的兒子是撒拔；撒拔的兒子是書提拉。以法蓮又生以謝、以列；這二人因為下去奪取迦特人的牲畜，被本地的迦特人殺了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Zabad his son, Shuthelah his son, and Ezer and Elead whom the men of Gath who were born in the land killed, because they came down to take their livestock."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Zabad his son and Shuthelah his son. Ezer and Elead were killed by the native-born men of Gath, when they went down to seize their livestock."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Zabad, Shuthelah, Ezer, and Elead. These two were killed trying to steal livestock from the local farmers near Gath."
      }
    },
