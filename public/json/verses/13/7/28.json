    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以法莲人的地业和住处包括伯特利及其四围的村庄、东部的拿兰、西部的基色及其四围的村庄、示剑及其四围的村庄，远至迦萨及其四围的村庄。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以法蓮人的地業和居住的地方，是伯特利和屬於伯特利的村莊，東邊有拿蘭，西邊有基色和屬於基色的村莊；示劍和屬於示劍的村莊，直到迦薩和屬於迦薩的村莊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以法蓮人的地業和住處是伯特利與其村莊；東邊拿蘭，西邊基色與其村莊；示劍與其村莊，直到迦薩與其村莊；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their possessions and settlements were Bethel with its towns, and to the east Naaran, and to the west Gezer with its towns, and Shechem with its towns as far as Ayyah with its towns,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their lands and settlements included Bethel and its surrounding villages, Naaran to the east, Gezer and its villages to the west, and Shechem and its villages all the way to Ayyah and its villages."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The descendants of Ephraim lived in the territory that included Bethel and its surrounding towns to the south, Naaran to the east, Gezer and its villages to the west, and Shechem and its surrounding villages to the north as far as Ayyah and its towns."
      }
    },
