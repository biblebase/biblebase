    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫年纪老迈，享尽富贵尊荣后，寿终正寝。他儿子所罗门继位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛年紀老邁，壽數滿足，享盡富足和尊榮才去世；他的兒子所羅門接續他作王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他年紀老邁，日子滿足，享受豐富、尊榮，就死了。他兒子所羅門接續他作王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he died in a ripe old age, full of days, riches and honor; and his son Solomon reigned in his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He died at a good old age, having enjoyed long life, wealth and honor. His son Solomon succeeded him as king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He died at a ripe old age, having enjoyed long life, wealth, and honor. Then his son Solomon ruled in his place."
      }
