    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们在你面前只是客旅，是寄居的，像我们祖先一样；我们在世的日子就像影子一样转瞬即逝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們在你面前是客旅，是寄居的，像我們的列祖一樣；我們在世上的日子好像影兒，沒有指望。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們在你面前是客旅，是寄居的，與我們列祖一樣。我們在世的日子如影兒，不能長存。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For we are sojourners before You, and tenants, as all our fathers were; our days on the earth are like a shadow, and there is no hope."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We are foreigners and strangers in your sight, as were all our ancestors. Our days on earth are like a shadow, without hope."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We are here for only a moment, visitors and strangers in the land as our ancestors were before us. Our days on earth are like a passing shadow, gone so soon without a trace."
      }
    },
