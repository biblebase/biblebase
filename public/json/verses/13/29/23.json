    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是，所罗门登上了耶和华所赐的王位，接替他父亲大卫做王。他凡事亨通，以色列众人都服从他，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是所羅門坐在耶和華所賜的位上，接續他父親大衛作王；他凡事亨通，以色列眾人都聽從他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是所羅門坐在耶和華所賜的位上，接續他父親大衛作王，萬事亨通；以色列眾人也都聽從他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Solomon sat on the throne of the Lord as king instead of David his father; and he prospered, and all Israel obeyed him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Solomon sat on the throne of the Lord as king in place of his father David. He prospered and all Israel obeyed him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Solomon took the throne of the Lord in place of his father, David, and he succeeded in everything, and all Israel obeyed him."
      }
    },
