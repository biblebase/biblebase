    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "结果，各族长、以色列各支派的首领、千夫长、百夫长和负责王室事务的官员都乐意奉献，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是各家族的領袖、以色列各支派的領袖、千夫長、百夫長和管理王的事務的領袖，都樂意奉獻。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，眾族長和以色列各支派的首領、千夫長、百夫長，並監管王工的官長，都樂意獻上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the rulers of the fathers’ households, and the princes of the tribes of Israel, and the commanders of thousands and of hundreds, with the overseers over the king’s work, offered willingly;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the leaders of families, the officers of the tribes of Israel, the commanders of thousands and commanders of hundreds, and the officials in charge of the king’s work gave willingly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the family leaders, the leaders of the tribes of Israel, the generals and captains of the army, and the king’s administrative officers all gave willingly."
      }
    },
