    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "米基罗是示米暗的父亲。这些人也住在耶路撒冷，与他们的亲族为邻。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "米基羅生示米暗。這些人也和他們的親族一同住在耶路撒冷，互相為鄰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "米基羅生示米暗。這些人和他們的弟兄在耶路撒冷對面居住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Mikloth became the father of Shimeah. And they also lived with their relatives in Jerusalem opposite their other relatives."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and Mikloth, who was the father of Shimeah. They too lived near their relatives in Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "and Mikloth, who was the father of Shimeam. All these families lived near each other in Jerusalem."
      }
