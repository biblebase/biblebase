      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把余下的军兵交给他的兄弟亚比筛领导，迎战亚扪人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把其餘的人交在他兄弟亞比篩手下，使他們擺陣去迎戰亞捫人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其餘的兵交與他兄弟亞比篩，對着亞捫人擺陣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The rest of his men he put in the charge of Abishai his brother, and they were arrayed against the Ammonites."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the remainder of the people he placed in the hand of Abshai his brother; and they arrayed themselves against the sons of Ammon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He put the rest of the men under the command of Abishai his brother, and they were deployed against the Ammonites."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the rest of the people he put under the command of Abishai his brother, and they set themselves in battle array against the people of Ammon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He left the rest of the army under the command of his brother Abishai, who was to attack the Ammonites."
      }
