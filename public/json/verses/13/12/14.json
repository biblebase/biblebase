    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们都是迦得支派的将领，级别最低的统领一百人，最高的统领一千人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些都是迦得支派中的軍長，最小的一個能抵擋一百人，最大的一個能抵擋一千人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這都是迦得人中的軍長，至小的能抵一百人，至大的能抵一千人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These of the sons of Gad were captains of the army; he who was least was equal to a hundred and the greatest to a thousand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These Gadites were army commanders; the least was a match for a hundred, and the greatest for a thousand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These warriors from Gad were army commanders. The weakest among them could take on a hundred regular troops, and the strongest could take on a thousand!"
      }
