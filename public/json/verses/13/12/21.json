    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们都是将领，作战英勇，帮助大卫抗击匪徒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們幫助大衛，與他一同攻擊賊匪；他們全是英勇的戰士，是軍隊的領袖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些人幫助大衛攻擊羣賊；他們都是大能的勇士，且作軍長。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They helped David against the band of raiders, for they were all mighty men of valor, and were captains in the army."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They helped David against raiding bands, for all of them were brave warriors, and they were commanders in his army."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They helped David chase down bands of raiders, for they were all brave and able warriors who became commanders in his army."
      }
    },
