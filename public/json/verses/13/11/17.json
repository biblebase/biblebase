      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David had a craving and said, “Oh that someone would give me water to drink from the well of Bethlehem, which is by the gate!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "David longed for water and said, “Oh, that someone would get me a drink of water from the well near the gate of Bethlehem!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "David remarked longingly to his men, “Oh, how I would love some of that good water from the well by the gate in Bethlehem.”"
      }
    },
