    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫和以色列众人来到耶路撒冷，即耶布斯。那时耶布斯人住在那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大衛和以色列眾人來到耶路撒冷，耶路撒冷就是耶布斯。那時，當地的居民耶布斯人住在那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛和以色列眾人到了耶路撒冷，就是耶布斯；那時耶布斯人住在那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then David and all Israel went to Jerusalem (that is, Jebus); and the Jebusites, the inhabitants of the land, were there."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "David and all the Israelites marched to Jerusalem (that is, Jebus). The Jebusites who lived there"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then David and all Israel went to Jerusalem (or Jebus, as it used to be called), where the Jebusites, the original inhabitants of the land, were living."
      }
