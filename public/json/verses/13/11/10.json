    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以下是大卫手下众勇士的统领，他们和以色列百姓竭力拥护大卫做王，正如耶和华对以色列的应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以下這些人是大衛眾勇士的首領，就是全力幫助他得國的，又與以色列人照著耶和華對以色列人所說的話一同立他作王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以下記錄跟隨大衛勇士的首領，就是奮勇幫助他得國、照着耶和華吩咐以色列人的話、與以色列人一同立他作王的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now these are the chiefs of David’s mighty men, who gave him strong support in his kingdom, together with all Israel, to make him king, according to the word of the Lord concerning Israel."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now these are the heads of the mighty men whom David had, who gave him strong support in his kingdom, together with all Israel, to make him king, according to the word of the Lord concerning Israel."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These were the chiefs of David’s mighty warriors—they, together with all Israel, gave his kingship strong support to extend it over the whole land, as the Lord had promised—"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now these were the heads of the mighty men whom David had, who strengthened themselves with him in his kingdom, with all Israel, to make him king, according to the word of the Lord concerning Israel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These are the leaders of David’s mighty warriors. Together with all Israel, they decided to make David their king, just as the Lord had promised concerning Israel."
      }
    },
