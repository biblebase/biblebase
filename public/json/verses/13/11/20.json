    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约押的兄弟亚比筛是这三个勇士的统领，他曾挥矛刺死三百人，在三勇士中出了名，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約押的兄弟亞比篩，是三個勇士的首領；他揮舞矛槍刺死了三百人，因此在三個勇士中贏得了名聲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約押的兄弟亞比篩是這三個勇士的首領；他舉槍殺了三百人，就在三個勇士裏得了名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As for Abshai the brother of Joab, he was chief of the thirty, and he swung his spear against three hundred and killed them; and he had a name as well as the thirty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Abishai the brother of Joab was chief of the Three. He raised his spear against three hundred men, whom he killed, and so he became as famous as the Three."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Abishai, the brother of Joab, was the leader of the Thirty. He once used his spear to kill 300 enemy warriors in a single battle. It was by such feats that he became as famous as the Three."
      }
    },
