    "interpretations": {
      "chenzhongdao": {
        "content": [
          {
            "verse": "1co.16.1-24",
            "paragraphs": [
              "六．愛心的吩咐（16:1-24）"
            ]
          },
          {
            "verse": "1co.16.13-18",
            "paragraphs": [
              "４．司提反一家（16:13-18）"
            ]
          }
        ],
        "title": "新約書信讀經講義",
        "author": "陳終道"
      },
      "dde": {
        "content": [
          {
            "verse": "1co.16.1-24",
            "paragraphs": [
              "Ⅷ結語（十六1～24）",
              "大題目既已論述完畢，保羅還有些話要說。這一段有點“喋喋”的作風，提到對窮人的捐款該怎麽做，他自己計劃中的行程，又略提到彼此相識的朋友們，然後全信收筆。"
            ]
          },
          {
            "verse": "1co.16.15-18",
            "paragraphs": [
              "Ｅ像司提反等人（十六15～18）"
            ]
          },
          {
            "verse": "1co.16.18",
            "paragraphs": [
              "18.他們叫保羅心靈快活（refreshed復新），這個動詞與主叫勞苦擔重擔的人得安息的字相同（太十一28）。這似乎表示保羅得不到哥林多的音訊，十分焦慮，這三人來到使他心靈大得快慰。你們一字很有趣，哥林多的消息不但叫保羅快活，也叫哥林多人因與保羅通音訊而快活。這樣的人是務要敬重的（被推崇deserverecognition）。信徒該認識他們的為人，並敬重他們的位分。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "matangna": {
        "content": [
          {
            "verse": "1co.16.10-24",
            "paragraphs": [
              "三．結束的勸勉及問候（一六10～24）"
            ]
          },
          {
            "verse": "1co.16.18",
            "paragraphs": [
              "一六18他們將關於哥林多的消息帶給保羅，又將關於保羅的消息帶回哥林多的本地教會去。保羅再一次向地方教會推薦他們是應得敬重的。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们使我和你们心里都感到十分欣慰，你们要敬重这样的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "使我和你們的心都得著暢快。這樣的人你們要敬重他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們叫我和你們心裏都快活。這樣的人，你們務要敬重。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they have refreshed my spirit and yours. Therefore acknowledge such men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For they refreshed my spirit and yours also. Such men deserve recognition."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They have been a wonderful encouragement to me, as they have been to you. You must show your appreciation to all who serve so well."
      }
    },
