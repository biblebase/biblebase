  "sng.8.1": {
    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "sng.8.1-3",
            "paragraphs": [
              "八1～3書拉密女希望所羅門是她的\"兄弟\"，好讓她公開表達對他的情感，因為在當時的文化環境裏，已婚夫婦公開地示愛是不合宜的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你是我的同胞兄弟就好了。 这样，在外头遇见你， 我也可以亲吻你， 没有人会取笑我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但願你像我的兄弟， 像吃我母親奶的兄弟。 這樣，我在外面遇見你，就可以吻你， 不會被人藐視。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴不得你像我的兄弟， 像吃我母親奶的兄弟； 我在外頭遇見你就與你親嘴， 誰也不輕看我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Oh that you were like a brother to me Who nursed at my mother’s breasts. If I found you outdoors, I would kiss you; No one would despise me, either."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If only you were to me like a brother, who was nursed at my mother’s breasts! Then, if I found you outside, I would kiss you, and no one would despise me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Oh, I wish you were my brother, who nursed at my mother’s breasts. Then I could kiss you no matter who was watching, and no one would criticize me."
      }
    }
  }
}
