  "sng.8.3": {
    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "sng.8.3",
            "paragraphs": [
              "3.這是二6的重復。在古代的情詩中，女孩子渴望親密接觸的主題很普遍276。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "sng.8.1-3",
            "paragraphs": [
              "八1～3書拉密女希望所羅門是她的\"兄弟\"，好讓她公開表達對他的情感，因為在當時的文化環境裏，已婚夫婦公開地示愛是不合宜的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的左手扶着我的头， 右手紧抱着我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的左手托住我的頭， 他的右手擁抱著我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的左手必在我頭下； 他的右手必將我抱住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let his left hand be under my head And his right hand embrace me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His left arm is under my head and his right arm embraces me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your left arm would be under my head, and your right arm would embrace me."
      }
    }
  }
