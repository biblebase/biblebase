    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的良人啊！ 你真英俊！真可爱！ 我们以青草作床榻，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的良人，你很英俊， 你很可愛。 我們的床榻是青綠的樹木；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的良人哪，你甚美麗可愛！ 我們以青草為牀榻，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“How handsome you are, my beloved, And so pleasant! Indeed, our couch is luxuriant!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How handsome you are, my beloved! Oh, how charming! And our bed is verdant."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You are so handsome, my love, pleasing beyond words! The soft grass is our bed;"
      }
    },
