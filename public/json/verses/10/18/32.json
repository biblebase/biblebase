    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王问古示人：“年轻的押沙龙平安吗？”古示人答道：“愿我主我王的仇敌和一切要加害我王的人，下场都与那青年一样。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王問古實人：“那年輕人押沙龍平安嗎？”古實人回答：“願我主我王的仇敵和一切來攻擊你、傷害你的人，都像那年輕人一樣。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王問古示人說：「少年人押沙龍平安不平安？」古示人回答說：「願我主我王的仇敵，和一切興起要殺害你的人，都與那少年人一樣。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king said to the Cushite, “Is it well with the young man Absalom?” And the Cushite answered, “Let the enemies of my lord the king, and all who rise up against you for evil, be as that young man!”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king asked the Cushite, “Is the young man Absalom safe?” The Cushite replied, “May the enemies of my lord the king and all who rise up to harm you be like that young man.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What about young Absalom?” the king demanded. “Is he all right?” And the Ethiopian replied, “May all of your enemies, my lord the king, both now and in the future, share the fate of that young man!”"
      }
