    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "2sa.18.1-33",
            "paragraphs": [
              "vi.押沙龍的挫敗與死亡（十八1～33）"
            ]
          },
          {
            "verse": "2sa.18.28-30",
            "paragraphs": [
              "28～30.平安了（希伯來：šālôm），撮述了亞希瑪斯的資訊，他俯伏在王的面前，為著王的仇敵戰敗而稱頌耶和華。王詢問關於押沙龍的事，他裝作不知道答案；他模糊的答復只有使大衛的焦慮益形強烈。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王说：“你先退到一边去。”亚希玛斯就退下，站在一边。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王說：“你退到一旁，站在那裡！”他就退到一旁，站在那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王說：「你退去，站在旁邊。」他就退去，站在旁邊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the king said, “Turn aside and stand here.” So he turned aside and stood still."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The king said, “Stand aside and wait here.” So he stepped aside and stood there."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Wait here,” the king told him. So Ahimaaz stepped aside."
      }
