    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "2sa.18.1-33",
            "paragraphs": [
              "vi.押沙龍的挫敗與死亡（十八1～33）"
            ]
          },
          {
            "verse": "2sa.18.19-20",
            "paragraphs": [
              "19～20.撒督的兒子亞希瑪斯是從耶路撒冷到約但河報信的那兩個人之一（撒下十七17），焦急地想要繼續盡報信的職責，將他自認的好消息報與王知。約押卻比較了解大衛，十分明白王所關切的就只有押沙龍發生了什麽事，對他而言，這絕對不是一個好消息。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撒督的儿子亚希玛斯对约押说：“请让我跑回去向王禀告，让王知道耶和华已从仇敌手中救了他。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒督的兒子亞希瑪斯說：“讓我跑去，把這好消息向王報告，就是耶和華已經把他從仇敵的手中拯救出來了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "撒督的兒子亞希瑪斯說：「容我跑去，將耶和華向仇敵給王報仇的信息報與王知。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Ahimaaz the son of Zadok said, “Please let me run and bring the king news that the Lord has freed him from the hand of his enemies.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now Ahimaaz son of Zadok said, “Let me run and take the news to the king that the Lord has vindicated him by delivering him from the hand of his enemies.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Zadok’s son Ahimaaz said, “Let me run to the king with the good news that the Lord has rescued him from his enemies.”"
      }
    },
