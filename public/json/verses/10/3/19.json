    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "2sa.3.6-21",
            "paragraphs": [
              "vii.押尼珥叛歸大衛（三6～21）",
              "押尼珥與伊施波設交惡，是掃羅繼承人敗落的一個因素，使大衛坐收漁翁之利。"
            ]
          },
          {
            "verse": "2sa.3.17-19",
            "paragraphs": [
              "17～19.押尼珥履行了大衛的條件，促使米甲與大衛復合；但押尼珥自己卻召集代表北邊支派的長老開會，又在往南到希伯侖的途中特別詢問夾在南北之間的便雅憫支派（19節），因為是掃羅自己的支派，所以必須特別靈巧的處理。押尼珥聲稱眾人普遍要求大衛作王，可能只是他處理公關事務策略的一個例子。他的提議令人順從下一個明顯的步驟，而他引用之耶和華的應許──原先是論到掃羅的（撒上九6）──更賦與他的提案權威；神論到大衛的這個宣告，除了押尼珥在此處引用之外，並沒有記載下來。押尼珥獲得眾人一致附和後，就準備回復大衛了。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "押尼珥也跟便雅悯人商谈，然后到希伯仑把以色列人和便雅悯人的愿望告诉大卫。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "押尼珥也把這話說給便雅憫人聽，然後又去希伯崙，把以色列人和便雅憫全都同意的一切事說給大衛聽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "押尼珥也用這話說給便雅憫人聽，又到希伯崙，將以色列人和便雅憫全家一切所喜悅的事說給大衛聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Abner also spoke in the hearing of Benjamin; and in addition Abner went to speak in the hearing of David in Hebron all that seemed good to Israel and to the whole house of Benjamin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Abner also spoke to the Benjamites in person. Then he went to Hebron to tell David everything that Israel and the whole tribe of Benjamin wanted to do."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Abner also spoke with the men of Benjamin. Then he went to Hebron to tell David that all the people of Israel and Benjamin had agreed to support him."
      }
    },
