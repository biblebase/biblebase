    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "米非波设再次叩拜，说：“仆人算什么？不过像一条死狗罢了，你竟这样眷顾我！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "米非波設再叩拜說：“僕人算甚麼？你竟然眷顧我這個像條死狗的人！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "米非波設又叩拜，說：「僕人算甚麼，不過如死狗一般，竟蒙王這樣眷顧！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Again he prostrated himself and said, “What is your servant, that you should regard a dead dog like me?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Mephibosheth bowed down and said, “What is your servant, that you should notice a dead dog like me?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Mephibosheth bowed respectfully and exclaimed, “Who is your servant, that you should show such kindness to a dead dog like me?”"
      }
