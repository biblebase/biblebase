    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫差人把她接来，她来了，大卫就与她同寝，那时她的月经刚洁净。事后，她便回家去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是大衛差派使者去把她接來。她來到大衛那裡，大衛就與她同房。那時她的月經剛得潔淨。事後，她就回家去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大衛差人去，將婦人接來；那時她的月經才得潔淨。她來了，大衛與她同房，她就回家去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "David sent messengers and took her, and when she came to him, he lay with her; and when she had purified herself from her uncleanness, she returned to her house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then David sent messengers to get her. She came to him, and he slept with her. (Now she was purifying herself from her monthly uncleanness.) Then she went back home."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then David sent messengers to get her; and when she came to the palace, he slept with her. She had just completed the purification rites after having her menstrual period. Then she returned home."
      }
    },
