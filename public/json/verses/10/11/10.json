    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大卫得知乌利亚没有回家，便对他说：“你远道而来，为什么不回家休息呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有人告訴大衛說：“烏利亞沒有下到自己家裡去。”大衛就問烏利亞：“你不是從遠路回來嗎？為甚麼不下到自己家裡去呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有人告訴大衛說：「烏利亞沒有回家去。」大衛就問烏利亞說：「你從遠路上來，為甚麼不回家去呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when they told David, saying, “Uriah did not go down to his house,” David said to Uriah, “Have you not come from a journey? Why did you not go down to your house?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "David was told, “Uriah did not go home.” So he asked Uriah, “Haven’t you just come from a military campaign? Why didn’t you go home?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When David heard that Uriah had not gone home, he summoned him and asked, “What’s the matter? Why didn’t you go home last night after being away for so long?”"
      }
