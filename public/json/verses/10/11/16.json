    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "于是约押围攻城池时，便派乌利亚到最强悍的敌人那里。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時，約押正在圍城，他知道哪裡有強悍的守軍，就派烏利亞到那裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約押圍城的時候，知道敵人那裏有勇士，便將烏利亞派在那裏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So it was as Joab kept watch on the city, that he put Uriah at the place where he knew there were valiant men."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So while Joab had the city under siege, he put Uriah at a place where he knew the strongest defenders were."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Joab assigned Uriah to a spot close to the city wall where he knew the enemy’s strongest men were fighting."
      }
    },
