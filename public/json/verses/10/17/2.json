    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "2sa.17.1-14",
            "paragraphs": [
              "一七1～14神使用大衛忠實的朋友戶篩去破壞亞希多弗的計謀，並把押沙龍引向死亡。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "趁他疲惫无力的时候袭击他，使他惊慌失措，他的部下必逃命。我只把王一个人杀掉，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "趁他疲乏手軟的時候，忽然追上他，使他驚惶失措，與他在一起的人就必逃跑；我只要單單擊殺王一人，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "趁他疲乏手軟，我忽然追上他，使他驚惶；跟隨他的民必都逃跑，我就單殺王一人，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will come upon him while he is weary and exhausted and terrify him, so that all the people who are with him will flee. Then I will strike down the king alone,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I would attack him while he is weary and weak. I would strike him with terror, and then all the people with him will flee. I would strike down only the king"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will catch up with him while he is weary and discouraged. He and his troops will panic, and everyone will run away. Then I will kill only the king,"
      }
    },
