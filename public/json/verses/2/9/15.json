    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "exo.9.15",
            "paragraphs": [
              "9:15",
              "15~16其實我叫你存立，是特要：當法老最先口出狂言：“耶和華是誰，使我聽他的話”時(5:2)，他就應當被剪除。但是神並沒有立刻毀滅他，反而容許他經歷一個又一個的災難，使他明白神的大能，不僅讓他，也要讓全世界都見到神的大能。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我若用瘟疫攻击你和你的百姓，你们早就灭亡了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果我現在伸手用瘟疫擊打你和你的人民，你就早已從地上消滅了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若伸手用瘟疫攻擊你和你的百姓，你早就從地上除滅了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if by now I had put forth My hand and struck you and your people with pestilence, you would then have been cut off from the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For by now I could have stretched out my hand and struck you and your people with a plague that would have wiped you off the earth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "By now I could have lifted my hand and struck you and your people with a plague to wipe you off the face of the earth."
      }
    },
