    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时冰雹夹杂着火降下来，极为严重，在埃及史无前例。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那時有了冰雹，冰雹與火混雜在一起，十分嚴重；埃及全地自從建國以來，未曾有過這樣的冰雹。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，雹與火攙雜，甚是厲害，自從埃及成國以來，遍地沒有這樣的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So there was hail, and fire flashing continually in the midst of the hail, very severe, such as had not been in all the land of Egypt since it became a nation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "hail fell and lightning flashed back and forth. It was the worst storm in all the land of Egypt since it had become a nation."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Never in all the history of Egypt had there been a storm like that, with such devastating hail and continuous lightning."
      }
    },
