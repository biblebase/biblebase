    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.9.25",
            "paragraphs": [
              "25.所有。這話也必須從修辭而非數學的角度理解。無論如何，從十章7節便可看出毀壞的程度，連埃及人也請願要求法老釋放以色列人離開，免得再有災難臨到。國家經濟已然陷於崩潰。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "冰雹摧毁了埃及境内田间的一切，包括人和牲畜，摧毁了田里的一切作物，打断了所有的树木，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在埃及全地，冰雹把田間的一切，連人和牲畜都擊打了；冰雹又把田間的一切蔬菜擊打了，也把田間的一切樹木打斷了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在埃及遍地，雹擊打了田間所有的人和牲畜，並一切的菜蔬，又打壞田間一切的樹木。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The hail struck down everything that was in the field in all the land of Egypt, both man and beast. And the hail struck down every plant of the field and broke every tree of the field."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The hail struck all that was in the field through all the land of Egypt, both man and beast; the hail also struck every plant of the field and shattered every tree of the field."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Throughout Egypt hail struck everything in the fields—both people and animals; it beat down everything growing in the fields and stripped every tree."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the hail struck throughout the whole land of Egypt, all that was in the field, both man and beast; and the hail struck every herb of the field and broke every tree of the field."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It left all of Egypt in ruins. The hail struck down everything in the open field—people, animals, and plants alike. Even the trees were destroyed."
      }
    },
