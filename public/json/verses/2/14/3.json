    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.14.1-31",
            "paragraphs": [
              "Ｂ渡過蘆海（十四1～31）",
              "十四1～9.渡海的地點"
            ]
          },
          {
            "verse": "exo.14.3",
            "paragraphs": [
              "3.繞迷，“混亂”或“迷途”（參帖三15）。以色列似乎誤入窮巷，前面有海或鹽澤攔路，左右則被沙漠包圍。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.14.3",
            "paragraphs": [
              "一四3\"困住了\"。更可作：混亂了或迷失。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老必定以为你们走迷了路，被困在旷野。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老必論到以色列人的事，說：‘他們在這地走迷了路，曠野把他們困住了。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老必說：『以色列人在地中繞迷了，曠野把他們困住了。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For Pharaoh will say of the sons of Israel, ‘They are wandering aimlessly in the land; the wilderness has shut them in.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Pharaoh will think, ‘The Israelites are wandering around the land in confusion, hemmed in by the desert.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Pharaoh will think, ‘The Israelites are confused. They are trapped in the wilderness!’"
      }
    },
