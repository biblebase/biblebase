    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.14.1-31",
            "paragraphs": [
              "Ｂ渡過蘆海（十四1～31）",
              "十四1～9.渡海的地點"
            ]
          },
          {
            "verse": "exo.14.24",
            "paragraphs": [
              "24.晨更。淩晨兩點到日出（約上午六時）間共有三更，晨更是最後一更；撒母耳記上十一章11節也有提及。日出之前天色最黑，人的警覺性最是低落，發動突擊慣常都在此刻。使……混亂，譯作“制造迷惑或恐慌”更佳。神用的是什麽方法，聖經並沒有說明。詩篇七十七篇16～20節暗示雷暴，若然，這便能解釋聖經為何形容耶和華從雲中俯視埃及軍兵。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.14.24",
            "paragraphs": [
              "一四24\"到了晨更的時候\"。從淩晨二時至破曉。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "到了破晓时分，耶和华从云柱火柱中俯视埃及军队，扰乱他们的队伍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到了晨更的時候，耶和華透過雲柱火柱向下觀看埃及人的軍兵，並且使埃及人的軍兵大起混亂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到了晨更的時候，耶和華從雲火柱中向埃及的軍兵觀看，使埃及的軍兵混亂了；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And in the morning watch the Lord in the pillar of fire and of cloud looked down on the Egyptian forces and threw the Egyptian forces into a panic,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "At the morning watch, the Lord looked down on the army of the Egyptians through the pillar of fire and cloud and brought the army of the Egyptians into confusion."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "During the last watch of the night the Lord looked down from the pillar of fire and cloud at the Egyptian army and threw it into confusion."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now it came to pass, in the morning watch, that the Lord looked down upon the army of the Egyptians through the pillar of fire and cloud, and He troubled the army of the Egyptians."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But just before dawn the Lord looked down on the Egyptian army from the pillar of fire and cloud, and he threw their forces into total confusion."
      }
    },
