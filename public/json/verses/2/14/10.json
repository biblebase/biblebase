    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.14.1-31",
            "paragraphs": [
              "Ｂ渡過蘆海（十四1～31）",
              "十四1～9.渡海的地點"
            ]
          },
          {
            "verse": "exo.14.10",
            "paragraphs": [
              "10.以色列可能真有向神哀求拯救，但他們接下來的行動（和日後在草原經常的作為沒什麽分別；參十六3等），卻是埋怨摩西。這種做法錯而又錯，卻是人之常情；在以色列人身上，我們一再看見自己。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人看见法老大军追了上来，非常害怕，便呼求耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "法老走近的時候，以色列人舉目觀看，看見埃及人正追趕過來，就非常懼怕，向耶和華呼求。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老臨近的時候，以色列人舉目看見埃及人趕來，就甚懼怕，向耶和華哀求。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As Pharaoh drew near, the sons of Israel looked, and behold, the Egyptians were marching after them, and they became very frightened; so the sons of Israel cried out to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As Pharaoh approached, the Israelites looked up, and there were the Egyptians, marching after them. They were terrified and cried out to the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As Pharaoh approached, the people of Israel looked up and panicked when they saw the Egyptians overtaking them. They cried out to the Lord,"
      }
    },
