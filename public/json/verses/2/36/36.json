    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把这幅幔子挂在四根用皂荚木造的柱子上，柱子外面包上金，加上金钩，四根柱子下面有四个银底座。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他替幔幕做了四根皂莢木柱子，用金子包裹，柱釘是金的，又替柱子鑄造四個銀座。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為幔子做四根皂莢木柱子，用金包裹，柱子上有金鈎，又為柱子鑄了四個帶卯的銀座。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And for it he made four pillars of acacia and overlaid them with gold. Their hooks were of gold, and he cast for them four bases of silver."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He made four pillars of acacia for it, and overlaid them with gold, with their hooks of gold; and he cast four sockets of silver for them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They made four posts of acacia wood for it and overlaid them with gold. They made gold hooks for them and cast their four silver bases."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He made for it four pillars of acacia wood, and overlaid them with gold, with their hooks of gold; and he cast four sockets of silver for them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For the curtain, he made four posts of acacia wood and four gold hooks. He overlaid the posts with gold and set them in four silver bases."
      }
    },
