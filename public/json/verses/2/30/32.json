    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要把这膏油用在普通人身上，也不可仿制，因为这是圣膏油，你们也要视之为圣物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不可倒在常人的身上，也不可照著這配製的分量製相似的膏油；這膏油是聖的，你們也要以它為聖。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可倒在別人的身上，也不可按這調和之法做與此相似的。這膏油是聖的，你們也要以為聖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It shall not be poured on anyone’s body, nor shall you make any like it in the same proportions; it is holy, and it shall be holy to you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not pour it on anyone else’s body and do not make any other oil using the same formula. It is sacred, and you are to consider it sacred."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It must never be used to anoint anyone else, and you must never make any blend like it for yourselves. It is holy, and you must treat it as holy."
      }
    },
