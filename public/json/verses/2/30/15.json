    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "富有的不用多缴，贫穷的也不可少付，每个人都要付六克银子，作为献给耶和华的礼物，用来赎他们自己的性命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "富有的人不可多出，貧窮的也不可少出。各人要納六克銀子，作禮物獻給耶和華，好為你們的性命作贖價。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們為贖生命將禮物奉給耶和華，富足的不可多出，貧窮的也不可少出，各人要出半舍客勒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The rich shall not pay more and the poor shall not pay less than the half shekel, when you give the contribution to the Lord to make atonement for yourselves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The rich are not to give more than a half shekel and the poor are not to give less when you make the offering to the Lord to atone for your lives."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When this offering is given to the Lord to purify your lives, making you right with him, the rich must not give more than the specified amount, and the poor must not give less."
      }
    },
