    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "任何人若仿制或用这油抹祭司以外的人，要将他从民中铲除。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "無論誰配製相似的膏油，或把這膏油抹在外人的身上，這人就一定要從他的族人中被剪除。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡調和與此相似的，或將這膏膏在別人身上的，這人要從民中剪除。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whoever shall mix any like it or whoever puts any of it on a layman shall be cut off from his people.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever makes perfume like it and puts it on anyone other than a priest must be cut off from their people.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who makes a blend like it or anoints someone other than a priest will be cut off from the community.’”"
      }
    },
