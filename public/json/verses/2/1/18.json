    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "埃及王召见那两个接生婆，质问她们：“你们为什么这样做？为什么让男婴活着？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是，埃及王把接生婦叫了來，對她們說：“你們為甚麼作這事，讓男孩活著呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "埃及王召了收生婆來，說：「你們為甚麼做這事，存留男孩的性命呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So the king of Egypt called for the midwives and said to them, “Why have you done this thing, and let the boys live?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the king of Egypt summoned the midwives and asked them, “Why have you done this? Why have you let the boys live?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the king of Egypt called for the midwives. “Why have you done this?” he demanded. “Why have you allowed the boys to live?”"
      }
