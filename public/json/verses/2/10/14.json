    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "埃及到处都布满了蝗虫，数目之多实在是空前绝后。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "蝗蟲上到埃及全地來，停留在埃及全境。牠們非常眾多，以前沒有像這樣多的蝗蟲，以後也沒有。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "蝗蟲上來，落在埃及的四境，甚是厲害；以前沒有這樣的，以後也必沒有。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The locusts came up over all the land of Egypt and settled in all the territory of Egypt; they were very numerous. There had never been so many locusts, nor would there be so many again."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "they invaded all Egypt and settled down in every area of the country in great numbers. Never before had there been such a plague of locusts, nor will there ever be again."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the locusts swarmed over the whole land of Egypt, settling in dense swarms from one end of the country to the other. It was the worst locust plague in Egyptian history, and there has never been another one like it."
      }
    },
