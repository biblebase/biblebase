    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "凡有细麻线，山羊毛，蓝色、紫色和朱红色的线，染成红色的公羊皮，海狗皮的，都把它们拿来了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "凡是有藍色紫色朱紅色線、細麻、山羊毛、染紅的公羊皮、海狗皮的，都送了來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡有藍色、紫色、朱紅色線，細麻，山羊毛，染紅的公羊皮，海狗皮的，都拿了來；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every man, who had in his possession blue and purple and scarlet material and fine linen and goats’ hair and rams’ skins dyed red and porpoise skins, brought them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Everyone who had blue, purple or scarlet yarn or fine linen, or goat hair, ram skins dyed red or the other durable leather brought them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All those who owned the following items willingly brought them: blue, purple, and scarlet thread; fine linen and goat hair for cloth; and tanned ram skins and fine goatskin leather."
      }
    },
