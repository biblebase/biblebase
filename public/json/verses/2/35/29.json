    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人，不论男女、凡是甘心乐意奉献的，都把礼物带来献给耶和华，好完成耶和华借摩西吩咐他们的一切工作。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人無論男女，凡是甘心樂意為作耶和華藉摩西吩咐的一切工程的，都把自願奉獻的禮物帶了來獻給耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人，無論男女，凡甘心樂意獻禮物給耶和華的，都將禮物拿來，做耶和華藉摩西所吩咐的一切工。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Israelites, all the men and women, whose heart moved them to bring material for all the work, which the Lord had commanded through Moses to be done, brought a freewill offering to the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All the Israelite men and women who were willing brought to the Lord freewill offerings for all the work the Lord through Moses had commanded them to do."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the people of Israel—every man and woman who was eager to help in the work the Lord had given them through Moses—brought their gifts and gave them freely to the Lord."
      }
    },
