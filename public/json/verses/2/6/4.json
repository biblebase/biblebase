    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.6.4",
            "paragraphs": [
              "4.我與他們堅定所立的約。神在創世記十七章1～8節中，應許將迦南地賜給亞伯拉罕。“堅定”大概是指這個起初是與亞伯拉罕所立的約。然而它若是指神現時要藉摩西為以色列所作的事，也是不難想像的。以色列得以在迦南居住，聖經一貫都看作是對亞伯拉罕應許的成就（創十五18）。而迦南人被驅逐，則被視為神對他們惡行的審判；請參創世記十五章16節。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.6.4",
            "paragraphs": [
              "六4關於\"迦南地\"的應許，參看創世記十三章15節和十七章8節。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我向他们坚守所立的约，要把他们寄居的迦南赐给他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我不但與他們確立了我的約，要把迦南地，就是他們作客寄居之地賜給他們；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我與他們堅定所立的約，要把他們寄居的迦南地賜給他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I also established My covenant with them, to give them the land of Canaan, the land in which they sojourned."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I also established my covenant with them to give them the land of Canaan, where they resided as foreigners."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I reaffirmed my covenant with them. Under its terms, I promised to give them the land of Canaan, where they were living as foreigners."
      }
    },
