    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "exo.6.14-27",
            "paragraphs": [
              "六14～27這裏插入以色列人的族譜，為的是識別摩西與亞倫的家系，好指出他們是剛被委派去肩負拯救以色列人的重任（比較29節）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以下是以色列各宗族族长的名字： 以色列长子吕便的儿子是哈诺、法路、希斯仑和迦米。这些是吕便的宗族。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們各家族首領的名字記在下面：以色列的長子流本的兒子是哈諾、法路、希斯倫、迦米；這些是流本的各家族。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人家長的名字記在下面。以色列長子呂便的兒子是哈諾、法路、希斯倫、迦米；這是呂便的各家。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "These are the heads of their fathers’ houses: the sons of Reuben, the firstborn of Israel: Hanoch, Pallu, Hezron, and Carmi; these are the clans of Reuben."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "These are the heads of their fathers’ households. The sons of Reuben, Israel’s firstborn: Hanoch and Pallu, Hezron and Carmi; these are the families of Reuben."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These were the heads of their families: The sons of Reuben the firstborn son of Israel were Hanok and Pallu, Hezron and Karmi. These were the clans of Reuben."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "These are the heads of their fathers’ houses: The sons of Reuben, the firstborn of Israel, were Hanoch, Pallu, Hezron, and Carmi. These are the families of Reuben."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "These are the ancestors of some of the clans of Israel: The sons of Reuben, Israel’s oldest son, were Hanoch, Pallu, Hezron, and Carmi. Their descendants became the clans of Reuben."
      }
