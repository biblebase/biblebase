    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.6.29",
            "paragraphs": [
              "29.耶和華……對摩西說話。敘述因族譜中斷一下後，在本節恢復。神的說話再次以“我是耶和華”開始，這話不但是命令或應許的憑據和保證，更為理由和本質提供解釋（如，利十九18：“要愛人如己，我是耶和華”）。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我是耶和华，你要把我吩咐你的一切话告诉埃及王法老。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華告訴摩西說：“我是耶和華，你要把我對你所說的一切話都告訴埃及王法老。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他向摩西說：「我是耶和華；我對你說的一切話，你都要告訴埃及王法老。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "that the Lord spoke to Moses, saying, “I am the Lord; speak to Pharaoh king of Egypt all that I speak to you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "he said to him, “I am the Lord. Tell Pharaoh king of Egypt everything I tell you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "he said to him, “I am the Lord! Tell Pharaoh, the king of Egypt, everything I am telling you.”"
      }
