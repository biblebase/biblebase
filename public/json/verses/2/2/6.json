    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "公主打开一看，原来是一个男婴。公主看见孩子在哭，就可怜他，说：“他是希伯来人的孩子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她打開了，就看見那孩子；看哪，孩子哭了，她就憐憫他，說：“這是希伯來人的一個孩子。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她打開箱子，看見那孩子。孩子哭了，她就可憐他，說：「這是希伯來人的一個孩子。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When she opened it, she saw the child, and behold, the boy was crying. And she had pity on him and said, “This is one of the Hebrews’ children.”"
      },
      "niv": {
        "version_id": "NIV",
