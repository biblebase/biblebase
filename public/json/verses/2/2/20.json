    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他又问女儿们：“现在那人在哪里？你们怎么可以丢下他不管呢？去请他来吃点东西。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "父親對眾女兒說：“他在哪裡？你們為甚麼撇下那人呢？去請他來吃飯。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對女兒們說：「那個人在哪裏？你們為甚麼撇下他呢？你們去請他來吃飯。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to his daughters, “Where is he then? Why is it that you have left the man behind? Invite him to have something to eat.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“And where is he?” Reuel asked his daughters. “Why did you leave him? Invite him to have something to eat.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then where is he?” their father asked. “Why did you leave him there? Invite him to come and eat with us.”"
      }
