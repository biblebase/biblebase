    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这时，来了另外一群牧人要把她们赶走。摩西便起来帮助她们，打水饮她们的羊群。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有些牧人來了，把她們趕走；摩西卻起來，救了她們，也給她們的羊群喝水。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有牧羊的人來，把她們趕走了，摩西卻起來幫助她們，又飲了她們的羣羊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the shepherds came and drove them away, but Moses stood up and helped them and watered their flock."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Some shepherds came along and drove them away, but Moses got up and came to their rescue and watered their flock."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But some other shepherds came and chased them away. So Moses jumped up and rescued the girls from the shepherds. Then he drew water for their flocks."
      }
    },
