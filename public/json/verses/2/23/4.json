    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.23.4",
            "paragraphs": [
              "4.你仇敵的牛。依照上下文來看，仇敵相信是指“打官司的對方”。我們對待這些人的態度必須和一般鄰舍相同才算公正；我們更加不得拿無助的牲口“出氣”。本節實際關心敵人的利益，和馬太福音五章44節“愛你的仇敵”只有一線之差。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“倘若看见仇人的牛或驴走迷了路，总要牵回去交给他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果你遇見你仇敵的牛或是驢走迷了路，就應該把牠牽回來給他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「若遇見你仇敵的牛或驢失迷了路，總要牽回來交給他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If you meet your enemy’s ox or his donkey wandering away, you shall surely return it to him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If you come across your enemy’s ox or donkey wandering off, be sure to return it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If you come upon your enemy’s ox or donkey that has strayed away, take it back to its owner."
      }
    },
