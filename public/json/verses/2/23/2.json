    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可随众行恶。在法庭作证时不可附和多数人，颠倒是非。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不可隨眾行惡，不可在訴訟的事上隨眾說歪曲正義的話；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可隨眾行惡；不可在爭訟的事上隨眾偏行，作見證屈枉正直；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not follow the masses in doing evil, nor shall you testify in a dispute so as to turn aside after a multitude in order to pervert justice;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do not follow the crowd in doing wrong. When you give testimony in a lawsuit, do not pervert justice by siding with the crowd,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You must not follow the crowd in doing wrong. When you are called to testify in a dispute, do not be swayed by the crowd to twist justice."
      }
    },
