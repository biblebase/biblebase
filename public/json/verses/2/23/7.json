    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可诬告别人，不可杀害正直无辜的人，因为我必惩罚作恶之人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要遠離虛假的事；不可殺無辜和正義的人，因為我決不以惡人為義人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當遠離虛假的事。不可殺無辜和有義的人，因我必不以惡人為義。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Keep far from a false charge, and do not kill the innocent or the righteous, for I will not acquit the guilty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Have nothing to do with a false charge and do not put an innocent or honest person to death, for I will not acquit the guilty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Be sure never to charge anyone falsely with evil. Never sentence an innocent or blameless person to death, for I never declare a guilty person to be innocent."
      }
    },
