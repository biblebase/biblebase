    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.27.1-21",
            "paragraphs": [
              "iii.祭壇、幕院、夜明燈（二十七1～21）",
              "二十七1～8.祭壇",
              "納皮爾指出經文對聖物的描述是自內而外的。首先形容的是位於帳幕至聖之處的約櫃和主室的燈臺，然後暫停向外擴展，描寫帳幕本身的結構，出到帳幕外面，經文繼續描述祭壇和安置祭壇的院子。離開核心越遠，材料就越不貴重、結構就越簡單。這象征也很明顯。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.27.9-19",
            "paragraphs": [
              "二七9～19\"帳幕的院子\"長寬為150x75英尺（46x23米），用高七英尺半（2.3米）的細麻布簾子遮蓋，為裏面的敬拜者提供隱私。會幕本身只占這院子十五分之一的地方。\"桿子\"即帶子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "院子的入口要有门帘，长九米，用细麻线和蓝色、紫色、朱红色的线织成，上面有精致的刺绣，还有四根柱子和四个带凹槽的底座。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "院子的大門要有門簾，長九公尺，是用藍色紫色朱紅色線和撚的細麻，用刺繡的手工織成，柱子四根，插座四個。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "院子的門當有簾子，長二十肘，要拿藍色、紫色、朱紅色線，和撚的細麻，用繡花的手工織成，柱子四根，帶卯的座四個。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For the gate of the court there shall be a screen twenty cubits long, of blue and purple and scarlet yarns and fine twined linen, embroidered with needlework. It shall have four pillars and with them four bases."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the gate of the court there shall be a screen of twenty cubits, of blue and purple and scarlet material and fine twisted linen, the work of a weaver, with their four pillars and their four sockets."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“For the entrance to the courtyard, provide a curtain twenty cubits long, of blue, purple and scarlet yarn and finely twisted linen—the work of an embroiderer—with four posts and four bases."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“For the gate of the court there  shall  be a screen twenty cubits long, woven  of blue, purple, and scarlet thread, and fine woven linen, made by a weaver. It shall  have four pillars and four sockets."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“For the entrance to the courtyard, make a curtain that is 30 feet long. Make it from finely woven linen, and decorate it with beautiful embroidery in blue, purple, and scarlet thread. Support it with four posts, each securely set in its own base."
      }
    },
