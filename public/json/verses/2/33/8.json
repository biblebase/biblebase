    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当摩西离营去会幕的时候，百姓就都一同起来，各人站在自己的帐篷门口目送摩西走进会幕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西出營到會幕去的時候，全體的人民都起來，各人站在自己帳幕的門口，望著摩西，直到他進了會幕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當摩西出營到會幕去的時候，百姓就都起來，各人站在自己帳棚的門口，望着摩西，直等到他進了會幕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And it came about, whenever Moses went out to the tent, that all the people would arise and stand, each at the entrance of his tent, and gaze after Moses until he entered the tent."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And whenever Moses went out to the tent, all the people rose and stood at the entrances to their tents, watching Moses until he entered the tent."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Whenever Moses went out to the Tent of Meeting, all the people would get up and stand in the entrances of their own tents. They would all watch Moses until he disappeared inside."
      }
    },
