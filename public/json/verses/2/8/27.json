    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.8.27",
            "paragraphs": [
              "27.三天的路程。三章18節已經提及過。如今法老顯然同意容許他們在沙漠獻祭，但卻對充滿彈性的“三日路程”表示懷疑。然而他也沒有履行諾言釋放以色列，因此合約的範圍也變得無關重要。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们要照我们上帝耶和华的吩咐离开这里，走三天的路程到旷野去献祭给祂。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們要走三天的路程進到曠野，好照著耶和華吩咐我們的，向耶和華我們的　神獻祭。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們要往曠野去，走三天的路程，照着耶和華－我們神所要吩咐我們的祭祀他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We must go a three days’ journey into the wilderness and sacrifice to the Lord our God as He commands us.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We must take a three-day journey into the wilderness to offer sacrifices to the Lord our God, as he commands us.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We must take a three-day trip into the wilderness to offer sacrifices to the Lord our God, just as he has commanded us.”"
      }
    },
