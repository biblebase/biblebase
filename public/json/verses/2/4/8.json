    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“纵然他们不听你的话，不信第一个神迹，也必定相信第二个神迹。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華又說：“如果他們不信你，也不聽從頭一個神蹟的話，他們必信隨後來的神蹟。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又說：「倘或他們不聽你的話，也不信頭一個神蹟，他們必信第二個神蹟。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If they will not believe you or heed the witness of the first sign, they may believe the witness of the last sign."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the Lord said, “If they do not believe you or pay attention to the first sign, they may believe the second."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord said to Moses, “If they do not believe you and are not convinced by the first miraculous sign, they will be convinced by the second sign."
      }
    },
