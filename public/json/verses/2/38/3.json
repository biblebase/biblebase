    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他用铜造一切的器具，就是盆、铲、碗、肉叉、火鼎。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又做壇上的一切器具，就是盆、鏟、盤、肉叉和火盆；壇上的一切器具都是用銅做的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他做壇上的盆、鏟子、盤子、肉鍤子、火鼎；這一切器具都是用銅做的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He made all the utensils of the altar, the pails and the shovels and the basins, the flesh hooks and the firepans; he made all its utensils of bronze."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They made all its utensils of bronze—its pots, shovels, sprinkling bowls, meat forks and firepans."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he made all the altar utensils of bronze—the ash buckets, shovels, basins, meat forks, and firepans."
      }
    },
