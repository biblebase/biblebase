    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.38.21",
            "paragraphs": [
              "21.利未人所用，利未人受任的經過記載在民數記三章。以他瑪在該處經文也是擔任首領之職（其實他在出六23經已出現了）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以上是安放约柜的圣幕中所用的物品，是照摩西的吩咐，由亚伦祭司的儿子以他玛指挥利未人清点的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這是帳幕內利未人使用的物件的總數，是照著摩西的吩咐，經亞倫祭司的兒子以他瑪的手計算的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這是法櫃的帳幕中利未人所用物件的總數，是照摩西的吩咐，經祭司亞倫的兒子以他瑪的手數點的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "These are the records of the tabernacle, the tabernacle of the testimony, as they were recorded at the commandment of Moses, the responsibility of the Levites under the direction of Ithamar the son of Aaron the priest."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is the number of the things for the tabernacle, the tabernacle of the testimony, as they were numbered according to the command of Moses, for the service of the Levites, by the hand of Ithamar the son of Aaron the priest."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "These are the amounts of the materials used for the tabernacle, the tabernacle of the covenant law, which were recorded at Moses’ command by the Levites under the direction of Ithamar son of Aaron, the priest."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "This is the inventory of the tabernacle, the tabernacle of the Testimony, which was counted according to the commandment of Moses, for the service of the Levites, by the hand of Ithamar, son of Aaron the priest."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is an inventory of the materials used in building the Tabernacle of the Covenant. The Levites compiled the figures, as Moses directed, and Ithamar son of Aaron the priest served as recorder."
      }
    },
