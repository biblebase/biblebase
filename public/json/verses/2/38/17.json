    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "柱子带凹槽的底座是铜的，柱子上面的钩和横杆是银的，柱顶包银，院子所有的柱子都用银杆相连。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "柱座是銅做的，柱鉤和橫柱閂是銀做的，柱頂是用銀包裹的；院子的一切柱子都是用銀橫閂相連起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "柱子帶卯的座是銅的，柱子上的鈎子和杆子是銀的，柱頂是用銀子包的。院子一切的柱子都是用銀杆連絡的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sockets for the pillars were of bronze, the hooks of the pillars and their bands, of silver; and the overlaying of their tops, of silver, and all the pillars of the court were furnished with silver bands."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The bases for the posts were bronze. The hooks and bands on the posts were silver, and their tops were overlaid with silver; so all the posts of the courtyard had silver bands."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each post had a bronze base, and all the hooks and rings were silver. The tops of the posts of the courtyard were overlaid with silver, and the rings to hold up the curtains were made of silver."
      }
    },
