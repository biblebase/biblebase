    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "exo.18.13-27",
            "paragraphs": [
              "一八13～27眼見摩西在審斷民事上似乎陷入困境，葉忒羅建議他另選一些合乎資格的人去做這事（21節）。然而，這建議實際上到了頒布律法後才正式實行（申一5～15）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从百姓中挑选有才干的人，委派他们做千夫长、百夫长、五十夫长和十夫长管理百姓。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西從全體以色列人中選出了有才能的人，立他們作人民的首領，作千夫長、百夫長、五十夫長、十夫長。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西從以色列人中揀選了有才能的人，立他們為百姓的首領，作千夫長、百夫長、五十夫長、十夫長。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Moses chose able men out of all Israel and made them heads over the people, chiefs of thousands, of hundreds, of fifties, and of tens."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moses chose able men out of all Israel and made them heads over the people, leaders of thousands, of hundreds, of fifties and of tens."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He chose capable men from all Israel and made them leaders of the people, officials over thousands, hundreds, fifties and tens."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And Moses chose able men out of all Israel, and made them heads over the people: rulers of thousands, rulers of hundreds, rulers of fifties, and rulers of tens."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He chose capable men from all over Israel and appointed them as leaders over the people. He put them in charge of groups of one thousand, one hundred, fifty, and ten."
      }
    },
