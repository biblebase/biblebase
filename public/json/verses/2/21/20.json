    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.21.20",
            "paragraphs": [
              "20.他必要受刑，本節將奴隸當作人來看待，是古代思潮的一大躍進。奴隸雖是主人的“財產”，主人也無權故意將他打死。奴隸若是殘喘了幾天才死，便得假定主人只是有心懲戒，並無殺人的意思，可以算為誤殺。奴隸死亡對主人來說是很大的金錢損失，這損失已足作為懲罰和教訓。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“倘若有人用棍子打他的仆婢，导致他们当场死亡，他必须受惩罚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果有人用棍打他的奴僕或婢女，以致死在他的手下，他必須受刑罰；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「人若用棍子打奴僕或婢女，立時死在他的手下，他必要受刑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If a man strikes his male or female slave with a rod and he dies at his hand, he shall be punished."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Anyone who beats their male or female slave with a rod must be punished if the slave dies as a direct result,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If a man beats his male or female slave with a club and the slave dies as a result, the owner must be punished."
      }
    },
