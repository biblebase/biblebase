    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "exo.21.1-6",
            "paragraphs": [
              "二一1～6關乎希伯來奴仆的條例。（有關外地奴仆的條例，參看利二五44～46）。人通常為了還債而賣身為奴（比較利二五39；摩二6；八6），但他的主人要待他如雇工，而且在六年之後要把他釋放。\"錐子\"。類似冰錐。參看詩篇四十篇6節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若主人给他娶了妻，妻子生了儿女，那么期满的时候，他只能单独离开，他的妻儿要归主人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的主人若是給他妻子，妻子又給他生了兒女，那麼妻子和兒女都要歸給主人，他自己要單獨出去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他主人若給他妻子，妻子給他生了兒子或女兒，妻子和兒女要歸主人，他要獨自出去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If his master gives him a wife, and she bears him sons or daughters, the wife and her children shall belong to her master, and he shall go out alone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If his master gives him a wife and she bears him sons or daughters, the woman and her children shall belong to her master, and only the man shall go free."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If his master gave him a wife while he was a slave and they had sons or daughters, then only the man will be free in the seventh year, but his wife and children will still belong to his master."
      }
    },
