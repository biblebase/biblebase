      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「拐帶人口，或是把人賣了，或是留在他手下，必要把他治死。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“He who kidnaps a man, whether he sells him or he is found in his possession, shall surely be put to death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Anyone who kidnaps someone is to be put to death, whether the victim has been sold or is still in the kidnapper’s possession."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Kidnappers must be put to death, whether they are caught in possession of their victims or have already sold them as slaves."
      }
    },
