    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.26.1-37",
            "paragraphs": [
              "ii.帳幕（二十六1～37）",
              "本段經文雖然也有隱誨之處，大體上仍是清楚明白的。讀者最好是參看聖經辭典的插圖。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.26.7-13",
            "paragraphs": [
              "二六7～13\"幔子\"。這些幔子覆蓋裏面的細麻布幔子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "铺罩棚所剩下来的半幅幔子要垂到圣幕后面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "帳棚的幔子多出的那部分，即垂下來的半幅幔子，要垂在會幕的後面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "罩棚的幔子所餘那垂下來的半幅幔子，要垂在帳幕的後頭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The overlapping part that is left over in the curtains of the tent, the half curtain that is left over, shall lap over the back of the tabernacle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As for the additional length of the tent curtains, the half curtain that is left over is to hang down at the rear of the tabernacle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The remaining 3 feet of this tent covering will be left to hang over the back of the Tabernacle."
      }
    },
