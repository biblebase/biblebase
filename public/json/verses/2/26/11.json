        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又要做五十個銅鈎，鈎在鈕扣中，使罩棚連成一個。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“You shall make fifty clasps of bronze, and put the clasps into the loops, and couple the tent together that it may be a single whole."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You shall make fifty clasps of bronze, and you shall put the clasps into the loops and join the tent together so that it will be a unit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then make fifty bronze clasps and put them in the loops to fasten the tent together as a unit."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And you shall make fifty bronze clasps, put the clasps into the loops, and couple the tent together, that it may be one."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then make fifty bronze clasps, and fasten the loops of the long curtains with the clasps. In this way, the tent covering will be made of one continuous piece."
      }
