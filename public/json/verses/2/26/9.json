    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.26.1-37",
            "paragraphs": [
              "ii.帳幕（二十六1～37）",
              "本段經文雖然也有隱誨之處，大體上仍是清楚明白的。讀者最好是參看聖經辭典的插圖。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.26.7-13",
            "paragraphs": [
              "二六7～13\"幔子\"。這些幔子覆蓋裏面的細麻布幔子。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "然后，你们要把五幅连成一大幅，其余六幅连成一大幅，第六幅要在圣幕前叠起来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要把五幅幔子連在一起，又把另外六幅幔子連在一起，你要在帳棚前面把第六幅幔子摺疊起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要把五幅幔子連成一幅，又把六幅幔子連成一幅，這第六幅幔子要在罩棚的前面摺上去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall join five curtains by themselves and the other six curtains by themselves, and you shall double over the sixth curtain at the front of the tent."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Join five of the curtains together into one set and the other six into another set. Fold the sixth curtain double at the front of the tent."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Join five of these curtains together to make one long curtain, and join the other six into a second long curtain. Allow 3 feet of material from the second set of curtains to hang over the front of the sacred tent."
      }
    },
