    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西对耶和华说：“百姓不能上西奈山，因为你已经吩咐我们要在山的四围划定界限，使这山成为圣山。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西對耶和華說：“人民不能上西奈山，因為你曾經警告我們，說：‘要給山定界限，也要把山分別為聖。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西對耶和華說：「百姓不能上西奈山，因為你已經囑咐我們說：『要在山的四圍定界限，叫山成聖。』」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And Moses said to the Lord, “The people cannot come up to Mount Sinai, for you yourself warned us, saying, ‘Set limits around the mountain and consecrate it.’”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moses said to the Lord, “The people cannot come up to Mount Sinai, for You warned us, saying, ‘Set bounds about the mountain and consecrate it.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moses said to the Lord, “The people cannot come up Mount Sinai, because you yourself warned us, ‘Put limits around the mountain and set it apart as holy.’ ”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But Moses said to the Lord, “The people cannot come up to Mount Sinai; for You warned us, saying, ‘Set bounds around the mountain and consecrate it.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But Lord,” Moses protested, “the people cannot come up to Mount Sinai. You already warned us. You told me, ‘Mark off a boundary all around the mountain to set it apart as holy.’”"
      }
    },
