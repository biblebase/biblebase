    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.19.1-25",
            "paragraphs": [
              "Ａ立約的準備（十九1～25）",
              "十九1～15.指令"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "exo.19.10-15",
            "paragraphs": [
              "一九10～15在神顯現之前，人必須做好三方面的準備：潔凈身體和洗衣服（10節），在山的四圍放置障礙物，把百姓限制於營內（12,13節；比較21節），並戒絕性行為（15節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "又吩咐他们说：“到后天一切都要准备好，这期间你们不可亲近女人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他對人民說：“第三天，你們要預備好了；不可親近女人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他對百姓說：「到第三天要預備好了。不可親近女人。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He said to the people, “Be ready for the third day; do not go near a woman.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he said to the people, “Prepare yourselves for the third day. Abstain from sexual relations.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He told them, “Get ready for the third day, and until then abstain from having sexual intercourse.”"
      }
    },
