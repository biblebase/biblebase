    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.32.35",
            "paragraphs": [
              "35.耶和華差遣瘟疫（和合本作“耶和華殺百姓”），大概是指五經常見的那種立時而短暫的懲罰（如：民十一33）。然而本節亦可能遙指以後整代人要受的刑罰。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华击杀百姓，是因为他们曾与亚伦一起造牛犊。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華擊殺人民是因為他們敬拜亞倫做的牛犢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華殺百姓的緣故是因他們同亞倫做了牛犢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the Lord smote the people, because of what they did with the calf which Aaron had made."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Lord struck the people with a plague because of what they did with the calf Aaron had made."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the Lord sent a great plague upon the people because they had worshiped the calf Aaron had made."
      }
    },
