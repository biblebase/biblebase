    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西见百姓放肆，亚伦纵容他们，使他们成为敌人的笑柄，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西看見人民放肆原來是亞倫縱容他們，使他們成為那些起來與他們為敵的人的笑柄，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西見百姓放肆（亞倫縱容他們，使他們在仇敵中間被譏刺），"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when Moses saw that the people were out of control—for Aaron had let them get out of control to be a derision among their enemies—"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moses saw that the people were running wild and that Aaron had let them get out of control and so become a laughingstock to their enemies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moses saw that Aaron had let the people get completely out of control, much to the amusement of their enemies."
      }
