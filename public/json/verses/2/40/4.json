    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.40.1-38",
            "paragraphs": [
              "Ｄ帳幕分別為聖（四十1～38）",
              "四十1～15.分別為聖的指令",
              "這幾節和二十五至三十一章一樣，嚴格地說是期待將來的。神再次給予摩西的指示不但包括紮起帳幕、將一切聖物安放妥當，更包括了“膏立”帳幕、陳設、祭司，使之永遠歸神所用（15節）。參較出二十九章9節亞倫一脈的“永久性祭司職分”。這裏發出的指令將要在本章余下的部分執行。",
              "四十16～33.分別為聖的禮儀"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "把桌子搬进去，摆好桌上的器具，再把灯台搬进去，点上灯，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "把桌子搬進去，擺上陳設物；把燈臺也搬進去，把燈臺上的燈盞點著。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "把桌子搬進去，擺設上面的物。把燈臺搬進去，點其上的燈。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall bring in the table and arrange what belongs on it; and you shall bring in the lampstand and mount its lamps."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Bring in the table and set out what belongs on it. Then bring in the lampstand and set up its lamps."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then bring in the table, and arrange the utensils on it. And bring in the lampstand, and set up the lamps."
      }
