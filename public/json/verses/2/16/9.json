    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩西对亚伦说：“你去叫以色列全体会众到耶和华面前来，因为祂已听见他们的怨言。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩西對亞倫說：“你要對以色列全體會眾說：‘你們走近耶和華面前，因為他已經聽見了你們的怨言了。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩西對亞倫說：「你告訴以色列全會眾說：『你們就近耶和華面前，因為他已經聽見你們的怨言了。』」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Then Moses said to Aaron, “Say to the whole congregation of the people of Israel, ‘Come near before the Lord, for he has heard your grumbling.’”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Moses said to Aaron, “Say to all the congregation of the sons of Israel, ‘Come near before the Lord, for He has heard your grumblings.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Moses told Aaron, “Say to the entire Israelite community, ‘Come before the Lord, for he has heard your grumbling.’ ”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Then Moses spoke to Aaron, “Say to all the congregation of the children of Israel, ‘Come near before the Lord, for He has heard your complaints.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Moses said to Aaron, “Announce this to the entire community of Israel: ‘Present yourselves before the Lord, for he has heard your complaining.’”"
      }
    },
