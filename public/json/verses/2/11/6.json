    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "exo.11.6",
            "paragraphs": [
              "6.大哀號，是本書的另一主題。以色列向耶和華“哀求”拯救（出二23），後來他們又在極度痛苦中向法老“哀求”，卻得不應允（出五15）。如今卻輪到了埃及人在神審判的痛苦中“哀求”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那时整个埃及必充满悲惨的哭号声，惨况空前绝后。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在埃及全地必有大哀號；這樣的哀號從前未曾有過，以後也必再沒有。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "埃及遍地必有大哀號；從前沒有這樣的，後來也必沒有。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover, there shall be a great cry in all the land of Egypt, such as there has not been before and such as shall never be again."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "There will be loud wailing throughout Egypt—worse than there has ever been or ever will be again."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then a loud wail will rise throughout the land of Egypt, a wail like no one has heard before or will ever hear again."
      }
