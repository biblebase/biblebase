    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他用纯金造了一座灯台，灯台的灯座、灯柱、油杯、花瓣和花苞都是用一块纯金打成的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他用純金做了一個燈臺，他用鎚錘成燈臺；燈臺的座和幹，跟杯、球、花，都是連在一起的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他用精金做一個燈臺；這燈臺的座和幹，與杯、球、花，都是接連一塊錘出來的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he made the lampstand of pure gold. He made the lampstand of hammered work, its base and its shaft; its cups, its bulbs and its flowers were of one piece with it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They made the lampstand of pure gold. They hammered out its base and shaft, and made its flowerlike cups, buds and blossoms of one piece with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Bezalel made the lampstand of pure, hammered gold. He made the entire lampstand and its decorations of one piece—the base, center stem, lamp cups, buds, and petals."
      }
    },
