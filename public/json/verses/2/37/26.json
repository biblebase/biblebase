    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "坛顶、坛的四面和坛上的角状物包上纯金，四周镶上金边。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又用純金把壇包裹，壇頂、壇的四邊和壇的四角都包裹，又替壇做了金牙邊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又用精金把壇的上面與壇的四面並壇的四角包裹，又在壇的四圍鑲上金牙邊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He overlaid it with pure gold, its top and its sides all around, and its horns; and he made a gold molding for it all around."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They overlaid the top and all the sides and the horns with pure gold, and made a gold molding around it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He overlaid the top, sides, and horns of the altar with pure gold, and he ran a gold molding around the entire altar."
      }
    },
