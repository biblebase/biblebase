    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他造了两个金环，安在坛两侧的金边下面，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又替壇做了兩個金環，安放在牙邊以下，壇的兩側，就是壇的兩旁，作穿槓的地方，用來抬壇。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "做兩個金環，安在牙子邊以下，在壇的兩旁、兩根橫撐上，作為穿槓的用處，以便擡壇。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He made two golden rings for it under its molding, on its two sides—on opposite sides—as holders for poles with which to carry it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They made two gold rings below the molding—two on each of the opposite sides—to hold the poles used to carry it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He made two gold rings and attached them on opposite sides of the altar below the gold molding to hold the carrying poles."
      }
