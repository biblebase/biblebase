    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "再缝在以弗得的两条肩带上，作以色列人的纪念石。这些都是照耶和华对摩西的吩咐做的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "把這兩塊寶石放在以弗得的兩條肩帶上，作以色列人的記念石，是照著耶和華吩咐摩西的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "將這兩塊寶石安在以弗得的兩條肩帶上，為以色列人做紀念石，是照耶和華所吩咐摩西的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And he placed them on the shoulder pieces of the ephod, as memorial stones for the sons of Israel, just as the Lord had commanded Moses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they fastened them on the shoulder pieces of the ephod as memorial stones for the sons of Israel, as the Lord commanded Moses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He fastened these stones on the shoulder-pieces of the ephod as a reminder that the priest represents the people of Israel. All this was done just as the Lord had commanded Moses."
      }
    },
