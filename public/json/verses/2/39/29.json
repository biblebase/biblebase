    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "还用细麻线和蓝色、紫色、朱红色的线绣制腰带。这是照耶和华对摩西的吩咐做的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以及用撚的細麻和藍色紫色朱紅色線，用刺繡的手工做腰帶；都是照著耶和華吩咐摩西的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又用藍色、紫色、朱紅色線，並撚的細麻，以繡花的手工做腰帶，是照耶和華所吩咐摩西的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "and the sash of fine twined linen and of blue and purple and scarlet yarns, embroidered with needlework, as the Lord had commanded Moses."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and the sash of fine twisted linen, and blue and purple and scarlet material, the work of the weaver, just as the Lord had commanded Moses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The sash was made of finely twisted linen and blue, purple and scarlet yarn—the work of an embroiderer—as the Lord commanded Moses."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and a sash of fine woven linen with blue, purple, and scarlet thread, made by a weaver, as the Lord had commanded Moses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The sashes were made of finely woven linen and embroidered with blue, purple, and scarlet thread, just as the Lord had commanded Moses."
      }
