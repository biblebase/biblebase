      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Then you will know that I, the Lord your God, live in Zion, my holy mountain. Jerusalem will be holy forever, and foreign armies will never conquer her again."
      }
