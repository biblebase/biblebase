    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "锡安的人民啊， 你们应当欢喜快乐， 从你们的上帝耶和华那里得到喜乐。 因为祂按时降下甘霖， 像从前一样赐给你们秋雨、 春雨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "錫安的人民哪！你們要歡喜， 要靠耶和華你們的　神快樂， 因為他賜給了你們合時的秋雨。 他給你們降下時雨， 就是秋雨春雨，像以前一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "錫安的民哪，你們要快樂， 為耶和華－你們的神歡喜； 因他賜給你們合宜的秋雨， 為你們降下甘霖， 就是秋雨、春雨，和先前一樣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Be glad, O children of Zion, and rejoice in the Lord your God, for he has given the early rain for your vindication; he has poured down for you abundant rain, the early and the latter rain, as before."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So rejoice, O sons of Zion, And be glad in the Lord your God; For He has given you the early rain for your vindication. And He has poured down for you the rain, The early and latter rain as before."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be glad, people of Zion, rejoice in the Lord your God, for he has given you the autumn rains because he is faithful. He sends you abundant showers, both autumn and spring rains, as before."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Be glad then, you children of Zion, And rejoice in the Lord your God; For He has given you the former rain faithfully, And He will cause the rain to come down for you— The former rain, And the latter rain in the first month."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Rejoice, you people of Jerusalem! Rejoice in the Lord your God! For the rain he sends demonstrates his faithfulness. Once more the autumn rains will come, as well as the rains of spring."
      }
    },
