      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第六位天使吹号的时候，我听见有一个声音从上帝面前的金祭坛的四个角发出来，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "第六位天使吹號，我聽見有一個聲音從　神面前金壇的四角發出來，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "第六位天使吹號，我就聽見有聲音從神面前金壇的四角出來，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the sixth angel sounded, and I heard a voice from the four horns of the golden altar which is before God,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The sixth angel sounded his trumpet, and I heard a voice coming from the four horns of the golden altar that is before God."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the sixth angel blew his trumpet, and I heard a voice speaking from the four horns of the gold altar that stands in the presence of God."
      }
    },
