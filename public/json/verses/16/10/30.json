        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们不将女儿嫁给当地的人，也不为儿子娶当地的女子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們必不把我們的女兒嫁給這地的外族人，也不為我們的兒子娶他們的女兒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並不將我們的女兒嫁給這地的居民，也不為我們的兒子娶他們的女兒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and that we will not give our daughters to the peoples of the land or take their daughters for our sons."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“We promise not to give our daughters in marriage to the peoples around us or take their daughters for our sons."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“We promise not to let our daughters marry the pagan people of the land, and not to let our sons marry their daughters."
      }
