    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "neh.3.28-31",
            "paragraphs": [
              "三28～31\"馬門\"。位於東面的聖殿附近。往北是\"東門\"，再往北是\"哈米弗甲門\"（今天的金門附近）。耶穌榮耀地進入耶路撒冷時，所穿過的可能就是哈米弗甲門或東門（太二一10）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "接下来，祭司修筑了马门以北的城墙，他们各自修筑自家对面的部分。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從馬門往上的一段，祭司們各人修築自己房屋對面的城牆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從馬門往上，眾祭司各對自己的房屋修造。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Above the Horse Gate the priests carried out repairs, each in front of his house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Above the Horse Gate, the priests made repairs, each in front of his own house."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Above the Horse Gate, the priests repaired the wall. Each one repaired the section immediately across from his own house."
      }
