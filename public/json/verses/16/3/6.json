    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴西亚的儿子耶何耶大和比所玳的儿子米书兰修筑古门，架上横梁，安上门，装上门闩和门锁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴西亞的兒子耶何耶大和比所玳的兒子米書蘭修築古門；他們安置橫梁，裝上門扇、門插和門閂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴西亞的兒子耶何耶大與比所玳的兒子米書蘭修造古門，架橫樑，安門扇和閂鎖。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joiada the son of Paseah and Meshullam the son of Besodeiah repaired the Old Gate; they laid its beams and hung its doors with its bolts and its bars."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Jeshanah Gate was repaired by Joiada son of Paseah and Meshullam son of Besodeiah. They laid its beams and put its doors with their bolts and bars in place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Old City Gate was repaired by Joiada son of Paseah and Meshullam son of Besodeiah. They laid the beams, set up its doors, and installed its bolts and bars."
      }
    },
