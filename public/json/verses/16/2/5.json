    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "neh.2.1-8",
            "paragraphs": [
              "事情被提出（二1～8）"
            ]
          },
          {
            "verse": "neh.2.4-5",
            "paragraphs": [
              "4～5.這個親密、快速的場景使我們永生難忘，我們也置身於其中，屏息凝神看著尼希米發出禱告，畢恭畢敬地回答王的問題。他們的對話具一般通性：請求者礙於禮數，需多加解說，王則因習於掌管乾坤，只簡單扼要地說話。他的每個問話都直接了當引到下一問題。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "回答说：“如果王认为好，如果仆人在王面前蒙恩，请王派我到犹大，重建我祖坟所在的城邑。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後對王說：“如果王認為好，僕人能在你面前蒙恩寵，就請你差派我往猶大，到我列祖墳墓所在的城去，讓我重建那城。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我對王說：「僕人若在王眼前蒙恩，王若喜歡，求王差遣我往猶大，到我列祖墳墓所在的那城去，我好重新建造。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And I said to the king, “If it pleases the king, and if your servant has found favor in your sight, that you send me to Judah, to the city of my fathers’ graves, that I may rebuild it.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I said to the king, “If it please the king, and if your servant has found favor before you, send me to Judah, to the city of my fathers’ tombs, that I may rebuild it.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and I answered the king, “If it pleases the king and if your servant has found favor in his sight, let him send me to the city in Judah where my ancestors are buried so that I can rebuild it.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And I said to the king, “If it pleases the king, and if your servant has found favor in your sight, I ask that you send me to Judah, to the city of my fathers’ tombs, that I may rebuild it.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I replied, “If it please the king, and if you are pleased with me, your servant, send me to Judah to rebuild the city where my ancestors are buried.”"
      }
    },
