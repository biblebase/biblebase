      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你曾看見我們列祖在埃及所受的困苦，垂聽他們在紅海邊的哀求，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“You saw the affliction of our fathers in Egypt, And heard their cry by the Red Sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“You saw the suffering of our ancestors in Egypt; you heard their cry at the Red Sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“You saw the misery of our ancestors in Egypt, and you heard their cries from beside the Red Sea."
      }
    },
