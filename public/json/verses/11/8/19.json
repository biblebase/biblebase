    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.8.14-21",
            "paragraphs": [
              "b.所羅門的宣言（八14～21）",
              "所羅門的宣告由第12節開始，但由第15節起，他重述神在這個國家歷史中的作為，祂揀選一個子民、一處地方（錫安）及一個人（藉著大衛支派，參：詩六十八，八十九，一三二）。王面對會眾，站著祝福他們brk，這並非只是祭司才能作的；15節的稱頌（NIV譯為“贊美”，RSV仍作“祝福”）。神藉著拿單（撒下七5～16）傳達祂給大衛的應許。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但你不要建，要由你的亲生儿子为我建殿。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然而你不要建造這殿，只有你親生的兒子，他必為我的名建造這殿。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是你不可建殿，惟你所生的兒子必為我名建殿。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nevertheless you shall not build the house, but your son who will be born to you, he will build the house for My name.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nevertheless, you are not the one to build the temple, but your son, your own flesh and blood—he is the one who will build the temple for my Name.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "but you are not the one to do it. One of your own sons will build the Temple to honor me.’"
      }
