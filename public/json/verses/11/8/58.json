      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願他使我們的心歸向他，遵行他的一切道，謹守他吩咐我們列祖的誡命、律例和典章。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使我們的心歸向他，遵行他的道，謹守他吩咐我們列祖的誡命、律例、典章。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
