    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "明天这个时候，我会派属下搜查你的王宫和你臣仆的家，他们要拿走你珍爱的一切。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "明天大約這個時間，我要差派我的臣僕到你那裡去；他們要搜查你的王宮和你臣僕的家；凡是他們看為寶貴的，他們都要下手拿走。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但明日約在這時候，我還要差遣臣僕到你那裏，搜查你的家和你僕人的家，將你眼中一切所喜愛的都拿了去。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "but about this time tomorrow I will send my servants to you, and they will search your house and the houses of your servants; and whatever is desirable in your eyes, they will take in their hand and carry away.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But about this time tomorrow I am going to send my officials to search your palace and the houses of your officials. They will seize everything you value and carry it away.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But about this time tomorrow I will send my officials to search your palace and the homes of your officials. They will take away everything you consider valuable!’”"
      }
