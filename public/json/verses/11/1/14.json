      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, while you are still there speaking with the king, I will come in after you and confirm your words.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "While you are still there talking to the king, I will come in and add my word to what you have said.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And while you are still talking with him, I will come and confirm everything you have said.”"
      }
