    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "1ki.16.1-5",
            "paragraphs": [
              "一六1～5由於巴沙選擇象耶羅波安一樣活在罪中，所以他會得到耶羅波安家那種不名譽的下場（比較11,12節；一四10～13）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我从尘土中提拔你，立你为我以色列子民的王，你却重蹈耶罗波安的覆辙，使我的子民陷入罪中，惹我发怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我既然從塵埃中高舉了你，立你作我的子民以色列的領袖；然而，你卻走耶羅波安的路，使我的子民以色列犯罪，因他們的罪惹我發怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我既從塵埃中提拔你，立你作我民以色列的君，你竟行耶羅波安所行的道，使我民以色列陷在罪裏，惹我發怒，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Inasmuch as I exalted you from the dust and made you leader over My people Israel, and you have walked in the way of Jeroboam and have made My people Israel sin, provoking Me to anger with their sins,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I lifted you up from the dust and appointed you ruler over my people Israel, but you followed the ways of Jeroboam and caused my people Israel to sin and to arouse my anger by their sins."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I lifted you out of the dust to make you ruler of my people Israel, but you have followed the evil example of Jeroboam. You have provoked my anger by causing my people Israel to sin."
      }
    },
