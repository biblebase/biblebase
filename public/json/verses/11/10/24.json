    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.10.14-29",
            "paragraphs": [
              "iii.所羅門的財富（十14～29）",
              "參：歷代誌下九13～24。所羅門的榮華成為人盡皆知的諺語（太六29），一如他的智慧之名（太十二42）。若有人認為他一年收入六百六十六他連得（約計21.6噸）金子是誇大其詞的話，不妨去與大約此時古代埃及有記錄的收入相比：“金子如地上的塵沙那麽多”，Osorkon一世在他頭四年（約主前924～920年）的統治中累積了十八噸金子，在此以外，還應加上他父親示撒自耶路撒冷取得的擄物（參十四25～27）。有證據顯示米索不達米亞也有類似如此大量金子的取用236。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天下的人都纷纷来朝见他，聆听上帝赐给他的智言慧语。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "世上所有的人都要求晉見所羅門，聆聽　神賦予他心中的智慧。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "普天下的王都求見所羅門，要聽神賜給他智慧的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "All the earth was seeking the presence of Solomon, to hear his wisdom which God had put in his heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The whole world sought audience with Solomon to hear the wisdom God had put in his heart."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People from every nation came to consult him and to hear the wisdom God had given him."
      }
    },
