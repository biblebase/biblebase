    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.9.10-28",
            "paragraphs": [
              "i.總論（九10～28）"
            ]
          },
          {
            "verse": "1ki.9.26-28",
            "paragraphs": [
              "e.所羅門的海上貿易（九26～28）",
              "請與十章11～12節、歷代誌下八17～18同讀，以得更全面之看見。船只（'$ni^,AV作“海軍”）乃由有經驗的腓尼基造船匠人建造及操作I^，一如西拿基立的亞述船隊在波斯灣運作一樣。基地港口為以旬迦別，現在被認為是GuzaratalFar~un，其附近的儲藏設施亦被葛路克（N.Glueck）在亞喀巴海灣的Tellel-Kheleifeh所發現。該地一向是交通樞紐，同時也控制由阿拉伯來的旅行商隊路線，於主前八四八年為約蘭重建，後又被亞撒利雅重建（約主前780年；二十二48）。以祿（靠近現代的Elat；參：以拉他，王下十六6；申二8）也同樣在亞喀巴海灣（ya{mso^p{，耶四十九21），與出埃及記（十三18）的紅海（ya{msu^p{，蘆葦海）不同。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希兰派有经验的水手与所罗门的水手一起出海，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希蘭派遣他船隊的僕人，就是熟悉航海的船員，與所羅門的僕人一同航海。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希蘭差遣他的僕人，就是熟悉泛海的船家，與所羅門的僕人一同坐船航海。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And Hiram sent his servants with the fleet, sailors who knew the sea, along with the servants of Solomon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And Hiram sent his men—sailors who knew the sea—to serve in the fleet with Solomon’s men."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Hiram sent experienced crews of sailors to sail the ships with Solomon’s men."
      }
    },
