    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.12.1-24",
            "paragraphs": [
              "i.羅波安（十二1～24）",
              "參：歷代誌下十1～十一4；約瑟夫《猶太古史》viii.8.1-3."
            ]
          },
          {
            "verse": "1ki.12.6-15",
            "paragraphs": [
              "b.兩種建議（十二6～15）",
              "民眾要求減免稅收、苦工及兵役是合理的，因為他們在所羅門的治下深受其苦。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "过了三天，耶罗波安和民众遵照罗波安王的话来见他。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶羅波安和眾民，照王所說“三天以後再來見我”的那話，第三天來見羅波安。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶羅波安和眾百姓遵着羅波安王所說「你們第三日再來見我」的那話，第三日他們果然來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jeroboam and all the people came to Rehoboam on the third day as the king had directed, saying, “Return to me on the third day.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Three days later Jeroboam and all the people returned to Rehoboam, as the king had said, “Come back to me in three days.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Three days later Jeroboam and all the people returned to hear Rehoboam’s decision, just as the king had ordered."
      }
    },
