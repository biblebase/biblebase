    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "希兰听了所罗门的话后，非常高兴，说：“今天当赞美耶和华！祂赐给大卫一个有智慧的儿子治理这伟大的民族。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "希蘭聽見了所羅門的話，就十分歡喜，說：“今天耶和華是應當稱頌的，因為他賜給了大衛一個有智慧的兒子治理這麼眾多的人民。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "希蘭聽見所羅門的話，就甚喜悅，說：「今日應當稱頌耶和華；因他賜給大衛一個有智慧的兒子，治理這眾多的民。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Hiram heard the words of Solomon, he rejoiced greatly and said, “Blessed be the Lord today, who has given to David a wise son over this great people.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Hiram heard Solomon’s message, he was greatly pleased and said, “Praise be to the Lord today, for he has given David a wise son to rule over this great nation.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Hiram received Solomon’s message, he was very pleased and said, “Praise the Lord today for giving David a wise son to be king of the great nation of Israel.”"
      }
    },
