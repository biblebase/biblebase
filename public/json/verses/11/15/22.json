    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚撒王令全国的犹大人出动，将巴沙修筑拉玛城的石头和木材搬走，用来修建便雅悯的迦巴和米斯巴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是亞撒王通告猶大眾人，沒有一個人例外。他們就把巴沙修築拉瑪所用的石頭和木料運走；亞撒王用那些材料來修築便雅憫的迦巴和米斯巴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是亞撒王宣告猶大眾人，不准一個推辭，吩咐他們將巴沙修築拉瑪所用的石頭、木頭都運去，用以修築便雅憫的迦巴和米斯巴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then King Asa made a proclamation to all Judah—none was exempt—and they carried away the stones of Ramah and its timber with which Baasha had built. And King Asa built with them Geba of Benjamin and Mizpah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then King Asa issued an order to all Judah—no one was exempt—and they carried away from Ramah the stones and timber Baasha had been using there. With them King Asa built up Geba in Benjamin, and also Mizpah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then King Asa sent an order throughout Judah, requiring that everyone, without exception, help to carry away the building stones and timbers that Baasha had been using to fortify Ramah. Asa used these materials to fortify the town of Geba in Benjamin and the town of Mizpah."
      }
