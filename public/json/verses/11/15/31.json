    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "1ki.15.25-32",
            "paragraphs": [
              "iv.以色列的拿答（十五25～32）",
              "歷史敘事重新回到與亞撒同時代的以色列君王身上。巴沙的政變預示到耶羅波安家族會滅亡。資料來源包括當地有關叛亂的記錄（27～28節）以及先知所記載的歷史（29～30節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拿答其他的事及其一切所作所为都记在以色列的列王史上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "拿答其餘的事蹟和他所行的一切，不是都記在以色列諸王的年代誌上嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拿答其餘的事，凡他所行的，都寫在以色列諸王記上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the rest of the acts of Nadab and all that he did, are they not written in the Book of the Chronicles of the Kings of Israel?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As for the other events of Nadab’s reign, and all he did, are they not written in the book of the annals of the kings of Israel?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The rest of the events in Nadab’s reign and everything he did are recorded in The Book of the History of the Kings of Israel."
      }
    },
