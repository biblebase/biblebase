    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.16.7-16",
            "paragraphs": [
              "夏甲與天使（十六7～16）"
            ]
          },
          {
            "verse": "gen.16.7-9",
            "paragraphs": [
              "7～9.夏甲朝向她的出生地埃及進發（書珥的曠野在埃及東北邊界），在到達加低斯附近（14節）之前，她恐怕已經走了好些天。可是如今她的命運與亞伯蘭相聯，而神的美意要她持定這點（9、15節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "gen.16.7",
            "paragraphs": [
              "一六7\"在曠野書珥路上\"。在從別是巴往埃及的某段路上；埃及是夏甲的本國。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的天使在旷野中通往书珥之路的水泉旁找到了夏甲，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的使者在曠野的水泉旁邊，就是在到書珥路上的水旁邊，遇到了她，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的使者在曠野書珥路上的水泉旁遇見她，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the angel of the Lord found her by a spring of water in the wilderness, by the spring on the way to Shur."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The angel of the Lord found Hagar near a spring in the desert; it was the spring that is beside the road to Shur."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The angel of the Lord found Hagar beside a spring of water in the wilderness, along the road to Shur."
      }
    },
