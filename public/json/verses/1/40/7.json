    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.40.1-23",
            "paragraphs": [
              "酒政與膳長之夢（四十1～23）"
            ]
          },
          {
            "verse": "gen.40.6-8",
            "paragraphs": [
              "6～8.除了效率與正直外，約瑟對人迅速的關懷，披露出他性格的另一方面；他立刻提及神，聽來是出乎真誠：這是他思想的習慣（參三十九9，四十一16、51、52，四十五8，等）。至於這兩位作夢者，他們同樣確信夢是有意義的：在埃及一般人認為，它們具有預測性；有關如何解釋它們的著作愈來愈多。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就问：“你们今天为什么满面愁容？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就問法老的兩個臣宰，就是與他一同關在他主人府內的監房裡的，說：“今天你們的臉色為甚麼這樣難看呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他便問法老的二臣，就是與他同囚在他主人府裏的，說：「你們今日為甚麼面帶愁容呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He asked Pharaoh’s officials who were with him in confinement in his master’s house, “Why are your faces so sad today?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So he asked Pharaoh’s officials who were in custody with him in his master’s house, “Why do you look so sad today?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why do you look so worried today?” he asked them."
      }
    },
