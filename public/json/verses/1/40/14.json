    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当你再被重用的时候，请你记得恩待我，在法老面前提及我，救我出狱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不過，你一切順利的時候，求你記念我，施恩給我，在法老面前提拔我，救我脫離這監獄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你得好處的時候，求你記念我，施恩與我，在法老面前提說我，救我出這監牢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Only keep me in mind when it goes well with you, and please do me a kindness by mentioning me to Pharaoh and get me out of this house."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But when all goes well with you, remember me and show me kindness; mention me to Pharaoh and get me out of this prison."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And please remember me and do me a favor when things go well for you. Mention me to Pharaoh, so he might let me out of this place."
      }
    },
