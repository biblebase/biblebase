    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.46.28-34",
            "paragraphs": [
              "約瑟和父親團圓（四十六28～34）"
            ]
          },
          {
            "verse": "gen.46.30",
            "paragraphs": [
              "30.這是一種舊約的辭世告別語。從三十七35起，每處記載雅各的話都提到死，但自四十五28的轉捩點開始，悲苦的味道漸漸被應許實現的欣喜與盼望所取代。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列对约瑟说：“我现在看到你还活着，就是死也安心了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列對約瑟說：“這一次我看見了你的面，知道你還在，我死了也甘心。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列對約瑟說：「我既得見你的面，知道你還在，就是死我也甘心。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Israel said to Joseph, “Now let me die, since I have seen your face, that you are still alive.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Israel said to Joseph, “Now I am ready to die, since I have seen for myself that you are still alive.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Finally, Jacob said to Joseph, “Now I am ready to die, since I have seen your face again and know you are still alive.”"
      }
    },
