    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.41.1-45",
            "paragraphs": [
              "法老的夢與約瑟躍升為宰相（四十一1～45）",
              "1、2.RSV正確地指出此即尼羅河，如希伯來文的明示。我們可能順帶註意到，這個夢具埃及色彩，它描述母牛從河裏上來（弗格特指出，母牛喜歡沈浸在水中以躲避熱氣和蒼蠅），尋求蘆葦的草場（RV、RSV）或紙草的草圃。後者的希伯來文~a{h]u%^%%%，是從埃及借用來的字289；在約伯記八11中再次出現（參閱七十士譯本賽十九7和傳四十16）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "后来我又梦见一株麦子，上面长了七个饱满的穗子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在夢裡我又看見一根麥莖，長出七個麥穗，又飽滿又美好。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我又夢見一棵麥子，長了七個穗子，又飽滿又佳美，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I saw also in my dream, and behold, seven ears, full and good, came up on a single stalk;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“In my dream I saw seven heads of grain, full and good, growing on a single stalk."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“In my dream I also saw seven heads of grain, full and beautiful, growing on a single stalk."
      }
    },
