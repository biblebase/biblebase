    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.41.1-45",
            "paragraphs": [
              "法老的夢與約瑟躍升為宰相（四十一1～45）",
              "1、2.RSV正確地指出此即尼羅河，如希伯來文的明示。我們可能順帶註意到，這個夢具埃及色彩，它描述母牛從河裏上來（弗格特指出，母牛喜歡沈浸在水中以躲避熱氣和蒼蠅），尋求蘆葦的草場（RV、RSV）或紙草的草圃。後者的希伯來文~a{h]u%^%%%，是從埃及借用來的字289；在約伯記八11中再次出現（參閱七十士譯本賽十九7和傳四十16）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "接着又长出七个干瘪的穗子，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "接著，又長出了七個麥穗，又枯乾又乾瘦又被東風吹焦了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "隨後又長了七個穗子，枯槁細弱，被東風吹焦了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and lo, seven ears, withered, thin, and scorched by the east wind, sprouted up after them;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After them, seven other heads sprouted—withered and thin and scorched by the east wind."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then seven more heads of grain appeared, but these were blighted, shriveled, and withered by the east wind."
      }
