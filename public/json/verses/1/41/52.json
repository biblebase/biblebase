      "leili": {
        "content": [
          {
            "verse": "gen.41.52",
            "paragraphs": [
              "四一52\"以法蓮\"的意思是\"昌盛\"。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约瑟给次子取名叫以法莲，因为他说：“上帝使我在受苦之地昌盛。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他給次子起名叫以法蓮，因為他說：“　神使我在受苦之地繁盛起來。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他給次子起名叫以法蓮，因為他說：「神使我在受苦的地方昌盛。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He named the second Ephraim, “For,” he said, “God has made me fruitful in the land of my affliction.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The second son he named Ephraim and said, “It is because God has made me fruitful in the land of my suffering.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Joseph named his second son Ephraim, for he said, “God has made me fruitful in this land of my grief.”"
      }
    },
