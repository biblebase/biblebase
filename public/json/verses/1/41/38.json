    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.41.1-45",
            "paragraphs": [
              "法老的夢與約瑟躍升為宰相（四十一1～45）",
              "1、2.RSV正確地指出此即尼羅河，如希伯來文的明示。我們可能順帶註意到，這個夢具埃及色彩，它描述母牛從河裏上來（弗格特指出，母牛喜歡沈浸在水中以躲避熱氣和蒼蠅），尋求蘆葦的草場（RV、RSV）或紙草的草圃。後者的希伯來文~a{h]u%^%%%，是從埃及借用來的字289；在約伯記八11中再次出現（參閱七十士譯本賽十九7和傳四十16）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "gen.41.37-38",
            "paragraphs": [
              "四一37,38法老和他一切的臣仆都深信約瑟有超乎常人的洞見（比較但五11）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法老对臣仆说：“我们哪里找像这样有上帝的灵同在的人呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是法老對臣僕說：“像這樣的人，有　神的靈在他裡面，我們怎能找得著呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "法老對臣僕說：「像這樣的人，有神的靈在他裏頭，我們豈能找得着呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Pharaoh said to his servants, “Can we find a man like this, in whom is a divine spirit?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Pharaoh asked them, “Can we find anyone like this man, one in whom is the spirit of God?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Pharaoh asked his officials, “Can we find anyone else like this man so obviously filled with the spirit of God?”"
      }
