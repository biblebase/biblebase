      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now these are the records of the generations of Shem, Ham, and Japheth, the sons of Noah; and sons were born to them after the flood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is the account of Shem, Ham and Japheth, Noah’s sons, who themselves had sons after the flood."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
