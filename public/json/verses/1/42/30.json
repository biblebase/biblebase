    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.42.1-38",
            "paragraphs": [
              "約瑟的兄弟在埃及尋糧（四十二1～38）"
            ]
          },
          {
            "verse": "gen.42.18-38",
            "paragraphs": [
              "18～38.西緬被留下來當人質。他們有三天全被下在監獄裏，充分證明治理者大權在握；既是如此，他卻對他們的家庭表示關切，又坦承自己的動機是敬畏神，這必然導致他們深自反省（18～20節）。這新的決定很可能是因真的改變心意而來；但無論如何，它顯出了敬虔對於治理會產生美好的影響。詩篇十四1、4則描述與此相悖的光景。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说：“埃及的宰相对我们说话很凶，还把我们当成奸细。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“那地的主人對我們說了一些嚴厲的話，把我們當作偵察那地的間諜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「那地的主對我們說嚴厲的話，把我們當作窺探那地的奸細。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“The man, the lord of the land, spoke harshly with us, and took us for spies of the country."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“The man who is lord over the land spoke harshly to us and treated us as though we were spying on the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The man who is governor of the land spoke very harshly to us,” they told him. “He accused us of being spies scouting the land."
      }
    },
