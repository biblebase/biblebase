    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.43.1-34",
            "paragraphs": [
              "第二次訪問埃及（四十三1～34）"
            ]
          },
          {
            "verse": "gen.43.15-34",
            "paragraphs": [
              "15～34.約瑟的十一位客人。弟兄們的害怕（18節）透露出他們的無知，以為埃及人像氣量狹小的部落，要奪取他們的驢。這情采地刻劃出這住帳棚的人與周遭新奇環境之間的對比。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们回答说：“你仆人——我们的父亲仍然健在、安康。”说完了，又向他下拜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們回答：“你僕人我們的父親平安，他還在。”於是他們低頭下拜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們回答說：「你僕人－我們的父親平安；他還在。」於是他們低頭下拜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They said, “Your servant our father is well; he is still alive.” They bowed down in homage."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They replied, “Your servant our father is still alive and well.” And they bowed down, prostrating themselves before him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yes,” they replied. “Our father, your servant, is alive and well.” And they bowed low again."
      }
    },
