    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.43.1-34",
            "paragraphs": [
              "第二次訪問埃及（四十三1～34）"
            ]
          },
          {
            "verse": "gen.43.15-34",
            "paragraphs": [
              "15～34.約瑟的十一位客人。弟兄們的害怕（18節）透露出他們的無知，以為埃及人像氣量狹小的部落，要奪取他們的驢。這情采地刻劃出這住帳棚的人與周遭新奇環境之間的對比。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约瑟来了，他们就把带来的礼物献给他，向他俯伏下拜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約瑟回到家裡，他們就把手中的禮物帶進屋裡送給他，又俯伏在地向他下拜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約瑟來到家裏，他們就把手中的禮物拿進屋去給他，又俯伏在地，向他下拜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Joseph came home, they brought into the house to him the present which was in their hand and bowed to the ground before him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Joseph came home, they presented to him the gifts they had brought into the house, and they bowed down before him to the ground."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Joseph came home, they gave him the gifts they had brought him, then bowed low to the ground before him."
      }
    },
