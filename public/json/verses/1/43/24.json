    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.43.1-34",
            "paragraphs": [
              "第二次訪問埃及（四十三1～34）"
            ]
          },
          {
            "verse": "gen.43.15-34",
            "paragraphs": [
              "15～34.約瑟的十一位客人。弟兄們的害怕（18節）透露出他們的無知，以為埃及人像氣量狹小的部落，要奪取他們的驢。這情采地刻劃出這住帳棚的人與周遭新奇環境之間的對比。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "接着带他们进约瑟的家，给他们水洗脚，给他们的驴喂草料。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "管家把他們領到約瑟的家裡，給他們水洗腳，又給他們飼料餵驢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "家宰就領他們進約瑟的屋裏，給他們水洗腳，又給他們草料餵驢。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then the man brought the men into Joseph’s house and gave them water, and they washed their feet; and he gave their donkeys fodder."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The steward took the men into Joseph’s house, gave them water to wash their feet and provided fodder for their donkeys."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The manager then led the men into Joseph’s palace. He gave them water to wash their feet and provided food for their donkeys."
      }
    },
