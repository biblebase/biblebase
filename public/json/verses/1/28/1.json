    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.28.1-9",
            "paragraphs": [
              "雅各被打發到米所波大米（二十八1～9）",
              "雅各踏上了他生平的第二階段（參二十五19～34的註釋）；在神的安排下，透過利百加的外交手腕（參上段），他離家之時，前有明確的目標，後有父親的祝福支持──這是對以掃的明言警告，叫他不要插手。以掃看見這一切，就想靠自己來作父母喜歡的事，但這種努力正像天然人對宗教的熱忱252，盡屬表面，判斷也錯誤。雖然以實瑪利人比赫人略勝一籌，但娶第三個妻子絕非重獲祝福之途。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以撒把雅各叫来，给他祝福，又嘱咐他说：“你不要娶迦南的女子为妻。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以撒把雅各叫了來，給他祝福，並且吩咐他，說：“你不可娶迦南的女子為妻。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以撒叫了雅各來，給他祝福，並囑咐他說：「你不要娶迦南的女子為妻。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Isaac called Jacob and blessed him and charged him, and said to him, “You shall not take a wife from the daughters of Canaan."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Isaac called for Jacob and blessed him. Then he commanded him: “Do not marry a Canaanite woman."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Isaac called for Jacob, blessed him, and said, “You must not marry any of these Canaanite women."
      }
    },
