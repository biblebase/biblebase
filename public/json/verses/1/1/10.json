    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.1.1-23",
            "paragraphs": [
              "Ａ創造的故事（一1～23）",
              "序言（一1、2）"
            ]
          },
          {
            "verse": "gen.1.9-13",
            "paragraphs": [
              "第三日（一9～13）",
              "神繼續以分開的程序來使世界成形（9、10節，見4、5節）；但重點開始轉變，朝向充滿的主題（11、12節），在本章余下部分益見鮮明。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "gen.1.10",
            "paragraphs": [
              "一10\"神稱\"。在閃族文化裏，給宇宙萬物起名的行為，已經是擁有主權的明證（比較王下二三34）。留意這一點在第二章19節的重要性。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝称干地为陆地，称水汇聚的地方为海洋。上帝看了，感到满意。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "　神稱旱地為地，稱水的聚處為海。　神看這是好的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "神称干处为“地”，称水的聚集之处为“海”。神看这是好的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神稱旱地為「地」，稱水的聚處為「海」。神看着是好的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "God called the dry land Earth, and the waters that were gathered together he called Seas. And God saw that it was good."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "God called the dry land earth, and the gathering of the waters He called seas; and God saw that it was good."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "God called the dry ground “land,” and the gathered waters he called “seas.” And God saw that it was good."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And God called the dry land Earth, and the gathering together of the waters He called Seas. And God saw that it  was good."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "God called the dry ground “land” and the waters “seas.” And God saw that it was good."
      }
