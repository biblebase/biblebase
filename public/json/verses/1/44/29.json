    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要是你们把他的弟弟也带走，遇上什么意外的话，你们会使我这白发苍苍的老人凄凄惨惨地进坟墓。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在你們又要把這個從我面前帶去，如果他遇到不幸，你們就使我這個白髮老人，悲悲慘慘地下陰間去了。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在你們又要把這個帶去離開我，倘若他遭害，那便是你們使我白髮蒼蒼、悲悲慘慘地下陰間去了。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you take this one also from me, and harm befalls him, you will bring my gray hair down to Sheol in sorrow.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you take this one from me too and harm comes to him, you will bring my gray head down to the grave in misery.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now if you take his brother away from me, and any harm comes to him, you will send this grieving, white-haired man to his grave.’"
      }
    },
