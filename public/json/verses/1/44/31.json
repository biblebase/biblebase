    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他一看这孩子不在，一定会死。仆人们会使自己白发苍苍的父亲凄凄惨惨地进坟墓。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他一見沒有孩子，就必定死；這樣，你僕人們就使你僕人我們的父親，這個白髮老人，愁愁苦苦地下陰間去了。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我父亲见那少年不在了，就必会死。这样，你仆人们就使你仆人我们的父亲，一个白发苍苍的老人，悲悲惨惨地下阴间去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們的父親見沒有童子，他就必死。這便是我們使你僕人－我們的父親白髮蒼蒼、悲悲慘慘地下陰間去了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "as soon as he sees that the boy is not with us, he will die, and your servants will bring down the gray hairs of your servant our father with sorrow to Sheol."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "when he sees that the lad is not with us, he will die. Thus your servants will bring the gray hair of your servant our father down to Sheol in sorrow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "sees that the boy isn’t there, he will die. Your servants will bring the gray head of our father down to the grave in sorrow."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "it will happen, when he sees that the lad is not with  us, that he will die. So your servants will bring down the gray hair of your servant our father with sorrow to the grave."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If he sees that the boy is not with us, our father will die. We, your servants, will indeed be responsible for sending that grieving, white-haired man to his grave."
      }
    },
