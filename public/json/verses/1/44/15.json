    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.44.1-17",
            "paragraphs": [
              "便雅憫被捕（四十四1～17）",
              "約瑟的策略已經大放光芒，成功地制造了他想要的狀況和忐忑的心情，現在更要施展最高明的一招。就像所羅門的審判一樣，這回對便雅憫突如其來的威脅，直插入心臟：剎那間兄弟的真況便暴露無遺。他們在第9節的抗議中作下允諾，家宰加以改變，只要留下便雅憫，其他人都有機會得自由；所有條件對他們再一次的背叛都甚有利，而且所能獲得的是自由之身，這比當年他們平分的二十舍客勒銀子要寶貴得多了。他們的回應，從全體一致（13節）、坦承不諱（16節）、堅持不變（因為同樣的條件再一次提出，17節），明顯可見管教的招數已經奏效。猶大懇請替便雅憫坐牢之舉，是所有請求中最美好、最動人的例子（見以下18～34的註釋）。"
            ]
          },
          {
            "verse": "gen.44.15",
            "paragraphs": [
              "15.見第5節註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "fengsheng": {
        "content": [
          {
            "verse": "gen.44.15",
            "paragraphs": [
              "44:15",
              "必能占蔔嗎：參5節註。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约瑟对他们说：“你们做的是什么事？难道你们不知道像我这样的人是懂得占卜的吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約瑟對他們說：“你們所作的是甚麼事呢？你們不知道像我這樣的人必能占卜嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約瑟對他們說：「你們做的是甚麼事呢？你們豈不知像我這樣的人必能占卜嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Joseph said to them, “What is this deed that you have done? Do you not know that such a man as I can indeed practice divination?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Joseph said to them, “What is this you have done? Don’t you know that a man like me can find things out by divination?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What have you done?” Joseph demanded. “Don’t you know that a man like me can predict the future?”"
      }
