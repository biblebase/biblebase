    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "gen.44.33",
            "paragraphs": [
              "四四33猶大情詞懇切之請求的高潮，就是自己願意代替便雅憫受罰--一個美妙的事例，說明猶大偉大的後人耶穌在各各他山上所成就的事。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在求你准许仆人代替这孩子做你的奴仆，让他跟他哥哥们回去吧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在求你容許僕人留下，代替這孩子作我主的奴僕，讓這孩子與他的哥哥們一同上去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在求你容僕人住下，替這童子作我主的奴僕，叫童子和他哥哥們一同上去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now, therefore, please let your servant remain instead of the lad a slave to my lord, and let the lad go up with his brothers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Now then, please let your servant remain here as my lord’s slave in place of the boy, and let the boy return with his brothers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“So please, my lord, let me stay here as a slave instead of the boy, and let the boy return with his brothers."
      }
