    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们告诉他，我们不能来，只有我们的弟弟同来，我们才能来。否则，我们不能见你的面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們就說：‘我們不能下去，如果有我們最小的弟弟與我們同去，我們就下去；因為我們最小的弟弟不與我們同去，我們就不得見那人的面。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們就說：『我們不能下去。我們的小兄弟若和我們同往，我們就可以下去。因為，小兄弟若不與我們同往，我們必不得見那人的面。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But we said, ‘We cannot go down. If our youngest brother is with us, then we will go down; for we cannot see the man’s face unless our youngest brother is with us.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But we said, ‘We cannot go down. Only if our youngest brother is with us will we go. We cannot see the man’s face unless our youngest brother is with us.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "we replied, ‘We can’t go unless you let our youngest brother go with us. We’ll never get to see the man’s face unless our youngest brother is with us.’"
      }
    },
