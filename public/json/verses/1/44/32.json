    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "仆人曾经向父亲保证这孩子的安全，说我若不把这孩子带回他身边，情愿一生担罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為僕人曾經向父親擔保這孩子的安全，說：‘如果我不把他帶回來交還給你，我就在父親面前終生承擔這罪。’"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你的仆人曾经为了那少年的缘故向父亲担保，说：‘如果我不把他带回来给你，我就在父亲面前终生担罪。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為僕人曾向我父親為這童子作保，說：『我若不帶他回來交給父親，我便在父親面前永遠擔罪。』"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "For your servant became a pledge of safety for the boy to my father, saying, ‘If I do not bring him back to you, then I shall bear the blame before my father all my life.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For your servant became surety for the lad to my father, saying, ‘If I do not bring him back to you, then let me bear the blame before my father forever.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your servant guaranteed the boy’s safety to my father. I said, ‘If I do not bring him back to you, I will bear the blame before you, my father, all my life!’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For your servant became surety for the lad to my father, saying, ‘If I do not bring him back to you, then I shall bear the blame before my father forever.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My lord, I guaranteed to my father that I would take care of the boy. I told him, ‘If I don’t bring him back to you, I will bear the blame forever.’"
      }
    },
