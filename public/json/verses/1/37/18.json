    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约瑟的哥哥们远远看见他走来，趁他还没有到跟前，就合谋要害死他。"
      },
      "cnv": {
        "version_id": "CNV",
