    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.37.25-36",
            "paragraphs": [
              "約瑟被賣到埃及（三十七25～36）"
            ]
          },
          {
            "verse": "gen.37.32",
            "paragraphs": [
              "32.你兒子為極冷酷的表達，不屑於提名字；路加福音十五30以下，也捕捉了這一神情：“你這個兒子……”（該處受到溫和的指正：“你這個兄弟……”）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "拿去给父亲，说：“我们捡到这件衣服，你看看是不是你儿子的？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後打發人把彩衣送到他們父親那裡，說：“這是我們找到的。請你認一認，是你兒子的長衣不是？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "打發人送到他們的父親那裏，說：「我們撿了這個；請認一認是你兒子的外衣不是？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they sent the varicolored tunic and brought it to their father and said, “We found this; please examine it to see whether it is your son’s tunic or not.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They took the ornate robe back to their father and said, “We found this. Examine it to see whether it is your son’s robe.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They sent the beautiful robe to their father with this message: “Look at what we found. Doesn’t this robe belong to your son?”"
      }
    },
