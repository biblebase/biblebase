        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "現在這地的饑荒已經有兩年了，還有五年不能耕種，也沒有收成。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "現在這地的饑荒已經二年了，還有五年不能耕種，不能收成。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For the famine has been in the land these two years, and there are still five years in which there will be neither plowing nor harvesting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For two years now there has been famine in the land, and for the next five years there will be no plowing and reaping."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This famine that has ravaged the land for two years will last five more years, and there will be neither plowing nor harvesting."
      }
