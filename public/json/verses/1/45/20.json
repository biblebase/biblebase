    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "gen.45.16-28",
            "paragraphs": [
              "法老差人去接以色列（四十五16～28）",
              "接近絕望邊緣的以色列，和罪擔沈重的十兄弟，因著約瑟的緣故，受到王室的邀請；這不能不讓基督徒聯想到那神聖的邀請：“到我這裏來，我就使……”（參18節）──這段話既表示歡迎，又充滿挑戰。不過，在歷史上，這邀請卻帶來另一種轉變，正如早年的預言所示（十五13～16）：這家族開始進入隔離階段（他們在當地完全是外國人，因此可以繁衍，不致失去種族特色），最後被奴役，並遭解救，如此便產生一種民族，他們永遠深知自己是蒙召的，也是蒙救贖的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要担心你们在迦南的产业，因为埃及全国的美物都是你们的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要顧惜你們的家具，因為埃及全地美好的產物都是你們的。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們眼中不要愛惜你們的家具，因為埃及全地的美物都是你們的。』」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not concern yourselves with your goods, for the best of all the land of Egypt is yours.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Never mind about your belongings, because the best of all Egypt will be yours.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t worry about your personal belongings, for the best of all the land of Egypt is yours.’”"
      }
