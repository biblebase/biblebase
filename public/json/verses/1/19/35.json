    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当天晚上，她们又灌醉了罗得，小女儿进去与他睡。她什么时候躺下，什么时候起来，罗得完全不知道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "當夜，她們又叫父親喝酒，小女兒就起來，與父親同睡；她甚麼時候躺下，甚麼時候起來，她父親都不知道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是，那夜她們又叫父親喝酒，小女兒起來與她父親同寢；她幾時躺下，幾時起來，父親都不知道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they made their father drink wine that night also, and the younger arose and lay with him; and he did not know when she lay down or when she arose."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So they got their father to drink wine that night also, and the younger daughter went in and slept with him. Again he was not aware of it when she lay down or when she got up."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So that night they got him drunk with wine again, and the younger daughter went in and had intercourse with him. As before, he was unaware of her lying down or getting up again."
      }
