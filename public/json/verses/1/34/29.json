    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "gen.34.25-29",
            "paragraphs": [
              "三四25～29\"到第三天\"，在哈抹家能力最薄弱的時候，\"西緬和利未\"兩人進行了一場報復戰，殺了示劍所有的人，包括哈抹和示劍，救了底拿，並奪取了該城的婦女和財物作戰利品。由於他們的殘忍不仁，他們的後裔因而要分散在色列各處。參看第四十九章5至7節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并带走所有财物、妇孺以及房屋里的一切。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "又把他們所有的財物、孩子和婦女都擄去。屋裡所有的東西，都搶掠一空。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "又掳走、掠夺了他们所有的财富、孩童、妇女，以及房屋里的一切。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又把他們一切貨財、孩子、婦女，並各房中所有的，都擄掠去了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "All their wealth, all their little ones and their wives, all that was in the houses, they captured and plundered."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they captured and looted all their wealth and all their little ones and their wives, even all that was in the houses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They carried off all their wealth and all their women and children, taking as plunder everything in the houses."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and all their wealth. All their little ones and their wives they took captive; and they plundered even all that was in the houses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They looted all their wealth and plundered their houses. They also took all their little children and wives and led them away as captives."
      }
    },
