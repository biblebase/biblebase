    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "gen.34.1",
            "paragraphs": [
              "34:1",
              "底拿……那地的女子們：雅各在外邦城示劍附近住下，他沒有遵照神的命令回到他父親以撒的地方(比較31:13；35:1)。後來他對此選擇感到非常懊悔。在他女兒底拿的悲劇性事件發生以後，雅各才終於決定要上到伯特利，除掉家中一切外邦的神(參下一條註釋及35:2註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一天，利亚给雅各所生的女儿底娜到外面去见当地的女子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "利亞給雅各所生的女兒底拿，出去要看看當地的女子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "利亞給雅各所生的女兒底拿出去，要見那地的女子們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now Dinah the daughter of Leah, whom she had borne to Jacob, went out to visit the daughters of the land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now Dinah, the daughter Leah had borne to Jacob, went out to visit the women of the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "One day Dinah, the daughter of Jacob and Leah, went to visit some of the young women who lived in the area."
      }
    },
