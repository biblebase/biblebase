      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他主人見耶和華與他同在，又見耶和華使他手裏所辦的盡都順利，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
