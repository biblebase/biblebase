      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "水继续消退，到十月一日，山顶都露出来了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "水繼續消退，直到十月；到了十月初一，山頂都露出來了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "水又漸消，到十月初一日，山頂都現出來了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The water decreased steadily until the tenth month; in the tenth month, on the first day of the month, the tops of the mountains became visible."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The waters continued to recede until the tenth month, and on the first day of the tenth month the tops of the mountains became visible."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Two and a half months later, as the waters continued to go down, other mountain peaks became visible."
      }
