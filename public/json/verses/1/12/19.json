      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你為甚麼說她是你的妹妹，以致我娶了她作妻子呢？現在，你的妻子在這裡，帶她走吧！”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你为什么说‘她是我的妹妹’，以致我要娶她为妻呢？现在，你的妻子在这里，带她走吧！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為甚麼說她是你的妹子，以致我把她取來要作我的妻子？現在你的妻子在這裏，可以帶她走吧。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Why did you say, ‘She is my sister,’ so that I took her for my wife? Now then, here is your wife; take her, and go.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
