    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“盗贼深夜洗劫你的家， 不会洗劫一空； 人们摘葡萄，不会摘光； 但你会被彻底毁灭！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "竊賊若來到你那裡， 強盜若夜間來到， 豈不會偷竊直到夠了呢？ 你將會怎樣被滅絕！ 摘葡萄的若來到你那裡， 豈不只剩下幾粒葡萄嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "盜賊若來在你那裏， 或強盜夜間而來－ 你何竟被剪除－ 豈不偷竊直到夠了呢？ 摘葡萄的若來到你那裏， 豈不剩下些葡萄呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If thieves came to you, if plunderers came by night— how you have been destroyed!— would they not steal only enough for themselves? If grape gatherers came to you, would they not leave gleanings?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If thieves came to you, If robbers by night— O how you will be ruined!— Would they not steal only until they had enough? If grape gatherers came to you, Would they not leave some gleanings?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If thieves came to you, if robbers in the night— oh, what a disaster awaits you!— would they not steal only as much as they wanted? If grape pickers came to you, would they not leave a few grapes?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“If thieves had come to you, If robbers by night— Oh, how you will be cut off!— Would they not have stolen till they had enough? If grape-gatherers had come to you, Would they not have left some gleanings?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“If thieves came at night and robbed you (what a disaster awaits you!), they would not take everything. Those who harvest grapes always leave a few for the poor. But your enemies will wipe you out completely!"
      }
