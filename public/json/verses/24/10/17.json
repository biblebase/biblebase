      "fengsheng": {
        "content": [
          {
            "verse": "jer.10.17",
            "paragraphs": [
              "10:17",
              "17~22當收拾你的財物：巴比倫大軍的入侵已經迫在眉睫，對於耶利米而言這場災難如此真切，他不禁哀聲悲嘆，預先道出了民眾在被擄之地的悲慘心聲(19~20節)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "被围困的犹大人啊， 收拾行装吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "住在被圍困的城中的人哪！ 你們從地上拾起自己的行囊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "受圍困的人哪，當收拾你的財物， 從國中帶出去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Pick up your bundle from the ground, You who dwell under siege!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Gather up your belongings to leave the land, you who live under siege."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Pack your bags and prepare to leave; the siege is about to begin."
      }
