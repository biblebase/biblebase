    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂一声令下，天上大水涌动； 祂使云从地极升起， 使闪电在雨中发出， 祂从自己的仓库吹出风来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他一發聲，空中眾水就澎湃； 他使雲霧從地極上騰； 他造了閃電和雷雨， 又使風從他的倉庫裡吹出來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他一發聲，空中便有多水激動； 他使雲霧從地極上騰； 他造電隨雨而閃， 從他府庫中帶出風來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He utters His voice, there is a tumult of waters in the heavens, And He causes the clouds to ascend from the end of the earth; He makes lightning for the rain, And brings out the wind from His storehouses."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he thunders, the waters in the heavens roar; he makes clouds rise from the ends of the earth. He sends lightning with the rain and brings out the wind from his storehouses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When he speaks in the thunder, the heavens roar with rain. He causes the clouds to rise over the earth. He sends the lightning with the rain and releases the wind from his storehouses."
      }
    },
