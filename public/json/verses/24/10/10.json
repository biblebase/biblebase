    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "唯有耶和华是真神， 是永活的上帝， 是永恒的君王。 祂一发怒，大地便震动， 万国都无法承受。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "只有耶和華是真神； 他是永活的　神，是永遠的君王。 他一發怒，大地就震動， 萬國都不能承受他的忿怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟耶和華是真神， 是活神，是永遠的王。 他一發怒，大地震動； 他一惱恨，列國都擔當不起。 （"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the Lord is the true God; He is the living God and the everlasting King. At His wrath the earth quakes, And the nations cannot endure His indignation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the Lord is the true God; he is the living God, the eternal King. When he is angry, the earth trembles; the nations cannot endure his wrath."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Lord is the only true God. He is the living God and the everlasting King! The whole earth trembles at his anger. The nations cannot stand up to his wrath."
      }
