    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我有祸了！因我的创伤难愈。 但我说：“这是疾病，我必须忍受。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因我的損傷，我有禍了！ 我的創傷不能醫治； 但我說：“這是我的痛苦， 我必須忍受。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "民說：禍哉！我受損傷； 我的傷痕極其重大。 我卻說：這真是我的痛苦， 必須忍受。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Woe is me, because of my injury! My wound is incurable. But I said, “Truly this is a sickness, And I must bear it.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Woe to me because of my injury! My wound is incurable! Yet I said to myself, “This is my sickness, and I must endure it.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My wound is severe, and my grief is great. My sickness is incurable, but I must bear it."
      }
    },
