    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为耶和华说： “看啊，这次我要把这地方的居民抛出去， 使他们苦不堪言。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為耶和華這樣說： “看哪！在這時候， 我要把這地的居民投擲出去； 又使他們受困苦， 以致他們被壓倒。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為耶和華如此說： 這時候，我必將此地的居民， 好像用機弦甩出去， 又必加害在他們身上， 使他們覺悟。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus says the Lord, “Behold, I am slinging out the inhabitants of the land At this time, And will cause them distress, That they may be found.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this is what the Lord says: “At this time I will hurl out those who live in this land; I will bring distress on them so that they may be captured.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For this is what the Lord says: “Suddenly, I will fling out all you who live in this land. I will pour great troubles upon you, and at last you will feel my anger.”"
      }
    },
