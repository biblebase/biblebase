    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华施展大能， 用智慧创造大地和世界， 巧妙地铺展穹苍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華用自己的能力創造了大地， 用自己的智慧堅立了世界， 又用自己的聰明展開了諸天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華用能力創造大地， 用智慧建立世界， 用聰明鋪張穹蒼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "It is He who made the earth by His power, Who established the world by His wisdom; And by His understanding He has stretched out the heavens."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But God made the earth by his power; he founded the world by his wisdom and stretched out the heavens by his understanding."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the Lord made the earth by his power, and he preserves it by his wisdom. With his own understanding he stretched out the heavens."
      }
