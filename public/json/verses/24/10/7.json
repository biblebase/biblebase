    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万国的王啊，谁不敬畏你？ 敬畏你是理所当然的。 因为万国的智者和君王中无人能与你相比。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬國的王啊！誰敢不敬畏你呢？ 這是你應得的。 因為在列國所有的智慧人中， 以及在他們的全國裡， 沒有可以跟你相比的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬國的王啊，誰不敬畏你？ 敬畏你本是合宜的； 因為在列國的智慧人中， 雖有政權的尊榮， 也不能比你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who would not fear You, O King of the nations? Indeed it is Your due! For among all the wise men of the nations And in all their kingdoms, There is none like You."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who should not fear you, King of the nations? This is your due. Among all the wise leaders of the nations and in all their kingdoms, there is no one like you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Who would not fear you, O King of nations? That title belongs to you alone! Among all the wise people of the earth and in all the kingdoms of the world, there is no one like you."
      }
    },
