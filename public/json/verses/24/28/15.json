    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.28.1-17",
            "paragraphs": [
              "二八1～17假先知哈拿尼雅質疑耶利米說猶大要被擄七十年的預言，並且自己預言聖殿的器皿和耶哥尼雅要在兩年之內返回耶路撒冷。耶利米確實希望情況會是這樣（6節），但再次預言事實並非如此（14節）。留意耶利米仍然帶他的\"視覺教具\"--頸上的軛（10節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米先知就对哈拿尼雅先知说：“哈拿尼雅，听着！耶和华并没有派你来，你却诱使这些民众相信你的谎话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米先知就對哈拿尼雅先知說：“哈拿尼雅啊！你要聽，耶和華並沒有差遣你，你竟使這人民信靠謊言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是先知耶利米對先知哈拿尼雅說：「哈拿尼雅啊，你應當聽！耶和華並沒有差遣你，你竟使這百姓倚靠謊言。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jeremiah the prophet said to Hananiah the prophet, “Listen now, Hananiah, the Lord has not sent you, and you have made this people trust in a lie."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then the prophet Jeremiah said to Hananiah the prophet, “Listen, Hananiah! The Lord has not sent you, yet you have persuaded this nation to trust in lies."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jeremiah the prophet said to Hananiah, “Listen, Hananiah! The Lord has not sent you, but the people believe your lies."
      }
    },
