          {
            "verse": "jer.33.3",
            "paragraphs": [
              "三三3\"又大又難\"。難得到，象一座堅固城一樣（民一三28）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘你呼求我，我就回应你，并且把你不知道的伟大、奥妙之事告诉你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘你呼求我，我就應允你，並且把你所不知道的那些偉大奧祕的事告訴你。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你求告我，我就應允你，並將你所不知道、又大又難的事指示你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Call to Me and I will answer you, and I will tell you great and mighty things, which you do not know.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘Call to me and I will answer you and tell you great and unsearchable things you do not know.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ask me and I will tell you remarkable secrets you do not know about things to come."
      }
    },
