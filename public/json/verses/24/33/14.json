    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.33.14-26",
            "paragraphs": [
              "三三14～26彌賽亞君王（15節）要從大衛王朝而出，統治千禧年的國度（參看二三5）。即將被尼布甲尼撒攻陷的耶路撒冷將得到復興，並且被稱為\"耶和華我們的義\"，因為它要由公義的王來統治（16節）。大衛之約（17節）以及與非尼哈所立的約（18節；參看民二五13的腳註）重新得到確認。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“看啊，时候将到，我要实现我给以色列人和犹大人的美好应许。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪！日子快到，我必實踐我向以色列家和猶大家應許賜福的諾言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「日子將到，我應許以色列家和猶大家的恩言必然成就。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘Behold, days are coming,’ declares the Lord, ‘when I will fulfill the good word which I have spoken concerning the house of Israel and the house of Judah."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘The days are coming,’ declares the Lord, ‘when I will fulfill the good promise I made to the people of Israel and Judah."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The day will come, says the Lord, when I will do for Israel and Judah all the good things I have promised them."
      }
    },
