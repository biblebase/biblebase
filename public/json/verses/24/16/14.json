    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“时候将到，那时人不再凭把以色列人带出埃及的耶和华起誓，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪！日子快到，人必不再指著那領以色列人從埃及地上來的永活的耶和華起誓。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「日子將到，人必不再指着那領以色列人從埃及地上來之永生的耶和華起誓，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Therefore, behold, the days are coming, declares the Lord, when it shall no longer be said, ‘As the Lord lives who brought up the people of Israel out of the land of Egypt,’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Therefore behold, days are coming,” declares the Lord, “when it will no longer be said, ‘As the Lord lives, who brought up the sons of Israel out of the land of Egypt,’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“However, the days are coming,” declares the Lord, “when it will no longer be said, ‘As surely as the Lord lives, who brought the Israelites up out of Egypt,’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Therefore behold, the days are coming,” says the Lord, “that it shall no more be said, ‘The Lord lives who brought up the children of Israel from the land of Egypt,’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But the time is coming,” says the Lord, “when people who are taking an oath will no longer say, ‘As surely as the Lord lives, who rescued the people of Israel from the land of Egypt.’"
      }
    },
