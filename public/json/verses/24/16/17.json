    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为我鉴察他们的一切行为，他们无法隐藏，他们的罪恶逃不过我的眼晴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“因為我的眼察看他們所行的一切；在我面前這一切都無法隱藏，他們的罪孽不能從我眼前遮掩起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因我的眼目察看他們的一切行為；他們不能在我面前遮掩，他們的罪孽也不能在我眼前隱藏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For My eyes are on all their ways; they are not hidden from My face, nor is their iniquity concealed from My eyes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My eyes are on all their ways; they are not hidden from me, nor is their sin concealed from my eyes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am watching them closely, and I see every sin. They cannot hope to hide from me."
      }
    },
