      "leili": {
        "content": [
          {
            "verse": "jer.16.1-9",
            "paragraphs": [
              "一六1～9耶利米被禁止結婚（2節），也不能參加喪禮（5節）和快樂的宴會（8節），以此為即將來臨之被擄的強烈警告。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你不可进入宴乐之家与他们同坐吃喝。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你也不可進入歡宴之家，跟他們同坐，一起吃喝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你不可進入宴樂的家，與他們同坐吃喝。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moreover you shall not go into a house of feasting to sit with them to eat and drink.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“And do not enter a house where there is feasting and sit down to eat and drink."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“And do not go to their feasts and parties. Do not eat and drink with them at all."
      }
    },
