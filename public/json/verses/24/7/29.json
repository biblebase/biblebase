    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘你们要剪掉头发， 在光秃的山上哀哭， 因为耶和华已弃绝这惹祂发怒的百姓。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“‘你要剪去頭髮，並且把它拋掉， 上到光禿的高岡舉哀， 因為耶和華擯斥、棄絕了 他所憎恨的世代。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷啊，要剪髮拋棄， 在淨光的高處舉哀； 因為耶和華丟掉離棄了惹他忿怒的世代。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Cut off your hair and cast it away, And take up a lamentation on the bare heights; For the Lord has rejected and forsaken The generation of His wrath.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘Cut off your hair and throw it away; take up a lament on the barren heights, for the Lord has rejected and abandoned this generation that is under his wrath."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Shave your head in mourning, and weep alone on the mountains. For the Lord has rejected and forsaken this generation that has provoked his fury.’"
      }
    },
