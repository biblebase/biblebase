    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "jer.7.5-7",
            "paragraphs": [
              "5～7.在百姓正式取得西乃之約中的恩惠之前，必須要有徹底的改變。社會上不公義的現象必須立予匡正，此外，此處還指出了公元前七世紀時那些更嚴重的罪行。在摩西律法中最根本的人道精神，長久以來竟被以色列社會忽視（參：申十四29，二十四19～21，等等）。無辜人的血，可能指的是由約雅敬下手加害的司法謀殺（二十六23）。但一切敗壞的根源是偶像崇拜，以及伴隨而至的錯誤價值觀。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“‘如果你们真正改过自新，彼此公平相待，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們實在改正自己的所作所為，人與人之間認真施行公正；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你們若實在改正行動作為，在人和鄰舍中間誠然施行公平，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“For if you truly amend your ways and your deeds, if you truly execute justice one with another,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For if you truly amend your ways and your deeds, if you truly practice justice between a man and his neighbor,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you really change your ways and your actions and deal with each other justly,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“For if you thoroughly amend your ways and your doings, if you thoroughly execute judgment between a man and his neighbor,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I will be merciful only if you stop your evil thoughts and deeds and start treating each other with justice;"
      }
    },
