    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.22.13-19",
            "paragraphs": [
              "二二13～19對沙龍之兄長和繼任人約雅敬的譴責。他強迫百姓建造精美華麗的宮殿（13,14節）。耶利米預言約雅敬死後，人們不會照例給他舉哀，他也不會被埋葬，而是被拖到城外，丟棄在垃圾堆中（列王紀下二十四章6節沒有提到\"葬在……\"是值得註意的）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "可是，你一心贪图不义之财， 滥杀无辜，横征暴敛。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“但你的眼你的心甚麼都不顧， 只顧你的不義之財， 只知流無辜人的血， 施行欺壓和迫害的事。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惟有你的眼和你的心專顧貪婪， 流無辜人的血， 行欺壓和強暴。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“But your eyes and your heart Are intent only upon your own dishonest gain, And on shedding innocent blood And on practicing oppression and extortion.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But your eyes and your heart are set only on dishonest gain, on shedding innocent blood and on oppression and extortion.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But you! You have eyes only for greed and dishonesty! You murder the innocent, oppress the poor, and reign ruthlessly.”"
      }
    },
