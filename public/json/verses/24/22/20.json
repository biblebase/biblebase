    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“耶路撒冷人啊， 你们要上黎巴嫩大声哭喊， 在巴珊哀号， 在亚巴琳痛哭， 因为你们的盟友被消灭了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“耶路撒冷啊，上黎巴嫩哀叫吧！ 在巴珊揚聲吧！ 從亞巴琳哀叫吧！ 因為你的盟友都毀滅了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要上黎巴嫩哀號， 在巴珊揚聲， 從亞巴琳哀號， 因為你所親愛的都毀滅了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Go up to Lebanon and cry out, And lift up your voice in Bashan; Cry out also from Abarim, For all your lovers have been crushed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Go up to Lebanon and cry out, let your voice be heard in Bashan, cry out from Abarim, for all your allies are crushed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Weep for your allies in Lebanon. Shout for them in Bashan. Search for them in the regions east of the river. See, they are all destroyed. Not one is left to help you."
      }
    },
