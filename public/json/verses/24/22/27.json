    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.22.24-30",
            "paragraphs": [
              "二二24～30這段描述約雅敬之子約雅斤的結局（這裏和在第三十七章1節稱為\"哥尼雅\"；也稱為\"耶哥尼雅\"。二四1；二七20）。他即位三個月之後，便被帶往巴比倫，後來再也沒有返國（王下二四8～15；二五27～30），盡管哈拿尼雅說了相反的假預言（耶二八4；比較五二31～34）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们渴望回到故乡，却不能如愿以偿。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們渴望歸回本國，卻不得回去。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但心中甚想歸回之地，必不得歸回。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But as for the land to which they desire to return, they will not return to it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will never come back to the land you long to return to.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will never again return to the land you yearn for."
      }
