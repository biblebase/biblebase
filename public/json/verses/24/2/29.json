    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "jer.2.28-29",
            "paragraphs": [
              "28～29.先知在此時刻語帶諷刺。他說：“你為自己所作的神，數目與你城的數目相等，當然可以叫他們救你這敬拜者脫離困境。”耶利米在此明白指出，當時猶太社會中有許多的神和許多的主（參：林前八5）。雖然百姓抱怨遭到不公平的待遇，但他們其實是罪有應得，因為他們對神的悖逆便是罪的本質。猶大既已被判決與以色列同罪，那麽她亦必須分擔以色列所受的處罰。神對西乃之約的忠實和永不改變，在耶利米書中不斷與選民背信、頑梗的行徑相互對比。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你们为什么指控我呢？ 你们都背叛了我。 这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們為甚麼跟我爭辯呢？ 你們都悖逆了我！” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說： 你們為何與我爭辯呢？ 你們都違背了我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Why do you contend with Me? You have all transgressed against Me,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Why do you bring charges against me? You have all rebelled against me,” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Why do you accuse me of doing wrong? You are the ones who have rebelled,” says the Lord."
      }
    },
