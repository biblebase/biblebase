    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "盖房、撒种、栽植葡萄或拥有葡萄园，一辈子都要住帐篷，以便在异乡长久居住。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們也不可建造房屋，不可撒種，不可栽種或擁有葡萄園；你們一生的日子只可住帳棚，使你們在寄居的地上活得長久。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "也不可蓋房、撒種、栽種葡萄園，但一生的年日要住帳棚，使你們的日子在寄居之地得以延長。』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You shall not build a house, and you shall not sow seed and you shall not plant a vineyard or own one; but in tents you shall dwell all your days, that you may live many days in the land where you sojourn.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Also you must never build houses, sow seed or plant vineyards; you must never have any of these things, but must always live in tents. Then you will live a long time in the land where you are nomads.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And do not build houses or plant crops or vineyards, but always live in tents. If you follow these commands, you will live long, good lives in the land.’"
      }
    },
