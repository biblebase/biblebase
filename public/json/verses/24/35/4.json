    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "领到耶和华的殿，进入上帝的仆人伊基大利之子哈难众儿子的房间。这房间靠近官长的房间，在殿门守卫沙龙的儿子玛西雅的房间上面。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "都領進耶和華的殿裡，進了神人伊基大利的兒子哈難眾子的房間；這房間是在眾領袖的房間旁邊，在沙龍的兒子守衛殿門的瑪西雅的房間上面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "領到耶和華的殿，進入神人伊基大利的兒子哈難眾子的屋子。那屋子在首領的屋子旁邊，在沙龍之子把門的瑪西雅屋子以上。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I brought them to the house of the Lord into the chamber of the sons of Hanan the son of Igdaliah, the man of God, which was near the chamber of the officials, above the chamber of Maaseiah the son of Shallum, keeper of the threshold."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and I brought them into the house of the Lord, into the chamber of the sons of Hanan the son of Igdaliah, the man of God, which was near the chamber of the officials, which was above the chamber of Maaseiah the son of Shallum, the doorkeeper."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I brought them into the house of the Lord, into the room of the sons of Hanan son of Igdaliah the man of God. It was next to the room of the officials, which was over that of Maaseiah son of Shallum the doorkeeper."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "and I brought them into the house of the Lord, into the chamber of the sons of Hanan the son of Igdaliah, a man of God, which was by the chamber of the princes, above the chamber of Maaseiah the son of Shallum, the keeper of the door."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I took them to the Temple, and we went into the room assigned to the sons of Hanan son of Igdaliah, a man of God. This room was located next to the one used by the Temple officials, directly above the room of Maaseiah son of Shallum, the Temple gatekeeper."
      }
    },
