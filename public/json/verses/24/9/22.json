    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“人们必尸横遍野， 像散布在田野上的粪便， 又像收割者身后没有收集的禾捆。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要這樣說： “人的屍體橫臥著， 像田野上的糞肥， 又像遺留在收割者後面的麥穗， 無人拾取。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你當說，耶和華如此說： 人的屍首必倒在田野像糞土， 又像收割的人遺落的一把禾稼， 無人收取。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Speak, “Thus says the Lord, ‘The corpses of men will fall like dung on the open field, And like the sheaf after the reaper, But no one will gather them.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Say, “This is what the Lord declares: “ ‘Dead bodies will lie like dung on the open field, like cut grain behind the reaper, with no one to gather them.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord says: “Bodies will be scattered across the fields like clumps of manure, like bundles of grain after the harvest. No one will be left to bury them.”"
      }
    },
