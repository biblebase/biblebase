    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "各城的人听到骑兵和弓箭手的呐喊， 都纷纷逃命。 有的跑入丛林，有的爬进岩洞。 各城荒弃，无人居住。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因著騎兵和弓箭手的響聲， 各城的人盡都逃跑。 他們進入密林，爬上巖穴； 各城都被撇下， 無人住在其中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "各城的人因馬兵和弓箭手的響聲就都逃跑， 進入密林，爬上磐石； 各城被撇下， 無人住在其中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "At the sound of the horseman and bowman every city flees; They go into the thickets and climb among the rocks; Every city is forsaken, And no man dwells in them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At the sound of horsemen and archers every town takes to flight. Some go into the thickets; some climb up among the rocks. All the towns are deserted; no one lives in them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At the noise of charioteers and archers, the people flee in terror. They hide in the bushes and run for the mountains. All the towns have been abandoned— not a person remains!"
      }
    },
