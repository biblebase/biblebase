    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.43.8-13",
            "paragraphs": [
              "四三8～13另一次的實物教導：把石頭藏在尼布甲尼撒入侵埃及時（主前568～567年）將會統治的地點。"
            ]
          },
          {
            "verse": "jer.43.11",
            "paragraphs": [
              "四三11尼布甲尼撒在主前568至567年確實入侵了埃及。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他要前来攻打埃及，叫那些该死的死、该被掳的被掳、该被杀的被杀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他必來攻打埃及地；那些注定死亡的，必要死亡；那些注定被擄的，必要被擄；那些注定被刀殺的，必被刀殺。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他要來攻擊埃及地，定為死亡的，必致死亡；定為擄掠的，必被擄掠；定為刀殺的，必交刀殺。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He will also come and strike the land of Egypt; those who are meant for death will be given over to death, and those for captivity to captivity, and those for the sword to the sword."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He will come and attack Egypt, bringing death to those destined for death, captivity to those destined for captivity, and the sword to those destined for the sword."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And when he comes, he will destroy the land of Egypt. He will bring death to those destined for death, captivity to those destined for captivity, and war to those destined for war."
      }
    },
