    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘它们必被掳到巴比伦，一直留在那里，直到有一天我眷顾以色列人，把它们带回原处。这是耶和华说的。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘這些器皿必被送到巴比倫，存在那裡，直到我眷顧這些器皿的日子。那時，我必把它們帶上來，放回這地方。’”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "必被帶到巴比倫存在那裏，直到我眷顧以色列人的日子。那時，我必將這器皿帶回來，交還此地。這是耶和華說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘They will be carried to Babylon and they will be there until the day I visit them,’ declares the Lord. ‘Then I will bring them back and restore them to this place.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘They will be taken to Babylon and there they will remain until the day I come for them,’ declares the Lord. ‘Then I will bring them back and restore them to this place.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "‘They will all be carried away to Babylon and will stay there until I send for them,’ says the Lord. ‘Then I will bring them back to Jerusalem again.’”"
      }
    },
