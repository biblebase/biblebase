    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们不可听信他们的话。你们要臣服于巴比伦王，以便存活。何必使这城沦为废墟呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們不要聽從他們，只要服事巴比倫王，就可以生存。為甚麼要使這城變為廢墟呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可聽從他們，只管服事巴比倫王便得存活。這城何致變為荒場呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not listen to them; serve the king of Babylon, and live! Why should this city become a ruin?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not listen to them. Serve the king of Babylon, and you will live. Why should this city become a ruin?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not listen to them. Surrender to the king of Babylon, and you will live. Why should this whole city be destroyed?"
      }
    },
