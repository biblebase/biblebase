    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.27.12-15",
            "paragraphs": [
              "二七12～15耶利米警告西底家不要背叛，並且不要相信假先知的話。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要听信那些先知骗人的预言，他们说巴比伦王不会征服你们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要聽從那些對你們說‘你們必不會服事巴比倫王’的先知的話，因為他們向你們說的是虛假的預言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可聽那些先知對你們所說的話；他們說：『你們不致服事巴比倫王』，其實他們向你們說假預言。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So do not listen to the words of the prophets who speak to you, saying, ‘You will not serve the king of Babylon,’ for they prophesy a lie to you;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not listen to the words of the prophets who say to you, ‘You will not serve the king of Babylon,’ for they are prophesying lies to you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do not listen to the false prophets who keep telling you, ‘The king of Babylon will not conquer you.’ They are liars."
      }
    },
