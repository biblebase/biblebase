    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的预言是骗人的，只会使你们远离家园。我会赶走你们，使你们灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們向你們說虛假的預言，使你們遠離你們的土地；我要趕逐你們，你們必要滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們向你們說假預言，要叫你們遷移，遠離本地，以致我將你們趕出去，使你們滅亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For they prophesy a lie to you in order to remove you far from your land; and I will drive you out and you will perish."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They prophesy lies to you that will only serve to remove you far from your lands; I will banish you and you will perish."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They are all liars, and their lies will lead to your being driven out of your land. I will drive you out and send you far away to die."
      }
