    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為萬軍之耶和華這樣說，論到這些柱子、銅海、盆座和留在這城其餘的器皿，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為萬軍之耶和華論到柱子、銅海、盆座，並剩在這城裏的器皿，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For thus says the Lord of hosts concerning the pillars, concerning the sea, concerning the stands and concerning the rest of the vessels that are left in this city,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For this is what the Lord Almighty says about the pillars, the bronze Sea, the movable stands and the other articles that are left in this city,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“For the Lord of Heaven’s Armies has spoken about the pillars in front of the Temple, the great bronze basin called the Sea, the water carts, and all the other ceremonial articles."
      }
    },
