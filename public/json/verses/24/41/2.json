    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.41.2",
            "paragraphs": [
              "41:2",
              "用刀殺了……基大利：以實瑪利是一個極端反對巴比倫的人。他殺害了尼布甲尼撒王所設立的省長基大利。以實瑪利可能以為基大利曾經積極支持巴比倫人殘酷惡待西底家王(39:6~7)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尼探雅的儿子以实玛利和同来的十个人突然起来拔刀杀了巴比伦王委任管理犹大的沙番的孙子、亚希甘的儿子基大利，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "尼探雅的兒子以實瑪利和與他一起的那十個人就起來，用刀襲擊沙番的孫子、亞希甘的兒子基大利，就是巴比倫王指派管理那地的，把他殺掉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "尼探雅的兒子以實瑪利和同他來的那十個人起來，用刀殺了沙番的孫子亞希甘的兒子基大利，就是巴比倫王所立為全地省長的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Ishmael the son of Nethaniah and the ten men who were with him arose and struck down Gedaliah the son of Ahikam, the son of Shaphan, with the sword and put to death the one whom the king of Babylon had appointed over the land."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ishmael son of Nethaniah and the ten men who were with him got up and struck down Gedaliah son of Ahikam, the son of Shaphan, with the sword, killing the one whom the king of Babylon had appointed as governor over the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ishmael and his ten men suddenly jumped up, drew their swords, and killed Gedaliah, whom the king of Babylon had appointed governor."
      }
    },
