    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.38.5",
            "paragraphs": [
              "38:5",
              "他在你們手中：西底家王因害怕軍中首領們的權勢和影響，而將耶利米交給他們處置。在他執政的整個過程中，他始終都顯得唯唯諾諾，缺乏正義感。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "西底迦王说：“你们可以随意处置他，我不会阻止你们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "西底家王說：“看哪！他在你們手中；王不能作甚麼反對你們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "西底家王說：「他在你們手中，無論何事，王也不能與你們反對。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So King Zedekiah said, “Behold, he is in your hands; for the king can do nothing against you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“He is in your hands,” King Zedekiah answered. “The king can do nothing to oppose you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King Zedekiah agreed. “All right,” he said. “Do as you like. I can’t stop you.”"
      }
    },
