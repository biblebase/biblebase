    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.31.1",
            "paragraphs": [
              "31:1",
              "1~40那時：本章經文籠統地論述了以色列的歸回(2~22節)，其中特別論到猶大的歸回(23~26節)。神子民將共同生活在他的美福之中(27~30節)。耶利米指出，神把他的子民招聚在一起之後，他要與他的子民另立更美的新約，賜給他們屬靈的能力去遵守他的命令。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.31.1",
            "paragraphs": [
              "三一1本章繼續以色列得復興的主題。第1至22節關乎北國，第23至26節關乎南國，而第27至40節關乎南、北兩國。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“那时，我要做以色列各宗族的上帝，他们必做我的子民。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“到那時，我必作以色列各家族的　神，他們要作我的子民。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「那時，我必作以色列各家的神；他們必作我的子民。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“At that time,” declares the Lord, “I will be the God of all the families of Israel, and they shall be My people.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“At that time,” declares the Lord, “I will be the God of all the families of Israel, and they will be my people.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“In that day,” says the Lord, “I will be the God of all the families of Israel, and they will be my people."
      }
