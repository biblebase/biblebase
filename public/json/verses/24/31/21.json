    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.31.21",
            "paragraphs": [
              "三一21\"引路柱\"。在被擄到巴比倫的路上設立路標，能幫助他們記得返回的路。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“以色列人啊， 你们要为自己设置路标， 竖起路牌， 牢记你们走过的大道。 以色列人啊， 回来吧！回到你们的城邑吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要為自己設立路標， 豎立路牌； 你要留心大路， 就是你走過的路。 童女以色列啊，回轉吧！ 回到你這些城市那裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列民哪， 你當為自己設立指路碑， 豎起引路柱。 你要留心向大路， 就是你所去的原路； 你當回轉，回轉到你這些城邑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Set up for yourself roadmarks, Place for yourself guideposts; Direct your mind to the highway, The way by which you went. Return, O virgin of Israel, Return to these your cities."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Set up road signs; put up guideposts. Take note of the highway, the road that you take. Return, Virgin Israel, return to your towns."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Set up road signs; put up guideposts. Mark well the path by which you came. Come back again, my virgin Israel; return to your towns here."
      }
    },
