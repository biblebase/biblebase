    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.31.35-37",
            "paragraphs": [
              "三一35～37大自然周期的持久（35,36節）和天與地的無法測量（37節），保證希伯來民族得以存留。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "使阳光白天照耀、 星月晚上发光、 大海波涛汹涌的万军之耶和华说："
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那使太陽白晝發光， 派定月亮和星辰照耀黑夜， 又攪動海洋，使海浪澎湃的， 萬軍之耶和華是他的名， 他這樣說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那使太陽白日發光， 使星月有定例，黑夜發亮， 又攪動大海，使海中波浪匉訇的， 萬軍之耶和華是他的名。 他如此說："
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord, Who gives the sun for light by day And the fixed order of the moon and the stars for light by night, Who stirs up the sea so that its waves roar; The Lord of hosts is His name:"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
