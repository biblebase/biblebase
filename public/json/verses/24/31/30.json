    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.31.29-30",
            "paragraphs": [
              "三一29,30百姓為自己辯解說他們是因祖先的罪而受罰；事實上，每個人都是為自己的罪而受罰（比較申二四16；結一八2～4）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为各人都要承担自己的罪，谁吃酸葡萄，谁的牙酸倒。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為各人必因自己的罪孽死亡；吃酸葡萄的，那人的牙齒必酸倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但各人必因自己的罪死亡；凡吃酸葡萄的，自己的牙必酸倒。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But everyone will die for his own iniquity; each man who eats the sour grapes, his teeth will be set on edge."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Instead, everyone will die for their own sin; whoever eats sour grapes—their own teeth will be set on edge."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All people will die for their own sins—those who eat the sour grapes will be the ones whose mouths will pucker."
      }
    },
