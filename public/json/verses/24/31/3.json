    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.31.3",
            "paragraphs": [
              "三一3\"慈愛\"。希伯來文是hesed（比較二2；九24）。參看何西阿書二章19節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从前，耶和华向以色列显现，说： “我以永远不变的爱爱你， 我以慈爱吸引你到我身边。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華在古時曾向以色列顯現，說： “我以永遠的愛愛你， 因此，我對你的慈愛延續不息。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "古時耶和華向以色列顯現，說： 我以永遠的愛愛你， 因此我以慈愛吸引你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord appeared to him from afar, saying, “I have loved you with an everlasting love; Therefore I have drawn you with lovingkindness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord appeared to us in the past, saying: “I have loved you with an everlasting love; I have drawn you with unfailing kindness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Long ago the Lord said to Israel: “I have loved you, my people, with an everlasting love. With unfailing love I have drawn you to myself."
      }
    },
