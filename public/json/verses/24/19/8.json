    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必摧毁这城，使它令人惊惧、嗤笑，它的满目疮痍必令路人惊惧、嗤笑。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要使這城變為荒涼，成為被人嗤笑的對象；每一個經過這裡的，都必因這城所遭受的一切創傷而驚駭，並且嗤笑她。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我必使這城令人驚駭嗤笑；凡經過的人，必因這城所遭的災驚駭嗤笑。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will also make this city a desolation and an object of hissing; everyone who passes by it will be astonished and hiss because of all its disasters."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will devastate this city and make it an object of horror and scorn; all who pass by will be appalled and will scoff because of all its wounds."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will reduce Jerusalem to ruins, making it a monument to their stupidity. All who pass by will be astonished and will gasp at the destruction they see there."
      }
    },
