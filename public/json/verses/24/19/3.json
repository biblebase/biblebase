    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你要说，‘犹大王和耶路撒冷的居民啊，你们要听耶和华的话！以色列的上帝——万军之耶和华说，看啊，我要在这里降下灾祸，听见这消息的人都必耳鸣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要說：‘猶大列王和耶路撒冷的居民哪！你們要聽耶和華的話。萬軍之耶和華以色列的　神這樣說：看哪！我必使災禍臨到這地方；凡聽見的，都必耳鳴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：『猶大君王和耶路撒冷的居民哪，當聽耶和華的話。萬軍之耶和華－以色列的神如此說：我必使災禍臨到這地方，凡聽見的人都必耳鳴；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and say, ‘Hear the word of the Lord, O kings of Judah and inhabitants of Jerusalem: thus says the Lord of hosts, the God of Israel, “Behold I am about to bring a calamity upon this place, at which the ears of everyone that hears of it will tingle."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and say, ‘Hear the word of the Lord, you kings of Judah and people of Jerusalem. This is what the Lord Almighty, the God of Israel, says: Listen! I am going to bring a disaster on this place that will make the ears of everyone who hears of it tingle."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Say to them, ‘Listen to this message from the Lord, you kings of Judah and citizens of Jerusalem! This is what the Lord of Heaven’s Armies, the God of Israel, says: I will bring a terrible disaster on this place, and the ears of those who hear about it will ring!"
      }
    },
