      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但亚希甘的儿子基大利说：“你不可杀他，你说他要杀我这件事不是真的。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但亞希甘的兒子基大利對加利亞的兒子約哈難說：“你不可作這事，因為你所說關於以實瑪利的事都是謊話！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞希甘的兒子基大利對加利亞的兒子約哈難說：「你不可行這事，你所論以實瑪利的話是假的。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But Gedaliah the son of Ahikam said to Johanan the son of Kareah, “You shall not do this thing, for you are speaking falsely of Ishmael.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Gedaliah the son of Ahikam said to Johanan the son of Kareah, “Do not do this thing, for you are telling a lie about Ishmael.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Gedaliah son of Ahikam said to Johanan son of Kareah, “Don’t do such a thing! What you are saying about Ishmael is not true.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "But Gedaliah the son of Ahikam said to Johanan the son of Kareah, “You shall not do this thing, for you speak falsely concerning Ishmael.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Gedaliah said to Johanan, “I forbid you to do any such thing, for you are lying about Ishmael.”"
      }
    },
