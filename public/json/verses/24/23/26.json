    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.23.18-40",
            "paragraphs": [
              "二三18～40這段指出真先知的生命（18節）與信息（28節）跟假先知（32節）有所不同。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这些先知谎话连篇，凭空预言要到何时呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那些先知心裡存著這樣的意念要到幾時呢？他們說虛假的預言，說出自己心中的詭詐；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說假預言的先知，就是預言本心詭詐的先知，他們這樣存心要到幾時呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How long? Is there anything in the hearts of the prophets who prophesy falsehood, even these prophets of the deception of their own heart,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How long will this continue in the hearts of these lying prophets, who prophesy the delusions of their own minds?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "How long will this go on? If they are prophets, they are prophets of deceit, inventing everything they say."
      }
    },
