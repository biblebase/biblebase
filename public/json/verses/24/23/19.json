    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.23.18-40",
            "paragraphs": [
              "二三18～40這段指出真先知的生命（18節）與信息（28節）跟假先知（32節）有所不同。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，耶和华的怒气像暴风一样袭来， 像旋风一样吹到恶人头上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！耶和華的旋風， 在震怒中發出， 是旋轉的狂風， 必捲到惡人的頭上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看哪！耶和華的忿怒 好像暴風，已經發出； 是暴烈的旋風， 必轉到惡人的頭上。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Behold, the storm of the Lord! Wrath has gone forth, a whirling tempest; it will burst upon the head of the wicked."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, the storm of the Lord has gone forth in wrath, Even a whirling tempest; It will swirl down on the head of the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "See, the storm of the Lord will burst out in wrath, a whirlwind swirling down on the heads of the wicked."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Behold, a whirlwind of the Lord has gone forth in fury— A violent whirlwind! It will fall violently on the head of the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Look! The Lord’s anger bursts out like a storm, a whirlwind that swirls down on the heads of the wicked."
      }
    },
