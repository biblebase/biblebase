    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.23.6",
            "paragraphs": [
              "二三6基督再來的時候，以色列和猶大將要得救（羅一一26）、聯合，並且在應許給亞伯拉罕的地上安然居住（創一五18～21）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在他掌权的日子，犹大必得拯救，以色列必国泰民安。他将被称为‘耶和华是我们的公义’。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在他執政的日子，猶大必得救， 以色列也必安然居住。 人要稱他的名字為‘耶和華我們的義’。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在他的日子，猶大必得救，以色列也安然居住。他的名必稱為『耶和華－我們的義』。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "In his days Judah will be saved, and Israel will dwell securely. And this is the name by which he will be called: ‘The Lord is our righteousness.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In His days Judah will be saved, And Israel will dwell securely; And this is His name by which He will be called, ‘The Lord our righteousness.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In his days Judah will be saved and Israel will live in safety. This is the name by which he will be called: The Lord Our Righteous Savior."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "In His days Judah will be saved, And Israel will dwell safely; Now this is His name by which He will be called: THE LORD OUR RIGHTEOUSNESS."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And this will be his name: ‘The Lord Is Our Righteousness.’ In that day Judah will be saved, and Israel will live in safety."
      }
    },
