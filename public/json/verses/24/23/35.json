    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.23.18-40",
            "paragraphs": [
              "二三18～40這段指出真先知的生命（18節）與信息（28節）跟假先知（32節）有所不同。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们各人要问自己的亲友，‘耶和华有什么答复？’或‘耶和华说了什么？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們各人對自己的鄰舍，對自己的兄弟要這樣說：“耶和華回答了甚麼？耶和華說了甚麼？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們各人要對鄰舍，各人要對弟兄如此說：『耶和華回答甚麼？』『耶和華說了甚麼呢？』"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus will each of you say to his neighbor and to his brother, ‘What has the Lord answered?’ or, ‘What has the Lord spoken?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what each of you keeps saying to your friends and other Israelites: ‘What is the Lord’s answer?’ or ‘What has the Lord spoken?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You should keep asking each other, ‘What is the Lord’s answer?’ or ‘What is the Lord saying?’"
      }
    },
