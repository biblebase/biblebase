    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们彼此传讲所做的梦，想借此使我的子民忘记我，正如他们的祖先因祭拜巴力而忘记了我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們以為藉著互相傳述自己的夢，就可以使我的子民忘記我的名，好像他們的列祖因巴力忘記了我的名一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們各人將所做的夢對鄰舍述說，想要使我的百姓忘記我的名，正如他們列祖因巴力忘記我的名一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "who intend to make My people forget My name by their dreams which they relate to one another, just as their fathers forgot My name because of Baal?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They think the dreams they tell one another will make my people forget my name, just as their ancestors forgot my name through Baal worship."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "By telling these false dreams, they are trying to get my people to forget me, just as their ancestors did by worshiping the idols of Baal."
      }
    },
