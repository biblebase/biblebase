    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我看见撒玛利亚的先知行为可憎， 他们靠巴力说预言， 引诱我的子民走入歧途。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“在撒瑪利亞的先知中， 我看見可厭的事： 他們竟奉巴力的名說預言， 使我的子民以色列走錯了路。"
      },
