    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“看啊，时候将到，那时人不再凭把以色列人带出埃及的耶和华起誓，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“因此，看哪！日子快到，人不再指著那領以色列人從埃及地上來的永活的耶和華起誓，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「日子將到，人必不再指着那領以色列人從埃及地上來永生的耶和華起誓，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Therefore behold, the days are coming,” declares the Lord, “when they will no longer say, ‘As the Lord lives, who brought up the sons of Israel from the land of Egypt,’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“So then, the days are coming,” declares the Lord, “when people will no longer say, ‘As surely as the Lord lives, who brought the Israelites up out of Egypt,’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“In that day,” says the Lord, “when people are taking an oath, they will no longer say, ‘As surely as the Lord lives, who rescued the people of Israel from the land of Egypt.’"
      }
    },
