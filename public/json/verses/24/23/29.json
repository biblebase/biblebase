    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.23.18-40",
            "paragraphs": [
              "二三18～40這段指出真先知的生命（18節）與信息（28節）跟假先知（32節）有所不同。"
            ]
          },
          {
            "verse": "jer.23.28-29",
            "paragraphs": [
              "二三28,29神的話好象\"麥子\"（滋養人；彼前二2）、\"火\"（煉凈人；林前三13）和\"大錘\"（透徹分解，解明其信息；來四12）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“难道我的话不像火吗？不像击碎岩石的铁锤吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我的話不是像火，不是像能打碎巖石的大鎚嗎？”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「我的話豈不像火，又像能打碎磐石的大錘嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Is not My word like fire?” declares the Lord, “and like a hammer which shatters a rock?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Is not my word like fire,” declares the Lord, “and like a hammer that breaks a rock in pieces?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Does not my word burn like fire?” says the Lord. “Is it not like a mighty hammer that smashes a rock to pieces?"
      }
    },
