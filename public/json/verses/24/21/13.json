    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.21.13",
            "paragraphs": [
              "二一13耶路撒冷認為自己是固若金湯，無法攻取的。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说，耶路撒冷啊， 你坐落在山谷之上， 如平原的磐石， 自以为无人能攻击你， 无人能闯入你的住处。 但我要攻击你，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！在山谷上、在巖石平原上的耶路撒冷啊！ 我要攻擊你。’ 這是耶和華的宣告。 ‘你們說：誰能下來攻擊我們， 誰能進入我們的住處呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：住山谷和平原磐石上的居民， 你們說：誰能下來攻擊我們？ 誰能進入我們的住處呢？ 看哪，我與你們為敵。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Behold, I am against you, O valley dweller, O rocky plain,” declares the Lord, “You men who say, ‘Who will come down against us? Or who will enter into our habitations?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am against you, Jerusalem, you who live above this valley on the rocky plateau, declares the Lord— you who say, “Who can come against us? Who can enter our refuge?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will personally fight against the people in Jerusalem, that mighty fortress— the people who boast, “No one can touch us here. No one can break in here.”"
      }
    },
