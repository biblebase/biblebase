    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.21.11",
            "paragraphs": [
              "21:11",
              "11~14猶大王的家：耶利米預告猶大王說，神原本期望他們在民眾當中施行公平，但是他們卻慫恿罪惡，不為受壓的人伸張正義；因此神的忿怒將如火一般燒滅他們。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你要对犹大的王室说，‘你们要听耶和华的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要對猶大王室說： ‘你們要聽耶和華的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「至於猶大王的家，你們當聽耶和華的話。"
      },
      "nasb": {
