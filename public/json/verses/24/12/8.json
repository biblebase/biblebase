    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的子民像林中的狮子一样向我吼叫， 因此我憎恶他们；"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的產業對我， 就像樹林中的獅子向我咆哮， 因此我恨惡她。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的產業向我如林中的獅子， 她發聲攻擊我， 因此我恨惡她。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My inheritance has become to Me Like a lion in the forest; She has roared against Me; Therefore I have come to hate her."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My inheritance has become to me like a lion in the forest. She roars at me; therefore I hate her."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My chosen people have roared at me like a lion of the forest, so I have treated them with contempt."
      }
