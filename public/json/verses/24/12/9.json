      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的子民就像一只带斑点的鸷鸟， 被其他鸷鸟围攻。 招聚野兽来吞吃它吧！"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的產業對我，不就像一隻帶斑點的鷙鳥， 其他的鷙鳥都四圍攻擊她？ 你們去聚集田野各樣的走獸， 帶牠們來吃吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的產業向我豈如斑點的鷙鳥呢？ 鷙鳥豈在她四圍攻擊她呢？ 你們要去聚集田野的百獸， 帶來吞吃吧！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Is My inheritance like a speckled bird of prey to Me? Are the birds of prey against her on every side? Go, gather all the beasts of the field, Bring them to devour!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Has not my inheritance become to me like a speckled bird of prey that other birds of prey surround and attack? Go and gather all the wild beasts; bring them to devour."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My chosen people act like speckled vultures, but they themselves are surrounded by vultures. Bring on the wild animals to pick their corpses clean!"
      }
    },
