    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.52.1-34",
            "paragraphs": [
              "五二1～34本章在二十五年後由另一個人所寫，這個歷史附錄顯示耶利米的預言怎樣應驗，而當中的記載跟列王紀下二十四章18節至二十五章30節幾乎完全相同。"
            ]
          },
          {
            "verse": "jer.52.31-34",
            "paragraphs": [
              "五二31～34\"以未米羅達\"。尼布甲尼撒的兒子，從主前562年至560年執政。在巴比倫伊斯他爾門遺址發現的泥版，證實約雅斤受到恩待的這個記載。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "并好言相待，使他的地位高过被掳到巴比伦的其他各王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "並且安慰他，使他的地位高過和他一起在巴比倫的眾王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又對他說恩言，使他的位高過與他一同在巴比倫眾王的位，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then he spoke kindly to him and set his throne above the thrones of the kings who were with him in Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He spoke kindly to him and gave him a seat of honor higher than those of the other kings who were with him in Babylon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He spoke kindly to Jehoiachin and gave him a higher place than all the other exiled kings in Babylon."
      }
    },
