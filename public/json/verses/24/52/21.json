    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.52.1-34",
            "paragraphs": [
              "五二1～34本章在二十五年後由另一個人所寫，這個歷史附錄顯示耶利米的預言怎樣應驗，而當中的記載跟列王紀下二十四章18節至二十五章30節幾乎完全相同。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "两根铜柱中空，高八米，周长五点三米，铜壁厚四指。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "至於那些柱子，每根高八公尺，圓周是五公尺三公分，柱子是空心的。銅的厚度有七十五公釐。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這一根柱子高十八肘，厚四指，是空的，圍十二肘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As for the pillars, the height of each pillar was eighteen cubits, and it was twelve cubits in circumference and four fingers in thickness, and hollow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Each pillar was eighteen cubits high and twelve cubits in circumference; each was four fingers thick, and hollow."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each of the pillars was 27 feet tall and 18 feet in circumference. They were hollow, with walls 3 inches thick."
      }
    },
