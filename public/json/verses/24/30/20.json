    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们的儿女要如往日一样兴旺， 我要使他们的国家坚立， 我要惩罚所有压迫他们的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的子孫必像以前一樣蒙恩， 他們的國家必在我面前堅立； 我必懲罰所有壓迫他們的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的兒女要如往日； 他們的會眾堅立在我面前； 凡欺壓他們的，我必刑罰他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their children also will be as formerly, And their congregation shall be established before Me; And I will punish all their oppressors."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their children will be as in days of old, and their community will be established before me; I will punish all who oppress them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Their children will prosper as they did long ago. I will establish them as a nation before me, and I will punish anyone who hurts them."
      }
