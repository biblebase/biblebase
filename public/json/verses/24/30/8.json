    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华说：“那日，我要折断他们颈上的轭，砍断他们的锁链，使他们不再受外族人奴役。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“萬軍之耶和華宣告說：‘當那日，我必從他們的頸項上折斷仇敵的軛，扯斷他們的繩套；他們必不再作外族人的奴隸。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華說：「到那日，我必從你頸項上折斷仇敵的軛，扭開他的繩索；外邦人不得再使你作他們的奴僕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "‘It shall come about on that day,’ declares the Lord of hosts, ‘that I will break his yoke from off their neck and will tear off their bonds; and strangers will no longer make them their slaves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“ ‘In that day,’ declares the Lord Almighty, ‘I will break the yoke off their necks and will tear off their bonds; no longer will foreigners enslave them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For in that day,” says the Lord of Heaven’s Armies, “I will break the yoke from their necks and snap their chains. Foreigners will no longer be their masters."
      }
