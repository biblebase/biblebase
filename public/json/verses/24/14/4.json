    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.14.1-6",
            "paragraphs": [
              "一四1～6一次影響城中居民（2,3節）、農夫（4節），甚至動物（5,6節）的\"幹旱\"（旱災），是從神而來的警告，但卻沒有人註意。\"家僮\"。奴仆。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "天不下雨， 土地干裂， 农夫沮丧地抱着头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "土地乾裂， 因為地上沒有雨水； 農夫失望， 都蒙著自己的頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耕地的也蒙羞抱頭； 因為無雨降在地上， 地都乾裂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Because the ground is cracked, For there has been no rain on the land; The farmers have been put to shame, They have covered their heads."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The ground is cracked because there is no rain in the land; the farmers are dismayed and cover their heads."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
