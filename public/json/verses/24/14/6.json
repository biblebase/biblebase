    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.14.1-6",
            "paragraphs": [
              "一四1～6一次影響城中居民（2,3節）、農夫（4節），甚至動物（5,6節）的\"幹旱\"（旱災），是從神而來的警告，但卻沒有人註意。\"家僮\"。奴仆。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为没有草， 野驴站在光秃的山头上像豺狼一样喘气，双目无神。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "野驢站在光禿的高岡上， 像野狗喘著氣； 牠們雙目發呆， 因為沒有青草。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "野驢站在淨光的高處，喘氣好像野狗； 因為無草，眼目失明。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wild donkeys stand on the bare heights; They pant for air like jackals, Their eyes fail For there is no vegetation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Wild donkeys stand on the barren heights and pant like jackals; their eyes fail for lack of food.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wild donkeys stand on the bare hills panting like thirsty jackals. They strain their eyes looking for grass, but there is none to be found.”"
      }
    },
