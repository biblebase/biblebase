    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.32.37",
            "paragraphs": [
              "32:37",
              "37~44必從那裏將他們招聚出來：神再次告訴耶利米，民眾將歸回此地，與他建立正確的關系(38節)。此後，神將與他們立定永遠的約(參31:31~34註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "jer.32.26-44",
            "paragraphs": [
              "三二26～44神重新向祂的先知作出保證，被擄歸回之後，祂要使子民在地上再次興旺起來。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "‘看啊，我极其愤怒地把他们驱散到各地，我也必招聚他们，领他们回到故土，使他们安居乐业。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘看哪！我曾在怒氣、烈怒和大忿怒中，把以色列人趕逐到各地，將來我必從那裡把他們招聚出來，領他們回到這地方，使他們安然居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我在怒氣、忿怒，和大惱恨中，將以色列人趕到各國。日後我必從那裏將他們招聚出來，領他們回到此地，使他們安然居住。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Behold, I will gather them from all the countries to which I drove them in my anger and my wrath and in great indignation. I will bring them back to this place, and I will make them dwell in safety."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I will gather them out of all the lands to which I have driven them in My anger, in My wrath and in great indignation; and I will bring them back to this place and make them dwell in safety."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will surely gather them from all the lands where I banish them in my furious anger and great wrath; I will bring them back to this place and let them live in safety."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Behold, I will gather them out of all countries where I have driven them in My anger, in My fury, and in great wrath; I will bring them back to this place, and I will cause them to dwell safely."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will certainly bring my people back again from all the countries where I will scatter them in my fury. I will bring them back to this very city and let them live in peace and safety."
      }
    },
