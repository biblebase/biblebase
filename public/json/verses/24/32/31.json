    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.32.26-44",
            "paragraphs": [
              "三二26～44神重新向祂的先知作出保證，被擄歸回之後，祂要使子民在地上再次興旺起來。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这城自从建立以来一直惹我发怒，以致我不得不毁灭它。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘因為這城自從建造的那天，直到今日，都不斷惹起我的怒氣和烈怒，以致我要把她從我面前除掉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這城自從建造的那日直到今日，常惹我的怒氣和忿怒，使我將這城從我面前除掉；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "This city has aroused my anger and wrath, from the day it was built to this day, so that I will remove it from my sight"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Indeed this city has been to Me a provocation of My anger and My wrath from the day that they built it, even to this day, so that it should be removed from before My face,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the day it was built until now, this city has so aroused my anger and wrath that I must remove it from my sight."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "‘For this city has been to Me a  provocation  of My anger and My fury from the day that they built it, even to this day; so I will remove it from before My face"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“From the time this city was built until now, it has done nothing but anger me, so I am determined to get rid of it."
      }
    },
