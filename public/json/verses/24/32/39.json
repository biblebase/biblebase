    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.32.26-44",
            "paragraphs": [
              "三二26～44神重新向祂的先知作出保證，被擄歸回之後，祂要使子民在地上再次興旺起來。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使他们永远全心全意地敬畏我，使他们和子孙得到福乐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要使他們有同一的心志和同一的行為，終生敬畏我，使他們和他們的子孫都得到福樂。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我要使他們彼此同心同道，好叫他們永遠敬畏我，使他們和他們後世的子孫得福樂，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and I will give them one heart and one way, that they may fear Me always, for their own good and for the good of their children after them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will give them singleness of heart and action, so that they will always fear me and that all will then go well for them and for their children after them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And I will give them one heart and one purpose: to worship me forever, for their own good and for the good of all their descendants."
      }
    },
