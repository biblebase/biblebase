    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.17.14-18",
            "paragraphs": [
              "一七14～18耶利米作出有理的請求，求神為他辯解。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿你使那些迫害我的人蒙羞， 不要使我蒙羞； 愿你使他们惊慌， 不要使我惊慌。 求你使他们大祸临头， 彻底毁灭他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願那些迫害我的蒙羞，卻不要叫我蒙羞； 願他們驚恐，卻不要使我驚恐； 求你使災禍的日子臨到他們，加倍毀滅他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願那些逼迫我的蒙羞， 卻不要使我蒙羞； 使他們驚惶， 卻不要使我驚惶； 使災禍的日子臨到他們， 以加倍的毀壞毀壞他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let those who persecute me be put to shame, but as for me, let me not be put to shame; Let them be dismayed, but let me not be dismayed. Bring on them a day of disaster, And crush them with twofold destruction!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let my persecutors be put to shame, but keep me from shame; let them be terrified, but keep me from terror. Bring on them the day of disaster; destroy them with double destruction."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Bring shame and dismay on all who persecute me, but don’t let me experience shame and dismay. Bring a day of terror on them. Yes, bring double destruction upon them!"
      }
    },
