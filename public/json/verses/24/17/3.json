    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.17.3",
            "paragraphs": [
              "一七3\"我田野的山哪\"。以詩的表達法來提到耶路撒冷（賽二2）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "山野间的丘坛和亚舍拉神像。因为他们境内的罪恶，我必使敌人掳掠他们的财宝和神庙。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我在田野的山哪！ 因你在四境之內所犯的罪， 我必把你的財產、一切寶物和你的邱壇， 給敵人作掠物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我田野的山哪，我必因你在四境之內所犯的罪，把你的貨物、財寶，並邱壇當掠物交給仇敵；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "O mountain of Mine in the countryside, I will give over your wealth and all your treasures for booty, Your high places for sin throughout your borders."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My mountain in the land and your wealth and all your treasures I will give away as plunder, together with your high places, because of sin throughout your country."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So I will hand over my holy mountain— along with all your wealth and treasures and your pagan shrines— as plunder to your enemies, for sin runs rampant in your land."
      }
    },
