    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.17.19-27",
            "paragraphs": [
              "一七19～27呼籲百姓視安息日為神聖之日，以此表示他們悔改之心（比較尼一○31；一三15～22），因為他們利用安息日來運送農作物與其它物品進城，違犯了律法。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说，如果你们想保住性命，就要小心，不要在安息日带货物进耶路撒冷的城门，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華這樣說：為了自己的性命，你們要謹慎，不可在安息日挑擔子，不可挑進耶路撒冷的城門。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華如此說：你們要謹慎，不要在安息日擔甚麼擔子進入耶路撒冷的各門；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Thus says the Lord: Take care for the sake of your lives, and do not bear a burden on the Sabbath day or bring it in by the gates of Jerusalem."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord, “Take heed for yourselves, and do not carry any load on the sabbath day or bring anything in through the gates of Jerusalem."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord says: Be careful not to carry a load on the Sabbath day or bring it through the gates of Jerusalem."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Thus says the Lord: “Take heed to yourselves, and bear no burden on the Sabbath day, nor bring it in by the gates of Jerusalem;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord says: Listen to my warning! Stop carrying on your trade at Jerusalem’s gates on the Sabbath day."
      }
    },
