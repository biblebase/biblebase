    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.44.1-14",
            "paragraphs": [
              "四四1～14耶利米這個最後的信息（給埃及的猶太人）跟他一直所傳講的相同：神必定懲罰以色列拜偶像的罪。\"密奪、答比匿、挪弗（孟斐斯）\"皆位於下埃及（埃及北部），\"巴忒羅\"泛指上埃及（埃及南部）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道你们忘记了你们的祖先、犹大列王及其嫔妃，以及你们自己和妻子在犹大和耶路撒冷街上的恶行吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們列祖的惡行、猶大列王的惡行、他們妻子的惡行、你們自己的惡行和你們妻子的惡行，就是在猶大地和耶路撒冷街上所行的，你們忘記了嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們列祖的惡行，猶大列王和他們后妃的惡行，你們自己和你們妻子的惡行，就是在猶大地、耶路撒冷街上所行的，你們都忘了嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Have you forgotten the evil of your fathers, the evil of the kings of Judah, the evil of their wives, your own evil, and the evil of your wives, which they committed in the land of Judah and in the streets of Jerusalem?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Have you forgotten the wickedness of your fathers, the wickedness of the kings of Judah, and the wickedness of their wives, your own wickedness, and the wickedness of your wives, which they committed in the land of Judah and in the streets of Jerusalem?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Have you forgotten the wickedness committed by your ancestors and by the kings and queens of Judah and the wickedness committed by you and your wives in the land of Judah and the streets of Jerusalem?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Have you forgotten the wickedness of your fathers, the wickedness of the kings of Judah, the wickedness of their wives, your own wickedness, and the wickedness of your wives, which they committed in the land of Judah and in the streets of Jerusalem?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Have you forgotten the sins of your ancestors, the sins of the kings and queens of Judah, and the sins you and your wives committed in Judah and Jerusalem?"
      }
