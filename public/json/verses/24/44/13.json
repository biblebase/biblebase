    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.44.1-14",
            "paragraphs": [
              "四四1～14耶利米這個最後的信息（給埃及的猶太人）跟他一直所傳講的相同：神必定懲罰以色列拜偶像的罪。\"密奪、答比匿、挪弗（孟斐斯）\"皆位於下埃及（埃及北部），\"巴忒羅\"泛指上埃及（埃及南部）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要像惩罚耶路撒冷一样用战争、饥荒和瘟疫惩罚那些住在埃及的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我必用刀劍、饑荒和瘟疫懲罰那些住在埃及地的人，好像懲罰耶路撒冷一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我怎樣用刀劍、饑荒、瘟疫刑罰耶路撒冷，也必照樣刑罰那些住在埃及地的猶大人；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And I will punish those who live in the land of Egypt, as I have punished Jerusalem, with the sword, with famine and with pestilence."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will punish those who live in Egypt with the sword, famine and plague, as I punished Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will punish them in Egypt just as I punished them in Jerusalem, by war, famine, and disease."
      }
    },
