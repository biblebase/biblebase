    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米又对在场的男女说：“你们住在埃及的犹大人都要听耶和华的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米又對眾民和眾婦女說：“在埃及地所有的猶大人哪，要聽耶和華的話！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶利米又對眾民和眾婦女說：「你們在埃及地的一切猶大人當聽耶和華的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jeremiah said to all the people, including all the women, “Hear the word of the Lord, all Judah who are in the land of Egypt,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Jeremiah said to all the people, including the women, “Hear the word of the Lord, all you people of Judah in Egypt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Jeremiah said to them all, including the women, “Listen to this message from the Lord, all you citizens of Judah who live in Egypt."
      }
    },
