    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "看啊，我一定要给你们降下灾祸而非祝福，使所有住在埃及的犹大人死于战争和饥荒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "看哪！我要留意向他們降禍不降福；在埃及地所有的猶大人都必因刀劍和饑荒滅亡，直到滅絕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我向他們留意降禍不降福；在埃及地的一切猶大人必因刀劍、饑荒所滅，直到滅盡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I am watching over them for harm and not for good, and all the men of Judah who are in the land of Egypt will meet their end by the sword and by famine until they are completely gone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I am watching over them for harm, not for good; the Jews in Egypt will perish by sword and famine until they are all destroyed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For I will watch over you to bring you disaster and not good. Everyone from Judah who is now living in Egypt will suffer war and famine until all of you are dead."
      }
    },
