    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.44.1-14",
            "paragraphs": [
              "四四1～14耶利米這個最後的信息（給埃及的猶太人）跟他一直所傳講的相同：神必定懲罰以色列拜偶像的罪。\"密奪、答比匿、挪弗（孟斐斯）\"皆位於下埃及（埃及北部），\"巴忒羅\"泛指上埃及（埃及南部）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我屡次差遣我的仆人——众先知警告他们不要再做令我深恶痛绝的事，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖然我不斷差遣我的僕人眾先知到你們那裡去，說：你們千萬不可行我所恨惡，這可憎的事！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我從早起來差遣我的僕人眾先知去說，你們切不要行我所厭惡這可憎之事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Yet I sent you all My servants the prophets, again and again, saying, “Oh, do not do this abominable thing which I hate.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Again and again I sent my servants the prophets, who said, ‘Do not do this detestable thing that I hate!’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Again and again I sent my servants, the prophets, to plead with them, ‘Don’t do these horrible things that I hate so much.’"
      }
    },
