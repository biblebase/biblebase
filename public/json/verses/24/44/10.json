    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.44.1-14",
            "paragraphs": [
              "四四1～14耶利米這個最後的信息（給埃及的猶太人）跟他一直所傳講的相同：神必定懲罰以色列拜偶像的罪。\"密奪、答比匿、挪弗（孟斐斯）\"皆位於下埃及（埃及北部），\"巴忒羅\"泛指上埃及（埃及南部）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们至今既不悔悟也不惧怕，不遵行我给你们和你们祖先设立的律法和诫命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "直到今日，他們還沒有痛悔，沒有畏懼，也沒有遵行我在你們和你們列祖面前所立的律法和條例。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "到如今還沒有懊悔，沒有懼怕，沒有遵行我在你們和你們列祖面前所設立的法度律例。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But they have not become contrite even to this day, nor have they feared nor walked in My law or My statutes, which I have set before you and before your fathers.” ’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To this day they have not humbled themselves or shown reverence, nor have they followed my law and the decrees I set before you and your ancestors."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "To this very hour you have shown no remorse or reverence. No one has chosen to follow my word and the decrees I gave to you and your ancestors before you."
      }
    },
