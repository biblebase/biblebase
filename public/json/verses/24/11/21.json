    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.11.18-23",
            "paragraphs": [
              "一一18～23這裏描述耶利米在家鄉面對百姓的敵意。他們想把他（\"樹\"）殺了，使他的信息（\"果子\"，19節）安靜下來。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因此耶和華指責那些亞拿突人，他們尋索你命，向你恐嚇說：“不要奉耶和華的名說預言，免得你死在我們手中。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，耶和華論到尋索你命的亞拿突人如此說：「他們說：你不要奉耶和華的名說預言，免得你死在我們手中。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Therefore thus says the Lord concerning the men of Anathoth, who seek your life, and say, “Do not prophesy in the name of the Lord, or you will die by our hand”—"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore thus says the Lord concerning the men of Anathoth, who seek your life, saying, “Do not prophesy in the name of the Lord, so that you will not die at our hand”;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore this is what the Lord says about the people of Anathoth who are threatening to kill you, saying, “Do not prophesy in the name of the Lord or you will die by our hands”—"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Therefore thus says the Lord concerning the men of Anathoth who seek your life, saying, ‘Do not prophesy in the name of the Lord, lest you die by our hand’—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord says about the men of Anathoth who wanted me dead. They had said, “We will kill you if you do not stop prophesying in the Lord’s name.”"
      }
    },
