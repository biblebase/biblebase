    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "护卫长尼布撒拉旦把城中的余民以及投降的人都掳到巴比伦，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "城裡剩下的人民，和那些已經向巴比倫王投降的人，以及其餘的人民，都被護衛長尼布撒拉旦擄到巴比倫去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，護衛長尼布撒拉旦將城裏所剩下的百姓和投降他的逃民，以及其餘的民都擄到巴比倫去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As for the rest of the people who were left in the city, the deserters who had gone over to him and the rest of the people who remained, Nebuzaradan the captain of the bodyguard carried them into exile in Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nebuzaradan commander of the imperial guard carried into exile to Babylon the people who remained in the city, along with those who had gone over to him, and the rest of the people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then Nebuzaradan, the captain of the guard, took as exiles to Babylon the rest of the people who remained in the city, those who had defected to him, and everyone else who remained."
      }
    },
