    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.39.18",
            "paragraphs": [
              "三九18以伯米勒（16節）為耶利米求情的時候（三八7～13）證明他對真神的信靠。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必拯救你，使你保全性命，不致丧身刀下，因为你信靠我。这是耶和华说的。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "‘因為我必定搭救你；你必不會倒在刀下，必可以撿回自己的性命，因為你信靠我。’”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我定要搭救你，你不致倒在刀下，卻要以自己的命為掠物，因你倚靠我。這是耶和華說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For I will certainly rescue you, and you will not fall by the sword; but you will have your own life as booty, because you have trusted in Me,” declares the Lord.’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will save you; you will not fall by the sword but will escape with your life, because you trust in me, declares the Lord.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because you trusted me, I will give you your life as a reward. I will rescue you and keep you safe. I, the Lord, have spoken!’”"
      }
    },
