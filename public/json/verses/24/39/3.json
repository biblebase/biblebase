    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "尼甲·沙利薛、三甲·尼波、撒西金和另一个尼甲·沙利薛等巴比伦王的所有将领都进城坐在中门。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴比倫王所有的將領：尼甲．沙利薛、三甲．尼波、大臣撒西金、術士長尼甲．沙利薛，以及巴比倫王所有其餘的將領，都進城，坐在中門。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶路撒冷被攻取的時候，巴比倫王的首領尼甲‧沙利薛、三甲‧尼波、撒西金－拉撒力、尼甲‧沙利薛－拉墨，並巴比倫王其餘的一切首領都來坐在中門。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then all the officials of the king of Babylon came in and sat down at the Middle Gate: Nergal-sar-ezer, Samgar-nebu, Sar-sekim the Rab-saris, Nergal-sar-ezer the Rab-mag, and all the rest of the officials of the king of Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then all the officials of the king of Babylon came and took seats in the Middle Gate: Nergal-Sharezer of Samgar, Nebo-Sarsekim a chief officer, Nergal-Sharezer a high official and all the other officials of the king of Babylon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All the officers of the Babylonian army came in and sat in triumph at the Middle Gate: Nergal-sharezer of Samgar, and Nebo-sarsekim, a chief officer, and Nergal-sharezer, the king’s adviser, and all the other officers of the king of Babylon."
      }
    },
