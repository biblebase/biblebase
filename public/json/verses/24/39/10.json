    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "却让那些一贫如洗的穷人留在犹大，将葡萄园和田地分给他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "但護衛長尼布撒拉旦把民中一些一無所有的窮人留在猶大地，同時把葡萄園和田地分給他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "護衛長尼布撒拉旦卻將民中毫無所有的窮人留在猶大地，當時給他們葡萄園和田地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But some of the poorest people who had nothing, Nebuzaradan the captain of the bodyguard left behind in the land of Judah, and gave them vineyards and fields at that time."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But Nebuzaradan the commander of the guard left behind in the land of Judah some of the poor people, who owned nothing; and at that time he gave them vineyards and fields."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But Nebuzaradan allowed some of the poorest people to stay behind in the land of Judah, and he assigned them to care for the vineyards and fields."
      }
    },
