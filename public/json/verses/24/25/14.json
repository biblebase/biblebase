    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们必受许多国家和强大君王的奴役，我要按他们的所作所为报应他们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為許多國家和大君王將要奴役迦勒底人，我必按著他們的作為和他們手所作的報應他們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為有多國和大君王必使迦勒底人作奴僕；我也必照他們的行為，按他們手所做的報應他們。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "(For many nations and great kings will make slaves of them, even them; and I will recompense them according to their deeds and according to the work of their hands.)’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They themselves will be enslaved by many nations and great kings; I will repay them according to their deeds and the work of their hands.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Many nations and great kings will enslave the Babylonians, just as they enslaved my people. I will punish them in proportion to the suffering they cause my people.”"
      }
