    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.25.2-7",
            "paragraphs": [
              "二五2～7二十三年以來，耶利米一直勸百姓從他們邪惡的道路轉回。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们对你们说，‘要改邪归正，停止作恶，以便可以永远住在耶和华赐给你们和你们祖先的土地上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們對你們說：‘你們各人要悔改，離開自己的惡道和邪惡的行為，就可以住在耶和華賜給你們和你們列祖的土地上，直到永永遠遠。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說：『你們各人當回頭，離開惡道和所作的惡，便可居住耶和華古時所賜給你們和你們列祖之地，直到永遠。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "saying, ‘Turn now everyone from his evil way and from the evil of your deeds, and dwell on the land which the Lord has given to you and your forefathers forever and ever;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They said, “Turn now, each of you, from your evil ways and your evil practices, and you can stay in the land the Lord gave to you and your ancestors for ever and ever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Each time the message was this: ‘Turn from the evil road you are traveling and from the evil things you are doing. Only then will I let you live in this land that the Lord gave to you and your ancestors forever."
      }
    },
