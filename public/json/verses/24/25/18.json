    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我给耶路撒冷和犹大的城邑，以及犹大的君王和官员喝这杯烈怒，使他们的城邑沦为废墟，令人惊惧，被嘲笑和咒诅，正如今日一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "包括耶路撒冷、猶大的城鎮，和她的君王、領袖，使這些城鎮荒廢，成為令人驚駭、被人嗤笑、咒罵的對象，好像今天一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就是耶路撒冷和猶大的城邑，並耶路撒冷的君王與首領，使這城邑荒涼，令人驚駭、嗤笑、咒詛，正如今日一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jerusalem and the cities of Judah and its kings and its princes, to make them a ruin, a horror, a hissing and a curse, as it is this day;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jerusalem and the towns of Judah, its kings and officials, to make them a ruin and an object of horror and scorn, a curse—as they are today;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I went to Jerusalem and the other towns of Judah, and their kings and officials drank from the cup. From that day until this, they have been a desolate ruin, an object of horror, contempt, and cursing."
      }
    },
