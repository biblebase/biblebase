    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "万军之耶和华说： “看啊，灾难正蔓延到列国， 有暴风从地极卷起。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "萬軍之耶和華這樣說： “看哪！必有災禍發出， 從這國直到那國； 必有大風暴從地極颳起。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "萬軍之耶和華如此說： 看哪，必有災禍從這國發到那國， 並有大暴風從地極颳起。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thus says the Lord of hosts, “Behold, evil is going forth From nation to nation, And a great storm is being stirred up From the remotest parts of the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is what the Lord Almighty says: “Look! Disaster is spreading from nation to nation; a mighty storm is rising from the ends of the earth.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This is what the Lord of Heaven’s Armies says: “Look! Disaster will fall upon nation after nation! A great whirlwind of fury is rising from the most distant corners of the earth!”"
      }
    },
