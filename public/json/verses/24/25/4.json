    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.25.2-7",
            "paragraphs": [
              "二五2～7二十三年以來，耶利米一直勸百姓從他們邪惡的道路轉回。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华屡次差遣祂的仆人——众先知警告你们，你们却充耳不闻，毫不理会。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華雖然不斷差遣他的僕人眾先知到你們那裡去；但你們卻沒有聽從，毫不留心。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華也從早起來，差遣他的僕人眾先知到你們這裏來（只是你們沒有聽從，也沒有側耳而聽），"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the Lord has sent to you all His servants the prophets again and again, but you have not listened nor inclined your ear to hear,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And though the Lord has sent all his servants the prophets to you again and again, you have not listened or paid any attention."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Again and again the Lord has sent you his servants, the prophets, but you have not listened or even paid attention."
      }
    },
