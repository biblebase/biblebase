      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要向耶和華唱歌； 讚美耶和華！ 因他救了窮人的性命脫離惡人的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Sing to the Lord, praise the Lord! For He has delivered the soul of the needy one From the hand of evildoers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Sing to the Lord! Give praise to the Lord! He rescues the life of the needy from the hands of the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Sing to the Lord! Praise the Lord! For though I was poor and needy, he rescued me from my oppressors."
      }
