    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿那人像被耶和华无情毁灭的城！ 愿他早晨听见哀鸣， 中午听见战争的呐喊，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "願那人像耶和華所傾覆而不顧惜的城市， 願他早晨聽見哀號， 午間聽見戰爭的吶喊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "願那人像耶和華所傾覆而不後悔的城邑； 願他早晨聽見哀聲， 晌午聽見吶喊；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But let that man be like the cities Which the Lord overthrew without relenting, And let him hear an outcry in the morning And a shout of alarm at noon;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May that man be like the towns the Lord overthrew without pity. May he hear wailing in the morning, a battle cry at noon."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Let him be destroyed like the cities of old that the Lord overthrew without mercy. Terrify him all day long with battle shouts,"
      }
    },
