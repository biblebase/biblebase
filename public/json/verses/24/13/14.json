    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使他们彼此撞得粉碎，就算父子也要彼此相撞；我要毁灭他们，决不心慈手软。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要使他們彼此碰撞，連父子也要互相碰撞；我必不愛惜、不可憐，也不憐憫他們，我必把他們滅絕。’”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「我要使他們彼此相碰，就是父與子彼此相碰；我必不可憐，不顧惜，不憐憫，以致滅絕他們。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will dash them against each other, both the fathers and the sons together,” declares the Lord. “I will not show pity nor be sorry nor have compassion so as not to destroy them.” ’ ”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will smash them one against the other, parents and children alike, declares the Lord. I will allow no pity or mercy or compassion to keep me from destroying them.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will smash them against each other, even parents against children, says the Lord. I will not let my pity or mercy or compassion keep me from destroying them.’”"
      }
    },
