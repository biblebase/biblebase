    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.13.18-19",
            "paragraphs": [
              "一三18,19這是哀悼約雅斤王（當時只有18歲）和太後（尼護施他；王下二四8）被擄到巴比倫（耶二九2）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华让我对君王和太后说： “从宝座上下来吧， 因为你们头上华美的王冠将要掉落。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你要對君王和太后說： “你們應從寶座下來， 因為華麗的冠冕 已經從你們的頭上脫下來了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你要對君王和太后說： 你們當自卑，坐在下邊； 因你們的頭巾， 就是你們的華冠，已經脫落了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Say to the king and the queen mother, “Take a lowly seat, For your beautiful crown Has come down from your head.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Say to the king and to the queen mother, “Come down from your thrones, for your glorious crowns will fall from your heads.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Say to the king and his mother, “Come down from your thrones and sit in the dust, for your glorious crowns will soon be snatched from your heads.”"
      }
    },
