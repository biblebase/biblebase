    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.13.20-27",
            "paragraphs": [
              "一三20～27最後一個警告，指出猶大若繼續犯罪，懲罰（被擄到巴比倫）就是他們無可避免的後果。第23節指出要猶大改變她的行為，是多麽不可能的事。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，耶和华说： “我要把你分散到四方， 像旷野的风吹散碎秸一样。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“因此，我要使你們四散， 像被曠野的風吹走的碎稭一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以我必用曠野的風吹散他們， 像吹過的碎稭一樣。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I will scatter you  like chaff driven by the wind from the desert."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore I will scatter them like drifting straw To the desert wind."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will scatter you like chaff driven by the desert wind."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Therefore I will scatter them like stubble That passes away by the wind of the wilderness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I will scatter you like chaff that is blown away by the desert winds."
      }
    },
