    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.37.20",
            "paragraphs": [
              "三七20年屆六十的耶利米害怕返回潮濕的貯水池監牢之後，會\"死\"在那裏。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "现在，我主我王啊，请听我说！求你不要把我送回书记约拿单的房子，免得我死在那里。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我主我王啊！現在求你垂聽，求你容我向你懇求！不要把我送回書記約拿單的房子去，免得我死在那裡。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主－我的王啊，求你現在垂聽，准我在你面前的懇求：不要使我回到文士約拿單的房屋中，免得我死在那裏。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now hear, please, O my lord the king: let my humble plea come before you and do not send me back to the house of Jonathan the secretary, lest I die there.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But now, please listen, O my lord the king; please let my petition come before you and do not make me return to the house of Jonathan the scribe, that I may not die there.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But now, my lord the king, please listen. Let me bring my petition before you: Do not send me back to the house of Jonathan the secretary, or I will die there.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore please hear now, O my lord the king. Please, let my petition be accepted before you, and do not make me return to the house of Jonathan the scribe, lest I die there.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Listen, my lord the king, I beg you. Don’t send me back to the dungeon in the house of Jonathan the secretary, for I will die there.”"
      }
    },
