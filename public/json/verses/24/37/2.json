    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.37.1-3",
            "paragraphs": [
              "三七1～3傀儡王西底家雖然要求耶利米為百姓禱告，卻不聽耶利米的警告。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "西底迦和他的臣仆及人民却没有听从耶和华借耶利米先知说的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "西底家和他的臣僕，以及那地的人民，都不聽從耶和華藉耶利米先知所傳的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但西底家和他的臣僕，並國中的百姓，都不聽從耶和華藉先知耶利米所說的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But neither he nor his servants nor the people of the land listened to the words of the Lord which He spoke through Jeremiah the prophet."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Neither he nor his attendants nor the people of the land paid any attention to the words the Lord had spoken through Jeremiah the prophet."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But neither King Zedekiah nor his attendants nor the people who were left in the land listened to what the Lord said through Jeremiah."
      }
    },
