    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.36.4",
            "paragraphs": [
              "三六4\"巴錄\"。參看第三十二章12至25節、第三十六章10至19、26節及第四十三章3、6節。他是耶利米的隨從和書記。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米就叫来尼利亚的儿子巴录。巴录按耶利米的口述把耶和华的话写在卷轴上，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是耶利米把尼利亞的兒子巴錄叫了來，巴錄就從耶利米口中，把耶和華向耶利米所說的一切話，都寫在書卷上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以，耶利米召了尼利亞的兒子巴錄來；巴錄就從耶利米口中，將耶和華對耶利米所說的一切話寫在書卷上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jeremiah called Baruch the son of Neriah, and Baruch wrote on a scroll at the dictation of Jeremiah all the words of the Lord which He had spoken to him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Jeremiah called Baruch son of Neriah, and while Jeremiah dictated all the words the Lord had spoken to him, Baruch wrote them on the scroll."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So Jeremiah sent for Baruch son of Neriah, and as Jeremiah dictated all the prophecies that the Lord had given him, Baruch wrote them on a scroll."
      }
    },
