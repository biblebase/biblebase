      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把卷轴存放在书记以利沙玛的房间，然后进宫把这件事禀告王。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾領袖把書卷存在書記以利沙瑪的房間裡，然後進內院去見王，把這一切話都稟告給王聽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾首領進院見王，卻先把書卷存在文士以利沙瑪的屋內，以後將這一切話說給王聽。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So they went into the court to the king, having put the scroll in the chamber of Elishama the secretary, and they reported all the words to the king."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they went to the king in the court, but they had deposited the scroll in the chamber of Elishama the scribe, and they reported all the words to the king."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After they put the scroll in the room of Elishama the secretary, they went to the king in the courtyard and reported everything to him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And they went to the king, into the court; but they stored the scroll in the chamber of Elishama the scribe, and told all the words in the hearing of the king."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then the officials left the scroll for safekeeping in the room of Elishama the secretary and went to tell the king what had happened."
      }
    },
