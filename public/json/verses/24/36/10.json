    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.36.10",
            "paragraphs": [
              "三六10書卷要念三遍：在聖殿的上院（10節），在眾首領面前（15節）和在約雅敬王面前（21節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴录来到耶和华殿新门入口处的上院，在沙番的儿子书记基玛利雅的房间，向众人宣读耶利米写在卷轴上的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴錄就在耶和華的殿裡，向眾民宣讀書上耶利米的話。那時他是在沙番的兒子書記基瑪利雅的房間裡，這房間是在上院，靠近耶和華殿裡新門的入口。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴錄就在耶和華殿的上院，耶和華殿的新門口，沙番的兒子文士基瑪利雅的屋內，念書上耶利米的話給眾民聽。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Baruch read from the book the words of Jeremiah in the house of the Lord in the chamber of Gemariah the son of Shaphan the scribe, in the upper court, at the entry of the New Gate of the Lord’s house, to all the people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the room of Gemariah son of Shaphan the secretary, which was in the upper courtyard at the entrance of the New Gate of the temple, Baruch read to all the people at the Lord’s temple the words of Jeremiah from the scroll."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Baruch read Jeremiah’s words on the scroll to all the people. He stood in front of the Temple room of Gemariah, son of Shaphan the secretary. This room was just off the upper courtyard of the Temple, near the New Gate entrance."
      }
    },
