    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.42.7-22",
            "paragraphs": [
              "四二7～22耶利米的警告是最清楚不過的：留在巴勒斯坦信靠那保護你們的神；或前往埃及然後死在那裏。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大的余民啊，耶和华已告诉你们不要去埃及。别忘了我今日给你们的警告，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大餘剩的人哪，耶和華曾對你們說：‘你們不要去埃及。’你們要確實知道，我今天已經警告你們了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所剩下的猶大人哪，耶和華論到你們說：『不要進入埃及去。』你們要確實地知道我今日警教你們了。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The Lord has said to you, O remnant of Judah, ‘Do not go to Egypt.’ Know for a certainty that I have warned you this day"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord has spoken to you, O remnant of Judah, “Do not go into Egypt!” You should clearly understand that today I have testified against you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Remnant of Judah, the Lord has told you, ‘Do not go to Egypt.’ Be sure of this: I warn you today"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“The Lord has said concerning you, O remnant of Judah, ‘Do not go to Egypt!’ Know certainly that I have admonished you this day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Listen, you remnant of Judah. The Lord has told you: ‘Do not go to Egypt!’ Don’t forget this warning I have given you today."
      }
    },
