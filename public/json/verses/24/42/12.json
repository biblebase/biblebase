    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.42.7-22",
            "paragraphs": [
              "四二7～22耶利米的警告是最清楚不過的：留在巴勒斯坦信靠那保護你們的神；或前往埃及然後死在那裏。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我必怜悯你们，也要让他怜悯你们，使你们重归故土。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要向你們施憐憫，他也會憐憫你們，使你們歸回自己的土地。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我也要使他發憐憫，好憐憫你們，叫你們歸回本地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will also show you compassion, so that he will have compassion on you and restore you to your own soil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will show you compassion so that he will have compassion on you and restore you to your land.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will be merciful to you by making him kind, so he will let you stay here in your land.’"
      }
    },
