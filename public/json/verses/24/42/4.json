    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.42.1-6",
            "paragraphs": [
              "四二1～6眾人求問耶利米，看神是否贊成他們移居埃及的計劃。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶利米先知说：“好。我必照你们说的向你们的上帝耶和华祈求，并把祂的话毫不隐瞒地告诉你们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶利米先知對他們說：“我聽見了！我必照著你們的話禱告耶和華你們的　神。耶和華回答你們所說的一切話，我必告訴你們，一句也不會向你們隱瞞。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "先知耶利米對他們說：「我已經聽見你們了，我必照着你們的話禱告耶和華－你們的神。耶和華無論回答甚麼，我必都告訴你們，毫不隱瞞。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then Jeremiah the prophet said to them, “I have heard you. Behold, I am going to pray to the Lord your God in accordance with your words; and I will tell you the whole message which the Lord will answer you. I will not keep back a word from you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I have heard you,” replied Jeremiah the prophet. “I will certainly pray to the Lord your God as you have requested; I will tell you everything the Lord says and will keep nothing back from you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“All right,” Jeremiah replied. “I will pray to the Lord your God, as you have asked, and I will tell you everything he says. I will hide nothing from you.”"
      }
    },
