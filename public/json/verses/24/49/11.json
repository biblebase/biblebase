    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "撇下你们的孤儿吧！ 我必使他们存活； 你们的寡妇也可以倚靠我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有人會說：‘你撇下你的孤兒， 我必使他們存活； 你的寡婦可以倚靠我。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你撇下孤兒，我必保全他們的命； 你的寡婦可以倚靠我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Leave your orphans behind, I will keep them alive; And let your widows trust in Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "‘Leave your fatherless children; I will keep them alive. Your widows too can depend on me.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I will protect the orphans who remain among you. Your widows, too, can depend on me for help.”"
      }
