    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.49.19-22",
            "paragraphs": [
              "四九19～22神沒有應許以東將來會得到復興。由於土地被拿巴提人占領了，以東人要移居到猶大南部，稱為以土買人，並且不再是一個獨特的民族。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以东倾覆的声音震动大地，哭喊声直传到红海。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因他們倒下的響聲，地就震動；並且有哀叫的聲音，在紅海那裡也聽得到。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他們仆倒的聲音，地就震動。人在紅海那裏必聽見呼喊的聲音。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The earth has quaked at the noise of their downfall. There is an outcry! The noise of it has been heard at the Red Sea."
      },
      "niv": {
        "version_id": "NIV",
