    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "大马士革丧失勇气，转身逃命。 她像分娩的妇人一样惊恐不安， 充满痛苦和忧愁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大馬士革失去勇氣，轉身逃跑； 但恐慌抓住它， 憂慮痛苦把它捉住， 像正在生產的婦人的痛苦一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "大馬士革發軟，轉身逃跑。 戰兢將她捉住； 痛苦憂愁將她抓住， 如產難的婦人一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Damascus has become helpless; She has turned away to flee, And panic has gripped her; Distress and pangs have taken hold of her Like a woman in childbirth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Damascus has become feeble, she has turned to flee and panic has gripped her; anguish and pain have seized her, pain like that of a woman in labor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Damascus has become feeble, and all her people turn to flee. Fear, anguish, and pain have gripped her as they grip a woman in labor."
      }
    },
