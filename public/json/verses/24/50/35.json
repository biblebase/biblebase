      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A sword against the Chaldeans,” declares the Lord, “And against the inhabitants of Babylon And against her officials and her wise men!"
      },
