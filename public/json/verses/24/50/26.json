    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要从远方前来攻打巴比伦， 打开她的粮仓， 使她沦为一堆废墟， 彻底毁灭她，不留一物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要從四面八方攻擊巴比倫， 打開它的穀倉， 把它堆起來好像堆穀物一樣； 把它徹底消滅， 甚麼都不留下。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們要從極遠的邊界來攻擊她， 開她的倉廩， 將她堆如高堆， 毀滅淨盡，絲毫不留。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Come against her from every quarter; open her granaries; pile her up like heaps of grain, and devote her to destruction; let nothing be left of her."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Come to her from the farthest border; Open up her barns, Pile her up like heaps And utterly destroy her, Let nothing be left to her."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Come against her from afar. Break open her granaries; pile her up like heaps of grain. Completely destroy her and leave her no remnant."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Come against her from the farthest border; Open her storehouses; Cast her up as heaps of ruins, And destroy her utterly; Let nothing of her be left."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, come against her from distant lands. Break open her granaries. Crush her walls and houses into heaps of rubble. Destroy her completely, and leave nothing!"
      }
    },
