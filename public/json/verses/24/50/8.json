    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我的子民啊，你们要逃离巴比伦，离开迦勒底人的土地，要像带领羊群的公山羊一样离开。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的子民哪！你們要從巴比倫中間逃跑， 從迦勒底人的地出來； 要像公山羊走在羊群前面。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我民哪，你們要從巴比倫中逃走，從迦勒底人之地出去，要像羊羣前面走的公山羊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Wander away from the midst of Babylon And go forth from the land of the Chaldeans; Be also like male goats at the head of the flock."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Flee out of Babylon; leave the land of the Babylonians, and be like the goats that lead the flock."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But now, flee from Babylon! Leave the land of the Babylonians. Like male goats at the head of the flock, lead my people home again."
      }
