    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们要杀掉她所有的士兵， 把他们全部屠杀。 他们大祸临头了， 惩罚他们的日子到了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要擊殺它所有的公牛， 使他們下去被屠殺！ 他們有禍了！因為他們的日子， 就是他們受懲罰的時候，來到了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要殺她的一切牛犢， 使他們下去遭遇殺戮。 他們有禍了， 因為追討他們的日子已經來到。 （"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Put all her young bulls to the sword; Let them go down to the slaughter! Woe be upon them, for their day has come, The time of their punishment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Kill all her young bulls; let them go down to the slaughter! Woe to them! For their day has come, the time for them to be punished."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Destroy even her young bulls— it will be terrible for them, too! Slaughter them all! For Babylon’s day of reckoning has come."
      }
    },
