    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华已打开祂的军库， 取出倾泻祂愤怒的兵器。 因为主——万军之耶和华要惩罚迦勒底人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華打開了自己的軍械庫， 拿出他忿怒的武器； 因為主萬軍之耶和華 在迦勒底人的地有事要作。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華已經開了武庫， 拿出他惱恨的兵器； 因為主－萬軍之耶和華 在迦勒底人之地有當做的事。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord has opened His armory And has brought forth the weapons of His indignation, For it is a work of the Lord God of hosts In the land of the Chaldeans."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord has opened his arsenal and brought out the weapons of his wrath, for the Sovereign Lord Almighty has work to do in the land of the Babylonians."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord has opened his armory and brought out weapons to vent his fury. The terror that falls upon the Babylonians will be the work of the Sovereign Lord of Heaven’s Armies."
      }
    },
