      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "敌人要攻击巴比伦人的战车战马和他们的盟军， 使他们柔弱得像妇女； 敌人要攻打他们的库房， 肆意抢夺他们的财宝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "刀劍必攻擊它的馬匹和戰車， 攻擊它境內所有外族的人， 他們必像婦女一樣柔弱； 刀劍必攻擊它的一切寶庫， 它們必被搶掠；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有刀劍臨到她的馬匹、車輛， 和其中雜族的人民； 他們必像婦女一樣。 有刀劍臨到她的寶物， 就被搶奪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A sword against their horses and against their chariots And against all the foreigners who are in the midst of her, And they will become women! A sword against her treasures, and they will be plundered!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A sword against her horses and chariots and all the foreigners in her ranks! They will become weaklings. A sword against her treasures! They will be plundered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The sword will strike her horses and chariots and her allies from other lands, and they will all become like women. The sword will strike her treasures, and they all will be plundered."
      }
