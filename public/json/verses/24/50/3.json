    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.50.3",
            "paragraphs": [
              "50:3",
              "使他的地荒涼：耶利米預見到巴比倫將遭受一次極大的毀滅。當時從北方來的一國就是瑪代波斯帝國；主前539年，波斯王古列率領大軍攻破巴比倫。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为有一个来自北方的国家要攻击它， 使它荒凉、人畜绝迹。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為有一個國家從北方來攻擊它， 使它的地荒涼； 沒有人在那裡居住， 人畜都逃跑無蹤。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「因有一國從北方上來攻擊她，使她的地荒涼，無人居住，連人帶牲畜都逃走了。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“For out of the north a nation has come up against her, which shall make her land a desolation, and none shall dwell in it; both man and beast shall flee away."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For a nation has come up against her out of the north; it will make her land an object of horror, and there will be no inhabitant in it. Both man and beast have wandered off, they have gone away!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A nation from the north will attack her and lay waste her land. No one will live in it; both people and animals will flee away."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "For out of the north a nation comes up against her, Which shall make her land desolate, And no one shall dwell therein. They shall move, they shall depart, Both man and beast."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For a nation will attack her from the north and bring such destruction that no one will live there again. Everything will be gone; both people and animals will flee."
      }
    },
