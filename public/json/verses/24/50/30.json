    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她的青年要倒毙街头，那时她所有的战士都要灭亡。”这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所以，它的年輕人必仆倒在它的街道上， 到那一天，它所有的戰士都必被消滅。” 這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以她的少年人必仆倒在街上。當那日，一切兵丁必默默無聲。這是耶和華說的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore her young men will fall in her streets, And all her men of war will be silenced in that day,” declares the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Therefore, her young men will fall in the streets; all her soldiers will be silenced in that day,” declares the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Her young men will fall in the streets and die. Her soldiers will all be killed,” says the Lord."
      }
    },
