    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.15.15-17",
            "paragraphs": [
              "一五15～17耶利米希望神為他伸冤，因為他吸取（\"吃了\"）神的話語（16節），逃避壞朋友（17節），並且因猶大的罪\"滿心憤恨\"（17節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我没有与作乐的人为伍， 没有与他们同乐。 你要我独坐一处， 他们的罪恶使我怒气填胸。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我沒有坐在歡笑者的集會中一同歡樂； 因為你的手在我身上， 你使我滿了忿怒， 我就獨自靜坐。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我沒有坐在宴樂人的會中， 也沒有歡樂； 我因你的感動獨自靜坐， 因你使我滿心憤恨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I did not sit in the circle of merrymakers, Nor did I exult. Because of Your hand upon me I sat alone, For You filled me with indignation."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I never sat in the company of revelers, never made merry with them; I sat alone because your hand was on me and you had filled me with indignation."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I never joined the people in their merry feasts. I sat alone because your hand was on me. I was filled with indignation at their sins."
      }
    },
