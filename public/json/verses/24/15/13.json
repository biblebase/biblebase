    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你们境内罪恶泛滥，我要使敌人任意抢掠你们的财宝，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因你在四境之內所犯的一切罪， 我必把你的財產和寶物， 白白給敵人作掠物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我必因你在四境之內所犯的一切罪，把你的貨物財寶當掠物，白白地交給仇敵。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Your wealth and your treasures I will give as spoil, without price, for all your sins, throughout all your territory."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your wealth and your treasures I will give for booty without cost, Even for all your sins And within all your borders."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Your wealth and your treasures I will give as plunder, without charge, because of all your sins throughout your country."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Your wealth and your treasures I will give as plunder without price, Because of all your sins, Throughout your territories."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "At no cost to them, I will hand over your wealth and treasures as plunder to your enemies, for sin runs rampant in your land."
      }
    },
