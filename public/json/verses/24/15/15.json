    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.15.15-17",
            "paragraphs": [
              "一五15～17耶利米希望神為他伸冤，因為他吸取（\"吃了\"）神的話語（16節），逃避壞朋友（17節），並且因猶大的罪\"滿心憤恨\"（17節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，你知道我的遭遇。 求你顾念我，眷顾我， 报应迫害我的人。 求你不要取我的性命， 因为你不轻易发怒， 你知道我为了你忍受辱骂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！你知道這一切； 求你記念我，看顧我， 替我向那些逼迫我的人報復。 求你忍怒，不要取去我的性命； 要知道，我為你的緣故受了侮辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，你是知道的； 求你記念我，眷顧我， 向逼迫我的人為我報仇； 不要向他們忍怒取我的命， 要知道我為你的緣故受了凌辱。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You who know, O Lord, Remember me, take notice of me, And take vengeance for me on my persecutors. Do not, in view of Your patience, take me away; Know that for Your sake I endure reproach."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Lord, you understand; remember me and care for me. Avenge me on my persecutors. You are long-suffering—do not take me away; think of how I suffer reproach for your sake."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then I said, “Lord, you know what’s happening to me. Please step in and help me. Punish my persecutors! Please give me time; don’t let me die young. It’s for your sake that I am suffering."
      }
    },
