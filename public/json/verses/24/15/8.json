    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我要使他们的寡妇比海沙还多， 使毁灭者在午间突然夺取青年的性命， 以致母亲们陷入痛苦、惊恐中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我使他們寡婦的數目 比海沙還多； 我使行毀滅的在午間臨到我的子民， 打擊年輕人的母親， 使痛苦的驚恐忽然臨到她們身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們的寡婦在我面前比海沙更多； 我使滅命的午間來， 攻擊少年人的母親， 使痛苦驚嚇忽然臨到她身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Their widows will be more numerous before Me Than the sand of the seas; I will bring against them, against the mother of a young man, A destroyer at noonday; I will suddenly bring down on her Anguish and dismay."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I will make their widows more numerous than the sand of the sea. At midday I will bring a destroyer against the mothers of their young men; suddenly I will bring down on them anguish and terror."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "There will be more widows than the grains of sand on the seashore. At noontime I will bring a destroyer against the mothers of young men. I will cause anguish and terror to come upon them suddenly."
      }
    },
