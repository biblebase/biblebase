    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.15.10-11",
            "paragraphs": [
              "一五10,11耶利米因他出生之日而悲傷，但神重新給他保證。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“我必拯救你，我必使仇敌在灾祸、困难来临时哀求你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華說：“我必為你的好處釋放你； 在遭遇禍患和災難的時候， 我必叫你的仇敵懇求你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「我必要堅固你，使你得好處。災禍苦難臨到的時候，我必要使仇敵央求你。（"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Lord said, “Surely I will set you free for purposes of good; Surely I will cause the enemy to make supplication to you In a time of disaster and a time of distress."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord said, “Surely I will deliver you for a good purpose; surely I will make your enemies plead with you in times of disaster and times of distress."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord replied, “I will take care of you, Jeremiah. Your enemies will ask you to plead on their behalf in times of trouble and distress."
      }
    },
