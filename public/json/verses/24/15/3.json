    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我要让他们遭受四种祸患——刀杀、狗撕、鸟啄、兽吞。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要命定四種刑罰對付他們，就是刀劍殺戮，屍體被野狗拖走，被空中的飛鳥和地上的走獸吞吃與毀滅。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「我命定四樣害他們，就是刀劍殺戮、狗類撕裂、空中的飛鳥，和地上的野獸吞吃毀滅；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I will appoint over them four kinds of doom,” declares the Lord: “the sword to slay, the dogs to drag off, and the birds of the sky and the beasts of the earth to devour and destroy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will send four kinds of destroyers against them,” declares the Lord, “the sword to kill and the dogs to drag away and the birds and the wild animals to devour and destroy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I will send four kinds of destroyers against them,” says the Lord. “I will send the sword to kill, the dogs to drag away, the vultures to devour, and the wild animals to finish up what is left."
      }
