    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.18.18-23",
            "paragraphs": [
              "一八18～23另一個針對耶利米的陰謀，還有他的反應（比較一一18～23；一二1～6；一五10～21）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华啊，求你眷顾我， 听听敌人对我的指控。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華啊！求你傾聽我的申訴， 聽那些指控我的人的聲音。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華啊，求你理會我， 且聽那些與我爭競之人的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do give heed to me, O Lord, And listen to what my opponents are saying!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Listen to me, Lord; hear what my accusers are saying!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Lord, hear me and help me! Listen to what my enemies are saying."
      }
    },
