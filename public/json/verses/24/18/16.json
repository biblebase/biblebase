    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因此，他们的土地必荒凉， 永远遭人嗤笑， 经过此地的人无不惊惧， 摇头叹息。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們的土地變為荒涼， 成為永遠被人嗤笑的對象； 經過這地的， 都必驚駭搖頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以致他們的地令人驚駭， 常常嗤笑； 凡經過這地的必驚駭搖頭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "To make their land a desolation, An object of perpetual hissing; Everyone who passes by it will be astonished And shake his head."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Their land will be an object of horror and of lasting scorn; all who pass by will be appalled and will shake their heads."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Therefore, their land will become desolate, a monument to their stupidity. All who pass by will be astonished and will shake their heads in amazement."
      }
    },
