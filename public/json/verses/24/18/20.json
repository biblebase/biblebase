    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.18.18-23",
            "paragraphs": [
              "一八18～23另一個針對耶利米的陰謀，還有他的反應（比較一一18～23；一二1～6；一五10～21）。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人岂能以恶报善？ 他们竟设陷阱谋害我， 求你回想我怎样站在你面前为他们求情， 怎样求你不要向他们发烈怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "良善的應得惡報嗎？ 他們竟然挖陷坑害我。 求你記念我怎樣站在你面前， 為他們說好話， 使你的烈怒離開他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "豈可以惡報善呢？ 他們竟挖坑要害我的性命！ 求你記念我怎樣站在你面前為他們代求， 要使你的忿怒向他們轉消。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Should good be repaid with evil? Yet they have dug a pit for my life. Remember how I stood before you to speak good for them, to turn away your wrath from them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Should good be repaid with evil? For they have dug a pit for me. Remember how I stood before You To speak good on their behalf, So as to turn away Your wrath from them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Should good be repaid with evil? Yet they have dug a pit for me. Remember that I stood before you and spoke in their behalf to turn your wrath away from them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Shall evil be repaid for good? For they have dug a pit for my life. Remember that I stood before You To speak good for them, To turn away Your wrath from them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Should they repay evil for good? They have dug a pit to kill me, though I pleaded for them and tried to protect them from your anger."
      }
    },
