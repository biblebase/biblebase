    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们却说，‘不！我们喜欢随心所欲，为所欲为。’”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們卻說：‘沒用的！我們要隨著自己的計劃而行，各人要按著自己頑梗的惡心行事。’”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「他們卻說：『這是枉然。我們要照自己的計謀去行。』各人隨自己頑梗的惡心做事。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But they will say, ‘It’s hopeless! For we are going to follow our own plans, and each of us will act according to the stubbornness of his evil heart.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But they will reply, ‘It’s no use. We will continue with our own plans; we will all follow the stubbornness of our evil hearts.’ ”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the people replied, “Don’t waste your breath. We will continue to live as we want to, stubbornly following our own evil desires.”"
      }
    },
