    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果他们做我视为恶的事，不听从我的话，我必改变心意，取消我原本要赐给他们的福气。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果他們行我眼中看為惡的事，不聽從我的話，我就必回心轉意，不把我應許的好處賜給他們。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們若行我眼中看為惡的事，不聽從我的話，我就必後悔，不將我所說的福氣賜給他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "if it does evil in My sight by not obeying My voice, then I will think better of the good with which I had promised to bless it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and if it does evil in my sight and does not obey me, then I will reconsider the good I had intended to do for it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "but then that nation turns to evil and refuses to obey me, I will not bless it as I said I would."
      }
    },
