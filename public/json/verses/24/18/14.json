    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.18.14",
            "paragraphs": [
              "一八14這裏的意思似乎是：大自然的路徑是不變的，猶大卻因背叛而違反自然，改變她的道路。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "黎巴嫩山岭上的积雪会融化吗？ 从远方流来的冰凉溪水会停止吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "黎巴嫩山上巖石的積雪融化過嗎？ 從遠處流下清涼的溪水乾涸過嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "黎巴嫩的雪從田野的磐石上豈能斷絕呢？ 從遠處流下的涼水豈能乾涸呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Does the snow of Lebanon forsake the rock of the open country? Or is the cold flowing water from a foreign land ever snatched away?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Does the snow of Lebanon ever vanish from its rocky slopes? Do its cool waters from distant sources ever stop flowing?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Does the snow ever disappear from the mountaintops of Lebanon? Do the cold streams flowing from those distant mountains ever run dry?"
      }
    },
