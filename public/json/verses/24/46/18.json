    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "名叫万军之耶和华的王说： “我凭我的永恒起誓， 将有一位要来， 他的气势如群山中的他泊山， 又像海边的迦密山。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "名叫萬軍之耶和華的王宣告說： “我指著我的永生起誓， 必有一位要來， 像群山中的他泊山， 像海邊的迦密山一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "君王－名為萬軍之耶和華的說： 我指着我的永生起誓： 尼布甲尼撒  來的勢派 必像他泊在眾山之中， 像迦密在海邊一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "As I live,” declares the King Whose name is the Lord of hosts, “Surely one shall come who looms up like Tabor among the mountains, Or like Carmel by the sea."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“As surely as I live,” declares the King, whose name is the Lord Almighty, “one will come who is like Tabor among the mountains, like Carmel by the sea."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“As surely as I live,” says the King, whose name is the Lord of Heaven’s Armies, “one is coming against Egypt who is as tall as Mount Tabor, or as Mount Carmel by the sea!"
      }
