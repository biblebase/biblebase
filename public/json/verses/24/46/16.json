    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "使他们跌跌绊绊，纷纷倒下。 他们说，‘起来，我们回到同胞那里，回到故乡吧， 好躲避敌人的刀剑。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他使眾人絆跌，倒在一起； 他們說： ‘起來！我們要回到我們本族， 和我們出生之地那裡去， 躲避欺壓者的刀劍。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "使多人絆跌； 他們也彼此撞倒，說： 起來吧！我們再往本民本地去， 好躲避欺壓的刀劍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They have repeatedly stumbled; Indeed, they have fallen one against another. Then they said, ‘Get up! And let us go back To our own people and our native land Away from the sword of the oppressor.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They will stumble repeatedly; they will fall over each other. They will say, ‘Get up, let us go back to our own people and our native lands, away from the sword of the oppressor.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They stumble and fall over each other and say among themselves, ‘Come, let’s go back to our people, to the land of our birth. Let’s get away from the sword of the enemy!’"
      }
    },
