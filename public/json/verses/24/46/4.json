    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.46.4",
            "paragraphs": [
              "四六4最優良的馬匹來自埃及（王上一○28）。\"貫甲\"的\"甲\"指盔甲。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "要套好战车， 骑上战马， 擦亮矛枪， 顶盔贯甲。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "要裝備馬匹，騎上戰馬； 站好崗位，戴上頭盔； 磨快槍矛，穿上鐵甲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們套上車， 騎上馬！ 頂盔站立， 磨槍貫甲！"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Harness the horses; mount, O horsemen! Take your stations with your helmets, polish your spears, put on your armor!"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Harness the horses, And mount the steeds, And take your stand with helmets on! Polish the spears, Put on the scale-armor!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Harness the horses, mount the steeds! Take your positions with helmets on! Polish your spears, put on your armor!"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Harness the horses, And mount up, you horsemen! Stand forth with your helmets, Polish the spears, Put on the armor!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Harness the horses, and mount the stallions. Take your positions. Put on your helmets. Sharpen your spears, and prepare your armor."
      }
    },
