    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "埃及的佣兵好像肥牛犊， 都转身逃之夭夭，无力抵挡， 因为他们遭难、受罚的日子到了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "它中間的雇傭兵像棚裡的肥牛犢， 他們一同轉身逃跑， 沒有一個站立得住； 因為他們遭災的日子、受懲罰的時候， 已經臨到他們身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其中的雇勇好像圈裏的肥牛犢， 他們轉身退後， 一齊逃跑，站立不住； 因為他們遭難的日子、 追討的時候已經臨到。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Also her mercenaries in her midst Are like fattened calves, For even they too have turned back and have fled away together; They did not stand their ground. For the day of their calamity has come upon them, The time of their punishment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The mercenaries in her ranks are like fattened calves. They too will turn and flee together, they will not stand their ground, for the day of disaster is coming upon them, the time for them to be punished."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Egypt’s mercenaries have become like fattened calves. They, too, will turn and run, for it is a day of great disaster for Egypt, a time of great punishment."
      }
    },
