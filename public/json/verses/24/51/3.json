    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.51.3",
            "paragraphs": [
              "五一3\"貫甲\"的\"甲\"指盔甲。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要让他们的弓箭手有机会射箭， 不要让他们的战士有机会穿铠甲， 不要放过他们的青年， 要使他们全军覆没。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要容拉弓的拉他的弓， 也不要容戰士穿上盔甲站起來； 不要顧惜巴比倫的年輕人， 要把它的全軍滅絕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "拉弓的，要向拉弓的和貫甲挺身的射箭。 不要憐惜她的少年人； 要滅盡她的全軍。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let not him who bends his bow bend it, Nor let him rise up in his scale-armor; So do not spare her young men; Devote all her army to destruction."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Let not the archer string his bow, nor let him put on his armor. Do not spare her young men; completely destroy her army."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t let the archers put on their armor or draw their bows. Don’t spare even her best soldiers! Let her army be completely destroyed."
      }
    },
