    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "巴比伦的勇士不再作战， 躲在堡垒里， 士气消沉，像妇女一样无力； 巴比伦的房屋被火焚烧， 城门被攻破。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "巴比倫的勇士停止作戰， 留在堡壘裡面； 他們的勇士消退， 變成像婦人一樣軟弱。 巴比倫的房屋被火焚燒， 她的門閂都被折斷。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "巴比倫的勇士止息爭戰， 藏在堅壘之中。 他們的勇力衰盡，好像婦女一樣。 巴比倫的住處有火着起， 門閂都折斷了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The mighty men of Babylon have ceased fighting, They stay in the strongholds; Their strength is exhausted, They are becoming like women; Their dwelling places are set on fire, The bars of her gates are broken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Babylon’s warriors have stopped fighting; they remain in their strongholds. Their strength is exhausted; they have become weaklings. Her dwellings are set on fire; the bars of her gates are broken."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Her mightiest warriors no longer fight. They stay in their barracks, their courage gone. They have become like women. The invaders have burned the houses and broken down the city gates."
      }
    },
