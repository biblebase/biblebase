    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.51.1",
            "paragraphs": [
              "51:1",
              "1~64攻擊巴比倫：本章經文再次論到了巴比倫，預告說神將徹底摧毀巴比倫，使他的子民脫離轄制。在末後的日子，基督將徹底毀壞那被稱為“大巴比倫”的整個罪惡敗壞的世界體系(啟18:2)。這是神對撒但和罪惡掌權的世界所施行的公義審判(參啟17:1註)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说： “看啊，我要使毁灭的暴风袭击巴比伦和住在立加米的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華這樣說： “看哪！我必激動毀滅者的心， 去攻擊巴比倫和住在立加米的人。"
      },
      "cunp": {
