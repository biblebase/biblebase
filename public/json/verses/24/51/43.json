    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "她的城邑沦为干旱的荒漠， 杳无人迹。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "它的城鎮變為荒涼， 成為乾旱的地和沙漠， 沒有人住在裡面， 也沒有人經過。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她的城邑變為荒場、旱地、沙漠， 無人居住，無人經過之地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Her cities have become an object of horror, A parched land and a desert, A land in which no man lives And through which no son of man passes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Her towns will be desolate, a dry and desert land, a land where no one lives, through which no one travels."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Her cities now lie in ruins; she is a dry wasteland where no one lives or even passes by."
      }
    },
