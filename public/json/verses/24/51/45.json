    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我的子民啊，你们要离开巴比伦， 各自逃命，躲避耶和华的烈怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的子民哪！你們要從它那裡出來， 各自逃命， 躲避耶和華的烈怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的民哪，你們要從其中出去！ 各人拯救自己，躲避耶和華的烈怒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Come forth from her midst, My people, And each of you save yourselves From the fierce anger of the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Come out of her, my people! Run for your lives! Run from the fierce anger of the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Come out, my people, flee from Babylon. Save yourselves! Run from the Lord’s fierce anger."
      }
    },
