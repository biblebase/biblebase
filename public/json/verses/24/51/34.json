    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "以色列人说： “巴比伦王尼布甲尼撒吞噬我们、击垮我们， 把我们洗劫一空。 他像海怪一样吞吃我们， 饱餐我们的美物， 然后把我们赶走。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "以色列人說：“巴比倫王尼布甲尼撒吞滅了我們， 使我們十分混亂， 又使我們成為空的器皿； 他像怪獸把我們吞下，他的肚塞滿了我們的美物， 他卻把我們吐出去。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "以色列人說： 巴比倫王尼布甲尼撒吞滅我，壓碎我， 使我成為空虛的器皿。 他像大魚將我吞下， 用我的美物充滿他的肚腹， 又將我趕出去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Nebuchadnezzar king of Babylon has devoured me and crushed me, He has set me down like an empty vessel; He has swallowed me like a monster, He has filled his stomach with my delicacies; He has washed me away."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Nebuchadnezzar king of Babylon has devoured us, he has thrown us into confusion, he has made us an empty jar. Like a serpent he has swallowed us and filled his stomach with our delicacies, and then has spewed us out."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“King Nebuchadnezzar of Babylon has eaten and crushed us and drained us of strength. He has swallowed us like a great monster and filled his belly with our riches. He has thrown us out of our own country."
      }
    },
