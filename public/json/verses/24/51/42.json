    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.51.42",
            "paragraphs": [
              "五一42\"海水……海浪\"。即好象洪水湧來的波斯軍隊。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "海水涨溢，汹涌的波涛淹没了巴比伦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "大海上漲漫過巴比倫， 它被洶湧的波浪淹沒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "海水漲起，漫過巴比倫； 她被許多海浪遮蓋。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The sea has come up over Babylon; She has been engulfed with its tumultuous waves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The sea will rise over Babylon; its roaring waves will cover her."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The sea has risen over Babylon; she is covered by its crashing waves."
      }
    },
