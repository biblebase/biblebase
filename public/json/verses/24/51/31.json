    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.51.31",
            "paragraphs": [
              "五一31巴比倫人著名的送快信制度有助傳達巴比倫的被毀。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "报信的人接踵而来， 向巴比伦王禀告， ‘整座城已失守，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "驛卒一個接一個跑來， 報信的也相繼而至， 向巴比倫王報告， 他的京城已經完全被攻取了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "跑報的要彼此相遇， 送信的要互相迎接， 報告巴比倫王說： 城的四方被攻取了，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "One runner runs to meet another, and one messenger to meet another, to tell the king of Babylon that his city is taken on every side;"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "One courier runs to meet another, And one messenger to meet another, To tell the king of Babylon That his city has been captured from end to end;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One courier follows another and messenger follows messenger to announce to the king of Babylon that his entire city is captured,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "One runner will run to meet another, And one messenger to meet another, To show the king of Babylon that his city is taken on all sides;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The news is passed from one runner to the next as the messengers hurry to tell the king that his city has been captured."
      }
    },
