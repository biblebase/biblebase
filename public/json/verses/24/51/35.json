    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "锡安的居民说： “巴比伦人残暴地对待我们， 愿他们受报应。” 耶路撒冷人说： “愿迦勒底人偿还我们的血债。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "錫安的居民要說： “願巴比倫以強暴待我們的罪歸到巴比倫身上。” 耶路撒冷要說： “願流我血的罪歸到迦勒底的居民身上。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "錫安的居民要說： 巴比倫以強暴待我， 損害我的身體， 願這罪歸給她。 耶路撒冷人要說： 願流我們血的罪歸到迦勒底的居民。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "May the violence done to me and to my flesh be upon Babylon,” The inhabitant of Zion will say; And, “May my blood be upon the inhabitants of Chaldea,” Jerusalem will say."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "May the violence done to our flesh be on Babylon,” say the inhabitants of Zion. “May our blood be on those who live in Babylonia,” says Jerusalem."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Make Babylon suffer as she made us suffer,” say the people of Zion. “Make the people of Babylonia pay for spilling our blood,” says Jerusalem."
      }
    },
