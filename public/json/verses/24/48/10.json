    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jer.48.10",
            "paragraphs": [
              "48:10",
              "咒詛：這節經文指那些即將滅絕摩押人的人。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不殷勤地为耶和华做工， 不挥刀斩杀摩押人的必受咒诅。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不努力去作耶和華的工作的，該受咒詛； 不使自己的刀劍見血的，應受咒詛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "懶惰為耶和華行事的，必受咒詛；禁止刀劍不經血的，必受咒詛。）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Cursed be the one who does the Lord’s work negligently, And cursed be the one who restrains his sword from blood."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“A curse on anyone who is lax in doing the Lord’s work! A curse on anyone who keeps their sword from bloodshed!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Cursed are those who refuse to do the Lord’s work, who hold back their swords from shedding blood!"
      }
    },
