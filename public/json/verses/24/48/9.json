    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“给摩押翅膀让她飞走吧， 因为她的城邑将沦为废墟， 杳无人迹。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們要在摩押上面撒鹽， 因為它必成為廢墟； 它的城鎮必成為荒涼， 沒有人在裡面居住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "要將翅膀給摩押， 使她可以飛去。 她的城邑必致荒涼， 無人居住。 （"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Give wings to Moab, For she will flee away; And her cities will become a desolation, Without inhabitants in them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Put salt on Moab, for she will be laid waste; her towns will become desolate, with no one to live in them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Oh, that Moab had wings so she could fly away, for her towns will be left empty, with no one living in them."
      }
