    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押人痛哭着走上鲁希坡， 他们在何罗念的山坡下听见悲惨的哀号。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在上魯希的路上， 摩押人隨走隨哭； 從何羅念下來的途中， 聽見他們因城被毀發出悽慘的哀號聲。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人上魯希坡隨走隨哭， 因為在何羅念的下坡聽見毀滅的哀聲。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For by the ascent of Luhith They will ascend with continual weeping; For at the descent of Horonaim They have heard the anguished cry of destruction."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They go up the hill to Luhith, weeping bitterly as they go; on the road down to Horonaim anguished cries over the destruction are heard."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Her refugees weep bitterly, climbing the slope to Luhith. They cry out in terror, descending the slope to Horonaim."
      }
    },
