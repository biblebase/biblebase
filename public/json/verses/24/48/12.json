    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“不过，看啊，时候将到，我要派倒酒的人去把她倒出来，倒空酒缸，把缸打碎。这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“看哪！日子快到，我必差派倒酒的人到它那裡去，把它倒出來，直至倒空，又把酒缸打碎。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「日子將到，我必打發倒酒的往她那裏去，將她倒出來，倒空她的器皿，打碎她的罈子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore behold, the days are coming,” declares the Lord, “when I will send to him those who tip vessels, and they will tip him over, and they will empty his vessels and shatter his jars."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But days are coming,” declares the Lord, “when I will send men who pour from pitchers, and they will pour her out; they will empty her pitchers and smash her jars."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But the time is coming soon,” says the Lord, “when I will send men to pour him from his jar. They will pour him out, then shatter the jar!"
      }
    },
