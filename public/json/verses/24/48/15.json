    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押要灭亡了， 她的城邑要被摧毁， 她最勇敢的青年将被杀戮。 这是名叫万军之耶和华的君王说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩押被毀滅；敵人上來攻擊它的城鎮； 摩押最精銳的少年下去被屠殺。” 這是君王的宣告，他的名是萬軍之耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩押變為荒場， 敵人上去進了她的城邑。 她所特選的少年人下去遭了殺戮； 這是君王－名為萬軍之耶和華說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moab has been destroyed and men have gone up to his cities; His choicest young men have also gone down to the slaughter,” Declares the King, whose name is the Lord of hosts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moab will be destroyed and her towns invaded; her finest young men will go down in the slaughter,” declares the King, whose name is the Lord Almighty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But now Moab and his towns will be destroyed. His most promising youth are doomed to slaughter,” says the King, whose name is the Lord of Heaven’s Armies."
      }
    },
