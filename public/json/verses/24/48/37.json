      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「各人頭上光禿，鬍鬚剪短，手有劃傷，腰束麻布。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For every head is bald and every beard cut short; there are gashes on all the hands and sackcloth on the loins."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Every head is shaved and every beard cut off; every hand is slashed and every waist is covered with sackcloth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The people shave their heads and beards in mourning. They slash their hands and put on clothes made of burlap."
      }
    },
