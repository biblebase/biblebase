    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.48.42",
            "paragraphs": [
              "四八42摩押地在主前第一世紀有拿巴提人（Nabataeans）居住，後來有亞拉伯人住在其上。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押必被摧毁，彻底灭亡， 因为她藐视耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩押必被摧毀，不再成為一個民族， 因為它向耶和華妄自尊大。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摩押必被毀滅，不再成國， 因她向耶和華誇大。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Moab will be destroyed from being a people Because he has become arrogant toward the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Moab will be destroyed as a nation because she defied the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Moab will no longer be a nation, for it has boasted against the Lord."
      }
