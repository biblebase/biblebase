    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.48.29-39",
            "paragraphs": [
              "四八29～39一度驕傲的摩押將要成為被嘲笑的對象（比較賽一六6）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "摩押的沃土上再没有欢喜快乐的声音。 我使榨酒池不出酒， 再没有踩踏葡萄的欢呼声， 欢呼被呐喊取代。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摩押肥沃的土地上不再有歡喜和快樂； 我使榨酒池不再流出酒； 再沒有人歡呼踹酒； 有人呼喊，卻不是踹酒的歡呼聲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "肥田和摩押地的歡喜快樂都被奪去； 我使酒醡的酒絕流， 無人踹酒歡呼； 那歡呼卻變為仇敵的吶喊。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So gladness and joy are taken away From the fruitful field, even from the land of Moab. And I have made the wine to cease from the wine presses; No one will tread them with shouting, The shouting will not be shouts of joy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Joy and gladness are gone from the orchards and fields of Moab. I have stopped the flow of wine from the presses; no one treads them with shouts of joy. Although there are shouts, they are not shouts of joy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Joy and gladness are gone from fruitful Moab. The presses yield no wine. No one treads the grapes with shouts of joy. There is shouting, yes, but not of joy."
      }
    },
