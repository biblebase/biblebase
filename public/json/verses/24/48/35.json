      "leili": {
        "content": [
          {
            "verse": "jer.48.29-39",
            "paragraphs": [
              "四八29～39一度驕傲的摩押將要成為被嘲笑的對象（比較賽一六6）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华说：“我要灭绝摩押那些在丘坛献祭和向假神烧香的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我要滅絕在摩押那些邱壇上獻祭和向他們的神燒香的。”這是耶和華的宣告。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說：「我必在摩押地使那在邱壇獻祭的，和那向他的神燒香的都斷絕了。"
      },
      "nasb": {
        "version_id": "NASB",
