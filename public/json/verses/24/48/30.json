    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "jer.48.29-39",
            "paragraphs": [
              "四八29～39一度驕傲的摩押將要成為被嘲笑的對象（比較賽一六6）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我知道狂傲的摩押不过是外强中干， 她的夸耀不过是虚张声势。 这是耶和华说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我洞悉它的狂傲 ， 它的狂傲毫無根據， 它的誇耀不能成事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華說： 我知道他的忿怒是虛空的； 他誇大的話一無所成。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I know his fury,” declares the Lord, “But it is futile; His idle boasts have accomplished nothing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I know her insolence but it is futile,” declares the Lord, “and her boasts accomplish nothing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I know about his insolence,” says the Lord, “but his boasts are empty— as empty as his deeds."
      }
    },
