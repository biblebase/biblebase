    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.30",
            "paragraphs": [
              "一帖補藥（十五30）",
              "眼有光或許可能是指朋友的臉上容光煥發（參，十六15）；若是如此，這則箴言的前後兩行必定是分別說到，人與事所能帶出的溫馨果效，參第13節，十七22，二十五25，也見創世記四十五27、28；以賽亞書五十二7、8。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.15.30",
            "paragraphs": [
              "一五30\"眼有光\"。即聽見好消息時眼睛所發出的光亮。\"使骨滋潤\"。就是使身體健壯。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "笑颜令人心喜， 喜讯滋润骨头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眼中的光采使人心快樂； 好消息使骨頭滋潤。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眼有光，使心喜樂； 好信息，使骨滋潤。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Bright eyes gladden the heart; Good news puts fat on the bones."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Light in a messenger’s eyes brings joy to the heart, and good news gives health to the bones."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A cheerful look brings joy to the heart; good news makes for good health."
      }
    },
