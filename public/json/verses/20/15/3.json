    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.3",
            "paragraphs": [
              "耶和華的眼目（十五3）",
              "歷代誌下十六9，同樣令人驚奇地以這個詞語說明了神的拯救旨意。我們正在討論的這節經文，則是指出這種遍察的範圍與持續性，第11節是論到它的洞悉力，第8、9節則是它的敏銳。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的眼目无所不在， 善人和恶人都被祂鉴察。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的眼目無所不在； 壞人好人他都鑒察。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的眼目無處不在； 惡人善人，他都鑒察。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The eyes of the Lord are in every place, Watching the evil and the good."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The eyes of the Lord are everywhere, keeping watch on the wicked and the good."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord is watching everywhere, keeping his eye on both the evil and the good."
      }
