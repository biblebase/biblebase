    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.26",
            "paragraphs": [
              "惹是生非與使人和睦（十五26）",
              "呂譯、思高、RSV將第二行的意思表明出來：“純潔的真言，上帝所喜悅”。第一行之“思想”（新譯、AV）的意思是“計謀”（呂譯；和合：惡謀），第二行相對的語句強調這樣的計謀是神所恨惡的，即使還沒有表諸於言語或行為都是如此。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华憎恨恶人的意念， 喜爱纯洁的言语。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "邪惡的思想是耶和華厭惡的； 恩慈的話卻是純淨的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡謀為耶和華所憎惡； 良言乃為純淨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Evil plans are an abomination to the Lord, But pleasant words are pure."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord detests the thoughts of the wicked, but gracious words are pure in his sight."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord detests evil plans, but he delights in pure words."
      }
    },
