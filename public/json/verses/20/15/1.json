    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.1",
            "paragraphs": [
              "柔和的回答（十五1）",
              "相關的參考經文，見分題研究：“Ⅲ絕妙之言”及附屬部分，原書第47～48頁。",
              "“一句暴戾的言語”（RSV）正確地表達出單數的含義（而不是像AV之復數），一句“傷人的”（呂譯，這也是希伯來文的含義）話語，就足以“激起忿怒”（呂譯、現中）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "温和的回答平息怒气， 粗暴的言词激起愤怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "柔和的回答使烈怒消退， 暴戾的話激動怒氣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "回答柔和，使怒消退； 言語暴戾，觸動怒氣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A gentle answer turns away wrath, But a harsh word stirs up anger."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A gentle answer turns away wrath, but a harsh word stirs up anger."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A gentle answer deflects anger, but harsh words make tempers flare."
      }
