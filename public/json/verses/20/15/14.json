    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.14",
            "paragraphs": [
              "尋求者與虛度光陰者（十五14）",
              "不相配的動詞加強了這個對比：尋求的決心，與隨便亂咬的吃（這個希伯來動詞指牛的放牧）；成對的名詞也值得研讀。關於這則箴言的應用，見十二11的註釋\\cf0。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "哲士渴慕知识， 愚人安于愚昧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "聰明人的心尋求知識， 愚昧人的口只吃愚妄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "聰明人心求知識； 愚昧人口吃愚昧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The mind of the intelligent seeks knowledge, But the mouth of fools feeds on folly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The discerning heart seeks knowledge, but the mouth of a fool feeds on folly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A wise person is hungry for knowledge, while the fool feeds on trash."
      }
    },
