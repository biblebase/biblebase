    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.13",
            "paragraphs": [
              "士氣（十五13）",
              "心是代表思想與態度，具有決定性的乃是這些，而不是環境。Knox：“快活的心，快活的外貌；悲傷的思想把靈壓碎。”參第15節，十七22，十八14。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.15.13",
            "paragraphs": [
              "一五13憂傷的思想能壓碎人的心靈。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "心中喜乐，容光焕发； 心里悲伤，精神颓丧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "喜樂的心使人臉上容光煥發； 心中愁苦使人精神頹喪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心中喜樂，面帶笑容； 心裏憂愁，靈被損傷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A joyful heart makes a cheerful face, But when the heart is sad, the spirit is broken."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A happy heart makes the face cheerful, but heartache crushes the spirit."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A glad heart makes a happy face; a broken heart crushes the spirit."
      }
    },
