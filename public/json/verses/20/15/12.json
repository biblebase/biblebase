    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.12",
            "paragraphs": [
              "向我們說委婉動聽的話吧！（十五12）",
              "受責備（和合、思高、RV、RSV）是正確的，不是“責備他的人”（新譯、呂譯，參，AV）。褻慢人其實並不是像他表面所裝出的那麽毫不懼怕。參亞哈與米該雅，列王紀上二十二8。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.15.12",
            "paragraphs": [
              "一五12\"褻慢人\"逃避智慧人，因為他討厭智慧人的責備。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "嘲讽者不爱听责备， 也不愿请教智者。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "好譏笑人的不喜愛責備他的人， 也不到智慧人那裡去。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "褻慢人不愛受責備； 他也不就近智慧人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A scoffer does not love one who reproves him, He will not go to the wise."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Mockers resent correction, so they avoid the wise."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Mockers hate to be corrected, so they stay away from the wise."
      }
