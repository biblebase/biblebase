    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.15.28",
            "paragraphs": [
              "負責任的發言（十五28）",
              "這節與第2節相近，顯明了智慧與公義一致，其關系何等密切。思量有沈思、默想的觀念在內（詩六十三6），故思高作“默思”，但在二十四2以同樣不辭勞苦的心態來描述惡人。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人三思而后答， 恶人张口吐恶言。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的心思想怎樣回答； 惡人的口發出惡言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人的心，思量如何回答； 惡人的口吐出惡言。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The heart of the righteous ponders how to answer, But the mouth of the wicked pours out evil things."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The heart of the righteous weighs its answers, but the mouth of the wicked gushes evil."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The heart of the godly thinks carefully before speaking; the mouth of the wicked overflows with evil words."
      }
    },
