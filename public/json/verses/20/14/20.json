    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "穷人遭邻舍厌， 富人朋友众多。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "窮人連鄰舍也厭惡他； 財主卻有很多人愛他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "貧窮人連鄰舍也恨他； 富足人朋友最多。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The poor is hated even by his neighbor, But those who love the rich are many."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The poor are shunned even by their neighbors, but the rich have many friends."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The poor are despised even by their neighbors, while the rich have many “friends.”"
      }
    },
