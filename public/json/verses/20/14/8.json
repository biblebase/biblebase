    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.8",
            "paragraphs": [
              "真正的通達與真正的愚昧（十四8）",
              "道在箴言書中的意思乃是“為人”，而不是“事業”，所以這句話乃是針對通達的精義所作的道德反思；但我們卻很容易將通達衰減為只有業務的含義。同樣的，愚昧的精義乃是內心的不誠實，不是只有欠缺真實（我們一定會以為只有這樣而已），而是逃避真實。下一則箴言不憚其煩地說明這一點。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.14.8",
            "paragraphs": [
              "一四8精明人小心地考慮他的行為和習慣，愚昧人卻回避有關其輕浮之生活方式的問題。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "明哲凭智慧辨道， 愚人被愚昧欺骗。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "精明人的智慧能分辨自己的道路； 愚昧人的愚妄卻欺騙自己。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "通達人的智慧在乎明白己道； 愚昧人的愚妄乃是詭詐。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wisdom of the sensible is to understand his way, But the foolishness of fools is deceit."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wisdom of the prudent is to give thought to their ways, but the folly of fools is deception."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The prudent understand where they are going, but fools deceive themselves."
      }
    },
