    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.29",
            "paragraphs": [
              "29.它的智慧。第一行的對句是第17a節，見該處的註釋。“高擡”（呂譯；和合、思高：大顯）的意思是“展現在公眾眼前”（參，現中“暴露”）或“促進”。參三35，但那裏乃是以人為受詞。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不轻易发怒者深明事理， 鲁莽急躁的人显出愚昧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不輕易動怒的，十分聰明； 輕易動怒的，大顯愚妄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不輕易發怒的，大有聰明； 性情暴躁的，大顯愚妄。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who is slow to anger has great understanding, But he who is quick-tempered exalts folly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever is patient has great understanding, but one who is quick-tempered displays folly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People with understanding control their anger; a hot temper shows great foolishness."
      }
    },
