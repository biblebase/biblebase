        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "行为正直的人敬畏耶和华， 行事邪僻的人轻视耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "行事正直的，敬畏耶和華； 行為乖僻的，卻藐視他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行動正直的，敬畏耶和華； 行事乖僻的，卻藐視他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who walks in his uprightness fears the Lord, But he who is devious in his ways despises Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever fears the Lord walks uprightly, but those who despise him are devious in their ways."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who follow the right path fear the Lord; those who take the wrong path despise him."
      }
