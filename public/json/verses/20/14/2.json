    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.2",
            "paragraphs": [
              "不是只用我們的嘴唇（十四2）",
              "這裏的“藐視”可能不是故意的，但真實的程度絲毫不減。每一次背離神的道路之行動，都是一個人的意誌落入陷阱中，也是一個人的判斷力違逆祂的判斷；但是它所導致的輕蔑，卻是太過不合理而無法承認。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "行为正直的人敬畏耶和华， 行事邪僻的人轻视耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "行事正直的，敬畏耶和華； 行為乖僻的，卻藐視他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行動正直的，敬畏耶和華； 行事乖僻的，卻藐視他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who walks in his uprightness fears the Lord, But he who is devious in his ways despises Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever fears the Lord walks uprightly, but those who despise him are devious in their ways."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who follow the right path fear the Lord; those who take the wrong path despise him."
      }
    },
