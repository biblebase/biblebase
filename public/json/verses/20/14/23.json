    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.23",
            "paragraphs": [
              "勤勞或多言（十四23）",
              "見分題研究：“言語”及附屬部分，原書第45～48頁。",
              "譯作益處（呂譯“贏余”）與窮乏（RSV“缺乏”）的希伯來文，同樣也出現在二十一5。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
