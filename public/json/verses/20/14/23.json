        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "殷勤工作，带来益处； 满嘴空谈，导致贫穷。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "一切勞苦都有益處， 嘴上空談引致貧窮。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "一切的劳苦都有益处； 嘴上的空谈必致缺乏。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "諸般勤勞都有益處； 嘴上多言乃致窮乏。"
      },
      "esv": {
        "version_id": "ESV",
