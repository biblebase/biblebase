    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.24",
            "paragraphs": [
              "冠冕或愚昧的帽子（十四24）",
              "呂譯、思高、現中、RSV都假設原文有兩個相當小的誤抄，故讀作“智慧人的精明是他們的冠冕；愚頑人的愚妄是他們的華冠”，這讀法有一些證據的支持。但希伯來文的經文（和合、AV、RV）仍是深具意義的：第一行與（如）八18一致，第二行則以完全相同之字眼的反覆來強調愚昧的人一無是處；愚昧就是愚昧本身的恥辱，也是它本身的收成（見第18節的註釋）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.14.24",
            "paragraphs": [
              "一四24智慧能帶來財富，愚昧人卻不能成就什麽好事。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智者以财富为冠冕， 愚人以愚昧为装饰。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "智慧人的冠冕是他們的財富； 愚昧人的愚妄，始終是愚妄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧人的財為自己的冠冕； 愚妄人的愚昧終是愚昧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The crown of the wise is their riches, But the folly of fools is foolishness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wealth of the wise is their crown, but the folly of fools yields folly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wealth is a crown for the wise; the effort of fools yields only foolishness."
      }
    },
