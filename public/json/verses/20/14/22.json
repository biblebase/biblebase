    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.14.22",
            "paragraphs": [
              "以其人之道還治其人之身（十四22）",
              "走入迷途，或“走迷了路”（呂譯），乃是論及他們被人誤導的計謀（參，第二行）；他們的道德無需置評。Moffatt末句的譯法令人激賞：“好心腸的人發現人都是仁慈而真實的。”"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "图谋恶事的步入歧途， 行善的受爱戴和拥护。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "圖謀惡事的，不是走錯了嗎？ 謀求善事的，必得慈愛和誠實。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "謀惡的，豈非走入迷途嗎？ 謀善的，必得慈愛和誠實。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will they not go astray who devise evil? But kindness and truth will be to those who devise good."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not those who plot evil go astray? But those who plan what is good find love and faithfulness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you plan to do evil, you will be lost; if you plan to do good, you will receive unfailing love and faithfulness."
      }
    },
