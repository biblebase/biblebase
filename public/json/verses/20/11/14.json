    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.14",
            "paragraphs": [
              "廣納諫言（十一14）",
              "“領導”（思高、現中、RSV）妙極地等同於來自船的索具之希伯來字，所以也指索具的掌握或操縱，參一5，與分題研究：“Ⅰ多面的智慧”第4點。一個人雖然可能會接受太多的意見（參，西底家王，耶三十八）；但輕易地將令人不安的聲音排拒在外，卻是極為致命的。這個主題也重復出現在十五22，二十18，二十四6。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.11.14",
            "paragraphs": [
              "一一14\"智謀\"。直譯作：掌舵，在人生旅程中有智慧地航行。\"安居\"。直譯作：拯救。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "缺乏智谋，国必败亡； 谋士众多，战无不胜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "沒有智謀，國家敗落； 謀士眾多，就能得勝。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無智謀，民就敗落； 謀士多，人便安居。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Where there is no guidance the people fall, But in abundance of counselors there is victory."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For lack of guidance a nation falls, but victory is won through many advisers."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Without wise leadership, a nation falls; there is safety in having many advisers."
      }
    },
