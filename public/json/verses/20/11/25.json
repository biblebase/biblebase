    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "慷慨好施的必得昌盛， 恩待他人的必蒙恩待。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "樂善好施的人，必得豐裕； 施惠於人的，自己也必蒙施惠。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "好施捨的，必得豐裕； 滋潤人的，必得滋潤。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The generous man will be prosperous, And he who waters will himself be watered."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A generous person will prosper; whoever refreshes others will be refreshed."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The generous will prosper; those who refresh others will themselves be refreshed."
      }
    },
