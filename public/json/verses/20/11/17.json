    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.17-19",
            "paragraphs": [
              "收割果效（十一17～19）"
            ]
          },
          {
            "verse": "pro.11.17",
            "paragraphs": [
              "17.行為影響行事之人最大（參十一29，十二20）。這個原則本身表現在人際關系（路六38）、性格（賽五十八10、11），與命運（雅二13）的領域中。這裏所說的仁慈乃是h]esed[（堅貞的愛），像神的愛一樣；而擾害有亞幹的故事（書七25、26）與以利亞所回敬給亞哈的罪名（王上十八17、18）為不吉利的前例；見十五6的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "仁慈的人造福自己， 残酷的人惹祸上身。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "仁慈的人自己獲益， 殘忍的人自己受害。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "仁慈的人，自己获益； 残忍的人，自己受害。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "仁慈的人善待自己； 殘忍的人擾害己身。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "A man who is kind benefits himself, but a cruel man hurts himself."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The merciful man does himself good, But the cruel man does himself harm."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who are kind benefit themselves, but the cruel bring ruin on themselves."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The merciful man does good for his own soul, But he  who  is cruel troubles his own flesh."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Your kindness will reward you, but your cruelty will destroy you."
      }
    },
