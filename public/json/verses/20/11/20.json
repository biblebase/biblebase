    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.20",
            "paragraphs": [
              "神所憎惡的或神所喜悅的（十一20）",
              "乖僻（和合、呂譯、AV）或“邪惡”（現中、思高、RV、RSV）基本的意義是“扭曲”，而它與第二行之“正直”（現中；和合：完全，呂譯“純全”，思高“無瑕”）的對比，偏向了Knox的譯法“虛假的”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "心术不正的人令耶和华憎恶， 纯全无过的人蒙耶和华喜爱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "欺詐的心是耶和華厭惡的； 行為完全是他所喜悅的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "心中乖僻的，為耶和華所憎惡； 行事完全的，為他所喜悅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The perverse in heart are an abomination to the Lord, But the blameless in their walk are His delight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord detests those whose hearts are perverse, but he delights in those whose ways are blameless."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord detests people with crooked hearts, but he delights in those with integrity."
      }
