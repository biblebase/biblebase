    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.28",
            "paragraphs": [
              "亨通──虛假的與真實的（十一28）",
              "第一個人有突然被提升的危險，第二個人則有生命與成長的韌力。",
              "RSV將跌倒（yippo{l）改成“雕落”（yibbo{l，參，現中），這是一個迷人的筆觸，但卻沒有經文的支持，事實上也沒有必要。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倚仗财势者必衰败， 义人必兴旺如绿叶。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "倚賴自己財富的，必然衰落； 義人卻必繁茂，好像綠葉。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "倚仗自己財物的，必跌倒； 義人必發旺，如青葉。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who trusts in his riches will fall, But the righteous will flourish like the green leaf."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who trust in their riches will fall, but the righteous will thrive like a green leaf."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Trust in your money and down you go! But the godly flourish like leaves in spring."
      }
    },
