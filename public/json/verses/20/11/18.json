    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.17-19",
            "paragraphs": [
              "收割果效（十一17～19）"
            ]
          },
          {
            "verse": "pro.11.18",
            "paragraphs": [
              "18.惟獨誠實能令人滿意。AV譯作“工作”的這個希伯來字，也有工價（和合、呂譯、現中、思高、RV、RSV）之意；這裏的第二行指出應該是後一個意思。Moffatt生動地譯作：“惡人所得的，不實在。”"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.11.18",
            "paragraphs": [
              "一一18\"得虛浮的工價\"。直譯作：獲得欺騙的報償。惡人的勞力得不祝福，也不能帶來永久的得益，義人卻相反，他會賺得恒久的賞賜。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人赚来的转眼成空， 播种公义的必获奖赏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人賺得的工價是虛假的， 播種公義的得著實在的賞賜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人經營，得虛浮的工價； 撒義種的，得實在的果效。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wicked earns deceptive wages, But he who sows righteousness gets a true reward."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A wicked person earns deceptive wages, but the one who sows righteousness reaps a sure reward."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Evil people get rich for the moment, but the reward of the godly will last."
      }
    },
