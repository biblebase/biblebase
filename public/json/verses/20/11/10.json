    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.11.10-11",
            "paragraphs": [
              "一一10,11義人的生活（傳播道德）和惡人的死亡（排除某些罪）對全\"城\"的人都有好的影響。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人亨通，全城快乐； 恶人灭亡，人人欢呼。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人亨通，全城歡樂； 惡人滅亡，大家歡呼。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人享福，合城喜樂； 惡人滅亡，人都歡呼。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When it goes well with the righteous, the city rejoices, And when the wicked perish, there is joyful shouting."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the righteous prosper, the city rejoices; when the wicked perish, there are shouts of joy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The whole city celebrates when the godly succeed; they shout for joy when the wicked die."
      }
