    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.3-9",
            "paragraphs": [
              "公義助人到底（十一3～9）",
              "第3～5節將公義與生命中復雜的抉擇連接起來，又將團體中的其余人與其危險連在一起。第3a、5a節一個驚人的例證乃是約瑟，他的純正乃是最佳的引導；但要緊的是：必須要做的誠實事必定是智慧的事（九10）。關於智慧所提供給人的安全（4、6～9節），見十2、3的註釋，並分題研究：“生命與死亡”及附屬部分，原書第53～57頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在降怒之日，财富毫无益处， 唯有公义能救人免于死亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在　神發怒的日子，財物毫無益處； 唯有公義能救人脫離死亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "發怒的日子資財無益； 惟有公義能救人脫離死亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Riches do not profit in the day of wrath, But righteousness delivers from death."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Wealth is worthless in the day of wrath, but righteousness delivers from death."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Riches won’t help on the day of judgment, but right living can save you from death."
      }
    },
