    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.3-9",
            "paragraphs": [
              "公義助人到底（十一3～9）",
              "第3～5節將公義與生命中復雜的抉擇連接起來，又將團體中的其余人與其危險連在一起。第3a、5a節一個驚人的例證乃是約瑟，他的純正乃是最佳的引導；但要緊的是：必須要做的誠實事必定是智慧的事（九10）。關於智慧所提供給人的安全（4、6～9節），見十2、3的註釋，並分題研究：“生命與死亡”及附屬部分，原書第53～57頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.11.7",
            "paragraphs": [
              "一一7\"罪人\"。直譯作：力量；即強人。無論惡人在世的時候多麽有力，死後他的盼望卻都消滅了。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人一死，他的希望就破灭， 他对财势的冀望转眼成空。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人一死，他的希望就幻滅； 有能力的人的盼望也消滅了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人一死，他的指望必滅絕； 罪人的盼望也必滅沒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When a wicked man dies, his expectation will perish, And the hope of strong men perishes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Hopes placed in mortals die with them; all the promise of their power comes to nothing."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the wicked die, their hopes die with them, for they rely on their own feeble strength."
      }
    },
