      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who despises his neighbor lacks sense, But a man of understanding keeps silent."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever derides their neighbor has no sense, but the one who has understanding holds their tongue."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is foolish to belittle one’s neighbor; a sensible person keeps quiet."
      }
