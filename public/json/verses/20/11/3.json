        "version_name": "新譯本",
        "lang": "cht",
        "text": "正直人的純正必引導他們自己， 奸詐人的奸惡卻毀滅自己。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正直人的純正必引導自己； 奸詐人的乖僻必毀滅自己。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The integrity of the upright will guide them, But the crookedness of the treacherous will destroy them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The integrity of the upright guides them, but the unfaithful are destroyed by their duplicity."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Honesty guides good people; dishonesty destroys treacherous people."
      }
    },
