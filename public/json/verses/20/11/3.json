    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.11.3-9",
            "paragraphs": [
              "公義助人到底（十一3～9）",
              "第3～5節將公義與生命中復雜的抉擇連接起來，又將團體中的其余人與其危險連在一起。第3a、5a節一個驚人的例證乃是約瑟，他的純正乃是最佳的引導；但要緊的是：必須要做的誠實事必定是智慧的事（九10）。關於智慧所提供給人的安全（4、6～9節），見十2、3的註釋，並分題研究：“生命與死亡”及附屬部分，原書第53～57頁。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.11.3",
            "paragraphs": [
              "一一3\"奸詐人\"。直譯作：行事詭詐的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "正直人靠诚实引导， 奸诈者被奸诈毁灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "正直人的純正必引導他們自己， 奸詐人的奸惡卻毀滅自己。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "正直人的純正必引導自己； 奸詐人的乖僻必毀滅自己。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The integrity of the upright will guide them, But the crookedness of the treacherous will destroy them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The integrity of the upright guides them, but the unfaithful are destroyed by their duplicity."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Honesty guides good people; dishonesty destroys treacherous people."
      }
    },
