    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.11",
            "paragraphs": [
              "寬宏大量（十九11）",
              "見識（和合、RSV）與榮耀指的是這個素養實際的與道德的價值；從大衛早年的歷史可以充足地說明這兩點。譯作榮耀的這個字，有時候也譯作“華美”（如：出二十八2）：它是暗示出裝飾，所以在這裏是說明美德鮮明的色彩，雖然它實際看起來可能不夠突出，令人覺得單調乏味。神自己宣告祂“全能的力量最主要是在於顯出憐憫與同情”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智者不轻易发怒， 饶恕是他的荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人的明慧使他不輕易動怒； 寬恕別人過失的，是自己的榮耀。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "人有见识就不轻易发怒； 宽恕过犯，是自己的荣耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人有見識就不輕易發怒； 寬恕人的過失便是自己的榮耀。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Good sense makes one slow to anger, and it is his glory to overlook an offense."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A man’s discretion makes him slow to anger, And it is his glory to overlook a transgression."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A person’s wisdom yields patience; it is to one’s glory to overlook an offense."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The discretion of a man makes him slow to anger, And his glory is to overlook a transgression."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Sensible people control their temper; they earn respect by overlooking wrongs."
      }
    },
