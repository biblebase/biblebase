    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.10",
            "paragraphs": [
              "珍珠與豬（十九10）",
              "“愉悅”（AV）在這裏的意思是宴樂（和合、RSV）。雖然神喜愛用恩典來讓不配之人感到驚奇，卻是要使他們更為尊貴。祂並不喜悅不搭調的事物。論及互相沖突之荒謬情況的其他箴言有十一22，十七7，二十六1，三十21～23。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.19.10",
            "paragraphs": [
              "一九10舒適和愉快會加強愚昧人愚昧荒唐的行徑。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愚人奢华宴乐不相宜， 奴隶管辖王子更离谱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愚昧人生活奢侈是不相稱的， 何況奴僕管轄領袖呢！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚昧人宴樂度日是不合宜的； 何況僕人管轄王子呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Luxury is not fitting for a fool; Much less for a slave to rule over princes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It is not fitting for a fool to live in luxury— how much worse for a slave to rule over princes!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It isn’t right for a fool to live in luxury or for a slave to rule over princes!"
      }
    },
