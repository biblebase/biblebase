    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.17",
            "paragraphs": [
              "“就是作在我身上了”（十九17）",
              "“他所給的”（AV）應該譯作“他的行為”（RSV；和合：他的善行）。這節經文所應許的是信實的回報，未必是得回他的金錢。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.19.17",
            "paragraphs": [
              "一九17對於貸款給貧窮人的人，耶和華必因他的善行而給他報償。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "善待穷人等于借贷给耶和华， 耶和华必回报他的善行。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "恩待窮人的，等於借錢給耶和華； 他所行的，耶和華必償還。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "憐憫貧窮的，就是借給耶和華； 他的善行，耶和華必償還。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "One who is gracious to a poor man lends to the Lord, And He will repay him for his good deed."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever is kind to the poor lends to the Lord, and he will reward them for what they have done."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you help the poor, you are lending to the Lord— and he will repay you!"
      }
    },
