    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.14",
            "paragraphs": [
              "14.耶和華是強調用詞，RV與RSV，恰當地將“但是”擺在前面（和合：惟有）。它暗示出這種恩賜是無與倫比的，是超過人的安排的。參十八22，至於賢慧的詳細解說，見三十一10～31。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "房屋钱财是祖先的遗产， 贤慧之妻乃耶和华所赐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "房屋與財富是祖宗遺留的產業； 唯有明慧的妻子是耶和華所賜的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "房屋錢財是祖宗所遺留的； 惟有賢慧的妻是耶和華所賜的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "House and wealth are an inheritance from fathers, But a prudent wife is from the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Houses and wealth are inherited from parents, but a prudent wife is from the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fathers can give their sons an inheritance of houses and wealth, but only the Lord can give an understanding wife."
      }
