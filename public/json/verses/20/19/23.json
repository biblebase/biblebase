    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.23",
            "paragraphs": [
              "敬虔的知足（十九23）",
              "參十一19，並分題研究：“生命與死亡”及附屬部分，原書第53～57頁。",
              "第二行的希伯來經文極度突兀，或許受到損壞。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.19.23",
            "paragraphs": [
              "一九23下更可作：以至他滿足地度過夜晚，不遭禍患。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "敬畏耶和华使人得享生命， 安然满足，免遭祸患。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "敬畏耶和華的，得著生命； 他必安居知足，不會遭受禍患。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "敬畏耶和华，就得到生命； 这样的人睡得饱足，不遭祸患。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "敬畏耶和華的，得着生命； 他必恆久知足，不遭禍患。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The fear of the Lord  leads to life, and whoever has it rests satisfied; he will not be visited by harm."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The fear of the Lord  leads to life, So that one may sleep satisfied, untouched by evil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The fear of the Lord leads to life; then one rests content, untouched by trouble."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The fear of the Lord  leads to life, And he  who  has  it will abide in satisfaction; He will not be visited with evil."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fear of the Lord leads to life, bringing security and protection from harm."
      }
    },
