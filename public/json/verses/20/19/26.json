    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.19.26",
            "paragraphs": [
              "無親情的兒子（十九26）",
              "第二行乍看之下似乎是令人泄氣的突降法，其實不是，因為“糟蹋”（和合：虐待，AV“使衰弱”）與攆出由於是從兒子而來，更顯得格外痛苦、陰暗。參以賽亞書一2沈痛的強調語句：“我養育兒女，將他們養大……”。見分題研究：“Ⅱ父母與兒女”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.19.26",
            "paragraphs": [
              "一九26\"虐待\"。更可作：攻擊。不聽從父母的教導是已經夠壞的了，虐待他們以及把他們趕走的做法有多可恥，就更不用說（提前五8）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "苛待父亲的人可鄙， 逼走母亲的人可耻。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "虐待父親，趕走母親的， 是貽羞可恥的兒子。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "虐待父親、攆出母親的， 是貽羞致辱之子。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who assaults his father and drives his mother away Is a shameful and disgraceful son."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever robs their father and drives out their mother is a child who brings shame and disgrace."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Children who mistreat their father or chase away their mother are an embarrassment and a public disgrace."
      }
    },
