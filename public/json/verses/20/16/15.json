    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.16.10-15",
            "paragraphs": [
              "權勢的重擔（十六10～15）"
            ]
          },
          {
            "verse": "pro.16.14-15",
            "paragraphs": [
              "14～15.王的權勢。這一組箴言可能是應用在其他百姓身上，多於王的身上，像第12、13節一樣。我們絕大多數人都擁有幾乎隨意可以造成某些人悲傷或快樂的力量，就像這裏所說的一樣，而且都可能扮演相當暴虐、令人不寒而栗的角色。至於屬下這方面，見十九12，二十2，二十五15的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.16.10-15",
            "paragraphs": [
              "一六10～15君王的責任。"
            ]
          },
          {
            "verse": "pro.16.15",
            "paragraphs": [
              "一六15\"春雲時雨\"。即春天的雨，能保證農作物的長成。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "王的笑容带给人生命， 他的恩惠像春雨之云。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "君王臉上欣悅的光采，使人得生命； 君王的恩寵好像春日雨雲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王的臉光使人有生命； 王的恩典好像春雲時雨。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "In the light of a king’s face is life, And his favor is like a cloud with the spring rain."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When a king’s face brightens, it means life; his favor is like a rain cloud in spring."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the king smiles, there is life; his favor refreshes like a spring rain."
      }
    },
