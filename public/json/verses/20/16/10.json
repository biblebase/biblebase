      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A divine decision is in the lips of the king; His mouth should not err in judgment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The lips of a king speak as an oracle, and his mouth does not betray justice."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king speaks with divine wisdom; he must never judge unfairly."
      }
