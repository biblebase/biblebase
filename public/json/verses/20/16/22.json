    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.16.22",
            "paragraphs": [
              "你必須力求自保（十六22）",
              "見分題研究：“生命與死亡”及附屬部分，原書第53～57頁。",
              "智慧（和合；呂譯、AV、RV“明智”）是s*ekel：“好的感知”（見分題研究：“Ⅰ多面的智慧”第3點，原書第33～34頁）；“訓誨”（AV）應該譯作懲治（和合、RSV），也就是說：愚昧人自找苦吃。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.16.22",
            "paragraphs": [
              "一六22\"生命的泉源\"。即屬靈生命力的源頭。\"懲治\"。借行為來教訓。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智慧是智者的生命泉， 愚昧为愚人带来惩罚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "明慧人的明慧是他生命的泉源； 愚妄人的懲罰就是愚妄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人有智慧就有生命的泉源； 愚昧人必被愚昧懲治。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Understanding is a fountain of life to one who has it, But the discipline of fools is folly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Prudence is a fountain of life to the prudent, but folly brings punishment to fools."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Discretion is a life-giving fountain to those who possess it, but discipline is wasted on fools."
      }
    },
