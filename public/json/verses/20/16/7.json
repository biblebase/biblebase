    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.16.1-9",
            "paragraphs": [
              "耶和華主宰的權柄（十六1～9）"
            ]
          },
          {
            "verse": "pro.16.7",
            "paragraphs": [
              "7.“神若幫助我們……”。聖經其他地方（如：約十五18起），顯示出這並非一條一成不變的定律，但卻是鼓勵人不要懼怕。“要顧念神的旨意，而不是人的意願；祂能處理你所懼怕的人！”參二十九25。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.16.1-9",
            "paragraphs": [
              "一六1～9本段的論題是神以及祂對人類活動最終的控制。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人所行的若蒙耶和华喜悦， 耶和华必使仇敌与他和好。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人所行的，若是蒙耶和華喜悅， 耶和華也使他的仇敵與他和好。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人所行的，若蒙耶和華喜悅， 耶和華也使他的仇敵與他和好。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When a man’s ways are pleasing to the Lord, He makes even his enemies to be at peace with him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the Lord takes pleasure in anyone’s way, he causes their enemies to make peace with them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When people’s lives please the Lord, even their enemies are at peace with them."
      }
