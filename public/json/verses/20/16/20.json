    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.16.20",
            "paragraphs": [
              "誠實與倚靠（十六20）",
              "AV（“智慧地處理一件事的”）同時稱贊效率與信心，是個可能的譯法。但RV與大部分現代譯本都正確地找到一個比較接近的平行句，采取希伯來文da{b[a{r（AV“事”；和合本：訓言）的首要含義“話”（留意這話的……）。這裏帶有詩歌的簡潔，故把“話”前面的定冠詞刪掉，就像十三13一樣，那裏顯然是指神的話語。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听从训言的人受益匪浅， 信靠耶和华的人蒙祝福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "留心聽訓言的必定得益； 倚靠耶和華的是有福的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "謹守訓言的，必得好處； 倚靠耶和華的，便為有福。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who gives attention to the word will find good, And blessed is he who trusts in the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever gives heed to instruction prospers, and blessed is the one who trusts in the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who listen to instruction will prosper; those who trust the Lord will be joyful."
      }
