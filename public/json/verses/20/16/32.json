    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不轻易发怒者胜过勇士， 能自我控制胜过攻陷城池。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不輕易動怒的，勝過勇士； 克服己心的，勝過把城攻取的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不輕易發怒的，勝過勇士； 治服己心的，強如取城。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who is slow to anger is better than the mighty, And he who rules his spirit, than he who captures a city."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Better a patient person than a warrior, one with self-control than one who takes a city."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Better to be patient than powerful; better to have self-control than to conquer a city."
      }
    },
