    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.16.27-30",
            "paragraphs": [
              "惹是生非的人（十六27～30）",
              "陰謀、惡言漫罵（27節），或只是耳語傳言（28節）；粗暴（29節）、或奸滑（30節）──這些都是足以散播傷害的途徑。見分題研究：“言語的力量”第2點，第46頁。"
            ]
          },
          {
            "verse": "pro.16.28",
            "paragraphs": [
              "28.播散（和合、RSV）比譯作“種”（AV）更好。這詞曾恰當地用來指，尾巴綁著火把的狐貍放在非利士人的禾稼中，士師記十五5；參箴言十七9。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "邪僻的人散播纷争， 造谣的人破坏友情。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "乖謬的人散播紛爭， 搬弄是非的離間親密的朋友。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "乖僻人播散紛爭； 傳舌的，離間密友。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A perverse man spreads strife, And a slanderer separates intimate friends."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A perverse person stirs up conflict, and a gossip separates close friends."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A troublemaker plants seeds of strife; gossip separates the best of friends."
      }
    },
