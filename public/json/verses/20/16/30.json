    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.16.27-30",
            "paragraphs": [
              "惹是生非的人（十六27～30）",
              "陰謀、惡言漫罵（27節），或只是耳語傳言（28節）；粗暴（29節）、或奸滑（30節）──這些都是足以散播傷害的途徑。見分題研究：“言語的力量”第2點，第46頁。"
            ]
          },
          {
            "verse": "pro.16.30",
            "paragraphs": [
              "30.緊合……緊閉（AV“動”）；RSV“閃爍示意……縮小”，像六13一樣，說明了意圖與禍害是可以不需要說只字片語就散播開來的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.16.30",
            "paragraphs": [
              "一六30\"眼目緊合\"。即眨眼，使眼色。\"嘴唇緊閉\"。即不說一句話便傳達了惡意。許多邪惡與乖僻都是不用說一句話便能傳播。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人眯起眼睛图谋不轨， 歹徒咬着嘴唇策划恶事。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "瞇著眼睛的，圖謀乖謬的事； 緊抿著嘴唇的，作成惡事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眼目緊合的，圖謀乖僻； 嘴唇緊閉的，成就邪惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who winks his eyes does so to devise perverse things; He who compresses his lips brings evil to pass."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever winks with their eye is plotting perversity; whoever purses their lips is bent on evil."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "With narrowed eyes, people plot evil; with a smirk, they plan their mischief."
      }
    },
