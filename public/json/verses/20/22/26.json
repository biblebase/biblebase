    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.22.26",
            "paragraphs": [
              "二二26\"擊掌\"。即確認一分協議。\"作保\"。同意在別人不履行責任的時候，使自己為他負上償還債務的責任。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要为人击掌作保， 不要为欠债的抵押。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要替別人擊掌擔保， 也不可為欠債的作保證人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "不要成为击掌作保的人， 也不要为债务担保；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要與人擊掌， 不要為欠債的作保。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Be not one of those who give pledges, who put up security for debts."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not be among those who give pledges, Among those who become guarantors for debts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not be one who shakes hands in pledge or puts up security for debts;"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Do not be one of those who shakes hands in a pledge, One of those who is surety for debts;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t agree to guarantee another person’s debt or put up security for someone else."
      }
