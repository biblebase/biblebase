    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.22.4",
            "paragraphs": [
              "他們必承受地土（二十二4）",
              "頭一個希伯來字（和合、RV：為賞賜；AV“藉著”）的意思是“……的結果”。因為神是神，自然會有這種結果──但卻是照著祂的時間。參二十一21；路十四11。（在謙卑之後沒有“和”；但補充這個字可能是正確的，勝於將下一個詞語指敬畏耶和華；和合本這1節經文的次序正好顛倒過來──當作是“謙卑”的定義，那是一個比較粗糙的結構。）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "心存谦卑、敬畏耶和华， 必得财富、尊荣和生命。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "謙卑和敬畏耶和華的賞賜， 就是財富、榮耀和生命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "敬畏耶和華心存謙卑， 就得富有、尊榮、生命為賞賜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The reward of humility and the fear of the Lord Are riches, honor and life."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Humility is the fear of the Lord; its wages are riches and honor and life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "True humility and fear of the Lord lead to riches, honor, and long life."
      }
