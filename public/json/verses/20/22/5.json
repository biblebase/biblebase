    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.22.5",
            "paragraphs": [
              "罪孽之輩的道路（二十二5）",
              "關於乖僻人（和合、AV、RV），見二14、15註釋。Moffatt譯得好：“走在崎嶇的路上，人就踏入網羅”，參十三15。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "奸徒之道有荆棘和陷阱， 想保全生命的必须远避。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "奸詐人的道路，滿布荊棘和網羅； 謹慎自己的，必遠離這些。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "乖僻人的路上有荊棘和網羅； 保守自己生命的，必要遠離。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Thorns and snares are in the way of the perverse; He who guards himself will be far from them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the paths of the wicked are snares and pitfalls, but those who would preserve their life stay far from them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Corrupt people walk a thorny, treacherous road; whoever values life will avoid it."
      }
    },
