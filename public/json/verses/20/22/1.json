    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.22.1",
            "paragraphs": [
              "名譽（二十二1）",
              "我們的主在路加福音十20對這個教訓作了進一步的發揮，祂指出：在一個更高的層次上，我們合宜的喜樂不是在於我們所掌握的權能，而是在於我們被握在其中的那愛。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.22.1",
            "paragraphs": [
              "二二1\"美名\"。美好的品格，以及隨之而來的榮譽。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "美名胜过巨富， 恩宠比金银宝贵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "名譽勝過多財， 恩寵勝過金銀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "美名勝過大財； 恩寵強如金銀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A good name is to be more desired than great wealth, Favor is better than silver and gold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A good name is more desirable than great riches; to be esteemed is better than silver or gold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Choose a good reputation over great riches; being held in high esteem is better than silver or gold."
      }
    },
