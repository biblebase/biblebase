        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "美名勝過大財； 恩寵強如金銀。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A good name is to be more desired than great wealth, Favor is better than silver and gold."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A good name is more desirable than great riches; to be esteemed is better than silver or gold."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Choose a good reputation over great riches; being held in high esteem is better than silver or gold."
      }
