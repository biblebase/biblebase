      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For why should you, my son, be exhilarated with an adulteress And embrace the bosom of a foreigner?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
