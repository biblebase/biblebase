    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.4",
            "paragraphs": [
              "國家的堅定（二十九4）",
              "第二行開始直譯為：“但供獻禮物之人”，也就是指意在賄賂之人。見第14節，與十五27的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "君王秉公行义，国家安定； 他若收受贿赂，国必倾倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "君王以公正使國堅立； 收受賄賂的，使國覆亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王藉公平，使國堅定； 索要賄賂，使國傾敗。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The king gives stability to the land by justice, But a man who takes bribes overthrows it."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "By justice a king gives a country stability, but those who are greedy for bribes tear it down."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A just king gives stability to his nation, but one who demands bribes destroys it."
      }
    },
