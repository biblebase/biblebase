    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.8",
            "paragraphs": [
              "惹是生非的人，使人和睦的人（二十九8）",
              "褻慢人（和合、AV、RV）指的是屬靈的（過於是社會上的）傲慢自大。AV之“引……進入網羅中”應該譯作煽惑（和合；參，RV、RSV）：紛爭結黨之煽動帶來一種擁有權力的快感；然而“和平的智慧”卻一定會作工，而且等候恰當的時機來到（見雅三13～18）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.29.8",
            "paragraphs": [
              "二九8\"煽惑通城\"。即煽動全城以至於混亂。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "狂徒煽动全城， 智者平息众怒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "好譏笑人的煽動全城騷亂， 智慧人卻止息眾怒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "褻慢人煽惑通城； 智慧人止息眾怒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Scorners set a city aflame, But wise men turn away anger."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Mockers stir up a city, but the wise turn away anger."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Mockers can get a whole town agitated, but the wise will calm anger."
      }
    },
