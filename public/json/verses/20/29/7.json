    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.7",
            "paragraphs": [
              "看顧沒有特權的人（二十九7）",
              "知道與“知識”（RSV）是論及個人所關註的事，這使得“知道”這個動詞在希伯來文中具有遠比中英文更為豐富的含義。第二行（直譯為“不明白知識”）在RSV中有很好的解釋──在“知識”之前加插了“這樣的”。這樣的看顧有一個極佳的例子，見於約伯記二十九12～17（也見二十二12的註釋）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.29.7",
            "paragraphs": [
              "二九7義人知道窮人的訴求，不敬虔的人沒有知識，不明白這樣的請求（直譯作：但惡人不明白知識）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人关心穷人的冤屈， 恶人对此漠不关心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人關注窮人的冤情； 惡人卻不分辨實情。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人知道查明窮人的案； 惡人沒有聰明，就不得而知。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The righteous is concerned for the rights of the poor, The wicked does not understand such concern."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The righteous care about justice for the poor, but the wicked have no such concern."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The godly care about the rights of the poor; the wicked don’t care at all."
      }
    },
