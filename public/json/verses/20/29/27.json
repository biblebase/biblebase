    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.27",
            "paragraphs": [
              "物以類聚（二十九27）",
              "共同的利益與在不同層面上相互的吸引，可能會遮蔽這種不和的現象；但沒有任何事物可以彌補它。在所羅門自己的誌業中，他所選擇的夥伴就顯露出他實際作抉擇的途徑（王上十一）。見哥林多後書六14～18。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为非作歹，义人厌恶； 行为正直，恶人憎恨。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不義的人是義人所厭惡的； 行為正直的人是惡人所厭惡的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為非作歹的，被義人憎嫌； 行事正直的，被惡人憎惡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "An unjust man is abominable to the righteous, And he who is upright in the way is abominable to the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The righteous detest the dishonest; the wicked detest the upright."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The righteous despise the unjust; the wicked despise the godly."
      }
    },
