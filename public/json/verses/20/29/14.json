    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.14",
            "paragraphs": [
              "人人效忠的君王（二十九14）",
              "參第4節，與十六12。要考驗一個掌權的人和他內在的力量，要看他對那些最不能向他施加壓力的人，是否仍舊誠實、忠心。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "君王若秉公审判穷人， 他的王位必永远坚立。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果君王誠實地審判窮人； 他的國位必永遠堅立。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "君王憑誠實判斷窮人； 他的國位必永遠堅立。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If a king judges the poor with truth, His throne will be established forever."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If a king judges the poor with fairness, his throne will be established forever."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If a king judges the poor fairly, his throne will last forever."
      }
    },
