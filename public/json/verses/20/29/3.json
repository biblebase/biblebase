    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.3",
            "paragraphs": [
              "父親的喜樂（二十九3）",
              "見二十八7，與十1的註釋。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "爱慕智慧的使父亲欢欣， 结交妓女的必耗尽钱财。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "喜愛智慧的，使父親喜樂； 親近妓女的，耗盡家財。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愛慕智慧的，使父親喜樂； 與妓女結交的，卻浪費錢財。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A man who loves wisdom makes his father glad, But he who keeps company with harlots wastes his wealth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A man who loves wisdom brings joy to his father, but a companion of prostitutes squanders his wealth."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The man who loves wisdom brings joy to his father, but if he hangs around with prostitutes, his wealth is wasted."
      }
    },
