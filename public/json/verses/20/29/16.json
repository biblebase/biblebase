    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.16",
            "paragraphs": [
              "惡人不及義人長命（二十九16）",
              "見第2節，與該處所列的參考經文。與和它同類的經文相較之下，這句話以其終結性的保證把這個主題更帶向前一步。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.29.16",
            "paragraphs": [
              "二九16義人會比惡人長命，並且看見他們跌倒。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人当道，罪恶泛滥； 义人必得见他们败亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人增多的時候，過犯也必增多； 義人必看見他們傾覆。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人加多，過犯也加多， 義人必看見他們跌倒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the wicked increase, transgression increases; But the righteous will see their fall."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the wicked thrive, so does sin, but the righteous will see their downfall."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the wicked are in authority, sin flourishes, but the godly will live to see their downfall."
      }
    },
