    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.29.24",
            "paragraphs": [
              "自殺性的共謀（二十九24）",
              "關於這整節經文，參一10～19那戲劇性的情節。",
              "第二行RV譯作：“他聽見嚴令……”，也就是說，他的合夥關系使他進一步涉及作假見證的罪（參，利五1）。這很有可能正是這則箴言的意思：“他其實是與小偷合夥，被傳喚來作證，卻不說什麽”（Hitzig，如托伊所撮述的）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.29.24",
            "paragraphs": [
              "二九24\"盜賊\"的同黨若拒絕發誓，就暗示他自己也是有罪的（比較利五1）。\"卻不言語\"。更可作：不透露任何事情。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "与盗贼为伍是憎恶自己， 他即使发誓也不敢作证。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "與盜賊分贓的，是恨惡自己的性命； 他雖然聽見要他起誓作證的聲音， 卻不說話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人與盜賊分贓，是恨惡自己的性命； 他聽見叫人發誓的聲音，卻不言語。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who is a partner with a thief hates his own life; He hears the oath but tells nothing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The accomplices of thieves are their own enemies; they are put under oath and dare not testify."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you assist a thief, you only hurt yourself. You are sworn to tell the truth, but you dare not testify."
      }
    },
