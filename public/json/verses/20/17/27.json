    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.17.27",
            "paragraphs": [
              "一七27\"寡少\"。抑制、束縛。\"溫良\"。直譯作：冷、冷靜；即沈。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谨言慎行的有知识， 温和冷静的有悟性。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有知識的約束自己的言語； 聰明人心平氣和。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "寡少言語的，有知識； 性情溫良的，有聰明。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who restrains his words has knowledge, And he who has a cool spirit is a man of understanding."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The one who has knowledge uses words with restraint, and whoever has understanding is even-tempered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A truly wise person uses few words; a person with understanding is even-tempered."
      }
    },
