      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "愚昧人既無聰明， 為何手拿價銀買智慧呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Why is there a price in the hand of a fool to buy wisdom, When he has no sense?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Why should fools have money in hand to buy wisdom, when they are not able to understand it?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "It is senseless to pay to educate a fool, since he has no heart for learning."
      }
    },
