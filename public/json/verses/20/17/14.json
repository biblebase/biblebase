    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.17.14",
            "paragraphs": [
              "怒氣的破壞力（十七14）",
              "“在它動亂之前”（AV）：更好是作“爆發之前”（RSV）。這個動詞只有再出現在十八1，二十3。這樣的水閘一旦打開，就不是一個人所能預料、控制，或補償的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "争端爆发如洪水决堤， 当在爆发前将其制止。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "紛爭的開始，如同決堤的水； 所以在爭執發生以前，就要制止。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "纷争的开始，就如放开的水； 卷入争执之前，当放弃争辩。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "紛爭的起頭如水放開， 所以，在爭鬧之先必當止息爭競。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The beginning of strife is like letting out water, so quit before the quarrel breaks out."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The beginning of strife is like letting out water, So abandon the quarrel before it breaks out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Starting a quarrel is like breaching a dam; so drop the matter before a dispute breaks out."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The beginning of strife is  like releasing water; Therefore stop contention before a quarrel starts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Starting a quarrel is like opening a floodgate, so stop before a dispute breaks out."
      }
    },
