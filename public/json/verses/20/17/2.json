    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.17.2",
            "paragraphs": [
              "才幹勝過特權（十七2）",
              "特權（可能成為障礙，也可能是個支持）在大部分人的心中顯得很重要，但在神心目中卻不是那麽重要，包括屬靈的事（摩九7；太八11、12）與世俗的事都是如此。所羅門的箴言很顯著地實行在他仆人與他兒子的事業上（王上十一28起）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.17.2",
            "paragraphs": [
              "一七2\"貽羞\"。行為可恥。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "精明的仆人必管辖主人的不肖子， 并与他们一同承受家业。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "明慧的僕人，必管轄主人貽羞的兒子； 又可以在眾兒子中同分產業。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "僕人辦事聰明，必管轄貽羞之子， 又在眾子中同分產業。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A servant who acts wisely will rule over a son who acts shamefully, And will share in the inheritance among brothers."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A prudent servant will rule over a disgraceful son and will share the inheritance as one of the family."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A wise servant will rule over the master’s disgraceful son and will share the inheritance of the master’s children."
      }
    },
