    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.17.10",
            "paragraphs": [
              "願意改進（十七10）",
              "見分題研究：“Ⅱ愚昧人”第1點，原書第37～39頁。",
              "“更多進入”（AV）：“更深穿透”（Moffatt），和合本作深入。",
              "參阿希家，XV（DOTT,p.272）：“用智慧話擊打一個人，好使它在他心中，有如夏季一樣發熱；……用許多杖擊打愚昧人，他必不領會它。”"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "责备哲士一句， 胜过杖打愚人百下。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "對聰明人說一句責備的話， 勝過責打愚昧人一百下。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "一句責備話深入聰明人的心， 強如責打愚昧人一百下。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A rebuke goes deeper into one who has understanding Than a hundred blows into a fool."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A rebuke impresses a discerning person more than a hundred lashes a fool."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A single rebuke does more for a person of understanding than a hundred lashes on the back of a fool."
      }
