    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.17.19",
            "paragraphs": [
              "自找麻煩（十七19）",
              "希伯來文的字序稍微偏向RSV、RV邊註的譯法，靈活一點地說，就是：“愛罪惡，愛爭競”，這與第二行一致，那裏說到傲慢自大（向著神與向著人都是如此）必受報應。參舍伯那的自負：以賽亞書二十二16起。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.17.19",
            "paragraphs": [
              "一七19\"高立家門\"。比喻人張開口說誇大、傲慢的話。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "喜爱争斗的喜爱犯罪， 骄傲自大的自招灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "喜愛爭競的就是喜愛過犯； 把家門建高的自取滅亡。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "喜爱纷争的，是喜爱过犯； 高立门户的，是自寻破败。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "喜愛爭競的，是喜愛過犯； 高立家門的，乃自取敗壞。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Whoever loves transgression loves strife; he who makes his door high seeks destruction."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who loves transgression loves strife; He who raises his door seeks destruction."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever loves a quarrel loves sin; whoever builds a high gate invites destruction."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He who loves transgression loves strife, And he who exalts his gate seeks destruction."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who loves to quarrel loves sin; anyone who trusts in high walls invites disaster."
      }
    },
