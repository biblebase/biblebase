    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.13.3",
            "paragraphs": [
              "3.供應彈藥給你的敵人。這種冒失莽撞的情形可能會表現在承諾、主見、開誠布公的言談上，敗亡可能是在經濟上、社會上、身體上，與靈性上的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说话谨慎，可保性命； 口无遮拦，自取灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "謹慎口舌的，可保性命； 口沒遮攔的，自取滅亡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "謹守口的，得保生命； 大張嘴的，必致敗亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The one who guards his mouth preserves his life; The one who opens wide his lips comes to ruin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who guard their lips preserve their lives, but those who speak rashly will come to ruin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who control their tongue will have a long life; opening your mouth can ruin everything."
      }
