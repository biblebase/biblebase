    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.13.18",
            "paragraphs": [
              "樂於接受批評（十三18）",
              "參一20～33就這個主題所作之廣泛的探討。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不受管教的贫穷羞愧， 接受责备的受到尊崇。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "輕忽管教的，必致窮乏受辱； 看重責備的，必得尊榮。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "忽视管教的，必受穷受辱； 接受责备的，必得尊荣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "棄絕管教的，必致貧受辱； 領受責備的，必得尊榮。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Poverty and disgrace come to him who ignores instruction, but whoever heeds reproof is honored."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Poverty and shame will come to him who neglects discipline, But he who regards reproof will be honored."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever disregards discipline comes to poverty and shame, but whoever heeds correction is honored."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Poverty and shame will  come to him who disdains correction, But he who regards a rebuke will be honored."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you ignore criticism, you will end in poverty and disgrace; if you accept correction, you will be honored."
      }
    },
