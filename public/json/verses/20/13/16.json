    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.13.16",
            "paragraphs": [
              "產品是人的宣傳（十三16）",
              "這兩種類型在十二23與十五2都有比較，但卻是在不同的層面上；那裏的對比是相反的，就是不張揚；這裏，一個人的特性（無論他自己是否隱藏它）顯然是顯示在他的一言一行中。（RSV以“通達人凡事都……”來代替凡通達人都……，並且有一些古譯本的支持；這是有可能的，只需要改變一個母音就可以了。）"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "明哲知而后行， 愚人炫耀愚昧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "精明的人都按知識行事， 愚昧人卻顯露自己的愚妄。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "聪明人都按知识行事， 愚昧人显露他的愚妄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡通達人都憑知識行事； 愚昧人張揚自己的愚昧。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Every prudent man acts with knowledge, but a fool flaunts his folly."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every prudent man acts with knowledge, But a fool displays folly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All who are prudent act with knowledge, but fools expose their folly."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Every prudent man acts with knowledge, But a fool lays open his folly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wise people think before they act; fools don’t—and even brag about their foolishness."
      }
    },
