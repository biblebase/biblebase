    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.13.13",
            "paragraphs": [
              "順服的報償（十三13）",
              "訓言與誡命乃是提醒我們：箴言中所預設的乃是一個啟示的宗教。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.13.13",
            "paragraphs": [
              "一三13\"訓言\"。即神的訓言。\"自取滅亡\"。直譯作：有誓言要他遵守，他有責任去遵從律法，惟恐他食言。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "蔑视训言，自招灭亡； 敬畏诫命，必得赏赐。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "藐視訓言的，必自取滅亡； 敬畏誡命的，必得善報。"
      },
      "cunp": {
        "version_id": "CUNP",
