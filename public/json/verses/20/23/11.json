    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.23.10-11",
            "paragraphs": [
              "二三10,11智慧的導師警告人不要剝削沒有防衛能力的窮人。"
            ]
          },
          {
            "verse": "pro.23.11",
            "paragraphs": [
              "二三11\"救贖主\"。直譯作：至近的親屬；即能保護家族利益的人。參看利未記二十五章25至28節和路得記三章9節的腳註。這用詞可能指神（參看創四八15,16；出六6；伯一九25～27的腳註。）"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因他们的救赎主强大， 祂必对付你，替他们申冤。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他們的救贖主大有能力， 他必向你為他們的案件伸冤。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因他們的救贖主大有能力， 他必向你為他們辨屈。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For their Redeemer is strong; He will plead their case against you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for their Defender is strong; he will take up their case against you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For their Redeemer is strong; he himself will bring their charges against you."
      }
