    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.23.22-25",
            "paragraphs": [
              "引以為榮的兒子（二十三22～25）",
              "這裏包含了第五條誡命的實際內容，直接的（22節）與間接的（23～25節）。也見第12～16節，並分題研究：“Ⅱ父母與兒女”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人的父亲喜乐无限， 智慧之子使父母欢欣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的父親必大有快樂； 生下智慧的兒子的，必因兒子歡喜。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人的父親必大得快樂； 人生智慧的兒子，必因他歡喜。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The father of the righteous will greatly rejoice, And he who sires a wise son will be glad in him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The father of a righteous child has great joy; a man who fathers a wise son rejoices in him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The father of godly children has cause for joy. What a pleasure to have children who are wise."
      }
