    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.23.29-35",
            "paragraphs": [
              "酒（二十三29～35）",
              "對酒鬼作了令人難以忘懷的論述：他在別人眼中看起來的情形（29節），與他眼中所看見的情形（33～35節）。他的想像力就跟他的雙腿一樣無法控制（34節）；如果他初次所受的誘惑有悲傷感（31節），那麽他最後的虛張聲勢就更傷感了（35節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.23.29-35",
            "paragraphs": [
              "二三29～35智慧的導師警告人不要醉酒的時候，說出一個酒徒的個案。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "谁有灾祸？谁有忧伤？ 谁有争吵？谁有怨言？ 谁无故受伤？谁两眼发红？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "誰有禍患？誰有愁苦？ 誰有紛爭？誰有怨言？ 誰無故受傷？ 誰的眼睛赤紅？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "谁有祸了？谁有痛苦？ 谁有争执？谁有苦情？ 谁无故受伤？谁的眼目赤红？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "誰有禍患？誰有憂愁？ 誰有爭鬥？誰有哀歎？ 誰無故受傷？誰眼目紅赤？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Who has woe? Who has sorrow? Who has strife? Who has complaining? Who has wounds without cause? Who has redness of eyes?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Who has woe? Who has sorrow? Who has contentions? Who has complaining? Who has wounds without cause? Who has redness of eyes?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Who has woe? Who has sorrow? Who has strife? Who has complaints? Who has needless bruises? Who has bloodshot eyes?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Who has woe? Who has sorrow? Who has contentions? Who has complaints? Who has wounds without cause? Who has redness of eyes?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Who has anguish? Who has sorrow? Who is always fighting? Who is always complaining? Who has unnecessary bruises? Who has bloodshot eyes?"
      }
