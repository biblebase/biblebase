    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.27",
            "paragraphs": [
              "良心（二十27）",
              "這個表象是一盞燈一個房間一個房間地傳遞，照亮陰暗的角落。參，30節，並該處的註釋。這裏的靈，直譯為“氣”，參創世記二7。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "心灵是耶和华的灯， 鉴察人的肺腑心肠。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人的靈是耶和華的燈， 探照人的臟腑。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人的靈是耶和華的燈， 鑒察人的心腹。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The spirit of man is the lamp of the Lord, Searching all the innermost parts of his being."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The human spirit is the lamp of the Lord that sheds light on one’s inmost being."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord’s light penetrates the human spirit, exposing every hidden motive."
      }
    },
