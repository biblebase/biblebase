    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.20",
            "paragraphs": [
              "不孝的行為（二十20）",
              "見分題研究：“Ⅱ父母與兒女”。現代人的習慣是評估人格過於身分（如：尊敬主人或忠於同伴，是因為他們能引起這樣的回應，而不是因為那是他們的地位所當得的）；舊約聖經的律法從孩提時代就慢慢灌輸以與現代人相反的態度，而新約聖經也支持這種作法（如：弗六1～9；彼前二13～18）。也見三十11、17。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "咒骂父母的人， 他的生命必消逝在黑暗中。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "咒罵父母的， 他的燈必在漆黑的幽暗中熄滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "咒罵父母的，他的燈必滅， 變為漆黑的黑暗。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who curses his father or his mother, His lamp will go out in time of darkness."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If someone curses their father or mother, their lamp will be snuffed out in pitch darkness."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you insult your father or mother, your light will be snuffed out in total darkness."
      }
    },
