    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.29",
            "paragraphs": [
              "少年與老年之美（二十29）",
              "老年人與少年人可能會向對方采取嫉妒、不耐煩與輕蔑的態度，這則箴言要提升讀者超越這種毫無益處的態度。每個年齡層都有它既定的優點，要適時予以尊重並享受，參十六31。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "力量是青年的荣耀， 白发是老人的尊荣。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "氣力是年輕人的榮耀， 白髮是老年人的尊榮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "強壯乃少年人的榮耀； 白髮為老年人的尊榮。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The glory of young men is their strength, And the honor of old men is their gray hair."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The glory of young men is their strength, gray hair the splendor of the old."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The glory of the young is their strength; the gray hair of experience is the splendor of the old."
      }
