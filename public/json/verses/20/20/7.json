    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.7",
            "paragraphs": [
              "父親最好的遺產（二十7）",
              "這是一個陳述（像RV、RSV一樣），而不是兩個，直譯為“一個行走在其純全中的人──福哉……”。它適用於應付“為了孩子的緣故”不計代價“追求成功”的試探。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人行为正直， 他的后代也蒙福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "行為完全的義人， 他的後代是有福的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行為純正的義人， 他的子孫是有福的！"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A righteous man who walks in his integrity— How blessed are his sons after him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The righteous lead blameless lives; blessed are their children after them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The godly walk with integrity; blessed are their children who follow them."
      }
