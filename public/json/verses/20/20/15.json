    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.15",
            "paragraphs": [
              "貴重的珍寶（二十15）",
              "見三14、15，並八10、11的註釋，但這裏的對比乃是各種的飾物，而不是財富。要受人公正地贊賞，就得學習捕捉人的耳朵，而不是吸引人的眼目；這件事比稀世的珍寶還要珍貴。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "智慧之言胜过黄金和许多珠宝。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "雖有金子、許多紅寶石和寶器， 但充滿知識的嘴唇比這一切更寶貴。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有金子和許多珍珠， 惟有知識的嘴乃為貴重的珍寶。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "There is gold, and an abundance of jewels; But the lips of knowledge are a more precious thing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Gold there is, and rubies in abundance, but lips that speak knowledge are a rare jewel."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wise words are more valuable than much gold and many rubies."
      }
    },
