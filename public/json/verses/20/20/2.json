    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.2",
            "paragraphs": [
              "不識時務（二十2）",
              "參十九12，但這裏的警告是明確的。AV的“懼怕”，意思是“所激起的懼怕”（參，和合：威嚇）。",
              "“得罪……性命”（AV）不是在神學含義上，而是（如，和合、RSV一樣）意味著“喪失他的性命”。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "君王震怒如狮子咆哮， 触怒他的人自寻死路。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "王的震怒好像獅子的吼叫； 觸怒他的是自害己命。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "王的威吓，如同少壮狮子的咆哮； 惹怒他的，会失去自己的性命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "王的威嚇如同獅子吼叫； 惹動他怒的，是自害己命。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The terror of a king is like the growling of a lion; whoever provokes him to anger forfeits his life."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The terror of a king is like the growling of a lion; He who provokes him to anger forfeits his own life."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A king’s wrath strikes terror like the roar of a lion; those who anger him forfeit their lives."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The wrath of a king is like the roaring of a lion; Whoever provokes him to anger sins against his own life."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The king’s fury is like a lion’s roar; to rouse his anger is to risk your life."
      }
    },
