    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.21",
            "paragraphs": [
              "最後的決算（二十21）",
              "這則箴言與十三11類似。後者所著重的是輕易得來之錢財固有的不穩定性；這裏則是著重因（所暗示之）不誠實而喪失神的祝福。也見下一節，並二十一5、6；二十八20、22。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "早年轻易得来之财， 最终不会成为祝福。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "起初迅速得來的產業， 最後卻不是福氣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "起初速得的產業， 終久卻不為福。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "An inheritance gained hurriedly at the beginning Will not be blessed in the end."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "An inheritance claimed too soon will not be blessed at the end."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "An inheritance obtained too early in life is not a blessing in the end."
      }
    },
