    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.20.19",
            "paragraphs": [
              "提防長舌的人（二十19）",
              "“諂媚”（AV）應該譯作“喃喃自語的人”（和合：大張嘴的），這個動詞與pet[i^（笨蛋、愚昧）有關。頭一行的論點是：他下一個要泄露的，可能就是你的密事。也見十一13。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "嚼舌的人泄露秘密， 不可结交多嘴的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "到處搬弄是非的，洩露祕密； 口無遮攔的，不可與他結交。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "到处搬弄是非的，泄露秘密； 口无遮拦的人，你不可与他结交。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "往來傳舌的，洩漏密事； 大張嘴的，不可與他結交。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Whoever goes about slandering reveals secrets; therefore do not associate with a simple babbler."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who goes about as a slanderer reveals secrets, Therefore do not associate with a gossip."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A gossip betrays a confidence; so avoid anyone who talks too much."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He who goes about as a talebearer reveals secrets; Therefore do not associate with one who flatters with his lips."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A gossip goes around telling secrets, so don’t hang around with chatterers."
      }
    },
