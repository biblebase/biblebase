    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.24.23-34",
            "paragraphs": [
              "Ⅲｂ智慧人的言語──續（二十四23～34）"
            ]
          },
          {
            "verse": "pro.24.30-34",
            "paragraphs": [
              "被制服的懶惰人（二十四30～34）",
              "參六6～11；見分題研究：“懶惰人”及附屬部分，原書第40～42頁。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.24.30-34",
            "paragraphs": [
              "二四30～34智慧的導師從懶惰人不理會葡萄園的個案來說明有關懶惰的教訓。他作出了觀察（30,31節）、解釋（32節）和應用（33,34節）。聰明人從懶惰人身上也能學到功課！"
            ]
          },
          {
            "verse": "pro.24.34",
            "paragraphs": [
              "二四34\"強盜\"。流浪者。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贫穷必像强盗一样临到你， 缺乏必像武士一样扑向你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的貧窮就必像強盜來到， 你的缺乏就必像拿兵器的人來到。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的貧窮就必如強盜速來， 你的缺乏彷彿拿兵器的人來到。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then your poverty will come as a robber And your want like an armed man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and poverty will come on you like a thief and scarcity like an armed man."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "then poverty will pounce on you like a bandit; scarcity will attack you like an armed robber."
      }
    },
