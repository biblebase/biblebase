      "leili": {
        "content": [
          {
            "verse": "pro.24.20",
            "paragraphs": [
              "二四20\"燈\"。即喜樂與興旺（比較斯八16）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为恶人毫无前途， 恶人的灯终必熄灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為惡人必沒有好結局， 惡人的燈必要熄滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，惡人終不得善報； 惡人的燈也必熄滅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For there will be no future for the evil man; The lamp of the wicked will be put out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for the evildoer has no future hope, and the lamp of the wicked will be snuffed out."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For evil people have no future; the light of the wicked will be snuffed out."
      }
    },
