    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.28.18",
            "paragraphs": [
              "無所隱藏，無所懼怕（二十八18）",
              "見類似的箴言，十9。立時（和合、AV、RV）直譯為“在一個之中”；也就是說：或是在一次的擊打中，或是在他所試著要結合的那“兩條路”中的一條（見第6節的註釋）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "纯全无过的必蒙拯救， 行为邪僻的转眼灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "行為完全的，必蒙拯救； 行事狡詐的，必立刻跌倒。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "行事纯全的，必得到拯救； 道路歪邪的，瞬间就仆倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "行動正直的，必蒙拯救； 行事彎曲的，立時跌倒。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Whoever walks in integrity will be delivered, but he who is crooked in his ways will suddenly fall."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who walks blamelessly will be delivered, But he who is crooked will fall all at once."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The one whose walk is blameless is kept safe, but the one whose ways are perverse will fall into the pit."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Whoever walks blamelessly will be saved, But he  who  is perverse in  his ways will suddenly fall."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The blameless will be rescued from harm, but the crooked will be suddenly destroyed."
      }
    },
