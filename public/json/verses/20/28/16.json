      "leili": {
        "content": [
          {
            "verse": "pro.28.16",
            "paragraphs": [
              "二八16\"貪財\"。更可作：不義的錢財。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "昏庸的君王残暴不仁， 恨不义之财的享长寿。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "昏庸的君主必多行強暴， 憎惡不義之財的，必享長壽。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "無知的君多行暴虐； 以貪財為可恨的，必年長日久。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A leader who is a great oppressor lacks understanding, But he who hates unjust gain will prolong his days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A tyrannical ruler practices extortion, but one who hates ill-gotten gain will enjoy a long reign."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A ruler with no understanding will oppress his people, but one who hates corruption will have a long life."
      }
