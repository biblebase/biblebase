        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人当道，人人躲藏； 恶人灭亡，义人增多。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人興起的時候，人就躲避； 惡人滅亡的時候，義人就增多。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人興起，人就躲藏； 惡人敗亡，義人增多。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the wicked rise, men hide themselves; But when they perish, the righteous increase."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the wicked rise to power, people go into hiding; but when the wicked perish, the righteous thrive."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the wicked take charge, people go into hiding. When the wicked meet disaster, the godly flourish."
      }
