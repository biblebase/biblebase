    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.28.24",
            "paragraphs": [
              "無人情味的兒子（二十八24）",
              "Knox譯作：“那搶劫父親或母親的豈能輕看這件事呢？他幾乎就是個殺人犯。”耶穌指出有人以高明的手法來犯這個罪（可七11）；參保羅在提摩太前書五4、8所說的有力話語。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.28.24",
            "paragraphs": [
              "二八24關於偷竊父母的罪，參看第十九章26節的腳註。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "窃取父母之财而不知罪者与匪类无异。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "偷竊自己父母財物， 並說：“這不是罪過”的， 這人是與強盜一夥。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "偷竊父母的，說：這不是罪， 此人就是與強盜同類。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who robs his father or his mother And says, “It is not a transgression,” Is the companion of a man who destroys."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever robs their father or mother and says, “It’s not wrong,” is partner to one who destroys."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who steals from his father and mother and says, “What’s wrong with that?” is no better than a murderer."
      }
    },
