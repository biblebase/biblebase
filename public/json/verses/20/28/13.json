    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.28.13",
            "paragraphs": [
              "埋藏罪惡，就保留罪惡（二十八13）",
              "舊約聖經對第一行所作的典型擴充是詩篇三十二1～4；而那首詩其余的部分則是第二行的詳細解釋。在新約聖經中，見約翰壹書一6～9。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.28.13",
            "paragraphs": [
              "二八13\"遮掩\"。即拒絕承認罪過（比較詩三二5）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "自掩其罪，必不亨通； 痛改前非，必蒙怜悯。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "遮掩自己過犯的，必不亨通； 承認並離棄過犯的，必蒙憐憫。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "遮掩自己罪過的，必不亨通； 承認離棄罪過的，必蒙憐恤。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who conceals his transgressions will not prosper, But he who confesses and forsakes them will find compassion."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever conceals their sins does not prosper, but the one who confesses and renounces them finds mercy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People who conceal their sins will not prosper, but if they confess and turn from them, they will receive mercy."
      }
