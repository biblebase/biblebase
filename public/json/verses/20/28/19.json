    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "勤奋耕耘，丰衣足食； 追求虚荣，穷困潦倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耕種自己田地的，必有充足糧食； 追求虛幻的，必飽受窮乏之苦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耕種自己田地的，必得飽食； 追隨虛浮的，足受窮乏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who tills his land will have plenty of food, But he who follows empty pursuits will have poverty in plenty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who work their land will have abundant food, but those who chase fantasies will have their fill of poverty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A hard worker has plenty of food, but a person who chases fantasies ends up in poverty."
      }
