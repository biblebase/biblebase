        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人得胜，遍地欢腾； 恶人当道，人人躲藏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人得勝的時候，有極大的榮耀； 惡人興起的時候，人就躲避。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人得志，有大榮耀； 惡人興起，人就躲藏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When the righteous triumph, there is great glory, But when the wicked rise, men hide themselves."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When the righteous triumph, there is great elation; but when the wicked rise to power, people go into hiding."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When the godly succeed, everyone is glad. When the wicked take charge, people go into hiding."
      }
