    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.25.19",
            "paragraphs": [
              "所托非人（二十五19）",
              "希伯來文容許我們將不忠誠的人理解為受托之人，或倚靠別人的人。每一種解法的含義都極為出色，但倚靠（和合、AV、RV）在其他地方傾向於支持後一個解釋（參，Moffatt，與和合、AV、RV、RSV的看法相反）：尤其參見約伯記八13～15。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.25.19",
            "paragraphs": [
              "二五19\"錯骨縫的腳\"。直譯作：滑倒的腳。錯置的信心能帶來煩惱與災難。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "危难时投靠奸诈之人， 形同倚靠坏牙和跛脚。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "遭遇災難的時候，倚靠奸詐的人， 就像倚靠壞牙和不穩的腳。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "患難時倚靠不忠誠的人， 好像破壞的牙，錯骨縫的腳。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like a bad tooth and an unsteady foot Is confidence in a faithless man in time of trouble."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like a broken tooth or a lame foot is reliance on the unfaithful in a time of trouble."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Putting confidence in an unreliable person in times of trouble is like chewing with a broken tooth or walking on a lame foot."
      }
    },
