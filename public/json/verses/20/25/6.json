    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.25.6",
            "paragraphs": [
              "二五6\"妄自尊大\"。直譯作：不尊重自己。那是一個宴會場合。不要坐在留給更尊貴之賓客的位置上（比較路一四7～11）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不可在王面前妄自尊大， 不要占据大人物的位子。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你在君王面前，不可妄自尊大； 也不可擅自站立在大人物中間。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要在王面前妄自尊大； 不要在大人的位上站立。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not claim honor in the presence of the king, And do not stand in the place of great men;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Do not exalt yourself in the king’s presence, and do not claim a place among his great men;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Don’t demand an audience with the king or push for a place among the great."
      }
    },
