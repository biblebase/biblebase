    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "对受教者而言， 智者的责备犹如金耳环和金饰物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "智慧人的責備在聽從的人耳中， 好像金耳環和精金的飾物。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "在听从者的耳中， 智慧的责备就如金环和黄金饰品。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "智慧人的勸戒，在順從的人耳中， 好像金耳環和精金的妝飾。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Like a gold ring or an ornament of gold is a wise reprover to a listening ear."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like an earring of gold and an ornament of fine gold Is a wise reprover to a listening ear."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like an earring of gold or an ornament of fine gold is the rebuke of a wise judge to a listening ear."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Like an earring of gold and an ornament of fine gold Is a wise rebuker to an obedient ear."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "To one who listens, valid criticism is like a gold earring or other gold jewelry."
      }
    },
