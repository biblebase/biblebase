    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.25.15",
            "paragraphs": [
              "安靜的堅持（二十五15）",
              "托伊提出反對的理由說：“一個人不會向君王表現出自制”。但這裏所稱贊的素養乃是拒絕被激動，其論點乃是：如此不顯眼的武器，可能會贏得令人驚奇的勝利（參，第二行；也參，如：撒上二十四17；彼前三15、16等處）。也見十五1，十六14、32。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.25.15",
            "paragraphs": [
              "二五15\"恒常忍耐\"。自制和溫柔的言語，對於固執的反對是有效的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "坚忍说服君王， 柔舌折断骨头。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "恆久忍耐可以勸服掌權的人， 柔和的舌頭，可以折斷骨頭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "恆常忍耐可以勸動君王； 柔和的舌頭能折斷骨頭。"
      },
      "nasb": {
