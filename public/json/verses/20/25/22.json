    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.25.21-22",
            "paragraphs": [
              "二五21,22善待敵人會使他感到羞愧，也給行善者帶來祝福。\"炭火\"能點燃仇敵的羞恥，也許還能叫他悔改。在埃及人的儀式裏，一個有罪的人，頭上要頂一盆炭火，以表示他的悔意。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为你这是把炭火堆在他头上， 耶和华必奖赏你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你這樣作，就是把炭火堆在他的頭上， 耶和華必還報你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為，你這樣行就是把炭火堆在他的頭上； 耶和華也必賞賜你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For you will heap burning coals on his head, And the Lord will reward you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In doing this, you will heap burning coals on his head, and the Lord will reward you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will heap burning coals of shame on their heads, and the Lord will reward you."
      }
    },
