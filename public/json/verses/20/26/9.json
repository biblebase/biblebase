        "text": "箴言在愚昧人口中， 就像荊棘在醉漢手裡。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "箴言在愚昧人的口中， 好像荊棘刺入醉漢的手。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like a thorn which falls into the hand of a drunkard, So is a proverb in the mouth of fools."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like a thornbush in a drunkard’s hand is a proverb in the mouth of a fool."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A proverb in the mouth of a fool is like a thorny branch brandished by a drunk."
      }
