    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.26.17-28",
            "paragraphs": [
              "惹是生非的人（二十六17～28）"
            ]
          },
          {
            "verse": "pro.26.20-28",
            "paragraphs": [
              "20～28.惡意的言談。這件惡事的不同層面引起了不同的描繪。",
              "20、21.炭。煽動余火的，乃是說悄悄話的人或好爭競的人自己，而不是（像他所說的）實話本身；因為他的心思把事實重新設計成燃料。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.26.17-28",
            "paragraphs": [
              "二六17～28這談及那卑鄙或胡鬧的人。"
            ]
          },
          {
            "verse": "pro.26.21",
            "paragraphs": [
              "二六21好爭論的人興旺爭辯的火。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "好斗之人煽动争端， 如同余火加炭、火上加柴。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "好爭競的人，煽動爭端， 就像餘燼上加炭，火上加柴一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "好爭競的人煽惑爭端， 就如餘火加炭，火上加柴一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Like charcoal to hot embers and wood to fire, So is a contentious man to kindle strife."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As charcoal to embers and as wood to fire, so is a quarrelsome person for kindling strife."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "A quarrelsome person starts fights as easily as hot embers light charcoal or fire lights wood."
      }
    },
