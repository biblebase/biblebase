    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.26.1-12",
            "paragraphs": [
              "大多是有關愚昧人的（二十六1～12）",
              "見分題研究：“Ⅱ愚昧人”第1點，原書第37～39頁。"
            ]
          },
          {
            "verse": "pro.26.6",
            "paragraphs": [
              "6.以愚昧人為使者。相反的情形，見二十五13。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.26.1-12",
            "paragraphs": [
              "二六1～12這段經文討論愚昧人。"
            ]
          },
          {
            "verse": "pro.26.6",
            "paragraphs": [
              "二六6\"砍斷自己的腳\"。即一個愚昧的報信者會有負發信人所托，以至他的目的不能達成。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "靠愚人传信， 如同砍断自己的脚， 自讨苦吃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "藉愚昧人的手傳遞信息的， 是自斷己足，自尋煩惱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "藉愚昧人手寄信的， 是砍斷自己的腳，自受損害。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
