    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "敬畏耶和华的享长寿， 恶人的寿数必被缩短。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "敬畏耶和華可以延年益壽， 惡人的年歲必被減少。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "敬畏耶和華使人日子加多； 但惡人的年歲必被減少。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The fear of the Lord prolongs life, But the years of the wicked will be shortened."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The fear of the Lord adds length to life, but the years of the wicked are cut short."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Fear of the Lord lengthens one’s life, but the years of the wicked are cut short."
      }
