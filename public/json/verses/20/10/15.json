    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.10.15",
            "paragraphs": [
              "別輕看金錢（十15）",
              "將本節與第2節，和第16節平衡起來。可能有人會告訴你沒有錢就算了；你當然必須評估金錢低於誠實，但別受這些話影響而輕看財富；別因著懶惰或浪漫主義而擁抱貧窮（關於必須面對之更進一步的冷酷的事實，見十四20，十八23，十九7，二十二7）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "钱财是富人的坚垒， 贫乏带给穷人毁灭。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "富翁的財物是他的堅城， 窮人的貧乏是他滅亡的因由。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "富戶的財物是他的堅城； 窮人的貧乏是他的敗壞。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The rich man’s wealth is his fortress, The ruin of the poor is their poverty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The wealth of the rich is their fortified city, but poverty is the ruin of the poor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wealth of the rich is their fortress; the poverty of the poor is their destruction."
      }
    },
