    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.10.32",
            "paragraphs": [
              "32.這裏的言論同時兼具恩典與真理，而乖僻不只被認為是與智慧相反（31節），也與能令人喜悅之事相反。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "义人说话得体合宜， 恶人的口胡言乱语。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "義人的嘴使人喜悅， 惡人的口說乖謬的話。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人的嘴能令人喜悅； 惡人的口說乖謬的話。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The lips of the righteous bring forth what is acceptable, But the mouth of the wicked what is perverted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The lips of the righteous know what finds favor, but the mouth of the wicked only what is perverse."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The lips of the godly speak helpful words, but the mouth of the wicked speaks perverse words."
      }
    },
