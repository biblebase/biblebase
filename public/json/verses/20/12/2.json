    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "善良的人蒙耶和华赐恩， 诡诈的人被耶和华定罪。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "良善的人必得到耶和華的恩寵； 圖謀奸計的人，耶和華必把他定罪。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "善人必蒙耶和華的恩惠； 設詭計的人，耶和華必定他的罪。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A good man will obtain favor from the Lord, But He will condemn a man who devises evil."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Good people obtain favor from the Lord, but he condemns those who devise wicked schemes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord approves of those who are good, but he condemns those who plan wickedness."
      }
    },
