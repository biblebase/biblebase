    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.1",
            "paragraphs": [
              "歡迎管教（十二1）",
              "如果你自認為是不屑接受批評的，你就不配得著批評。參第15節。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.12.1",
            "paragraphs": [
              "一二1\"管教\"。借言語和行為來教導。\"畜類\"。愚蠢的人。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "喜爱管教的喜爱知识， 厌恶责备的愚不可及。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "喜愛教訓的，就是喜愛知識； 厭惡責備的，卻是愚頑人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "喜愛管教的，就是喜愛知識； 恨惡責備的，卻是畜類。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whoever loves discipline loves knowledge, But he who hates reproof is stupid."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever loves discipline loves knowledge, but whoever hates correction is stupid."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "To learn, you must love discipline; it is stupid to hate correction."
      }
