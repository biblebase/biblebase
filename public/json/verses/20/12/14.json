    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.14",
            "paragraphs": [
              "14.言語可以像行為一樣帶來實質的回報，因為它們建立關系，並栽植觀念（見分題研究：“言語”及附屬部分，原書第45～48頁）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.12.14",
            "paragraphs": [
              "一二14言語能結果子，行為也一樣。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "口出良言，饱尝美福； 双手勤劳，终得回报。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人因口所結的果子必飽嘗美物； 人手所作的必歸到他身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人因口所結的果子，必飽得美福； 人手所做的，必為自己的報應。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A man will be satisfied with good by the fruit of his words, And the deeds of a man’s hands will return to him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From the fruit of their lips people are filled with good things, and the work of their hands brings them reward."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wise words bring many benefits, and hard work brings rewards."
      }
