    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.23",
            "paragraphs": [
              "慎思明辨（十二23）",
              "最後一個字有著額外的力量。單純的對比乃是在於能不能保持靜默；但愚昧人除了泄露他的秘密之外，還會使他自己原形畢露。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.12.23",
            "paragraphs": [
              "一二23精明人是謹慎和收斂的，愚昧人卻莽撞，顯出自己的無知。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "明哲不露锋芒， 愚人心吐愚昧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "精明的人隱藏自己的知識， 愚昧人的心卻顯出愚妄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "通達人隱藏知識； 愚昧人的心彰顯愚昧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A prudent man conceals knowledge, But the heart of fools proclaims folly."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The prudent keep their knowledge to themselves, but a fool’s heart blurts out folly."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The wise don’t make a show of their knowledge, but fools broadcast their foolishness."
      }
    },
