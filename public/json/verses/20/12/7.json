    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.7",
            "paragraphs": [
              "時間乃是考驗（十二7）",
              "頭一句最好或許是讀作（像RV邊註一樣）：“把惡人傾覆，他們就不存在了”。惡人天生之不安定的另一個有力的證據，參十25，十二3。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "恶人覆灭不复存在， 义人的家屹立不倒。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "惡人傾覆，就不再存在； 義人的家卻必站立得住。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "惡人傾覆，歸於無有； 義人的家必站得住。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The wicked are overthrown and are no more, But the house of the righteous will stand."
      },
      "niv": {
