    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华厌恶说谎的嘴， 祂喜爱行为诚实的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "撒謊的嘴唇是耶和華憎惡的； 行事誠實是他所喜悅的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說謊言的嘴為耶和華所憎惡； 行事誠實的，為他所喜悅。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Lying lips are an abomination to the Lord, But those who deal faithfully are His delight."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Lord detests lying lips, but he delights in people who are trustworthy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Lord detests lying lips, but he delights in those who tell the truth."
      }
