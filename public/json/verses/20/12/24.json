    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.12.24",
            "paragraphs": [
              "懶惰有其代價（十二24）",
              "苦工（和合、呂譯、AV），或更好是譯作“強迫勞力”（RSV），乃是所羅門太過熟悉以致極有資格去講論的一個論題。關於勤勞的賞賜，他所說的比他所能推斷的更為真實，見列王紀上十一28及下。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "殷勤的手必掌权， 懒惰者必做奴仆。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "殷勤人的手必掌權， 懶惰人的手必作苦工。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "殷勤人的手必掌權； 懶惰的人必服苦。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The hand of the diligent will rule, But the slack hand will be put to forced labor."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Diligent hands will rule, but laziness ends in forced labor."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Work hard and become a leader; be lazy and become a slave."
      }
    },
