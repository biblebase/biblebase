      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for riches do not endure forever, and a crown is not secure for all generations."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "for riches don’t last forever, and the crown might not be passed to the next generation."
      }
    },
