    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.27.11",
            "paragraphs": [
              "老師的喜樂（二十七11）",
              "參十1，與該處所列之參考經文。關於這個熱切的關懷，參帖撒羅尼迦前書二19、20，三8。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.27.11",
            "paragraphs": [
              "二七11名師希望他的門生都有智慧，以至沒有人批評他是差勁的老師。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "孩子啊，你要做智者， 使我的心喜乐， 我好回答讥笑我的人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我兒，你要作智慧人，使我的心快樂， 我也可以回答那羞辱我的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我兒，你要作智慧人，好叫我的心歡喜， 使我可以回答那譏誚我的人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Be wise, my son, and make my heart glad, That I may reply to him who reproaches me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Be wise, my son, and bring joy to my heart; then I can answer anyone who treats me with contempt."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Be wise, my child, and make my heart glad. Then I will be able to answer my critics."
      }
