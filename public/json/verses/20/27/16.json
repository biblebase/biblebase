    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.27.16",
            "paragraphs": [
              "二七16\"右手抓油\"。即右手抓油卻抓不住。本節的大意是：好爭吵的婦人（15節）是不能躲避，也不能制止的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "想制止她犹如制止狂风， 又像用手抓油一样困难。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "誰要攔阻她，就像攔阻風， 也像右手抓油。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "想要拦阻她的，就像拦阻风， 又像用右手去抓膏油。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "想攔阻她的，便是攔阻風， 也是右手抓油。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "to restrain her is to restrain the wind or to grasp oil in one’s right hand."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who would restrain her restrains the wind, And grasps oil with his right hand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "restraining her is like restraining the wind or grasping oil with the hand."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Whoever restrains her restrains the wind, And grasps oil with his right hand."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Stopping her complaints is like trying to stop the wind or trying to hold something with greased hands."
      }
