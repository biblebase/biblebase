    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.27.6",
            "paragraphs": [
              "二七6忠實的朋友會改正一個人的錯誤，而他的傷口也會復原；存心欺詐的仇敵會說好聽的話，卻不能提供真正的幫助。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "朋友加的创伤出自真诚， 仇敌连连亲吻却是多余。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "愛你的人加的創傷是出於忠誠； 恨你的人卻與你連連親吻。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "朋友加的傷痕出於忠誠； 仇敵連連親嘴卻是多餘。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Faithful are the wounds of a friend, But deceitful are the kisses of an enemy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Wounds from a friend can be trusted, but an enemy multiplies kisses."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Wounds from a sincere friend are better than many kisses from an enemy."
      }
    },
