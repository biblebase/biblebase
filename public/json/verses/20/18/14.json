    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.18.14",
            "paragraphs": [
              "主要動力（十八14）",
              "外在資源短缺，生活艱難；內在資源缺少，就難以為繼。所以，有人說得好：在為彼此禱告時，我們應該註意帖撒羅尼迦前書五23所提到的次序。",
              "參十二25，十五13。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.18.14",
            "paragraphs": [
              "一八14\"憂傷\"即破碎。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人的心灵能忍受疾病， 谁能忍受破碎的心灵？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人的心靈能忍受疾病， 心靈憂鬱，誰能承受呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人有疾病，心能忍耐； 心靈憂傷，誰能承當呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The spirit of a man can endure his sickness, But as for a broken spirit who can bear it?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The human spirit can endure in sickness, but a crushed spirit who can bear?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The human spirit can endure a sick body, but who can bear a crushed spirit?"
      }
    },
