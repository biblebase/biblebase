      "dde": {
        "content": [
          {
            "verse": "pro.18.13",
            "paragraphs": [
              "冒然下定論（十八13）",
              "這是自尊自大一個特別的網羅，第2節（見該處註釋的口（十八2）}）對此作了一個尖銳的註解，第17節則是進一步的考量。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "未听先答的人， 自显愚昧和羞辱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不先聆聽就回答的， 這就是他的愚妄和羞辱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "未曾聽完先回答的， 便是他的愚昧和羞辱。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who gives an answer before he hears, It is folly and shame to him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "To answer before listening— that is folly and shame."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Spouting off before listening to the facts is both shameful and foolish."
      }
