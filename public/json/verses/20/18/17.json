    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.18.17",
            "paragraphs": [
              "不可只聽片面之詞（十八17）",
              "RSV的譯法比古老的譯本更為清楚：先訴情由的似乎有理，但鄰舍來到，就細查他。這是本章中第三度對急促形成的見解提出警告（見2、13節）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.18.17",
            "paragraphs": [
              "一八17在作出判斷之前先聆聽雙方的陳述總是明智的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "先告状的看似有理， 一经对质真相大白。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "首先陳述情由的，似乎有理， 等他的鄰舍來到，才可以查明實情。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "争讼中先陈说的虽显得公义； 但对方一来，就能查验他的话。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "先訴情由的，似乎有理； 但鄰舍來到，就察出實情。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "The one who states his case first seems right, until the other comes and examines him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The first to plead his case seems right, Until another comes and examines him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In a lawsuit the first to speak seems right, until someone comes forward and cross-examines."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "The first one to plead his cause seems right, Until his neighbor comes and examines him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The first to speak in court sounds right— until the cross-examination begins."
      }
