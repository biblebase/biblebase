    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "pro.18.10",
            "paragraphs": [
              "一八10這裏\"耶和華的名\"代表耶和華自己。祂是義人的保護者。\"得安穩\"。直譯作：被舉起超越危險之上。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶和华的名是坚固保障， 义人投奔其中就得安稳。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶和華的名是堅固的高臺， 義人投奔，就得安全。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶和華的名是堅固臺； 義人奔入便得安穩。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The name of the Lord is a strong tower; The righteous runs into it and is safe."
