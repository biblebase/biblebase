      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you have been foolish in exalting yourself Or if you have plotted evil, put your hand on your mouth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If you play the fool and exalt yourself, or if you plan evil, clap your hand over your mouth!"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you have been a fool by being proud or plotting evil, cover your mouth in shame."
      }
