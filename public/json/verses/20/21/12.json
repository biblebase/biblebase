    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.21.12",
            "paragraphs": [
              "公義必要施行（二十一12）",
              "這句話直譯為：“義人（希伯來文s]addi^q）思想惡人的家，（而且）將眾惡人傾覆於災難之中”。如果我們把這個義人當作是神，這個譯法的含義就最容易明白。如此毫無修飾地使用形容詞，並不是陌生的：參約伯記三十四17，也見於“聖潔”在以賽亞書四十25；哈巴谷書三3；約伯記六10的類似用法。若不是這樣，我們就必須假定這或者是指公義的統治者，或隨意地解釋經文。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "公义的上帝鉴察恶人的家， 祂必使他们灭亡。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "公義的　神留意惡人的家， 使惡人在禍患中毀滅。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "義人思想惡人的家， 知道惡人傾倒，必致滅亡。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The righteous one considers the house of the wicked, Turning the wicked to ruin."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The Righteous One takes note of the house of the wicked and brings the wicked to ruin."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The Righteous One knows what is going on in the homes of the wicked; he will bring disaster on them."
      }
    },
