    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.21.16",
            "paragraphs": [
              "道德上的流浪癖（二十一16）",
              "第二行的每一個字都充滿了諷刺的味道，背逆者一定是隨意徘徊的，只是在急速喪失他的遷移率（“必要安頓”，RV）、他的獨立性（在……的會中，和合、AV、RV）與他的性命（陰魂的）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "pro.21.16",
            "paragraphs": [
              "二一16那些忽視智慧的人，不久便要來到墳墓，即死去。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人偏离明智之道， 终必与阴魂为伍。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "偏離明慧之道的人， 必住在陰魂的會中。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "迷離通達道路的， 必住在陰魂的會中。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A man who wanders from the way of understanding Will rest in the assembly of the dead."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever strays from the path of prudence comes to rest in the company of the dead."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The person who strays from common sense will end up in the company of the dead."
      }
    },
