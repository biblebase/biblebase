    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.21.13",
            "paragraphs": [
              "總會輪到他（二十一13）",
              "參二十四11、12，二十五21嚴厲的要求。淋漓盡致的解釋是財主與拉撒路的故事（路十六19～31），與馬太福音二十五31～46那幅審判的景象。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "掩耳不听穷人呼求的， 他将来求救也无人管。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "塞耳不聽貧窮人呼求的， 他將來呼求，也得不到應允。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "塞耳不聽窮人哀求的， 他將來呼籲也不蒙應允。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who shuts his ear to the cry of the poor Will also cry himself and not be answered."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Whoever shuts their ears to the cry of the poor will also cry out and not be answered."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Those who shut their ears to the cries of the poor will be ignored in their own time of need."
      }
    },
