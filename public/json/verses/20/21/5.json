    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.21.5",
            "paragraphs": [
              "周到才能奏效（二十一5）",
              "“思想”（AV、RV）應該譯作籌畫（和合、RSV），就如這個字常見的譯法一樣（如：十六3；耶二十九11）。急躁的希伯來文（參，二十八20）所暗示的乃是沖動，過於指不加思索。關於“迅速致富”的主題，也見下一節，並二十21的註釋（那裏用了一個不同的字眼）。關於豐裕（和合、AV、RV）與缺乏，參十四23。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "勤劳者的计划必致富足， 行事急躁的人必致贫困。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "殷勤人的籌劃必使他獲利； 行事急躁的必致貧窮。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "殷勤籌劃的，足致豐裕； 行事急躁的，都必缺乏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The plans of the diligent lead surely to advantage, But everyone who is hasty comes surely to poverty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The plans of the diligent lead to profit as surely as haste leads to poverty."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Good planning and hard work lead to prosperity, but hasty shortcuts lead to poverty."
      }
    },
