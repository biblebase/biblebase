    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "pro.21.2",
            "paragraphs": [
              "被稱在天平裏（二十一2）",
              "“仔細思量”（AV；和合：衡量）與十六2的“衡量”是同一個字，其實這句話就是那一句的重復。在我們的臆測與神的深知之間的對比，是那麽重要，值得一再地強調。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "人自以为行事正确， 但耶和华衡量人心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人看自己一切所行的，都是正直的； 耶和華卻衡量人心。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "人一切的道路，在自己眼中看为正直； 而耶和华衡量人的心。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人所行的，在自己眼中都看為正； 惟有耶和華衡量人心。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Every way of a man is right in his own eyes, but the Lord  weighs the heart."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every man’s way is right in his own eyes, But the Lord weighs the hearts."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A person may think their own ways are right, but the Lord weighs the heart."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Every way of a man is right in his own eyes, But the Lord weighs the hearts."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People may be right in their own eyes, but the Lord examines their heart."
      }
    },
