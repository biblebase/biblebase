    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为我已经把你赐给我的道赐给他们，他们也接受了，并且确实知道我是从你那里来的，也相信是你差遣了我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為你賜給我的話，我已經給了他們，他們也領受了，又確實知道我是從你那裡來的，並且信你差了我來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為你所賜給我的道，我已經賜給他們，他們也領受了，又確實知道，我是從你出來的，並且信你差了我來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "for the words which You gave Me I have given to them; and they received them and truly understood that I came forth from You, and they believed that You sent Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For I gave them the words you gave me and they accepted them. They knew with certainty that I came from you, and they believed that you sent me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "for I have passed on to them the message you gave me. They accepted it and know that I came from you, and they believe you sent me."
      }
    },
