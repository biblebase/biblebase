    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我不但为这些人祈求，也为那些因他们的话而信我的人祈求，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我不但為他們求，也為那些因他們的話而信我的人求，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我不但為這些人祈求，也為那些因他們的話信我的人祈求，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I do not ask on behalf of these alone, but for those also who believe in Me through their word;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“My prayer is not for them alone. I pray also for those who will believe in me through their message,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I am praying not only for these disciples but also for all who will ever believe in me through their message."
      }
    },
