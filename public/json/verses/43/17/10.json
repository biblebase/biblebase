        "text": "我的一切都是你的，你的一切也是我的，並且我因著他們得了榮耀。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "一切属于我的都是你的， 属于你的也是我的， 并且我在他们里面得了荣耀。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡是我的，都是你的；你的也是我的，並且我因他們得了榮耀。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "All mine are yours, and yours are mine, and I am glorified in them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and all things that are Mine are Yours, and Yours are Mine; and I have been glorified in them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "All I have is yours, and all you have is mine. And glory has come to me through them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And all Mine are Yours, and Yours are Mine, and I am glorified in them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "All who are mine belong to you, and you have given them to me, so they bring me glory."
      }
