    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一切属于我的都是你的，属于你的也是我的，我从他们身上得了荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
