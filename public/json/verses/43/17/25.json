    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.17.20-26",
            "paragraphs": [
              "二十五．耶穌為所有信徒禱告（一七20～26）"
            ]
          },
          {
            "verse": "jhn.17.25",
            "paragraphs": [
              "一七25世人體察不到耶穌已將神彰顯出來。但這裏的幾位門徒，卻體察得到，相信神差了耶穌來。在主被釘十字架的前夕，蕓蕓眾生之中，只有這幾顆忠信的心──但可惜連他們也將要離棄主！"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_sermon-2016-04-10",
        "title": "耶穌的禱告",
        "date": "2016-04-10",
        "preacher": "蕭旻奇弟兄",
        "verses": "約翰福音 17:1-26",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/2016/Sermon-2016-04-10.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/2016/Sermon-2016-04-10.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "公义的父啊！世界还未认识你以前，我已经认识你了，这些人也知道是你差我来的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "公義的父啊，世人雖然不認識你，我卻認識你，這些人也知道你差了我來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "公義的父啊，世人未曾認識你，我卻認識你；這些人也知道你差了我來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“O righteous Father, although the world has not known You, yet I have known You; and these have known that You sent Me;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Righteous Father, though the world does not know you, I know you, and they know that you have sent me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“O righteous Father, the world doesn’t know you, but I do; and these disciples know you sent me."
      }
    },
