    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说完后，就抬头望着天说：“父啊！时候到了，愿你使你的儿子得荣耀，好让你的儿子也使你得荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌講完了這些話，就舉目望天，說：“父啊，時候到了，求你榮耀你的兒子，讓兒子也榮耀你，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說了這話，就舉目望天，說：「父啊，時候到了，願你榮耀你的兒子，使兒子也榮耀你；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus spoke these things; and lifting up His eyes to heaven, He said, “Father, the hour has come; glorify Your Son, that the Son may glorify You,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After Jesus said this, he looked toward heaven and prayed: “Father, the hour has come. Glorify your Son, that your Son may glorify you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After saying all these things, Jesus looked up to heaven and said, “Father, the hour has come. Glorify your Son so he can give glory back to you."
      }
    },
