    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我不为世人祈求，却为他们祈求，就是为你赐给我的那些人祈求，因为他们属于你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我為他們求；我不為世人求，而是為你賜給我的人求，因為他們是你的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "我为他们祈求。 我不是为世人， 而是为你所赐给我的人祈求， 因为他们是你的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我為他們祈求，不為世人祈求，卻為你所賜給我的人祈求，因他們本是你的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "I am praying for them. I am not praying for the world but for those whom you have given me, for they are yours."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I ask on their behalf; I do not ask on behalf of the world, but of those whom You have given Me; for they are Yours;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I pray for them. I am not praying for the world, but for those you have given me, for they are yours."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“I pray for them. I do not pray for the world but for those whom You have given Me, for they are Yours."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“My prayer is not for the world, but for those you have given me, because they belong to you."
      }
