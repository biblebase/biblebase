    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我又将你赐给我的荣耀赐给他们，使他们像我们一样合而为一。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你賜給我的榮耀，我已經賜給了他們，使他們合而為一，像我們合而為一。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你所賜給我的榮耀，我已賜給他們，使他們合而為一，像我們合而為一。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The glory which You have given Me I have given to them, that they may be one, just as We are one;"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have given them the glory that you gave me, that they may be one as we are one—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I have given them the glory you gave me, so they may be one as we are one."
      }
