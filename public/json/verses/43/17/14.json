    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我已将你的道赐给他们，世人恨他们，因为他们像我一样不属于这个世界。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我把你的道賜給了他們；世人恨他們，因為他們不屬於這世界，像我不屬於這世界一樣。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我已將你的道賜給他們。世界又恨他們；因為他們不屬世界，正如我不屬世界一樣。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I have given them Your word; and the world has hated them, because they are not of the world, even as I am not of the world."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have given them your word and the world has hated them, for they are not of the world any more than I am of the world."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I have given them your word. And the world hates them because they do not belong to the world, just as I do not belong to the world."
      }
    },
