    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我就是门，从我进来的必定得救，并且会安然出入，找到草吃。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我就是門，如果有人藉著我進來，就必定得救，並且可以出、可以入，也可以找到草場。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我就是門；凡從我進來的，必然得救，並且出入得草吃。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I am the door; if anyone enters through Me, he will be saved, and will go in and out and find pasture."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am the gate; whoever enters through me will be saved.  They will come in and go out, and find pasture."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, I am the gate. Those who come in through me will be saved. They will come and go freely and will find good pastures."
      }
    },
