    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "圣经上的话是不能废的，既然上帝称那些承受祂道的人为神，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "聖經是不能廢除的，如果那些承受　神的道的人，　神尚且稱他們是神，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "經上的話是不能廢的；若那些承受神道的人尚且稱為神，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If he called them gods, to whom the word of God came (and the Scripture cannot be broken),"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If he called them ‘gods,’ to whom the word of God came—and Scripture cannot be set aside—"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And you know that the Scriptures cannot be altered. So if those people who received God’s message were called ‘gods,’"
      }
    },
