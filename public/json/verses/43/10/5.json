    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "羊从不跟随陌生人，见了陌生人就逃跑，因为它们不认得陌生人的声音。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠們決不會跟隨陌生人，反而逃避他，因為不認得陌生人的聲音。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "羊不跟着生人；因為不認得他的聲音， 必要逃跑。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "A stranger they simply will not follow, but will flee from him, because they do not know the voice of strangers.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But they will never follow a stranger; in fact, they will run away from him because they do not recognize a stranger’s voice.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They won’t follow a stranger; they will run from him because they don’t know his voice.”"
      }
