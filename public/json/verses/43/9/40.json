    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.9.13-41",
            "paragraphs": [
              "十．猶太人越來越對抗主（九13～41）"
            ]
          },
          {
            "verse": "jhn.9.40",
            "paragraphs": [
              "九40有些法利賽人知道主耶穌是在說他們，並說他們的眼瞎，故上前來，厚臉皮問祂是否暗示他們也瞎了眼。他們期待主回答“不是”。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%91%bd%e9%81%8b%e7%9a%84%e8%bd%89%e6%8d%a9%e9%bb%9e",
        "title": "命運的轉捩點",
        "date": "2005-07-17",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 9:1-41",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-19-2005-07-17.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-19-2005-07-17.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "有些跟祂在一起的法利赛人听了这句话，就问：“难道我们也瞎了眼吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有些和耶穌在一起的法利賽人聽了這話，就說：“難道我們也是瞎眼的嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "同他在那裏的法利賽人聽見這話，就說：「難道我們也瞎了眼嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Those of the Pharisees who were with Him heard these things and said to Him, “We are not blind too, are we?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Some Pharisees who were with him heard him say this and asked, “What? Are we blind too?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Some Pharisees who were standing nearby heard him and asked, “Are you saying we’re blind?”"
      }
    },
