    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.9.13-41",
            "paragraphs": [
              "十．猶太人越來越對抗主（九13～41）"
            ]
          },
          {
            "verse": "jhn.9.31",
            "paragraphs": [
              "九31那人作證愈來愈勇敢。他的信心也在增加。他提醒他們，總的說，神不聽罪人，也不藉他們行神跡。神不喜悅惡人，也不給他們能力行偉大的奇事。只有敬拜神的人才受神的稱許，更能得到神的悅納。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%91%bd%e9%81%8b%e7%9a%84%e8%bd%89%e6%8d%a9%e9%bb%9e",
        "title": "命運的轉捩點",
        "date": "2005-07-17",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 9:1-41",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-19-2005-07-17.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-19-2005-07-17.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我们知道上帝不听罪人的祷告，只听那些敬拜祂、遵行祂旨意者的祷告。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們知道　神不聽罪人的祈求，只聽那敬畏　神，遵行他旨意的人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們知道神不聽罪人，惟有敬奉神、遵行他旨意的，神才聽他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "We know that God does not hear sinners; but if anyone is God-fearing and does His will, He hears him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "We know that God does not listen to sinners. He listens to the godly person who does his will."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "We know that God doesn’t listen to sinners, but he is ready to hear those who worship him and do his will."
      }
    },
