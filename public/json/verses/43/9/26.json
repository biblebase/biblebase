    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们就问他：“祂向你做了些什么？祂是怎样医好你眼睛的？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們就問：“他向你作了甚麼呢？他怎樣開了你的眼睛呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們就問他說：「他向你做甚麼？是怎麼開了你的眼睛呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they said to him, “What did He do to you? How did He open your eyes?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they asked him, “What did he do to you? How did he open your eyes?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“But what did he do?” they asked. “How did he heal you?”"
      }
    },
