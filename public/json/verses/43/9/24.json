    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "法利赛人又把那个从前失明的人叫来，对他说：“你应该把荣耀归给上帝！我们知道那个人是罪人。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是法利賽人第二次把那從前瞎眼的人叫來，對他說：“你應當歸榮耀給　神，我們知道這人是個罪人。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他们再次把那原来瞎眼的人叫来，对他说：“你要把荣耀归给神！我们知道那人是个罪人。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "所以法利賽人第二次叫了那從前瞎眼的人來，對他說：「你該將榮耀歸給神，我們知道這人是個罪人。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So for the second time they called the man who had been blind and said to him, “Give glory to God. We know that this man is a sinner.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So a second time they called the man who had been blind, and said to him, “Give glory to God; we know that this man is a sinner.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "A second time they summoned the man who had been blind. “Give glory to God by telling the truth,” they said. “We know this man is a sinner.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So they again called the man who was blind, and said to him, “Give God the glory! We know that this Man is a sinner.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So for the second time they called in the man who had been blind and told him, “God should get the glory for this, because we know this man Jesus is a sinner.”"
      }
