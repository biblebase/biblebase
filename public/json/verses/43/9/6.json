    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣讲完后，便吐唾沫在地上，用唾沫和泥抹在那盲人的眼睛上，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說了這話，就吐唾沫在地上，用唾沫和了一點泥，把泥抹在瞎子的眼睛上，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說了這話，就吐唾沫在地上，用唾沫和泥抹在瞎子的眼睛上，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He had said this, He spat on the ground, and made clay of the spittle, and applied the clay to his eyes,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After saying this, he spit on the ground, made some mud with the saliva, and put it on the man’s eyes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he spit on the ground, made mud with the saliva, and spread the mud over the blind man’s eyes."
      }
    },
