    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.9.13-41",
            "paragraphs": [
              "十．猶太人越來越對抗主（九13～41）"
            ]
          },
          {
            "verse": "jhn.9.32-33",
            "paragraphs": [
              "九32，33這人知道自己是人類史上第一個生來是瞎子的能重見光明的人。他不明白法利賽人眼見如此的神跡，為何仍不滿、挑剔行了神跡的人。",
              "若主耶穌不是從神來的，祂斷不能行這樣的一個神跡。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%91%bd%e9%81%8b%e7%9a%84%e8%bd%89%e6%8d%a9%e9%bb%9e",
        "title": "命運的轉捩點",
        "date": "2005-07-17",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 9:1-41",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-19-2005-07-17.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-19-2005-07-17.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从创世以来，从未听过有人能把天生失明的人医好。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "自古以來，沒有人聽過生下來就是瞎眼的，有人可以開他們的眼睛。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從創世以來，未曾聽見有人把生來是瞎子的眼睛開了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Since the beginning of time it has never been heard that anyone opened the eyes of a person born blind."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nobody has ever heard of opening the eyes of a man born blind."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Ever since the world began, no one has been able to open the eyes of someone born blind."
      }
    },
