    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说完这番话，耶稣心里忧伤，便宣布说：“我实实在在地告诉你们，你们中间有一个人要出卖我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說了這話，心裡很難過，就明明地說：“我實實在在告訴你們，你們中間有一個人要出賣我。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "说完这些话，耶稣灵里烦扰，就指名说：“我确确实实地告诉你们：你们当中有一个人要出卖我。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說了這話，心裏憂愁，就明說：「我實實在在地告訴你們，你們中間有一個人要賣我了。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "After saying these things, Jesus was troubled in his spirit, and testified, “Truly, truly, I say to you, one of you will betray me.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When Jesus had said this, He became troubled in spirit, and testified and said, “Truly, truly, I say to you, that one of you will betray Me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After he had said this, Jesus was troubled in spirit and testified, “Very truly I tell you, one of you is going to betray me.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "When Jesus had said these things, He was troubled in spirit, and testified and said, “Most assuredly, I say to you, one of you will betray Me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Now Jesus was deeply troubled, and he exclaimed, “I tell you the truth, one of you will betray me!”"
      }
    },
