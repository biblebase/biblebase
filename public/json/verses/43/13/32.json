    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "上帝既然在人子身上得了荣耀，也要让人子在祂身上得荣耀，并且马上要使人子得荣耀。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也要在自己身上榮耀人子，並且要立刻榮耀他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "神要因自己榮耀人子，並且要快快地榮耀他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "if God is glorified in Him, God will also glorify Him in Himself, and will glorify Him immediately."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If God is glorified in him,  God will glorify the Son in himself, and will glorify him at once."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And since God receives glory because of the Son, he will give his own glory to the Son, and he will do so at once."
      }
