    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣给门徒洗完了脚，便穿上外衣再次坐下来，然后问道：“我为你们做的，你们明白吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌洗完了門徒的腳，就穿上外衣，再坐下來，對他們說：“我給你們作的，你們明白嗎？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "耶稣洗完了他们的脚以后，穿上外衣，又坐下来，对他们说：“你们明白我为你们做的事吗？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌洗完了他們的腳，就穿上衣服，又坐下，對他們說：「我向你們所做的，你們明白嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When he had washed their feet and put on his outer garments and resumed his place, he said to them, “Do you understand what I have done to you?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So when He had washed their feet, and taken His garments and reclined at the table again, He said to them, “Do you know what I have done to you?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When he had finished washing their feet, he put on his clothes and returned to his place. “Do you understand what I have done for you?” he asked them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So when He had washed their feet, taken His garments, and sat down again, He said to them, “Do you know what I have done to you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After washing their feet, he put on his robe again and sat down and asked, “Do you understand what I was doing?"
      }
    },
