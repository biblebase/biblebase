    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣知道父将一切交给了祂，也知道自己从上帝那里来、要回到上帝那里，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌知道父已經把萬有交在他手中，並且知道自己從　神而來，又要回到　神那裡去，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌知道父已將萬有交在他手裏，且知道自己是從神出來的，又要歸到神那裏去，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus, knowing that the Father had given all things into His hands, and that He had come forth from God and was going back to God,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus knew that the Father had put all things under his power, and that he had come from God and was returning to God;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus knew that the Father had given him authority over everything and that he had come from God and would return to God."
      }
