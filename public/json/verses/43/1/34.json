    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.1.19-34",
            "paragraphs": [
              "一．施洗約翰的見證（一19～34）"
            ]
          },
          {
            "verse": "jhn.1.34",
            "paragraphs": [
              "一34根據他為耶穌洗禮時的情景，約翰證明拿撒勒人耶穌就是神的兒子，意即子神。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
