    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约翰又做见证说：“我看见圣灵好像鸽子一样从天降下，住在祂身上。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約翰又作見證說：“我曾看見聖靈，好像鴿子從天上降下來，停留在他的身上。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約翰又作見證說：「我曾看見聖靈，彷彿鴿子從天降下，住在他的身上。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "John testified saying, “I have seen the Spirit descending as a dove out of heaven, and He remained upon Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then John gave this testimony: “I saw the Spirit come down from heaven as a dove and remain on him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then John testified, “I saw the Holy Spirit descending like a dove from heaven and resting upon him."
      }
    },
