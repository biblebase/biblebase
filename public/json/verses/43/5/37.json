      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "差我来的父曾亲自为我做过见证。你们从未听过祂的声音，从未见过祂，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "差我來的父親自為我作了見證。他的聲音，你們從沒有聽過；他的容貌，你們從沒有見過；"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "派我来的父也亲自为我做了见证。你们从来没有听到他的声音，也没有看到他的相貌，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "差我來的父也為我作過見證。你們從來沒有聽見他的聲音，也沒有看見他的形像。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "And the Father who sent me has himself borne witness about me. His voice you have never heard, his form you have never seen,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And the Father who sent Me, He has testified of Me. You have neither heard His voice at any time nor seen His form."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And the Father who sent me has himself testified concerning me. You have never heard his voice nor seen his form,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And the Father Himself, who sent Me, has testified of Me. You have neither heard His voice at any time, nor seen His form."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the Father who sent me has testified about me himself. You have never heard his voice or seen him face to face,"
      }
    },
