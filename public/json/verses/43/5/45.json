    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要以为我会在父面前控告你们，其实控告你们的是你们一直信赖的摩西。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要以為我要向父控告你們，有一位控告你們的，就是你們所仰賴的摩西。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不要想我在父面前要告你們；有一位告你們的，就是你們所仰賴的摩西。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not think that I will accuse you before the Father; the one who accuses you is Moses, in whom you have set your hope."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“But do not think I will accuse you before the Father. Your accuser is Moses, on whom your hopes are set."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yet it isn’t I who will accuse you before the Father. Moses will accuse you! Yes, Moses, in whom you put your hopes."
      }
    },
