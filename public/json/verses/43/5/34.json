    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其实我并不需要人的见证，我之所以提起这些事是为了使你们得救。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我不接受從人而來的見證，但我說這些事，是要你們得救。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其實，我所受的見證不是從人來的；然而，我說這些話，為要叫你們得救。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the testimony which I receive is not from man, but I say these things so that you may be saved."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Not that I accept human testimony; but I mention it that you may be saved."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Of course, I have no need of human witnesses, but I say these things so you might be saved."
      }
    },
