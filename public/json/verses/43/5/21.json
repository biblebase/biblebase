    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "父如何使死人复活、赐生命给他们，子也照样想赐生命给谁，就赐给谁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "父怎樣叫死人復活，使他們得生命，子也照樣隨自己的意思使人得生命。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "父怎樣叫死人起來，使他們活着，子也照樣隨自己的意思使人活着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For just as the Father raises the dead and gives them life, even so the Son also gives life to whom He wishes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For just as the Father raises the dead and gives them life, even so the Son gives life to whom he is pleased to give it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For just as the Father gives life to those he raises from the dead, so the Son gives life to anyone he wants."
      }
    },
