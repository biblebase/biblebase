    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.49",
            "paragraphs": [
              "八49耶穌回答這群敵視祂的人，用的竟是溫婉的\\cs9說話。祂的教訓不是一個被鬼附的人能說的。說這教訓的人，尋求尊敬父神。猶太人輕慢耶穌，不是因為祂是個瘋子，而是因為祂專心一意的為天上的父工作。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“我没有被鬼附身。我尊敬我的父，你们却侮辱我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌回答：“我不是鬼附的。我尊敬我的父，你們卻侮辱我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「我不是鬼附着的；我尊敬我的父，你們倒輕慢我。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus answered, “I do not have a demon; but I honor My Father, and you dishonor Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I am not possessed by a demon,” said Jesus, “but I honor my Father and you dishonor me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“No,” Jesus said, “I have no demon in me. For I honor my Father—and you dishonor me."
      }
    },
