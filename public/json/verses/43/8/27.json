    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.27",
            "paragraphs": [
              "八27猶太人不明白耶穌剛才所說的話是指父神說的。他們的心思常被蒙蔽。在此之前，耶穌自稱是神的兒子，他們還能意會祂將自己與父神看作平等，但現在已不能明白了。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们不明白耶稣是指着父说的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們不明白耶穌是對他們講論父的事。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們不明白耶穌是指着父說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They did not realize that He had been speaking to them about the Father."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They did not understand that he was telling them about his Father."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But they still didn’t understand that he was talking about his Father."
      }
