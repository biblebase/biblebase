    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.37",
            "paragraphs": [
              "八37主耶穌承認，以血統來說，這些猶太人確是亞伯拉罕的子孫（直譯為“後裔”）。但他們卻不是亞伯拉罕的屬靈後裔。他們不像亞伯拉罕般虔誠，想要殺害主耶穌，因為主耶穌的教訓他們容不下。這是說他們不讓基督的道掌管他們的生命，又抗拒祂的教訓，並不服從祂。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我知道你们是亚伯拉罕的子孙，但你们却想杀我，因为你们心里容不下我的道。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我知道你們是亞伯拉罕的後裔；但你們想殺我，因為你們心裡不能容納我的道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我知道你們是亞伯拉罕的子孫，你們卻想要殺我，因為你們心裏容不下我的道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "I know that you are Abraham’s descendants; yet you seek to kill Me, because My word has no place in you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I know that you are Abraham’s descendants. Yet you are looking for a way to kill me, because you have no room for my word."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Yes, I realize that you are descendants of Abraham. And yet some of you are trying to kill me because there’s no room in your hearts for my message."
      }
    },
