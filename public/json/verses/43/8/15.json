    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.12-20",
            "paragraphs": [
              "七．耶穌是世界的光（八12～20）"
            ]
          },
          {
            "verse": "jhn.8.15",
            "paragraphs": [
              "八15法利賽人憑外貌判斷人，都是依照人的標準。他們認為耶穌只是拿撒勒的木匠，從未想過祂和以往任何一個人有什麽不同的地方。主耶穌說祂從不判斷人。祂從不以屬世的標準判斷人，像法利賽人一樣。祂到世上來的目的並不是要判斷人，乃是叫人得救。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们只凭外表判断别人，我却不判断人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們按著外貌判斷人，我卻不判斷人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們是以外貌判斷人，我卻不判斷人。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You judge according to the flesh; I am not judging anyone."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You judge by human standards; I pass judgment on no one."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You judge me by human standards, but I do not judge anyone."
      }
