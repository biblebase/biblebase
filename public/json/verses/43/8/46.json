    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.46",
            "paragraphs": [
              "八46只有基督，神無罪的兒子，才能實在地說出這樣的話，世上沒有一個人能指證祂有半點罪過。祂的行為完全無瑕疵，祂只說真理的話，可惜，他們並不信祂。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们谁能指证我有罪呢？我既然把真理告诉了你们，你们为什么还不信我呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們中間有誰能指證我有罪呢？我既然講真理，你們為甚麼不信我呢？"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你们当中谁能指证我有罪呢？既然我讲的是真理，你们为什么不相信我呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們中間誰能指證我有罪呢？我既然將真理告訴你們，為甚麼不信我呢？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Which one of you convicts me of sin? If I tell the truth, why do you not believe me?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Which one of you convicts Me of sin? If I speak truth, why do you not believe Me?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Can any of you prove me guilty of sin? If I am telling the truth, why don’t you believe me?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Which of you convicts Me of sin? And if I tell the truth, why do you not believe Me?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Which of you can truthfully accuse me of sin? And since I am telling you the truth, why don’t you believe me?"
      }
    },
