    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.8.21-59",
            "paragraphs": [
              "八．猶太人與耶穌辯論（八21～59）"
            ]
          },
          {
            "verse": "jhn.8.53",
            "paragraphs": [
              "八53他們知道主稱自己比他們的祖宗亞伯拉罕和眾先知還大。亞伯拉罕不曾使人脫離死亡，自己也逃不出死亡的手，眾先知也一樣。但眼前人竟自稱能救人脫離死亡，祂一定以為自己比列祖還大。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "难道你比我们的祖先亚伯拉罕还大吗？他死了，先知们也死了，你以为你是谁？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "難道你比我們的祖宗亞伯拉罕還大嗎？他死了，先知們也死了；你把自己當作甚麼人呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "難道你比我們的祖宗亞伯拉罕還大嗎？他死了，眾先知也死了，你將自己當作甚麼人呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Surely You are not greater than our father Abraham, who died? The prophets died too; whom do You make Yourself out to be?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Are you greater than our father Abraham? He died, and so did the prophets. Who do you think you are?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Are you greater than our father Abraham? He died, and so did the prophets. Who do you think you are?”"
      }
    },
