    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "第二天清晨，祂又回到圣殿。百姓聚集在祂那里，祂就坐下来教导他们。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "黎明的時候，他又到聖殿去，眾人都來到他那裡，他就坐下教導他們。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "清晨，耶稣又来到圣殿，全体民众到他那里，他就坐下，开始教导他们。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "清早又回到殿裏。眾百姓都到他那裏去，他就坐下，教訓他們。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Early in the morning he came again to the temple. All the people came to him, and he sat down and taught them."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Early in the morning He came again into the temple, and all the people were coming to Him; and He sat down and began to teach them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "At dawn he appeared again in the temple courts, where all the people gathered around him, and he sat down to teach them."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now early in the morning He came again into the temple, and all the people came to Him; and He sat down and taught them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "but early the next morning he was back again at the Temple. A crowd soon gathered, and he sat down and taught them."
      }
