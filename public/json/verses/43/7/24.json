    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.10-31",
            "paragraphs": [
              "二．耶穌在殿裏教訓人（七10～31）"
            ]
          },
          {
            "verse": "jhn.7.24",
            "paragraphs": [
              "七24猶太人所犯的錯誤就是按外貌斷定是非，而不憑事實的真相。他們的判斷不公正；他們所做的事就合法，主耶穌做的就全然是錯。人就是喜歡憑眼見的作出判斷，而不按實情做決定。主耶穌根本就沒有犯摩西的律法，犯的其實是他們，因他們無理的仇視耶穌。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不要根据外表来断定是非，要按公义的原则判断。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不要按外貌判斷人，總要公公平平地判斷人。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "不要按着外表做判断，而要按着公义做判断。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不可按外貌斷定是非，總要按公平斷定是非。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Do not judge by appearances, but judge with right judgment.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Do not judge according to appearance, but judge with righteous judgment.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Stop judging by mere appearances, but instead judge correctly.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Do not judge according to appearance, but judge with righteous judgment.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Look beneath the surface so you can judge correctly.”"
      }
    },
