    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "jhn.7.17",
            "paragraphs": [
              "7:17",
              "人若立誌遵著他的旨意行：只有當人願意遵行神的旨意時，才能實實在在地領悟神的教訓，並得到使人得救的真信心。“信”意味著立誌順服(參“信心與恩典”一文)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.10-31",
            "paragraphs": [
              "二．耶穌在殿裏教訓人（七10～31）"
            ]
          },
          {
            "verse": "jhn.7.17",
            "paragraphs": [
              "七17若猶太人真的希望知道耶穌的教訓屬真屬偽，有一個很簡單的方法──人若立誌遵神的旨意行，那麽神便會向他啟示基督的教訓是出於神還是祂憑自己說的。這是給所有真誠尋求真理的人奇妙的應許。一個人若懇切，誠心想要知道真理，神便會向他啟示。“順服是屬靈知識的鑰匙。”"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e6%98%8e%e7%99%bd%e7%a5%9e%e7%9a%84%e6%97%a8%e6%84%8f",
        "title": "明白神的旨意",
        "date": "2005-05-15",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 7:11-18",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-16-2005-05-15.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-16-2005-05-15.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你们愿意遵行上帝的旨意，就能分辨这些教导是出于上帝，还是出于我自己。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人若願意遵行　神的旨意，就會知道這教訓是出於　神，還是我憑著自己的意思說的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人若立志遵着他的旨意行，就必曉得這教訓或是出於神，或是我憑着自己說的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If anyone is willing to do His will, he will know of the teaching, whether it is of God or whether I speak from Myself."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who chooses to do the will of God will find out whether my teaching comes from God or whether I speak on my own."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who wants to do the will of God will know whether my teaching is from God or is merely my own."
      }
    },
