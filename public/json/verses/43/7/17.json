    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "如果你们愿意遵行上帝的旨意，就能分辨这些教导是出于上帝，还是出于我自己。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人若願意遵行　神的旨意，就會知道這教訓是出於　神，還是我憑著自己的意思說的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "人如果愿意遵行神的旨意，就会明白这教导究竟是从神而来的，还是我凭自己而讲的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人若立志遵着他的旨意行，就必曉得這教訓或是出於神，或是我憑着自己說的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If anyone’s will is to do God’s  will, he will know whether the teaching is from God or whether I am speaking on my own authority."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If anyone is willing to do His will, he will know of the teaching, whether it is of God or whether I speak from Myself."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who chooses to do the will of God will find out whether my teaching comes from God or whether I speak on my own."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "If anyone wills to do His will, he shall know concerning the doctrine, whether it is from God or whether I speak on My own authority."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who wants to do the will of God will know whether my teaching is from God or is merely my own."
      }
    },
