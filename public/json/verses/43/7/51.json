    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.40-53",
            "paragraphs": [
              "五．有關主耶穌的紛論（七40～53）"
            ]
          },
          {
            "verse": "jhn.7.51",
            "paragraphs": [
              "七51尼哥底母認為猶太人沒有給主耶穌辯護的機會。根據猶太的律法，若不是先有聆訊，律法並不會定他的罪。但猶太的領袖那時所做的，正正不合乎律法。他們是否害怕知道真相？答案很明顯是的。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“难道不听被告申辩，不查明事实真相，我们的律法就定祂的罪吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“如果不先聽取他本人的口供，查明他所作的事，我們的律法怎能把他定罪呢？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“如果不先听本人的口供，并且了解他做了什么，我们的律法就定不了他的罪，难道不是吗？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「不先聽本人的口供，不知道他所做的事，難道我們的律法還定他的罪嗎？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“Does our law judge a man without first giving him a hearing and learning what he does?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Our Law does not judge a man unless it first hears from him and knows what he is doing, does it?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Does our law condemn a man without first hearing him to find out what he has been doing?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“Does our law judge a man before it hears him and knows what he is doing?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Is it legal to convict a man before he is given a hearing?” he asked."
      }
    },
