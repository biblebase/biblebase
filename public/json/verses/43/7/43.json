    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.40-53",
            "paragraphs": [
              "五．有關主耶穌的紛論（七40～53）"
            ]
          },
          {
            "verse": "jhn.7.43",
            "paragraphs": [
              "七43因為各人的意見不同和無知，眾人就為基督起了分爭。今天也是一樣。人們會為耶穌基督而爭論。有些說祂只是一個凡人，像我們一樣。有些認祂是歷史上最偉大傑出的人，但相信神的話的人就知道“基督……是在萬有之上，永遠可稱頌的神”（羅九5）。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人因为对耶稣的看法不同，就起了纷争。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "群眾因為耶穌的緣故，就起了紛爭。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "於是眾人因着耶穌起了紛爭。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So a division occurred in the crowd because of Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Thus the people were divided because of Jesus."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "So the crowd was divided about him."
      }
