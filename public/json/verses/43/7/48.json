    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.7.40-53",
            "paragraphs": [
              "五．有關主耶穌的紛論（七40～53）"
            ]
          },
          {
            "verse": "jhn.7.47-48",
            "paragraphs": [
              "七47，48法利賽人想要威嚇差役，指他們被耶穌迷惑了，提醒他們整個猶太民族的官長都沒有信他的。這番話實是令人吃驚！若所有猶太民族的領袖，在彌賽亞來臨時，都不認識祂，那真是他們的羞恥。",
              "這些法利賽人自己不單不願相信耶穌，更不想其他人信祂。今天也有這樣的人。很多自己不願得救的人，也會用盡所有辦法來阻止他們的親朋戚友信主。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "哪有官长和法利赛人信祂呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "官長或法利賽人中間有誰是信他的呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "官長或是法利賽人豈有信他的呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "No one of the rulers or Pharisees has believed in Him, has he?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Have any of the rulers or of the Pharisees believed in him?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Is there a single one of us rulers or Pharisees who believes in him?"
      }
