    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们若认识我，也会认识我的父。从现在起，你们不但认识祂，而且也看见祂了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "如果你們認識我，就必認識我的父；從今以後，你們認識他，並且看見了他。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們若認識我，也就認識我的父。從今以後，你們認識他，並且已經看見他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If you had known Me, you would have known My Father also; from now on you know Him, and have seen Him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If you really know me, you will know  my Father as well. From now on, you do know him and have seen him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you had really known me, you would know who my Father is. From now on, you do know him and have seen him!”"
      }
    },
