    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "犹大，不是后来出卖耶稣的犹大，问耶稣：“主啊，你为什么只向我们显现而不向世人显现呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶大對耶穌說：“主啊，你為甚麼要親自向我們顯現，不向世人顯現呢？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "不是加略人的那个犹大问耶稣：“主啊，怎么会这样呢？你为什么要向我们显明自己，而不向世人显明呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶大（不是加略人猶大）問耶穌說：「主啊，為甚麼要向我們顯現，不向世人顯現呢？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Judas (not Iscariot) said to him, “Lord, how is it that you will manifest yourself to us, and not to the world?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Judas (not Iscariot) *said to Him, “Lord, what then has happened that You are going to disclose Yourself to us and not to the world?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then Judas (not Judas Iscariot) said, “But, Lord, why do you intend to show yourself to us and not to the world?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Judas (not Iscariot) said to Him, “Lord, how is it that You will manifest Yourself to us, and not to the world?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Judas (not Judas Iscariot, but the other disciple with that name) said to him, “Lord, why are you going to reveal yourself only to us and not to the world at large?”"
      }
    },
