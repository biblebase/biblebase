    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不爱我的人不会遵行我的道。你们所听见的道不是出于我自己，而是出于差我来的父。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不愛我的，就不會遵守我的話。你們所聽見的道，不是我的，而是那差我來的父的道。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "不愛我的人就不遵守我的道。你們所聽見的道不是我的，乃是差我來之父的道。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who does not love Me does not keep My words; and the word which you hear is not Mine, but the Father’s who sent Me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Anyone who does not love me will not obey my teaching. These words you hear are not my own; they belong to the Father who sent me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Anyone who doesn’t love me will not obey me. And remember, my words are not my own. What I am telling you is from the Father who sent me."
      }
