    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.14.27-31",
            "paragraphs": [
              "十五．耶穌將平安留給門徒（一四27～31）"
            ]
          },
          {
            "verse": "jhn.14.29",
            "paragraphs": [
              "一四29主知道門徒很易害怕，於是對門徒顯出無私的關心，把將來的事預先告訴他們，叫他們不要憂愁、沮喪、或害怕。主乃是叫他們信。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e4%b8%8d%e8%a6%81%e6%86%82%e6%84%81%ef%bc%8c%e4%b9%9f%e4%b8%8d%e8%a6%81%e6%87%bc%e6%80%95",
        "title": "不要憂愁，也不要懼怕",
        "date": "2006-05-21",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 14:25-31",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-32-2006-05-21.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-32-2006-05-21.pdf"
      }
    ],
    "versions": {
      "ccb": {
