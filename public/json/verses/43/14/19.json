    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不久，世人就看不见我了，而你们却能看见我，因为我活着，你们也要活着。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "不久以後，世人不再看見我，你們卻要看見我，因為我活著，你們也要活著。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "“再过一会儿，世人不再看到我了，而你们将看到我。因为我活着，你们也将活着。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "還有不多的時候，世人不再看見我，你們卻看見我；因為我活着，你們也要活着。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Yet a little while and the world will see me no more, but you will see me. Because I live, you also will live."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After a little while the world will no longer see Me, but you will see Me; because I live, you will live also."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Before long, the world will not see me anymore, but you will see me. Because I live, you also will live."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“A little while longer and the world will see Me no more, but you will see Me. Because I live, you will live also."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Soon the world will no longer see me, but you will see me. Since I live, you also will live."
      }
    },
