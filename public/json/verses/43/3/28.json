    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.3.22-36",
            "paragraphs": [
              "九．施洗約翰在猶太的工作（三22～36）"
            ]
          },
          {
            "verse": "jhn.3.28",
            "paragraphs": [
              "三28約翰提醒門徒，他已不時指出他不是基督，只是奉差遣為彌賽亞鋪路的。他們為何替他爭辯？為何替他另組一黨？他不是重要的那位，只希望領人歸向主耶穌。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e4%ba%ba%e7%94%9f%e7%9a%84%e4%b8%89%e9%a0%85%e6%8a%89%e6%93%87",
        "title": "人生的三項抉擇",
        "date": "2005-01-09",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 3:17-35",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-06-2005-01-09.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-06-2005-01-09.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们自己可以为我做见证，我说过我不是基督，我只是奉差在祂前面预备道路的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們自己可以為我作證：我曾說，我不是基督，不過是奉差遣作他的先鋒的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你们自己可以为我见证：我曾经说过‘我不是基督，而是奉差派在他前面的。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我曾說：『我不是基督，是奉差遣在他前面的』，你們自己可以給我作見證。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You yourselves bear me witness, that I said, ‘I am not the Christ, but I have been sent before him.’"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You yourselves are my witnesses that I said, ‘I am not the Christ,’ but, ‘I have been sent ahead of Him.’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You yourselves can testify that I said, ‘I am not the Messiah but am sent ahead of him.’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You yourselves bear me witness, that I said, ‘I am not the Christ,’ but, ‘I have been sent before Him.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You yourselves know how plainly I told you, ‘I am not the Messiah. I am only here to prepare the way for him.’"
      }
    },
