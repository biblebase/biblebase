    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.3.1-21",
            "paragraphs": [
              "八．耶穌教尼哥底母重生的道理（三1～21）"
            ]
          },
          {
            "verse": "jhn.3.12",
            "paragraphs": [
              "三12什麽是主所說的地上的事呢？那就是祂地上的國度。尼哥底母學習舊約，知道有一天彌賽亞會來到，在地上成立國度，建都於耶路撒冷。但他不曉得若要進入此國，就必須重生。什麽是主所說的天上的事呢？就是耶穌在以下經節所說的道理，論述人重生的奇妙方法。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我对你们说地上的事，你们尚且不信，要是说天上的事，你们怎么会信呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我對你們講地上的事，你們尚且不信，如果講天上的事，怎能相信呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我對你們說地上的事，你們尚且不信，若說天上的事，如何能信呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If I told you earthly things and you do not believe, how will you believe if I tell you heavenly things?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I have spoken to you of earthly things and you do not believe; how then will you believe if I speak of heavenly things?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But if you don’t believe me when I tell you about earthly things, how can you possibly believe if I tell you about heavenly things?"
      }
    },
