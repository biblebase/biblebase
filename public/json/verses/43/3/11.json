    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.3.1-21",
            "paragraphs": [
              "八．耶穌教尼哥底母重生的道理（三1～21）"
            ]
          },
          {
            "verse": "jhn.3.11",
            "paragraphs": [
              "三11主耶穌繼而強調祂的教訓是真確、絕無謬誤的，但人卻總不相信。自亙古以來，主耶穌已知道這一切，祂也只教導所見過和知道的，而且真實無誤。但當日尼哥底母和很多猶太人都不相信祂的見證。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我实实在在地告诉你，我们所说的是自己知道的，所见证的是自己见过的，可是你们不肯接受我们的见证。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我實實在在告訴你，我們知道的，才講論；見過的，就作證，然而你們卻不接受我們的見證。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我實實在在地告訴你，我們所說的是我們知道的；我們所見證的是我們見過的；你們卻不領受我們的見證。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Truly, truly, I say to you, we speak of what we know and testify of what we have seen, and you do not accept our testimony."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Very truly I tell you, we speak of what we know, and we testify to what we have seen, but still you people do not accept our testimony."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I assure you, we tell you what we know and have seen, and yet you won’t believe our testimony."
      }
    },
