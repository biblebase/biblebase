    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.3.1-21",
            "paragraphs": [
              "八．耶穌教尼哥底母重生的道理（三1～21）"
            ]
          },
          {
            "verse": "jhn.3.6",
            "paragraphs": [
              "三6就算尼哥底母能用方法重新進入母腹再生出來，他也不能改變人的惡性。從肉身生的就是肉身。人生出來的孩子都在罪中，絕無方法自救。從靈生的就是靈。一個人相信主耶穌就從靈而生，重生後，他便有新的品格，可進入神的國。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "从肉体生的还是肉体，从圣灵生的才是灵。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "從肉身生的就是肉身，從靈生的就是靈。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "從肉身生的就是肉身；從靈生的就是靈。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "That which is born of the flesh is flesh, and that which is born of the Spirit is spirit."
      },
