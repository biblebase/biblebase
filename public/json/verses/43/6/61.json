    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.6.35-65",
            "paragraphs": [
              "四．耶穌就是生命的糧（六35～65）"
            ]
          },
          {
            "verse": "jhn.6.61",
            "paragraphs": [
              "六61我們再次找到主是全知的證據。祂知道門徒所說的一字一句。祂知道他們正議論祂所說的話，不滿祂稱自己是從天上降下來的，並人若要得永生，就要吃祂的肉、喝祂的血。耶穌知道他們，問他們說：“這話是叫你們厭棄麽？”"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e7%94%9f%e5%91%bd%e7%9a%84%e7%b3%a7",
        "title": "生命的糧",
        "date": "2005-05-01",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 6:24-71",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-15-2005-05-01.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-15-2005-05-01.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣知道门徒在议论，就说：“这话叫你们失去信心吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌心裡知道門徒為了這事議論紛紛，就對他們說：“這話使你們動搖嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌心裏知道門徒為這話議論，就對他們說：「這話是叫你們厭棄嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But Jesus, conscious that His disciples grumbled at this, said to them, “Does this cause you to stumble?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Aware that his disciples were grumbling about this, Jesus said to them, “Does this offend you?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus was aware that his disciples were complaining, so he said to them, “Does this offend you?"
      }
    },
