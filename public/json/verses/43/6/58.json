    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我是从天上降下来的真粮，吃这粮的人必永远活着，不像你们的祖先，虽然吃过吗哪，最后还是死了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這就是從天上降下來的食物，不像嗎哪，你們的祖宗吃過，還是死了；吃這食物的，必活到永遠。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "这才是从天上降下来的粮，不像吗哪。你们的祖先吃过吗哪还是死了，而吃这粮的人就将活着，直到永远。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這就是從天上降下來的糧。吃這糧的人就永遠活着，不像你們的祖宗吃過嗎哪還是死了。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "This is the bread that came down from heaven, not like the bread  the fathers ate, and died. Whoever feeds on this bread will live forever.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is the bread which came down out of heaven; not as the fathers ate and died; he who eats this bread will live forever.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is the bread that came down from heaven. Your ancestors ate manna and died, but whoever feeds on this bread will live forever.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "This is the bread which came down from heaven—not as your fathers ate the manna, and are dead. He who eats this bread will live forever.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I am the true bread that came down from heaven. Anyone who eats this bread will not die as your ancestors did (even though they ate the manna) but will live forever.”"
      }
    },
