    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.6.22-34",
            "paragraphs": [
              "三．眾人要求神跡（六22～34）"
            ]
          },
          {
            "verse": "jhn.6.28",
            "paragraphs": [
              "六28眾人問主他們該做什麽才算作神的工。人總想自行尋出通往天堂的路，總以為他們該做一些東西，好賺取救恩。但人若真的能為其靈魂得救而付上努力，那麽他就有理由吹噓自大，這就合乎人的心意。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e7%94%9f%e5%91%bd%e7%9a%84%e7%b3%a7",
        "title": "生命的糧",
        "date": "2005-05-01",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 6:24-71",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-15-2005-05-01.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-15-2005-05-01.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "众人问祂：“我们要怎样做，才算是做上帝的工作呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "眾人又問他：“我們應該作甚麼，才算是作　神的工作呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "眾人問他說：「我們當行甚麼才算做神的工呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore they said to Him, “What shall we do, so that we may work the works of God?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then they asked him, “What must we do to do the works God requires?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They replied, “We want to perform God’s works, too. What should we do?”"
      }
    },
