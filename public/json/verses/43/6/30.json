    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.6.22-34",
            "paragraphs": [
              "三．眾人要求神跡（六22～34）"
            ]
          },
          {
            "verse": "jhn.6.30",
            "paragraphs": [
              "六30本節更加證明人心的醜惡。昨天，他們看見主耶穌只用了五餅二魚，就給五千人吃飽。隔了一天，他們竟來到主前，問祂行什麽神跡以證明祂是神的兒子。正如很多不信的人一樣，他們先要看見，然後才相信。“叫我們看見就信你。”但這不是神的次序。神對罪人說：“你若相信，就能看見。”信心永遠是要在前頭的。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e7%94%9f%e5%91%bd%e7%9a%84%e7%b3%a7",
        "title": "生命的糧",
        "date": "2005-05-01",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 6:24-71",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-15-2005-05-01.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-15-2005-05-01.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们又问：“那么，你行什么神迹让我们看了可以信你呢？你要做什么呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是他們就說：“你要行甚麼神蹟，讓我們看了就信你呢？你到底能作甚麼呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們又說：「你行甚麼神蹟，叫我們看見就信你；你到底做甚麼事呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So they said to Him, “What then do You do for a sign, so that we may see, and believe You? What work do You perform?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So they asked him, “What sign then will you give that we may see it and believe you? What will you do?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They answered, “Show us a miraculous sign if you want us to believe in you. What can you do?"
      }
    },
