    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "差我来者的旨意是，祂所赐给我的，我一个也不失掉，在末日我要叫他们复活。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那差我來者的旨意就是：他所賜給我的人，我連一個也不失落，並且在末日我要使他們復活。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "差我來者的意思就是：他所賜給我的，叫我一個也不失落，在末日卻叫他復活。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is the will of Him who sent Me, that of all that He has given Me I lose nothing, but raise it up on the last day."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "And this is the will of him who sent me, that I shall lose none of all those he has given me, but raise them up at the last day."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And this is the will of God, that I should not lose even one of all those he has given me, but that I should raise them up at the last day."
      }
