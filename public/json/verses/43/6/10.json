    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“你们叫大家坐下。”那地方草很多，众人便坐下来，当时在场的男人大约有五千。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌吩咐他們：“你們叫眾人坐下。”原來那地方的草很多，眾人就坐下，單是男人的數目約有五千。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「你們叫眾人坐下。」原來那地方的草多，眾人就坐下，數目約有五千。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus said, “Have the people sit down.” Now there was much grass in the place. So the men sat down, in number about five thousand."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus said, “Have the people sit down.” There was plenty of grass in that place, and they sat down (about five thousand men were there)."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Tell everyone to sit down,” Jesus said. So they all sat down on the grassy slopes. (The men alone numbered about 5,000.)"
      }
    },
