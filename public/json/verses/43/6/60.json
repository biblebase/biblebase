    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.6.35-65",
            "paragraphs": [
              "四．耶穌就是生命的糧（六35～65）"
            ]
          },
          {
            "verse": "jhn.6.60",
            "paragraphs": [
              "六60直到這時候，耶穌已有好些門徒，遠較原先的十二個為多。任何人跟從祂，並承認、接受祂的教訓的，都是祂的門徒。但不是所有稱為門徒的人都是真信徒。這時候，有好些口稱是他的門徒的人說：“這話甚難。”他們認為主的教訓惹人討厭。其實，祂的教訓並不使他們難於明白，使他們不快的是接受祂的教訓。他們說“誰能聽呢”，暗示“誰能夠忍受這樣討厭的教訓”？"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e7%94%9f%e5%91%bd%e7%9a%84%e7%b3%a7",
        "title": "生命的糧",
        "date": "2005-05-01",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 6:24-71",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-15-2005-05-01.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-15-2005-05-01.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂的许多门徒听了，就议论说：“这话实在难懂，谁能接受呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的門徒中，有許多人聽了，就說：“這話很難，誰能接受呢？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他的门徒当中有许多人听了，就说：“这话实在严厉，谁能听呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的門徒中有好些人聽見了，就說：「這話甚難，誰能聽呢？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "When many of his disciples heard it, they said, “This is a hard saying; who can listen to it?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore many of His disciples, when they heard this said, “This is a difficult statement; who can listen to it?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "On hearing it, many of his disciples said, “This is a hard teaching. Who can accept it?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Therefore many of His disciples, when they heard this, said, “This is a hard saying; who can understand it?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Many of his disciples said, “This is very hard to understand. How can anyone accept it?”"
      }
    },
