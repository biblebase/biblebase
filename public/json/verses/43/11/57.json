    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "当时祭司长和法利赛人早已下令，如果有人知道耶稣在哪里，就来报告，他们好去抓祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "祭司長和法利賽人早已下了命令：如果有人知道耶穌在哪裡，就要前來報告，好去逮捕他。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "原来，祭司长们和法利赛人早就下令，如果有人知道耶稣在哪里，就要报告，好抓住他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那時，祭司長和法利賽人早已吩咐說，若有人知道耶穌在哪裏，就要報明，好去拿他。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Now the chief priests and the Pharisees had given orders that if anyone knew where he was, he should let them know, so that they might arrest him."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now the chief priests and the Pharisees had given orders that if anyone knew where He was, he was to report it, so that they might seize Him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But the chief priests and the Pharisees had given orders that anyone who found out where Jesus was should report it so that they might arrest him."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now both the chief priests and the Pharisees had given a command, that if anyone knew where He was, he should report it, that they might seize Him."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Meanwhile, the leading priests and Pharisees had publicly ordered that anyone seeing Jesus must report it immediately so they could arrest him."
      }
    },
