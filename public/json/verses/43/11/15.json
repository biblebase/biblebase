    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为了你们的缘故，我很高兴自己不在那里，好叫你们信我。现在我们可以去了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我為你們歡喜，因為我不在那裡，是要使你們相信。現在我們到他那裡去吧。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我沒有在那裏就歡喜，這是為你們的緣故，好叫你們相信。如今我們可以往他那裏去吧。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and I am glad for your sakes that I was not there, so that you may believe; but let us go to him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and for your sake I am glad I was not there, so that you may believe. But let us go to him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And for your sakes, I’m glad I wasn’t there, for now you will really believe. Come, let’s go see him.”"
      }
