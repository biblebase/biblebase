    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们没有认识到，祂一个人替众人死，而不是整个民族灭亡，对你们来说更好。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也不去想想，一個人代替人民死，免得整個民族滅亡，這對你們是有益的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "獨不想一個人替百姓死，免得通國滅亡，就是你們的益處。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "nor do you take into account that it is expedient for you that one man die for the people, and that the whole nation not perish.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You do not realize that it is better for you that one man die for the people than that the whole nation perish.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You don’t realize that it’s better for you that one man should die for the people than for the whole nation to be destroyed.”"
      }
    },
