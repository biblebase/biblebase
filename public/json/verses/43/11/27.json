    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "玛大说：“主啊，我信！我相信你是来到世界的基督，是上帝的儿子。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她說：“主啊，我信；我已經信了，你是基督，是　神的兒子，是那要到世上來的。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "馬大說：「主啊，是的，我信你是基督，是神的兒子，就是那要臨到世界的。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She *said to Him, “Yes, Lord; I have believed that You are the Christ, the Son of God, even He who comes into the world.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Yes, Lord,” she replied, “I believe that you are the Messiah, the Son of God, who is to come into the world.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Yes, Lord,” she told him. “I have always believed you are the Messiah, the Son of God, the one who has come into the world from God.”"
      }
    },
