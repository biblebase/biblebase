    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "那死者就出来了，手脚都缠着布条，脸上也包着布。 耶稣对他们说：“给他解开，让他走！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "那死了的人就出來，他的手腳都纏著布，臉上裹著巾。耶穌說：“解開他，讓他走！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "那死人就出來了，手腳裹着布，臉上包着手巾。耶穌對他們說：「解開，叫他走！」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The man who had died came forth, bound hand and foot with wrappings, and his face was wrapped around with a cloth. Jesus *said to them, “Unbind him, and let him go.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The dead man came out, his hands and feet wrapped with strips of linen, and a cloth around his face. Jesus said to them, “Take off the grave clothes and let him go.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "And the dead man came out, his hands and feet bound in graveclothes, his face wrapped in a headcloth. Jesus told them, “Unwrap him and let him go!”"
      }
    },
