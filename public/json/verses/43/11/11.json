    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "接着耶稣又说：“我们的朋友拉撒路已经睡了，我去叫醒他。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說完了這些話，跟著又對他們說：“我們的朋友拉撒路睡了，我要去喚醒他。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說了這話，隨後對他們說：「我們的朋友拉撒路睡了，我去叫醒他。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This He said, and after that He *said to them, “Our friend Lazarus has fallen asleep; but I go, so that I may awaken him out of sleep.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After he had said this, he went on to tell them, “Our friend Lazarus has fallen asleep; but I am going there to wake him up.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then he said, “Our friend Lazarus has fallen asleep, but now I will go and wake him up.”"
      }
