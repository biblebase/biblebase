    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "其实这句话不是出于他自己，只因那年他是大祭司，上帝借着他预言耶稣将要替犹太民族死。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他說這話不是由於自己，而是因為他是那年的大祭司，所以預言耶穌要替猶太民族死；"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他说这话不是出于自己，只因他是当年的大祭司，就预言了耶稣要替全民族而死；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他這話不是出於自己，是因他本年作大祭司，所以預言耶穌將要替這一國死；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He did not say this of his own accord, but being high priest that year he prophesied that Jesus would die for the nation,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now he did not say this on his own initiative, but being high priest that year, he prophesied that Jesus was going to die for the nation,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He did not say this on his own, but as high priest that year he prophesied that Jesus would die for the Jewish nation,"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Now this he did not say on his own authority; but being high priest that year he prophesied that Jesus would die for the nation,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He did not say this on his own; as high priest at that time he was led to prophesy that Jesus would die for the entire nation."
      }
    },
