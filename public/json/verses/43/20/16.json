    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“玛丽亚。” 玛丽亚立即转过头来用希伯来话对祂说：“拉波尼！”拉波尼的意思是老师。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌對她說：“馬利亞！”她轉過身來，用希伯來話對他說：“拉波尼！”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「馬利亞。」馬利亞就轉過來，用希伯來話對他說：「拉波尼！」（拉波尼就是夫子的意思。）"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Jesus *said to her, “Mary!” She turned and *said to Him in Hebrew, “Rabboni!” (which means, Teacher)."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jesus said to her, “Mary.” She turned toward him and cried out in Aramaic, “Rabboni!” (which means “Teacher”)."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Mary!” Jesus said. She turned to him and cried out, “Rabboni!” (which is Hebrew for “Teacher”)."
      }
    },
