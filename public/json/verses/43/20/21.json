    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣又说：“愿你们平安！父怎样差遣我，我也照样差遣你们。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌又對他們說：“願你們平安。父怎樣差遣了我，我也怎樣差遣你們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌又對他們說：「願你們平安！父怎樣差遣了我，我也照樣差遣你們。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Jesus said to them again, “Peace be with you; as the Father has sent Me, I also send you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Again Jesus said, “Peace be with you! As the Father has sent me, I am sending you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Again he said, “Peace be with you. As the Father has sent me, so I am sending you.”"
      }
