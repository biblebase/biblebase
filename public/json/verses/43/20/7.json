    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣的裹头巾则卷着放在另一处，没有和细麻布放在一起。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "也看見耶穌的裹頭巾，沒有和細麻布放在一起，而是捲著放在一邊。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又看見耶穌的裹頭巾沒有和細麻布放在一處，是另在一處捲着。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and the face-cloth which had been on His head, not lying with the linen wrappings, but rolled up in a place by itself."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "as well as the cloth that had been wrapped around Jesus’ head. The cloth was still lying in its place, separate from the linen."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "while the cloth that had covered Jesus’ head was folded up and lying apart from the other wrappings."
      }
    },
