    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "说完，让他们看祂的手和肋旁。门徒看见主，十分欢喜。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "說了這話，就把手和肋旁給他們看。門徒看見主，就歡喜了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "說了這話，就把手和肋旁指給他們看。門徒看見主，就喜樂了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And when He had said this, He showed them both His hands and His side. The disciples then rejoiced when they saw the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After he said this, he showed them his hands and side. The disciples were overjoyed when they saw the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "As he spoke, he showed them the wounds in his hands and his side. They were filled with joy when they saw the Lord!"
      }
    },
