    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.21.18-23",
            "paragraphs": [
              "三．耶穌預言彼得的死（二一18～23）"
            ]
          },
          {
            "verse": "jhn.21.21",
            "paragraphs": [
              "二一21彼得看見約翰，頓時想到：“約翰又如何呢？”他是否也會殉道呢？還是會活，直等到主再來呢？他這樣問主有關約翰的將來。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e4%b8%bb%e5%95%8a-%e4%bd%a0%e7%9f%a5%e9%81%93%e6%88%91%e6%84%9b%e4%bd%a0",
        "title": "主啊, 你知道我愛你!",
