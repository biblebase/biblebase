    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "为这些事做见证、记录这些事的就是那个门徒，我们都知道他的见证是真实的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "為這些事作證，並且記述這些事的，就是這門徒；我們知道他的見證是真實的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "為這些事作見證，並且記載這些事的就是這門徒；我們也知道他的見證是真的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "This is the disciple who is testifying to these things and wrote these things, and we know that his testimony is true."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "This is the disciple who testifies to these things and who wrote them down. We know that his testimony is true."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This disciple is the one who testifies to these events and has recorded them here. And we know that his account of these things is accurate."
      }
    },
