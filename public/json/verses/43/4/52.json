    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.4.43-54",
            "paragraphs": [
              "十三．第二件神跡：治好大臣的兒子（四43～54）"
            ]
          },
          {
            "verse": "jhn.4.51-52",
            "paragraphs": [
              "四51，52當他快要回到家的時候，他的仆人出來迎見他，告知他兒子痊愈的好消息。他不感驚奇，因他相信主耶穌的承諾，而他先相信了，現在便見到證明。父親詢問仆人，兒子什麽時候見好的。仆人的回答說明兒子不是一步步痊愈的，他一下子就好了。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%a6%82%e4%bd%95%e6%89%8d%e8%83%bd%e7%b6%93%e6%ad%b7%e5%88%b0%e7%a5%9e%e8%b9%9f%ef%bc%9f",
        "title": "如何才能經歷到神蹟？",
        "date": "2005-02-06",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 4:43-54",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-10-2005-02-06.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-10-2005-02-06.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就问什么时候好的。仆人说：“昨天下午一点钟，烧就退了。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他就向僕人查問孩子是甚麼時候好轉的。他們告訴他：“昨天下午一點鐘，熱就退了。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他就問甚麼時候見好的。他們說：「昨日未時熱就退了。」"
      },
      "nasb": {
        "version_id": "NASB",
