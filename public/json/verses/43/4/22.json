    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们不知道你们敬拜的是谁，我们知道我们敬拜的是谁，因为救恩是从犹太人出来的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們敬拜你們所不知道的，我們卻敬拜我們所知道的，因為救恩是從猶太人出來的。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "你们敬拜你们所不认识的；我们却敬拜我们所认识的，因为救恩是出自犹太人的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們所拜的，你們不知道；我們所拜的，我們知道，因為救恩是從猶太人出來的。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "You worship what you do not know; we worship what we know, for salvation is from the Jews."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You worship what you do not know; we worship what we know, for salvation is from the Jews."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You Samaritans worship what you do not know; we worship what we do know, for salvation is from the Jews."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "You worship what you do not know; we know what we worship, for salvation is of the Jews."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You Samaritans know very little about the one you worship, while we Jews know all about him, for salvation comes through the Jews."
