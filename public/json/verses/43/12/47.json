    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.12.44-50",
            "paragraphs": [
              "七．不信的危機（一二44～50）"
            ]
          },
          {
            "verse": "jhn.12.47",
            "paragraphs": [
              "一二47基督第一次到世上來的目的，並不是要審判世界，乃是要拯救世界。祂並沒有審判那些不聽祂道，不相信祂的人。這並不是說將來祂不定這些不信人的罪，但祂第一次降世的目的並不是審判。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%9f%ba%e7%9d%a3%e5%be%92%e7%94%9f%e6%b4%bb%e7%9a%84%e7%a7%98%e8%a8%a3",
        "title": "基督徒生活的秘訣",
        "date": "2005-10-09",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 12:20-50",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-25-2005-10-09.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-25-2005-10-09.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听了我的话却不遵守的人，我不审判他，因为我来不是要审判世人，而是要拯救世人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人若聽見我的話卻不遵守的，我不審判他，因為我來不是要審判世人，而是要拯救世人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若有人聽見我的話不遵守，我不審判他。我來本不是要審判世界，乃是要拯救世界。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If anyone hears My sayings and does not keep them, I do not judge him; for I did not come to judge the world, but to save the world."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If anyone hears my words but does not keep them, I do not judge that person. For I did not come to judge the world, but to save the world."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will not judge those who hear me but don’t obey me, for I have come to save the world and not to judge it."
      }
    },
