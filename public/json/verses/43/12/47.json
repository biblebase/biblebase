    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "听了我的话却不遵守的人，我不审判他，因为我来不是要审判世人，而是要拯救世人。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人若聽見我的話卻不遵守的，我不審判他，因為我來不是要審判世人，而是要拯救世人。"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "如果有人听了我的话却不遵守，我不定他的罪；因为我来，不是为了定世人的罪，而是为了拯救世人。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若有人聽見我的話不遵守，我不審判他。我來本不是要審判世界，乃是要拯救世界。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "If anyone hears my words and does not keep them, I do not judge him; for I did not come to judge the world but to save the world."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If anyone hears My sayings and does not keep them, I do not judge him; for I did not come to judge the world, but to save the world."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If anyone hears my words but does not keep them, I do not judge that person. For I did not come to judge the world, but to save the world."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And if anyone hears My words and does not believe,  I do not judge him; for I did not come to judge the world but to save the world."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I will not judge those who hear me but don’t obey me, for I have come to save the world and not to judge it."
      }
    },
