    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.12.1-8",
            "paragraphs": [
              "一．耶穌在伯大尼受膏（一二1～8）"
            ]
          },
          {
            "verse": "jhn.12.7",
            "paragraphs": [
              "一二7主即是說：“不要阻她。他是為我安葬之日存留這香膏的40。現在她用香膏大方地膏我，是因為她愛我、敬拜我的緣故，要容她這樣行。”"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣说：“由她吧！她这是为我安葬作预备。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌就說：“由她吧，這香膏是她留下來為我安葬的日子用的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說：「由她吧！她是為我安葬之日存留的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Therefore Jesus said, “Let her alone, so that she may keep it for the day of My burial."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Leave her alone,” Jesus replied. “It was intended that she should save this perfume for the day of my burial."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Jesus replied, “Leave her alone. She did this in preparation for my burial."
      }
    },
