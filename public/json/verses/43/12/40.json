    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“主使他们眼瞎、心硬， 免得他们眼睛看见， 心里明白，回心转意， 就得到我的医治。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“　神使他們瞎了眼，硬了心， 免得他們眼睛看見， 心裡明白而回轉過來； 我就醫好他們。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "主叫他們瞎了眼， 硬了心， 免得他們眼睛看見， 心裏明白，回轉過來， 我就醫治他們。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“He has blinded their eyes and He hardened their heart, so that they would not see with their eyes and perceive with their heart, and be converted and I heal them.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“He has blinded their eyes and hardened their hearts, so they can neither see with their eyes, nor understand with their hearts, nor turn—and I would heal them.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“The Lord has blinded their eyes and hardened their hearts— so that their eyes cannot see, and their hearts cannot understand, and they cannot turn to me and have me heal them.”"
      }
    },
