    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.12.27-36",
            "paragraphs": [
              "五．耶穌面對將臨的死亡（一二27～36）"
            ]
          },
          {
            "verse": "jhn.12.28",
            "paragraphs": [
              "一二28主耶穌並沒有求要脫離十字架的使命，祂只希望父的名能得榮耀。祂關心神能得到榮耀，輕看自己的安全享樂。神在此時從天上說話，祂已經榮耀了祂的名，還要再榮耀。耶穌傳道的生涯中，已榮耀了神的名。救主三十年在拿撒勒默默度過的光陰，三年傳道工作中所行所說的神跡雋語，都大大榮耀了父的名，但神將會因基督的受死、埋葬、復活與升天，受到更大的榮耀。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%9f%ba%e7%9d%a3%e5%be%92%e7%94%9f%e6%b4%bb%e7%9a%84%e7%a7%98%e8%a8%a3",
        "title": "基督徒生活的秘訣",
        "date": "2005-10-09",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 12:20-50",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-25-2005-10-09.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-25-2005-10-09.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "父啊！愿你使自己的名得荣耀！” 当时天上有声音说：“我已使自己的名得了荣耀，并且还要得荣耀。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "父啊，願你榮耀你的名！”當時有聲音從天上來，說：“我已經榮耀了我的名，還要再榮耀。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "父啊，願你榮耀你的名！」當時就有聲音從天上來，說：「我已經榮耀了我的名，還要再榮耀。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Father, glorify Your name.” Then a voice came out of heaven: “I have both glorified it, and will glorify it again.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Father, glorify your name!” Then a voice came from heaven, “I have glorified it, and will glorify it again.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Father, bring glory to your name.” Then a voice spoke from heaven, saying, “I have already brought glory to my name, and I will do so again.”"
      }
    },
