    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们说：“这座殿用了四十六年才建成，你三天之内就要把它重建起来吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "猶太人說：“這殿建了四十六年，你三天之內就能把它建造起來嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "猶太人便說：「這殿是四十六年才造成的，你三日內就再建立起來嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The Jews then said, “It took forty-six years to build this temple, and will You raise it up in three days?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They replied, “It has taken forty-six years to build this temple, and you are going to raise it in three days?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“What!” they exclaimed. “It has taken forty-six years to build this Temple, and you can rebuild it in three days?”"
      }
