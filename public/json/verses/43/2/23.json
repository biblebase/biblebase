    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣在耶路撒冷过逾越节期间，许多人看见祂行的神迹，就信了祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌在耶路撒冷過逾越節的時候，許多人看見他所行的神蹟，就信了他的名。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "當耶穌在耶路撒冷過逾越節的時候，有許多人看見他所行的神蹟，就信了他的名。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Now when He was in Jerusalem at the Passover, during the feast, many believed in His name, observing His signs which He was doing."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Now while he was in Jerusalem at the Passover Festival, many people saw the signs he was performing and believed in his name."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Because of the miraculous signs Jesus did in Jerusalem at the Passover celebration, many began to trust in him."
      }
    },
