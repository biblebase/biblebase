    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.2.12-17",
            "paragraphs": [
              "五．神兒子潔凈父的殿（二12～17）"
            ]
          },
          {
            "verse": "jhn.2.12",
            "paragraphs": [
              "二12耶穌與他的母親、弟兄和門徒離開迦拿，下迦百農去。他們只在那裏逗留了幾日，之後，耶穌便上耶路撒冷去。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e8%80%b6%e7%a9%8c%e8%88%87%e5%ae%b6%e5%ba%ad",
        "title": "耶穌與家庭",
        "date": "2004-11-14",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 2:1-25",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-03-2004-11-14.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-03-2004-11-14.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这事以后，耶稣和祂的母亲、弟弟并门徒一起去迦百农住了几天。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這事以後，耶穌和母親、弟弟、門徒，都下到迦百農去，在那裡住了沒有幾天。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這事以後，耶穌與他的母親、弟兄，和門徒都下迦百農去，在那裏住了不多幾日。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After this He went down to Capernaum, He and His mother and His brothers and His disciples; and they stayed there a few days."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After this he went down to Capernaum with his mother and brothers and his disciples. There they stayed for a few days."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "After the wedding he went to Capernaum for a few days with his mother, his brothers, and his disciples."
      }
    },
