    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂的门徒想起圣经上说：“我对你的殿充满炙热的爱。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他的門徒就想起經上記著：“我為你的殿心中迫切，如同火燒。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他的門徒就想起經上記着說：「我為你的殿心裏焦急，如同火燒。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "His disciples remembered that it was written, “Zeal for Your house will consume me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "His disciples remembered that it is written: “Zeal for your house will consume me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then his disciples remembered this prophecy from the Scriptures: “Passion for God’s house will consume me.”"
      }
