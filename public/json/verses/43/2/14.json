    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂看见圣殿区有人在卖牛羊和鸽子，还有人在兑换银币，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他在聖殿的外院裡看見有賣牛羊鴿子的，和坐在那裡兌換銀錢的，"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "他看见圣殿里有卖牛、羊、鸽子的，还有兑换银币的人坐在那里。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "看見殿裏有賣牛、羊、鴿子的，並有兌換銀錢的人坐在那裏，"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "In the temple he found those who were selling oxen and sheep and pigeons, and the money-changers sitting there."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He found in the temple those who were selling oxen and sheep and doves, and the money changers seated at their tables."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "In the temple courts he found people selling cattle, sheep and doves, and others sitting at tables exchanging money."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "And He found in the temple those who sold oxen and sheep and doves, and the money changers doing business."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In the Temple area he saw merchants selling cattle, sheep, and doves for sacrifices; he also saw dealers at tables exchanging foreign money."
      }
