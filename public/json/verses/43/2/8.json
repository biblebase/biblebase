    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣又说：“现在可以舀些出来，送给宴席总管。”他们就送了去。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌又吩咐他們：“現在舀出來，送給筵席的總管！”他們就送去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌又說：「現在可以舀出來，送給管筵席的。」他們就送了去。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And He *said to them, “Draw some out now and take it to the headwaiter.” So they took it to him."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Then he told them, “Now draw some out and take it to the master of the banquet.” They did so,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "he said, “Now dip some out, and take it to the master of ceremonies.” So the servants followed his instructions."
      }
