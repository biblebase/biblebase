    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "来到祂面前说：“犹太人的王万岁！”然后又用手掌打祂。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然後來到他面前，說：“猶太人的王萬歲！”並且用手掌打他。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "又挨近他，說：「恭喜，猶太人的王啊！」他們就用手掌打他。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "and they began to come up to Him and say, “Hail, King of the Jews!” and to give Him slaps in the face."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "and went up to him again and again, saying, “Hail, king of the Jews!” And they slapped him in the face."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Hail! King of the Jews!” they mocked, as they slapped him across the face."
      }
    },
