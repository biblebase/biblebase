    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼拉多又走到外面对众人说：“我把祂带到你们面前，好让你们知道我查不出祂有什么罪。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼拉多再次出到外面，對猶太人說：“看！我把他帶出來給你們，讓你們知道我查不出他有甚麼罪。”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "彼拉多再次出来对犹太人说：“看！我把他带出来给你们，是要你们知道，我查不出他有什么罪。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼拉多又出來對眾人說：「我帶他出來見你們，叫你們知道我查不出他有甚麼罪來。」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Pilate went out again and said to them, “See, I am bringing him out to you that you may know that I find no guilt in him.”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Pilate came out again and *said to them, “Behold, I am bringing Him out to you so that you may know that I find no guilt in Him.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Once more Pilate came out and said to the Jews gathered there, “Look, I am bringing him out to you to let you know that I find no basis for a charge against him.”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Pilate then went out again, and said to them, “Behold, I am bringing Him out to you, that you may know that I find no fault in Him.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Pilate went outside again and said to the people, “I am going to bring him out to you now, but understand clearly that I find him not guilty.”"
      }
    },
