    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.16.5-15",
            "paragraphs": [
              "十九．真理聖靈的來臨（一六5～15）"
            ]
          },
          {
            "verse": "jhn.16.6",
            "paragraphs": [
              "一六6門徒關心自己的前途多於基督。在主前頭有十字架、墳墓；他們事奉基督時，前面將有逼害來到。他們滿心憂愁，只因想起自己未來的際遇境況，並不是想起主。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为我把这些事告诉了你们，你们心里充满了忧愁。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "然而因為我把這些事告訴了你們，你們心裡就充滿憂愁。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只因我將這事告訴你們，你們就滿心憂愁。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But because I have said these things to you, sorrow has filled your heart."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Rather, you are filled with grief because I have said these things."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Instead, you grieve because of what I’ve told you."
      }
    },
