    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.16.1",
            "paragraphs": [
              "一六1門徒可能一如其他的猶太人，希望彌賽亞建立祂的國度，粉碎羅馬的政權。然而，主卻告訴他們，祂將去受死、復活、回到天上。聖靈將降臨，門徒要四出為基督作見證，他們將會為世人所憎恨、逼害。主預先將這一切告訴他們，使他們不致幻想破滅、跌倒、驚愕。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我把这些事告诉你们，以免你们失去信心。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我把這些事告訴了你們，使你們不致後退。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「我已將這些事告訴你們，使你們不至於跌倒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“These things I have spoken to you so that you may be kept from stumbling."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“All this I have told you so that you will not fall away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I have told you these things so that you won’t abandon your faith."
      }
    },
