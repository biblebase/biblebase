    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们从来没有奉我的名求过什么，你们现在求，就必得到，好使你们的喜乐满溢。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們向來沒有奉我的名求甚麼；現在你們祈求，就必定得著，讓你們的喜樂滿溢。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "向來你們沒有奉我的名求甚麼，如今你們求，就必得着，叫你們的喜樂可以滿足。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Until now you have asked for nothing in My name; ask and you will receive, so that your joy may be made full."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Until now you have not asked for anything in my name. Ask and you will receive, and your joy will be complete."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You haven’t done this before. Ask, using my name, and you will receive, and you will have abundant joy."
      }
