    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "不过按照惯例，在逾越节的时候，我要给你们释放一个人。现在，你们要我释放这个犹太人的王吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你們有個慣例，每逢逾越節，要我給你們釋放一個囚犯。你們願意我給你們釋放這個猶太人的王嗎？”"
      },
      "csbs": {
        "version_id": "CSBS",
        "version_name": "中文标准译本",
        "lang": "chs",
        "text": "不过你们有一个惯例：每逢逾越节，我可以给你们释放一个囚犯。那么，你们愿意我给你们释放这犹太人的王吗？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "但你們有個規矩，在逾越節要我給你們釋放一個人，你們要我給你們釋放猶太人的王嗎？」"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "But you have a custom that I should release one man for you at the Passover. So do you want me to release to you the King of the Jews?”"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But you have a custom that I release someone for you at the Passover; do you wish then that I release for you the King of the Jews?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But it is your custom for me to release to you one prisoner at the time of the Passover. Do you want me to release ‘the king of the Jews’?”"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“But you have a custom that I should release someone to you at the Passover. Do you therefore want me to release to you the King of the Jews?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But you have a custom of asking me to release one prisoner each year at Passover. Would you like me to release this ‘King of the Jews’?”"
      }
    },
