    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们回答说：“如果祂没有为非作歹，我们也不会把祂送到你这里来。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們回答：“如果這個人沒有作惡，我們就不會把他交給你。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們回答說：「這人若不是作惡的，我們就不把他交給你。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They answered and said to him, “If this Man were not an evildoer, we would not have delivered Him to you.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If he were not a criminal,” they replied, “we would not have handed him over to you.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“We wouldn’t have handed him over to you if he weren’t a criminal!” they retorted."
      }
    },
