    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "耶稣话才说完，站在旁边的差役就打了祂一耳光，说：“你敢这样回答大祭司！”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "耶穌說了這些話，站在旁邊的一個差役，就打他一巴掌，說：“你竟敢這樣回答大祭司嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "耶穌說了這話，旁邊站着的一個差役用手掌打他，說：「你這樣回答大祭司嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "When He had said this, one of the officers standing nearby struck Jesus, saying, “Is that the way You answer the high priest?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When Jesus said this, one of the officials nearby slapped him in the face. “Is this the way you answer the high priest?” he demanded."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then one of the Temple guards standing nearby slapped Jesus across the face. “Is that the way to answer the high priest?” he demanded."
      }
    },
