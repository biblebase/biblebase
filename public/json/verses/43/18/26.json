    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "一个大祭司的奴仆，就是被彼得削掉耳朵的那个人的亲戚说：“我不是看见你和祂一起在园子里吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "有一個大祭司的僕人，就是彼得削掉耳朵的那個人的親戚，說：“我不是看見你跟他在園子裡嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "有大祭司的一個僕人，是彼得削掉耳朵那人的親屬，說：「我不是看見你同他在園子裏嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "One of the slaves of the high priest, being a relative of the one whose ear Peter cut off, *said, “Did I not see you in the garden with Him?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "One of the high priest’s servants, a relative of the man whose ear Peter had cut off, challenged him, “Didn’t I see you with him in the garden?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But one of the household slaves of the high priest, a relative of the man whose ear Peter had cut off, asked, “Didn’t I see you out there in the olive grove with Jesus?”"
      }
    },
