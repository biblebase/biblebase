    "interpretations": {
      "matangna": {
        "content": [
          {
            "verse": "jhn.18.28-40",
            "paragraphs": [
              "六．耶穌在彼拉多面前受審（一八28～40）"
            ]
          },
          {
            "verse": "jhn.18.35",
            "paragraphs": [
              "一八35彼拉多語帶鄙視，問耶穌：“我豈是猶太人呢？”他暗示自己位高權重，事務繁忙，無瑕顧及這些猶太人地方的分爭。他同時暗認了他不知道有什麽真的指控耶穌，他所知的只是猶太人領袖告訴他的。"
            ]
          }
        ],
        "title": "馬唐納註釋",
        "author": "馬唐納"
      }
    },
    "sermons": [
      {
        "id": "home_sermons_%e5%90%9b%e7%8e%8b%e5%8f%97%e5%af%a9",
        "title": "君王受審",
        "date": "2006-10-08",
        "preacher": "王文堂牧師",
        "verses": "約翰福音 18:1-40",
        "audio": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-41-2006-10-08.mp3",
        "slides": "http://www.cbcwla.org/media/ch/SundaySermons/Book-of-John/John-41-2006-10-08.pdf"
      }
    ],
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "彼拉多说：“难道我是犹太人吗？是你们犹太人和祭司长把你送来的。你到底犯了什么罪？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "彼拉多說：“難道我是猶太人嗎？你本國的人和祭司長把你交給我，你究竟作了甚麼事？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "彼拉多說：「我豈是猶太人呢？你本國的人和祭司長把你交給我。你做了甚麼事呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Pilate answered, “I am not a Jew, am I? Your own nation and the chief priests delivered You to me; what have You done?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Am I a Jew?” Pilate replied. “Your own people and chief priests handed you over to me. What is it you have done?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Am I a Jew?” Pilate retorted. “Your own people and their leading priests brought you to me for trial. Why? What have you done?”"
      }
    },
