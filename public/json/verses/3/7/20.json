      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "身上不潔淨的人吃了獻與耶和華的平安祭的祭肉，那人必從自己的族人中被剪除。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "只是獻與耶和華平安祭的肉，人若不潔淨而吃了，這人必從民中剪除。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "But the person who eats the flesh of the sacrifice of peace offerings which belong to the Lord, in his uncleanness, that person shall be cut off from his people."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "But if anyone who is unclean eats any meat of the fellowship offering belonging to the Lord, they must be cut off from their people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If you are ceremonially unclean and you eat meat from a peace offering that was presented to the Lord, you will be cut off from the community."
      }
    },
