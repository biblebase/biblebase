      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "祭司中的男丁都可以吃這祭物；要在聖處吃，是至聖的。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Every male among the priests may eat of it. It shall be eaten in a holy place; it is most holy."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Any male in a priest’s family may eat it, but it must be eaten in the sanctuary area; it is most holy."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Any male from a priest’s family may eat the meat. It must be eaten in a sacred place, for it is most holy."
      }
