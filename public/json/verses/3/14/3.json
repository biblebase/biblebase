      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The priest is to go outside the camp and examine them. If they have been healed of their defiling skin disease,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "who will examine them at a place outside the camp. If the priest finds that someone has been healed of a serious skin disease,"
      }
    },
