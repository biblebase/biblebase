    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "lev.22.1-9",
            "paragraphs": [
              "二二1～9祭司象其它人一樣會變得不潔（自己不潔或因接觸不潔的人而變得不潔），而在不潔期間，他們不能執行祭司的職務。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你告诉亚伦父子们要谨慎处理以色列人献给我的圣物，免得亵渎我的圣名。我是耶和华。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你要告訴亞倫和他的兒子，叫他們謹慎處理以色列人分別為聖歸給我的聖物，免得他們褻瀆我的聖名；我是耶和華。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "「你吩咐亞倫和他子孫說：要遠離以色列人所分別為聖、歸給我的聖物，免得褻瀆我的聖名。我是耶和華。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Tell Aaron and his sons to be careful with the holy gifts of the sons of Israel, which they dedicate to Me, so as not to profane My holy name; I am the Lord."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Tell Aaron and his sons to treat with respect the sacred offerings the Israelites consecrate to me, so they will not profane my holy name. I am the Lord."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Tell Aaron and his sons to be very careful with the sacred gifts that the Israelites set apart for me, so they do not bring shame on my holy name. I am the Lord."
      }
    },
