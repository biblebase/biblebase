    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "lev.22.1-9",
            "paragraphs": [
              "二二1～9祭司象其它人一樣會變得不潔（自己不潔或因接觸不潔的人而變得不潔），而在不潔期間，他們不能執行祭司的職務。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "这人就不洁净，要到傍晚之后才能洁净。他要沐浴后才可吃圣物。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "摸了這些的人，必不潔淨到晚上；如果他不用水洗身，就不可吃聖物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "摸了這些人、物的，必不潔淨到晚上；若不用水洗身，就不可吃聖物。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "a person who touches any such shall be unclean until evening, and shall not eat of the holy gifts unless he has bathed his body in water."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The one who touches any such thing will be unclean till evening. He must not eat any of the sacred offerings unless he has bathed himself with water."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The man who is defiled in any of these ways will remain unclean until evening. He may not eat from the sacred offerings until he has bathed himself in water."
      }
    },
