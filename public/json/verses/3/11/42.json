      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Whatever crawls on its belly, and whatever walks on all fours, whatever has many feet, in respect to every swarming thing that swarms on the earth, you shall not eat them, for they are detestable."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You are not to eat any creature that moves along the ground, whether it moves on its belly or walks on all fours or on many feet; it is unclean."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "This includes all animals that slither along on their bellies, as well as those with four legs and those with many feet. All such animals that scurry along the ground are detestable, and you must never eat them."
      }
    },
