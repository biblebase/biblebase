    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "蝗虫吃尽地上的青苗时，我说：“主耶和华啊，求你赦免！雅各那么弱小，他怎么受得了呢？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "蝗蟲吃盡地上的農產時，我說： “主耶和華啊！求你寬恕， 因雅各太弱小， 怎能站立得住呢？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "蝗蟲吃盡那地的青物，我就說：「主耶和華啊，求你赦免；因為雅各微弱，他怎能站立得住呢？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "And it came about, when it had finished eating the vegetation of the land, that I said, “Lord God, please pardon! How can Jacob stand, For he is small?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When they had stripped the land clean, I cried out, “Sovereign Lord, forgive! How can Jacob survive? He is so small!”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "In my vision the locusts ate every green plant in sight. Then I said, “O Sovereign Lord, please forgive us or we will not survive, for Israel is so small.”"
      }
    },
