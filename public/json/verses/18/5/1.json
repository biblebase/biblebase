    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.5.1",
            "paragraphs": [
              "五1以利法警告約伯不要向天使（諸聖）上訴。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你只管呼救吧， 谁会回应你呢？ 你向哪位圣者求助呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你只管呼籲吧，有誰回答你？ 在眾聖者之中你轉向哪一位？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你且呼求，有誰答應你？ 諸聖者之中，你轉向哪一位呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Call now, is there anyone who will answer you? And to which of the holy ones will you turn?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Call if you will, but who will answer you? To which of the holy ones will you turn?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Cry for help, but will anyone answer you? Which of the angels will help you?"
      }
    },
