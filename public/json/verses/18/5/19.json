    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.5.17-26",
            "paragraphs": [
              "17～26.以利法的文集中，第五首179詩歌唱出了一個以正確的靈來面對苦難者的音樂。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.5.8-27",
            "paragraphs": [
              "五8～27以利法力主約伯服從神的管教；他若是悔改，神總會祝福他。\"六次遭難\"（19節）。即在各種有可能的患難裏。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "六次遭难，祂都救你； 第七次，灾祸也伤不到你。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你六次遭遇患難，他都救拔你， 就算第七次，災禍也傷不了你。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你六次遭難，他必救你； 就是七次，災禍也無法害你。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "From six troubles He will deliver you, Even in seven evil will not touch you."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "From six calamities he will rescue you; in seven no harm will touch you."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "From six disasters he will rescue you; even in the seventh, he will keep you from evil."
      }
