    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.5.17-26",
            "paragraphs": [
              "17～26.以利法的文集中，第五首179詩歌唱出了一個以正確的靈來面對苦難者的音樂。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.5.8-27",
            "paragraphs": [
              "五8～27以利法力主約伯服從神的管教；他若是悔改，神總會祝福他。\"六次遭難\"（19節）。即在各種有可能的患難裏。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为祂打伤，祂也包扎； 祂击伤，祂也医治。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "因為他打傷，又纏裹； 他擊傷，又親手醫治。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "因為他打破，又纏裹； 他擊傷，用手醫治。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For He inflicts pain, and gives relief; He wounds, and His hands also heal."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "For he wounds, but he also binds up; he injures, but his hands also heal."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For though he wounds, he also bandages. He strikes, but his hands also heal."
      }
    },
