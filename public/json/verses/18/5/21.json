    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.5.17-26",
            "paragraphs": [
              "17～26.以利法的文集中，第五首179詩歌唱出了一個以正確的靈來面對苦難者的音樂。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.5.8-27",
            "paragraphs": [
              "五8～27以利法力主約伯服從神的管教；他若是悔改，神總會祝福他。\"六次遭難\"（19節）。即在各種有可能的患難裏。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你必免受恶毒的毁谤， 灾难来临也不惧怕。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你必不受人口舌之害， 災殃臨到，也不害怕。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你必被隱藏，不受口舌之害； 災殃臨到，你也不懼怕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "You will be hidden from the scourge of the tongue, And you will not be afraid of violence when it comes."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You will be protected from the lash of the tongue, and need not fear when destruction comes."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You will be safe from slander and have no fear when destruction comes."
      }
