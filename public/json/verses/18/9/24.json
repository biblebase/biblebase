      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The earth is given into the hand of the wicked; He covers the faces of its judges. If it is not He, then who is it?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "When a land falls into the hands of the wicked, he blindfolds its judges. If it is not he, then who is it?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The whole earth is in the hands of the wicked, and God blinds the eyes of the judges. If he’s not the one who does it, who is?"
      }
