      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He who is at ease holds calamity in contempt, As prepared for those whose feet slip."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Those who are at ease have contempt for misfortune as the fate of those whose feet are slipping."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "People who are at ease mock those in trouble. They give a push to people who are stumbling."
      }
