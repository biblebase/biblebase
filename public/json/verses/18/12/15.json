    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.12.14-15",
            "paragraphs": [
              "14～15.在自然與歷史之間，並沒有畫出任何一條線來區隔。創世記中創造的故事如何毫無打斷地轉入亞伯拉罕的故事，也可以照樣在神絕對地支配一切中，察覺出人們所住在其中的這一個世界，與大地、天空、水，以及它們周圍的事情之間的統一性來。如果這裏是在說到任何一個特殊事件的話，那就是洪水，舉例來說，動詞留住正是用在創世記七16的那個字；但翻字卻是傾覆所多瑪與蛾摩拉所特有的。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "祂拦住水源，便有干旱； 祂放开水源，大地被淹没。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他若把水阻截，水就乾涸， 他再把水放出，水就使地翻倒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他把水留住，水便枯乾； 他再發出水來，水就翻地。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, He restrains the waters, and they dry up; And He sends them out, and they inundate the earth."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "If he holds back the waters, there is drought; if he lets them loose, they devastate the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If he holds back the rain, the earth becomes a desert. If he releases the waters, they flood the earth."
      }
