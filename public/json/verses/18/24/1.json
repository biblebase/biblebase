    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "job.24.1",
            "paragraphs": [
              "24:1",
              "本章沒有帶註解"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      },
      "leili": {
        "content": [
          {
            "verse": "job.24.1",
            "paragraphs": [
              "二四1約伯抱怨神沒有設定懲罰行惡之人的時間，叫世人看見祂確實會懲罰行惡者（比較12節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“全能者为何不定下审判的日期？ 为何信靠祂的人要空等一场？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“為甚麼全能者不保留賞善罰惡的時間？ 為甚麼認識他的人不能看見他的日子？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "全能者既定期罰惡， 為何不使認識他的人看見那日子呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Why are times not stored up by the Almighty, And why do those who know Him not see His days?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Why does the Almighty not set times for judgment? Why must those who know him look in vain for such days?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Why doesn’t the Almighty bring the wicked to judgment? Why must the godly wait for him in vain?"
      }
    },
