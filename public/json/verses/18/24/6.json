        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在田裡收割草料， 在惡人的葡萄園中摘取剩餘的葡萄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "收割別人田間的禾稼， 摘取惡人餘剩的葡萄，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They harvest their fodder in the field And glean the vineyard of the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They gather fodder in the fields and glean in the vineyards of the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They harvest a field they do not own, and they glean in the vineyards of the wicked."
      }
