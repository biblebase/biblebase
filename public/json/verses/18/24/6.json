    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.24.5-7",
            "paragraphs": [
              "5～7.約伯描寫社會上無助之人可憐的光景，遭受無情的犧牲，任何親切的人都會因這類的傷害而感到憤慨，而約伯自己的同情心也表現在他自己的抗議中。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.24.2-24",
            "paragraphs": [
              "二四2～24約伯描述世上邪惡的猖獗，而那情況顯然是神所容許的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们割田间的草料， 摘恶人剩余的葡萄。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們在田裡收割草料， 在惡人的葡萄園中摘取剩餘的葡萄。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "收割別人田間的禾稼， 摘取惡人餘剩的葡萄，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They harvest their fodder in the field And glean the vineyard of the wicked."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They gather fodder in the fields and glean in the vineyards of the wicked."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "They harvest a field they do not own, and they glean in the vineyards of the wicked."
      }
    },
