    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.24.5-7",
            "paragraphs": [
              "5～7.約伯描寫社會上無助之人可憐的光景，遭受無情的犧牲，任何親切的人都會因這類的傷害而感到憤慨，而約伯自己的同情心也表現在他自己的抗議中。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.24.2-24",
            "paragraphs": [
              "二四2～24約伯描述世上邪惡的猖獗，而那情況顯然是神所容許的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "贫穷的人如荒漠中的野驴， 殷勤觅食，给儿女糊口。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "這些貧窮人像曠野的野驢， 出外勞碌，殷勤尋覓食物， 野地為他們和他們的孩子供應食物。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "這些貧窮人如同野驢出到曠野，殷勤尋找食物； 他們靠着野地給兒女糊口，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, as wild donkeys in the wilderness They go forth seeking food in their activity, As bread for their children in the desert."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Like wild donkeys in the desert, the poor go about their labor of foraging food; the wasteland provides food for their children."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Like wild donkeys in the wilderness, the poor must spend all their time looking for food, searching even in the desert for food for their children."
      }
    },
