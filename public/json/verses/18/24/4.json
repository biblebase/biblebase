    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.24.4",
            "paragraphs": [
              "4.我們不清楚“推著窮人離開大路”（呂譯）為什麽那麽嚴重，以至於必須提起它；這裏需要有比“推擠”（NEB）更該受譴責的原因，它或許是比喻拒絕了公民權。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.24.2-24",
            "paragraphs": [
              "二四2～24約伯描述世上邪惡的猖獗，而那情況顯然是神所容許的。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他们把贫民赶离正路， 地上的穷人被迫东躲西藏。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們使窮人離開大道， 世上的貧民都一起躲藏起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們使窮人離開正道； 世上的貧民盡都隱藏。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "They push the needy aside from the road; The poor of the land are made to hide themselves altogether."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "They thrust the needy from the path and force all the poor of the land into hiding."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The poor are pushed off the path; the needy must hide together for safety."
      }
    },
