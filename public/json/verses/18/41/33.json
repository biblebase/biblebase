    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.41.33-34",
            "paragraphs": [
              "四一33,34在所有驕傲的生物當中，鱷魚是\"王\"，但神卻比牠更大；而且只有神能馴服牠。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "地上没有动物能与它相比， 像它那样无所畏惧。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在世上沒有一樣像牠的， 牠是無所懼怕的動物；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "在地上沒有像牠造的那樣， 無所懼怕。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nothing on earth is like him, One made without fear."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nothing on earth is its equal— a creature without fear."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Nothing on earth is its equal, no other creature so fearless."
      }
