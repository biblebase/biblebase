    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.41.31",
            "paragraphs": [
              "四一31牠遊泳的時候，水便翻滾如燒開水一樣，或象藥碗裏的藥混合而起泡沫那樣。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它使深渊如锅翻腾， 大海如油锅滚动。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "牠使深淵沸騰， 牠攪動海洋如在鼎中調製膏油。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "牠使深淵開滾如鍋， 使洋海如鍋中的膏油。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "He makes the deep boil like a pot; he makes the sea like a pot of ointment."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He makes the depths boil like a pot; He makes the sea like a jar of ointment."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It makes the depths churn like a boiling caldron and stirs up the sea like a pot of ointment."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "He makes the deep boil like a pot; He makes the sea like a pot of ointment."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Leviathan makes the water boil with its commotion. It stirs the depths like a pot of ointment."
      }
