    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "企图捕捉它的都会失望， 人看到它就会心惊胆战。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "人希望捉住牠是徒然的， 人一見牠，就心驚膽戰。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人指望捉拿牠是徒然的； 一見牠，豈不喪膽嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, your expectation is false; Will you be laid low even at the sight of him?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Any hope of subduing it is false; the mere sight of it is overpowering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No, it is useless to try to capture it. The hunter who attempts it will be knocked down."
      }
    },
