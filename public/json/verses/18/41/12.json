    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.41.12-25",
            "paragraphs": [
              "四一12～25這段經文描述鱷魚令人望而生畏的身體結構。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“论到它的四肢、大力和姣美的身体， 我不能缄默不言。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "論到鱷魚的四肢和牠的力氣， 以及美好的體態，我不能緘默不言。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "論到鱷魚的肢體和其大力，並美好的骨骼， 我不能緘默不言。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I will not keep silence concerning his limbs, Or his mighty strength, or his orderly frame."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I will not fail to speak of Leviathan’s limbs, its strength and its graceful form."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I want to emphasize Leviathan’s limbs and its enormous strength and graceful form."
      }
    },
