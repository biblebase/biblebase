      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Will you play with him as with a bird, Or will you bind him for your maidens?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Can you make a pet of it like a bird or put it on a leash for the young women in your house?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Can you make it a pet like a bird, or give it to your little girls to play with?"
      }
