    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.41.33-34",
            "paragraphs": [
              "四一33,34在所有驕傲的生物當中，鱷魚是\"王\"，但神卻比牠更大；而且只有神能馴服牠。",
              "──《雷歷研讀本聖經》"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "它藐视群雄， 在高傲的百兽中称王。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "所有高大的動物，牠都藐視， 牠在一切狂傲的野獸之上作王。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "凡高大的，牠無不藐視； 牠在驕傲的水族上作王。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He looks on everything that is high; He is king over all the sons of pride.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "It looks down on all that are haughty; it is king over all that are proud.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Of all the creatures, it is the proudest. It is the king of beasts.”"
      }
    },
