      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "As I have observed, those who plow evil and those who sow trouble reap it."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My experience shows that those who plant trouble and cultivate evil will harvest the same."
      }
