    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.31.5-8",
            "paragraphs": [
              "5～8.不誠實約伯否認虛謊與詭詐。他令人驚奇地將貪婪定義為心隨著眼（7節），顯出他認為純正是內在的；谷物欠收的自我咒詛（8節）暗示出第5節是指可疑的交易行為。雖然虛謊與詭詐是抽象的，可以被位格化成為約伯的同伴，這兩個名詞可能是作集合用法，指實際之“虛假的”與“詭詐的”同夥。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "愿我种的庄稼被别人享用， 我田中的出产被连根拔起。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "就願我所種的，讓別人來吃， 願我田中出產的，都連根拔起來。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "就願我所種的有別人吃， 我田所產的被拔出來。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Let me sow and another eat, And let my crops be uprooted."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "then may others eat what I have sown, and may my crops be uprooted."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "then let someone else eat the crops I have planted. Let all that I have planted be uprooted."
      }
