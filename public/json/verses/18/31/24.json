    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.31.24-25",
            "paragraphs": [
              "24～25.貪婪在廿二24～25，以利法曾勸約伯以神為他的寶藏；他在這裏否認將任何信心放在他的財富上。這個條件子句（否認）沒有帶結果子句（咒詛），完整的格式是沒有必要的。但避免貪心可能與拜偶像（26～27節）有關，與第28節作為共通的罪。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我若仰仗金子， 依靠纯金，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若以黃金為我所信靠的， 又對精金說：‘你是我所靠賴的’；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我若以黃金為指望， 對精金說：你是我的倚靠；"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "“If I have made gold my trust or called fine gold my confidence,"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If I have put my confidence in gold, And called fine gold my trust,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“If I have put my trust in gold or said to pure gold, ‘You are my security,’"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "“If I have made gold my hope, Or said to fine gold, ‘You  are my confidence’;"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Have I put my trust in money or felt secure because of my gold?"
      }
    },
