    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.31.5-8",
            "paragraphs": [
              "5～8.不誠實約伯否認虛謊與詭詐。他令人驚奇地將貪婪定義為心隨著眼（7節），顯出他認為純正是內在的；谷物欠收的自我咒詛（8節）暗示出第5節是指可疑的交易行為。雖然虛謊與詭詐是抽象的，可以被位格化成為約伯的同伴，這兩個名詞可能是作集合用法，指實際之“虛假的”與“詭詐的”同夥。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "倘若我的脚偏离正路， 贪图眼目之欲， 或手上粘着罪污，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的腳步若偏離正道， 我的心若隨從眼目而行， 我的手掌若黏有任何污點，"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的腳步若偏離正路， 我的心若隨着我的眼目， 若有玷污粘在我手上；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "If my step has turned from the way, Or my heart followed my eyes, Or if any spot has stuck to my hands,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "if my steps have turned from the path, if my heart has been led by my eyes, or if my hands have been defiled,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "If I have strayed from his pathway, or if my heart has lusted for what my eyes have seen, or if I am guilty of any other sin,"
      }
    },
