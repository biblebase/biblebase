    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.32.6-7",
            "paragraphs": [
              "6～7.以利戶讓年老的人先說，但卻失望了；朋友們也已經倚老賣老過了（十五10），以利戶並不認為他只是要提供另一種意見而已，這個字的意思是“知識”（NEB）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "就说： “我年纪轻，你们是长辈， 所以我畏惧，不敢陈明观点。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "布西人巴拉迦的兒子以利戶回答說： “我年紀輕，你們年紀大， 因此我拘束畏懼，不敢向你們表達我的意見。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "布西人巴拉迦的兒子以利戶回答說： 我年輕，你們老邁； 因此我退讓，不敢向你們陳說我的意見。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Elihu the son of Barachel the Buzite spoke out and said, “I am young in years and you are old; Therefore I was shy and afraid to tell you what I think."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "So Elihu son of Barakel the Buzite said: “I am young in years, and you are old; that is why I was fearful, not daring to tell you what I know."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Elihu son of Barakel the Buzite said, “I am young and you are old, so I held back from telling you what I think."
      }
    },
