    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.32.15",
            "paragraphs": [
              "15.從第14節的第二人稱，變到第15節的第三人稱，提示出以利戶在這時轉向約伯，以之為說話的主要對象；但當他繼續說時，偶爾也會對朋友們說話（比較卅三1與卅四2開頭的話）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“他们惊慌失措，无言以对， 再也说不出话来。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們驚惶失措，不再回應， 他們沒有甚麼可說的話了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他們驚奇不再回答， 一言不發。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“They are dismayed, they no longer answer; Words have failed them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“They are dismayed and have no more to say; words have failed them."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You sit there baffled, with nothing more to say."
      }
