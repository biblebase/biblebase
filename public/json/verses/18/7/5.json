    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.7.5",
            "paragraphs": [
              "5.心理的痛苦與身體的疼痛是分不開的，並且由他對逐漸惡化的疾病所作之冷淡的描述來加強了：“我的肉身穿上蛆蟲為衣，我的皮膚與塵土結成硬塊，渾身長滿疙瘩而流膿”213。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.7.1-6",
            "paragraphs": [
              "七1～6約伯把他的存在比作（1）服兵役（1節），（2）\"雇工人\"（1,2節）和（3）\"奴仆\"（2節）那樣艱苦勞累。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我身上布满蛆虫、伤疤， 皮肤破裂，流脓不止。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的肉體以蟲子和土塊為衣裳， 我的皮膚裂開又流膿。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的肉體以蟲子和塵土為衣； 我的皮膚才收了口又重新破裂。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "My flesh is clothed with worms and a crust of dirt, My skin hardens and runs."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My body is clothed with worms and scabs, my skin is broken and festering."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "My body is covered with maggots and scabs. My skin breaks open, oozing with pus."
      }
    },
