    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.6.19",
            "paragraphs": [
              "19.來自提瑪與示巴的沙漠商人到處張望，拼命地希望找到水，就像約伯希望從他朋友們獲得慰藉一樣。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.6.14-30",
            "paragraphs": [
              "六14～30約伯說出他因友人的不仁而受到深切的傷害，但對於他們的論點，他卻不為所動。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "提玛的商队来找水喝， 示巴的旅客指望解渴，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "提瑪的商隊瞻望溪水， 示巴的客旅等候水流。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "提瑪結伴的客旅瞻望； 示巴同夥的人等候。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The caravans of Tema looked, The travelers of Sheba hoped for them."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "The caravans of Tema look for water, the traveling merchants of Sheba look in hope."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "The caravans from Tema search for this water; the travelers from Sheba hope to find it."
      }
    },
