    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.6.11",
            "paragraphs": [
              "11.約伯大膽的言語──“我要在淒慘的痛苦中狂歡”202──所暗示出的任何狂傲，都被他下一句話的絕望給除去了。他根本沒有力量，可以接受以利法在五17～27所應許的復興。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我有何力量可以支撑下去？ 有何前景让我忍耐下去？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我有甚麼力量使我能等候呢？ 我的結局是甚麼好叫我忍耐呢？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我有甚麼氣力使我等候？ 我有甚麼結局使我忍耐？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "What is my strength, that I should wait? And what is my end, that I should endure?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“What strength do I have, that I should still hope? What prospects, that I should be patient?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "But I don’t have the strength to endure. I have nothing to live for."
      }
    },
