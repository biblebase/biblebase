    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.6.28-30",
            "paragraphs": [
              "28～30.這開始了一個新的講論，就像引語現在……所顯示出來的。他勸他們改變想法210。他使用一個嚴肅的法律誓詞211，堅持說他可以分辨他的立場是有效的，而他們的立場都是無效的，這是假設我的舌與我的“味覺”（和合本：口；呂譯“上膛”是直譯的）是辨別的象征；然而，新英語聖經比較按著字面來翻譯這些字，指約伯講論的公正。──《丁道爾聖經註釋》"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.6.14-30",
            "paragraphs": [
              "六14～30約伯說出他因友人的不仁而受到深切的傷害，但對於他們的論點，他卻不為所動。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "我岂会说诡诈之言？ 我岂会是非不辨？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我的舌上有不義嗎？ 我的口不能辨別災害嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的舌上豈有不義嗎？ 我的口裏豈不辨奸惡嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Is there injustice on my tongue? Cannot my palate discern calamities?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Is there any wickedness on my lips? Can my mouth not discern malice?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Do you think I am lying? Don’t I know the difference between right and wrong?"
      }
    },
