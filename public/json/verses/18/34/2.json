    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.34.2-9",
            "paragraphs": [
              "2～9.約伯是不虔誠的。以利戶對有知識的智慧人說話，這些可能是朋友們，而且，因為他期望他們會同意，所以已經站在他們那邊；但也可能是要指一個較大的聽眾。比較不像是在訴諸於讀者們。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“各位智者，请听我言； 明达之士，要侧耳听我的话。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“智慧人哪！你們要聽我的話； 有知識的人哪！你們要側耳聽我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你們智慧人要聽我的話； 有知識的人要留心聽我說。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Hear my words, you wise men, And listen to me, you who know."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Hear my words, you wise men; listen to me, you men of learning."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Listen to me, you wise men. Pay attention, you who have knowledge."
      }
