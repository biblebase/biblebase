      "dde": {
        "content": [
          {
            "verse": "job.16.6",
            "paragraphs": [
              "6.無論是說話或默不作聲，約伯都得不著釋放。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我若申辩，痛苦不减； 我若缄默，痛苦犹在。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我若說話，痛苦仍不消解， 我若閉口不說，痛苦也不離開我。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我雖說話，憂愁仍不得消解； 我雖停住不說，憂愁就離開我嗎？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“If I speak, my pain is not lessened, And if I hold back, what has left me?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Yet if I speak, my pain is not relieved; and if I refrain, it does not go away."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Instead, I suffer if I defend myself, and I suffer no less if I refuse to speak."
      }
    },
