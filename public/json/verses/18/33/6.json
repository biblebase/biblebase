    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.33.6-7",
            "paragraphs": [
              "三三6,7以利戶聲明，既然他象約伯一樣是人，他不會用恐怖來威嚇約伯（比較九34；一三21）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "在上帝面前，我与你无异， 也是泥土造的。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "在　神面前我與你一樣， 也是用土捏造的。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我在神面前與你一樣， 也是用土造成。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "Behold, I am toward God as you are; I too was pinched off from a piece of clay."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Behold, I belong to God like you; I too have been formed out of the clay."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "I am the same as you in God’s sight; I too am a piece of clay."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "Truly I am as your spokesman before God; I also have been formed out of clay."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Look, you and I both belong to God. I, too, was formed from clay."
      }
