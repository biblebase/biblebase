    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.35.9-16",
            "paragraphs": [
              "三五9～16以利戶說明他的信念，那就是禱告得不回應--即使是義人的禱告--是因為人缺乏信心（10節），並因為禱告的虛妄（13節；比較雅四3）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "但无人问，‘创造我的上帝在哪里？ 祂使人夜间欢唱，"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "卻沒有說：‘　神我的創造主在哪裡呢？ 他使人在夜間歌唱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "卻無人說：造我的神在哪裏？ 他使人夜間歌唱，"
      },
      "nasb": {
