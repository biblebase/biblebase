    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.35.9-16",
            "paragraphs": [
              "三五9～16以利戶說明他的信念，那就是禱告得不回應--即使是義人的禱告--是因為人缺乏信心（10節），並因為禱告的虛妄（13節；比較雅四3）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“人们因饱受压迫而呼求， 因强权者的压制而求救。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他們因多受欺壓而呼叫， 因受強權者的壓制而呼求。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "人因多受欺壓就哀求， 因受能者的轄制便求救，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Because of the multitude of oppressions they cry out; They cry for help because of the arm of the mighty."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“People cry out under a load of oppression; they plead for relief from the arm of the powerful."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“People cry out when they are oppressed. They groan beneath the power of the mighty."
      }
    },
