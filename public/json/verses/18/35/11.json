    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.35.9-16",
            "paragraphs": [
              "三五9～16以利戶說明他的信念，那就是禱告得不回應--即使是義人的禱告--是因為人缺乏信心（10節），並因為禱告的虛妄（13節；比較雅四3）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "使我们比地上的走兽聪明， 比天上的飞鸟有智慧。’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他教導我們過於教導地上的走獸， 使我們有智慧勝於空中的飛鳥。’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "教訓我們勝於地上的走獸， 使我們有聰明勝於空中的飛鳥。"
      },
      "nasb": {
        "version_id": "NASB",
