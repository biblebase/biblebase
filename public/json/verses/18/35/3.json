    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.35.1-4",
            "paragraphs": [
              "三五1～4以利戶陳明約伯的抱怨，虔誠對人來說一點用處都沒有。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你还说，‘这与我何干？ 我不犯罪又有何益处？’"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你還說：‘你有甚麼益處呢？ 我不犯罪有甚麼好處？’"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "才說這與我有甚麼益處？ 我不犯罪比犯罪有甚麼好處呢？"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For you say, ‘What advantage will it be to You? What profit will I have, more than if I had sinned?’"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Yet you ask him, ‘What profit is it to me, and what do I gain by not sinning?’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For you also ask, ‘What’s in it for me? What’s the use of living a righteous life?’"
      }
    },
