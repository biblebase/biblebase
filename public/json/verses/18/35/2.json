    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.35.1-4",
            "paragraphs": [
              "三五1～4以利戶陳明約伯的抱怨，虔誠對人來說一點用處都沒有。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“你在上帝面前自以为义， 你认为这合理吗？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“你說：‘我在　神面前更顯為義’， 你自以為這話有理嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你以為有理， 或以為你的公義勝於神的公義，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“Do you think this is according to justice? Do you say, ‘My righteousness is more than God’s’?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Do you think this is just? You say, ‘I am in the right, not God.’"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“Do you think it is right for you to claim, ‘I am righteous before God’?"
      }
    },
