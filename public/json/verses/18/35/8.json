    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.35.5-8",
            "paragraphs": [
              "三五5～8以利戶斷言神是高超的，祂不會受到人的虔誠或不虔誠的影響；這些態度只會影響其它人（8節）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你的罪恶只能伤害你的同类， 你的公义只能令世人受益。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "你的邪惡只能害像你的人， 你的公義也只能叫世人得益。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "你的過惡或能害你這類的人； 你的公義或能叫世人得益處。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Your wickedness is for a man like yourself, And your righteousness is for a son of man."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Your wickedness only affects humans like yourself, and your righteousness only other people."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "No, your sins affect only people like yourself, and your good deeds also affect only humans."
      }
