    "interpretations": {
      "leili": {
        "content": [
          {
            "verse": "job.35.9-16",
            "paragraphs": [
              "三五9～16以利戶說明他的信念，那就是禱告得不回應--即使是義人的禱告--是因為人缺乏信心（10節），並因為禱告的虛妄（13節；比較雅四3）。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "更何况你说你看不见祂， 你的案子已呈上，在等祂裁决。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "更何況你說你不得見他。 你的案件在他面前，你忍耐地等候他吧！"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "何況你說，你不得見他； 你的案件在他面前，你等候他吧。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How much less when you say you do not behold Him, The case is before Him, and you must wait for Him!"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How much less, then, will he listen when you say that you do not see him, that your case is before him and you must wait for him,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "You say you can’t see him, but he will bring justice if you will only wait."
      }
