        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Terrors are turned against me; They pursue my honor as the wind, And my prosperity has passed away like a cloud."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Terrors overwhelm me; my dignity is driven away as by the wind, my safety vanishes like a cloud."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "I live in terror now. My honor has blown away in the wind, and my prosperity has vanished like a cloud."
      }
