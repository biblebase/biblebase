    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "随后雷声隆隆， 祂发出威严之声。 祂一发声，雷电交加。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "接著雷聲霹靂， 　神以自己威嚴之聲打雷， 人聽到雷聲的時候， 　神並沒有留住風霜雨雲。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "隨後人聽見有雷聲轟轟，大發威嚴， 雷電接連不斷。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "After it, a voice roars; He thunders with His majestic voice, And He does not restrain the lightnings when His voice is heard."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "After that comes the sound of his roar; he thunders with his majestic voice. When his voice resounds, he holds nothing back."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then comes the roaring of the thunder— the tremendous voice of his majesty. He does not restrain it when he speaks."
      }
    },
