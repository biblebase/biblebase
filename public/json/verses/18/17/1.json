    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "job.17.1",
            "paragraphs": [
              "17:1",
              "我的心靈消耗：心靈破碎的約伯相信自己已不久於人世。他看自己為神所丟棄的人，又是同伴們譏誚的對象。他除了持守自己的義，別無他法(9節)，盡管一切看起來都事與願違，他仍堅信神的公義(16:19~22)。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我的心灵破碎，我的年日将尽， 坟墓等候着我。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我的氣息將斷， 我的日子快盡， 墳墓已為我預備好了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我的心靈消耗，我的日子滅盡； 墳墓為我預備好了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“My spirit is broken, my days are extinguished, The grave is ready for me."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "My spirit is broken, my days are cut short, the grave awaits me."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“My spirit is crushed, and my life is nearly snuffed out. The grave is ready to receive me."
      }
    },
