    "interpretations": {
      "fengsheng": {
        "content": [
          {
            "verse": "job.10.1",
            "paragraphs": [
              "10:1",
              "心裏苦惱：在第10章裏約伯繼續向神傾吐他的愁苦和倍感不公的心情。然而盡管約伯感到神收回了他的慈愛，他仍堅信神的公義，並竭力向神祈求，求神為他在困境中開出路。"
            ]
          }
        ],
        "title": "豐盛生命研讀本"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "“我厌恶自己的生命， 我要倾诉我的哀怨， 吐露心中的苦水。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "“我厭惡我的性命， 我要盡情吐苦水， 傾訴心中的痛苦。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我厭煩我的性命， 必由着自己述說我的哀情； 因心裏苦惱，我要說話，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "“I loathe my own life; I will give full vent to my complaint; I will speak in the bitterness of my soul."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“I loathe my very life; therefore I will give free rein to my complaint and speak out in the bitterness of my soul."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "“I am disgusted with my life. Let me complain freely. My bitter soul must complain."
      }
    },
