    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "job.8.1-22",
            "paragraphs": [
              "iii.比勒達（八1～22）"
            ]
          },
          {
            "verse": "job.8.8-10",
            "paragraphs": [
              "8～10.就像經常發生的一樣，論據越弱，陳述得越有確信。比勒達訴諸於古代的智慧，仿佛他正在說的是普通常識一樣。在這一小段中，有著輕微之諷刺文學的筆觸；約伯記的作者在此似乎要暗示出他這本書的目的之一：要質疑這類的傳說，打敗那盲目地持守它的人。比勒達與約伯從相同的起點開始：我們在世的日子好像影兒（9b節；參七7、16）；但他們卻是朝著相反的方向前進，兩個人都承認：我們一無所知。一個人有限的經歷必須由過去所累積的經歷來擴大，或更好是由黃金時代起初較純的智慧來擴大223。追念224列祖225的尋索結果，是可靠知識的來源。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      },
      "leili": {
        "content": [
          {
            "verse": "job.8.8-10",
            "paragraphs": [
              "八8～10比勒達以先人的教訓為權威，期望約伯會聽他們智慧的話。"
            ]
          }
        ],
        "title": "雷厲研讀本聖經",
        "author": "雷厲"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "因为我们就像昨天才出生，一无所知， 我们在世的日子如同掠影。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "我們只是昨天才有的，所以一無所知； 我們在世的日子不過是影兒。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "我們不過從昨日才有，一無所知； 我們在世的日子好像影兒。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "For we are only of yesterday and know nothing, Because our days on earth are as a shadow."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "for we were born only yesterday and know nothing, and our days on earth are but a shadow."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "For we were born but yesterday and know nothing. Our days on earth are as fleeting as a shadow."
      }
    },
