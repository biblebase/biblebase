      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他吃得一無所剩， 所以他的福樂不能持久；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "其餘的沒有一樣他不吞滅， 所以他的福樂不能長久。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Nothing remains for him to devour, Therefore his prosperity does not endure."
      },
      "niv": {
        "version_id": "NIV",
