    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他做耶和华视为恶的事，不过没有他父母那么败坏，因为他除掉了他父亲立的巴力神柱。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他行耶和華看為惡的事，只是還不至於像他的父親和母親一樣，因為他除去了他父親所做的巴力神柱。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "他行耶和華眼中看為惡的事，但不致像他父母所行的，因為除掉他父所造巴力的柱像。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "He did evil in the sight of the Lord, though not like his father and his mother; for he put away the sacred pillar of Baal which his father had made."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "He did evil in the eyes of the Lord, but not as his father and mother had done. He got rid of the sacred stone of Baal that his father had made."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "He did what was evil in the Lord’s sight, but not to the same extent as his father and mother. He at least tore down the sacred pillar of Baal that his father had set up."
      }
    },
