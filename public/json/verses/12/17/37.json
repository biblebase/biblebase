      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "The statutes and the ordinances and the law and the commandment which He wrote for you, you shall observe to do forever; and you shall not fear other gods."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "You must always be careful to keep the decrees and regulations, the laws and commands he wrote for you. Do not worship other gods."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Be careful at all times to obey the decrees, regulations, instructions, and commands that he wrote for you. You must not worship other gods."
      }
