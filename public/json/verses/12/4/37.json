    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "2ki.4.8-37",
            "paragraphs": [
              "b.書念婦人的兒子（四8～37）",
              "有關書念婦人及她兒子死裏復活的事件印證了以利沙繼承了預言及醫治的大能，成為人所津津樂道的故事（參八1～6），也同樣彰顯了款待人而獲得的福分及神的關顧。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "妇人进来，在以利沙脚前下拜，然后抱起儿子出去了。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是她進去，在他的腳前下拜，俯伏在地上；然後把兒子抱起來，出去了。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "婦人就進來，在以利沙腳前俯伏於地，抱起她兒子出去了。"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then she went in and fell at his feet and bowed herself to the ground, and she took up her son and went out."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She came in, fell at his feet and bowed to the ground. Then she took her son and went out."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She fell at his feet and bowed before him, overwhelmed with gratitude. Then she took her son in her arms and carried him downstairs."
      }
    },
