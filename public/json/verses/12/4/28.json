    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "2ki.4.8-37",
            "paragraphs": [
              "b.書念婦人的兒子（四8～37）",
              "有關書念婦人及她兒子死裏復活的事件印證了以利沙繼承了預言及醫治的大能，成為人所津津樂道的故事（參八1～6），也同樣彰顯了款待人而獲得的福分及神的關顧。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "妇人说：“我何尝向我主求过儿子呢？我不是对你说不要骗我吗？”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她說：“我有向我主求過兒子嗎？我不是說過：‘不要欺哄我’的嗎？”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "婦人說：「我何嘗向我主求過兒子呢？我豈沒有說過，不要欺哄我嗎？」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "Then she said, “Did I ask for a son from my lord? Did I not say, ‘Do not deceive me’?”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "“Did I ask you for a son, my lord?” she said. “Didn’t I tell you, ‘Don’t raise my hopes’?”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Then she said, “Did I ask you for a son, my lord? And didn’t I say, ‘Don’t deceive me and get my hopes up’?”"
      }
    },
