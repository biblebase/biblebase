    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "你们即使依靠埃及的战车和骑兵，又怎能打败我主人的一个最小的将领呢？"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "否則，你怎能使我主最小的一位臣僕轉臉逃跑呢？你竟然倚賴埃及供應戰車和戰馬嗎？"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "若不然，怎能打敗我主臣僕中最小的軍長呢？你竟倚靠埃及的戰車馬兵嗎？"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "How then can you repulse a single captain among the least of my master’s servants, when you trust in Egypt for chariots and for horsemen?"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "How then can you repulse one official of the least of my master’s servants, and rely on Egypt for chariots and for horsemen?"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "How can you repulse one officer of the least of my master’s officials, even though you are depending on Egypt for chariots and horsemen?"
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "How then will you repel one captain of the least of my master’s servants, and put your trust in Egypt for chariots and horsemen?"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "With your tiny army, how can you think of challenging even the weakest contingent of my master’s troops, even with the help of Egypt’s chariots and charioteers?"
      }
    },
