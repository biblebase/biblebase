    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "2ki.8.16-29",
            "paragraphs": [
              "vi.王朝歷史（八16～29）"
            ]
          },
          {
            "verse": "2ki.8.16-24",
            "paragraphs": [
              "a.猶大的約蘭（八16～24）",
              "參：歷代誌下二十一1～20。這裏繼續列王紀上二十二50的猶大歷史。",
              "常見的前言公式（16～17節）及結束日期（23～24節；見導論Ⅵ“文學形式”）中間有作者對此王較長的評價（18～19節）以及他與以東爭戰的情節（20～22節）。此時期恰巧猶大王及以色列王乃為同名，各抄本對於使用\\cs16Jehoram全名或是其簡寫Joram並不一致，但為方便起見，本書將用Jehoram以指猶大王約蘭，Joram指以色列王約蘭。請註意他們兩位都被歸類為惡王。"
            ]
          },
          {
            "verse": "2ki.8.23-24",
            "paragraphs": [
              "23～24.同樣的，作者並未記載約蘭因為患上無藥可治的腸病，導致慢性痢疾及嚴重直腸脫垂而死（代下二十一18～19）。他也並未記載約蘭雖被埋葬在大衛城中，但卻並非在王室陵墓裏榮譽埋葬的。歷代誌作者納入了一封信，以以利亞所用的句法為本，嚴厲責備約蘭與以色列聯盟（代下二十一12～15）。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "约兰与祖先同眠后，葬在大卫城他的祖坟里。他儿子亚哈谢继位。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "約蘭和他的祖先同睡；人把他埋葬在大衛城和他的祖先一起；他的兒子亞哈謝接續他作王。"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "約蘭與他列祖同睡，葬在大衛城他列祖的墳地裏。他兒子亞哈謝接續他作王。"
      },
      "esv": {
        "version_id": "ESV",
        "version_name": "English Standard Version",
        "lang": "en",
        "text": "So Joram slept with his fathers and was buried with his fathers in the city of David, and Ahaziah his son reigned in his place."
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Joram slept with his fathers and was buried with his fathers in the city of David; and Ahaziah his son became king in his place."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Jehoram rested with his ancestors and was buried with them in the City of David. And Ahaziah his son succeeded him as king."
      },
      "nkjv": {
        "version_id": "NKJV",
        "version_name": "New King James Version",
        "lang": "en",
        "text": "So Joram rested with his fathers, and was buried with his fathers in the City of David. Then Ahaziah his son reigned in his place."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "When Jehoram died, he was buried with his ancestors in the City of David. Then his son Ahaziah became the next king."
      }
    },
