    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "亚哈斯差遣使者去见亚述王提革拉·毗列色，对他说：“我是你的仆人、你的儿子，现在亚兰王和以色列王攻打我，求你从他们手中拯救我。”"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "於是亞哈斯差派使者去見亞述王提革拉．毘列色，說：“我是你的臣僕，你的兒子，求你上來，救我脫離那起來攻擊我的亞蘭王和以色列王的手。”"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "亞哈斯差遣使者去見亞述王提革拉‧毗列色，說：「我是你的僕人、你的兒子。現在亞蘭王和以色列王攻擊我，求你來救我脫離他們的手。」"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So Ahaz sent messengers to Tiglath-pileser king of Assyria, saying, “I am your servant and your son; come up and deliver me from the hand of the king of Aram and from the hand of the king of Israel, who are rising up against me.”"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Ahaz sent messengers to say to Tiglath-Pileser king of Assyria, “I am your servant and vassal. Come up and save me out of the hand of the king of Aram and of the king of Israel, who are attacking me.”"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "King Ahaz sent messengers to King Tiglath-pileser of Assyria with this message: “I am your servant and your vassal. Come up and rescue me from the attacking armies of Aram and Israel.”"
      }
    },
