    "interpretations": {
      "dde": {
        "content": [
          {
            "verse": "2ki.24.8-17",
            "paragraphs": [
              "iii.猶大的約雅斤（二十四8～17）",
              "約雅斤的統治（主前597年）亦記錄於歷代誌下三十六9～10；耶利米書二十九2；以斯得拉一書41～44；約瑟夫《猶太古史》x.6.3，7.1及列王紀下二十五27～30。"
            ]
          },
          {
            "verse": "2ki.24.15-16",
            "paragraphs": [
              "15～16.約雅斤的被擄（參27～30節）應驗了耶利米的預言（二十二24～27）。巴比倫的泥版能證明他曾經到過巴比倫，其中記載他的家庭及五個兒子於主前五九二至五六九年所得到的油及谷的供應，並稱他為“猶大人的王Yaukin”453。"
            ]
          }
        ],
        "title": "丁道爾聖經註釋",
        "author": "丁道爾"
      }
    },
    "versions": {
      "ccb": {
        "version_id": "CCB",
        "version_name": "圣经当代译本",
        "lang": "chs",
        "text": "他把约雅斤及其母亲、妃嫔、臣仆和国中政要从耶路撒冷掳到巴比伦。"
      },
      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "他把約雅斤擄到巴比倫去，又把太后、后妃、太監和國中的要員都從耶路撒冷擄到巴比倫；"
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "並將約雅斤和王母、后妃、太監，與國中的大官，都從耶路撒冷擄到巴比倫去了；"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "So he led Jehoiachin away into exile to Babylon; also the king’s mother and the king’s wives and his officials and the leading men of the land, he led away into exile from Jerusalem to Babylon."
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "Nebuchadnezzar took Jehoiachin captive to Babylon. He also took from Jerusalem to Babylon the king’s mother, his wives, his officials and the prominent people of the land."
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "Nebuchadnezzar led King Jehoiachin away as a captive to Babylon, along with the queen mother, his wives and officials, and all Jerusalem’s elite."
      }
    },
