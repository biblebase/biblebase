      "cnv": {
        "version_id": "CNV",
        "version_name": "新譯本",
        "lang": "cht",
        "text": "她對他們說：“耶和華以色列的　神這樣說：‘告訴那差你們到我這裡來的人說："
      },
      "cunp": {
        "version_id": "CUNP",
        "version_name": "和合本新標點（神版）",
        "lang": "cht",
        "text": "她對他們說：「耶和華－以色列的神如此說：『你們可以回覆那差遣你們來見我的人說，"
      },
      "nasb": {
        "version_id": "NASB",
        "version_name": "New American Standard Bible",
        "lang": "en",
        "text": "She said to them, “Thus says the Lord God of Israel, ‘Tell the man who sent you to me,"
      },
      "niv": {
        "version_id": "NIV",
        "version_name": "New International Version",
        "lang": "en",
        "text": "She said to them, “This is what the Lord, the God of Israel, says: Tell the man who sent you to me,"
      },
      "nlt": {
        "version_id": "NLT",
        "version_name": "New Living Translation",
        "lang": "en",
        "text": "She said to them, “The Lord, the God of Israel, has spoken! Go back and tell the man who sent you,"
      }
    },
